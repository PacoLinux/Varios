                                     00C7   176 ;++
                                     00C7   177 ; RSX$SDAT - RSX11M SEND DATA DIRECTIVE
                                     00C7   178 ;
                                     00C7   179 ; FUNCTIONAL DESCRIPTION:
                                     00C7   180 ;
                                     00C7   181 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     00C7   182 ; "SDAT$" DIRECTIVE.
                                     00C7   183 ;
                                     00C7   184 ; INPUT PARAMETERS:
                                     00C7   185 ;
                                     00C7   186 ;	R5 = ADDRESS OF DPB+2
                                     00C7   187 ;	R6 = CM IMAGE STACK ADDRESS
                                     00C7   188 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     00C7   189 ;	R11 = ADDRESS OF IMAGE HEADER
                                     00C7   190 ;
                                     00C7   191 ; OUTPUT PARAMETERS:
                                     00C7   192 ;
                                     00C7   193 ;	R10,R11 ARE PRESERVED
                                     00C7   194 ;
                                     00C7   195 ; IMPLICIT OUTPUTS:
                                     00C7   196 ;
                                     00C7   197 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     00C7   198 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     00C7   199 ;
                                     00C7   200 ; DSW RETURN CODES:
                                     00C7   201 ;
                                     00C7   202 ;	IS.SUC	SUCCESSFUL
                                     00C7   203 ;	IE.INS	RECEIVER IS UNKNOWN
                                     00C7   204 ;	IE.IEF	ILLEGAL EVENT FLAG
                                     00C7   205 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     00C7   206 ;--
                                     00C7   207 NOBOX:					; NO MAILBOX ERROR
                                     00C7   208 	$DSW	#IE.INS			;
                                     00D2   209 RSX$SDAT::				; SEND DATA DIRECTIVE
                                     00D2   210  
                                     00D2   211 	.IF NDF	C$$ORE
                                     00D2   212  
                                     00D2   213 	CMPL	(R5),#^XB37C66E4	; SEND TO "PRT..."?
                                     00D2   214 	BNEQ	1$
                                     00D2   215 	BRW	200$
                                     00D2   216  
                                     00D2   217 	.ENDC
                                     00D2   218  
                          FF2B'  30  00D2   219 1$:	BSBW	RSX$CVTNAME		; CONVERT TARGET NAME TO ASCII
