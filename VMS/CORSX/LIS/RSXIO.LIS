                                     0036   101 ;++
                                     0036   102 ; RSX$RCVD - RSX11M RECEIVE DATA DIRECTIVE
                                     0036   103 ; RSX$RCVX - RSX11M RECEIVE DATA OR EXIT DIRECTIVE
                                     0036   104 ;
                                     0036   105 ; FUNCTIONAL DESCRIPTION:
                                     0036   106 ;
                                     0036   107 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS THE RSX11M
                                     0036   108 ; "RCVD$" AND "RCVX$" DIRECTIVES.
                                     0036   109 ;
                                     0036   110 ; INPUT PARAMETERS:
                                     0036   111 ;
                                     0036   112 ;	R5 = ADDRESS OF DPB+2
                                     0036   113 ;	R6 = CM IMAGE STACK ADDRESS
                                     0036   114 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0036   115 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0036   116 ;
                                     0036   117 ; OUTPUT PARAMETERS:
                                     0036   118 ;
                                     0036   119 ;	R10,R11 ARE PRESERVED
                                     0036   120 ;
                                     0036   121 ; IMPLICIT OUTPUTS:
                                     0036   122 ;
                                     0036   123 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0036   124 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0036   125 ;
                                     0036   126 ; DSW RETURN CODES:
                                     0036   127 ;
                                     0036   128 ;	IS.SUC	SUCCESSFUL
                                     0036   129 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     0036   130 ;	IE.ITS	NO DATA ( ANY QIO ERROR )
                                     0036   131 ;--
                                     0036   132 RSX$RCVX::				; RECEIVE OR EXIT
            57            01     9A  0036   133 	MOVZBL	#1,R7			; SET EXIT FLAG
                          02     11  0039   134 	BRB	10$			; AND CONTINUE IN COMMON
                                     003B   135 RSX$RCVD::				; RECEIVE DATA DIRECTIVE
                          57     D4  003B   136 	CLRL	R7			; SET FLAG FOR NO EXIT
                                     003D   137 10$:					; COMMON ENTRY POINT TO RECEIVES
            54         04 A5     3C  003D   138 	MOVZWL	4(R5),R4		; ADDRESS BUFFER SPECIFIED
            52       013C CA     D0  0041   139 	MOVL	I_RCVMAIL(R10),R2	; GET MAILBOX CHANNEL
                          29     13  0046   140 	BEQL	20$			; IF EQL THEN NO BOX AND NO DATA
                                     0048   141 ;
                                     0048   142 ; READ THE MAILBOX
                                     0048   143 ;
                                     0048   144 	$QIOW_S	#7,I_RCVMAIL(R10),#IO$_READVBLK!IO$M_NOW,I_SCRATCH(R10)-
                                     0048   145 	,,,4(R4),#26
            06            50     E9  0068   146 	BLBC	R0,20$			; BR IF NOT SUCCESS
            50            6A     3C  006B   147 	MOVZWL	I_SCRATCH(R10),R0	; SET ERROR CODE IF ANY
            0C            50     E8  006E   148 	BLBS	R0,25$			; IF LOW SET THEN SUCCESS
            48            57     E9  0071   149 20$:	BLBC	R7,35$			; IF CLEAR THEN NO EXIT
                                     0074   150 	$EXIT_S				; EXIT IMAGE
                                     007D   151 ;
                                     007D   152 ; RETURN MESSAGE TO IMAGE
                                     007D   153 ;
                                     007D   154 ; CONVERT NAME OF SENDER TO RAD50
