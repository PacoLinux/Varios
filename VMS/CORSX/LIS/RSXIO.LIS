                                     0000    47 	.SBTTL	RSX$GMCR - RSX11M GMCR$ DIRECTIVE
                                     0000    48 ;++
                                     0000    49 ; RSX$GMCR - RSX11M GET MCR LINE DIRECTIVE
                                     0000    50 ;
                                     0000    51 ; FUNCTIONAL DESCRIPTION:
                                     0000    52 ;
                                     0000    53 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0000    54 ; "GMCR$" DIRECTIVE.
                                     0000    55 ;
                                     0000    56 ; INPUT PARAMETERS:
                                     0000    57 ;
                                     0000    58 ;	R5 = ADDRESS OF DPB+2
                                     0000    59 ;	R6 = CM IMAGE STACK ADDRESS
                                     0000    60 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0000    61 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0000    62 ;
                                     0000    63 ; OUTPUT PARAMETERS:
                                     0000    64 ;
                                     0000    65 ;	R10,R11 ARE PRESERVED
                                     0000    66 ;
                                     0000    67 ; IMPLICIT OUTPUTS:
                                     0000    68 ;
                                     0000    69 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0000    70 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0000    71 ;
                                     0000    72 ; DSW RETURN CODES:
                                     0000    73 ;
                                     0000    74 ;	IS.SUC	SUCCESSFUL
                                     0000    75 ;	IE.AST	NO DATA AVAILABLE
                                     0000    76 ;--
                                     0000    77 RSX$GMCR::				; GET MCR LINE DIRECTIVE
            57       00C0 CA     3C  0000    78 	MOVZWL	I_CMDSZ(R10),R7		; GET THE SIZE OF THE COMMAND
            57       0050 8F     B1  0005    79 	CMPW	#80,R7			; 80 OR GREATER?
                          04     1E  000A    80 	BGEQU	5$			; IF GEQU THEN OK
            57         50 8F     9A  000C    81 	MOVZBL	#80,R7			; SET MAX SIZE
         26 AB            57     B0  0010    82 5$:	MOVW	R7,H.DSW(R11)		; INSERT COMMAND SIZE
                          15     13  0014    83 	BEQL	10$			; IF EQL THEN ERROR
                                     0016    84 ;
                                     0016    85 ; MOVE THE COMMAND TO THE USER BUFFER - BUFFER HAS BEEN VALIDATED
                                     0016    86 ;
       00C4 DA            57     28  0016    87 	MOVC3	R7,@I_CMDAD(R10),(R5)	; MOVE COMMAND TEXT
                          65         001B       
                     00C0 CA     D4  001C    88 	CLRL	I_CMDSZ(R10)		; SET NO NEXT COMMAND
            57       0050 8F     B1  0020    89 	CMPW	#80,R7			; EXACTLY 80?
                          03     13  0025    90 	BEQL	7$			; IF EQL THEN YES
            63            0D     90  0027    91 	MOVB	#^X0D,(R3)		; INSERT CR AS TERMINATOR
                                 05  002A    92 7$:	RSB				; RETURN
                                     002B    93 ;
                                     002B    94 ; NO COMMAND LINE
                                     002B    95 ;
                                     002B    96 10$:	$DSW	#IE.AST			;
