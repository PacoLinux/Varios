                                     008D   194 	.SBTTL	RSX$GTIM - RSX11M GTIM$ DIRECTIVE
                                     008D   195 ;++
                                     008D   196 ; RSX$GTIM - RSX11M GET TIME DIRECTIVE
                                     008D   197 ;
                                     008D   198 ; FUNCTIONAL DESCRIPTION:
                                     008D   199 ;
                                     008D   200 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     008D   201 ; "GTIM$" DIRECTIVE.
                                     008D   202 ;
                                     008D   203 ; INPUT PARAMETERS:
                                     008D   204 ;
                                     008D   205 ;	R5 = ADDRESS OF DPB+2
                                     008D   206 ;	R6 = CM IMAGE STACK ADDRESS
                                     008D   207 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     008D   208 ;	R11 = ADDRESS OF IMAGE HEADER
                                     008D   209 ;
                                     008D   210 ; OUTPUT PARAMETERS:
                                     008D   211 ;
                                     008D   212 ;	R10,R11 ARE PRESERVED
                                     008D   213 ;
                                     008D   214 ; IMPLICIT OUTPUTS:
                                     008D   215 ;
                                     008D   216 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     008D   217 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     008D   218 ;
                                     008D   219 ; DSW RETURN CODES:
                                     008D   220 ;
                                     008D   221 ;	IS.SUC	SUCCESSFUL
                                     008D   222 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     008D   223 ;--
                                     008D   224 RSX$GTIM::				; GET TIME DIRECTIVE
            54            65     3C  008D   225 	MOVZWL	(R5),R4			; ADDRESS BUFFER
                                     0090   226 	$NUMTIM_S	(R4),		; INSERT TIME IN BUFFER
            06            50     E9  009B   227 	BLBC	R0,10$			; IF LOW CLEAR THEN ACCESS VIOLATION
            64       076C 8F     A2  009E   228 	SUBW	#1900,(R4)		; TAKE OUT YEAR SINCE 0
                                 05  00A3   229 	RSB				; RETURN
                                     00A4   230 10$:	$DSW	#IE.ADP			; SET STATUS AND RETURN
                                     00AF   231 ;
                                     00AF   232 	.END
