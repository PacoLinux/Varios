                                     005F   149 	.SBTTL	RSX$GTSK - RSX11M GTSK$ DIRECTIVE
                                     005F   150 ;++
                                     005F   151 ; RSX$GTSK - RSX11M GET TASK PARAMETERS DIRECTIVE
                                     005F   152 ;
                                     005F   153 ; FUNCTIONAL DESCRIPTION:
                                     005F   154 ;
                                     005F   155 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     005F   156 ; "GTSK$" DIRECTIVE.
                                     005F   157 ;
                                     005F   158 ; INPUT PARAMETERS:
                                     005F   159 ;
                                     005F   160 ;	R5 = ADDRESS OF DPB+2
                                     005F   161 ;	R6 = CM IMAGE STACK ADDRESS
                                     005F   162 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     005F   163 ;	R11 = ADDRESS OF IMAGE HEADER
                                     005F   164 ;
                                     005F   165 ; OUTPUT PARAMETERS:
                                     005F   166 ;
                                     005F   167 ;	R10,R11 ARE PRESERVED
                                     005F   168 ;
                                     005F   169 ; IMPLICIT OUTPUTS:
                                     005F   170 ;
                                     005F   171 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     005F   172 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     005F   173 ;
                                     005F   174 ; DSW RETURN CODES:
                                     005F   175 ;
                                     005F   176 ;	IS.SUC	SUCCESSFUL
                                     005F   177 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     005F   178 ;--
                                     005F   179 RSX$GTSK::				; GET TASK PARAMETERS DIRECTIVE
            54            65     3C  005F   180 	MOVZWL	(R5),R4			; ADDRESS BUFFER
            84       0124 CA     D0  0062   181 	MOVL	I_RAD50NAME(R10),(R4)+	; INSERT TASK NAME
            84       2C8F 8F     3C  0067   182 	MOVZWL	#^O026217,(R4)+		; INSERT "GEN   " AS PARTITION
                          84     D4  006C   183 	CLRL	(R4)+			; INSERT UNDEFINED WORDS
            84         30 AB     B0  006E   184 	MOVW	H.SPRI(R11),(R4)+	; INSERT PRIORITY
            84         08 AB     B0  0072   185 	MOVW	H.CUIC(R11),(R4)+	; INSERT UIC
            84         3C AB     B0  0076   186 	MOVW	H.NLUN(R11),(R4)+	; INSERT NUMBER OF LUNS
                          84     D4  007A   187 	CLRL	(R4)+			; INSERT UNDEFINED WORDS
            84         16 AB     B0  007C   188 	MOVW	H.TKVA(R11),(R4)+	; INSERT SST VECTOR DATA
            84         18 AB     B0  0080   189 	MOVW	H.TKVL(R11),(R4)+	;
            84       00DC CA     B0  0084   190 	MOVW	I_ADR0+ADR_HIGHLIM(R10),(R4)+; INSERT SIZE IN BYTES
            64            05     9A  0089   191 	MOVZBL	#5,(R4)			; TELL ITS THE RSX!!!!!
                                 05  008C   192 	RSB				; AND RETURN
