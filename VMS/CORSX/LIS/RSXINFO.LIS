                                     0048   109 	.SBTTL	RSX$GPRT - RSX11M GPRT$ DIRECTIVE
                                     0048   110 ;++
                                     0048   111 ; RSX$GPRT - RSX11M GET PARTITION PARAMETERS DIRECTIVE
                                     0048   112 ;
                                     0048   113 ; FUNCTIONAL DESCRIPTION:
                                     0048   114 ;
                                     0048   115 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0048   116 ; "GPRT$" DIRECTIVE.
                                     0048   117 ;
                                     0048   118 ; INPUT PARAMETERS:
                                     0048   119 ;
                                     0048   120 ;	R5 = ADDRESS OF DPB+2
                                     0048   121 ;	R6 = CM IMAGE STACK ADDRESS
                                     0048   122 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0048   123 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0048   124 ;
                                     0048   125 ; OUTPUT PARAMETERS:
                                     0048   126 ;
                                     0048   127 ;	R10,R11 ARE PRESERVED
                                     0048   128 ;
                                     0048   129 ; IMPLICIT OUTPUTS:
                                     0048   130 ;
                                     0048   131 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0048   132 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0048   133 ;
                                     0048   134 ; DSW RETURN CODES:
                                     0048   135 ;
                                     0048   136 ;	IS.SUC	SUCCESSFUL
                                     0048   137 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     0048   138 ;
                                     0048   139 ;--
                                     0048   140 RSX$GPRT::				; GET PARTITION PARAMETERS DIRECTIVE
            54         04 A5     3C  0048   141 	MOVZWL	4(R5),R4		; GET BUFFER ADDRESS SPECIFIED
            84       0100 8F     B0  004C   142 	MOVW	#^X100,(R4)+		; INSERT PARTITION BASE ADDRESS
            0A            06     EF  0051   143 	EXTZV	#6,#10,I_ADR0+ADR_HIGHLIM(R10),R3; INSERT TASK SIZE IN 32 WORD BLOCKS
            53       00DC CA         0054       
            84            53     B0  0058   144 	MOVW	R3,(R4)+		;
                                     005B   145 					; SYSTEM CONTROLED PARTITION
                       26 AB     B4  005B   146 	CLRW	H.DSW(R11)		; SET PARTITION BASE ADDRESS
                                 05  005E   147 	RSB				; AND RETURN
