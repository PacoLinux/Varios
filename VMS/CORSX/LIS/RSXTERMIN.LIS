                                     0111   356 ; RSX$INTERNEXIT - TERMINATE IMAGE EXECUTION
                                     0111   357 ;
                                     0111   358 ; FUNCTIONAL DESCRIPION:
                                     0111   359 ;
                                     0111   360 ; THIS ROUTINE CLEANS UP THE EXECUTION OF THIS IMAGE AND RETURNS TO THE
                                     0111   361 ; ORIGIONAL CALLER.
                                     0111   362 ; 
                                     0111   363 ; INPUTS:
                                     0111   364 ;
                                     0111   365 ;	R0 = STATUS OF THE EXECUTION
                                     0111   366 ;	R10 = ADDRESS OF THE IMAGE IMPURE DATA BASE
                                     0111   367 ;	R11 = ADDRESS OF THE IMAGE
                                     0111   368 ;
                                     0111   369 ; OUTPUTS:
                                     0111   370 ;
                                     0111   371 ;	R0 = STATUS OF THE EXECUTION
                                     0111   372 ;--
                                     0111   373 RSX$INTERNEXIT::			; EXIT THE IMAGE
       0004'CF       FEF7 CF     9E  0111   374 	MOVAB	W^RSX$RETEXIT,W^RSX$A_EXITBLOCK+4; CHANGE EXIT ADDRESS
            59            50     D0  0118   375 	MOVL	R0,R9			; SAVE THE STATUS
                                     011B   376 INTERNEXIT:				;
                                     011B   377 ;
                                     011B   378 ; CLEAN UP THE COMMON EVENT FLAGS
                                     011B   379 ;
                                     011B   380 	$DACEFC_S	#95		;
                                     0128   381 ;
                                     0128   382 ; DEASSIGN RECEIVE MAILBOX
                                     0128   383 ;
                                     0128   384 	$DASSGN_S	I_RCVMAIL(R10)	;
                                     0134   385 ;
                                     0134   386 ; DEASSIGN ALL ASSIGNED DEVICES
