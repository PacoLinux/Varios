                          0A     10  00B7   194 20$:	BSBB	50$			; OUTPUT THE LINE
       007E'CF            83     D1  00B9   195 	CMPL	(R3)+,W^SPM		; REGISTER 6 DONE?
                          F7     12  00BE   196 	BNEQ	20$			; IF NEQ THEN NO
