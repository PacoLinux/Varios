V03.0           PRINT ERROR MESSAGE TO ERROR                                                                                     (5)

                                     00D7   217 	.SBTTL	PRINT ERROR MESSAGE TO ERROR
                                     00D7   218 ;
                                     00D7   219 ;++
                                     00D7   220 ; RSX$PRINTERR - PRINT ERROR MESSAGE ON ERROR DEVICE
                                     00D7   221 ;
                                     00D7   222 ; FUNCTIONAL DESCRIPTION:
                                     00D7   223 ;
                                     00D7   224 ; THE ROUTINE OUTPUTS A MESSAGE ON THE ERROR DEVICE.
                                     00D7   225 ;
                                     00D7   226 ; INPUTS:
                                     00D7   227 ;
                                     00D7   228 ;	I_SCRATCH,I_SCRATCH+4 IS THE STRING DESCRIPTER
                                     00D7   229 ;
                                     00D7   230 ;	R10 = ADDRESS OF THE IMAGE IMPURE DATA BASE
                                     00D7   231 ;	R11 = ADDRESS OF THE IMAGE
                                     00D7   232 ;
                                     00D7   233 ; OUTPUTS:
                                     00D7   234 ;
                                     00D7   235 ;	ALL REGISTERS ARE PRESERVED.
                                     00D7   236 ;--
                                     00D7   237 RSX$PRINTERR::				; PRINT ERROR ON ERROR DEVICE
                          06     BB  00D7   238 	PUSHR	#^M<R1,R2>		; SAVE REGISTERS
                                     00D9   239  
                                     00D9   240 	.IF NDF	C$$ORE
                                     00D9   241  
                                     00D9   242 	MOVAB	W^RSX$A_FAB,R2		; ADDRESS FAB
                                     00D9   243 	CLRW	FAB$W_IFI(R2)		; DISCONNECT
                                     00D9   244 	MOVAB	W^RSX$T_SYSERROR,R0	; ADDRESS NAME OF SYSERROR DEVICE
                                     00D9   245 	MOVB	(R0)+,FAB$B_FNS(R2)	; INSERT NAME IN FAB
                                     00D9   246 	MOVL	R0,FAB$L_FNA(R2)	;
                                     00D9   247 	MOVB	#FAB$M_CR,FAB$B_RAT(R2)	; SET CARRIAGE CONTROL DESIRED
                                     00D9   248 	$OPEN	FAB=R2			; OPEN SYS$ERROR
                                     00D9   249 	BLBC	R0,100$			; IF LOW CLEAR THEN ERROR
                                     00D9   250 	MOVW	FAB$W_IFI(R2),W^RAB$W_ISI+RSX$A_RAB; CONNECT RAB
                                     00D9   251 	CVTWL	FAB$W_IFI(R2),-(SP)	;
                                     00D9   252 	MOVZBW	I_SCRATCH(R10),W^RAB$W_RSZ+RSX$A_RAB;
                                     00D9   253 	MOVL	I_SCRATCH+4(R10),W^RAB$L_RBF+RSX$A_RAB;
                                     00D9   254 50$:	$PUT	RAB=W^RSX$A_RAB		; OUTPUT MESSAGE
                                     00D9   255 	CLRW	FAB$W_IFI(R2)		; DISCONNECT FAB
                                     00D9   256 	MOVAB	W^RSX$T_SYSOUTPUT,R0	; ADDRESS NAME OF SYSOUTPUT DEVICE
                                     00D9   257 	MOVB	(R0)+,FAB$B_FNS(R2)	; INSERT DATA IN FAB
                                     00D9   258 	MOVL	R0,FAB$L_FNA(R2)	;
                                     00D9   259 	$OPEN	FAB=R2			; OPEN SYS$OUT
                                     00D9   260 	BLBC	R0,100$			; IF LOW CLEAR THEN ERROR
                                     00D9   261 	CMPW	W^RAB$W_ISI+RSX$A_RAB,FAB$W_IFI(R2); OUTPUT SAME AS ERROR?
                                     00D9   262 	BEQL	100$			; IF EQL THEN DONE
                                     00D9   263 	MOVW	FAB$W_IFI(R2),W^RAB$W_ISI+RSX$A_RAB
                                     00D9   264 	BRB	50$			; OUTPUT IT AGAIN
                                     00D9   265 100$:	$CLOSE	FAB=R2			;
                                     00D9   266 	CVTLW	(SP)+,FAB$W_IFI(R2)	; RESTORE ERROR IFI
                                     00D9   267 	$CLOSE	FAB=R2			;
                                     00D9   268 	MOVZBL	#SS$_NORMAL,R0		; ALWAYS SUCCESS
                                     00D9   269  
                                     00D9   270 	.IFF
                                     00D9   271  
            51            6A     9B  00D9   272 	MOVZBW	I_SCRATCH(R10),R1	; LENGTH OF TRANSFER (BYTES)
                                     00DC   273 	$OUTPUT	-			; OUTPUT TO TI DEVICE
