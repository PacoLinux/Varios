                                     000F   119 	.SBTTL	RSX$ABORT - ABORT IMAGE EXECUTION
                                     000F   120 ;++
                                     000F   121 ; RSX$ABORT - ABORT EXECUTION AS A RESULT OF AN UNVECTORED CM TRAP OR RSX ERROR
                                     000F   122 ;
                                     000F   123 ; FUNCTIONAL DESCRIPTION:
                                     000F   124 ;
                                     000F   125 ; THIS ROUTINE IS ENTERED AS A RESULT OF AN UNVECTORED CM TRAP OR AN ERROR
                                     000F   126 ; IN THE EXECUTION OF THE RSX.
                                     000F   127 ;
                                     000F   128 ; INPUTS:	
                                     000F   129 ;
                                     000F   130 ;	R0 = ABORT CODE
                                     000F   131 ;	R10 = ADDRESS OF THE IMAGE DATA BASE
                                     000F   132 ;	R11 = ADDRESS OF THE IMAGE HEADER
                                     000F   133 ;
                                     000F   134 ; OUTPUTS:
                                     000F   135 ;
                                     000F   136 ;  THE RSX CALLER IS RETURNED TO WITH R0 SIGNALING THE ERROR.
                                     000F   137 ;--
                                     000F   138 ACCVIO:
                                     000F   139 	$EXIT_S	R9			; EXIT WITH OUT MESSAGE
                                     0018   140 RSX$ABORT::				; ABORT A CM IMAGE
       0004'CF       FFF0 CF     9E  0018   141 	MOVAB	W^RSX$RETEXIT,W^RSX$A_EXITBLOCK+4; CHANGE ADDRESS OF EXIT
            59            50     D0  001F   142 	MOVL	R0,R9			; COPY ABORT REASON
            5A       0000'CF     9E  0022   143 	MOVAB	W^RSX$A_IMPURE,R10	; ADDRESS IMPURE DATA BASE
                                     0027   144 ;
                                     0027   145 ; MAKE SURE THAT THE PER IMAGE IMPURE DATA BASE IS ACCESSIBLE
                                     0027   146 ;
                                     0027   147 	IFNORD	#I_LENGTH,(R10),ACCVIO	; IF NOT THEN EXIT DIRECTLY
                                     002F   148 ;
                                     002F   149 ; OUTPUT HEADER MESSAGE
                                     002F   150 ;
         0C AA         10 AA     9E  002F   151 	MOVAB	I_SCRATCH+16(R10),I_SCRATCH+12(R10); SET UP BUFFER DESC.
         04 AA         10 AA     9E  0034   152 	MOVAB	I_SCRATCH+16(R10),I_SCRATCH+4(R10);
         08 AA         64 8F     9A  0039   153 	MOVZBL	#100,I_SCRATCH+8(R10)	;
            50       0000'CF     9E  003E   154 	MOVAB	W^NAMEDABORT,R0		; ASSUME NAMED ABORT MESSAGE
            53       011C CA     9E  0043   155 	MOVAB	I_ASCIINAME(R10),R3	; ADDRESS NAME STRING
                                     0048   156 10$:	$FAO_S	(R0),I_SCRATCH(R10),I_SCRATCH+8(R10),R3,#0
                          007A   30  005A   157 	BSBW	RSX$PRINTERR		; PRINT ERROR
                                     005D   158 ;
                                     005D   159 ; PRINT DESCRIPTION LINE
                                     005D   160 ;
         60 AA         64 AA     9E  005D   161 	MOVAB	I_SCRATCH+100(R10),I_SCRATCH+96(R10)
         5C AA            28     9A  0062   162 	MOVZBL	#40,I_SCRATCH+92(R10)
                                     0066   163  
                                     0066   164 	.IF NDF	C$$ORE
                                     0066   165  
                                     0066   166 	$GETMSG_F	R9,I_SCRATCH+92(R10),I_SCRATCH+92(R10),#7
                                     0066   167  
                                     0066   168 	.IFF
                                     0066   169  
                                     0066   170 	GETMSG_S	R9,I_SCRATCH+92(R10),I_SCRATCH+92(R10)
                                     0075   171  
                                     0075   172 	.ENDC
                                     0075   173  
                                     0075   174 	$FAO_S	W^CAUSE,I_SCRATCH(R10),I_SCRATCH+8(R10),I_SCRATCH+92(R10),I_SCRATCH+96(R10)
                          0049   30  008B   175 	BSBW	RSX$PRINTERR		; OUTPUT MESSAGE
