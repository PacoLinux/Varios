            58            67     D0  0139   389 	MOVL	(R7),R8			; ADDRESS FIRST ENTRY
                                     013C   390 10$:	$DASSGN_S	LUT_CHAN(R8)	; DEASSIGN THE CHANNEL
            58            68     D0  0147   391 	MOVL	(R8),R8			; GET THE NEXT ENTRY
            57            58     D1  014A   392 	CMPL	R8,R7			; DONE?
                          ED     12  014D   393 	BNEQ	10$			; IF NEQ THEN NO
