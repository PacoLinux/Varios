                                     0000    94 ;++
                                     0000    95 ; RSX$FORCEEXIT - FORCE EXIT TERMINATION HANDLER FOR THE RSX
                                     0000    96 ; RSX$RETEXIT - NULL EXIT HANDLER
                                     0000    97 ;
                                     0000    98 ; FUNCTIONAL DESCRIPTION:
                                     0000    99 ;
                                     0000   100 ; THIS ROUTINE IS CALLED WHEN THIS IMAGE IS TO BE EXITED AS DIRECTED
                                     0000   101 ; FROM AN EXTERNAL SOURCE. THE ACTION IS TO CAUSE A TERMINATION MESSAGE
                                     0000   102 ; SET ON THE ERROR DEVICE.
                                     0000   103 ;
                                     0000   104 ; INPUTS:
                                     0000   105 ;
                                     0000   106 ; OUTPUTS:
                                     0000   107 ;
                                     0000   108 ;	CONTROL IS PASSED TO THE RSX TERMINATION ROUTINES
                                     0000   109 ;	REASON = AB_ABORT
                                     0000   110 ;--
                                     0000   111 RSX$FORCEEXIT::				; ABORTED IMAGE EXECUTION
                               0000  0000   112 	.WORD	0
            50   0006805C 8F     D0  0002   113 	MOVL	#RSX$_EXTERNABRT,R0	; SET STATUS
                          0105   31  0009   114 	BRW	RSX$INTERNEXIT		; ABORT EXECUTION
                                     000C   115 RSX$RETEXIT::				; NO ACTION EXIT HANDLER
                               0000  000C   116 	.WORD	0
                                 04  000E   117 	RET				; RETURN TO CALLER
