                                     0000    38 	.SBTTL	RSX$FILEIO	TRAP FILES I/O REQUESTS IN STAND-ALONE SYSTEM
                                     0000    39  
                                     0000    40 ;++
                                     0000    41 ; RSX$FILEIO - TRAP ALL FILE I/O ROUTINES FOR STAND-ALONE SYSTEM
                                     0000    42 ;
                                     0000    43 ; THIS ROUTINE IS ENTERED WHENEVER AN ATTEMPT IS MADE TO ISSUE A FILES
                                     0000    44 ; RELATED QIO FUNCTION IN THE STAND-ALONE SYSTEM. ALL SUCH REQUESTS ARE
                                     0000    45 ; TERMINATED WITH THE STATUS 'SS$_ILLFUNC'.
                                     0000    46 ;
                                     0000    47 ; INPUTS:
                                     0000    48 ;
                                     0000    49 ;	R2 = ADDRESS OF THE QIO AST CONTEXT BLOCK
                                     0000    50 ;	R3 = MAJOR FUNCTION CODE
                                     0000    51 ;	R5 = ADDRESS OF THE OPERATION DPB
                                     0000    52 ;	R6 = IMAGE STACK
                                     0000    53 ;	R10 = ADDRESS OF THE IMAGE DATA BASE
                                     0000    54 ;	R11 = ADDRESS OF THE IMAGE
                                     0000    55 ;
                                     0000    56 ; OUTPUTS:
                                     0000    57 ;
                                     0000    58 ;	THIS ROUTINE ALWAYS PASSES CONTROL TO "RSX$DLVRQIOAST" WITH
                                     0000    59 ;	A STATUS OF "SS$_ILLFUNC"
                                     0000    60 ;
                                     0000    61 ;	R0 = STATUS OF THE OPERATION
                                     0000    62 ;
                                     0000    63 ;	R2,R10,R11 ARE PRESERVED
                                     0000    64 ;
                                     0000    65 ;--
                                     0000    66  
                                 00000000    67 	.PSECT	RSXCODE	NOWRT
                                     0000    68  
                                     0000    69 RSX$FILEIO::				;
                          FFFD'  31  0000    70 	BRW	RSX$IEIFC		; ALL FILE I/O IS ILLEGAL
