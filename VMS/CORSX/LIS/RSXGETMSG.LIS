                                     0000    78 	.SBTTL RSX$GETMSG
                                     0000    79 ;++
                                     0000    80 ; FUNCTIONAL DESCRIPTION:
                                     0000    81 ;
                                     0000    82 ; THIS PROCEDURE IS CALLED TO SEARCH A LIST OF ERROR MESSAGES AND RETURN
                                     0000    83 ; THE TEXT TO THE CALLER. IT ATTACHES THE APPROPRIATE PREFIX TO THE MESS-
                                     0000    84 ; AGE BASED ON THE SEVERITY CODE.
                                     0000    85 ;
                                     0000    86 ; CALLING SEQUENCE:
                                     0000    87 ;
                                     0000    88 ;	CALL RSX$GETMSG (ERROR,LENGTH,BUFFER)
                                     0000    89 ;
                                     0000    90 ; INPUT PARAMETERS:
                                     0000    91 ;
                                     0000    92 ;	ERROR = LONGWORD CONTAINING THE ERROR CODE IN STANDARD VAX FORMAT
                                     0000    93 ;
                                     0000    94 ;	LENGTH = LONGWORD TO RECEIVE THE MESSAGE BYTE COUNT
                                     0000    95 ;
                                     0000    96 ;	BUFFER = ADDRESS OF BUFFER DESCRIPTOR
                                     0000    97 ;
                                     0000    98 ; IMPLICIT INPUTS:
                                     0000    99 ;
                                     0000   100 ;	RSX$A_MSGTXT = ADDRESS OF MESSAGE LIST
                                     0000   101 ;
                                     0000   102 ;	RSX$A_MSGPRE = ADDRESS OF MESSAGE PREFIX LIST
                                     0000   103 ;
                                     0000   104 ;
                                     0000   105 ; OUTPUT PARAMETERS:
                                     0000   106 ;
                                     0000   107 ;
                                     0000   108 ;	LENGTH = LONGWORD TO RECEIVE THE NUMBER OF BYTES IN THE MESSAGE
                                     0000   109 ;		 STRING.
                                     0000   110 ;
                                     0000   111 ; IMPLICIT OUTPUTS:
                                     0000   112 ;
                                     0000   113 ;	NONE
                                     0000   114 ;
                                     0000   115 ; COMPLETION CODES:
                                     0000   116 ;
                                     0000   117 ;	NONE
                                     0000   118 ;
                                     0000   119 ; SIDE EFFECTS:
                                     0000   120 ;
                                     0000   121 ;	NONE
                                     0000   122 ;
                                     0000   123 ;--
                                     0000   124  
                                 00000000   125 	.PSECT	RSXCODE	NOWRT
                                     0000   126  
                                     0000   127  
                                     0000   128 RSX$GETMSG::				;
                               00FE  0000   129 	.WORD	^M<R1,R2,R3,R4,R5,R6,R7> ; SAVE R1 - R7
                          56     D4  0002   130 	CLRL	R6			; CLEAR LENGTH
            0C            03     EF  0004   131 	EXTZV	#STS$V_CODE,#STS$S_CODE,ERROR(AP),R7 ; EXTRACT MESSAGE NUMBER
            57         04 AC         0007       
                          00     EF  000A   132 	EXTZV	#STS$V_SEVERITY,#STS$S_SEVERITY,- ; EXTRACT SEVERITY FIELD
         04 AC            03         000C   133 		ERROR(AP),R1		;
RSXGETMSG                                                        22-AUG-1978 00:29:31   VAX-11 MACRO X0.3-11               Page   4
00              RSX$GETMSG                                                                                                       (1)

                          51         000F       
            53   00000000'EF     9E  0010   134 	MOVAB	RSX$A_MSGPRE-MSG_SIZE,R3 ; GET ADDRESS OF PREFIXES
                          15     10  0017   135 	BSBB	10$			; GET PREFIX
                          50     D5  0019   136 	TSTL	R0			; BUFFER TOO SMALL?
                          0C     12  001B   137 	BNEQ	5$			; IF NEQ YES, SOME UNMOVED DATA
            51            57     D0  001D   138 	MOVL	R7,R1			; GET MESSAGE CODE
            53   00000000'EF     9E  0020   139 	MOVAB	RSX$A_MSGTXT-MSG_SIZE,R3 ; GET ADDRESS OF MESSAGE LIST
                          05     10  0027   140 	BSBB	10$			; GET MESSAGE STRING
                                     0029   141 5$:					;
         08 BC            56     D0  0029   142 	MOVL	R6,@LENGTH(AP)		; RETURN LENGTH
                                 04  002D   143 	RET				;
                                     002E   144  
                                     002E   145 ;
                                     002E   146 ; SEARCH FOR SPECIFIED ERROR CODE
                                     002E   147 ;
                                     002E   148  
                                     002E   149 10$:					;
            53            00'    C0  002E   150 	ADDL	S^#MSG_SIZE,R3		; POINT TO NEXT BLOCK
                          63     D5  0031   151 	TSTL	(R3)			; AT END OF LIST
                          1F     13  0033   152 	BEQL	20$			; IF EQL YES
         00'A3            51     B1  0035   153 	CMPW	R1,B^MSG_CODE(R3)	; HAVE CODE?
                          F3     12  0039   154 	BNEQ	10$			; IF NEQ NO
            54         0C BC     7D  003B   155 	MOVQ	@BUFFER(AP),R4		; GET BUFFER DESCRIPTOR
            55            56     C0  003F   156 	ADDL	R6,R5			; ADD LENGTH OF STRING MOVED SO FAR
            54            56     A2  0042   157 	SUBW	R6,R4			; SUBTRACT FROM REMAINING BUFFER SIZE
            56         00'A3     A0  0045   158 	ADDW	B^MSG_LENGTH(R3),R6	; UPDATE LENGTH
                       00'A3     2C  0049   159 	MOVC5	B^MSG_LENGTH(R3),@B^MSG_ADDR(R3),- ; MOVE STRING
            00         00'B3         004C   160 		#0,R4,(R5)		;
            65            54         004F       
            56            50     A2  0051   161 	SUBW	R0,R6			; SUBTRACT NUMBER OF UNMOVED BYTES
                                     0054   162 20$:					;
                                 05  0054   163 	RSB				;
                                     0055   164  
                                     0055   165 	.END
