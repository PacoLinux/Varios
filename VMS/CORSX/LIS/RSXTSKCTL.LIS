                                     0029   156 	.SBTTL	RSX$EXTK - RSX11M EXTK$ DIRECTIVE
                                     0029   157 ;++
                                     0029   158 ; RSX$EXTK - RSX11M EXTEND TASK DIRECTIVE
                                     0029   159 ;
                                     0029   160 ; FUNCTIONAL DESCRIPTION:
                                     0029   161 ;
                                     0029   162 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0029   163 ; "EXTK$" DIRECTIVE.
                                     0029   164 ;
                                     0029   165 ; INPUT PARAMETERS:
                                     0029   166 ;
                                     0029   167 ;	R5 = ADDRESS OF DPB+2
                                     0029   168 ;	R6 = CM IMAGE STACK ADDRESS
                                     0029   169 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0029   170 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0029   171 ;
                                     0029   172 ; OUTPUT PARAMETERS:
                                     0029   173 ;
                                     0029   174 ;	R10,R11 ARE PRESERVED
                                     0029   175 ;
                                     0029   176 ; IMPLICIT OUTPUTS:
                                     0029   177 ;
                                     0029   178 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0029   179 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0029   180 ;
                                     0029   181 ; DSW RETURN CODES:
                                     0029   182 ;
                                     0029   183 ;	IS.SUC	SUCCESSFUL
                                     0029   184 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     0029   185 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     0029   186 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     0029   187 ;	IE.ALG	INVALID EXTENSION OR CONTRACTION
                                     0029   188 ;--
                                     0029   189 RSX$EXTK::				; EXTEND TASK DIRECTIVE
            54            6A     9E  0029   190 	MOVAB	I_SCRATCH(R10),R4	; ADDRESS ARGLIST
            55            65     32  002C   191 	CVTWL	(R5),R5			;
            55            06     78  002F   192 	ASHL	#6,R5,R5		; MAKE BYTES
                          55         0032       
                          41     14  0033   193 	BGTR	50$			; IF GTRU THEN EXPAND
                          15     12  0035   194 	BNEQ	10$			; IF NEQ THEN CONTRACT
                                     0037   195 ;
                                     0037   196 ; MAKE INITIAL SIZE
                                     0037   197 ;
            64       00CC CA     D0  0037   198 	MOVL	I_INISIZE(R10),(R4)	; SET UP LOWER LIMIT
         04 A4       00DC CA     D0  003C   199 	MOVL	I_ADR0+ADR_HIGHLIM(R10),4(R4); SET UP UPPER LIMIT
            64         04 A4     C3  0042   200 	SUBL3	4(R4),(R4),R5		; GET DIFFERENCE
                          55         0046       
                          03     19  0047   201 	BLSS	10$			; IF LSS THEN CONTRACT
                          2B     14  0049   202 	BGTR	50$			; IF GTR THEN EXPAND
                                 05  004B   203 	RSB				; IF EQL THEN NO CHANGE
       00DC CA            55     C1  004C   204 10$:	ADDL3	R5,I_ADR0+ADR_HIGHLIM(R10),(R4); SET UP LOW LIMIT BY FUNNY SUBTRACT
                          64         0051       
         04 A4       00DC CA     D0  0052   205 15$:	MOVL	I_ADR0+ADR_HIGHLIM(R10),4(R4);
            55            64     D0  0058   206 	MOVL	(R4),R5			; SAVE BOT AS NEW HIGH SIZE
            55       00C8 CA     B1  005B   207 	CMPW	I_GARD(R10),R5		; DELETE TOO MUCH?
                          42     1A  0060   208 	BGTRU	75$			; IF GTRU THEN ERROR
                          4B     10  0062   209 	BSBB	100$			; CHECK AND ROUND PARAMS
RSXTSKCTL       - RSX AME TASK CONTROL DIRECTIVES                22-AUG-1978 00:27:34   VAX-11 MACRO X0.3-11               Page   6
V04.0           RSX$EXTK - RSX11M EXTK$ DIRECTIVE                                                                                (5)

                                     0064   210 	$DELTVA_S	(R4)		; DELETE THE SPACE
            30            50     E9  0071   211 	BLBC	R0,75$			; IF LOW CLEAR THEN ERROR
                          28     11  0074   212 	BRB	60$			; CONTINUE
            64       00DC CA     D0  0076   213 50$:	MOVL	I_ADR0+ADR_HIGHLIM(R10),(R4); INSERT HIGH LIMIT
            64            55     C1  007B   214 	ADDL3	R5,(R4),4(R4)		;
                       04 A4         007E       
            55         04 A4     D0  0080   215 	MOVL	4(R4),R5		; SAVE TOP AS NEW HIGH LIM
                          29     10  0084   216 	BSBB	100$			; ROUND SIZE
       00D0 CA         04 A4     D1  0086   217 	CMPL	4(R4),I_HIGHSIZE(R10)	; IN RANGE?
                          16     1E  008C   218 	BGEQU	75$			; BR IF ERROR
                                     008E   219 	$CRETVA_S	(R4)
            06            50     E9  009B   220 	BLBC	R0,75$
       00DC CA            55     D0  009E   221 60$:	MOVL	R5,I_ADR0+ADR_HIGHLIM(R10)
                                 05  00A3   222 	RSB
                                     00A4   223 75$:	$DSW	#IE.ALG			; SET ALLOCATION FAILURE
                                     00AF   224 ;
                                     00AF   225 ; SUBROUTINE TO CHECK AND ROUND PARAMETERS
                                     00AF   226 ;
                                     00AF   227 100$:	
            64       01FF 8F     AA  00AF   228 	BICW	#511,(R4)
         04 A4       01FF 8F     AA  00B4   229 	BICW	#511,4(R4)
         04 A4            64     D1  00BA   230 	CMPL	(R4),4(R4)		; IF EQL THEN NO CHANGE NEEDED
                          07     12  00BE   231 	BNEQ	110$
       00DC CA            55     D0  00C0   232 	MOVL	R5,I_ADR0+ADR_HIGHLIM(R10); ASSUME SUCCESS
                          01     BA  00C5   233 	POPR	#^M<R0>
            64       0200 8F     A0  00C7   234 110$:	ADDW	#512,(R4)		; ROUND LOWER LIMIT UP TO NEXT PAGE
                                 05  00CC   235 	RSB
RSXTSKCTL       - RSX AME TASK CONTROL DIRECTIVES                22-AUG-1978 00:27:34   VAX-11 MACRO X0.3-11               Page   7
