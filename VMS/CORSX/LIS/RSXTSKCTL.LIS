                                     01B3   513 	.SBTTL	MAPDSW - MAP DSW RETURN AFTER TASK CONTROL SERIVCE
                                     01B3   514 ;++
                                     01B3   515 ; MAPDSW - MAP THE PROPER DSW RETURN FOR THE TASK CONTROL DIRECTIVES
                                     01B3   516 ;
                                     01B3   517 ; FUNCTIONAL DESCRIPTION:
                                     01B3   518 ;
                                     01B3   519 ; THIS ROUTINE IS ENTERED AFTER A SYSTEM SERVICE TO MAP THE STARLET
                                     01B3   520 ; RETURN CODE INTO A DSW RETURN CODE.
                                     01B3   521 ; IF THE OPERATION WAS NOT SUCCESSFUL THEN THE POSSIBLE ERRORS
                                     01B3   522 ; ARE:
                                     01B3   523 ;
                                     01B3   524 ;	IE.PRI	PRIVILEGE VIOLATION
                                     01B3   525 ;	IE.INS	UNKNOWN	PROCESS
                                     01B3   526 ;
                                     01B3   527 ; INPUTS:
                                     01B3   528 ;
                                     01B3   529 ;	R0 = STATUS TO MAP
                                     01B3   530 ;	R10 = ADDRESS OF IMAGE DATA BASE
                                     01B3   531 ;	R11 = ADDRESS OF THE IMAGE HEADER
                                     01B3   532 ;
                                     01B3   533 ; OUTPUTS:
                                     01B3   534 ;
                                     01B3   535 ;	R10,R11 ARE PRESERVED
                                     01B3   536 ;
                                     01B3   537 ; IMPLICIT OUTPUTS:
                                     01B3   538 ;
                                     01B3   539 ;	THE IMAGE DSW IS SET UP TO REFLECT THE STATUS OF THE
                                     01B3   540 ;	SYSTEM SERVICE. SUCCESSFUL IS IMPLIED.
                                     01B3   541 ;--
                                     01B3   542 MAPDSW:					; MAP DSW RETURN
            41            50     E8  01B3   543 	BLBS	R0,20$			; IF LOW SET THEN SUCCESSFUL
       0000'8F            50     B1  01B6   544 	CMPW	R0,#SS$_NOPRIV		; PRIVILEGE VIOLATION?
                          0B     12  01BB   545 	BNEQ	5$			; IF NEQ THEN NO
                                     01BD   546 	$DSW	#IE.PRI			; SET STATUS AND RETURN
       0000'8F            50     B1  01C8   547 5$:	CMPW	R0,#SS$_INSFMEM		; NO MEMORY?
                          0B     12  01CD   548 	BNEQ	10$			; IF NEQ THEN NO
                                     01CF   549 	$DSW	#IE.UPN			; SET NO MEMORY ERROR
       0000'8F            50     B1  01DA   550 10$:	CMPW	R0,#SS$_EXQUOTA		; QUOTA PROBLEM?
                          0B     12  01DF   551 	BNEQ	15$			; IF NEQ THEN NO
                                     01E1   552 	$DSW	#IE.NOD			; SET QUOTA ERROR
                                     01EC   553 15$:	$DSW	#IE.INS			; SET NO SUCH PROCESS ERROR
                                 05  01F7   554 20$:	RSB				; RETURN
                                     01F8   555 ;
                                     01F8   556 	.END
