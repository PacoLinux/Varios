                                     0018   118 	.SBTTL	RSX$CSRQ - RSX11M CSRQ$ DIRECTIVE
                                     0018   119 ;++
                                     0018   120 ; RSX$CSRQ -  RSX11M CANCEL TIME BASED INITIATION REQUESTS DIRECTIVE
                                     0018   121 ;
                                     0018   122 ; FUNCTIONAL DESCRIPTION:
                                     0018   123 ;
                                     0018   124 ; THIS ROUTINE IMPLEMENTS THE RSX11M CANCEL TIMED INITIATION DIRECTIVE.
                                     0018   125 ; THE SPECIFIED TASK NAME IS CONVERTED TO ASCII FOLLOWED BY A "$CANWAK".
                                     0018   126 ;
                                     0018   127 ; INPUTS:
                                     0018   128 ;
                                     0018   129 ;	R5 = ADDRESS OF THE DPB+2
                                     0018   130 ;	R10 = ADDRESS OF THE IMAGE DATA BASE
                                     0018   131 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0018   132 ;
                                     0018   133 ; OUTPUTS:
                                     0018   134 ;
                                     0018   135 ;	R10,R11 ARE PRESERVED.
                                     0018   136 ;
                                     0018   137 ; IMPLICIT OUTPUTS:
                                     0018   138 ;
                                     0018   139 ;	THE PROPER DIRECTIVE STATUS IS SET IN THE DSW ALONG WITH
                                     0018   140 ;	THE PROPER SETTING OF THE C-BIT.
                                     0018   141 ;
                                     0018   142 ; DSW RETURN CODES:
                                     0018   143 ;
                                     0018   144 ;	IS.SUC	SUCCESSFUL
                                     0018   145 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     0018   146 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     0018   147 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     0018   148 ;	IE.PRI	PRIVILEGE VIOLATION
                                     0018   149 ;	IE.INS	UNKNOWN PROCESS
                                     0018   150 ;--
                                     0018   151 RSX$CSRQ::				; CANCEL TIMED INITIATION
                          FFE5'  30  0018   152 	BSBW	RSX$CVTNAME		; CONVERT R50 TASK NAME TO ASCII
                                     001B   153 	$CANWAK_S	,I_SCRATCH(R10)	; CANCEL WAKEUP FOR PROCESS
                          018A   31  0026   154 	BRW	MAPDSW			; MAP THE PROPER DSW RETURN
