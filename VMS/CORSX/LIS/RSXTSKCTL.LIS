                                     0000    46 ;++
                                     0000    47 ; RSX$ABRT - RSX11M ABORT TASK DIRECTIVE
                                     0000    48 ;
                                     0000    49 ; FUNCTIONAL DESCRIPTION:
                                     0000    50 ;
                                     0000    51 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0000    52 ; "ABRT$" DIRECTIVE. THE SPECIFIED TASK NAME IS CONVERTED TO ASCII
                                     0000    53 ; AND A "$FORCEX" IS DONE TO AFFECT THE DIRECTIVE. 
                                     0000    54 ;
                                     0000    55 ; INPUT PARAMETERS:
                                     0000    56 ;
                                     0000    57 ;	R5 = ADDRESS OF DPB+2
                                     0000    58 ;	R6 = CM IMAGE STACK ADDRESS
                                     0000    59 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0000    60 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0000    61 ;
                                     0000    62 ; OUTPUT PARAMETERS:
                                     0000    63 ;
                                     0000    64 ;	R10,R11 ARE PRESERVED
                                     0000    65 ;
                                     0000    66 ; IMPLICIT OUTPUTS:
                                     0000    67 ;
                                     0000    68 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0000    69 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0000    70 ;
                                     0000    71 ; DSW RETURN CODES:
                                     0000    72 ;
                                     0000    73 ;	IS.SUC	SUCCESSFUL
                                     0000    74 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     0000    75 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     0000    76 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     0000    77 ;	IE.PRI	PRIVILEGE VIOLATION
                                     0000    78 ;	IE.INS	UNKNOWN PROCESS
                                     0000    79 ;--
                                     0000    80 RSX$ABRT::				; ABORT TASK DIRECTIVE
                          FFFD'  30  0000    81 	BSBW	RSX$CVTNAME		; CONVERT R50 NAME TO ASCII
                                     0003    82 	$FORCEX_S	,I_SCRATCH(R10),#SS$_ABORT; FORCE TERMINATION
                          019C   31  0014    83 	BRW	MAPDSW			; MAP THE PROPER DSW RETURN
