                                     00CD   237 	.SBTTL	RSX$RQST - RSX11M RQST$ DIRECTIVE
                                     00CD   238 ;++
                                     00CD   239 ; RSX$RQST - RSX11M REQUEST DIRECTIVE
                                     00CD   240 ;
                                     00CD   241 ; FUNCTIONAL DESCRIPTION:
                                     00CD   242 ;
                                     00CD   243 ; THIS ROUTINE IMPLEMENTS THE RSX11M RQST$ DIRECTIVE.
                                     00CD   244 ; THE SPECIFIED PROCESS NAME IS COMVERTED TO ASCII AND THEN "$WAKE"'D.
                                     00CD   245 ;
                                     00CD   246 ; INPUTS:
                                     00CD   247 ;
                                     00CD   248 ;	R5 = ADDRESS OF DPB+2
                                     00CD   249 ;	R6 = CM IMAGE STACK POINTER
                                     00CD   250 ;	R10 = ADDRESS OF IMAGE DATA
                                     00CD   251 ;	R11 = ADDRESS OF IMAGE HEADER
                                     00CD   252 ;
                                     00CD   253 ; IMPLICIT INPUTS:
                                     00CD   254 ;
                                     00CD   255 ;	THE DPB IS ALREADY VALIDATED FOR READ ACCESS.
                                     00CD   256 ;	THE DPB SIZE AND TYPE ARE CORRECT.
                                     00CD   257 ;
                                     00CD   258 ; OUTPUTS:
                                     00CD   259 ;
                                     00CD   260 ;	R10,R11 ARE PRESERVED.
                                     00CD   261 ;
                                     00CD   262 ; IMPLICIT OUTPUTS:
                                     00CD   263 ;
                                     00CD   264 ;	THE REQUESTED PROCESS IS "$WAKE"'D AND "RSX$MAPDSW" IS ENTERED.
                                     00CD   265 ;
                                     00CD   266 ; DSW RETURN CODES:
                                     00CD   267 ;
                                     00CD   268 ;	IS.SUC	SUCCESSFUL
                                     00CD   269 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     00CD   270 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     00CD   271 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     00CD   272 ;	IE.PRI	PRIVILEGE VIOLATION
                                     00CD   273 ;	IE.INS	UNKNOWN PROCESS
                                     00CD   274 ;--
                                     00CD   275 RSX$RQST::				; REQUEST DIRECTIVE
   B37C66E4 8F            65     D1  00CD   276 	CMPL	(R5),#^X0B37C66E4	; PRT...?
                          11     13  00D4   277 	BEQL	11$			; IF EQL THEN SUCCESS
                          FF27'  30  00D6   278 	BSBW	RSX$CVTNAME		; CONVERT TASK NAME TO ASCII
                                     00D9   279 	$WAKE_S	,I_SCRATCH(R10)		; WAKE PROCESS
                          00CC   31  00E4   280 	BRW	MAPDSW			; MAP RETURN STATUS
                                 05  00E7   281 11$:	RSB
