                                     0236   377 WRITE_MSG4:
                                     0236   378 ;
                                     0236   379 ;++
                                     0236   380 ; *******************************************************
                                     0236   381 ; *
                                     0236   382 ; *	WRITE_MSG4 SUBROUTINE.  THIS ROUTINE FORMATS AND
                                     0236   383 ; *	PRINTS MESSAGES CONTAINING EXPECTED AND RECEIVED
                                     0236   384 ; *	VALUES FOR A DATA ITEM DISCOVERED TO BE IN ERROR
                                     0236   385 ; *	BY THE VERIFY SUBROUTINE.  A MESSAGE CONTAINING
                                     0236   386 ; *	THE APPROXIMATE VALUE OF PC AT THE TIME OF ERROR
                                     0236   387 ; *	IS ALSO FORMATTED AND PRINTED.
                                     0236   388 ; *
                                     0236   389 ; *	INPUTS:
                                     0236   390 ; *		MSG_CTXT:	A BYTE FIELD CONTAINING THE
                                     0236   391 ; *				CONTEXT FOR THE DATA ITEM
                                     0236   392 ; *				IN ERROR.
                                     0236   393 ; *
                                     0236   394 ; *		EXPV:		A QUADWORD FIELD CONTAINING
                                     0236   395 ; *				THE EXPECTED VALUE FOR THE
                                     0236   396 ; *				DATA ITEM IN ERROR.
                                     0236   397 ; *
                                     0236   398 ; *		RECV:		A QUADWORD FIELD CONTAINING
                                     0236   399 ; *				THE RECEIVED VALUE FOR THE
                                     0236   400 ; *				DATA ITEM IN ERROR.
                                     0236   401 ; *
                                     0236   402 ; *		PCV:		A LONGWORD FIELD CONTAINING
                                     0236   403 ; *				THE VALUE OF PC AT OR SOON 
                                     0236   404 ; *				AFTER THE ERROR.
                                     0236   405 ; *
                                     0236   406 ; *	OUTPUTS:
                                     0236   407 ; *		EXPECTED/RECEIVED/PC MESSAGES FORMATTED AND PRINTED.
                                     0236   408 ; *
                                     0236   409 ; **************************************************************
                                     0236   410 ;--
                                     0236   411 ;
   000000E1'EF   000000B9'EF     DE  0236   412 	MOVAL	EXP,MSG_A		; READY "EXPECTED" TEXT FOR FAO
   000000ED'EF   000000F9'EF     DE  0241   413 	MOVAL	EXPV,MSG_D		; GET ADDR OF EXPECTED VALUE READY FOR FAO
         00'8F   000000DC'EF     91  024C   414 	CMPB	MSG_CTXT,#DESC		; IS FAILING DATA ITEM A STRING DESCRIPTOR ?
                          1E     12  0254   415 	BNEQU	10$			; NO -- CONTINUE
                          00AE   30  0256   416 	BSBW	WRITE_MSG4_1		; YES -- FORMAT & WRITE EXPECTED STRING
   000000E1'EF   000000C2'EF     DE  0259   417 	MOVAL	REC,MSG_A		; READY "RECEIVED" TEXT FOR FAO
   000000ED'EF   00000101'EF     DE  0264   418 	MOVAL	RECV,MSG_D		; GET ADDR OF RECEIVED VALUE READY FOR FAO
                          0095   30  026F   419 	BSBW	WRITE_MSG4_1		; FORMAT & WRITE RECEIVED STRING
                          6B     11  0272   420 	BRB	40$			; GO DO PC MESSAGE
                                     0274   421 10$:
         00'8F   000000DC'EF     91  0274   422 	CMPB	MSG_CTXT,#QUAD		; DOES FAILING DATA ITEM HAVE QUAD CONTEXT ?
                          1C     12  027C   423 	BNEQU	20$			; NO -- HANDLE ALL OTHER CONTEXTS
   000000E5'EF            02     9A  027E   424 	MOVZBL	#2,MSG_B		; YES -- INDICATE 2 DATA ITEMS TO FAO
   000000E9'EF            09     9A  0285   425 	MOVZBL	#9,MSG_C		; ... EACH ONE 8 BYTES LONG + A BLANK
   000000ED'EF            04     C1  028C   426 	ADDL3	#4,MSG_D,MSG_E		; GIVE ADDRESS OF 2ND ITEM TO FAO
                 000000F1'EF         0293       
                          1D     11  0298   427 	BRB	30$			; ... AND GO FORMAT IT
                                     029A   428 20$:
   000000E5'EF            01     9A  029A   429 	MOVZBL	#1,MSG_B		; INDICATE JUST 1 DATA ITEM TO FAO
   000000E9'EF   000000DC'EF     9A  02A1   430 	MOVZBL	MSG_CTXT,MSG_C		; GET CONTEXT READY FOR FAO
   000000E9'EF   000000E9'EF     C0  02AC   431 	ADDL2	MSG_C,MSG_C		; ... DOUBLE CONTEXT TO GET PRINTABLE LENGTH
                                     02B7   432 30$:
