                                     0000   119 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   120 ;++
                                     0000   121 ; FUNCTIONAL DESCRIPTION:
                                     0000   122 ;
                                     0000   123 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   124 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   125 ; TEST MODULE EXECUTION.
                                     0000   126 ;
                                     0000   127 ; CALLING SEQUENCE:
                                     0000   128 ;
                                     0000   129 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   130 ;
                                     0000   131 ; INPUT PARAMETERS:
                                     0000   132 ;
                                     0000   133 ;	NONE
                                     0000   134 ;
                                     0000   135 ; IMPLICIT INPUTS:
                                     0000   136 ;
                                     0000   137 ;	NONE
                                     0000   138 ;
                                     0000   139 ; OUTPUT PARAMETERS:
                                     0000   140 ;
                                     0000   141 ;	NONE
                                     0000   142 ;
                                     0000   143 ; IMPLICIT OUTPUTS:
                                     0000   144 ;
                                     0000   145 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   146 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   147 ;
                                     0000   148 ; COMPLETION CODES:
                                     0000   149 ;
                                     0000   150 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   151 ;
                                     0000   152 ; SIDE EFFECTS:
                                     0000   153 ;
                                     0000   154 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   155 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   156 ;
                                     0000   157 ;--
                                     0000   158 
                                     0000   159 
                                     0000   160 
                                     0000   161 TM_SETUP::
                          52     D4  0000   162 	CLRL	R2			; INITIALIZE
                          53     D4  0002   163 	CLRL	R3			; .. CONDITION
                          54     D4  0004   164 	CLRL	R4			; .... TABLE
                          55     D4  0006   165 	CLRL	R5			; ...... INDEX
                          56     D4  0008   166 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   167 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
