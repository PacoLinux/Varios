                                     0000   135 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   136 ;++
                                     0000   137 ; FUNCTIONAL DESCRIPTION:
                                     0000   138 ;
                                     0000   139 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   140 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   141 ; TEST MODULE EXECUTION.
                                     0000   142 ;
                                     0000   143 ; CALLING SEQUENCE:
                                     0000   144 ;
                                     0000   145 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   146 ;
                                     0000   147 ; INPUT PARAMETERS:
                                     0000   148 ;
                                     0000   149 ;	NONE
                                     0000   150 ;
                                     0000   151 ; IMPLICIT INPUTS:
                                     0000   152 ;
                                     0000   153 ;	NONE
                                     0000   154 ;
                                     0000   155 ; OUTPUT PARAMETERS:
                                     0000   156 ;
                                     0000   157 ;	NONE
                                     0000   158 ;
                                     0000   159 ; IMPLICIT OUTPUTS:
                                     0000   160 ;
                                     0000   161 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   162 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   163 ;
                                     0000   164 ; COMPLETION CODES:
                                     0000   165 ;
                                     0000   166 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   167 ;
                                     0000   168 ; SIDE EFFECTS:
                                     0000   169 ;
                                     0000   170 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   171 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   172 ;
                                     0000   173 ;--
                                     0000   174 
                                     0000   175 
                                     0000   176 
                                     0000   177 TM_SETUP::
                          52     D4  0000   178 	CLRL	R2			; INITIALIZE
                          53     D4  0002   179 	CLRL	R3			; .. CONDITION
                          54     D4  0004   180 	CLRL	R4			; .... TABLE
                          55     D4  0006   181 	CLRL	R5			; ...... INDEX
                          56     D4  0008   182 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   183 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
