                                 00000000     5 	.PSECT	RODATA,RD,NOWRT,NOEXE
                                     0000     6 ; *
                                     0000     7 ; *	THIS ROUTINE RUNS IN A DETACHED PROCESS, CREATED BY
                                     0000     8 ; *	A PROCESS RUNNING THE SATSSF07 IMAGE.  THIS PROCESS
                                     0000     9 ; *	MERELY WAITS ON AN EVENT FLAG (DETFLAG) WHICH WILL
                                     0000    10 ; *	BE SET BY THE CREATING PROCESS, AFTER THAT PROCESS
                                     0000    11 ; *	HAS ISSUED A SYSTEM SERVICE WHICH REQUIRES THE EXIST-
                                     0000    12 ; *	ENCE OF THIS PROCESS.
                                     0000    13 ; *
                                     0000    14 DETNAME:	STRING I,<SATSSF07_DET> ; PRCNAM & CLUSTER NAME 
                           00000040  0014    15 DETFLAG		= 64			; EVENT FLAG 64
SATSUT03        SATS SYSTEM SERVICE TESTS                        22-AUG-1978 04:21:37   VAX-11 MACRO X0.3-11               Page   3
SATSUT03        EXECUTABLE CODE                                                                                                  (1)

                                     0014    17 	.SBTTL	EXECUTABLE CODE
                                 00000000    18 	.PSECT	SATSUT03,RD,WRT,EXE
                               0000  0000    19 START:	.WORD	0	; ENTRY MASK
                                     0002    20 SATSUT03:
                                     0002    21 	$SETPRN_S DETNAME	; SET PROCESS NAME FOR CREATOR
                                     000F    22 	$ASCEFC_S EFN=#DETFLAG, NAME=DETNAME ; ASSOCIATE A CLUSTER
                                     0026    23 	$CLREF_S #DETFLAG	; ... CLEAR AN EVENT FLAG
                                     0033    24 	$WAITFR_S #DETFLAG	; ... AND WAIT FOR CREATOR TO ISSUE A SERVICE
                                     0040    25 	$DACEFC_S #DETFLAG	; CREATOR IS FINISHED; DISASSOCIATE CLUSTER
                                 04  004D    26 	RET
                                     004E    27 	.END	START
