1.0             BUILD_CLUST SUBROUTINE                                                                                           (1)

                                     07A5   658 	.SBTTL	BUILD_CLUST SUBROUTINE
                                     07A5   659 ;
                                     07A5   660 ; **************************************************************
                                     07A5   661 ; *
                                     07A5   662 ; *	BUILD_CLUST SUBROUTINE
                                     07A5   663 ; *
                                     07A5   664 ; *	THIS SUBROUTINE CREATES A 32-BIT CLUSTER MASK BY
                                     07A5   665 ; *	  CONCATENATING THE LOW-ORDER BYTES OF REGS R2-R5.
                                     07A5   666 ; *	  IT THEN SETS CLUSTER A EQUAL TO THE MASK BY
                                     07A5   667 ; *	  ISSUING THE PROPER COMBINATION OF 32 SETEF/CLREF'S.
                                     07A5   668 ; *
                                     07A5   669 ; *	INPUTS:
                                     07A5   670 ; *
                                     07A5   671 ; *		R2,R3,R4,R5   -  CONDITION TABLE INDEX VALUES
                                     07A5   672 ; *
                                     07A5   673 ; *		R10      -       ANY EFN IN CLUSTER A
                                     07A5   674 ; *
                                     07A5   675 ; *	OUTPUTS:
                                     07A5   676 ; *
                                     07A5   677 ; *		CLUS_MASK   -    LONGWORD CONTAINING THE CREATED
                                     07A5   678 ; *		                   CLUSTER MASK.
                                     07A5   679 ; *
                                     07A5   680 ; *		SUBJECT CLUSTER - UPDATED TO LOOK LIKE CLUS_MASK.
                                     07A5   681 ; *
                                     07A5   682 ; *	VOLATILE REGISTERS:
                                     07A5   683 ; *
                                     07A5   684 ; *		R0, R1, R8, R9
                                     07A5   685 ; *
                                     07A5   686 ; **************************************************************
                                     07A5   687 ;
                                     07A5   688 BUILD_CLUST:
   000000E3'EF            55     90  07A5   689 	MOVB	R5,CLUS_MASK		; BUILD
   000000E4'EF            54     90  07AC   690 	MOVB	R4,CLUS_MASK+1		; .. CLUSTER
   000000E5'EF            53     90  07B3   691 	MOVB	R3,CLUS_MASK+2		; ...... MASK
   000000E6'EF            52     90  07BA   692 	MOVB	R2,CLUS_MASK+3		; ..............
                                     07C1   693 ;
                                     07C1   694 ; THE FOLLOWING CODE SETS SUBJECT CLUSTER EQUAL TO CLUS_MASK
                                     07C1   695 ;
            58            5A     D0  07C1   696 	MOVL	R10,R8			; ESTABLISH FIRST EFN (EVENT FLAG NO.)
                          59     D4  07C4   697 	CLRL	R9			; INIT OFFSET INTO CLUS_MASK
                                     07C6   698 20$:
   000000E3'EF            59     E0  07C6   699 	BBS	R9,CLUS_MASK,30$ 	; ISSUE $SETEF IF BIT FOR THIS FLAG IS SET
                          3A         07CD       
                                     07CE   700 	$CLREF_S EFN=R8			; ... OTHERWISE, ISSUE $CLREF
            68            50     E8  07D7   701 	BLBS	R0,40$			; IF NORMAL STATUS, PROCESS NEXT EVENT FLAG
                                     07DA   702 	SS_CHECK NORMAL			; USE SS_CHECK TO TERMINATE TEST MODULE
                                     0808   703 30$:
                                     0808   704 	$SETEF_S EFN=R8			; SET CURRENT EVENT FLAG
            2E            50     E8  0811   705 	BLBS	R0,40$			; IF NORMAL STATUS, PROCESS NEXT EVENT FLAG
                                     0814   706 	SS_CHECK NORMAL			; USE SS_CHECK TO TERMINATE TEST MODULE
                                     0842   707 40$:
                          58     B6  0842   708 	INCW	R8			; GET NEXT EFN
            01            1F     9D  0844   709 	ACBB	#31,#1,R9,20$		; GO DO NEXT EVENT FLAG
            FF7C          59         0847       
                                 05  084A   710 	RSB				; RETURN TO CALLER
