1.0             READ_DACEFC SUBROUTINE                                                                                           (1)

                                     084B   712 	.SBTTL	READ_DACEFC SUBROUTINE
                                     084B   713 ;
                                     084B   714 ; **************************************************************
                                     084B   715 ; *
                                     084B   716 ; *	READ_DACEFC SUBROUTINE
                                     084B   717 ; *
                                     084B   718 ; *	THIS SUBROUTINE COMPARES THE CLUSTER AGAINST THE MASK
                                     084B   719 ; *	  AND THEN DISASSOCIATES THE EVENT FLAG GROUP SPECIFIED
                                     084B   720 ; *	  BY R10 FROM THE CLUSTER.
                                     084B   721 ; *
                                     084B   722 ; *	INPUTS:
                                     084B   723 ; *
                                     084B   724 ; *		R10      -       ANY EFN IN THE EVENT FLAG GROUP
                                     084B   725 ; *				   TO BE DISASSOCIATED.
                                     084B   726 ; *
                                     084B   727 ; *		CLUS_MASK   -    LONGWORD CONTAINING THE IMAGE
                                     084B   728 ; *				   OF THE SUBJECT CLUSTER.
                                     084B   729 ; *	OUTPUTS:
                                     084B   730 ; *
                                     084B   731 ; *		NONE.
                                     084B   732 ; *
                                     084B   733 ; *	EXITS:
                                     084B   734 ; *
                                     084B   735 ; *		NORMAL     -     RSB TO CALLER IF CLUSTER EQUALS
                                     084B   736 ; *				   MASK AND SYSTEM SERVICES
                                     084B   737 ; *				   FINISH NORMALLY.
                                     084B   738 ; *
                                     084B   739 ; *		ERROR      -     ERR_EXIT MACRO EXECUTED IF ABOVE
                                     084B   740 ; *				   NORMAL CONDITIONS ARE NOT MET.
                                     084B   741 ; *
                                     084B   742 ; *	VOLATILE REGISTERS, AREAS:
                                     084B   743 ; *
                                     084B   744 ; *		R0, R1, R8, R9, CLUS_STATE, EXPV, RECV
                                     084B   745 ; *
                                     084B   746 ; **************************************************************
                                     084B   747 ;
                                     084B   748 READ_DACEFC:
                                     084B   749 	$READEF_S EFN=R10, STATE=CLUS_STATE ; READ CLUSTER
            2E            50     E8  085A   750 	BLBS	R0,10$			; CONTINUE IF NORMAL COMPLETION
                                     085D   751 	SS_CHECK NORMAL			; USE SS_CHECK TO TERMINATE TEST MODULE
                                     088B   752 10$:
   000000E3'EF   000000E7'EF     D1  088B   753 	CMPL	CLUS_STATE,CLUS_MASK 	; DOES CLUSTER STILL = MASK ?
                          6F     13  0896   754 	BEQLU	20$			; YES -- ISSUE DACEFC AND GET OUT
   00000000'EF   000000E3'EF     D0  0898   755 	MOVL	CLUS_MASK,EXPV		; NO -- LOAD EXPECTED AND ...
   00000000'EF   000000E7'EF     D0  08A3   756 	MOVL	CLUS_STATE,RECV		; ... RECEIVED VALUES, THEN EXIT
                                     08AE   757 	ERR_EXIT LONG,<CLUSTER STATE NOT MAINTAINED ACROSS DLCEFC OR DACEFC>
                                     0907   758 20$:
                                     0907   759 	$DACEFC_S EFN=R10		; DISASSOCIATE (DECR REF CT BY 1)
                                     0910   760 	SS_CHECK NORMAL			; CHECK FOR NORMAL COMPLETION
                                     093E   761 READ_DACEFCX:
                                 05  093E   762 	RSB				; RETURN TO CALLER
                                     093F   763 	.END
