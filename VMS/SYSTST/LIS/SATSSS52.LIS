                                     0172   190 	.SBTTL	CONDITION SUBROUTINES - SETUP AND CLEANUP 
                                     0172   191 ;++
                                     0172   192 ; FUNCTIONAL DESCRIPTION:
                                     0172   193 ;
                                     0172   194 ;		CONDX AND CONDX_CLEANUP ARE SUBROUTINES WHICH ARE EXECUTED
                                     0172   195 ; BEFORE AND AFTER THE VERIFY SUBROUTINE, RESPECTIVELY, WHENEVER A NEW
                                     0172   196 ; CONDITION X VALUE IS SELECTED (SEE FUNCTIONAL DESCRIPTION OF SUCCOMMON
                                     0172   197 ; ROUTINE IN SUCCOMMON.MAR). ANY SETUP FUNCTION PARTICULAR TO THE 
                                     0172   198 ; CONDITION X TABLE IS INCLUDED IN THE CONDX SUBROUTINE AND CLEANED
                                     0172   199 ; UP, IF NECESSARY, IN THE CONDX_CLEANUP SUBROUTINE. THIS INCLUDES,
                                     0172   200 ; ESPECIALLY, CODE TO DETECT CONFLICTS AMONG CURRENT ENTRIES IN TWO
                                     0172   201 ; OR MORE CONDITION TABLES. IF A CONFLICT IS DETECTED, A NON-ZERO
                                     0172   202 ; VALUE IS STORED INTO CONFLICT, WHICH CAUSES THE CALLING ROUTINE
                                     0172   203 ; (SUCCOMMON) TO SKIP THE CURRENT ENTRY IN THE CONDITION X TABLE.
                                     0172   204 ;
                                     0172   205 ; CALLING SEQUENCE:
                                     0172   206 ;
                                     0172   207 ;	BSBW CONDX   BSBW CONDX_CLEANUP
                                     0172   208 ;	  WHERE X = 1,2,3,4,5
                                     0172   209 ;
                                     0172   210 ; INPUT PARAMETERS:
                                     0172   211 ;
                                     0172   212 ;	CONFLICT = 0
                                     0172   213 ;
                                     0172   214 ; IMPLICIT INPUTS:
                                     0172   215 ;
                                     0172   216 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     0172   217 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     0172   218 ;
                                     0172   219 ; OUTPUT PARAMETERS:
                                     0172   220 ;
                                     0172   221 ;	CONFLICT SET TO NON-ZERO IF COND TABLE CONFLICT DETECTED.
                                     0172   222 ;
                                     0172   223 ; IMPLICIT OUTPUTS:
                                     0172   224 ;
                                     0172   225 ;	R2,3,4,5,6 PRESERVED
                                     0172   226 ;
                                     0172   227 ; COMPLETION CODES:
                                     0172   228 ;
                                     0172   229 ;	NONE
                                     0172   230 ;
                                     0172   231 ; SIDE EFFECTS:
                                     0172   232 ;
                                     0172   233 ;	NONE
                                     0172   234 ;
                                     0172   235 ;--
                                     0172   236 
                                     0172   237 
                                     0172   238 
                                     0172   239 COND1::
                                 05  0172   240 	RSB				; RETURN TO MAIN ROUTINE
                                     0173   241 COND1_CLEANUP::
                                 05  0173   242 	RSB				; RETURN TO MAIN ROUTINE
                                     0174   243 COND2::
                                 05  0174   244 	RSB				; RETURN TO MAIN ROUTINE
                                     0175   245 COND2_CLEANUP::
                                 05  0175   246 	RSB				; RETURN TO MAIN ROUTINE
