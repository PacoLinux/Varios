                                     00C0   157 	.SBTTL	CONDITION SUBROUTINES - SETUP AND CLEANUP 
                                     00C0   158 ;++
                                     00C0   159 ; FUNCTIONAL DESCRIPTION:
                                     00C0   160 ;
                                     00C0   161 ;		CONDX AND CONDX_CLEANUP ARE SUBROUTINES WHICH ARE EXECUTED
                                     00C0   162 ; BEFORE AND AFTER THE VERIFY SUBROUTINE, RESPECTIVELY, WHENEVER A NEW
                                     00C0   163 ; CONDITION X VALUE IS SELECTED (SEE FUNCTIONAL DESCRIPTION OF SUCCOMMON
                                     00C0   164 ; ROUTINE IN SUCCOMMON.MAR). ANY SETUP FUNCTION PARTICULAR TO THE 
                                     00C0   165 ; CONDITION X TABLE IS INCLUDED IN THE CONDX SUBROUTINE AND CLEANED
                                     00C0   166 ; UP, IF NECESSARY, IN THE CONDX_CLEANUP SUBROUTINE. THIS INCLUDES,
                                     00C0   167 ; ESPECIALLY, CODE TO DETECT CONFLICTS AMONG CURRENT ENTRIES IN TWO
                                     00C0   168 ; OR MORE CONDITION TABLES. IF A CONFLICT IS DETECTED, A NON-ZERO
                                     00C0   169 ; VALUE IS STORED INTO CONFLICT, WHICH CAUSES THE CALLING ROUTINE
                                     00C0   170 ; (SUCCOMMON) TO SKIP THE CURRENT ENTRY IN THE CONDITION X TABLE.
                                     00C0   171 ;
                                     00C0   172 ; CALLING SEQUENCE:
                                     00C0   173 ;
                                     00C0   174 ;	BSBW CONDX   BSBW CONDX_CLEANUP
                                     00C0   175 ;	  WHERE X = 1,2,3,4,5
                                     00C0   176 ;
                                     00C0   177 ; INPUT PARAMETERS:
                                     00C0   178 ;
                                     00C0   179 ;	CONFLICT = 0
                                     00C0   180 ;
                                     00C0   181 ; IMPLICIT INPUTS:
                                     00C0   182 ;
                                     00C0   183 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     00C0   184 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     00C0   185 ;
                                     00C0   186 ; OUTPUT PARAMETERS:
                                     00C0   187 ;
                                     00C0   188 ;	CONFLICT SET TO NON-ZERO IF COND TABLE CONFLICT DETECTED.
                                     00C0   189 ;
                                     00C0   190 ; IMPLICIT OUTPUTS:
                                     00C0   191 ;
                                     00C0   192 ;	R2,3,4,5,6 PRESERVED
                                     00C0   193 ;
                                     00C0   194 ; COMPLETION CODES:
                                     00C0   195 ;
                                     00C0   196 ;	NONE
                                     00C0   197 ;
                                     00C0   198 ; SIDE EFFECTS:
                                     00C0   199 ;
                                     00C0   200 ;	NONE
                                     00C0   201 ;
                                     00C0   202 ;--
                                     00C0   203 
                                     00C0   204 
                                     00C0   205 
                                     00C0   206 COND1::
                                 05  00C0   207 	RSB				; RETURN TO MAIN ROUTINE
                                     00C1   208 COND1_CLEANUP::
                                 05  00C1   209 	RSB				; RETURN TO MAIN ROUTINE
                                     00C2   210 COND2::
                                 05  00C2   211 	RSB				; RETURN TO MAIN ROUTINE
                                     00C3   212 COND2_CLEANUP::
                                 05  00C3   213 	RSB				; RETURN TO MAIN ROUTINE
