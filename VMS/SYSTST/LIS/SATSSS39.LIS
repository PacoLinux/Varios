                                     0000    94 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000    95 ;++
                                     0000    96 ; FUNCTIONAL DESCRIPTION:
                                     0000    97 ;
                                     0000    98 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000    99 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   100 ; TEST MODULE EXECUTION.
                                     0000   101 ;
                                     0000   102 ; CALLING SEQUENCE:
                                     0000   103 ;
                                     0000   104 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   105 ;
                                     0000   106 ; INPUT PARAMETERS:
                                     0000   107 ;
                                     0000   108 ;	NONE
                                     0000   109 ;
                                     0000   110 ; IMPLICIT INPUTS:
                                     0000   111 ;
                                     0000   112 ;	NONE
                                     0000   113 ;
                                     0000   114 ; OUTPUT PARAMETERS:
                                     0000   115 ;
                                     0000   116 ;	NONE
                                     0000   117 ;
                                     0000   118 ; IMPLICIT OUTPUTS:
                                     0000   119 ;
                                     0000   120 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   121 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   122 ;
                                     0000   123 ; COMPLETION CODES:
                                     0000   124 ;
                                     0000   125 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   126 ;
                                     0000   127 ; SIDE EFFECTS:
                                     0000   128 ;
                                     0000   129 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   130 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   131 ;
                                     0000   132 ;--
                                     0000   133 
                                     0000   134 
                                     0000   135 
                                     0000   136 TM_SETUP::
                          52     D4  0000   137 	CLRL	R2			; INITIALIZE
                          53     D4  0002   138 	CLRL	R3			; .. CONDITION
                          54     D4  0004   139 	CLRL	R4			; .... TABLE
                          55     D4  0006   140 	CLRL	R5			; ...... INDEX
                          56     D4  0008   141 	CLRL	R6			; ........ REGISTERS
