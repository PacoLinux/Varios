                                 00000000     6 	.PSECT	RWDATA,RD,WRT,NOEXE
                                     0000     7 ;
                                     0000     8 ; THIS IMAGE RUNS IN A PROCESS CREATED BY THE SATSSS61 PROCESS
                                     0000     9 ; FOR THE PURPOSE OF TESTING THE $SCHDWK AND $CANWAK SYSTEM 
                                     0000    10 ; SERVICES. ITS BASIC JOB IS TO HIBERNATE. ADDITIONALLY, IT
                                     0000    11 ; INDICATES TO THE CREATOR WHEN IT HAS FALLEN THRU HIBERNATION
                                     0000    12 ; BY SETTING EVENT FLAG 64; IT ALSO WAITS ON EVENT FLAG 65
                                     0000    13 ; BEFORE EXITING (THIS IS REQUIRED TO KEEP THIS PROCESS FROM
                                     0000    14 ; DISAPPEARING WHILE THE CREATOR IS STILL REFERENCING IT
                                     0000    15 ; WITH SYSTEM SERVICES). WHEN THE CREATOR IS FINISHED WITH
                                     0000    16 ; THIS PROCESS, IT SETS EVENT FLAG 65 TO ALLOW IMAGE EXIT
                                     0000    17 ; AND PROCESS DELETION TO OCCUR. AFTER SETTING THE FLAG,
                                     0000    18 ; THE CREATOR READS THIS PROCESS' MAILBOX TO ENSURE THAT
                                     0000    19 ; IT IS, INDEED, DELETED.
                                     0000    20 ;
                                     0000    21 JPILIST:				; $GETJPI ITEM LIST
                               0004  0000    22 		.WORD	4		; PID BUFFER LENGTH
                               0319  0002    23 		.WORD	JPI$_PID	; JPI ITEM CODE FOR PID
                           00000010' 0004    24 		.ADDRESS CREPID		; ADDRESS OF PID BUFFER
                  00000000 00000000  0008    25 		.LONG	0,0		; NEGATE LENGTH RETURN + END OF LIST
                           00000014  0010    26 CREPID:		.BLKL	1		; PID FOR THIS (CREATED) PROCESS
                           00000004  0014    27 CLUSTER:	.LONG	4		; STRING DESCRIPTOR FOR ...
                           00000010' 0018    28 		.ADDRESS CREPID		; ... EVENT FLAG CLUSTER
SATSUT07        SATS SYSTEM SERVICE TESTS                        22-AUG-1978 04:21:59   VAX-11 MACRO X0.3-11               Page   3
1.0             EXECUTABLE CODE                                                                                                  (1)

                                     001C    30 	.SBTTL	EXECUTABLE CODE
                                 00000000    31 	.PSECT	SATSUT07,RD,WRT,EXE
                               0000  0000    32 START:	.WORD	0			; ENTRY MASK
                                     0002    33 SATSUT07:
                                     0002    34 	$GETJPI_S ITMLST=JPILIST	; GET PID FOR USE AS CLUSTER NAME
                                     0019    35 	$ASCEFC_S EFN=#64, NAME=CLUSTER	; ASSOC CLUSTER FOR COMMUNICATION W CREATOR
                                     0030    36 	$HIBER_S			; HIBERNATE ... EXPECT SCHEDULED WAKES
                                     0037    37 	$SETEF_S  EFN=#64		; LET CREATOR KNOW THAT HIBER IS SATISFIED
                                     0044    38 	$WAITFR_S EFN=#65		; HANG AROUND WHILE CREATOR MAKES REFERENCES
                                     0051    39 	$DACEFC_S EFN=#64		; CREATOR IS FINISHED ... CLEAN UP CLUSTER
                                 04  005E    40 	RET				; EXIT IMAGE
                                     005F    41 	.END	START
