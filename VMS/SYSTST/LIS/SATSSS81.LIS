                                     0000   111 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   112 ;++
                                     0000   113 ; FUNCTIONAL DESCRIPTION:
                                     0000   114 ;
                                     0000   115 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   116 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   117 ; TEST MODULE EXECUTION.
                                     0000   118 ;
                                     0000   119 ; CALLING SEQUENCE:
                                     0000   120 ;
                                     0000   121 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   122 ;
                                     0000   123 ; INPUT PARAMETERS:
                                     0000   124 ;
                                     0000   125 ;	NONE
                                     0000   126 ;
                                     0000   127 ; IMPLICIT INPUTS:
                                     0000   128 ;
                                     0000   129 ;	NONE
                                     0000   130 ;
                                     0000   131 ; OUTPUT PARAMETERS:
                                     0000   132 ;
                                     0000   133 ;	NONE
                                     0000   134 ;
                                     0000   135 ; IMPLICIT OUTPUTS:
                                     0000   136 ;
                                     0000   137 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   138 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   139 ;
                                     0000   140 ; COMPLETION CODES:
                                     0000   141 ;
                                     0000   142 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   143 ;
                                     0000   144 ; SIDE EFFECTS:
                                     0000   145 ;
                                     0000   146 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   147 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   148 ;
                                     0000   149 ;--
                                     0000   150 
                                     0000   151 
                                     0000   152 
                                     0000   153 TM_SETUP::
                          52     D4  0000   154 	CLRL	R2			; INITIALIZE
                          53     D4  0002   155 	CLRL	R3			; .. CONDITION
                          54     D4  0004   156 	CLRL	R4			; .... TABLE
                          55     D4  0006   157 	CLRL	R5			; ...... INDEX
                          56     D4  0008   158 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   159 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
   00000000'EF   00000000'EF     DE  000D   160 	MOVAL	TEST_MOD_SUCC,TMD_ADDR	; ASSUME END MSG WILL SHOW SUCCESS
