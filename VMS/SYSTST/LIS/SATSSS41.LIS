                                     0400   476 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     0400   477 ;	FOR X = 1,2,3,4,5 :
                                     0400   478 ;		CONDX_E - ADDRESS OF TABLE OF DATA VALUES FOR CONDX
                                     0400   479 ;		  TABLE. IF THE CONTEXT OF TABLE X IS A SYSTEM SERVICE
                                     0400   480 ;		  ARGUMENT, THE ARGUMENT NAME MAY BE USED AS A SYNONYM
                                     0400   481 ;		  FOR CONDX_E.
                                     0400   482 ;
                                     0400   483 ; OUTPUT PARAMETERS:
                                     0400   484 ;
                                     0400   485 ;	NONE
                                     0400   486 ;
                                     0400   487 ; IMPLICIT OUTPUTS:
                                     0400   488 ;
                                     0400   489 ;	NONE
                                     0400   490 ;
                                     0400   491 ; COMPLETION CODES:
                                     0400   492 ;
                                     0400   493 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0400   494 ;
                                     0400   495 ; SIDE EFFECTS:
                                     0400   496 ;
                                     0400   497 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0400   498 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0400   499 ;
