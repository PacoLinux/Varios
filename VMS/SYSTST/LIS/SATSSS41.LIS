                          52     D4  0000   161 	CLRL	R2			; INITIALIZE
                          53     D4  0002   162 	CLRL	R3			; .. CONDITION
                          54     D4  0004   163 	CLRL	R4			; .... TABLE
                          55     D4  0006   164 	CLRL	R5			; ...... INDEX
                          56     D4  0008   165 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   166 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
