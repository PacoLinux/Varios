                                     0000   104 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   105 ;++
                                     0000   106 ; FUNCTIONAL DESCRIPTION:
                                     0000   107 ;
                                     0000   108 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   109 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   110 ; TEST MODULE EXECUTION.
                                     0000   111 ;
                                     0000   112 ; CALLING SEQUENCE:
                                     0000   113 ;
                                     0000   114 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   115 ;
                                     0000   116 ; INPUT PARAMETERS:
                                     0000   117 ;
                                     0000   118 ;	NONE
                                     0000   119 ;
                                     0000   120 ; IMPLICIT INPUTS:
                                     0000   121 ;
                                     0000   122 ;	NONE
                                     0000   123 ;
                                     0000   124 ; OUTPUT PARAMETERS:
                                     0000   125 ;
                                     0000   126 ;	NONE
                                     0000   127 ;
                                     0000   128 ; IMPLICIT OUTPUTS:
                                     0000   129 ;
                                     0000   130 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   131 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   132 ;
                                     0000   133 ; COMPLETION CODES:
                                     0000   134 ;
                                     0000   135 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   136 ;
                                     0000   137 ; SIDE EFFECTS:
                                     0000   138 ;
                                     0000   139 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   140 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   141 ;
                                     0000   142 ;--
                                     0000   143 
                                     0000   144 
                                     0000   145 
                                     0000   146 TM_SETUP::
                          52     D4  0000   147 	CLRL	R2			; INITIALIZE
                          53     D4  0002   148 	CLRL	R3			; .. CONDITION
                          54     D4  0004   149 	CLRL	R4			; .... TABLE
                          55     D4  0006   150 	CLRL	R5			; ...... INDEX
                          56     D4  0008   151 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   152 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
