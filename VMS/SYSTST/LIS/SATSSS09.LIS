                                     0000    92 ;++
                                     0000    93 ; FUNCTIONAL DESCRIPTION:
                                     0000    94 ;
                                     0000    95 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000    96 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000    97 ; TEST MODULE EXECUTION.
                                     0000    98 ;
                                     0000    99 ; CALLING SEQUENCE:
                                     0000   100 ;
                                     0000   101 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   102 ;
                                     0000   103 ; INPUT PARAMETERS:
                                     0000   104 ;
                                     0000   105 ;	NONE
                                     0000   106 ;
                                     0000   107 ; IMPLICIT INPUTS:
                                     0000   108 ;
                                     0000   109 ;	NONE
                                     0000   110 ;
                                     0000   111 ; OUTPUT PARAMETERS:
                                     0000   112 ;
                                     0000   113 ;	NONE
                                     0000   114 ;
                                     0000   115 ; IMPLICIT OUTPUTS:
                                     0000   116 ;
                                     0000   117 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   118 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   119 ;
                                     0000   120 ; COMPLETION CODES:
                                     0000   121 ;
                                     0000   122 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   123 ;
                                     0000   124 ; SIDE EFFECTS:
                                     0000   125 ;
                                     0000   126 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   127 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   128 ;
                                     0000   129 ;--
                                     0000   130 
                                     0000   131 
                                     0000   132 
