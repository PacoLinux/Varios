                                     00C0   154 	.SBTTL	CONDITION SUBROUTINES - SETUP AND CLEANUP 
                                     00C0   155 ;++
                                     00C0   156 ; FUNCTIONAL DESCRIPTION:
                                     00C0   157 ;
                                     00C0   158 ;		CONDX AND CONDX_CLEANUP ARE SUBROUTINES WHICH ARE EXECUTED
                                     00C0   159 ; BEFORE AND AFTER THE VERIFY SUBROUTINE, RESPECTIVELY, WHENEVER A NEW
                                     00C0   160 ; CONDITION X VALUE IS SELECTED (SEE FUNCTIONAL DESCRIPTION OF SUCCOMMON
                                     00C0   161 ; ROUTINE IN SUCCOMMON.MAR). ANY SETUP FUNCTION PARTICULAR TO THE 
                                     00C0   162 ; CONDITION X TABLE IS INCLUDED IN THE CONDX SUBROUTINE AND CLEANED
                                     00C0   163 ; UP, IF NECESSARY, IN THE CONDX_CLEANUP SUBROUTINE. THIS INCLUDES,
                                     00C0   164 ; ESPECIALLY, CODE TO DETECT CONFLICTS AMONG CURRENT ENTRIES IN TWO
                                     00C0   165 ; OR MORE CONDITION TABLES. IF A CONFLICT IS DETECTED, A NON-ZERO
                                     00C0   166 ; VALUE IS STORED INTO CONFLICT, WHICH CAUSES THE CALLING ROUTINE
                                     00C0   167 ; (SUCCOMMON) TO SKIP THE CURRENT ENTRY IN THE CONDITION X TABLE.
                                     00C0   168 ;
                                     00C0   169 ; CALLING SEQUENCE:
                                     00C0   170 ;
                                     00C0   171 ;	BSBW CONDX   BSBW CONDX_CLEANUP
                                     00C0   172 ;	  WHERE X = 1,2,3,4,5
                                     00C0   173 ;
                                     00C0   174 ; INPUT PARAMETERS:
                                     00C0   175 ;
                                     00C0   176 ;	CONFLICT = 0
                                     00C0   177 ;
                                     00C0   178 ; IMPLICIT INPUTS:
                                     00C0   179 ;
                                     00C0   180 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     00C0   181 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     00C0   182 ;
                                     00C0   183 ; OUTPUT PARAMETERS:
                                     00C0   184 ;
                                     00C0   185 ;	CONFLICT SET TO NON-ZERO IF COND TABLE CONFLICT DETECTED.
                                     00C0   186 ;
                                     00C0   187 ; IMPLICIT OUTPUTS:
                                     00C0   188 ;
                                     00C0   189 ;	R2,3,4,5,6 PRESERVED
                                     00C0   190 ;
                                     00C0   191 ; COMPLETION CODES:
                                     00C0   192 ;
                                     00C0   193 ;	NONE
                                     00C0   194 ;
                                     00C0   195 ; SIDE EFFECTS:
                                     00C0   196 ;
                                     00C0   197 ;	NONE
                                     00C0   198 ;
                                     00C0   199 ;--
                                     00C0   200 
                                     00C0   201 
                                     00C0   202 
