                                     04CF   502 ;++
                                     04CF   503 ; FUNCTIONAL DESCRIPTION:
                                     04CF   504 ;
                                     04CF   505 ;		VFY_CLEANUP EXECUTES SYSTEM SERVICES TO UNDO THE
                                     04CF   506 ; EFFECT OF THOSE ISSUED IN THE VERIFY SUBROUTINE. VFY_CLEANUP MUST
                                     04CF   507 ; ASSUME THAT VERIFY MAY NOT HAVE EXECUTED IN ITS ENTIRETY (IF AN
                                     04CF   508 ; ERROR IS FOUND). ALSO, VFY_CLEANUP MAY ISSUE SS_CHECK OR ERR_EXIT
                                     04CF   509 ; ONLY AFTER PERFORMING ALL OF ITS CLEANUP OPERATIONS; THIS IS REQUIRED
                                     04CF   510 ; IN THE EVENT THAT VFY_CLEANUP IS CALLED DURING ERROR PROCESSING,
                                     04CF   511 ; WHEN PERFORMING THE REQUIRED CLEANUP IS MORE IMPORTANT THAN
                                     04CF   512 ; POSSIBLY DISCOVERING A SECOND ERROR.
                                     04CF   513 ;
                                     04CF   514 ; CALLING SEQUENCE:
                                     04CF   515 ;
                                     04CF   516 ;	BSBW VFY_CLEANUP
                                     04CF   517 ;
                                     04CF   518 ; INPUT PARAMETERS:
                                     04CF   519 ;
                                     04CF   520 ;	NONE
                                     04CF   521 ;
                                     04CF   522 ; IMPLICIT INPUTS:
                                     04CF   523 ;
                                     04CF   524 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     04CF   525 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
