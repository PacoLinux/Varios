                                     04CF   527 ;		CONDX_E - ADDRESS OF TABLE OF DATA VALUES FOR CONDX
                                     04CF   528 ;		  TABLE. IF THE CONTEXT OF TABLE X IS A SYSTEM SERVICE
                                     04CF   529 ;		  ARGUMENT, THE ARGUMENT NAME MAY BE USED AS A SYNONYM
                                     04CF   530 ;		  FOR CONDX_E.
                                     04CF   531 ;
                                     04CF   532 ; OUTPUT PARAMETERS:
                                     04CF   533 ;
                                     04CF   534 ;	NONE
                                     04CF   535 ;
                                     04CF   536 ; IMPLICIT OUTPUTS:
                                     04CF   537 ;
                                     04CF   538 ;	NONE
                                     04CF   539 ;
                                     04CF   540 ; COMPLETION CODES:
                                     04CF   541 ;
                                     04CF   542 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     04CF   543 ;
