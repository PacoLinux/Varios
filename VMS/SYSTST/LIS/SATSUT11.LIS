                                 00000000     5 	.PSECT	RWDATA,RD,WRT,NOEXE
                                     0000     6 ; *
                                     0000     7 ; *	THIS ROUTINE RUNS IN A PROCESS CREATED BY
                                     0000     8 ; *	A PROCESS RUNNING THE SATSSS56 IMAGE. ITS 
                                     0000     9 ; *	NORMAL STATE IS HIBERNATION. WHEN AWAKENED
                                     0000    10 ; *	BY THE CREATING PROCESS, IT SETS ALL EVENT
                                     0000    11 ; *	FLAGS IN THE CLUSTER SATSSS56_CLUS; THEN
                                     0000    12 ; *	IT SIGNALS COMPLETION OF THIS TASK BY
                                     0000    13 ; *	WRITING A MAILBOX MESSAGE TO THE CREATOR;
                                     0000    14 ; *	FINALLY, IT RETURNS TO HIBERNATION,
                                     0000    15 ; *	AWAITING FURTHER REQUESTS.
                                     0000    16 ; *
                                     0000    17 CLUS_NAME:	STRING	I,<SATSSS56_CLUS> ; NAME FOR SUBJECT CLUSTER
                                     0015    18 CREATOR:	STRING	I,<SATSSS56>	; NAME OF CREATING PROCESS
                                     0025    19 MBXBUFF:	STRING	O,1		; MAILBOX BUFFER
                           00000030  002E    20 MBXCHAN:	.BLKW	1		; MAILBOX CHANNEL NUMBER
                                     0030    21 MBXNAME:	STRING	I,<SATSSS56_CRE> ; MAILBOX NAME
SATSUT11        SATS SYSTEM SERVICE TESTS                        22-AUG-1978 04:22:27   VAX-11 MACRO X0.3-11               Page   3
SATSUT11        EXECUTABLE CODE                                                                                                  (1)

                                     0044    23 	.SBTTL	EXECUTABLE CODE
                                 00000000    24 	.PSECT	SATSUT11,RD,WRT,EXE
                               0000  0000    25 START:	.WORD	0		; ENTRY MASK
                                     0002    26 SATSUT11:
                                     0002    27 	$ASSIGN_S DEVNAM=MBXNAME, CHAN=MBXCHAN
                                     0017    28 		  		; ASSIGN MAILBOX TO TALK TO CREATOR
                                     0017    29 10$:
                                     0017    30 	$HIBER_S		; SLEEP UNTIL CREATOR HAS WORK
            58         40 8F     9A  001E    31 	MOVZBL	#64,R8		; INIT R8 TO 1ST EFN IN CLUSTER
                                     0022    32 	$ASCEFC_S EFN=R8, NAME=CLUS_NAME ; ASSOC. WITH CREATOR'S SUBJECT CLUSTER
                                     0035    33 20$:
                                     0035    34 	$SETEF_S EFN=R8		; SET A FLAG IN THE CLUSTER
            01       005F 8F     3D  003E    35 	ACBW	#95,#1,R8,20$	; GET NEXT FLAG AND LOOP
            FFEF          58         0043       
                                     0046    36 	$DACEFC_S EFN=#64	; FLAGS ALL SET; DISASSOCIATE
                                     0053    37 	$QIOW_S	CHAN=MBXCHAN, FUNC=#IO$_WRITEVBLK, -
                                     0053    38 		P1=MBXBUFF+8, P2=MBXBUFF ; SIGNAL CREATOR THAT WORK IS DONE
                          FF98   31  007C    39 	BRW	10$		; GO SLEEP UNTIL CREATOR CALLS AGAIN
                                 04  007F    40 	RET			; RETURN TO SYSTEM
                                     0080    41 	.END	START
