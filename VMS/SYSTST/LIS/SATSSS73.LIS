                                     05C2   549 ;		  ARGUMENT, THE ARGUMENT NAME MAY BE USED AS A SYNONYM
                                     05C2   550 ;		  FOR CONDX_E.
                                     05C2   551 ;
                                     05C2   552 ; OUTPUT PARAMETERS:
                                     05C2   553 ;
                                     05C2   554 ;	NONE
                                     05C2   555 ;
                                     05C2   556 ; IMPLICIT OUTPUTS:
