                                     0000   129 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   130 ;++
                                     0000   131 ; FUNCTIONAL DESCRIPTION:
                                     0000   132 ;
                                     0000   133 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   134 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   135 ; TEST MODULE EXECUTION.
                                     0000   136 ;
                                     0000   137 ; CALLING SEQUENCE:
                                     0000   138 ;
                                     0000   139 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   140 ;
                                     0000   141 ; INPUT PARAMETERS:
                                     0000   142 ;
                                     0000   143 ;	NONE
                                     0000   144 ;
                                     0000   145 ; IMPLICIT INPUTS:
                                     0000   146 ;
                                     0000   147 ;	NONE
                                     0000   148 ;
                                     0000   149 ; OUTPUT PARAMETERS:
                                     0000   150 ;
                                     0000   151 ;	NONE
                                     0000   152 ;
                                     0000   153 ; IMPLICIT OUTPUTS:
                                     0000   154 ;
                                     0000   155 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   156 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   157 ;
                                     0000   158 ; COMPLETION CODES:
                                     0000   159 ;
                                     0000   160 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   161 ;
                                     0000   162 ; SIDE EFFECTS:
                                     0000   163 ;
                                     0000   164 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   165 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   166 ;
                                     0000   167 ;--
                                     0000   168 
                                     0000   169 
                                     0000   170 
                                     0000   171 TM_SETUP::
                          52     D4  0000   172 	CLRL	R2			; INITIALIZE
                          53     D4  0002   173 	CLRL	R3			; .. CONDITION
                          54     D4  0004   174 	CLRL	R4			; .... TABLE
                          55     D4  0006   175 	CLRL	R5			; ...... INDEX
                          56     D4  0008   176 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   177 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
