                                     0000   198 	.SBTTL	SATSSF02
                                     0000   199 ;++
                                     0000   200 ; FUNCTIONAL DESCRIPTION:
                                     0000   201 ;
                                     0000   202 ;		AFTER PERFORMING SOME INITIAL HOUSEKEEPING, SUCH AS
                                     0000   203 ; PRINTING THE MODULE BEGIN MESSAGE AND ACQUIRING ALL PRIVILEGES,
                                     0000   204 ; THE SATSSF02 ROUTINE EXECUTES THE TEST_SERV_EXEC MACRO TO RUN
                                     0000   205 ; ALL TEST CASES. WHEN THE MACRO COMPLETES ITS EXECUTION, SATSSF02
                                     0000   206 ; PRINTS A TEST MODULE SUCCESS OR FAIL MESSAGE AND EXITS TO THE
                                     0000   207 ; OPERATING SYSTEM. TEST_SERV_EXEC CALLS THE TC_CONTROL/TESTSERV
                                     0000   208 ; CO-ROUTINE PAIR ONCE PER TEST CASE GROUP TO EXECUTE ALL TEST
                                     0000   209 ; CASES IN THAT GROUP. EACH TEST CASE GROUP IS DEFINED BY BOUNDING
                                     0000   210 ; ITS TEST CASES WITH A TC_GROUP MACRO BEFORE THE FIRST TEST CASE
                                     0000   211 ; AND A TCEND MACRO AFTER THE LAST ONE. THE TEST CASES THEMSELVES
                                     0000   212 ; ARE DEFINED WITHIN THESE BOUNDS BY PRECEDING EACH WITH A
                                     0000   213 ; NEXT_TEST_CASE MACRO. TC_CONTROL/TESTSERV EXECUTES THE CODE 
                                     0000   214 ; FOLLOWING EACH NEXT_TEST_CASE MACRO IMMEDIATELY BEFORE ISSUING
                                     0000   215 ; THE SYSTEM SERVICE AS REQUESTED IN THE TESTSERV MACRO. TC_CONTROL/
                                     0000   216 ; TESTSERV ALSO CHECKS THE RESULTS OF THE SERVICE WITH RESPECT
                                     0000   217 ; TO ITS EXPECTED STATUS CODE AND PRINTS ANY REQUIRED FAILURE
                                     0000   218 ; MESSAGES FOR THE TEST CASE. THE CODE APPEARING AFTER EACH
                                     0000   219 ; NEXT_TEST_CASE MACRO IS MERELY TO SET UP CONDITIONS REQUIRED
                                     0000   220 ; FOR THE SYSTEM SERVICE AND TO CLEAN UP ANY RESOURCES ACQUIRED
                                     0000   221 ; BY THE PREVIOUS TEST CASE.
                                     0000   222 ;
                                     0000   223 ; CALLING SEQUENCE:
                                     0000   224 ;
                                     0000   225 ;	$ RUN SATSSF02  ...  (DCL COMMAND)
                                     0000   226 ;
                                     0000   227 ; INPUT PARAMETERS:
                                     0000   228 ;
                                     0000   229 ;	NONE
                                     0000   230 ;
                                     0000   231 ; IMPLICIT INPUTS:
                                     0000   232 ;
                                     0000   233 ;	NONE
                                     0000   234 ;
                                     0000   235 ; OUTPUT PARAMETERS:
                                     0000   236 ;
                                     0000   237 ;	NONE
                                     0000   238 ;
                                     0000   239 ; IMPLICIT OUTPUTS:
                                     0000   240 ;
                                     0000   241 ;	MESSAGES TO SYS$OUTPUT ARE THE ONLY OUTPUT FROM SATSSF02.
                                     0000   242 ;	THEY ARE OF THE FORM:
                                     0000   243 ;
                                     0000   244 ;		%UETP-S-SATSMS, TEST MODULE SATSSF02 BEGUN ... (BEGIN MSG)
                                     0000   245 ;		%UETP-S-SATSMS, TEST MODULE SATSSF02 SUCCESSFUL ... (END MSG)
                                     0000   246 ;		%UETP-E-SATSMS, TEST MODULE SATSSF02 FAILED ... (END MSG)
                                     0000   247 ;		%UETP-I-TEXT, ... (VARIABLE INFORMATION ABOUT A TEST MODULE FAILURE)
                                     0000   248 ;
                                     0000   249 ; COMPLETION CODES:
                                     0000   250 ;
                                     0000   251 ;	THE SATSSF02 ROUTINE TERMINATES WITH A $EXIT TO THE
                                     0000   252 ;	OPERATING SYSTEM WITH A STATUS CODE DEFINED BY UETP$_SATSMS.
                                     0000   253 ;
