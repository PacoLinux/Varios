                                     03D2   463 	.SBTTL	VFY_CLEANUP
                                     03D2   464 ;++
                                     03D2   465 ; FUNCTIONAL DESCRIPTION:
                                     03D2   466 ;
                                     03D2   467 ;		VFY_CLEANUP EXECUTES SYSTEM SERVICES TO UNDO THE
                                     03D2   468 ; EFFECT OF THOSE ISSUED IN THE VERIFY SUBROUTINE. VFY_CLEANUP MUST
                                     03D2   469 ; ASSUME THAT VERIFY MAY NOT HAVE EXECUTED IN ITS ENTIRETY (IF AN
                                     03D2   470 ; ERROR IS FOUND). ALSO, VFY_CLEANUP MAY ISSUE SS_CHECK OR ERR_EXIT
                                     03D2   471 ; ONLY AFTER PERFORMING ALL OF ITS CLEANUP OPERATIONS; THIS IS REQUIRED
                                     03D2   472 ; IN THE EVENT THAT VFY_CLEANUP IS CALLED DURING ERROR PROCESSING,
                                     03D2   473 ; WHEN PERFORMING THE REQUIRED CLEANUP IS MORE IMPORTANT THAN
                                     03D2   474 ; POSSIBLY DISCOVERING A SECOND ERROR.
                                     03D2   475 ;
                                     03D2   476 ; CALLING SEQUENCE:
                                     03D2   477 ;
                                     03D2   478 ;	BSBW VFY_CLEANUP
                                     03D2   479 ;
                                     03D2   480 ; INPUT PARAMETERS:
                                     03D2   481 ;
                                     03D2   482 ;	NONE
                                     03D2   483 ;
                                     03D2   484 ; IMPLICIT INPUTS:
                                     03D2   485 ;
                                     03D2   486 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     03D2   487 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     03D2   488 ;	FOR X = 1,2,3,4,5 :
                                     03D2   489 ;		CONDX_E - ADDRESS OF TABLE OF DATA VALUES FOR CONDX
                                     03D2   490 ;		  TABLE. IF THE CONTEXT OF TABLE X IS A SYSTEM SERVICE
                                     03D2   491 ;		  ARGUMENT, THE ARGUMENT NAME MAY BE USED AS A SYNONYM
                                     03D2   492 ;		  FOR CONDX_E.
                                     03D2   493 ;
                                     03D2   494 ; OUTPUT PARAMETERS:
                                     03D2   495 ;
                                     03D2   496 ;	NONE
                                     03D2   497 ;
                                     03D2   498 ; IMPLICIT OUTPUTS:
                                     03D2   499 ;
                                     03D2   500 ;	NONE
                                     03D2   501 ;
                                     03D2   502 ; COMPLETION CODES:
                                     03D2   503 ;
                                     03D2   504 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     03D2   505 ;
                                     03D2   506 ; SIDE EFFECTS:
                                     03D2   507 ;
                                     03D2   508 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     03D2   509 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     03D2   510 ;
                                     03D2   511 ;--
                                     03D2   512 
                                     03D2   513 
                                     03D2   514 
                                     03D2   515 VFY_CLEANUP::
                                 05  03D2   516 	RSB				; RETURN TO CALLER
                                     03D3   517 	.END
