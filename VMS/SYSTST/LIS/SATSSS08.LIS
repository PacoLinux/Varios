                                     024F   290 	.SBTTL	FORM_CONDS
                                     024F   291 ;++
                                     024F   292 ; FUNCTIONAL DESCRIPTION:
                                     024F   293 ;
                                     024F   294 ; 		FORM_CONDS FORMATS AND PRINTS INFORMATION ABOUT
                                     024F   295 ;  THE CURRENT ELEMENT IN EACH OF THE CONDITION TABLES.
                                     024F   296 ;
                                     024F   297 ; CALLING SEQUENCE:
                                     024F   298 ;
                                     024F   299 ;	BSBW FORM_CONDS
                                     024F   300 ;
                                     024F   301 ; INPUT PARAMETERS:
                                     024F   302 ;
                                     024F   303 ;	NONE
                                     024F   304 ;
                                     024F   305 ; IMPLICIT INPUTS:
                                     024F   306 ;
                                     024F   307 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     024F   308 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     024F   309 ;	FOR X = 1,2,3,4,5 :
                                     024F   310 ;		CONDX_T - TITLE TEXT FOR CONDX TABLE
                                     024F   311 ;		CONDX_TAB - ELEMENT TEXT FOR CONDX TABLE
                                     024F   312 ;		CONDX_C - CONTEXT OF THE CONDX TABLE
                                     024F   313 ;		CONDX_E - DATA ELEMENTS OF THE CONDX TABLE
                                     024F   314 ;
                                     024F   315 ; OUTPUT PARAMETERS:
                                     024F   316 ;
                                     024F   317 ;	NONE
                                     024F   318 ;
                                     024F   319 ; IMPLICIT OUTPUTS:
                                     024F   320 ;
                                     024F   321 ;	NONE
                                     024F   322 ;
