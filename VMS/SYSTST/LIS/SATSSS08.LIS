1.0             CONDITION SUBROUTINES - SETUP AND CLEANUP                                                                        (1)

                                     0245   220 	.SBTTL	CONDITION SUBROUTINES - SETUP AND CLEANUP 
                                     0245   221 ;++
                                     0245   222 ; FUNCTIONAL DESCRIPTION:
                                     0245   223 ;
                                     0245   224 ;		CONDX AND CONDX_CLEANUP ARE SUBROUTINES WHICH ARE EXECUTED
                                     0245   225 ; BEFORE AND AFTER THE VERIFY SUBROUTINE, RESPECTIVELY, WHENEVER A NEW
                                     0245   226 ; CONDITION X VALUE IS SELECTED (SEE FUNCTIONAL DESCRIPTION OF SUCCOMMON
                                     0245   227 ; ROUTINE IN SUCCOMMON.MAR). ANY SETUP FUNCTION PARTICULAR TO THE 
                                     0245   228 ; CONDITION X TABLE IS INCLUDED IN THE CONDX SUBROUTINE AND CLEANED
                                     0245   229 ; UP, IF NECESSARY, IN THE CONDX_CLEANUP SUBROUTINE. THIS INCLUDES,
                                     0245   230 ; ESPECIALLY, CODE TO DETECT CONFLICTS AMONG CURRENT ENTRIES IN TWO
                                     0245   231 ; OR MORE CONDITION TABLES. IF A CONFLICT IS DETECTED, A NON-ZERO
                                     0245   232 ; VALUE IS STORED INTO CONFLICT, WHICH CAUSES THE CALLING ROUTINE
                                     0245   233 ; (SUCCOMMON) TO SKIP THE CURRENT ENTRY IN THE CONDITION X TABLE.
                                     0245   234 ;
                                     0245   235 ; CALLING SEQUENCE:
                                     0245   236 ;
                                     0245   237 ;	BSBW CONDX   BSBW CONDX_CLEANUP
                                     0245   238 ;	  WHERE X = 1,2,3,4,5
                                     0245   239 ;
                                     0245   240 ; INPUT PARAMETERS:
                                     0245   241 ;
                                     0245   242 ;	CONFLICT = 0
                                     0245   243 ;
                                     0245   244 ; IMPLICIT INPUTS:
                                     0245   245 ;
                                     0245   246 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     0245   247 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     0245   248 ;
                                     0245   249 ; OUTPUT PARAMETERS:
                                     0245   250 ;
                                     0245   251 ;	CONFLICT SET TO NON-ZERO IF COND TABLE CONFLICT DETECTED.
                                     0245   252 ;
                                     0245   253 ; IMPLICIT OUTPUTS:
                                     0245   254 ;
                                     0245   255 ;	R2,3,4,5,6 PRESERVED
                                     0245   256 ;
                                     0245   257 ; COMPLETION CODES:
                                     0245   258 ;
                                     0245   259 ;	NONE
                                     0245   260 ;
                                     0245   261 ; SIDE EFFECTS:
                                     0245   262 ;
                                     0245   263 ;	NONE
                                     0245   264 ;
                                     0245   265 ;--
                                     0245   266 
                                     0245   267 
                                     0245   268 
                                     0245   269 COND1::
                                 05  0245   270 	RSB				; RETURN TO MAIN ROUTINE
                                     0246   271 COND1_CLEANUP::
                                 05  0246   272 	RSB				; RETURN TO MAIN ROUTINE
