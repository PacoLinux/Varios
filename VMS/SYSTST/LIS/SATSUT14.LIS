SATSUT14        MACROS, DATA                                                                                                     (1)

                                 00000000     5 	.PSECT	RWDATA,RD,WRT,NOEXE
                                     0000     6 LOGNAM_PID:	STRING	I,<SYSTST$PID> ; LOG NAME OF PID OF CREATING PROCESS
                                     0012     7 EQUIV_PID:	STRING	O,4	; BUFFER FOR CREATING PID (EQUIVALENCE NAME)
                                     001E     8 ; *
                                     001E     9 ; *	THIS ROUTINE RUNS IN A  PROCESS CREATED BY
                                     001E    10 ; *	A PROCESS RUNNING THE SATSSS36 IMAGE.  THIS PROCESS
                                     001E    11 ; *	SIGNALS THE CREATING PROCESS (VIA $WAKE) THAT IT HAS
                                     001E    12 ; *	BEEN FULLY CREATED AND THEN HIBERNATES, ANTICIPATING
                                     001E    13 ; *	EITHER A $WAKE FROM THE CREATING PROCESS, OR A
                                     001E    14 ; *	$DELPRC FROM THE CREATING PROCESS. IF THE CREATOR
                                     001E    15 ; *	ISSUES $WAKE, THEN THIS PROCESS DOES ITS OWN $DELPRC.
                                     001E    16 ; *	PROPER DELETION OF THIS PROCESS IS CHECKED IN THE
                                     001E    17 ; *	CREATING PROCESS THROUGH THE USE OF THE TERMINATION
                                     001E    18 ; *	MAILBOX MECHANISM.
                                     001E    19 ; *
SATSUT14        SATS SYSTEM SERVICE TESTS                        22-AUG-1978 04:22:51   VAX-11 MACRO X0.3-11               Page   3
SATSUT14        EXECUTABLE CODE                                                                                                  (1)

                                     001E    21 	.SBTTL	EXECUTABLE CODE
                                 00000000    22 	.PSECT	SATSUT14,RD,WRT,EXE
                               0000  0000    23 START:	.WORD	0	; ENTRY MASK
                                     0002    24 SATSUT14:
                                     0002    25 	$TRNLOG_S LOGNAM=LOGNAM_PID, - ; GET CREATOR'S PID FROM SYSTEM LOG NAME TBL
                                     0002    26 		  RSLBUF=EQUIV_PID,  -
                                     0002    27 		  DSBMSK=#^B110
                                     001D    28 	$WAKE_S PIDADR=EQUIV_PID+8; SIGNAL TO CREATOR: THIS PROC IS FULLY CREATED
                                     002C    29 	$HIBER_S		; SLEEP WHILE CREATOR DECIDES TO $WAKE OR $DELPRC
                                     0033    30 ;
                                     0033    31 ; ****** SYSTEM SERVICE CALL WHICH IS THE SUBJECT OF THIS TEST CASE ******
                                     0033    32 ;
                                     0033    33 	$DELPRC_S		; ISSUE SUBJECT DELPRC
                                     003E    34 	.END	START
