                                     0000   127 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   128 ;++
                                     0000   129 ; FUNCTIONAL DESCRIPTION:
                                     0000   130 ;
                                     0000   131 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   132 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   133 ; TEST MODULE EXECUTION.
                                     0000   134 ;
                                     0000   135 ; CALLING SEQUENCE:
                                     0000   136 ;
                                     0000   137 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   138 ;
                                     0000   139 ; INPUT PARAMETERS:
                                     0000   140 ;
                                     0000   141 ;	NONE
                                     0000   142 ;
                                     0000   143 ; IMPLICIT INPUTS:
                                     0000   144 ;
                                     0000   145 ;	NONE
                                     0000   146 ;
                                     0000   147 ; OUTPUT PARAMETERS:
                                     0000   148 ;
                                     0000   149 ;	NONE
                                     0000   150 ;
                                     0000   151 ; IMPLICIT OUTPUTS:
                                     0000   152 ;
                                     0000   153 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   154 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   155 ;
                                     0000   156 ; COMPLETION CODES:
                                     0000   157 ;
                                     0000   158 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   159 ;
                                     0000   160 ; SIDE EFFECTS:
                                     0000   161 ;
                                     0000   162 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   163 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   164 ;
                                     0000   165 ;--
                                     0000   166 
                                     0000   167 
                                     0000   168 
                                     0000   169 TM_SETUP::
                          52     D4  0000   170 	CLRL	R2			; INITIALIZE
                          53     D4  0002   171 	CLRL	R3			; .. CONDITION
                          54     D4  0004   172 	CLRL	R4			; .... TABLE
                          55     D4  0006   173 	CLRL	R5			; ...... INDEX
                          56     D4  0008   174 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   175 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
