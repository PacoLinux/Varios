                                     0000   173 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   174 ;++
                                     0000   175 ; FUNCTIONAL DESCRIPTION:
                                     0000   176 ;
                                     0000   177 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   178 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   179 ; TEST MODULE EXECUTION.
                                     0000   180 ;
                                     0000   181 ; CALLING SEQUENCE:
                                     0000   182 ;
                                     0000   183 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   184 ;
                                     0000   185 ; INPUT PARAMETERS:
                                     0000   186 ;
                                     0000   187 ;	NONE
                                     0000   188 ;
                                     0000   189 ; IMPLICIT INPUTS:
                                     0000   190 ;
                                     0000   191 ;	NONE
                                     0000   192 ;
                                     0000   193 ; OUTPUT PARAMETERS:
                                     0000   194 ;
                                     0000   195 ;	NONE
                                     0000   196 ;
                                     0000   197 ; IMPLICIT OUTPUTS:
                                     0000   198 ;
                                     0000   199 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   200 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   201 ;
                                     0000   202 ; COMPLETION CODES:
                                     0000   203 ;
                                     0000   204 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   205 ;
                                     0000   206 ; SIDE EFFECTS:
                                     0000   207 ;
                                     0000   208 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   209 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   210 ;
                                     0000   211 ;--
                                     0000   212 
                                     0000   213 
                                     0000   214 
