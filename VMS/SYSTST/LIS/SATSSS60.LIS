                                     00CA   306 	.SBTTL	FORM_CONDS
                                     00CA   307 ;++
                                     00CA   308 ; FUNCTIONAL DESCRIPTION:
                                     00CA   309 ;
                                     00CA   310 ; 		FORM_CONDS FORMATS AND PRINTS INFORMATION ABOUT
                                     00CA   311 ;  THE CURRENT ELEMENT IN EACH OF THE CONDITION TABLES.
                                     00CA   312 ;
                                     00CA   313 ; CALLING SEQUENCE:
                                     00CA   314 ;
                                     00CA   315 ;	BSBW FORM_CONDS
                                     00CA   316 ;
                                     00CA   317 ; INPUT PARAMETERS:
                                     00CA   318 ;
                                     00CA   319 ;	NONE
                                     00CA   320 ;
                                     00CA   321 ; IMPLICIT INPUTS:
                                     00CA   322 ;
                                     00CA   323 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
