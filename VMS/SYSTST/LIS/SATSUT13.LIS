SATSUT13        SATS SYSTEM SERVICE TESTS                        22-AUG-1978 04:22:45   VAX-11 MACRO X0.3-11               Page   2
SATSUT13        MACROS, DATA                                                                                                     (1)

                                 00000000     5 	.PSECT	RWDATA,RD,WRT,NOEXE
                           00000000  0000     6 CREPID:		.LONG	0	; PID OF CREATED (THIS) PROCESS
                                     0004     7 LOGNAM_PID:	STRING	I,<SYSTST$PID> ; LOG NAME OF PID OF CREATING PROCESS
                                     0016     8 EQUIV_PID:	STRING	O,4	; BUFFER FOR CREATING PID (EQUIVALENCE NAME)
                                     0022     9 ; *
                                     0022    10 ; *	THIS ROUTINE RUNS IN A  PROCESS CREATED BY
                                     0022    11 ; *	A PROCESS RUNNING THE SATSSS42 IMAGE.  THIS PROCESS
                                     0022    12 ; *	OBTAINS ITS PID FOR LATER USE, SIGNALS THE CREATING PROCESS
                                     0022    13 ; *	(VIA $WAKE) THAT IT IS FULLY CREATED, AND THEN HIBERNATES,
                                     0022    14 ; *	ANTICIPATING EITHER A $WAKE FROM THE CREATING PROCESS,
                                     0022    15 ; *	OR A $FORCEX FROM THE CREATING PROCESS. IF THE CREATOR
                                     0022    16 ; *	ISSUES $WAKE, THEN THIS PROCESS DOES ITS OWN $FORCEX,
                                     0022    17 ; *	USING ITS PID AS AN EXIT COMPLETION STATUS CODE.
                                     0022    18 ; *	THE STATUS CODE WILL BE CHECKED FOR CORRECTNESS BY 
                                     0022    19 ; *	THE CREATING PROCESS THROUGH THE USE OF THE
                                     0022    20 ; *	PROCESS TERMINATION MAILBOX MECHANISM.
                                     0022    21 ; *
SATSUT13        SATS SYSTEM SERVICE TESTS                        22-AUG-1978 04:22:45   VAX-11 MACRO X0.3-11               Page   3
SATSUT13        EXECUTABLE CODE                                                                                                  (1)

                                     0022    23 	.SBTTL	EXECUTABLE CODE
                                 00000000    24 	.PSECT	SATSUT13,RD,WRT,EXE
                               0000  0000    25 START:	.WORD	0	; ENTRY MASK
                                     0002    26 SATSUT13:
                                     0002    27 	$WAKE_S	CREPID		; OBTAIN PID OF THIS PROCESS
                                     0011    28 	$HIBER_S		; UNDO ABOVE WAKE
                                     0018    29 	$TRNLOG_S LOGNAM=LOGNAM_PID, - ; GET CREATOR'S PID FROM SYSTEM LOG NAME TBL
                                     0018    30 		  RSLBUF=EQUIV_PID,  -
                                     0018    31 		  DSBMSK=#^B110
                                     0033    32 	$WAKE_S PIDADR=EQUIV_PID+8; SIGNAL TO CREATOR: THIS PROC IS FULLY CREATED
                                     0042    33 	$HIBER_S		; SLEEP WHILE CREATOR DECIDES TO $WAKE OR $FORCEX
                                     0049    34 ;
                                     0049    35 ; ****** SYSTEM SERVICE CALL WHICH IS THE SUBJECT OF THIS TEST CASE ******
                                     0049    36 ;
                                     0049    37 	$FORCEX_S CODE=CREPID	; ISSUE SUBJECT FORCEX WITH PID AS STATUS CODE
                                     005A    38 	.END	START
