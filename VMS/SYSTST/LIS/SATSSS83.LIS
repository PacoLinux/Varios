                                     01C0   390 VERIFY::
                 00000000'EF     95  01C0   391 	TSTB	CFLAG			; SHOULD CONDITIONS BE PRINTED ?
                          03     13  01C6   392 	BEQL	5$			; NO -- CONTINUE
                          FEFF   30  01C8   393 	BSBW	FORM_CONDS		; YES -- FMT & PRINT ALL CONDS FOR THIS T.C.
                                     01CB   394 5$:
                                     01CB   395 	$SETSWM_S COND2_E[R3]		; ISSU PRELIM SERV TO ESTAB "PREV" CONDITION
                                     01D9   396 ;
                                     01D9   397 ; ****** SYSTEM SERVICE CALL WHICH IS THE SUBJECT OF THIS TEST CASE ******
                                     01D9   398 ;
                                     01D9   399 	$SETSWM_S SWPFLG[R2]		; ISSUE SUBJECT SERVICE
            59   00000008'EF43   9A  01E7   400 	MOVZBL	STATCODES[R3],R9 	; GET EXPECTED STATUS CODE
            59            50     D1  01EF   401 	CMPL	R0,R9			; CODE RECEIVED = CODE EXPECTED ?
                          03     12  01F2   402 	BNEQ	10$			; NO -- GO PROCESS ERROR
                          005B   31  01F4   403 	BRW	VERIFYX			; YES -- ALL FINISHED
                                     01F7   404 10$:
   00000000'EF            59     D0  01F7   405 	MOVL	R9,EXPV			; LOAD UP EXPECTED AND
   00000000'EF            50     D0  01FE   406 	MOVL	R0,RECV			; ... RECEIVED VALUES, THEN EXIT
                                     0205   407 	ERR_EXIT LONG,<INCORRECT STATUS CODE RETURNED BY SETSWM>
                                     0252   408 VERIFYX:
                                 05  0252   409 	RSB				; RETURN TO CALLER
