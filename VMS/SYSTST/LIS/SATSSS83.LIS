                                     0000   103 	.SBTTL	TM_SETUP, TM_CLEANUP
                                     0000   104 ;++
                                     0000   105 ; FUNCTIONAL DESCRIPTION:
                                     0000   106 ;
                                     0000   107 ;		TM_SETUP AND TM_CLEANUP ARE CALLED TO PERFORM
                                     0000   108 ; REQUIRED HOUSEKEEPING AT THE BEGINNING AND END, RESPECTIVELY, OF
                                     0000   109 ; TEST MODULE EXECUTION.
                                     0000   110 ;
                                     0000   111 ; CALLING SEQUENCE:
                                     0000   112 ;
                                     0000   113 ;	BSBW TM_SETUP   BSBW TM_CLEANUP
                                     0000   114 ;
                                     0000   115 ; INPUT PARAMETERS:
                                     0000   116 ;
                                     0000   117 ;	NONE
                                     0000   118 ;
                                     0000   119 ; IMPLICIT INPUTS:
                                     0000   120 ;
                                     0000   121 ;	NONE
                                     0000   122 ;
                                     0000   123 ; OUTPUT PARAMETERS:
                                     0000   124 ;
                                     0000   125 ;	NONE
                                     0000   126 ;
                                     0000   127 ; IMPLICIT OUTPUTS:
                                     0000   128 ;
                                     0000   129 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   130 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   131 ;
                                     0000   132 ; COMPLETION CODES:
                                     0000   133 ;
                                     0000   134 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   135 ;
                                     0000   136 ; SIDE EFFECTS:
                                     0000   137 ;
                                     0000   138 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0000   139 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0000   140 ;
                                     0000   141 ;--
                                     0000   142 
                                     0000   143 
                                     0000   144 
                                     0000   145 TM_SETUP::
                          52     D4  0000   146 	CLRL	R2			; INITIALIZE
                          53     D4  0002   147 	CLRL	R3			; .. CONDITION
                          54     D4  0004   148 	CLRL	R4			; .... TABLE
                          55     D4  0006   149 	CLRL	R5			; ...... INDEX
                          56     D4  0008   150 	CLRL	R6			; ........ REGISTERS
                          FFF3'  30  000A   151 	BSBW	MOD_MSG_PRINT		; PRINT TEST MODULE BEGIN MSG
   00000000'EF   00000000'EF     DE  000D   152 	MOVAL	TEST_MOD_SUCC,TMD_ADDR	; ASSUME END MSG WILL SHOW SUCCESS
            00   00000000'8F     F0  0018   153 	INSV	#SUCCESS,#0,#3,MOD_MSG_CODE ; ADJUST STATUS CODE FOR SUCCESS
   00000000'EF            03         001F       
                                     0025   154 	MODE	TO,5$,KRNL		; KERNEL MODE TO ACCESS PHD
            59   00000000'9F     D0  0048   155 	MOVL	@#CTL$GL_PHD,R9		; GET PROCESS HEADER ADDRESS
