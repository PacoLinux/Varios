                                     024B   250 	.SBTTL	CONDITION SUBROUTINES - SETUP AND CLEANUP 
                                     024B   251 ;++
                                     024B   252 ; FUNCTIONAL DESCRIPTION:
                                     024B   253 ;
                                     024B   254 ;		CONDX AND CONDX_CLEANUP ARE SUBROUTINES WHICH ARE EXECUTED
                                     024B   255 ; BEFORE AND AFTER THE VERIFY SUBROUTINE, RESPECTIVELY, WHENEVER A NEW
                                     024B   256 ; CONDITION X VALUE IS SELECTED (SEE FUNCTIONAL DESCRIPTION OF SUCCOMMON
                                     024B   257 ; ROUTINE IN SUCCOMMON.MAR). ANY SETUP FUNCTION PARTICULAR TO THE 
                                     024B   258 ; CONDITION X TABLE IS INCLUDED IN THE CONDX SUBROUTINE AND CLEANED
                                     024B   259 ; UP, IF NECESSARY, IN THE CONDX_CLEANUP SUBROUTINE. THIS INCLUDES,
                                     024B   260 ; ESPECIALLY, CODE TO DETECT CONFLICTS AMONG CURRENT ENTRIES IN TWO
                                     024B   261 ; OR MORE CONDITION TABLES. IF A CONFLICT IS DETECTED, A NON-ZERO
                                     024B   262 ; VALUE IS STORED INTO CONFLICT, WHICH CAUSES THE CALLING ROUTINE
                                     024B   263 ; (SUCCOMMON) TO SKIP THE CURRENT ENTRY IN THE CONDITION X TABLE.
                                     024B   264 ;
                                     024B   265 ; CALLING SEQUENCE:
                                     024B   266 ;
                                     024B   267 ;	BSBW CONDX   BSBW CONDX_CLEANUP
                                     024B   268 ;	  WHERE X = 1,2,3,4,5
                                     024B   269 ;
                                     024B   270 ; INPUT PARAMETERS:
                                     024B   271 ;
                                     024B   272 ;	CONFLICT = 0
                                     024B   273 ;
                                     024B   274 ; IMPLICIT INPUTS:
                                     024B   275 ;
                                     024B   276 ;	R2,3,4,5,6 CONTAIN CURRENT CONDITION TABLE INDEX VALUES
                                     024B   277 ;	  FOR COND TABLES 1,2,3,4,5, RESPECTIVELY.
                                     024B   278 ;
                                     024B   279 ; OUTPUT PARAMETERS:
                                     024B   280 ;
                                     024B   281 ;	CONFLICT SET TO NON-ZERO IF COND TABLE CONFLICT DETECTED.
                                     024B   282 ;
                                     024B   283 ; IMPLICIT OUTPUTS:
                                     024B   284 ;
                                     024B   285 ;	R2,3,4,5,6 PRESERVED
                                     024B   286 ;
                                     024B   287 ; COMPLETION CODES:
                                     024B   288 ;
                                     024B   289 ;	NONE
                                     024B   290 ;
                                     024B   291 ; SIDE EFFECTS:
                                     024B   292 ;
                                     024B   293 ;	NONE
                                     024B   294 ;
                                     024B   295 ;--
                                     024B   296 
                                     024B   297 
                                     024B   298 
