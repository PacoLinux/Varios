                                     0000   182 ;
                                     0000   183 ;	TM_SETUP:  COND TABLE INDEX REGISTERS (R2,3,4,5,6) CLEARED;
                                     0000   184 ;		   ALL PRIVILEGES ACQUIRED.
                                     0000   185 ;
                                     0000   186 ; COMPLETION CODES:
                                     0000   187 ;
                                     0000   188 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0000   189 ;
                                     0000   190 ; SIDE EFFECTS:
                                     0000   191 ;
                                     0000   192 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
