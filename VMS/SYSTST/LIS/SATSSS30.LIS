                                     0552   508 ;		  TABLE. IF THE CONTEXT OF TABLE X IS A SYSTEM SERVICE
                                     0552   509 ;		  ARGUMENT, THE ARGUMENT NAME MAY BE USED AS A SYNONYM
                                     0552   510 ;		  FOR CONDX_E.
                                     0552   511 ;
                                     0552   512 ; OUTPUT PARAMETERS:
                                     0552   513 ;
                                     0552   514 ;	NONE
                                     0552   515 ;
                                     0552   516 ; IMPLICIT OUTPUTS:
                                     0552   517 ;
                                     0552   518 ;	NONE
                                     0552   519 ;
                                     0552   520 ; COMPLETION CODES:
                                     0552   521 ;
                                     0552   522 ;	EFLAG SET TO NON-ZERO IF ERROR ENCOUNTERED.
                                     0552   523 ;
                                     0552   524 ; SIDE EFFECTS:
                                     0552   525 ;
                                     0552   526 ;	SS_CHECK AND ERR_EXIT MACROS CAUSE PREMATURE EXIT 
                                     0552   527 ;	(VIA RSB) IF ERROR ENCOUNTERED.
                                     0552   528 ;
                                     0552   529 ;--
