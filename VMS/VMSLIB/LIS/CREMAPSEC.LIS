                                     0000    81 	.SBTTL	CREATE AND MAP SECTION
                                     0000    82 ;++
                                     0000    83 ; FUNCTIONAL DESCRIPTION:
                                     0000    84 ;
                                     0000    85 ;	THIS ROUTINE PROVIDES A SLIGHTLY SIMPLER INTERFACE TO THE
                                     0000    86 ; CREATE AND MAP SECTION SYSTEM SERVICE.  IT'S FUNCTION HOWEVER IS
                                     0000    87 ; IDENTICAL, I.E. TO CREATE AND MAP A SECTION.
                                     0000    88 ;
                                     0000    89 ; CALLING SEQUENCE:
                                     0000    90 ;
                                     0000    91 ;	CALLG	ARGLIST,CREMAPSEC
                                     0000    92 ;
                                     0000    93 ; INPUT PARAMETERS:
                                     0000    94 ;
                                     0000    95 ;	INADR(AP)	=		;INPUT ADDRESS RANGE TO MAP
                                     0000    96 ;	RETADR(AP)	=		;RETURN ADDRESS RANGE
                                     0000    97 ;	FLAGS(AP)	=		;SECTION CONTROL FLAGS
                                     0000    98 ;	GSDNAM(AP)	=		;GLOBAL SECTION NAME DESCRIPTOR
                                     0000    99 ;	RELPAG(AP)	=		;RELATIVE PAGE IN SECTION TO START MAPPING
                                     0000   100 ;	FILNAM(AP)	=		;DESCRIPTOR OF FILE NAME STRING
                                     0000   101 ;	PAGCNT(AP)	=		;SIZE OF SECTION TO CREATE
                                     0000   102 ;	VBN(AP)		=		;STARTING VBN OF FILE TO CREATE SECTION FROM
                                     0000   103 ;					;IF THE ABOVE 2 PARAMETERS ARE LEFT OUT
                                     0000   104 ;					;THE SECTION STARTS AT VBN 1 OF THE FILE
                                     0000   105 ;					;AND IS THE FILE LENGTH IN SIZE
                                     0000   106 ;	ACMODE(AP)	=		;OPTIONAL PARAMETER SPECIFYING ACCESS MODE
                                     0000   107 ;
                                     0000   108 ; IMPLICIT INPUTS:
                                     0000   109 ;
                                     0000   110 ;	NONE
                                     0000   111 ;
                                     0000   112 ; OUTPUT PARAMETERS:
                                     0000   113 ;
                                     0000   114 ;	R0 = SYSTEM STATUS CODE
                                     0000   115 ;
                                     0000   116 ; IMPLICIT OUTPUTS:
                                     0000   117 ;
                                     0000   118 ;	NONE
                                     0000   119 ;
                                     0000   120 ; COMPLETION CODES:
                                     0000   121 ;
                                     0000   122 ;	NONE
                                     0000   123 ;
                                     0000   124 ; SIDE EFFECTS:
                                     0000   125 ;
                                     0000   126 ;	NONE
                                     0000   127 ;
                                     0000   128 ;--
