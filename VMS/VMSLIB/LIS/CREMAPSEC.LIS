                          02     11  00BA   184 	BRB	EXIT			;++X0205 ALL DONE
                          8E     D5  00BC   185 90$:	TSTL	(SP)+			;++X0205 RESTORE STACK POINTER
                                 04  00BE   186 EXIT:	RET				;AND RETURN TO CALLER
