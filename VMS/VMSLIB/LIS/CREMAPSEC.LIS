            40            50     E9  007B   168 	BLBC	R0,EXIT			;BRANCH IF FAILED
                                     007E   169 	$CRMPSC_S -			;CREATE AND MAP THE SECTION
                                     007E   170 		INADR=@INADR(AP),-	;INPUT RANGE DESCRIPTOR
                                     007E   171 		RETADR=@RETADR(AP),-	;RETURN RANGE DESCRIPTOR
                                     007E   172 		FLAGS=R4,-		;SECTION FLAGS
