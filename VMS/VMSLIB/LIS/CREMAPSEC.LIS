                                     0000   130 	.LIST	MEB
                                     0000   131 
                                 00000000   132 	.PSECT	LIB$CODE PIC,SHR,BYTE,RD,EXE,NOWRT
                                     0000   133 
                                     0000   134 LIB$_CREMAPSEC::
                               003C  0000   135 	.WORD	^M<R2,R3,R4,R5>
            5E         AC AE     DE  0002   136 	MOVAL	SCRATCHSIZE(SP),SP	;RESERVE SCRATCH STORAGE
            6E            00     2C  0006   137 	MOVC5	#0,(SP),#0,#-SCRATCHSIZE,(SP) ;ZERO THE SCRATCH STORAGE
       0054 8F            00         0009       
                          6E         000D       
            09            6C     D1  000E   138 	CMPL	(AP),#<ACMODE/4>	;IS ACCESS MODE PARAMETER PRESENT?
                          05     19  0011   139 	BLSS	20$			;BRANCH IF NOT
         FC AD         24 AC     D0  0013   140 	MOVL	ACMODE(AP),SVACMODE(FP)	;YES, SAVE IT IN A TEMP
            54         0C AC     D0  0018   141 20$:	MOVL	FLAGS(AP),R4		;GET CONTROL FLAGS
            54            00     E1  001C   142 	BBC	#SEC$V_GBL,R4,40$ ;BRANCH IF PROCESS SECTION
                          2C         001F       
            52         04 BC     7D  0020   143 	MOVQ	@INADR(AP),R2		;SAVE INPUT ADDRESS RANGE
                                     0024   144 					;COULD BE OVERWRITTEN BY RETADR IN
                                     0024   145 					;AN UNSUCCESSFUL MGBLSC ATTEMPT
                                     0024   146 	$MGBLSC_S -
                                     0024   147 		INADR=@INADR(AP),-	;ADDRESS OF INPUT ADDRESS RANGE
                                     0024   148 		RETADR=@RETADR(AP),-	;ADDRESS OF RETURN ADDRESS RANGE
                                     0024   149 		FLAGS=R4,-		;SECTION FLAGS
                                     0024   150 		ACMODE=SVACMODE(FP),-	;ACCESS MODE
                                     0024   151 		GSDNAM=@GSDNAM(AP),-	;ADDRESS OF GLOBAL SECTION NAME
                                     0024   152 		RELPAG=RELPAG(AP)	;RELATIVE PAGE IN SECTION
                       14 AC     DD  0024       		PUSHL	RELPAG(AP)
                          00     DD  0027       		PUSHL	#0
                       10 BC     7F  0029       		PUSHAQ	@GSDNAM(AP)
                          54     DD  002C       		PUSHL	R4
                       FC AD     DD  002E       		PUSHL	SVACMODE(FP)
                       08 BC     7F  0031       		PUSHAQ	@RETADR(AP)
                       04 BC     7F  0034       		PUSHAQ	@INADR(AP)
   00000000'GF            07     FB  0037       		CALLS	#7,G^SYS$MGBLSC
            7D            50     E8  003E   153 	BLBS	R0,EXIT			;BRANCH IF SUCCESSFUL, ALL DONE
                                     0041   154 	IFNOWRT	#8,@INADR(AP),40$	;UNLESS INPUT RANGE IS READ ONLY
            08            00     0D  0041       		PROBEW	#0,#8,@INADR(AP)
                       04 BC         0044       
                          04     13  0046       		BEQL	40$
         04 BC            52     7D  0048   155 	MOVQ	R2,@INADR(AP)		;RESTORE INPUT ADDRESS RANGE
            55         AC AD     DE  004C   156 40$:	MOVAL	FAB(FP),R5		;ADDRESS OF FAB
                                     0050   157 
                                     0050   158 	ASSUME	FAB$B_BLN EQ FAB$B_BID+1 ;ASSUME THESE ARE ADJACENT
            65       5003 8F     B0  0050   159 	MOVW	#<FAB$C_BLN@8 + FAB$C_BID>,FAB$B_BID(R5) ;SET ID AND SIZE OF FAB
            50         18 BC     7D  0055   160 	MOVQ	@FILNAM(AP),R0		;GET SIZE AND ADDRESS OF FILE NAME STRING
         34 A5            50     90  0059   161 	MOVB	R0,FAB$B_FNS(R5)	;SIZE OF FILE NAME STRING
         2C A5            51     D0  005D   162 	MOVL	R1,FAB$L_FNA(R5)	;ADDRESS OF FILE NAME STRING
            54            03     E1  0061   163 	BBC	#SEC$V_WRT,R4,60$ 	;BRANCH IF NOT MAPPING FOR WRITE
                          08         0064       
            54            01     E0  0065   164 	BBS	#SEC$V_CRF,R4,60$ 	;COPY ON REF IS STILL READ ONLY
                          04         0068       
         16 A5            01     90  0069   165 	MOVB	#FAB$M_PUT,FAB$B_FAC(R5) ;ACCESS FOR WRITING
         04 A5            11     E2  006D   166 60$:	BBSS	#FAB$V_UFO,FAB$L_FOP(R5),80$ ;SET USER FILE OPEN
                          00         0071       
                                     0072   167 80$:	$OPEN	(R5)			;OPEN THE FILE
                          65     DF  0072       		PUSHAL	(R5)
   00000000'9F            01     FB  0074       		CALLS	#$$.TMP1,@#SYS$OPEN
