                       04 BC     7F  009A       		PUSHAQ	@INADR(AP)
   00000000'GF            0C     FB  009D       		CALLS	#12,G^SYS$CRMPSC
            54            00     E1  00A4   179 	BBC	#SEC$V_GBL,R4,EXIT	;BRANCH IF NOT GLOBAL SECTION
                          16         00A7       
                          50     DD  00A8   180 	PUSHL	R0			;++X0205 REMEMBER STATUS CODE
                                     00AA   181 	$DASSGN_S CHAN=FAB$L_STV(R5)	;DEASSIGN THE CHANNEL FOR GLOBAL SECTION
            7E         0C A5     3C  00AA       		MOVZWL	FAB$L_STV(R5),-(SP)
   00000000'GF            01     FB  00AE       		CALLS	#1,G^SYS$DASSGN
