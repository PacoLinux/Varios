$DEF	ACF$L_DRVNAME		 		;ADDRESS OF DRIVER NAME COUNTED STRING 
			.BLKL	1 
$DEF	ACF$C_LENGTH
$DEF	ACF$K_LENGTH					;LENGTH OF DEVICE DESCRIPTOR ARGUMENT LIST 
 

	$DEFEND	ACF,$GBL,DEF

	.ENDM	$ACFDEF
 
