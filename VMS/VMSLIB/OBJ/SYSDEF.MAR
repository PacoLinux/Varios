$DEF	VEC$L_UNITDISC		 		;ADDRESS OF UNIT DISCONNECT 
			.BLKL	1 
$DEF	VEC$C_LENGTH
$DEF	VEC$K_LENGTH					;LENGTH OF STANDARD DISPATCHER 

	$DEFEND	VEC,$GBL,DEF

	.ENDM	$VECDEF
 
;+ 
; CXB - COMPLEX CHAINED BUFFER 
; 
; THESE OFFSETS ARE USED IN THE HEADER OF DISJOINT SEGMENTS 
