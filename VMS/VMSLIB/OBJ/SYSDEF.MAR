$DEF	WCB$C_MAP
$DEF	WCB$K_MAP					; MAP POINTERS START HERE 
$DEF	WCB$C_LENGTH
$DEF	WCB$K_LENGTH				; LENGTH OF STANDARD WCB SANS POINTERS 
