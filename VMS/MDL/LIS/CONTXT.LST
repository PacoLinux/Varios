        C	REVISION HISTORY:
        C		22-03-77	CP	IF MARK IN A NEGATIVE CONTEXT, JUST
        C					SAVE THE CURRENT NEGATIVE COUNT, AND
        C					OUTPUT THE LINE TO THE TMP FILE.
        C
        C		19-04-77	TC	USE THE ADDITIONAL CONTEX VARIABLE
        C					DISCUSSED IN IN:MDL.CMN
         
0001    	SUBROUTINE PMARK
         
        C	SAVES THE CONTEXT OF THE CURRENT STRUCTURE DECLARATION SO
        C	THAT THIS CONTEXT CAN BE RESTORED AT SOME LATER POINT.
         
0002    	INCLUDE 'SRC$:MDL.CMN/NOLIST'
         
0016    	INTEGER INDEX
         
0017    	CALL GETTOK
0018    	IF (TOKTYP .NE. NUMBER) GOTO 1000
0019    	INDEX = INTVAL (TOKSTG)
0020    	IF ((INDEX .LE. 0) .OR.
        	1	(INDEX .GT. 10)) GOTO 1000
0021    	IF (TMPOUT) GOTO 20
0022    	CALL SAVCTX (INDEX)
0023    	INPTR = INPTR - 1
0024    	CALL MDOUTL (INPTR)
0025    	CALL BDOUTL (INPTR)
0026    	RETURN
         
0027    20	CONTEX (1, INDEX) = NEGCNT
0028    	CONTEX (9, INDEX) = 1
0029    	CALL OUT2LN
0030    	RETURN
         
        C	ERROR REPORTING
         
0031    1000	TYPE 1010
0032    1010	FORMAT (' INVALID ARGUMENT TO MARK COMMAND')
0033    	RETURN
0034    	END
