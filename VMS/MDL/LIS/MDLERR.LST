MDLERR.FTN   	/TR:BLOCKS/WR

        C
        C
        C	ERRORS  --  ERROR MESSAGES FOR MACRO-11 CALLED BY FORTRAN PROGRAMS
        C
        C
        C	COPYRIGHT (C) 1976
        C	DIGITAL EQUIPMENT CORPORATION, MAYNARD, MASS.
        C
        C	THIS SOFTWARE IS FURNISHED UNDER  A LICENSE FOR USE ONLY  ON  A
        C	SINGLE COMPUTER SYSTEM AND MAY BE  COPIED ONLY WITH  THE INCLU-
        C	SION OF  THE  ABOVE  COPYRIGHT NOTICE.  THIS SOFTWARE,  OR  ANY
        C	OTHER COPIES THEREOF, MAY NOT BE  PROVIDED  OR  OTHERWISE  MADE
        C	AVAILABLE TO ANY OTHER PERSON EXCEPT  FOR  USE  ON  SUCH SYSTEM
        C	AND TO  ONE WHO AGREES  TO  THESE LICENSE  TERMS.  TITLE TO AND
        C	OWNERSHIP OF THE SOFTWARE SHALL AT ALL TIMES REMAIN IN DEC.
        C
        C	THE INFORMATION IN THIS SOFTWARE  IS  SUBJECT TO CHANGE WITHOUT
        C	NOTICE AND SHOULD NOT BE CONSTRUED  AS  A COMMITMENT BY DIGITAL
        C	EQUIPMENT CORPORATION.
        C
        C	DEC ASSUMES NO RESPONSIBILITY FOR THE USE OR RELIABILITY OF ITS
        C	SOFTWARE ON EQUIPMENT WHICH IS NOT SUPPLIED BY DEC.
        C
        C
        C	K.D. MORSE  07-NOV-76
        C
        C
        C
        C
        C
        C	THIS SUBROUTINE WRITES OUT THE ERROR MESSAGE PROVIDED BY THE
        C	CALLER.  THESE MESSAGES CORRESPOND TO ERRORS INCURRED DURING
        C	THE USE OF THE FOLLOWING MACRO ROUTINES (CALLED FROM FORTRAN):
        C	GETCML, SCANO, SCANI, AND SYNTX.
        C
        C
        C  MODIFICATIONS:
        C
        C  NO.		DATE		PROGRAMMER		PURPOSE
        C  ---		----		----------		-------
        C
        C
FORTRAN IV-PLUS V02-51D		03:13:01    22-AUG-78		PAGE 2   
MDLERR.FTN   	/TR:BLOCKS/WR

         
        C
        C
        C  VARIABLES:
        C
        C
        C	BRCODE--ERROR CODE FOR SPECIFIC ERROR, USED AS A BRANCH VALUE
        C	BYTE1--ONE BYTE OF OUTPUT DATA
        C	BYTE2--ONE BYTE OF OUTPUT DATA
        C	DATLNG--LENGTH OF ASCII OUTPUT DATA
        C	ERRTYP--ERROR CODE FOR TYPE OF ERROR, USED AS A BRANCH VALUE
        C	INTG1--ONE INTEGER OF OUTPUT DATA
        C	OUTDAT--ASCII OUTPUT DATA
        C	PROMPT--ASCII PROMPT STRING FOR UTILITY
        C
        C
        C
        C
0001    	SUBROUTINE ERRORS(ERRTYP,BRCODE,PROMPT,OUTDAT,DATLNG,BYTE1,
        	1INTG1,BYTE2)
        C
        C
        C
0002    	INTEGER BRCODE,DATLNG,ERRTYP
0003    	BYTE PROMPT(3),OUTDAT(1),BYTE1(1),BYTE2(1)
        C
        C
        C
        C
        C	FORMAT STATEMENTS
        C
        C
        C
0004    11	FORMAT(' ',3A1,' -- INVALID ERROR TYPE OF ',I5)
        C
        C
        C	ERROR MESSAGES
        C
        C
        C
        C	COMMAND LINE ERRORS
        C
0005    9511	FORMAT(' ',3A1,' -- COMMAND LINE I/O ERROR')
0006    9521	FORMAT(' ',3A1,' -- UNABLE TO OPEN COMMAND FILE')
0007    9531	FORMAT(' ',3A1,' -- ILLEGAL INDIRECT FILE SYNTAX')
0008    9541	FORMAT(' ',3A1,' -- INDIRECT FILES TOO DEEPLY NESTED')
0009    9551	FORMAT(' ',3A1,' -- INCORRECT SYNTAX IN:  ',<DATLNG>A1)
        C
        C	FILE SPECIFICATION ERRORS
        C
0010    9601	FORMAT(' ',3A1,' -- ',<DATLNG>A1,' FILE CONTAINS ILLEGAL '
        	1,'SWITCH')
0011    9611	FORMAT(' ',3A1,' -- NO ',<DATLNG>A1,' FILENAME OR DEFAULT'
        	1,' NAME SPECIFIED')
0012    9621	FORMAT(' ',3A1,' -- NO ',<DATLNG>A1,' FILE TYPE '
        	1,'OR DEFAULT TYPE SPECIFIED')
FORTRAN IV-PLUS V02-51D		03:13:01    22-AUG-78		PAGE 3   
MDLERR.FTN   	/TR:BLOCKS/WR

0013    9631	FORMAT(' ',3A1,' -- ASTERISK NOT ACCEPTABLE AS PART OF '
        	1/T9,<DATLNG>A1,' FILE SPECIFICATION')
FORTRAN IV-PLUS V02-51D		03:13:01    22-AUG-78		PAGE 4   
MDLERR.FTN   	/TR:BLOCKS/WR

         
        C
        C	BRANCH ON TYPE OF ERROR:
        C		1--GETCML
        C		2--SYNTX
        C		3--SCANO/SCANI
        C
0014    	GOTO (100,200,300),-ERRTYP
0015    	WRITE(5,11)PROMPT,ERRTYP
0016    	GOTO 1000
        C
        C
        C
        C	MACRO ROUTINE ERRORS
        C
        C
        C
        C	GETCML ERRORS
        C
        C		 0--END OF FILE, NO MESSAGE
        C		-1--GCML I/O ERROR
        C		-2--UNABLE TO OPEN COMMAND FILE
        C		-3--ILLEGAL INDIRECT FILE SYNTAX
        C		-4--INDIRECT FILES TOO DEEPLY NESTED
        C
        C		PARAMETERS=PROMPT
        C
0017    100	GOTO (9510,9520,9530,9540),-BRCODE
0018    	GOTO 1000
0019    9510	WRITE(5,9511)PROMPT
0020    	GOTO 1000
0021    9520	WRITE(5,9521)PROMPT
0022    	GOTO 1000
0023    9530	WRITE(5,9531)PROMPT
0024    	GOTO 1000
0025    9540	WRITE(5,9541)PROMPT
0026    	GOTO 1000
        C
        C
        C	SYNTX ERRORS
        C
        C
        C
        C		 PARAMETERS=PROMPT, LENGTH OF INVALID PART, INVALID PART
0027    200	IF (BRCODE.NE.0) WRITE(5,9551)PROMPT,(OUTDAT(I),I=1,DATLNG)
0028    	GOTO 1000
        C
        C
        C	SCANO/SCANI ERRORS
        C
        C
        C
        C		-1--ILLEGAL SWITCH ON FILE SPECIFICATION
        C		-2--NO FILENAME OR DEFAULT NAME SPECIFIED
        C		-3--NO FILE TYPE OR DEFAULT TYPE SPECIFIED
        C		-4--ASTERISK ILLEGAL AS PART OF FILE SPECIFICATION
FORTRAN IV-PLUS V02-51D		03:13:01    22-AUG-78		PAGE 5   
MDLERR.FTN   	/TR:BLOCKS/WR

        C
        C		PARAMETERS=PROMPT, TYPE OF FILE, LENGTH OF TYPE OF FILE
        C				(TYPE OF FILE=INPUT, OUTPUT, COMPARE, CORRECTION, ETC.)
        C
0029    300	GOTO (9600,9610,9620,9630),-BRCODE
0030    9600	WRITE(5,9601)PROMPT,(OUTDAT(I),I=1,DATLNG)
0031    	GOTO 1000
0032    9610	WRITE(5,9611)PROMPT,(OUTDAT(I),I=1,DATLNG)
0033    	GOTO 1000
0034    9620	WRITE(5,9621)PROMPT,(OUTDAT(I),I=1,DATLNG)
0035    	GOTO 1000
0036    9630	WRITE(5,9631)PROMPT,(OUTDAT(I),I=1,DATLNG)
0037    	GOTO 1000
        C
        C	RETURN TO CALLER.
        C
0038    1000	RETURN
0039    	END
