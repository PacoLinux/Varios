0001    	SUBROUTINE GETCMD
        C
        C	INCLUDE COMMON FILE
        C
0002    	INCLUDE 'SRC$:MDL.CMN/NOLIST'
        C
        C	DATA DECLARATIONS
        C
0016    	INTEGER INMLNG,ONMLNG,ODFLNG,ITYLNG
0017    	INTEGER ISWTCH,OSWTCH,PROLNG
0018    	INTEGER ERRTYP(3)
0019    	BYTE INPTYP(5),OUTTYP(6)
0020    	BYTE COMLIN(512)
0021    	BYTE INPNAM(42),MARNAM(42),BLINAM(42),TMPNAM(42)
        C
        C	THE NEXT DECLARATION DIMENSIONS THE DEFAULT FILENAME.  THIS VALUE
        C	SHOULD BE CHANGED ACCORDING TO THE NUMBER OF ASCII CHARACTERS
        C	SPECIFIED IN THE DEFAULT NAME.
        C
0022    	BYTE OUTDEF(42),INPDEF(10)
        C
        C	THIS ARRAY MUST BE DIMENSIONED ACCORDING TO THE LENGTH OF
        C	THE USER'S PROMPT.
        C
0023    	BYTE PROMPT(5),NOBYTE
        C
        C
0024    	DATA INPTYP/'I','N','P','U','T'/,ITYLNG/5/
0025    	DATA OUTTYP/'O','U','T','P','U','T'/,OTYLNG/6/
        C
        C	THIS DATA STATEMENT INITIALIZES THE PROMPT STRING AND ITS LENGTH.
        C	THE "X" 'S MAY BE REPLACED WITH ANY PRINTABLE CHARACTER.  THE ">"
        C	AND THE 10 MUST REMAIN.
        C
0026    	DATA PROMPT/10,'M','D','L','>'/PROLNG/5/
        C
        C
        C	THE NEXT DATA STATEMENT REPRESENTS THE DEFAULT FILENAME.
        C	AND ITS?  L LENGTH.  THIS DEFAULT MAY CONSIST OF FILENAME AND/OR
        C	FILE TYPE.  THE TYPE MUST CONSIST OF FOUR CHARACTERS IF SPECIFIED.
        C	THE LENGTH SHOULD BE SET TO SPECIFY THE NUMBER OF CHARACTERS IN
        C	THE STRING.
        C
0027    	DATA INPDEF/'S','Y','S','D','E','F','.','M','D','L'/IDFLNG/10/
        C
        C	THE ERROR TABLE IDENTIFIES AN ERROR TYPE.
        C
0028    	DATA ERRTYP/-1,-2,-3/
FORTRAN IV-PLUS V02-51D		03:13:14    22-AUG-78		PAGE 3   
MDLIO.FTN    	/TR:BLOCKS/WR

         
        C
        C	ASSIGN THE COMMAND LINE INPUT CHANNEL TO THE TERMINAL.
        C	NOTE:  CHANNEL FOUR SHOULD ONLY BE USED FOR COMMAND LINE I/O.
        C
0029    	CALL ASNLUN(4,'TI',0)
        C
        C	REQUEST A COMMAND LINE.
        C
0030    50	CALL GETCML(COMLIN,LENGTH,PROMPT,PROLNG,IERR)
        C	WRITE (5, 1000) (COMLIN (II), II = 1, LENGTH)
0031    1000	FORMAT (' ',<LENGTH>A1)
0032    	IF (IERR.LE.0) GOTO 9500
        C
        C	ANALYZE THE SYNTAX OF THE COMMAND LINE.
        C
0033    	CALL SYNTX(COMLIN,LENGTH,IERR)
0034    	IF (IERR.LE.0) GOTO 9510
        C
        C	REQUEST AN INPUT FILE SPECIFICATION.
        C
0035    	CALL SCANI(COMLIN,LENGTH,ISWTCH,INPNAM,INMLNG,INPDEF,IDFLNG,IERR)
0036    	IF (IERR.LT.0) GOTO 9530
        C
        C	CONSTRUCT AN OUTPUT NAME THAT IS THE SAME AS THE INPUT
        C	NAME EXCEPT THAT THE EXTENSION IS CHANGED TO MAR.
        C
0037    	K = 1
0038    	DO 60 I = 1, 42
0039    	OUTDEF (K) = INPNAM (I)
0040    	IF (INPNAM (I) .EQ. ':') K = 0
0041    	K = K + 1
0042    	IF (INPNAM (I) .EQ. '.') GOTO 70
0043    60	CONTINUE
        C
        C	INSERT THE CORRECT OUTPUT EXTENSION FOR MARS.
        C
0044    70	OUTDEF (K) = 'M'
0045    	OUTDEF (K + 1) = 'A'
0046    	OUTDEF (K + 2) = 'R'
0047    	ODFLNG = K + 2
        C
        C	REQUEST AN OUTPUT FILE SPECIFICATION.
        C
0048    	CALL SCANO(COMLIN,LENGTH,OSWTCH,MARNAM,ONMLNG,OUTDEF,ODFLNG,IERR)
0049    	IF (IERR.LT.0) GOTO 9520
        C
        C	CREATE A NAME FOR THE BLISS OUTPUT FILE THAT IS THE SAME
        C	AS FOR THE MARS OUTPUT FILE EXCEPT THAT THE EXTENSION IS
        C	.B32.
        C
0050    	DO 80 I = 1, 42
0051    	K = I
0052    	BLINAM (I) = MARNAM (I)
0053    	IF (MARNAM (I) .EQ. '.') GOTO 90
0054    80	CONTINUE
FORTRAN IV-PLUS V02-51D		03:13:14    22-AUG-78		PAGE 4   
MDLIO.FTN    	/TR:BLOCKS/WR

        C
        C	NOW CHANGE THE EXTENSION TO B32.
        C
0055    90	BLINAM (K + 1) = 'B'
0056    	BLINAM (K + 2) = '3'
0057    	BLINAM (K + 3) = '2'
0058    	BLINAM (K + 4) = 0
        C
        C	CREATE A NAME FOR THE TMP FILE THAT IS THE SAME AS
        C	FOR THE MARS OUTPUT FILE EXCEPT THAT THE EXTENSION IS .TMP.
        C
0059    	DO 95 I = 1, 42
0060    	K = I
0061    	TMPNAM (I) = MARNAM (I)
0062    	IF (MARNAM (I) .EQ. '.') GOTO 97
0063    95	CONTINUE
        C
        C	NOW CHANGE THE EXTENSION TO .TMP.
        C
0064    97	TMPNAM (K + 1) = 'T'
0065    	TMPNAM (K + 2) = 'M'
0066    	TMPNAM (K + 3) = 'P'
0067    	TMPNAM (K + 4) = 0
        C
        C	OPEN AND PROCESS THE FILES.
        C
0068    	OPEN( UNIT=INP,TYPE='OLD',NAME=INPNAM,
        	1	ACCESS='SEQUENTIAL',READONLY,ERR=500)
0069    	OPEN( UNIT=MAR,TYPE='NEW',NAME=MARNAM,
        	1	ACCESS='SEQUENTIAL',ERR=510)
0070    	OPEN( UNIT=BLI,TYPE='NEW',NAME=BLINAM,
        	1	ACCESS='SEQUENTIAL',ERR=520)
0071    	OPEN( UNIT=TMP,TYPE='SCRATCH',NAME=TMPNAM,
        	1	FORM='UNFORMATTED',ACCESS='SEQUENTIAL',ERR=530)
0072    	ENDFIL = .FALSE.
0073    100	CALL STPROC
0074    	IF (.NOT. ENDFIL) GOTO 100
0075    	CLOSE (UNIT = MAR)
0076    	CLOSE (UNIT = BLI)
0077    	CLOSE (UNIT = INP)
0078    	CLOSE (UNIT = TMP, DISPOSE='DELETE')
0079    	GOTO 50
        C
        C	ERROR OPENING INPUT FILE
        C
0080    500	WRITE (5, 1010)
0081    1010	FORMAT (' ERROR OPENING INPUT FILE')
0082    	RETURN
         
0083    510	WRITE (5, 1020)
0084    1020	FORMAT (' ERROR OPENING MARS OUTPUT FILE')
0085    	RETURN
         
0086    520	WRITE (5, 1030)
0087    1030	FORMAT (' ERROR OPENING BLISS OUTPUT FILE')
0088    	RETURN
FORTRAN IV-PLUS V02-51D		03:13:14    22-AUG-78		PAGE 5   
MDLIO.FTN    	/TR:BLOCKS/WR

         
0089    530	WRITE (5, 1040)
0090    1040	FORMAT (' ERROR OPENING TMP FILE')
0091    	RETURN
FORTRAN IV-PLUS V02-51D		03:13:14    22-AUG-78		PAGE 6   
MDLIO.FTN    	/TR:BLOCKS/WR

         
        C
        C	MACRO ROUTINE ERRORS
        C
        C	GETCML ERRORS
        C
0092    9500	IF (IERR.EQ.0) RETURN
0093    	CALL ERRORS(ERRTYP(1),IERR,PROMPT(2),NOBYTE,NOINTG,NOBYTE,
        	1NOINTG,NOBYTE)
0094    	GOTO 50
        C
        C	SYNTX ERRORS
        C
0095    9510	CALL ERRORS(ERRTYP(2),IERR,PROMPT(2),COMLIN,LENGTH,NOBYTE,
        	1NOINTG,NOBYTE)
0096    	GOTO 50
        C
        C	SCANO/SCANI ERRORS
        C
        C	OUTPUT ERROR
        C
0097    9520	CALL ERRORS(ERRTYP(3),IERR,PROMPT(2),OUTTYP,OTYLNG,NOBYTE,
        	1NOINTG,NOBYTE)
0098    	GOTO 50
        C
        C	INPUT ERROR
        C
0099    9530	CALL ERRORS(ERRTYP(3),IERR,PROMPT(2),INPTYP,ITYLNG,NOBYTE,
        	1NOINTG,NOBYTE)
0100    	GOTO 50
         
0101    	END
