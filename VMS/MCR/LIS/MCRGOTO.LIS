                                     0040    88 ;+
                                     0040    89 ; MCR$GOTO - GOTO LABEL
                                     0040    90 ;
                                     0040    91 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE GOTO LABEL MCR
                                     0040    92 ; COMMAND.
                                     0040    93 ;
                                     0040    94 ;	R8 = ADDRESS OF SCRATCH BUFFER DESCRIPTOR.
                                     0040    95 ;	R9 = ADDRESS OF SCRATCH STACK.
                                     0040    96 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0040    97 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0040    98 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0040    99 ;
                                     0040   100 ; OUTPUTS:
                                     0040   101 ;
                                     0040   102 ;	THE SPECIFIED LABEL IS SEARCHED FOR IN THE CURRENT LABEL TABLE, AND IF
                                     0040   103 ;	FOUND, A RANDOM FIND IS EXECUTED TO POSITION THE INPUT FILE FOR SUBSE-
                                     0040   104 ;	QUENT GET OPERATIONS. OTHERWISE A LABEL TABLE ENTRY IS CONSTRUCTED AND
                                     0040   105 ;	APPROPRIATE STATE INFORMATION IS SET UP SO THAT A FORWARD GOTO SEARCH
                                     0040   106 ;	IS EXECUTED.
                                     0040   107 ;-
                                     0040   108  
                                     0040   109 MCR$GOTO::				; GOTO LABEL
                          E3     10  0040   110 	BSBB	SETGO			; SET UP FOR GOTO
                                     0042   111 					; FALL INTO POSITION
MCRGOTO         - GOTO MCR COMMAND EXECUTION                     22-AUG-1978 01:38:09   VAX-11 MACRO X0.3-11               Page   4
01              POSITION FILE TO GOTO LABEL                                                                                      (1)

                                     0042   113 	.SBTTL	POSITION FILE TO GOTO LABEL
                                     0042   114 ;+
                                     0042   115 ; MCR$POSITION - POSITION FILE TO GOTO LABEL
                                     0042   116 ;
                                     0042   117 ; THIS ROUTINE IS CALLED TO SEARCH THE LABEL TABLE FOR A GOTO LABEL, AND IF
                                     0042   118 ; FOUND POSITION THE CURRENT COMMAND FILE TO THE LABEL.
                                     0042   119 ;
                                     0042   120 ; INPUTS:
                                     0042   121 ;
                                     0042   122 ;	R1 = LENGTH OF GOTO LABEL.
                                     0042   123 ;	R2 = ADDRESS OF GOTO LABEL.
                                     0042   124 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0042   125 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0042   126 ;
                                     0042   127 ;	IT IS ASSUMED THAT CONTROL Y/C AST'S ARE DISABLED.
                                     0042   128 ;
                                     0042   129 ; OUTPUTS:
                                     0042   130 ;
                                     0042   131 ;	THE LABEL TABLE IS SEARCHED FOR THE SPECIFIED GOTO LABEL. IF THE LABEL
                                     0042   132 ;	IS FOUND, THEN THE CURRENT COMMAND FILE IS POSITIONED TO THAT POINT.
                                     0042   133 ;	OTHERWISE A NEW LABEL TABLE ENTRY IS MADE AND A POINTER TO THE GOTO
                                     0042   134 ;	LABEL IS ESTABLISHED IN THE PROCESS WORK AREA.
                                     0042   135 ;-
                                     0042   136  
                                     0042   137 MCR$POSITION::				; POSITION FILE TO GOTO LABEL
            50         30 AB     9E  0042   138 	MOVAB	PRC_Q_LABEL(R11),R0	; GET ADDRESS OF LABEL TABLE LISTHEAD
                                     0046   139 	SEARCHT				; SEARCH TABLE FOR LABEL
            22            50     E8  0049   140 	BLBS	R0,30$			; IF LBS LABEL ENTRY FOUND
            54            51     7D  004C   141 	MOVQ	R1,R4			; SAVE SAVE AND ADDRESS OF LABEL
            51            09     C0  004F   142 	ADDL	#2+7,R1			; ADD IN FOR LABEL SIZE AND BLOCK SIZE
                          FFAB'  30  0052   143 	BSBW	MCR$ALLDYNMEM		; ALLOCATE DYNAMIC MEMORY
            15            50     E9  0055   144 	BLBC	R0,20$			; BR IF FAILED
            82            51     90  0058   145 	MOVB	R1,(R2)+		; SAVE SIZE OF BLOCK IN BLOCK
         4C AB            62     9E  005B   146 	MOVAB	(R2),PRC_L_SYMBOL(R11)	; SAVE TARGET FOR GOTO
            82            54     90  005F   147 	MOVB	R4,(R2)+		; SET SIZE OF LABEL STRING
            65            54     28  0062   148 	MOVC	R4,(R5),(R2)		; FILL IN LABEL NAME
                          62         0065       
         54 AB            10     A8  0066   149 	BISW	#PRC_M_GOTO,PRC_W_FLAGS(R11) ; SET FORWARD GOTO IN PROGRESS
                                     006A   150 10$:	STATUS	NORMAL			; SET NORMAL COMPLETION
                                 05  006D   151 20$:	RSB				; 
                                     006E   152  
                                     006E   153 ;
                                     006E   154 ; LABEL ALREADY DEFINED - POINT TO PROPER RECORD
                                     006E   155 ;
                                     006E   156  
            58         14 AB     D0  006E   157 30$:	MOVL	PRC_L_INDINPRAB(R11),R8	; GET ADDRESS OF INDIRECT FILE RAB
         0E A341          06     28  0072   158 	MOVC	#6,SYM_T_SYMBOL+2(R3)[R1],RAB$W_RFA(R8) ; RESTORE RECORD FILE ADDRESS
                       10 A8         0077       
         1E A8            02     90  0079   159 FIND:	MOVB	#RAB$C_RFA,RAB$B_RAC(R8) ; SET ACCESS MODE FOR RANDOM FIND
                                     007D   160 	$FIND	RAB=(R8)		; FIND RECORD THAT HAS LABEL
         1E A8            00     90  0086   161 	MOVB	#RAB$C_SEQ,RAB$B_RAC(R8) ; RESET TO SEQUENTIAL ACCESS MODE
                                 05  008A   162 	RSB				; 
                                     008B   163 	.DSABL	LSB
