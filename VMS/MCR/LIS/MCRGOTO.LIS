                                     0000    37 	.SBTTL	GOSUB LABEL
                                     0000    38 ;+
                                     0000    39 ; MCR$GOSUB - GOSUB LABEL
                                     0000    40 ;
                                     0000    41 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE GOSUB LABEL MCR
                                     0000    42 ; COMMAND.
                                     0000    43 ;
                                     0000    44 ;	R8 = ADDRESS OF SCRATCH BUFFER DESCRIPTOR.
                                     0000    45 ;	R9 = ADDRESS OF SCRATCH STACK.
                                     0000    46 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0000    47 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0000    48 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0000    49 ;
                                     0000    50 ; OUTPUTS:
                                     0000    51 ;
                                     0000    52 ;	THE SPECIFIED LABEL IS SEARCHED FOR IN THE CURRENT LABEL TABLE, AND IF
                                     0000    53 ;	FOUND, A RANDOM FIND IS EXECUTED TO POSITION THE INPUT FILE FOR SUBSE-
                                     0000    54 ;	QUENT GET OPERATIONS. OTHERWISE A LABEL TABLE ENTRY IS CONSTRUCTED AND
                                     0000    55 ;	APPROPRIATE STATE INFORMATION IS SET UP SO THAT A FORWARD GOSUB SEARCH
                                     0000    56 ;	IS EXECUTED.
                                     0000    57 ;-
                                     0000    58  
                                 00000000    59 	.PSECT	MCR$ZCODE,BYTE,RD,NOWRT
                                     0000    60 MCR$GOSUB::				; GO TO A COMMMAND SUBROUTINE
                          23     10  0000    61 	BSBB	SETGO			; SET UP THE GOTO
       0084 CB            08     C3  0002    62 	SUBL3	#8,PRC_L_STACKPT(R11),R0 ; ALLOCATE STACK SPACE FOR RETURN
                          50         0007       
       0088 CB            50     D1  0008    63 	CMPL	R0,PRC_L_STACKLM(R11)	; DID STACK OVERFLOW
                          11     1B  000D    64 	BLEQU	10$			; BR IF YES
       0084 CB            50     D0  000F    65 	MOVL	R0,PRC_L_STACKPT(R11)	; SET NEW STACK POINTER
            53         14 AB     D0  0014    66 	MOVL	PRC_L_INDINPRAB(R11),R3	; GET INPUT RAB
            60         0E A3     7D  0018    67 	MOVQ	RAB$W_RFA-2(R3),(R0)	; SET RECORD FILE ADDRESS IN STACK
                          60     B4  001C    68 	CLRW	(R0)			; ZERO 2 BYTES = RETURN ADDRESS
                          22     11  001E    69 	BRB	MCR$POSITION		; POSITION THE FILE TO LABEL
                                     0020    70 10$:	STATUS	STKOVF			; INDICATE NO SPACE FOR RETURN
                                 05  0024    71 	RSB				; 
                                     0025    72 ;
                                     0025    73 ; LOCAL SUBROUTINE TO SET UP FOR GOTO/GOSUB
                                     0025    74 ;
                                     0025    75 	.ENABL	LSB			;
                                     0025    76  
         54 AB            0B     E0  0025    77 SETGO:	BBS	#PRC_V_YLEVEL,PRC_W_FLAGS(R11),5$ ; IF SET, CONTROL Y/C LEVEL
                          12         0029       
            50         14 AB     D0  002A    78 	MOVL	PRC_L_INDINPRAB(R11),R0	;GET ADDRESS OF CURRENT INPUT RAB
         18 A0            1C     E1  002E    79 	BBC	#DEV$V_RND,RAB$L_CTX(R0),5$ ; IF CLR, NOT RANDOM ACCESS DEVICE
                          09         0032       
                                     0033    80 	GETOKEN				; GET SYMBOL NAME
                                     0036    81 	DISABLE				; DISABLE CONTROL Y/C AST'S
                          8E     7C  0039    82 	CLRQ	(SP)+			; REMOVE RETURN INFORMATION FROM STACK
                                 05  003B    83 	RSB
                          8E     D5  003C    84 5$:	TSTL	(SP)+			; CLEAR LOCAL SUBROUTINE
                          2A     11  003E    85 	BRB	10$			; RETURN
