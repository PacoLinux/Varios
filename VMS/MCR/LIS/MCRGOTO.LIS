01              DEALLOCTE GOTO CONTROL BLOCK                                                                                     (1)

                                     008B   165 	.SBTTL	DEALLOCTE GOTO CONTROL BLOCK
                                     008B   166 ;+
                                     008B   167 ; MCR$DEALGOTO-DEALLOCATE GOTO CONTROL BLOCK
                                     008B   168 ;
                                     008B   169 ; THIS ROUTINE IS CALLED TO DEALLOCATE AN OUTSTANDING GOTO CONTROL
                                     008B   170 ; BLOCK WHEN IT IS SATISFIED OR AN END OF FILE IS REACHED WITHOUT
                                     008B   171 ; FINDING THE LABEL.
                                     008B   172 ;
                                     008B   173 ;-
                                     008B   174 MCR$DEALGOTO::				; 
            50         4C AB     D0  008B   175 	MOVL	PRC_L_SYMBOL(R11),R0	; ADDRESS OF GOTO CONTROL BLOCK
            51            70     9A  008F   176 	MOVZBL	-(R0),R1		; GET LENGTH AND POINT AT START OF BLOCK
                          FF6B'  31  0092   177 	BRW	MCR$DEADYNMEM		; DEALLOCATE THE BLOCK
MCRGOTO         - GOTO MCR COMMAND EXECUTION                     22-AUG-1978 01:38:09   VAX-11 MACRO X0.3-11               Page   6
01              RETURN FROM SUBROUTINE                                                                                           (1)

                                     0095   179 	.SBTTL	RETURN FROM SUBROUTINE
                                     0095   180 ;+
                                     0095   181 ; MCR$RETURN - RETURN FROM SUBROUTINE
                                     0095   182 ;
                                     0095   183 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE RETURN MCR
                                     0095   184 ; COMMAND.
                                     0095   185 ;
                                     0095   186 ;	R8 = ADDRESS OF SCRATCH BUFFER DESCRIPTOR.
                                     0095   187 ;	R9 = ADDRESS OF SCRATCH STACK.
                                     0095   188 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0095   189 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0095   190 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0095   191 ;
                                     0095   192 ; OUTPUTS:
                                     0095   193 ;
                                     0095   194 ;	THE MOST RECENT SUBROUTINE CALL IS POPPED FROM THE INDIRECT STACK
                                     0095   195 ;	AND A RETURN TO THAT POINT IS MADE.
                                     0095   196 ;-
                                     0095   197 MCR$RETURN::				; RETURN FORM SUBROUTINE
            53       0084 CB     D0  0095   198 	MOVL	PRC_L_STACKPT(R11),R3	; SET TOP OF STACK
                          83     B5  009A   199 	TSTW	(R3)+			; IS TOP A SUBROUTINE?
                          1A     12  009C   200 	BNEQ	20$			; BR IF NO
            58         14 AB     D0  009E   201 	MOVL	PRC_L_INDINPRAB(R11),R8	; GET THE CURRENT INPUT RAB
         10 A8            83     D0  00A2   202 	MOVL	(R3)+,RAB$W_RFA(R8)	; SET 4 BYTES OF RECORD FILE ADDRESS
         14 A8            83     B0  00A6   203 	MOVW	(R3)+,RAB$W_RFA4(R8)	; AND THE REMAINING 2 BYTES
       0084 CB            53     D0  00AA   204 	MOVL	R3,PRC_L_STACKPT(R11)	; SET NEW TOP OF STACK
                          C8     10  00AF   205 	BSBB	FIND			; POSITION
                                     00B1   206 	FORNBLK				; READ THE SUBOUTINE CALL LINE
                                     00B4   207 	STATUS	NORMAL			; SET GOOD RETURN
                                 05  00B7   208 	RSB				;
                                     00B8   209 20$:	STATUS	BADRET			; BAD RETURN
                                 05  00BC   210 	RSB
                                     00BD   211  
                                     00BD   212 	.END
