                                     0000    36 ;+
                                     0000    37 ; MCR$PROCFILE - PROCESS FILE SPECIFICATION.
                                     0000    38 ;
                                     0000    39 ; THIS ROUTINE IS CALLED TO SCAN A FILE SPECIFICATION FOR SYNTACTIC CORRECT-
                                     0000    40 ; NESS AND TO EMIT A FILE DESCRIPTOR TO THE RESULT PARSE TABLE.
                                     0000    41 ;
                                     0000    42 ; INPUTS:
                                     0000    43 ;
                                     0000    44 ;	R9 = ADDRESS OF NEXT BYTE IN COMMAND BUFFER.
                                     0000    45 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0000    46 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0000    47 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0000    48 ;
                                     0000    49 ; OUTPUTS:
                                     0000    50 ;
                                     0000    51 ;	R0 LOW BIT CLEAR INDICATES A FAILURE TO PARSE FILE SPECIFICATION.
                                     0000    52 ;
                                     0000    53 ;		R0 = MCR$_DIRECT - INVALID DIRECTORY SYNTAX.
                                     0000    54 ;		R0 = MCR$_NUMBER - INVALID GROUP, MEMBER, OR VERSION NUMBER.
                                     0000    55 ;		R0 = MCR$_NULFIL - NULL FILE SPECIFICATION.
                                     0000    56 ;
                                     0000    57 ;	R0 LOW BIT SET INDICATES A SUCCESSFUL PARSE WITH THE FILE DESCRIPTOR
                                     0000    58 ;		EMITTED AND THE FILE SPECIFICATION COPIED TO THE COMMAND
                                     0000    59 ;		BUFFER.
                                     0000    60 ;
                                     0000    61 ;		R0 = MCR$_NORMAL - NORMAL COMPLETION.
                                     0000    62 ;-
                                     0000    63  
