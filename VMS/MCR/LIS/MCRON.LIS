                                     00B7   163 	.SBTTL	RESET ON ERROR PARAMETERS
                                     00B7   164 ;+
                                     00B7   165 ; MCR$ONRESET - RESET ON ERROR PARAMETERS
                                     00B7   166 ;
                                     00B7   167 ; THIS ROUTINE IS CALLED TO RESET THE ON ERROR PARAMETERS TO THEIR DEFAULT
                                     00B7   168 ; VALUES.
                                     00B7   169 ;
                                     00B7   170 ; INPUTS:
                                     00B7   171 ;
                                     00B7   172 ;	NONE.
                                     00B7   173 ;
                                     00B7   174 ;	IT IS ASSUMED THAT CONTROL Y/C AST'S ARE DISABLED.
                                     00B7   175 ;
                                     00B7   176 ; OUTPUTS:
                                     00B7   177 ;
                                     00B7   178 ;	IF AN ON ERROR COMMAND BUFFER IS CURRENTLY ALLOCATED, THEN IT IS RETURNED
                                     00B7   179 ;	TO THE DYNAMIC STORAGE REGION. THE ON ERROR LEVEL IS SET TO 'ERROR' AND
                                     00B7   180 ;	THE ON ERROR COMMAND TEXT POINTER IS CLEARED.
                                     00B7   181 ;-
                                     00B7   182  
                                     00B7   183 MCR$ONRESET::				;RESET ON ERROR PARAMETERS
            50         58 AB     D0  00B7   184 	MOVL	PRC_L_ONERROR(R11),R0	;GET ADDRESS OF ON ERROR COMMAND TEXT
                          06     13  00BB   185 	BEQL	10$			;IF EQL NONE
            51            60     9A  00BD   186 	MOVZBL	(R0),R1			;GET LENGTH OF ON ERROR COMMAND TEXT
                          FF3D'  30  00C0   187 	BSBW	MCR$DEADYNMEM		;DEALLOCTE THE MEMORY
                       58 AB     D4  00C3   188 10$:	CLRL	PRC_L_ONERROR(R11)	;CLEAR ADDRESS OF ON ERROR TEXT
         56 AB            02     B0  00C6   189 	MOVW	#2,PRC_W_ONLEVEL(R11)	;RESET ON ERROR LEVEL TO ERROR
                                 05  00CA   190 	RSB				;
                                     00CB   191  
                                     00CB   192 	.END
