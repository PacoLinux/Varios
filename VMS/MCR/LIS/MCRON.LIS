                                     000F    46 ; MCR$ON - ON ERROR
                                     000F    47 ;
                                     000F    48 ; THIS ROUTINE IS CALLED AS  AN INTERNAL COMMAND TO EXECUTE THE ON MCR COMMAND.
                                     000F    49 ;
                                     000F    50 ; INPUTS:
                                     000F    51 ;
                                     000F    52 ;	R6 = COMMAND BUFFER DISPLAY PARAMETER.
                                     000F    53 ;	R9 = ADDRESS OF NEXT BYTE IN COMMAND BUFFER.
                                     000F    54 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     000F    55 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     000F    56 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     000F    57 ;
                                     000F    58 ; OUTPUTS:
                                     000F    59 ;
                                     000F    60 ;	THE ON CLAUSE IS PARSED AND THE CORRESPONDING ERROR LEVEL IS COMPUTED.
                                     000F    61 ;	STORAGE IS ALLOCATED FOR THE THEN CLAUSE AND IT IS COPIED FOR SUBSEQUENT
                                     000F    62 ;	REFERENCE WHEN AN ON CONDITION ACTUALLY EXISTS.
                                     000F    63 ;
                                     000F    64 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO ESTABLISH ON CONDITION PARAMETERS.
                                     000F    65 ;
                                     000F    66 ;		R0 = MCR$_ONERR - INVALID ERROR LEVEL SPECIFIED.
                                     000F    67 ;		R0 = MCR$_ONLEVL - ON CONDITION CAN NOT BE ESTABLISHED AT CURRENT
                                     000F    68 ;			COMMAND LEVEL.
                                     000F    69 ;		R0 = MCR$ONOVF - NO ROOM FOR ON CONDITION COMMAND TEXT.
                                     000F    70 ;		R0 = MCR$_PARMDEL - INVALID PARAMETER DELIMITER.
                                     000F    71 ;
                                     000F    72 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     000F    73 ;
                                     000F    74 ;		R0 = MCR$NORMAL - NORMAL COMPLETION.
                                     000F    75 ;-
                                     000F    76  
                                     000F    77 MCR$ON::				;ON ERROR
            56            59     D0  000F    78 	MOVL	R9,R6			;SAVE COMMAND BUFFER BACKUP PARAMETER
                                     0012    79 	SETCHAR				;PEEK AT NEXT CHARACTER IN INPUT BUFFER
                          0084   30  0015    80 	BSBW	90$			;CHECK FOR BLANK TERMINATOR
                          007E   30  0018    81 	BSBW	80$			;MOVE TERMINATOR, GET TOKEN, AND CHECK
            51            04     D1  001B    82 	CMPL	#4,R1			;ERROR LEVEL LONGER THAN 4 CHARACTERS
                          03     18  001E    83 	BGEQ	10$			;IF GEQ NO
            51            04     D0  0020    84 	MOVL	#4,R1			;SET LENGTH OF ERROR LEVEL TO 4
            55         DA AF     9E  0023    85 10$:	MOVAB	LEVELTBL,R5		;GET ADDRESS OF ERROR LEVEL NAME TABLE
            57            03     D0  0027    86 	MOVL	#<LEVELEND-LEVELTBL>/5,R7 ;SET LOOP COUNT
            53            51     7D  002A    87 20$:	MOVQ	R1,R3			;COPY ERROR LEVEL NAME PARAMETERS
            5A            85     9A  002D    88 	MOVZBL	(R5)+,R10		;SAVE ERROR LEVEL NUMBER
            50            55     D0  0030    89 	MOVL	R5,R0			;SET ADDRESS OF ERROR LEVEL NAME
                          85     D5  0033    90 	TSTL	(R5)+			;POINT TO NEXT ENTRY IN TABLE
            84            80     91  0035    91 30$:	CMPB	(R0)+,(R4)+		;CHARACTERS MATCH?
                          05     12  0038    92 	BNEQ	40$			;IF NEQ NO
            F8            53     F5  003A    93 	SOBGTR	R3,30$			;ANY MORE CHARACTERS TO COMPARE?
                          07     11  003D    94 	BRB	50$			;
            E8            57     F5  003F    95 40$:	SOBGTR	R7,20$			;ANY MORE ENTRIES TO COMPARE?
                                     0042    96 	STATUS	ONERR			;SET INVALID ERROR LEVEL SPECIFIED STATUS
                                 05  0045    97 	RSB				;
                                     0046    98  
                                     0046    99 ;
                                     0046   100 ; LEGAL ERROR LEVEL - PARSE THEN CLAUSE
