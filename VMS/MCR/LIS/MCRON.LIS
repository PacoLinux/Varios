            66            59     28  008D   125 	MOVC	R9,(R6),(R2)		;MOVE TEXT TO ON ERROR BUFFER
                          62         0090       
         56 AB            5A     B0  0091   126 	MOVW	R10,PRC_W_ONLEVEL(R11)	;SAVE ON ERROR LEVEL
                                     0095   127 	STATUS	NORMAL			;SET NORMAL COMPLETION STATUS
