                                     0099   129  
                                     0099   130 ;
                                     0099   131 ; MOVE TERMINATOR, GET TOKEN, AND CHECK FOR BLANK TERMINATOR
                                     0099   132 ;
                                     0099   133  
                                     0099   134 80$:	MOVTOKN				;MOVE TERMINATOR AND GET TOKEN
            50            20     91  009C   135 90$:	CMPB	#^A/ /,R0		;BLANK TERMINATOR?
                          01     12  009F   136 	BNEQ	100$			;IF NEQ NO
                                 05  00A1   137 	RSB				;
                          8E     D5  00A2   138 100$:	TSTL	(SP)+			;CLEAN STACK
                                     00A4   139 	STATUS	PARMDEL			;SET INVALID PARAMETER DELIMITER
                                 05  00A8   140 	RSB				;
                                     00A9   141  
                                     00A9   142 ;
                                     00A9   143 ; INVALID ON CONDITION LEVEL
                                     00A9   144 ;
                                     00A9   145  
                                     00A9   146 110$:	STATUS	ONLEVL			;SET INVALID ON CONDITION LEVEL STATUS
                                 05  00AD   147 	RSB				;
                                     00AE   148  
                                     00AE   149 ;
                                     00AE   150 ; NO ROOM TO ALLOCATE ON COMMAND TEXT
                                     00AE   151 ;
                                     00AE   152  
                                     00AE   153 120$:	STATUS	ONOVF			;SET NO ROOM FOR COMMAND TEXT STATUS
                                 05  00B2   154 	RSB				;
