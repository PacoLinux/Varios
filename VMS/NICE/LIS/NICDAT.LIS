;	1139		   WORD('XA'), BYTE(0), BYTE(2), UPLIT BYTE('DA'),	!  8
;	1140		   WORD('XW'), BYTE(0), BYTE(3), UPLIT BYTE('DUP'),	! 10
;	1141		   WORD('XM'), BYTE(0), BYTE(3), UPLIT BYTE('DMC'),	! 12
;	1142		   WORD('XN'), BYTE(0), BYTE(2), UPLIT BYTE('DN'),	! 14
;	1143		   WORD('X@'), BYTE(0), BYTE(3), UPLIT BYTE('DLV'),	! 16
;	1144		   WORD('X@'), BYTE(0), BYTE(2), UPLIT BYTE('KL'),	! 18
;	1145		   WORD('X@'), BYTE(0), BYTE(3), UPLIT BYTE('DTE'),	! 20
