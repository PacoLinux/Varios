TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   3
X0.1-3          TST$CHECK_SS - CHECK SYSTEM SERVICE STATUS CODE                                                                  (3)

                                     0000    60 	.SBTTL	TST$CHECK_SS - CHECK SYSTEM SERVICE STATUS CODE
                                 00000000    61 	.PSECT	TST$CODE	NOWRT
                                     0000    62 C::					; SYMBOL FOR DEBUGGING PURPOSES
                                     0000    63 
                                     0000    64 ;++
                                     0000    65 ; FUNCTIONAL DESCRIPTION:
                                     0000    66 ;
                                     0000    67 ;	TST$CHECK_SS CHECKS THE STATUS CODE IN R0 FOLLOWING A CALL TO A
                                     0000    68 ;	SYSTEM SERVICE. IF FAILURE (EXCEPT AS NOTED BELOW) IS INDICATED
                                     0000    69 ;	THE IMAGE IS TERMINATED WITH R0 AS THE EXIT COMPLETION CODE.
                                     0000    70 ;
                                     0000    71 ; CALLING SEQUENCE:
                                     0000    72 ;
                                     0000    73 ;	BSB/JSB	TST$CHECK_SS
                                     0000    74 ;
                                     0000    75 ; INPUT PARAMETERS:
                                     0000    76 ;
                                     0000    77 ;	R0	SYSTEM SERVICE STATUS CODE
                                     0000    78 ;
                                     0000    79 ; IMPLICIT INPUTS:
                                     0000    80 ;
                                     0000    81 ;	NONE
                                     0000    82 ;
                                     0000    83 ; OUTPUT PARAMETERS:
                                     0000    84 ;
                                     0000    85 ;	R1	TST$CHECK_SS COMPLETION CODE
                                     0000    86 ;
                                     0000    87 ; IMPLICIT OUTPUTS:
                                     0000    88 ;
                                     0000    89 ;	NONE
                                     0000    90 ;
                                     0000    91 ; COMPLETION CODES:
                                     0000    92 ;
                                     0000    93 ;	R1	0 = STATUS CODE IS ABORT (SS$_ABORT) OR
                                     0000    94 ;		    STATUS CODE IS CANCEL (SS$_CANCEL) OR
                                     0000    95 ;		    STATUS CODE IS REJECT (SS$_REJECT) OR
                                     0000    96 ;		    STATUS CODE IS FILE NOT ACCESSED (SS$_FILNOTACC)
                                     0000    97 ;		1 = SUCCESS
                                     0000    98 ;
                                     0000    99 ; SIDE EFFECTS:
                                     0000   100 ;
                                     0000   101 ;	IF THE STATUS CODE INDICATES FAILURE (EXCEPT AS NOTED ABOVE),
                                     0000   102 ;	THE IMAGE IS TERMINATED WITH THE STATUS CODE AS THE EXIT
                                     0000   103 ;	COMPLETION CODE.
                                     0000   104 ;
                                     0000   105 ;--
                                     0000   106 
                                     0000   107 TST$CHECK_SS::				; CONTROL POINT
            51            01     D0  0000   108 	MOVL	#1,R1			; SET RETURN CODE TO SUCCESS
            25            50     E8  0003   109 	BLBS	R0,20$			; WAS SYSTEM SERVICE SUCCESSFUL?
            2C            50     B1  0006   110 	CMPW	R0,#<SS$_ABORT&^XFFFF>	; NO, CHECK FOR ABORTED I/O
                          1E     13  0009   111 	BEQLU	10$			; NON-FATAL IF ABORTED
       0830 8F            50     B1  000B   112 	CMPW	R0,#<SS$_CANCEL&^XFFFF> ; NO, CHECK FOR CANCELLED I/O
                          17     13  0010   113 	BEQLU	10$			; NON-FATAL IF CANCELLED
       0294 8F            50     B1  0012   114 	CMPW	R0,#<SS$_REJECT&^XFFFF> ; NO, CHECK FOR CONNECT REJECTED
                          10     13  0017   115 	BEQLU	10$			; NON-FATAL IF CONNECT REJECTED
       00AC 8F            50     B1  0019   116 	CMPW	R0,#<SS$_FILNOTACC&^XFFFF> ; NO, CHECK FOR FILE NOT ACCESSED
TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   4
X0.1-3          TST$CHECK_SS - CHECK SYSTEM SERVICE STATUS CODE                                                                  (3)

                          09     13  001E   117 	BEQLU	10$			; OCCURS IF DTR HAS EXITED
                                     0020   118 	$EXIT_S	R0			; TERMINATE THE IMAGE!!
                          51     D4  0029   119 10$:	CLRL	R1			; SET RETURN CODE TO FAILURE
                                 05  002B   120 20$:	RSB				; EXIT
TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   5
X0.1-3          TST$CHECK_RMS - CHECK RMS COMPLETION CODE                                                                        (4)

                                     002C   122 	.SBTTL	TST$CHECK_RMS - CHECK RMS COMPLETION CODE
                                 0000002C   123 	.PSECT	TST$CODE	NOWRT
                                     002C   124 
                                     002C   125 ;++
                                     002C   126 ; FUNCTIONAL DESCRIPTION:
                                     002C   127 ;
                                     002C   128 ;	TST$CHECK_RMS CHECKS THE COMPLETION CODE IN R0 FOLLOWING A CALL
                                     002C   129 ;	TO RMS. IF FAILURE (EXCEPT AS NOTED BELOW) IS INDICATED
                                     002C   130 ;	THE IMAGE IS TERMINATED WITH R0 AS THE EXIT COMPLETION CODE.
                                     002C   131 ;
                                     002C   132 ; CALLING SEQUENCE:
                                     002C   133 ;
                                     002C   134 ;	BSB/JSB	TST$CHECK_RMS
                                     002C   135 ;
                                     002C   136 ; INPUT PARAMETERS:
                                     002C   137 ;
                                     002C   138 ;	R0	RMS COMPLETION CODE
                                     002C   139 ;
                                     002C   140 ; IMPLICIT INPUTS:
                                     002C   141 ;
                                     002C   142 ;	NONE
                                     002C   143 ;
                                     002C   144 ; OUTPUT PARAMETERS:
                                     002C   145 ;
                                     002C   146 ;	R1	TST$CHECK_RMS COMPLETION CODE
                                     002C   147 ;
                                     002C   148 ; IMPLICIT OUTPUTS:
                                     002C   149 ;
                                     002C   150 ;	NONE
                                     002C   151 ;
                                     002C   152 ; COMPLETION CODES:
                                     002C   153 ;
                                     002C   154 ;	R1	0 = RMS COMPLETION CODE IS END-OF-FILE (RMS$_EOF) OR
                                     002C   155 ;		    RMS COMPLETION CODE IS TIME-OUT (RMS$_TMO)
                                     002C   156 ;		1 = SUCCESS
                                     002C   157 ;
                                     002C   158 ; SIDE EFFECTS:
                                     002C   159 ;
                                     002C   160 ;	IF THE RMS COMPLETION CODE INDICATES FAILURE (EXCPET AS NOTED
                                     002C   161 ;	ABOVE) THE IMAGE IS TERMINATED WITH R0 AS THE EXIT COMPLETION CODE.
                                     002C   162 ;
                                     002C   163 ;--
                                     002C   164 
                                     002C   165 TST$CHECK_RMS::				; CONTROL POINT
            51            01     D0  002C   166 	MOVL	#1,R1			; SET RETURN CODE TO SUCCESS
            19            50     E8  002F   167 	BLBS	R0,20$			; WAS RMS FUNCTION SUCCESSFUL?
       827A 8F            50     B1  0032   168 	CMPW	R0,#<RMS$_EOF&^XFFFF>	; NO, CHECK FOR END-OF-FILE
                          10     13  0037   169 	BEQLU	10$			; NON-FATAL IF END-OF-FILE
       81B0 8F            50     B1  0039   170 	CMPW	R0,#<RMS$_TMO&^XFFFF>	; NO, CHECK FOR TIME-OUT
                          09     13  003E   171 	BEQLU	10$			; NON-FATAL IF TIME-OUT
                                     0040   172 	$EXIT_S	R0			; TERMINATE THE IMAGE!!
                          51     D4  0049   173 10$:	CLRL	R1			; SET RETURN CODE TO FAILURE
                                 05  004B   174 20$:	RSB				; EXIT
TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   6
X0.1-3          TST$CHECK_IOSB - CHECK I/O STATUS BLOCK CODE                                                                     (5)

                                     004C   176 	.SBTTL	TST$CHECK_IOSB - CHECK I/O STATUS BLOCK CODE
                                 0000004C   177 	.PSECT	TST$CODE	NOWRT
                                     004C   178 
                                     004C   179 ;++
                                     004C   180 ; FUNCTIONAL DESCRIPTION:
                                     004C   181 ;
                                     004C   182 ;	TST$CHECK_IOSB CHECKS THE STATUS CODE IN THE SPECIFIED I/O STATUS
                                     004C   183 ;	BLOCK FOLLOWING A CALL TO THE QIO SYSTEM SERVICE. IF FAILURE
                                     004C   184 ;	(EXCPET AS NOTED BELOW) IS INDICATED, THE IMAGE IS TERMINATED
                                     004C   185 ;	WITH THE I/O STATUS CODE AS THE EXIT COMPLETION CODE.
                                     004C   186 ;
                                     004C   187 ; CALLING SEQUENCE:
                                     004C   188 ;
                                     004C   189 ;	BSB/JSB	TST$CHECK_IOSB
                                     004C   190 ;
                                     004C   191 ; INPUT PARAMETERS:
                                     004C   192 ;
                                     004C   193 ;	R0	ADDRESS OF IOSB TO EXAMINE
                                     004C   194 ;
                                     004C   195 ; IMPLICIT INPUTS:
                                     004C   196 ;
                                     004C   197 ;	NONE
                                     004C   198 ;
                                     004C   199 ; OUTPUT PARAMETERS:
                                     004C   200 ;
                                     004C   201 ;	R0	I/O STATUS CODE FROM IOSB
                                     004C   202 ;	R1	TST$CHECK_IOSB COMPLETION CODE
                                     004C   203 ;	R2	# BYTES TRANSFERRED FROM IOSB
                                     004C   204 ;
                                     004C   205 ; IMPLICIT OUTPUTS:
                                     004C   206 ;
                                     004C   207 ;	NONE
                                     004C   208 ;
                                     004C   209 ; COMPLETION CODES:
                                     004C   210 ;
                                     004C   211 ;	R1	0 = I/O STATUS CODE IS ABORT (SS$_ABORT) OR
                                     004C   212 ;		    STATUS CODE IS CANCEL (SS$_CANCEL) OR
                                     004C   213 ;		    STATUS CODE IS REJECT (SS$_REJECT) OR
                                     004C   214 ;		    STATUS CODE IS FILE NOT ACCESSED (SS$_FILNOTACC)
                                     004C   215 ;		1 = SUCCESS
                                     004C   216 ;
                                     004C   217 ; SIDE EFFECTS:
                                     004C   218 ;
                                     004C   219 ;	IF THE I/O STATUS CODE INDICATES FAILURE (EXCEPT AS NOTED ABOVE),
                                     004C   220 ;	THE IMAGE IS TERMINATED WITH THE STATUS CODE AS THE EXIT
                                     004C   221 ;	COMPLETION CODE.
                                     004C   222 ;
                                     004C   223 ;--
                                     004C   224 
                                     004C   225 TST$CHECK_IOSB::			; CONTROL POINT
            52         02 A0     3C  004C   226 	MOVZWL	2(R0),R2		; EXTRACT BYTE COUNT
            50            60     3C  0050   227 	MOVZWL	(R0),R0			; EXTRACT I/O STATUS CODE
                          AB     11  0053   228 	BRB	TST$CHECK_SS		; CHECK I/O STATUS CODE
TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   7
X0.1-3          TST$QIOW -  NETWORK QIO ROUTINES                                                                                 (6)

                                     0055   230 	.SBTTL	TST$QIOW -  NETWORK QIO ROUTINES
                                 00000055   231 	.PSECT	TST$CODE	NOWRT
                                     0055   232 
                                     0055   233 ;++
                                     0055   234 ; FUNCTIONAL DESCRIPTION:
                                     0055   235 ;
                                     0055   236 ;	BOTH TST$QIOW AND TST$QIOAST COMPLETE BUILDING A QIO PARAMETER
                                     0055   237 ;	BLOCK AND ISSUE A QIO REQUEST FOR THE ESTABLISHED COMMUNICATIONS
                                     0055   238 ;	LINK OR FOR THE ASSOCIATED MAILBOX. THE FUNCTION CODE PARAMETER
                                     0055   239 ;	DETERMINES WHICH OF SEVERAL QIO PARAMETER BLOCKS IS USED.
                                     0055   240 ;	TST$QIOW ISSUES A $QIOW_G REQUEST AND TST$QIOAST ISSUES A
                                     0055   241 ;	$QIO_G WITH AST REQUEST.
                                     0055   242 ;
                                     0055   243 ; CALLING SEQUENCE:
                                     0055   244 ;
                                     0055   245 ;	BSB/JSB	TST$QIOW
                                     0055   246 ;	BSB/JSB	TST$QIOAST
                                     0055   247 ;
                                     0055   248 ; INPUT PARAMETERS:
                                     0055   249 ;
                                     0055   250 ;	R2	INTERNAL FUNCTION CODE; ALSO SPECIFIES EFN TO USE
                                     0055   251 ;	R3	P1 PARAMETER; NOTE: NOT IMPLEMENTED AT PRESENT
                                     0055   252 ;	R4	P2 PARAMETER
                                     0055   253 ;	R5	ADDRESS OF AST ROUTITNE (FOR TST$QIOAST ONLY)
                                     0055   254 ;
                                     0055   255 ; IMPLICIT INPUTS:
                                     0055   256 ;
                                     0055   257 ;	SEVERAL CONTIGUOUS QIO PARAMETER BLOCKS BEGINNING AT TST$PARAMETER.
                                     0055   258 ;
                                     0055   259 ; OUTPUT PARAMETERS:
                                     0055   260 ;
                                     0055   261 ;	R0-R1	DESTROYED
                                     0055   262 ;
                                     0055   263 ; IMPLICIT OUTPUTS:
                                     0055   264 ;
                                     0055   265 ;	REFERENCED QIO PARAMETER BLOCK (OFFSET FROM TST$PARAMETER) IS
                                     0055   266 ;	MODIFIED.
                                     0055   267 ;
                                     0055   268 ; COMPLETION CODES:
                                     0055   269 ;
                                     0055   270 ;	NONE
                                     0055   271 ;
                                     0055   272 ; SIDE EFFECTS:
                                     0055   273 ;
                                     0055   274 ;	ON COMPLETION OF THE QIO ISSUED BY TST$QIOAST, AN AST ROUTINE
                                     0055   275 ;	WILL BE EXECUTED.
                                     0055   276 ;
                                     0055   277 ;--
                                     0055   278 
                                     0055   279 ;
                                     0055   280 ; QIO AND WAIT ROUTINE
                                     0055   281 ;
                                     0055   282 
                                     0055   283 TST$QIOW::				; CONTROL POINT
                          23     10  0055   284 	BSBB	QIO_COMMON		; EXECUTE COMMON SET-UP CODE
                       14 A0     7C  0057   285 	CLRQ	QIO$_ASTADR(R0)		; ZERO BOTH AST ADDRESS AND
                                     005A   286 					;  AST PARAMETER LONGWORDS
TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   8
X0.1-3          TST$QIOW -  NETWORK QIO ROUTINES                                                                                 (6)

                                     005A   287 	$QIOW_G	(R0)			; ISSUE THE QIO AND WAIT REQUEST
                                     0061   288 	CHECK_SS			; CHECK STATUS CODE
                                 05  0064   289 	RSB				; EXIT
                                     0065   290 
                                     0065   291 ;
                                     0065   292 ; QIO WITH AST ROUTINE
                                     0065   293 ;
                                     0065   294 
                                     0065   295 
                                     0065   296 TST$QIOAST::				; CONTROL POINT
                          13     10  0065   297 	BSBB	QIO_COMMON		; EXECUTE COMMON SET-UP CODE
         14 A0            55     D0  0067   298 	MOVL	R5,QIO$_ASTADR(R0)	; UPDATE AST ADDRESS
         18 A0            50     D0  006B   299 	MOVL	R0,QIO$_ASTPRM(R0)	; UPDATE AST PARAMETER WITH
                                     006F   300 					;  ADDRESS OF THIS PARAMETER BLOCK
                                     006F   301 	$QIO_G	(R0)			; ISSUE QIO WITH AST REQUEST
                                     0076   302 	CHECK_SS			; CHECK STATUS CODE
                                 05  0079   303 	RSB				; EXIT
                                     007A   304 
                                     007A   305 ;
                                     007A   306 ; SUBROUTINE THAT PERFORMS COMMON SET-UP FUNCTIONS
                                     007A   307 ;
                                     007A   308 
                                     007A   309 QIO_COMMON:				; CONTROL POINT
            52            0D     C5  007A   310 	MULL3	#<QIO$_NARGS+1>,R2,R1	; CALCULATE LONGWORD OFFSET OF
                          51         007D       
                                     007E   311 					;  DESIRED QIO PARAMETER BLOCK
                                     007E   312 					;  FROM THE FIRST PARAMETER BLOCK
            50       0000'CF41   DE  007E   313 	MOVAL	W^TST$PARAMETER[R1],R0	; GET ADDRESS OF PARAMATER BLOCK
         08 A0       0000'CF     3C  0084   314 	MOVZWL	W^TST$GW_LINKCHAN,QIO$_CHAN(R0) ; UPDATE CHANNEL #
                          52     D5  008A   315 	TSTL	R2			; IS DEVICE THE ASSOCIATED MAILBOX?
                          06     12  008C   316 	BNEQU	10$			; NO
         08 A0       0000'CF     3C  008E   317 	MOVZWL	W^TST$GW_MAILCHAN,QIO$_CHAN(R0) ; YES
                                     0094   318 ;10$:	MOVL	R3,QIO$_P1(R0)		; UPDATE BUFFER ADDRESS
         20 A0            54     D0  0094   319 10$:	MOVL	R4,QIO$_P2(R0)		; UPDATE P2 PARAMETER (EITHER DESC
                                     0098   320 					;  BLOCK ADDRESS OR BUFFER SIZE)
                                 05  0098   321 	RSB				; EXIT
TST$DTCOMMON    - COMMON ROUTINES FOR DTS/DTR                    22-AUG-1978 01:05:03   VAX-11 MACRO X0.3-11               Page   9
X0.1-3          TST$EXAM_MAIL - EXAMINE MAILBOX MESSAGE                                                                          (7)

                                     0099   323 	.SBTTL	TST$EXAM_MAIL - EXAMINE MAILBOX MESSAGE
                                 00000099   324 	.PSECT	TST$CODE	NOWRT
                                     0099   325 
                                     0099   326 ;++
                                     0099   327 ; FUNCTIONAL DESCRIPTION:
                                     0099   328 ;
                                     0099   329 ;	TST$EXAM_MAIL DISECTS A MAILBOX MESSAGE INTO ITS VARIOUS
                                     0099   330 ;	FIELDS.
                                     0099   331 ;
                                     0099   332 ; CALLING SEQUENCE:
                                     0099   333 ;
                                     0099   334 ;	BSB/JSB	TST$EXAM_MAIL
                                     0099   335 ;
                                     0099   336 ; INPUT PARAMETERS:
                                     0099   337 ;
                                     0099   338 ;	NONE
                                     0099   339 ;
                                     0099   340 ; IMPLICIT INPUTS:
                                     0099   341 ;
                                     0099   342 ;	TST$GB_MAILBUF
                                     0099   343 ;	TST$GQ_MAILIOSB
                                     0099   344 ;
                                     0099   345 ; OUTPUT PARAMETERS:
                                     0099   346 ;
                                     0099   347 ;	R0-R1	DESTROYED
                                     0099   348 ;	R6	MAILBOX MESSAGE CODE
                                     0099   349 ;	R7	ADDRESS OF RECEIVED MAILBOX DATA LESS HEADER STORED AS A
                                     0099   350 ;		COUNTED ASCII STRING
                                     0099   351 ;
                                     0099   352 ; IMPLICIT OUTPUTS:
                                     0099   353 ;
                                     0099   354 ;	TST$GW_MAILCODE
                                     0099   355 ;	TST$GW_DEV_UNIT
                                     0099   356 ;	TST$GT_DEV_NAME
                                     0099   357 ;	TST$GT_MAILDATA
                                     0099   358 ;
                                     0099   359 ; COMPLETION CODES:
                                     0099   360 ;
                                     0099   361 ;	NONE
                                     0099   362 ;
                                     0099   363 ; SIDE EFFECTS:
                                     0099   364 ;
                                     0099   365 ;	NONE
                                     0099   366 ;
                                     0099   367 ;--
                                     0099   368 
                                     0099   369 TST$EXAM_MAIL::				; CONTROL POINT
                          3C     BB  0099   370 	PUSHR	#^M<R2,R3,R4,R5>	; SAVE REGISTERS
