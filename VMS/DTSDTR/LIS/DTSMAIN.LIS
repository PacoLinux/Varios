                                     0000    60 ;++
                                     0000    61 ;
                                     0000    62 ; THE DTS COMMAND (FOR VAX/VMS) HAS THE FOLLOWING FORMAT:
                                     0000    63 ;
                                     0000    64 ; $ DTS[/qualifier,...]    parameter[/qualifier,...]
                                     0000    65 ;
                                     0000    66 ; WHERE THE COMMAND AND COMMAND QUALIFIERS ARE:
                                     0000    67 ;
                                     0000    68 ; $ DTS[/NODENAME=xxxxxx][/SPEED=nnnnnnn][/NOSTATISTICS][/NOPRINT]
                                     0000    69 ;                                        [/STATISTICS  ][/PRINT  ]
                                     0000    70 ;
                                     0000    71 ;      [/NODISPLAY ] ...
                                     0000    72 ;      [/DISPLAY=nn]
                                     0000    73 ;
                                     0000    74 ; AND WHERE THE PARAMETER AND PARAMETER QUALIFIERS ARE ONE OF THE FOLLOWING:
                                     0000    75 ;
                                     0000    76 ; ... CONNECT[/TYPE=REJECT][/NORETURN       ]
                                     0000    77 ;            [/TYPE=ACCEPT][/RETURN=STANDARD]
                                     0000    78 ;                          [/RETURN=RECEIVED]
                                     0000    79 ;
                                     0000    80 ; ... DATA[/TYPE=SINK    ][/SIZE=nnnnn][/HOURS=nnn     ][/SQUEUE=n][RQUEUE=n]
                                     0000    81 ;         [/TYPE=SEQUENCE]             [/MINUTES=nnnn  ]
                                     0000    82 ;         [/TYPE=PATTERN ]             [/SECONDS=nnnnnn]
                                     0000    83 ;         [/TYPE=ECHO    ]
                                     0000    84 ;
                                     0000    85 ;         [/NOFLOW      ][/NONAK  ][/NOBACK  ]
                                     0000    86 ;         [/FLOW=MESSAGE][/NAK=nnn][/BACK=nnn]
                                     0000    87 ;         [/FLOW=SEGMENT]
                                     0000    88 ;
                                     0000    89 ; ... DISCONNECT[/TYPE=SYNCHRONOUS][/NORETURN       ]
                                     0000    90 ;               [/TYPE=ABORT      ][/RETURN=STANDARD]
                                     0000    91 ;                                  [/RETURN=RECEIVED]
                                     0000    92 ;
                                     0000    93 ; ... INTERRUPT[/TYPE=SINK    ][/SIZE=nn][/HOURS=nnn     ][/SQUEUE=n][RQUEUE=n]
                                     0000    94 ;              [/TYPE=SEQUENCE]          [/MINUTES=nnnn  ]
                                     0000    95 ;              [/TYPE=PATTERN ]          [/SECONDS=nnnnnn]
                                     0000    96 ;              [/TYPE=ECHO    ]
                                     0000    97 ;
                                     0000    98 ; ... MISCELLANEOUS[/TYPE=NAME   ]
                                     0000    99 ;                  [/TYPE=OBJECT ]
                                     0000   100 ;                  [/TYPE=LOGICAL]
                                     0000   101 ;
                                     0000   102 ;
                                     0000   103 ; COMMAND QUALIFIER DEFAULTS:
                                     0000   104 ;
                                     0000   105 ; NODE = NULL STRING; I.E., THE LOCAL NODE
                                     0000   106 ; SPEED = 1000000 BAUD (USED FOR STATISTICS)
                                     0000   107 ; STATISTICS
                                     0000   108 ; NOPRINT
                                     0000   109 ; NODISPLAY
                                     0000   110 ;
                                     0000   111 ; CONNECT TEST PARAMETER QUALIFIER DEFAULTS:
                                     0000   112 ;
                                     0000   113 ; TYPE = ACCEPT
                                     0000   114 ; NORETURN
