            50         28 AA     9E  0111   335 	MOVAB	I_SCRATCH(R10),R0	; ADDRESS NAME STRING
                          60     95  0115   336 	TSTB	(R0)			; ANY DATA?
                          02     12  0117   337 	BNEQ	10$			; IF NEQ THEN YES
                          50     D4  0119   338 	CLRL	R0			; SET NO TASK NAME
                                     011B   339 10$:	
