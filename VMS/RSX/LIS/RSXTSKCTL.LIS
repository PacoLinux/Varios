                                     0022   122 	.SBTTL	RSX$CSRQ - RSX11M CSRQ$ DIRECTIVE
                                     0022   123 ;++
                                     0022   124 ; RSX$CSRQ -  RSX11M CANCEL TIME BASED INITIATION REQUESTS DIRECTIVE
                                     0022   125 ;
                                     0022   126 ; FUNCTIONAL DESCRIPTION:
                                     0022   127 ;
                                     0022   128 ; THIS ROUTINE IMPLEMENTS THE RSX11M CANCEL TIMED INITIATION DIRECTIVE.
                                     0022   129 ; THE SPECIFIED TASK NAME IS CONVERTED TO ASCII FOLLOWED BY A "$CANWAK".
                                     0022   130 ;
                                     0022   131 ; INPUTS:
                                     0022   132 ;
                                     0022   133 ;	R5 = ADDRESS OF THE DPB+2
                                     0022   134 ;	R10 = ADDRESS OF THE IMAGE DATA BASE
                                     0022   135 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0022   136 ;
                                     0022   137 ; OUTPUTS:
                                     0022   138 ;
                                     0022   139 ;	R10,R11 ARE PRESERVED.
                                     0022   140 ;
                                     0022   141 ; IMPLICIT OUTPUTS:
                                     0022   142 ;
                                     0022   143 ;	THE PROPER DIRECTIVE STATUS IS SET IN THE DSW ALONG WITH
                                     0022   144 ;	THE PROPER SETTING OF THE C-BIT.
                                     0022   145 ;
                                     0022   146 ; DSW RETURN CODES:
                                     0022   147 ;
                                     0022   148 ;	IS.SUC	SUCCESSFUL
                                     0022   149 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     0022   150 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     0022   151 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     0022   152 ;	IE.PRI	PRIVILEGE VIOLATION
                                     0022   153 ;	IE.INS	UNKNOWN PROCESS
                                     0022   154 ;--
                                     0022   155 RSX$CSRQ::				; CANCEL TIMED INITIATION
                          FFDB'  30  0022   156 	BSBW	RSX$CVTNAME		; CONVERT R50 TASK NAME TO ASCII
            50         28 AA     9E  0025   157 	MOVAB	I_SCRATCH(R10),R0	; ADDRESS NAME STRING
                          60     95  0029   158 	TSTB	(R0)			; ANY DATA?
                          02     12  002B   159 	BNEQ	10$			; IF NEQ THEN YES
                          50     D4  002D   160 	CLRL	R0			; SET NO TASK NAME
                                     002F   161 10$:	
                                     002F   162 	$CANWAK_S	,(R0)		; CANCEL WAKEUP FOR PROCESS
                          01C2   31  003A   163 	BRW	MAPDSW			; MAP THE PROPER DSW RETURN
