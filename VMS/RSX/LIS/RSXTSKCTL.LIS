                                     0129   345 ; RSX$RUN - RSX11M RUN TASK DIRECTIVE
                                     0129   346 ;
                                     0129   347 ; FUNCTIONAL DESCRIPTION:
                                     0129   348 ;
                                     0129   349 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0129   350 ; "RUN$" DIRECTIVE. THE SPECIFIED TASK NAME IS CONVERTED TO ASCII
                                     0129   351 ; AND A "$SCHDWK" IS DONE TO AFFECT THE DIRECTIVE. 
                                     0129   352 ;
                                     0129   353 ; INPUT PARAMETERS:
                                     0129   354 ;
                                     0129   355 ;	R5 = ADDRESS OF DPB+2
                                     0129   356 ;	R6 = CM IMAGE STACK ADDRESS
                                     0129   357 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0129   358 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0129   359 ;
                                     0129   360 ; OUTPUT PARAMETERS:
                                     0129   361 ;
                                     0129   362 ;	R10,R11 ARE PRESERVED
                                     0129   363 ;
                                     0129   364 ; IMPLICIT OUTPUTS:
                                     0129   365 ;
                                     0129   366 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0129   367 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0129   368 ;
                                     0129   369 ; DSW RETURN CODES:
                                     0129   370 ;
                                     0129   371 ;	IS.SUC	SUCCESSFUL
                                     0129   372 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     0129   373 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     0129   374 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     0129   375 ;	IE.PRI	PRIVILEGE VIOLATION
                                     0129   376 ;	IE.INS	UNKNOWN PROCESS
                                     0129   377 ;	IE.ITI	INVALID TIME PARAMETER
                                     0129   378 ;--
                                     0129   379 RSX$RUN::				; ABORT TASK DIRECTIVE
                          FED4'  30  0129   380 	BSBW	RSX$CVTNAME		; CONVERT TASK NAME TO ASCII
                       28 AA     95  012C   381 	TSTB	I_SCRATCH(R10)		; ANY NAME?
                          0A     12  012F   382 	BNEQ	10$			; IF NEQ THEN YES
                                     0131   383 	$DSW	#IE.INS			; BAD NAME
            55            0C     C0  013B   384 10$:	ADDL	#12,R5			; POINT TO SCHEDULE MAG.
