                                     003D   165 	.SBTTL	RSX$EXTK - RSX11M EXTK$ DIRECTIVE
                                     003D   166 ;++
                                     003D   167 ; RSX$EXTK - RSX11M EXTEND TASK DIRECTIVE
                                     003D   168 ;
                                     003D   169 ; FUNCTIONAL DESCRIPTION:
                                     003D   170 ;
                                     003D   171 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     003D   172 ; "EXTK$" DIRECTIVE.
                                     003D   173 ;
                                     003D   174 ; INPUT PARAMETERS:
                                     003D   175 ;
                                     003D   176 ;	R5 = ADDRESS OF DPB+2
                                     003D   177 ;	R6 = CM IMAGE STACK ADDRESS
                                     003D   178 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     003D   179 ;	R11 = ADDRESS OF IMAGE HEADER
                                     003D   180 ;
                                     003D   181 ; OUTPUT PARAMETERS:
                                     003D   182 ;
                                     003D   183 ;	R10,R11 ARE PRESERVED
                                     003D   184 ;
                                     003D   185 ; IMPLICIT OUTPUTS:
                                     003D   186 ;
                                     003D   187 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     003D   188 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     003D   189 ;
                                     003D   190 ; DSW RETURN CODES:
                                     003D   191 ;
                                     003D   192 ;	IS.SUC	SUCCESSFUL
                                     003D   193 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     003D   194 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     003D   195 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     003D   196 ;	IE.ALG	INVALID EXTENSION OR CONTRACTION
                                     003D   197 ;--
                                     003D   198 RSX$EXTK::				; EXTEND TASK DIRECTIVE
            54         28 AA     9E  003D   199 	MOVAB	I_SCRATCH(R10),R4	; ADDRESS ARGLIST
            55            65     32  0041   200 	CVTWL	(R5),R5			;
            55            06     78  0044   201 	ASHL	#6,R5,R5		; MAKE BYTES
                          55         0047       
                          41     14  0048   202 	BGTR	50$			; IF GTRU THEN EXPAND
                          15     12  004A   203 	BNEQ	10$			; IF NEQ THEN CONTRACT
                                     004C   204 ;
                                     004C   205 ; MAKE INITIAL SIZE
                                     004C   206 ;
            64       00F0 CA     D0  004C   207 	MOVL	I_INISIZE(R10),(R4)	; SET UP LOWER LIMIT
         04 A4       0100 CA     D0  0051   208 	MOVL	I_ADR0+ADR_HIGHLIM(R10),4(R4); SET UP UPPER LIMIT
            64         04 A4     C3  0057   209 	SUBL3	4(R4),(R4),R5		; GET DIFFERENCE
                          55         005B       
                          03     19  005C   210 	BLSS	10$			; IF LSS THEN CONTRACT
                          2B     14  005E   211 	BGTR	50$			; IF GTR THEN EXPAND
                                 05  0060   212 	RSB				; IF EQL THEN NO CHANGE
       0100 CA            55     C1  0061   213 10$:	ADDL3	R5,I_ADR0+ADR_HIGHLIM(R10),(R4); SET UP LOW LIMIT BY FUNNY SUBTRACT
                          64         0066       
         04 A4       0100 CA     D0  0067   214 15$:	MOVL	I_ADR0+ADR_HIGHLIM(R10),4(R4);
            55            64     D0  006D   215 	MOVL	(R4),R5			; SAVE BOT AS NEW HIGH SIZE
            55       00EC CA     B1  0070   216 	CMPW	I_GARD(R10),R5		; DELETE TOO MUCH?
                          42     1A  0075   217 	BGTRU	75$			; IF GTRU THEN ERROR
                          4A     10  0077   218 	BSBB	100$			; CHECK AND ROUND PARAMS
RSXTSKCTL       - RSX AME TASK CONTROL DIRECTIVES                22-AUG-1978 05:13:30   VAX-11 MACRO X0.3-11               Page   6
V06.0           RSX$EXTK - RSX11M EXTK$ DIRECTIVE                                                                                (5)

                                     0079   219 	$DELTVA_S	(R4)		; DELETE THE SPACE
            30            50     E9  0086   220 	BLBC	R0,75$			; IF LOW CLEAR THEN ERROR
                          28     11  0089   221 	BRB	60$			; CONTINUE
            64       0100 CA     D0  008B   222 50$:	MOVL	I_ADR0+ADR_HIGHLIM(R10),(R4); INSERT HIGH LIMIT
            64            55     C1  0090   223 	ADDL3	R5,(R4),4(R4)		;
                       04 A4         0093       
            55         04 A4     D0  0095   224 	MOVL	4(R4),R5		; SAVE TOP AS NEW HIGH LIM
                          28     10  0099   225 	BSBB	100$			; ROUND SIZE
       00F4 CA         04 A4     D1  009B   226 	CMPL	4(R4),I_HIGHSIZE(R10)	; IN RANGE?
                          16     1E  00A1   227 	BGEQU	75$			; BR IF ERROR
                                     00A3   228 	$CRETVA_S	(R4)
            06            50     E9  00B0   229 	BLBC	R0,75$
       0100 CA            55     D0  00B3   230 60$:	MOVL	R5,I_ADR0+ADR_HIGHLIM(R10)
                                 05  00B8   231 	RSB
                                     00B9   232 75$:	$DSW	#IE.ALG			; SET ALLOCATION FAILURE
                                     00C3   233 ;
                                     00C3   234 ; SUBROUTINE TO CHECK AND ROUND PARAMETERS
                                     00C3   235 ;
                                     00C3   236 100$:	
            64       01FF 8F     AA  00C3   237 	BICW	#511,(R4)
         04 A4       01FF 8F     AA  00C8   238 	BICW	#511,4(R4)
         04 A4            64     D1  00CE   239 	CMPL	(R4),4(R4)		; IF EQL THEN NO CHANGE NEEDED
                          07     12  00D2   240 	BNEQ	110$
       0100 CA            55     D0  00D4   241 	MOVL	R5,I_ADR0+ADR_HIGHLIM(R10); ASSUME SUCCESS
                          01     BA  00D9   242 	POPR	#^M<R0>
            64       0200 8F     A0  00DB   243 110$:	ADDW	#512,(R4)		; ROUND LOWER LIMIT UP TO NEXT PAGE
                                 05  00E0   244 	RSB
