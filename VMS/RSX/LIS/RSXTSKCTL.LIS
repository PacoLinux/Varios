                                     01D3   499 	.SBTTL	RSX$ENCP - RSX11M ENCP$ DIRECTIVE
                                     01D3   500 ;++
                                     01D3   501 ; RSX$ENCP - RSX11M ENABLE CHECKPOINTING TASK DIRECTIVE
                                     01D3   502 ;
                                     01D3   503 ; FUNCTIONAL DESCRIPTION:
                                     01D3   504 ;
                                     01D3   505 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     01D3   506 ; "ENCP$" DIRECTIVE. THE ONLY FUNCTION OF THEI DIRECTIVE, IS TO
                                     01D3   507 ; RETURN THE PROPER STATUS VALUE.
                                     01D3   508 ;
                                     01D3   509 ; INPUT PARAMETERS:
                                     01D3   510 ;
                                     01D3   511 ;	R5 = ADDRESS OF DPB+2
                                     01D3   512 ;	R6 = CM IMAGE STACK ADDRESS
                                     01D3   513 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     01D3   514 ;	R11 = ADDRESS OF IMAGE HEADER
                                     01D3   515 ;
                                     01D3   516 ; OUTPUT PARAMETERS:
                                     01D3   517 ;
                                     01D3   518 ;	R10,R11 ARE PRESERVED
                                     01D3   519 ;
                                     01D3   520 ; IMPLICIT OUTPUTS:
                                     01D3   521 ;
                                     01D3   522 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     01D3   523 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     01D3   524 ;
                                     01D3   525 ; DSW RETURN CODES:
                                     01D3   526 ;
                                     01D3   527 ;	IS.SUC	SUCCESSFUL
                                     01D3   528 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     01D3   529 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     01D3   530 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     01D3   531 ;	IE.ITS	CHECKPOINTING IS ALREADY ENABLED
                                     01D3   532 ;--
                                     01D3   533 RSX$ENCP::				; ENABLE CHECKPOINTING TASK DIRECTIVE
                                     01D3   534 	$SETSWM_S	#1		; ENABLE SWAP MODE
       0000'8F            50     B1  01DC   535 	CMPW	R0,#SS$_WASSET		; IF WAS SET THEN NO ERROR
                          1B     13  01E1   536 	BEQL	15$			; IF EQL THEN NO ERROR
       0000'8F            50     B1  01E3   537 	CMPW	R0,#SS$_WASCLR		; IF CLEAR THEN ERROR
                          0A     12  01E8   538 	BNEQ	10$			; IF NEQ THEN PRIVILEGE ERROR
                                     01EA   539 	$DSW	#IE.ITS			; SET ERROR
                                     01F4   540 10$:	$DSWIO	#IE.PRI			; SET ERROR
                                 05  01FE   541 15$:	RSB				;
RSXTSKCTL       - RSX AME TASK CONTROL DIRECTIVES                22-AUG-1978 05:13:30   VAX-11 MACRO X0.3-11               Page  14
V06.0           MAPDSW - MAP DSW RETURN AFTER TASK CONTROL SERI                                                                 (12)

                                     01FF   543 	.SBTTL	MAPDSW - MAP DSW RETURN AFTER TASK CONTROL SERIVCE
                                     01FF   544 ;++
                                     01FF   545 ; MAPDSW - MAP THE PROPER DSW RETURN FOR THE TASK CONTROL DIRECTIVES
                                     01FF   546 ;
                                     01FF   547 ; FUNCTIONAL DESCRIPTION:
                                     01FF   548 ;
                                     01FF   549 ; THIS ROUTINE IS ENTERED AFTER A SYSTEM SERVICE TO MAP THE VAX/VMS
                                     01FF   550 ; RETURN CODE INTO A DSW RETURN CODE.
                                     01FF   551 ; IF THE OPERATION WAS NOT SUCCESSFUL THEN THE POSSIBLE ERRORS
                                     01FF   552 ; ARE:
                                     01FF   553 ;
                                     01FF   554 ;	IE.PRI	PRIVILEGE VIOLATION
                                     01FF   555 ;	IE.INS	UNKNOWN	PROCESS
                                     01FF   556 ;
                                     01FF   557 ; INPUTS:
                                     01FF   558 ;
                                     01FF   559 ;	R0 = STATUS TO MAP
                                     01FF   560 ;	R10 = ADDRESS OF IMAGE DATA BASE
                                     01FF   561 ;	R11 = ADDRESS OF THE IMAGE HEADER
                                     01FF   562 ;
                                     01FF   563 ; OUTPUTS:
                                     01FF   564 ;
                                     01FF   565 ;	R10,R11 ARE PRESERVED
                                     01FF   566 ;
                                     01FF   567 ; IMPLICIT OUTPUTS:
                                     01FF   568 ;
                                     01FF   569 ;	THE IMAGE DSW IS SET UP TO REFLECT THE STATUS OF THE
                                     01FF   570 ;	SYSTEM SERVICE. SUCCESSFUL IS IMPLIED.
                                     01FF   571 ;--
                                     01FF   572 MAPDSW:					; MAP DSW RETURN
            3D            50     E8  01FF   573 	BLBS	R0,20$			; IF LOW SET THEN SUCCESSFUL
       0000'8F            50     B1  0202   574 	CMPW	R0,#SS$_NOPRIV		; PRIVILEGE VIOLATION?
                          0A     12  0207   575 	BNEQ	5$			; IF NEQ THEN NO
                                     0209   576 	$DSWIO	#IE.PRI			; SET STATUS AND RETURN
       0000'8F            50     B1  0213   577 5$:	CMPW	R0,#SS$_INSFMEM		; NO MEMORY?
                          0A     12  0218   578 	BNEQ	10$			; IF NEQ THEN NO
                                     021A   579 	$DSW	#IE.UPN			; SET NO MEMORY ERROR
       0000'8F            50     B1  0224   580 10$:	CMPW	R0,#SS$_EXQUOTA		; QUOTA PROBLEM?
                          0A     12  0229   581 	BNEQ	15$			; IF NEQ THEN NO
                                     022B   582 	$DSWIO	#IE.NOD			; SET QUOTA ERROR
                                     0235   583 15$:	$DSW	#IE.INS			; SET NO SUCH PROCESS ERROR
                                 05  023F   584 20$:	RSB				; RETURN
                                     0240   585 ;
                                     0240   586 	.END
