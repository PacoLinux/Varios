                                     00E1   246 	.SBTTL	RSX$RQST - RSX11M RQST$ DIRECTIVE
                                     00E1   247 ;++
                                     00E1   248 ; RSX$RQST - RSX11M REQUEST DIRECTIVE
                                     00E1   249 ;
                                     00E1   250 ; FUNCTIONAL DESCRIPTION:
                                     00E1   251 ;
                                     00E1   252 ; THIS ROUTINE IMPLEMENTS THE RSX11M RQST$ DIRECTIVE.
                                     00E1   253 ; THE SPECIFIED PROCESS NAME IS COMVERTED TO ASCII AND THEN "$WAKE"'D.
                                     00E1   254 ;
                                     00E1   255 ; INPUTS:
                                     00E1   256 ;
                                     00E1   257 ;	R5 = ADDRESS OF DPB+2
                                     00E1   258 ;	R6 = CM IMAGE STACK POINTER
                                     00E1   259 ;	R10 = ADDRESS OF IMAGE DATA
                                     00E1   260 ;	R11 = ADDRESS OF IMAGE HEADER
                                     00E1   261 ;
                                     00E1   262 ; IMPLICIT INPUTS:
                                     00E1   263 ;
                                     00E1   264 ;	THE DPB IS ALREADY VALIDATED FOR READ ACCESS.
                                     00E1   265 ;	THE DPB SIZE AND TYPE ARE CORRECT.
                                     00E1   266 ;
                                     00E1   267 ; OUTPUTS:
                                     00E1   268 ;
                                     00E1   269 ;	R10,R11 ARE PRESERVED.
                                     00E1   270 ;
                                     00E1   271 ; IMPLICIT OUTPUTS:
                                     00E1   272 ;
                                     00E1   273 ;	THE REQUESTED PROCESS IS "$WAKE"'D AND "RSX$MAPDSW" IS ENTERED.
                                     00E1   274 ;
                                     00E1   275 ; DSW RETURN CODES:
                                     00E1   276 ;
                                     00E1   277 ;	IS.SUC	SUCCESSFUL
                                     00E1   278 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     00E1   279 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     00E1   280 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     00E1   281 ;	IE.PRI	PRIVILEGE VIOLATION
                                     00E1   282 ;	IE.INS	UNKNOWN PROCESS
                                     00E1   283 ;--
                                     00E1   284 RSX$RQST::				; REQUEST DIRECTIVE
   B37C66E4 8F            65     D1  00E1   285 	CMPL	(R5),#^X0B37C66E4	; PRT...?
                          23     13  00E8   286 	BEQL	11$			; IF EQL THEN SUCCESS
                          FF13'  30  00EA   287 	BSBW	RSX$CVTNAME		; CONVERT TASK NAME TO ASCII
            50         28 AA     9E  00ED   288 	MOVAB	I_SCRATCH(R10),R0	; ADDRESS NAME STRING
                          60     95  00F1   289 	TSTB	(R0)			; ANY DATA?
                          0A     12  00F3   290 	BNEQ	10$			; IF NEQ THEN YES
                                     00F5   291 	$DSW	#IE.INS
                                     00FF   292 10$:	
                                     00FF   293 	$WAKE_S	,(R0)			; WAKE PROCESS
                          00F2   31  010A   294 	BRW	MAPDSW			; MAP RETURN STATUS
                                 05  010D   295 11$:	RSB
