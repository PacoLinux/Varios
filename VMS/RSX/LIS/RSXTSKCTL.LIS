                                     0000    44 	.SBTTL	RSX$ABRT - RSX11M ABRT$ DIRECTIVE
                                     0000    45 ;++
                                     0000    46 ; RSX$ABRT - RSX11M ABORT TASK DIRECTIVE
                                     0000    47 ;
                                     0000    48 ; FUNCTIONAL DESCRIPTION:
                                     0000    49 ;
                                     0000    50 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0000    51 ; "ABRT$" DIRECTIVE. THE SPECIFIED TASK NAME IS CONVERTED TO ASCII
                                     0000    52 ; AND A "$FORCEX" IS DONE TO AFFECT THE DIRECTIVE. 
                                     0000    53 ;
                                     0000    54 ; INPUT PARAMETERS:
                                     0000    55 ;
                                     0000    56 ;	R5 = ADDRESS OF DPB+2
                                     0000    57 ;	R6 = CM IMAGE STACK ADDRESS
                                     0000    58 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0000    59 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0000    60 ;
                                     0000    61 ; OUTPUT PARAMETERS:
                                     0000    62 ;
                                     0000    63 ;	R10,R11 ARE PRESERVED
                                     0000    64 ;
                                     0000    65 ; IMPLICIT OUTPUTS:
                                     0000    66 ;
                                     0000    67 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0000    68 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0000    69 ;
                                     0000    70 ; DSW RETURN CODES:
                                     0000    71 ;
                                     0000    72 ;	IS.SUC	SUCCESSFUL
                                     0000    73 ;	IS.PRI	PRIVILEGE VIOLATION -- SS$_NOPRIV
                                     0000    74 ;	IE.UPN	INSUFICIENT MEMORY -- SS$_INSFMEM
                                     0000    75 ;	IE.NOD	QUOTA EXCEEDED -- SS$_EXQUOTA
                                     0000    76 ;	IE.PRI	PRIVILEGE VIOLATION
                                     0000    77 ;	IE.INS	UNKNOWN PROCESS
                                     0000    78 ;--
                                     0000    79 RSX$ABRT::				; ABORT TASK DIRECTIVE
                          FFFD'  30  0000    80 	BSBW	RSX$CVTNAME		; CONVERT R50 NAME TO ASCII
            50         28 AA     9E  0003    81 	MOVAB	I_SCRATCH(R10),R0	; ADDRESS NAME STRING
                          60     95  0007    82 	TSTB	(R0)			; ANY DATA?
                          02     12  0009    83 	BNEQ	10$			; IF NEQ THEN YES
                          50     D4  000B    84 	CLRL	R0			; SET NO TASK NAME
                                     000D    85 10$:
                                     000D    86 	$FORCEX_S	,(R0),#SS$_ABORT; FORCE TERMINATION
                          01DE   31  001E    87 	BRW	MAPDSW			; MAP THE PROPER DSW RETURN
