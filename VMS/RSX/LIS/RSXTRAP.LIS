                                     0000    45 ; RSX$DSAR - RSX11M DISABLE AST RECOGNITION DIRECTIVE
                                     0000    46 ;
                                     0000    47 ; FUNCTIONAL DESCRIPTION:
                                     0000    48 ;
                                     0000    49 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0000    50 ; "DSAR$" DIRECTIVE.
                                     0000    51 ;
                                     0000    52 ; INPUT PARAMETERS:
                                     0000    53 ;
                                     0000    54 ;	R5 = ADDRESS OF DPB+2
                                     0000    55 ;	R6 = CM IMAGE STACK ADDRESS
                                     0000    56 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0000    57 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0000    58 ;
                                     0000    59 ; OUTPUT PARAMETERS:
                                     0000    60 ;
                                     0000    61 ;	R10,R11 ARE PRESERVED
                                     0000    62 ;
                                     0000    63 ; IMPLICIT OUTPUTS:
                                     0000    64 ;
                                     0000    65 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0000    66 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0000    67 ;
                                     0000    68 ; DSW RETURN CODES:
                                     0000    69 ;
                                     0000    70 ;	IS.SUC	SUCCESSFUL
                                     0000    71 ;	IE.ITS	AST'S ALREADY DISABLED
                                     0000    72 ;--
                                     0000    73 RSX$DSAR::				; DISABLE AST RECOGNITION DIRECTIVE
       0170 CA            03     E1  0000    74 	BBC	#SV_NOAST,I_STATE(R10),10$; BR IF AST'S NOT ALREADY DISABLED
                          0A         0005       
                                     0006    75 ;
                                     0006    76 ; AST'S ALREADY DISABLED
                                     0006    77 ;
                                     0006    78 	$DSW	#IE.ITS			; SET DSW AND RETURN
                                     0010    79 ;
                                     0010    80 ; DISABLE AST'S
                                     0010    81 ;
       0170 CA            03     E6  0010    82 10$:	BBSSI	#SV_NOAST,I_STATE(R10),15$; SET AST'S DISABLED
                          00         0015       
                                 05  0016    83 15$:	RSB				; AND RETURN
