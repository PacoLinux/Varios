                                     003D   106 ;
                                     003D   107 ; FUNCTIONAL DESCRIPTION:
                                     003D   108 ;
                                     003D   109 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS THE RSX11M
                                     003D   110 ; "RCVD$" AND "RCVX$" DIRECTIVES.
                                     003D   111 ;
                                     003D   112 ; INPUT PARAMETERS:
                                     003D   113 ;
                                     003D   114 ;	R5 = ADDRESS OF DPB+2
                                     003D   115 ;	R6 = CM IMAGE STACK ADDRESS
                                     003D   116 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     003D   117 ;	R11 = ADDRESS OF IMAGE HEADER
                                     003D   118 ;
                                     003D   119 ; OUTPUT PARAMETERS:
                                     003D   120 ;
                                     003D   121 ;	R10,R11 ARE PRESERVED
                                     003D   122 ;
                                     003D   123 ; IMPLICIT OUTPUTS:
                                     003D   124 ;
                                     003D   125 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     003D   126 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     003D   127 ;
                                     003D   128 ; DSW RETURN CODES:
                                     003D   129 ;
                                     003D   130 ;	IS.SUC	SUCCESSFUL
                                     003D   131 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     003D   132 ;	IE.ITS	NO DATA ( ANY QIO ERROR )
                                     003D   133 ;--
                                     003D   134 RSX$RCVX::				; RECEIVE OR EXIT
            57            01     9A  003D   135 	MOVZBL	#1,R7			; SET EXIT FLAG
                          02     11  0040   136 	BRB	10$			; AND CONTINUE IN COMMON
                                     0042   137 RSX$RCVD::				; RECEIVE DATA DIRECTIVE
                          57     D4  0042   138 	CLRL	R7			; SET FLAG FOR NO EXIT
                                     0044   139 10$:					; COMMON ENTRY POINT TO RECEIVES
            54         04 A5     3C  0044   140 	MOVZWL	4(R5),R4		; ADDRESS BUFFER SPECIFIED
            3D            54     E8  0048   141 	BLBS	R4,22$			; IF LOW SET THEN ERROR
            52       0160 CA     D0  004B   142 	MOVL	I_RCVMAIL(R10),R2	; GET MAILBOX CHANNEL
                          2B     13  0050   143 	BEQL	20$			; IF EQL THEN NO BOX AND NO DATA
                                     0052   144 ;
                                     0052   145 ; READ THE MAILBOX
                                     0052   146 ;
                                     0052   147 	$QIOW_S	#7,I_RCVMAIL(R10),#IO$_READVBLK!IO$M_NOW,I_SCRATCH(R10)-
                                     0052   148 	,,,4(R4),#26
            07            50     E9  0073   149 	BLBC	R0,20$			; BR IF NOT SUCCESS
            50         28 AA     3C  0076   150 	MOVZWL	I_SCRATCH(R10),R0	; SET ERROR CODE IF ANY
            15            50     E8  007A   151 	BLBS	R0,25$			; IF LOW SET THEN SUCCESS
            53            57     E9  007D   152 20$:	BLBC	R7,35$			; IF CLEAR THEN NO EXIT
            50       0000'8F     3C  0080   153 	MOVZWL	#SS$_NORMAL,R0		;
                          FF78'  31  0085   154 	BRW	RSX$INTERNEXIT		;
                                     0088   155 22$:	$DSW	#IE.ADP			; ADDRESS INVALID ERROR
                                     0092   156 ;
