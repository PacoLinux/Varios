                                     0000    49 ; RSX$GMCR - RSX11M GET MCR LINE DIRECTIVE
                                     0000    50 ;
                                     0000    51 ; FUNCTIONAL DESCRIPTION:
                                     0000    52 ;
                                     0000    53 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0000    54 ; "GMCR$" DIRECTIVE.
                                     0000    55 ;
                                     0000    56 ; INPUT PARAMETERS:
                                     0000    57 ;
                                     0000    58 ;	R5 = ADDRESS OF DPB+2
                                     0000    59 ;	R6 = CM IMAGE STACK ADDRESS
                                     0000    60 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0000    61 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0000    62 ;
                                     0000    63 ; OUTPUT PARAMETERS:
                                     0000    64 ;
                                     0000    65 ;	R10,R11 ARE PRESERVED
                                     0000    66 ;
                                     0000    67 ; IMPLICIT OUTPUTS:
                                     0000    68 ;
                                     0000    69 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0000    70 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0000    71 ;
                                     0000    72 ; DSW RETURN CODES:
                                     0000    73 ;
                                     0000    74 ;	IS.SUC	SUCCESSFUL
                                     0000    75 ;	IE.AST	NO DATA AVAILABLE
                                     0000    76 ;--
                                     0000    77 RSX$GMCR::				; GET MCR LINE DIRECTIVE
                          50     D4  0000    78 	CLRL	R0
                       28 AA     9F  0002    79 	PUSHAB	I_SCRATCH(R10)		; ADDRESS TARGET
       00E8 DA            01     FB  0005    80 	CALLS	#1,@I_CMDROUT(R10)	; CALL CALLER BACK
            26            50     E9  000A    81 	BLBC	R0,20$			; IF LOW CLEAR THEN NONE
         28 AA       0050 8F     B1  000D    82 	CMPW	#80,I_SCRATCH(R10)	; 80 OR GREATER?
                          05     1E  0013    83 	BGEQU	5$			; IF GEQU THEN OK
         28 AA         50 8F     9A  0015    84 	MOVZBL	#80,I_SCRATCH(R10)	; SET MAX SIZE
         26 AB         28 AA     B0  001A    85 5$:	MOVW	I_SCRATCH(R10),H.DSW(R11); INSERT COMMAND SIZE
                          12     13  001F    86 	BEQL	20$			; IF EQL THEN ERROR
                                     0021    87 ;
                                     0021    88 ; MOVE THE COMMAND TO THE USER BUFFER - BUFFER HAS BEEN VALIDATED
                                     0021    89 ;
         2C BA         28 AA     28  0021    90 	MOVC3	I_SCRATCH(R10),@I_SCRATCH+4(R10),(R5); MOVE COMMAND TEXT
                          65         0026       
         28 AA       0050 8F     B1  0027    91 	CMPW	#80,I_SCRATCH(R10)	; EXACTLY 80?
                          03     13  002D    92 	BEQL	10$			; IF EQL THEN YES
            63            0D     90  002F    93 	MOVB	#^X0D,(R3)		; INSERT CR AS TERMINATOR
                                 05  0032    94 10$:	RSB				; RETURN
                                     0033    95 ;
                                     0033    96 ; NO COMMAND LINE
                                     0033    97 ;
                                     0033    98 20$:	$DSW	#IE.AST			;
