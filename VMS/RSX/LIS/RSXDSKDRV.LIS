                                     0039    72 	.SBTTL	DSK_RVB AND DSK_RLB FUNCTIONS
                                     0039    73  
                                     0039    74 	.ENABL	LSB
                                     0039    75 ;
                                     0039    76 ; READ VIRTUAL BLOCK FUNCTION
                                     0039    77 ;
                                     0039    78 DSK_RVB:				; READ VIRTUAL BLOCK
         34 AA       0000'8F     3C  0039    79 	MOVZWL	#IO$_READVBLK,I_SCRATCH+12(R10); SET UP FUNCTION CODE
                          2F     11  003F    80 	BRB	20$			; CONTINUE IN COMMON
                                     0041    81 DSK_RLB:				; READ LOGICAL BLOCK
         34 AA       0000'8F     3C  0041    82 	MOVZWL	#IO$_READLBLK,I_SCRATCH+12(R10); SET UP FUNCTION CODE
            08            57     91  0047    83 	CMPB	R7,#<IO.LOV&^X0FF>	; LOAD OVERLAY
                          19     12  004A    84 	BNEQ	15$			; IF NEQ THEN NO
                     0000'CF     9F  004C    85 	PUSHAB	W^RSX$A_OVDEV		; GET ADDRESS OF OVERLAY LUT
            58            8E     D1  0050    86 	CMPL	(SP)+,R8		; CORRECT LUT?
                          10     12  0053    87 	BNEQ	15$			; IF NO TREAT NORMALLY
            54            65     3C  0055    88 	MOVZWL	(R5),R4			; SET ADDRESS OF TARGET
            53         02 A5     3C  0058    89 	MOVZWL	2(R5),R3		; GET SIZE TO READ
            55         08 A5     D0  005C    90 	MOVL	8(R5),R5		; GET VBN OF IMAGE
                          11     10  0060    91 	BSBB	RSX$OVERLAY		; LOAD THE OVERLAY
                          FF9B'  31  0062    92 	BRW	RSX$DLVRQIOAST		; POST THE STATUS
            57            01     B3  0065    93 15$:	BITW	#IQ.X,R7		; INHIBIT ERROR RETRY?
                          06     13  0068    94 	BEQL	20$			; IF EQL THEN NO
         34 AA       0000'8F     A8  006A    95 	BISW	#IO$M_INHRETRY,I_SCRATCH+12(R10); INHIBIT RETRY
                          FF8D'  31  0070    96 20$:	BRW	RSX$ISSUQIO		; DO FUNCTION
                                     0073    97  
                                     0073    98 	.DSABL	LSB
RSXDSKDRV       - RSX11M AME DISK DRIVER                         22-AUG-1978 05:14:03   VAX-11 MACRO X0.3-11               Page   4
V06.0           RSX$OVERLAY - LOAD OVERLAY                                                                                       (4)

                                     0073   100 	.SBTTL	RSX$OVERLAY - LOAD OVERLAY
                                     0073   101 ;++
                                     0073   102 ; RSX$OVERLAY - LOAD OVERLAY
                                     0073   103 ;
                                     0073   104 ; FUNCTIONAL DESCRIPTION:
                                     0073   105 ;
                                     0073   106 ; THIS ROUTINE IS USED FOR IO.LOV AND INITIAL HEADER/ROOT SEGMENT LOADS.
                                     0073   107 ;
                                     0073   108 ; INPUTS:
                                     0073   109 ;
                                     0073   110 ;	R3 = NUMBER OF BYTES TO LOAD
                                     0073   111 ;	R4 = VIRTUAL ADDRESS OF LOAD
                                     0073   112 ;	R5 = VIRTUAL BLOCK NUMBER
                                     0073   113 ;	R10 = ADDRESS OF THE IMAGE IMPURE DATA BASE
                                     0073   114 ;	R11 = ADDRESS OF THE IMAGE
                                     0073   115 ;
                                     0073   116 ; OUTPUTS:
                                     0073   117 ;
                                     0073   118 ;	R0 = STATUS OF THE OPERATION
                                     0073   119 ;
                                     0073   120 ;	ALL OTHER REGISTERS ARE PRESERVED.
                                     0073   121 ;
                                     0073   122 ;--
                                     0073   123 RSX$OVERLAY::				;
                     00FC 8F     BB  0073   124 	PUSHR	#^M<R2,R3,R4,R5,R6,R7>	; SAVE USED REGISTERS
            55       00F8 CA     C0  0077   125 	ADDL	I_IMAGEVBN(R10),R5	; POINT TO HEADER RELATIVE BLOCK
                                     007C   126 	$QIOW_S	I_SCRATCH+4(R10),W^RSX$A_OVDEV+LUT_CHAN,#IO$_READVBLK,I_SCRATCH+8(R10),,,-
                                     007C   127 		(R4),R3,R5		;
            4E            50     E8  009F   128 	BLBS	R0,25$			; IF NO ERROR THEN GO
            50       0000'8F     B1  00A2   129 	CMPW	#SS$_INSFWSL,R0		; INSUFF. WORKING SET LIMIT?
                          4B     12  00A7   130 	BNEQ	30$			; IF NEQ THEN ACTUAL ERROR
            56       2000 8F     3C  00A9   131 	MOVZWL	#<8*1024>,R6		; SET UP STARTING SIZE
            57            54     D0  00AE   132 	MOVL	R4,R7			; COPY STARTING VA
                          54     D4  00B1   133 	CLRL	R4			; SET UP FOR QUAD ARITH.
            53            56     7B  00B3   134 	EDIV	R6,R3,R4,R2		; GET PARAMETERS
            52            54         00B6       
                          03     12  00B8   135 15$:	BNEQ	20$			; IF NEQ THEN READ A FULL 8K
            56            52     D0  00BA   136 	MOVL	R2,R6			; ELSE READ REMAINDER
                                     00BD   137 20$:	$QIOW_S	I_SCRATCH+4(R10),W^RSX$A_OVDEV+LUT_CHAN,#IO$_READVBLK,I_SCRATCH+8(R10),
                                     00BD   138 		(R7),R6,R5		;
            11            50     E9  00E0   139 	BLBC	R0,30$			; IF LOW CLEAR THEN ERROR
            09         30 AA     E9  00E3   140 	BLBC	I_SCRATCH+8(R10),25$	; AND BR IF IOSB SHOWS ERROR
            57            56     C0  00E7   141 	ADDL	R6,R7			; POINT TO NEXT AREA
            55            10     C0  00EA   142 	ADDL	#16,R5			; POINT TO NEXT AREA IN FILE
            C8            54     F4  00ED   143 	SOBGEQ	R4,15$			; LOOP TILL DONE
            50         30 AA     7D  00F0   144 25$:	MOVQ	I_SCRATCH+8(R10),R0	; LOAD IOSB FOR RETURN
                     00FC 8F     BA  00F4   145 30$:	POPR	#^M<R2,R3,R4,R5,R6,R7>	; RESTORE REGISTERS
            10            53     F0  00F8   146 	INSV	R3,#16,#16,R0		; LOAD UPPER HALF FOR TRANSFER SIZE
            50            10         00FB       
                                 05  00FD   147 	RSB
