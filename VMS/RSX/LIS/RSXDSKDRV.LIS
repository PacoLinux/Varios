                          1C     11  0104   156 	BRB	25$			; CONTINUE IN COMMON
                                     0106   157 ;
                                     0106   158 ; WRITE LOGICAL BLOCK
                                     0106   159 ;
                                     0106   160 DSK_WLB:				; WRITE LOGICAL BLOCK
         34 AA       0000'8F     3C  0106   161 	MOVZWL	#IO$_WRITELBLK,I_SCRATCH+12(R10); SET FUNCTION CODE
            20            57     91  010C   162 	CMPB	R7,#<IO.WPB&^X0FF>	; WRITE PHYSICAL?
                          06     12  010F   163 	BNEQ	15$	
         34 AA       0000'8F     3C  0111   164 	MOVZWL	#IO$_WRITEPBLK,I_SCRATCH+12(R10)
            57            01     B3  0117   165 15$:	BITW	#IQ.X,R7		; INHIBIT RETRY?
                          06     13  011A   166 	BEQL	25$			; IF EQL THEN NO
         34 AA       0000'8F     A8  011C   167 	BISW	#IO$M_INHRETRY,I_SCRATCH+12(R10); SET TO INHIBIT RETRY
                          FEDB'  31  0122   168 25$:	BRW	RSX$ISSUQIO
                                     0125   169 ;
