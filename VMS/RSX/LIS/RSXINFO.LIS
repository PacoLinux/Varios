                                     0053   144 ;--
                                     0053   145 RSX$GPRT::				; GET PARTITION PARAMETERS DIRECTIVE
            54         04 A5     3C  0053   146 	MOVZWL	4(R5),R4		; GET BUFFER ADDRESS SPECIFIED
            13            54     E8  0057   147 	BLBS	R4,10$			; BR IF BAD BUFFER
            84       0100 8F     B0  005A   148 	MOVW	#^X100,(R4)+		; INSERT PARTITION BASE ADDRESS
            0A            06     EF  005F   149 	EXTZV	#6,#10,I_ADR0+ADR_HIGHLIM(R10),R3; INSERT TASK SIZE IN 32 WORD BLOCKS
            53       0100 CA         0062       
            84            53     B0  0066   150 	MOVW	R3,(R4)+		;
                                     0069   151 					; SYSTEM CONTROLED PARTITION
                       26 AB     B4  0069   152 	CLRW	H.DSW(R11)		; SET PARTITION BASE ADDRESS
                                 05  006C   153 	RSB				; AND RETURN
                                     006D   154 10$:	$DSW	#IE.ADP			; INVALID BUFFER
