                                     0000    46 ; RSX$GLUN - RSX11M GET LUN IMFORMATION DIRECTIVE
                                     0000    47 ;
                                     0000    48 ; FUNCTIONAL DESCRIPTION:
                                     0000    49 ;
                                     0000    50 ; THIS IS A RSX DIRECTIVE HANDLER WHICH IMPLEMENTS AN RSX11M
                                     0000    51 ; "GLUN$" DIRECTIVE.
                                     0000    52 ;
                                     0000    53 ; INPUT PARAMETERS:
                                     0000    54 ;
                                     0000    55 ;	R5 = ADDRESS OF DPB+2
                                     0000    56 ;	R6 = CM IMAGE STACK ADDRESS
                                     0000    57 ;	R10 = ADDRESS OF CM IMAGE DATA BASE
                                     0000    58 ;	R11 = ADDRESS OF IMAGE HEADER
                                     0000    59 ;
                                     0000    60 ; OUTPUT PARAMETERS:
                                     0000    61 ;
                                     0000    62 ;	R10,R11 ARE PRESERVED
                                     0000    63 ;
                                     0000    64 ; IMPLICIT OUTPUTS:
                                     0000    65 ;
                                     0000    66 ;	THE PROPER DIRECTIVE STATUS IS PLACED IN THE IMAGE DSW
                                     0000    67 ;	ALONG WITH THE PROPER SETTING OF THE C-BIT.
                                     0000    68 ;
                                     0000    69 ; DSW RETURN CODES
                                     0000    70 ;
                                     0000    71 ;	IS.SUC	SUCCESSFUL
                                     0000    72 ;	IE.ULN	UNASSIGNED LUN
                                     0000    73 ;	IE.ILU	INVALID LOGICAL UNIT NUMBER
                                     0000    74 ;	IE.ADP	BUFFER ACCESS VIOLATION
                                     0000    75 ;--
                                     0000    76 RSX$GLUN::				; GET LUN IMFORMATION DIRECTIVE
                                     0000    77 ;
                                     0000    78 ; GET THE LUN NUMBER OF INTREST
                                     0000    79 ;
            51            85     3C  0000    80 	MOVZWL	(R5)+,R1		; 
                          44     15  0003    81 	BLEQ	110$			; IF LEQ THEN ERROR
       0164 CA            51     B1  0005    82 	CMPW	R1,I_NLUN(R10)		; IN RANGE?
                          3D     1A  000A    83 	BGTRU	110$			; IF GTRU THEN ERROR
                                     000C    84 ;
                                     000C    85 ; GET THE LUT ENTRY FOR THE LUN NUMBER
                                     000C    86 ;
            58       0168 DA41   D0  000C    87 	MOVL	@I_LUT(R10)[R1],R8	; ADDRESS THE LUT TABLE ENTRY
                          2B     13  0012    88 	BEQL	100$			; IF EQL THEN ERROR
            55            65     3C  0014    89 	MOVZWL	(R5),R5			; COPY ADDRESS OF BUFFER
            1B            55     E8  0017    90 	BLBS	R5,90$			; IF LOW SET THEN ERROR
                                     001A    91 ;
                                     001A    92 ; COPY DATA TO USER BUFFER
                                     001A    93 ;
            85         2E A8     B0  001A    94 	MOVW	LUT_INFONAME(R8),(R5)+	; INSERT INFO NAME
            85         30 A8     90  001E    95 	MOVB	LUT_INFOUNIT(R8),(R5)+	; INSERT INFO UNIT
            85            01     90  0022    96 	MOVB	#1,(R5)+		; INSERT LOADED DRIVER FLAG
         12 A8       8000 8F     A9  0025    97 	BISW3	#^X08000,LUT_CHAR(R8),(R5)+; INSERT CHARACTERISTICS
                          85         002B       
            85         1A A8     D0  002C    98 	MOVL	LUT_CHAR+8(R8),(R5)+	; INSERT DEVICE DEPENDENT WORDS
