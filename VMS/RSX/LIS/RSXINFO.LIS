                                     00D5   239 RSX$GTIM::				; GET TIME DIRECTIVE
            54            65     3C  00D5   240 	MOVZWL	(R5),R4			; ADDRESS BUFFER
            1A            54     E8  00D8   241 	BLBS	R4,10$			; IF BAD BUFFER THEN BR
                                     00DB   242 	$NUMTIM_S	(R4)		; INSERT TIME IN BUFFER
            0C            50     E9  00E6   243 	BLBC	R0,10$			; IF LOW CLEAR THEN ACCESS VIOLATION
            64       076C 8F     A2  00E9   244 	SUBW	#1900,(R4)		; TAKE OUT YEAR SINCE 0
         0E A4       0064 8F     B0  00EE   245 	MOVW	#100,14(R4)		; INSERT TICKS PER SECOND
                                 05  00F4   246 	RSB				; RETURN
