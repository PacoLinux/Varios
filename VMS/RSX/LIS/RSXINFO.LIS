                                     0077   185 ;--
                                     0077   186 RSX$GTSK::				; GET TASK PARAMETERS DIRECTIVE
            54            65     3C  0077   187 	MOVZWL	(R5),R4			; ADDRESS BUFFER
            4E            54     E8  007A   188 	BLBS	R4,20$			; BR IF BAD BUFFER
            84       0148 CA     D0  007D   189 	MOVL	I_RAD50NAME(R10),(R4)+	; INSERT TASK NAME
            84       2C8F 8F     3C  0082   190 	MOVZWL	#^O026217,(R4)+		; INSERT "GEN   " AS PARTITION
            84       8B9B 8F     B0  0087   191 	MOVW	#^O0105633,(R4)+	; INSERT VMS OWNER
            50       51CA 8F     3C  008C   192 	MOVZWL	#^O050712,R0		; ASSUME MCR 
       0170 CA            02     E1  0091   193 	BBC	#SV_DCL,I_STATE(R10),10$; BR IF NOT DCL
                          05         0096       
            50       1984 8F     3C  0097   194 	MOVZWL	#^O014604,R0		;
            84            50     B0  009C   195 10$:	MOVW	R0,(R4)+		; INSERT TO COMPLETE OWNER WORD
            84         30 AB     B0  009F   196 	MOVW	H.SPRI(R11),(R4)+	; INSERT PRIORITY
            84         08 AB     B0  00A3   197 	MOVW	H.CUIC(R11),(R4)+	; INSERT UIC
            84       0164 CA     B0  00A7   198 	MOVW	I_NLUN(R10),(R4)+	; INSERT NUMBER OF LUNS
                          84     D4  00AC   199 	CLRL	(R4)+			; INSERT UNDEFINED WORDS
            84       0194 CA     B0  00AE   200 	MOVW	I_SSTVEC(R10),(R4)+	; INSERT SST VECTOR DATA
            84       0196 CA     B0  00B3   201 	MOVW	I_SSTVECL(R10),(R4)+	;
            84       0100 CA     B0  00B8   202 	MOVW	I_ADR0+ADR_HIGHLIM(R10),(R4)+; INSERT SIZE IN BYTES
            84            05     B0  00BD   203 	MOVW	#5,(R4)+		; TELL ITS THE RSX!!!!!
            84       013D CA     90  00C0   204 	MOVB	I_UIC+1(R10),(R4)+	; INSERT UIC REMAINDER
            64       013F CA     90  00C5   205 	MOVB	I_UIC+3(R10),(R4)	;
