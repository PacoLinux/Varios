                                     0000    60 ;++
                                     0000    61 ;
                                     0000    62 ; FUNCTIONAL DESCRIPTION:
                                     0000    63 ;
                                     0000    64 ;	THIS ROUTINE POSTS I/O COMPLETION FOR THE INDICATED FCP REQUEST.
                                     0000    65 ;
                                     0000    66 ; CALLING SEQUENCE:
                                     0000    67 ;	CALL	IODONE (ARG1)
                                     0000    68 ;
                                     0000    69 ; INPUT PARAMETERS:
                                     0000    70 ;	ARG1: ADDRESS OF I/O PACKET
                                     0000    71 ;
                                     0000    72 ; IMPLICIT INPUTS:
                                     0000    73 ;	USER_STATUS: STATUS OF I/O REQUEST
                                     0000    74 ;
                                     0000    75 ; OUTPUT PARAMETERS:
                                     0000    76 ;	NONE
                                     0000    77 ;
                                     0000    78 ; IMPLICIT OUTPUTS:
                                     0000    79 ;	IOC$GL_PSBL: TAIL OF I/O POST QUEUE
                                     0000    80 ;
                                     0000    81 ; ROUTINE VALUE:
                                     0000    82 ;	NONE
                                     0000    83 ;
                                     0000    84 ; SIDE EFFECTS:
                                     0000    85 ;	I/O PACKET PLACED ON I/O POST QUEUE
                                     0000    86 ;	OPERATION COUNTED IN UCB
                                     0000    87 ;	VOLUME CHECKED FOR DISMOUNT
                                     0000    88 ;
                                     0000    89 ;--
                                     0000    90 
                                 00000000    91 	.PSECT	$CODE$,NOWRT,LONG
                                     0000    92 
                                     0000    93 IO_DONE::
                               01FC  0000    94 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8>	; SAVE REGISTERS
            56         04 AC     D0  0002    95 	MOVL	PACKET(AP),R6		; GET PACKET ADDRESS
         34 A6       0000'CF     7D  0006    96 	MOVQ	W^USER_STATUS,IRP$L_MEDIA(R6) ; SET STATUS IN PACKET
            58         1C A6     D0  000C    97 	MOVL	IRP$L_UCB(R6),R8	; GET UCB ADDRESS
            54         30 A8     D0  0010    98 	MOVL	UCB$L_VCB(R8),R4	; GET VCB ADDRESS
                       60 A8     D6  0014    99 	INCL	UCB$L_OPCNT(R8)		; COUNT COMPLETED OPERATION
                          00     EF  0017   100 	EXTZV	#IRP$V_FCODE,#IRP$S_FCODE,-
         20 A6            06         0019   101 		IRP$W_FUNC(R6),R7	; GET FUNCTION CODE WITHOUT QUALIFIERS
                          57         001C       
            0C            57     91  001D   102 	CMPB	R7,#IO$_READPBLK	; IF READ PHYSICAL
                          33     13  0020   103 	BEQL	10$
            0B            57     91  0022   104 	CMPB	R7,#IO$_WRITEPBLK	; OR WRITE DO SPECIAL PROCESSING
                          2E     13  0025   105 	BEQL	10$
                                     0027   106 ;
                                     0027   107 ; POST PROCESSING FOR ALL ACP FUNCTIONS: BUMP DOWN THE VOLUME TRANSACTION
                                     0027   108 ; COUNT AND DO THE FIXUPS FOR THE BUFFER PACKET.
                                     0027   109 ;
                       0C A4     B7  0027   110 	DECW	VCB$W_TRANS(R4)		; DEDUCT THIS REQ FROM TRANS COUNT
         2A A6            03     E1  002A   111 	BBC	#IRP$V_COMPLX,IRP$W_STS(R6),30$ ; BRANCH IF NO BUFFER PACKET
                          35         002E       
            54         2C B6     D0  002F   112 	MOVL	@IRP$L_SVAPTE(R6),R4	; GET BUFFER DESCRIPTOR ADDRESS
                       12 A4     B4  0033   113 	CLRW	<ABD$C_NAME*ABD$C_LENGTH>+ABD$W_COUNT(R4)
                                     0036   114 					; INHIBIT WRITE-BACK OF NAME STRING
IODONE          - POST REQUEST DONE TO USER                      21-AUG-1978 23:24:26   VAX-11 MACRO X0.3-11               Page   4
A0001                                                                                                                            (2)

            52         08 A4     9E  0036   115 	MOVAB	<ABD$C_FIB*ABD$C_LENGTH>+ABD$W_TEXT(R4),R2
            53            62     3C  003A   116 	MOVZWL	(R2),R3			; GET OFFSET ADDRESS OF FIB IN PACKET
            52            53     C0  003D   117 	ADDL	R3,R2			; COMPUTE ABSOLUTE ADDRESS
       0000'CF            2C     2C  0040   118 	MOVC5	#FIB$C_LENGTH,W^LOCAL_FIB,#0,-
                                     0045       
         0A A4            00         0045   119 		<ABD$C_FIB*ABD$C_LENGTH>+ABD$W_COUNT(R4),1(R2)
                       01 A2         0048       
                                     004A   120 					; COPY LOCAL FIB BACK INTO PACKET
         2A A6            01     E2  004A   121 	BBSS	#IRP$V_FUNC,IRP$W_STS(R6),30$ ; IF READ BIT IS SET, KEEP
                          15         004E       
         32 A6            05     B0  004F   122 	MOVW	#ABD$C_ATTRIB,IRP$W_BCNT(R6) ; ELSE DUMP ATTRIBUTE TEXT
                          0F     11  0053   123 	BRB	30$
                                     0055   124 ;
                                     0055   125 ; FOR READ/WRITE PHYSICAL, KNOCK DOWN THE VIRTUAL BIT IN THE PACKET. ONLY
                                     0055   126 ; ERRORS COME THROUGH HERE, AND WE DON'T WANT TO SEE THEM AGAIN (I/O POST
                                     0055   127 ; RECYCLES VIRTUAL I/O ERRORS FOR ACP ERROR PROCESSING). WE ALSO RESTORE
                                     0055   128 ; THE ORIGINAL SVAPTE AND BYTE COUNT FROM WHERE THEY WERE SAVED IN THE PACKET
                                     0055   129 ; SO THAT THE PAGES CAN BE UNLOCKED.
                                     0055   130 ;
         2A A6            04     E5  0055   131 10$:	BBCC	#IRP$V_VIRTUAL,IRP$W_STS(R6),20$ ; CLEAR THE VIRTUAL BIT
                          00         0059       
         2C A6         44 A6     D0  005A   132 20$:	MOVL	IRP$L_DIAGBUF(R6),IRP$L_SVAPTE(R6) ; RESTORE ORIGINAL PAGE ADDRESS
         32 A6         3E A6     B0  005F   133 	MOVW	IRP$W_OBCNT(R6),IRP$W_BCNT(R6)	   ; AND BYTE COUNT FOR PAGE UNLOCK
                                     0064   134 
                                     0064   135 
                          58     DD  0064   136 30$:	PUSHL	R8
   00000000'EF            01     FB  0066   137 	CALLS	#1,CHECK_DISMOUNT	; PUT CHECK HERE TO STOP RACE WITH USER DEALLOCATE
   00000000'FF            66     0E  006D   138 	INSQUE	(R6),@IOC$GL_PSBL	; INSERT PACKET INTO QUEUE
                          03     12  0074   139 	BNEQ	40$			; BRANCH IF QUEUE NOT EMPTY
                                     0076   140 	SOFTINT	#IPL$_IOPOST		; SIGNAL I/O POST INTERRUPT
                                 04  0079   141 40$:	RET
                                     007A   142 
                                     007A   143 
                                     007A   144 
                                     007A   145 	.END
