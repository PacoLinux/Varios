;	0424		IO_CHANNEL	: REF BBLOCK,		!CHANNEL # FOR I/O
;	0425		DISK_UCB	: REF BBLOCK,		!UCB ADDRESS OF 'SYS$DISK'
;	0426		IOC$GL_AQBLIST	: REF BBLOCK ADDRESSING_MODE(ABSOLUTE),
;	0427		FREE_PAGE_HEAD	: VECTOR [2],		!ADDRESS OF TWO LONG WORDS WHICH ARE THE FREE PAGE LIST HEAD

