; Digital Equipment Corporation
;
;	0428		LAST_PAGE,				!LAST PAGE IN PROGRAM REGION
;	0429		MAIL_CHANNEL,				!CHANNEL # FOR MAILBOX
;	0430		QUEUE_HEAD	: REF BBLOCK,		!ADDRESS OF QUEUE HEAD
;	0431		SCH$GL_CURPCB	: REF BBLOCK ADDRESSING_MODE(ABSOLUTE);	!ADDR OF CUR PCB
;	0432	
;	0433	EXTERNAL ROUTINE
;	0434		SYS$ASSIGN:	ADDRESSING_MODE(ABSOLUTE),	!ASSIGN DEVICE
;	0435		SYS$CREMBX:	ADDRESSING_MODE(ABSOLUTE);	!CREATE A MAILBOX
;	0436	
;	0437	
;	0438	! DISABLE THE KERNEL EXCEPTION VECTORS
;	0439	
;	0440	$SETEXV	(VECTOR = 0, ACMODE = EXEC_MODE, PRVHND = SYSEXV[0]);
;	0441	$SETEXV	(VECTOR = 1, ACMODE = EXEC_MODE, PRVHND = SYSEXV[1]);
;	0442	
;	0443	! ***** FOR DEBUG:  IF THE PREVIOUS HANDLER ADDRESSES ARE FOUND TO BE IN 
;	0444	! PROCESS SPACE, PUT THEM BACK SINCE THEY BELONG TO THE DEBUGGER
;	0445	
;	0446	IF .SYSEXV[0] GTR 0
;	0447	THEN $SETEXV (VECTOR = 0, ACMODE = EXEC_MODE, ADDRES = .SYSEXV[0]);
;	0448	IF .SYSEXV[1] GTR 0
;	0449	THEN $SETEXV (VECTOR = 1, ACMODE = EXEC_MODE, ADDRES = .SYSEXV[1]);
;	0450	
;	0451	! FIND THE QUEUE HEADER FOR THIS ACP BY SEARCHING THE SYSTEM AQB LIST
;	0452	! FOR AN AQB WITH A MATCHING PID.  THEN ASSIGN A CHANNEL TO 
;	0453	! 'SYS$DISK' (AS GOOD A DEVICE AS ANY) AND RECORD THE CHANNEL NUMBER.
;	0454	! SAID CHANNEL NUMBER IS USED FOR ALL I/O - IT IS ASSIGNED TO THE RIGHT
;	0455	! DEVICE SIMPLY BY STUFFING THE UCB POINTER IN THE CCB.
;	0456	! ALSO SAVE THE UCB ADDRESS IN THE CHANNEL SO THAT IT CAN BE RESTORED
;	0457	! TO PROPERLY DEASSIGN THE CHANNEL WHEN WE EXIT.
;	0458	AQB = .IOC$GL_AQBLIST;
;	0459	UNTIL .AQB EQL 0 OR .AQB[AQB$L_ACPPID] EQL .SCH$GL_CURPCB[PCB$L_PID] 
;	0460	DO AQB = .AQB[AQB$L_LINK];
;	0461	
;	0462	IF .AQB NEQ 0
;	0463	THEN QUEUE_HEAD = .AQB
;	0464	ELSE $EXIT(CODE = SS$_NOAQB);
;	0465	
;	0466	!
;	0467	! NOW LOCK SOME CODE AND DATA INTO THE WORKING SET.  SOME IS NECESSARY
;	0468	! INORDER TO RAISE IPL.
;	0469	!
;	0470	$ADJWSL	(PAGCNT = -10000);		!ADJUST WORKING SET TO MINIMUM
;	0471	$ADJWSL	(PAGCNT = 64);			!THEN ADD DESIRED SIZE
;	0472	
;	0473	(LOCAL STATUS;
;	0474	STATUS = $LKWSET(INADR = UPLIT(L_CODE_START, L_CODE_END));
;	0475	IF NOT .STATUS THEN $EXIT (CODE =.STATUS));
;	0476	
;	0477	(LOCAL STATUS;
;	0478	STATUS = $LKWSET(INADR = UPLIT (L_DATA_START, L_DATA_END));
;	0479	IF NOT .STATUS THEN $EXIT(CODE = .STATUS));
;	0480	
;	0481	! SETUP FREE PAGE HEADER
;	0482	FREE_PAGE_HEAD[0] = FREE_PAGE_HEAD;

; Bliss-32 7.352	Saturday 21-AUG-1978 23:36:00	DBB3:[MTAACP.SRC]INIMTA.B32;10					Page 2-2
; Digital Equipment Corporation
;
;	0483	FREE_PAGE_HEAD[1] = FREE_PAGE_HEAD;
;	0484	LAST_PAGE = 0;
;	0485	
;	0486	! ASSIGN CHANNEL FOR I/O AND CREATE A MAILBOX TO COMMUNICATE WITH OPERATOR
;	0487	IO_CHANNEL = 0;			!CLEAR UPPER WORD
;	0488	IF NOT SYS$ASSIGN(DESCRIPTOR('SYS$SYSTEM'),IO_CHANNEL,EXEC_MODE,0)
;	0489	THEN BUG_CHECK(NOACPCHAN);
;	0490	
;	0491	CCB = .CTL$GL_CCBBASE - .IO_CHANNEL;
;	0492	DISK_UCB = .CCB[CCB$L_UCB];
;	0493	
;	0494	MAIL_CHANNEL = 0;
;	0495	IF NOT SYS$CREMBX (0,MAIL_CHANNEL,MSGSIZE,,PROTECTION,EXEC_MODE,) 
;	0496	THEN BUG_CHECK (NOACPMAIL);
;	0497	
;	0498	
;	0499	! CLEAR THE TRANSITION BIT IN THE AQB TO TELL MOUNT THAT WE ARE ALIVE AND WELL.
;	0500	QUEUE_HEAD[AQB$V_CREATING] = 0;
;	0501	END;						!END OF ROUTINE


