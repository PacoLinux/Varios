;	0376	!
;	0377	! FUNCTIONAL DESCRIPTION:
;	0378	!	ONE TIME INITIALIZATION OF MTAACP
;	0379	!
;	0380	! CALLING SEQUENCE:
;	0381	!	INIT_MTAACP()
;	0382	!
;	0383	! INPUT PARAMETERS:
;	0384	!	NONE
;	0385	!
;	0386	! IMPLICIT INPUTS:
;	0387	!	IO DATA BASE
;	0388	!
;	0389	! OUTPUT PARAMETERS:
;	0390	!	NONE
;	0391	!
;	0392	! IMPLICIT OUTPUTS:
;	0393	!	IO_CHANNEL	CHANNEL # FOR I/O
;	0394	!	MAIL_CHANNEL	CHANNEL # FOR MAILBOX
;	0395	!	QUEUE_HEAD	ACP QUEUE LIST HEAD
;	0396	!	FREE_PAGE_HEAD - INITIALIZED TO BE EMPTY
;	0397	!	LAST_PAGE      - 0 (NOT DETERMINED YET)
;	0398	!
;	0399	! ROUTINE VALUE:
;	0400	!	NONE
;	0401	!
;	0402	! SIDE EFFECTS:
;	0403	!	NONE
;	0404	!
;	0405	!--
;	0406	
;	0407	BEGIN
;	0408	
;	0409	LOCAL
;	0410		SYSEXV	: VECTOR [2],			!SYSTEM EXCEPTION HANDLERS
;	0411		AQB	: REF BBLOCK,			!POINTER TO SCAN AQB LIST
;	0412		CCB	: REF BBLOCK;			!POINTER TO CHANNEL CONTROL BLOCK
;	0413	
;	0414	
;	0415	
