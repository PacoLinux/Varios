                                     0012    88 	.SBTTL	LOGIN DETACHED JOB
                                     0012    89 ;++
                                     0012    90 ; FUNCTIONAL DESCRIPTION:
                                     0012    91 ;
                                     0012    92 ;	THIS ROUTINE IS CALLED TO INITIATE THE NEXT COMMAND FILE
                                     0012    93 ;	FOR A BATCH JOB.  IF THIS IS THE INITIAL ENTRY FOR THE
                                     0012    94 ;	PROCESS, SOME ADDITIONAL INITIALIZATION IS COMPLETED.
                                     0012    95 ;	THIS INCLUDES EXTENDING THE RMS DATA AREA TO INCLUDE
                                     0012    96 ;	"OUTPUT" FILE AND DIRECTORY ID'S AND THE FILE NAME TO
                                     0012    97 ;	BE USED FOR SPOOLING THIS FILE WHEN THE JOB COMPLETES.
                                     0012    98 ;
                                     0012    99 ; CALLING SEQUENCE:
                                     0012   100 ;
                                     0012   101 ;	BSBW	LGI$INIDET		; INITIATE A DETACHED JOB
                                     0012   102 ;	BSBW	LGI$NXTINP		; NEXT INPUT FOR A DETACHED JOB
                                     0012   103 ;
                                     0012   104 ; INPUT PARAMETERS:
                                     0012   105 ;
                                     0012   106 ;	R11 IS PROCESS IMPURE DATA AREA
                                     0012   107 ;
                                     0012   108 ; IMPLICIT INPUTS:	NONE
                                     0012   109 ;
                                     0012   110 ; OUTPUT PARAMETERS:	NONE
                                     0012   111 ;
                                     0012   112 ; IMPLICIT OUTPUTS:
                                     0012   113 ;
                                     0012   114 ;	THE NEXT JOB STEP INPUT FILE IS OPEN AND LOGIN IS ALLOWED
                                     0012   115 ;	TO PROCEED, OR THE PROCESS IS LOGGED OUT.
                                     0012   116 ;
                                     0012   117 ; COMPLETION CODES:	NONE
                                     0012   118 ;
                                     0012   119 ; SIDE EFFECTS:	NONE
                                     0012   120 ;
                                     0012   121 ;--
                                     0012   122  
                                     0012   123 LGI$INIDET::				;
         54 AB       0040 8F     A8  0012   124 	BISW	#PRC_M_MODE,PRC_W_FLAGS(R11) ; INDICATE BATCH JOB
                                 C0  0018   125 	ADDL	#<PRD_K_XLENGTH-PRD_K_LENGTH>,- ; ADJUST STACK LIMIT
       0088 CB            20         0019   126 		PRC_L_STACKLM(R11)	; FOR EXTRA SPACE NEEDED FOR BATCH
                       02 AA     B4  001D   127 	CLRW	IDF_W_OUTIFI(R10)	; FLAG NO OUTPUT OPEN
                                     0020   128  
                                     0020   129 LGI$NXTINP::				;
            50         A0 8F     9A  0020   130 	MOVZBL	#MBREADSIZ,R0		; SIZE OF BUFFER TO READ
                                     0024   131 	$CREMBX_S	-		; CREATE A MAILBOX
                                     0024   132 		PROMSK	= #0,-		; NO PROTECTION
                                     0024   133 		MAXMSG	= R0,-		; BIGEST MESSAGE EVER
                                     0024   134 		BUFQUO	= R0,-		; ENOUGH BUFFER FOR 1 MESSAGES
                                     0024   135 		PRMFLG	= #0,-		; NON PERMANENT
                                     0024   136 		CHAN=LGI$W_MBCHAN(AP)	; CHANNEL ASSIGNED RETURNED HERE
            56       0000'CF     9E  0038   137 	MOVAB	W^LGI$G_OUTFAB,R6	; SET OUTPUT FAB
            57       0154 C8     9E  003D   138 	MOVAB	PRD_G_OUTRAB(R8),R7	; SET PROCESS OUTPUT RAB
         02 A6         02 AA     B0  0042   139 	MOVW	IDF_W_OUTIFI(R10),FAB$W_IFI(R6) ; SET IFI OF OUTPUT FILE IF ONE
            07       0094 CB     E8  0047   140 	BLBS	PRC_L_LSTSTATUS(R11),3$ ; BR IF EXIT STATUS WAS SUCESS
       0094 CB            06     93  004C   141 	BITB	#6,PRC_L_LSTSTATUS(R11)	; WAS TERMINAL STATUS ERROR OR SEVER
                          0D     12  0051   142 	BNEQ	7$			; IF NEQ YES, TERMINATE THIS JOB NOW!
            50       FFB1 CF     9E  0053   143 3$:	MOVAB	W^SMBMSG,R0		; ADDRESS OF MESSAGE DESCRIPTOR
                          0180   30  0058   144 	BSBW	SNDSMB			; SEND THE MESSAGE
