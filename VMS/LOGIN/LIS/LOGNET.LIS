                                     001B    68 ; LGI$NETWORK - LOGIN NETWORK CONNECTED JOB
                                     001B    69 ;
                                     001B    70 ; FUNCTIONAL DESCRIPTION:
                                     001B    71 ;
                                     001B    72 ; THIS ROUTINE IS ENTERED TO SET UP THE PROCESS PERMANENTFILES AND LOGICAL
                                     001B    73 ; NAMES FOR NETWORK CONNECTED JOBS.
                                     001B    74 ; IT IS ENTERED FROM LOGIN BASED ON PCB$V_NETWORK. (SET BY CREPRC)
                                     001B    75 ;
                                     001B    76 ; INPUT PARAMETERS:
                                     001B    77 ;
                                     001B    78 ;	R8 = ADDRESS OF THE PROCESS RMS DATA AREA
                                     001B    79 ;	R10 = ADDRESS OF THE CURRENT INDIRECT FRAME
                                     001B    80 ;	R11 = ADDRESS OF THE PROCESS LOGIN DATA AREA
                                     001B    81 ;
                                     001B    82 ; OUTPUT PARAMETERS:	NONE
                                     001B    83 ;
                                     001B    84 ;--
                                     001B    85 LGI$NETWORK::				; LOGIN NETWORK JOB
         54 AB       0040 8F     A8  001B    86 	BISW	#PRC_M_MODE,PRC_W_FLAGS(R11) ; INDICATE NON-INTERACTIVE JOB
                                 C0  0021    87 	ADDL	#<PRD_K_XLENGTH-PRD_K_LENGTH>,- ; ADJUST STACK LIMIT
       0088 CB            20         0022    88 		PRC_L_STACKLM(R11)	; FOR EXTRA SPACE NEEDED FOR BATCH
                       02 AA     B4  0026    89 	CLRW	IDF_W_OUTIFI(R10)	; FLAG NO OUTPUT OPEN
                                     0029    90 ;
                                     0029    91 ; VALIDATE THE USER
                                     0029    92 ;
            55       0000'CF     9E  0029    93 	MOVAB	W^LGI$Q_SYSODSC,R5	; ADDRESS TRANSLATION FOR SYS$OUTPUT
            55         04 A5     D0  002E    94 	MOVL	4(R5),R5		; ADDRESS ACTUAL STRING
                       01 A5     9F  0032    95 	PUSHAB	1(R5)			; BEGIN PARAM BUILD FOR VALIDATE WITH NAME
            7E            85     9A  0035    96 	MOVZBL	(R5)+,-(SP)		;
            55            6E     C0  0038    97 	ADDL	(SP),R5			; ADDRESS PASSWORD SECTION
                       01 A5     9F  003B    98 	PUSHAB	1(R5)			; INSERT PASSWORD DATA
            7E            85     9A  003E    99 	MOVZBL	(R5)+,-(SP)		;
            55            6E     C0  0041   100 	ADDL	(SP),R5			; ADDRESS NEXT SECTION
            50            85     9A  0044   101 	MOVZBL	(R5)+,R0		; GET ACCOUNT SECTION SIZE
            55            50     C0  0047   102 	ADDL	R0,R5			; PASS IT
                     0000'CF     7F  004A   103 	PUSHAQ	W^LGI$Q_UAFREC		;
                       04 AE     9F  004E   104 	PUSHAB	4(SP)			; INSERT PASSWORD ADDRESS
                       10 AE     9F  0051   105 	PUSHAB	16(SP)			;
   00000000'EF            07     FB  0054   106 	CALLS	#<3+4>,LGI$VALIDATE	; FIND USERS RECORD
            2E            50     E8  005B   107 	BLBS	R0,40$			; BR IF USER FOUND OK
            10            10     ED  005E   108 	CMPZV	#16,#16,R0,#1		; WAS ERROR FROM RMS
            01            50         0061       
                          22     13  0063   109 	BEQL	30$			; BR IF YES - LET THE JOB RUN
       0094 CB            50     D0  0065   110 	MOVL	R0,PRC_L_LSTSTATUS(R11)	; SAVE STATUS FOR EXIT
                          50     D4  006A   111 	CLRL	R0			; SET NO OTHER ERROR
                          FF91'  30  006C   112 	BSBW	LGI$FATAL		; DELARE MESSAGE
65 73 75 20 6B 72 6F 77 74 65 6E 00' 006F   113 	.ASCIC	/network user validation/
6E 6F 69 74 61 64 69 6C 61 76 20 72  007B       
                                 17  006F       
