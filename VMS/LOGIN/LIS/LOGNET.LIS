                          06     11  008A   115 	BRB	50$			;
                          FF71'  30  008C   116 40$:	BSBW	LGI$SETUSRDEF		; SET USER DEFAULTS
                          FF6E'  30  008F   117 	BSBW	LGI$CHK_JOBCNT		; SEE IF SYSTEM QUOTA EXCEEDED
