                                     005F   232 	.SBTTL	SETDEF - 
                                     005F   233 ;++
                                     005F   234 ; FUNCTIONAL DESCRIPTION:
                                     005F   235 ;
                                     005F   236 ; THIS ROUTINE INITIALIZES THE DEFAULT FILE NAME STRING, OPENS OR
                                     005F   237 ; CREATES THE FILE SPECIFIED, AND THE RETURNS THE RESULTANT FILE NAME
                                     005F   238 ; INSTEAD OF THE NAME PASSED TO OPEN.
                                     005F   239 ;
                                     005F   240 ; CALLING SEQUENCE:
                                     005F   241 ;
                                     005F   242 ;	CALL SETDEF(FILNAM,FILSIZ,MAXSIZ,DEFNAM,DEFSIZ,OPNTYP)
                                     005F   243 ;
                                     005F   244 ; INPUT PARAMETERS:
                                     005F   245 ;
                                     005F   246 ;	FILNAM(AP) - CONTAINS THE ADDRESS OF THE FILE NAME
                                     005F   247 ;	FILSIZ(AP) - CONTAINS THE ADDRESS OF THE FILE NAME SIZE
                                     005F   248 ;	MAXSIZ(AP) - CONTAINS THE ADDRESS OF THE MAXIMUM FILE NAME SIZE
                                     005F   249 ;	DEFNAM(AP) - CONTAINS THE ADDRESS OF THE DEFAULT FILE NAME
                                     005F   250 ;	DEFSIZ(AP) - CONTAINS THE ADDRESS OF THE DEFAULT FILE NAME SIZE
                                     005F   251 ;	OPNTYP(AP) - CONTAINS THE ADDRESS OF THE OPEN TYPE CODE, WHERE
                                     005F   252 ;			1 = NEW FILE
                                     005F   253 ;			0 = OLD FILE
                                     005F   254 ;
                                     005F   255 ; IMPLICIT INPUTS:
                                     005F   256 ;
                                     005F   257 ;	NONE
                                     005F   258 ;
                                     005F   259 ; OUTPUT PARAMETERS:
                                     005F   260 ;
                                     005F   261 ;	NONE
                                     005F   262 ;
                                     005F   263 ; IMPLICIT OUTPUTS:
                                     005F   264 ;
                                     005F   265 ;	FILE NAME AND SIZE ARE REPLACED WITH RESULTANT FILE NAME AND SIZE.
                                     005F   266 ;
                                     005F   267 ; COMPLETION CODES:
                                     005F   268 ;
                                     005F   269 ;	ERROR CODES WHEN UNABLE TO OPEN/CREATE A FILE.
                                     005F   270 ;
                                     005F   271 ; SIDE EFFECTS:
                                     005F   272 ;
                                     005F   273 ;	NONE
                                     005F   274 ;
                                     005F   275 ;--
                                     005F   276 
                                     005F   277 
                                     005F   278 
                                     005F   279 SETDEF::
                               0404  005F   280 	.WORD	^M<R2,R10>			; ENTRY POINT (OR MASK)
            5A       0000'CF     9E  0061   281 	MOVAB	W^INPFAB,R10			; ASSUME INPUT FILE
                       18 BC     95  0066   282 	TSTB	@OPNTYP(AP)			; IS IT AN OUTPUT FILE?
                          05     13  0069   283 	BEQL	5$				; BRANCH ON NO
            5A       0050'CF     9E  006B   284 	MOVAB	W^OUTFAB,R10			; GET OUTPUT FILE FAB
         2C AA         04 AC     D0  0070   285 5$:	MOVL	FILNAM(AP),FAB$L_FNA(R10)	; SET FILE NAME ADDR
         34 AA         08 BC     90  0075   286 	MOVB	@FILSIZ(AP),FAB$B_FNS(R10)	; SET FILE NAME SIZE
         30 AA         10 AC     D0  007A   287 	MOVL	DEFNAM(AP),FAB$L_DNA(R10)	; SET DEFAULT NAME ADDR
         35 AA         14 BC     90  007F   288 	MOVB	@DEFSIZ(AP),FAB$B_DNS(R10)	; SET DEFAULT NAME SIZE
CLIDAT          .IDENT /02/                                      22-AUG-1978 02:31:51   VAX-11 MACRO X0.3-11               Page   8
                SETDEF -                                                                                                         (4)

                       18 BC     95  0084   289 	TSTB	@OPNTYP(AP)			; NEW OR OLD FILE?
                          0E     13  0087   290 	BEQL	10$				; BRANCH ON OLD FILE
                                     0089   291 	$CREATE	FAB=(R10)			; CREATE NEW FILE
            3E            50     E9  0092   292 	BLBC	R0,50$				; BRANCH ON FAILURE
                          0C     11  0095   293 	BRB	20$				; GO GET RESULTANT NAME
                                     0097   294 10$:	$OPEN	FAB=(R10)			; OPEN OLD FILE
            30            50     E9  00A0   295 	BLBC	R0,50$				; BRANCH ON FAILURE
            50         28 AA     D0  00A3   296 20$:	MOVL	FAB$L_NAM(R10),R0		; GET FILE NAME BLOCK ADDR
            50         03 A0     9A  00A7   297 	MOVZBL	NAM$B_RSL(R0),R0		; GET RESULTANT NAME SIZE
         0C BC            50     91  00AB   298 	CMPB	R0,@MAXSIZ(AP)			; TOO LONG TO RETURN?
                          04     1B  00AF   299 	BLEQU	30$				; NO, NAME FITS
            50         0C BC     9A  00B1   300 	MOVZBL	@MAXSIZ(AP),R0			; SET MAX SIZE FOR RETURN NAME
            51       0110'CF     9E  00B5   301 30$:	MOVAB	W^RESNAM,R1			; GET ADDR OF RESULTANT NAME
            52         04 AC     D0  00BA   302 	MOVL	FILNAM(AP),R2			; GET RETURN NAME ADDR
         08 BC            50     B0  00BE   303 	MOVW	R0,@FILSIZ(AP)			; RETURN RESULTANT NAME SIZE
            82            81     90  00C2   304 40$:	MOVB	(R1)+,(R2)+			; SET RESULTANT NAME
            FA            50     F5  00C5   305 	SOBGTR	R0,40$				; MOVE IN ALL OF NAME
                                     00C8   306 	$CLOSE	FAB=(R10)			; CLOSE FILE
                          1B     11  00D1   307 	BRB	70$				; RETURN
            5A         28 AA     D0  00D3   308 50$:	MOVL	FAB$L_NAM(R10),R10		; GET ADDR OF FILE NAME BLOCK
            51         0B AA     9A  00D7   309 	MOVZBL	NAM$B_ESL(R10),R1		; GET EXPANDED NAME LENGTH
         08 BC            51     B0  00DB   310 	MOVW	R1,@FILSIZ(AP)			; SET LENGTH OF EXPANDED NAME
            52         04 AC     D0  00DF   311 	MOVL	FILNAM(AP),R2			; GET ADDR FOR NAME
            53       0110'CF     9E  00E3   312 	MOVAB	W^RESNAM,R3			; GET ADDR OF EXPANDED NAME
            82            83     90  00E8   313 60$:	MOVB	(R3)+,(R2)+			; MOVE IN ALL OF
            FA            51     F5  00EB   314 	SOBGTR	R1,60$				; EXPANDED NAME STRING
