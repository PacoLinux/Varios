                                     029D   161 	.SBTTL	UTILITY_START - 
                                     029D   162 ;++
                                     029D   163 ; FUNCTIONAL DESCRIPTION:
                                     029D   164 ;
                                     029D   165 ; THIS ROUTINE IS THE DRIVER FOR ANY FORTRAN UTILITY.  IT PASSES THE
                                     029D   166 ; CLI CALL-BACK ROUTINE ADDRESS TO THE UTILITY.  IT ALSO CONTAINS THE 
                                     029D   167 ; ACTION ROUTINES FOR THE CLI PARSING.  THE ROUTINES SETINP AND SETOUT
                                     029D   168 ; MOVE THE INPUT AND OUTPUT FILE NAMES FROM THE CLI WORK AREA TO THE
                                     029D   169 ; UTILITY'S STORAGE AREAS, ALONG WITH THE STRING LENGTHS.
                                     029D   170 ;
                                     029D   171 ; CALLING SEQUENCE:
                                     029D   172 ;
                                     029D   173 ;	CALLED FROM CLI TO INITIALIZE THE UTILITY.
                                     029D   174 ;
                                     029D   175 ; INPUT PARAMETERS:
                                     029D   176 ;
                                     029D   177 ;	4(AP) - CONTAINS THE TRANSFER ADDRESS
                                     029D   178 ;	8(AP) - CONTAINS THE CLI CALL-BACK ADDRESS
                                     029D   179 ;
                                     029D   180 ; IMPLICIT INPUTS:
                                     029D   181 ;
                                     029D   182 ;	NONE
                                     029D   183 ;
                                     029D   184 ; OUTPUT PARAMETERS:
                                     029D   185 ;
                                     029D   186 ;	INPUT PARAMETERS ARE PASSED ON TO MAIN ROUTINE.
                                     029D   187 ;
                                     029D   188 ; IMPLICIT OUTPUTS:
                                     029D   189 ;
                                     029D   190 ;	NONE
                                     029D   191 ;
                                     029D   192 ; COMPLETION CODES:
                                     029D   193 ;
                                     029D   194 ;	ALL ERROR CODES COMMON TO CLI AND THE UTILITY.
                                     029D   195 ;
                                     029D   196 ; SIDE EFFECTS:
                                     029D   197 ;
                                     029D   198 ;	NONE
                                     029D   199 ;
                                     029D   200 ;--
                                     029D   201 
                                     029D   202 
                                     029D   203 
                                 00000000   204 	.PSECT	CODE,RD,NOWRT,EXE
                                     0000   205 
