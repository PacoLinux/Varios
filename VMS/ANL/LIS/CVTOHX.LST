        C
        C	THIS SUBROUTINE CONVERTS A 4 BYTE INTEGER TO
        C	A HEXADECIMAL CHARACTER STRING WITH LEADING ZEROES SUPPRESSED.
        C
        C
0001    	SUBROUTINE CVTOHX(DUMMY,HEXARA)
        C
        C
0002    	BYTE	HEXARA(8),DUMMY(4),ZERO,BLANK
        C
0003    	DATA ZERO,BLANK/48,32/
        C
0004    	DO 500 I = 1,4
        C
0005    		DO 400 K = 0,1
        C
0006    			J = 2*I + K - 1
0007    			MASK = 15*( 16**K )
0008    			ICHAR = DUMMY(I) .AND. MASK
0009    			ICHAR = ICHAR/(16**K)
        C
0010    			IF( ICHAR .GT. 9 ) GO TO 200
0011    			ICHAR = ICHAR + 48
0012    			GO TO 300
        C
0013    200			ICHAR = ICHAR + 55
        C
0014    300			HEXARA(9-J) = ICHAR
FORTRAN IV-PLUS V02-51D		02:22:47    22-AUG-78		PAGE 2   
CVTOHX.FTN   	/TR:BLOCKS/WR

        C
0015    400		CONTINUE
        C
0016    500	CONTINUE
        C
0017    	DO 600 J = 1,7
        C
0018    		IF( HEXARA(J) .NE. ZERO ) GO TO 700
0019    		HEXARA(J) = BLANK
        C
0020    600	CONTINUE
        C
0021    700	RETURN
0022    	END
