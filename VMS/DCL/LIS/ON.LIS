         56 AB            02     B0  00E5   200 	MOVW	#2,PRC_W_ONLEVEL(R11)	;RESET ON ERROR LEVEL TO ERROR
            50            61     D0  00E9   201 10$:	MOVL	(R1),R0			;GET PRVIOUS HANDLER
                          11     13  00EC   202 	BEQL	20$			;IF EQL NONE
