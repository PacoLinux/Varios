                                     0014    47 ; DCL$ON - ON ERROR
                                     0014    48 ;
                                     0014    49 ; THIS ROUTINE IS CALLED AS  AN INTERNAL COMMAND TO EXECUTE THE ON DCLS COMMAND.
                                     0014    50 ;
                                     0014    51 ; INPUTS:
                                     0014    52 ;
                                     0014    53 ;	R6 = COMMAND BUFFER DISPLAY PARAMETER.
                                     0014    54 ;	R9 = ADDRESS OF NEXT BYTE IN COMMAND BUFFER.
                                     0014    55 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0014    56 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0014    57 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0014    58 ;
                                     0014    59 ; OUTPUTS:
                                     0014    60 ;
                                     0014    61 ;	THE ON CLAUSE IS PARSED AND THE CORRESPONDING ERROR LEVEL IS COMPUTED.
                                     0014    62 ;	STORAGE IS ALLOCATED FOR THE THEN CLAUSE AND IT IS COPIED FOR SUBSEQUENT
                                     0014    63 ;	REFERENCE WHEN AN ON CONDITION ACTUALLY EXISTS.
                                     0014    64 ;
                                     0014    65 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO ESTABLISH ON CONDITION PARAMETERS.
                                     0014    66 ;
                                     0014    67 ;		R0 = DCL$_ONERR - INVALID ERROR LEVEL SPECIFIED.
                                     0014    68 ;		R0 = DCL$_ONLEVL - ON CONDITION CAN NOT BE ESTABLISHED AT CURRENT
                                     0014    69 ;			COMMAND LEVEL.
                                     0014    70 ;		R0 = DCL$ONOVF - NO ROOM FOR ON CONDITION COMMAND TEXT.
                                     0014    71 ;		R0 = DCL$_PARMDEL - INVALID PARAMETER DELIMITER.
                                     0014    72 ;
                                     0014    73 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0014    74 ;
                                     0014    75 ;		R0 = DCL$NORMAL - NORMAL COMPLETION.
                                     0014    76 ;-
                                     0014    77  
                                     0014    78 DCL$ON::				;ON ERROR
            56            59     D0  0014    79 	MOVL	R9,R6			;SAVE COMMAND BUFFER BACKUP PARAMETER
                                     0017    80 	SETCHAR				;PEEK AT NEXT CHARACTER IN INPUT BUFFER
                          00A2   30  001A    81 	BSBW	90$			;CHECK FOR BLANK TERMINATOR
                          009C   30  001D    82 	BSBW	80$			;MOVE TERMINATOR, GET TOKEN, AND CHECK
            51            04     D1  0020    83 	CMPL	#4,R1			;ERROR LEVEL LONGER THAN 4 CHARACTERS
                          03     18  0023    84 	BGEQ	10$			;IF GEQ NO
            51            04     D0  0025    85 	MOVL	#4,R1			;SET LENGTH OF ERROR LEVEL TO 4
            55         D5 AF     9E  0028    86 10$:	MOVAB	LEVELTBL,R5		;GET ADDRESS OF ERROR LEVEL NAME TABLE
            57            04     D0  002C    87 	MOVL	#<LEVELEND-LEVELTBL>/5,R7 ;SET LOOP COUNT
            53            51     7D  002F    88 20$:	MOVQ	R1,R3			;COPY ERROR LEVEL NAME PARAMETERS
            58            85     9A  0032    89 	MOVZBL	(R5)+,R8		;SAVE ERROR LEVEL NUMBER
            50            55     D0  0035    90 	MOVL	R5,R0			;SET ADDRESS OF ERROR LEVEL NAME
                          85     D5  0038    91 	TSTL	(R5)+			;POINT TO NEXT ENTRY IN TABLE
            84            80     91  003A    92 30$:	CMPB	(R0)+,(R4)+		;CHARACTERS MATCH?
                          05     12  003D    93 	BNEQ	40$			;IF NEQ NO
            F8            53     F5  003F    94 	SOBGTR	R3,30$			;ANY MORE CHARACTERS TO COMPARE?
                          07     11  0042    95 	BRB	50$			;
            E8            57     F5  0044    96 40$:	SOBGTR	R7,20$			;ANY MORE ENTRIES TO COMPARE?
                                     0047    97 	STATUS	ONERR			;SET INVALID ERROR LEVEL SPECIFIED STATUS
                                 05  004A    98 	RSB				;
                                     004B    99  
                                     004B   100 ;
                                     004B   101 ; LEGAL ERROR LEVEL - PARSE THEN CLAUSE
