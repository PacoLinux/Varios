                                     00DA   174 ;+
                                     00DA   175 ; DCL$ONRESET - RESET ON ERROR PARAMETERS
                                     00DA   176 ;
                                     00DA   177 ; THIS ROUTINE IS CALLED TO RESET THE ON ERROR PARAMETERS TO THEIR DEFAULT
                                     00DA   178 ; VALUES.
                                     00DA   179 ;
                                     00DA   180 ; INPUTS:
                                     00DA   181 ;
                                     00DA   182 ;	NONE.
                                     00DA   183 ;
                                     00DA   184 ;	IT IS ASSUMED THAT CONTROL Y/C AST'S ARE DISABLED.
                                     00DA   185 ;
                                     00DA   186 ; OUTPUTS:
                                     00DA   187 ;
                                     00DA   188 ;	IF AN ON ERROR COMMAND BUFFER IS CURRENTLY ALLOCATED, THEN IT IS RETURNED
                                     00DA   189 ;	TO THE DYNAMIC STORAGE REGION. THE ON ERROR LEVEL IS SET TO 'ERROR' AND
                                     00DA   190 ;	THE ON ERROR COMMAND TEXT POINTER IS CLEARED.
                                     00DA   191 ;-
                                     00DA   192 	.ENABL	LSB
                                     00DA   193  
                                     00DA   194 DCL$ONCTLYRST::				;ON CONTROL Y RESET
