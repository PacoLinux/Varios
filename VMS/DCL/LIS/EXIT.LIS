                                     0000    33 	.SBTTL	EXIT COMMAND 
                                     0000    34 ;+
                                     0000    35 ; DCL$EXIT - EXIT COMMAND
                                     0000    36 ;
                                     0000    37 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE EXIT DCLS COMMAND.
                                     0000    38 ;
                                     0000    39 ; INPUTS:
                                     0000    40 ;
                                     0000    41 ;	R6 = COMMAND BUFFER DISPLAY PARAMETER.
                                     0000    42 ;	R9 = ADDRESS OF NEXT BYTE IN COMMAND BUFFER.
                                     0000    43 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0000    44 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0000    45 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0000    46 ;
                                     0000    47 ; OUTPUTS:
                                     0000    48 ;
                                     0000    49 ;	THE EXIT EXPRESSION IS EVALUATED
                                     0000    50 ;
                                     0000    51 ;	R0 LOW BIT CLEAR INDICATES EXPRESSION EVALUATION FAILURE.
                                     0000    52 ;
                                     0000    53 ;		R0 = DCL$_COMPLX - EXPRESSION TOO COMPLEX.
                                     0000    54 ;		R0 = DCL$_EXPSYN - EXPRESSION SYNTAX ERROR.
                                     0000    55 ;		R0 = DCL$_IVCHAR - INVALID CHARACTER IN NUMERIC STRING.
                                     0000    56 ;		R0 = DCL$_IVOPER - INVALID EXPRESSION OPERATOR.
                                     0000    57 ;		R0 = DCL$_UNDSYM - UNDEFINED SYMBOL.
                                     0000    58 ;
                                     0000    59 ;	IF THE EXPRESSION EVALUTES OK, THE INDIRECT LEVEL IS UNSTACKED
                                     0000    60 ;	ONE LEVEL AND THE STATUS IS SET TO THE VALUE OF THE EXPRESSION,
                                     0000    61 ;	OR IS LEFT AS THE ORIGINAL VALUE IF THE EXPRESSION WAS NULL.
                                     0000    62 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    63 ;
                                     0000    64 ;		R0 = DCL$_NORMAL - NORMAL COMPLETION.
                                     0000    65 ;-
                                     0000    66  
                                 00000000    67 	.PSECT	DCL$ZCODE,BYTE,RD,NOWRT
                                     0000    68 DCL$EXIT::				; EXIT COMMAND
       0094 CB   10000000 8F     C9  0000    69 	BISL3	#1@28,PRC_L_LSTSTATUS(R11),R8 ; GET LAST STATUS WITH INHIBIT MSG
                          58         0009       
                                     000A    70 	SETNBLK				; FIND NON-BLANK CHARACTER
                          09     13  000D    71 	BEQL	10$			; BR IF NONE-STATUS GETS SAVED
                                     000F    72 	EXPRESS				; EVALUATE EXPRESSION
            17            50     E9  0012    73 	BLBC	R0,40$			; BR IF ERROR EVALUATING EXPRESSION
            58            51     D0  0015    74 	MOVL	R1,R8			; SET RESULT OF EXPRESSION EVALUATION
                                 D1  0018    75 10$:	CMPL	PRC_L_INDINPRAB(R11),-	; IS INDIRECT LEVEL ABOVE THE
         08 AB         14 AB         0019    76 		PRC_L_INPRAB(R11)	; BASIC LEVEL?
                          0E     13  001D    77 	BEQL	50$			; BR IF NO THERE IS NOTHING TO UNSTACK
                                     001F    78 	UNSTACK				; REMOVE ONE LEVEL OF INDIRECT
            03            50     E9  0022    79 	BLBC	R0,30$			; BR IF FAILED
            50            58     D0  0025    80 20$:	MOVL	R8,R0			; SET RESULTANT STATUS
         FA AD            02     A8  0028    81 30$:	BISW	#WRK_M_COMMAND,WRK_W_FLAGS(FP) ; SET COMMAND IN EXECUTION
                                 05  002C    82 40$:	RSB
                                     002D    83  
                                     002D    84 ;
                                     002D    85 ; AN EXIT COMMAND ISSUED FOR COMMAND LEVEL 0
                                     002D    86 ; THIS IS A NOP FOR INTERACTIVE JOBS AND
                                     002D    87 ; A LOGOUT FOR BATCH JOBS.
                                     002D    88 ;
EXIT            - EXIT DCLS COMMAND EXECUTION                    22-AUG-1978 01:15:24   VAX-11 MACRO X0.3-11               Page   3
01              EXIT COMMAND                                                                                                     (1)

            58            01     D0  002D    89 50$:	MOVL	#1,R8			; SET GOOD STATUS INCASE CONTINUING
                                     0030    90 	SETBIT	WRK_V_NOSTAT,WRK_W_FLAGS(FP) ; AND NO TO CHANGE CURRENT STATUS
         54 AB            06     E1  0035    91 	BBC	#PRC_V_MODE,PRC_W_FLAGS(R11),20$ ; BR IF INTERACTIVE JOB
                          EB         0039       
                          FFC3'  31  003A    92 	BRW	DCL$LOGOUT		; ELSE FORCE LOGOUT
                                     003D    93 	.SBTTL	WAIT COMMAND 
                                     003D    94 ;+
                                     003D    95 ; DCL$WAIT - WAIT COMMAND
                                     003D    96 ;
                                     003D    97 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE WAIT COMMAND.
                                     003D    98 ;
                                     003D    99 ; INPUTS:
                                     003D   100 ;
                                     003D   101 ;	R6 = COMMAND BUFFER DISPLAY PARAMETER.
                                     003D   102 ;	R9 = ADDRESS OF NEXT BYTE IN COMMAND BUFFER.
                                     003D   103 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     003D   104 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     003D   105 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     003D   106 ;
                                     003D   107 ;	THE NEXT TOKEN ON THE COMMAND LINE MUST BE A VALID
                                     003D   108 ;	EXPRESSION FOR DELTA TIME MINUS THE NUMBER OF DAYS FIELD.
                                     003D   109 ;
                                     003D   110 ; OUTPUTS:
                                     003D   111 ;
                                     003D   112 ;	THE COMMAND FILE EXECUTION IS WAITED FOR THE SPECIFIED TIME.
                                     003D   113 ;-
                                     003D   114 DCL$WAIT::				;
            52            59     D0  003D   115 	MOVL	R9,R2			; COPY START OF TIME FIELD
            89       2030 8F     B0  0040   116 	MOVW	#^A/0 /,(R9)+		; SET ZERO DAY COUNT FOR WAIT
            56            59     D0  0045   117 	MOVL	R9,R6			; SET ERROR BACKUP POINTER
                                     0048   118 10$:	MOVCHAR				; MOVE A CHARACTER TO PARSE BUFFER
                          FB     12  004B   119 	BNEQ	10$			; BR IF MORE
                          59     D7  004D   120 	DECL	R9			; BACK UP OVER END_OF_LINE.
            59            52     C3  004F   121 	SUBL3	R2,R9,R1		; FIND LENGTH OF TIME SPECIFIED.
                          51         0052       
                          06     BB  0053   122 	PUSHR	#^M<R1,R2>		; BUILD DESCRIPTOR FOR TIME STRING
                                     0055   123 	$CANTIM_S			; CANCEL ANY PREVIOUS TIMERS
            53            5E     D0  0060   124 	MOVL	SP,R3			; SAVE ADDRESS OF QUAD WORD DECRIPTOR
                                     0063   125 	$BINTIM_S  (R3),(R3)		; CONVERT TIME TO BINARY DELTA FORMAT
            1B            50     E9  006E   126 	BLBC	R0,30$			; BR IF ERROR IN TIME
                                     0071   127 	$SETIMR_S  #31,(R3)		; SET TIMER FOR SPECIFIED TIME
            09            50     E9  0080   128 	BLBC	R0,30$			; BR IF ERROR SETTING TIMER
                                     0083   129 	$WAITFR_S  #31			; WAIT THE SPECIFIED TIME
                          8E     7C  008C   130 30$:	CLRQ	(SP)+			; REMOVE SCRATCH QUAD WORD FROM STACK
                                 05  008E   131 	RSB
                                     008F   132  
                                     008F   133 	.END
