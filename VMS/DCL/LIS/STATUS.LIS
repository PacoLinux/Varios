STATUS          - SET COMPLETION STATUS                          22-AUG-1978 01:13:02   VAX-11 MACRO X0.3-11               Page   2
01              SET COMPLETION STATUS                                                                                            (1)

                                     0006    41 	.SBTTL	SET COMPLETION STATUS
                                     0006    42 ;+
                                     0006    43 ; DCL$SET_STATUS - SET COMPLETION STATUS
                                     0006    44 ;
                                     0006    45 ; THIS ROUTINE IS CALLED TO SET THE COMPLETION STATUS OF A COMMAND. IT IS
                                     0006    46 ; CALLED FROM A POINT AT WHICH INDIRECT FILES AND ALL OTHER CONTEXT CAN BE
                                     0006    47 ; DESTROYED IF APPROPRIATE.
                                     0006    48 ;
                                     0006    49 ; INPUTS:
                                     0006    50 ;
                                     0006    51 ;	R0 = COMPLETION STATUS VALUE.
                                     0006    52 ;
                                     0006    53 ;	IT IS ASSUMED THAT "ON ERROR" CONDITIONS HAVE BEEN ESTABLISHED.
                                     0006    54 ;
                                     0006    55 ; OUTPUTS:
                                     0006    56 ;
                                     0006    57 ;	THE THREE COMPONENTS OF THE COMPLETION STATUS ARE CONVERTED TO ASCII
                                     0006    58 ;	AND EQUATED TO THEIR RESPECTIVE SYMBOL NAMES.
                                     0006    59 ;-
                                     0006    60  
                                     0006    61 DCL$SET_STATUS::			;SET COMPLETION STATUS
            05            50     E9  0006    62 	BLBC	R0,5$			;BR IF ERROR STATUS OF SOME KIND
         FA AD            09     E0  0009    63 	BBS	#WRK_V_NOSTAT,WRK_W_FLAGS(FP),40$ ; BR IF NO STATUS COMMAND
                          76         000D       
                                     000E    64 5$:	DISABLE				;DISABLE CONTROL Y/C AST'S
                          8E     7C  0011    65 	CLRQ	(SP)+			;REMOVE RETURN INFORMATION FROM STACK
                          50     DD  0013    66 	PUSHL	R0			;SAVE COMPLETION STATUS VALUE
                          0C     18  0015    67 	BGEQ	10$			;IF GEQ NOT COMMAND INTERPRETER STATUS
            50            50     CE  0017    68 	MNEGL	R0,R0			;NEGATE COMPLETION STATUS
            50       E000 8F     A8  001A    69 	BISW	#^X0E000,R0		;ADD CLI SUBSYSTEM SPECIFY DIVIDED BY 4
            50            04     C5  001F    70 	MULL3	#4,R0,(SP)		;CONVERT TO SCALED ERROR NUMBER
                          6E         0022       
            03            00     EF  0023    71 10$:	EXTZV	#0,#3,(SP),R0		;EXTRACT SEVERITY LEVEL
            50            6E         0026       
            50            30     C0  0028    72 	ADDL	#^A/0/,R0		;CONVERT SEVERITY LEVEL TO ASCII
            51         40 AB     D0  002B    73 	MOVL	PRC_L_SEVERITY(R11),R1	;GET ADDRESS OF SEVERITY LEVEL VALUE
            61            50     90  002F    74 	MOVB	R0,(R1)			;SET SEVERITY LEVEL VALUE
            71            01     90  0032    75 	MOVB	#1,-(R1)		;SET LENGTH OF SEVERITY LEVEL VALUE
            52         44 AB     D0  0035    76 	MOVL	PRC_L_STATUS(R11),R2	;GET ADDRESS OF STATUS VALUE VALUE
            50            6E     D0  0039    77 	MOVL	(SP),R0			;GET STATUS VALUE
         FF A2            0A     90  003C    78 	MOVB	#10,-1(R2)		;SET LENGTH OF STRING
            82       5825 8F     B0  0040    79 	MOVW	#^A/%X/,(R2)+		;SET RADIX CONTROL CHARACTERS
                          FFB8'  30  0045    80 	BSBW	DCL$CNVHXL		;CONVERT TO HEX LONGWORD
                          04     BA  0048    81 	POPR	#^M<R2>			;RETRIEVE FINAL STATUS
         54 AB            0B     E0  004A    82 	BBS	#PRC_V_YLEVEL,PRC_W_FLAGS(R11),40$ ;IF SET, CONTROL Y/C LEVEL
                          35         004E       
       0094 CB            52     D0  004F    83 	MOVL	R2,PRC_L_LSTSTATUS(R11)	;SAVE LAST STATUS VALUE FOR PROCESS
            2D            52     E8  0054    84 	BLBS	R2,40$			;IF LBS SUCCESSFUL COMPLETION
            03            00     EF  0057    85 	EXTZV	#0,#3,R2,R0		;EXTRACT SEVERITY LEVEL
            50            52         005A       
         56 AB            50     91  005C    86 	CMPB	R0,PRC_W_ONLEVEL(R11)	;FATAL ERROR?
                          22     19  0060    87 	BLSS	40$			;IF LSS NO
                                     0062    88 	FLUSH				;FLUSH COMMAND BUFFER
            50         58 AB     D0  0065    89 	MOVL	PRC_L_ONERROR(R11),R0	;GET ADDRESS OF ON COMMAND TEXT
                          04     12  0069    90 	BNEQ	30$			;IF NEQ COMMAND SPECIFIED
            50         92 AF     9E  006B    91 	MOVAB	EXIT,R0			;GET ADDRESS OF EXIT COMMAND TEXT
            51            80     9A  006F    92 30$:	MOVZBL	(R0)+,R1		;GET LENGTH OF ON COMMAND TEXT
