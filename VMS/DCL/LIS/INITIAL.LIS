                                     0000    43 	.SBTTL	COMMAND INTERPRETER START UP
                                     0000    44 ;+
                                     0000    45 ; DCL$STARTUP - COMMAND INTERPRETER START UP
                                     0000    46 ;
                                     0000    47 ; THIS ENTRY POINT IS JUMPED TO AT THE CONCLUSION OF LOGGING A USER ONTO
                                     0000    48 ; THE SYSTEM. ALL INPUT AND OUTPUT FILES ARE OPEN AND THE COMMAND LANGUAGE
                                     0000    49 ; INDEPENDENT DATA AREA HAS BEEN INITIALIZED.
                                     0000    50 ;-
                                     0000    51  
                                 00000000    52 	.PSECT	DCL$$BASE,BYTE,RD,NOWRT
                                     0000    53 DCL$STARTUP::				;COMMAND INTERPRETER START UP
                     0000'CF     17  0000    54 	JMP	W^START			;TRANSFER TO START UP CODE
                                 00000000    55 	.PSECT	DCL$ZCODE,BYTE,RD,NOWRT
            5E   00000008'9F     D0  0000    56 START:	MOVL	@#CTL$AL_STACK+8,SP	;RELOAD SUPERVISOR STACK POINTER
                                     0007    57 	$RUNDWN_S #PSL$C_USER		;RUNDOWN LOGIN IMAGE
                          7E     D4  0010    58 	CLRL	-(SP)			;SET UP DUMMY ZERO PSL
         16'AF            6E     FA  0012    59 	CALLG	(SP),B^10$		;CREATE INITIAL CALL FRAME
                          0000       0016    60 10$:	.WORD	^M<>			;ENTRY MASK
            5B   00000000'9F     9E  0018    61 	MOVAB	@#CTL$AG_CLIDATA,R11	;GET ADDRESS OF COMMAND WORK AREA
            6B            5C     7D  001F    62 	MOVQ	AP,PRC_L_SAVAP(R11)	;SAVE INITIAL ARGUMENT AND FRAME POINTERS
   00000000'9F       0002'CF     9E  0022    63 	MOVAB	W^DCL$UTLSERV+2,@#CTL$AL_CLICALBK ;SET CALL BACK VECTOR
                                     002B    64 	ENBCONTRLY			;ENABLE CONTROL Y AST'S
       0090 CB            04     B0  002E    65 	MOVW	#4,PRC_B_EXMDEPWID(R11)	;SET EXAMINE MODE TO HEX,WIDTH TO 4
                                     0033    66 	$DCLCMH_S W^DCL$CHANGE_MODE	; SET CHANGE MODE TO SUPER HANDLER
                          FFBB'  31  0042    67 	BRW	DCL$RESTART		;START COMMAND INTERPRETATION
                                     0045    68  
                                     0045    69 	.END
