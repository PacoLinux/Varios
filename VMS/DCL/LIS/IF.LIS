                                     0000    33 	.SBTTL	IF COMMAND 
                                     0000    34 ;+
                                     0000    35 ; DCL$IF - IF COMMAND
                                     0000    36 ;
                                     0000    37 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE IF DCLS COMMAND.
                                     0000    38 ;
                                     0000    39 ; INPUTS:
                                     0000    40 ;
                                     0000    41 ;	R6 = COMMAND BUFFER DISPLAY PARAMETER.
                                     0000    42 ;	R9 = ADDRESS OF NEXT BYTE IN COMMAND BUFFER.
                                     0000    43 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0000    44 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0000    45 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0000    46 ;
                                     0000    47 ; OUTPUTS:
                                     0000    48 ;
                                     0000    49 ;	THE IF CLAUSE IS EVALUATED AND IF THE RESULTANT VALUE IS TRUE, THE
                                     0000    50 ;	THEN CLAUSE IS EXECUTED BY RETURNING CONTROL TO THE COMMAND EXECUTION
                                     0000    51 ;	ENTRY POINT. OTHERWISE CONTROL IS RETURNED VIA THE RETURN ADDRESS.
                                     0000    52 ;
                                     0000    53 ;	R0 LOW BIT CLEAR INDICATES EXPRESSION EVALUATION FAILURE.
                                     0000    54 ;
                                     0000    55 ;		R0 = DCL$_COMPLX - EXPRESSION TOO COMPLEX.
                                     0000    56 ;		R0 = DCL$_EXPSYN - EXPRESSION SYNTAX ERROR.
                                     0000    57 ;		R0 = DCL$_IVCHAR - INVALID CHARACTER IN NUMERIC STRING.
                                     0000    58 ;		R0 = DCL$_IVOPER - INVALID EXPRESSION OPERATOR.
                                     0000    59 ;		R0 = DCL$_UNDSYM - UNDEFINED SYMBOL.
                                     0000    60 ;
                                     0000    61 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    62 ;
                                     0000    63 ;		R0 = DCL$_NORMAL - NORMAL COMPLETION.
                                     0000    64 ;-
                                     0000    65  
                                 00000000    66 	.PSECT	DCL$ZCODE,BYTE,RD,NOWRT
                                     0000    67 DCL$IF::				;IF COMMAND
                                     0000    68 	EXPRESS				;EVALUATE ARITHMETIC EXPRESSION
            09            50     E9  0003    69 	BLBC	R0,20$			;IF LBC EXPRESSION EVALUATION ERROR
            03            51     E9  0006    70 	BLBC	R1,10$			;IF LBC FALSE RESULT
                                     0009    71  
                                     0009    72 ;
                                     0009    73 ; EXECUTE THEN CLAUSE OF IF COMMAND
                                     0009    74 ;
                                     0009    75  
                          FFF4'  31  0009    76 	BRW	DCL$CMDSTART		;
                                     000C    77  
                                     000C    78 ;
                                     000C    79 ; BYPASS THEN CLAUSE OF IF COMMAND
                                     000C    80 ;
                                     000C    81  
                                     000C    82 10$:	STATUS	NORMAL			;SET NORMAL COMPLETION STATUS
                                 05  000F    83 20$:	RSB				;
                                     0010    84  
                                     0010    85 	.END
