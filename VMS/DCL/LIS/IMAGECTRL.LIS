                                     0036   126 	.SBTTL	STOP IMAGE EXECUTION
                                     0036   127 ;+
                                     0036   128 ; DCL$STOP - STOP IMAGE EXECUTION
                                     0036   129 ;
                                     0036   130 ; THIS ROUTINE IS CALLED AS AN INTERNAL COMMAND TO EXECUTE THE STOP DCLS
                                     0036   131 ; COMMAND.
                                     0036   132 ;
                                     0036   133 ; INPUTS:
                                     0036   134 ;
                                     0036   135 ;	R8 = ADDRESS OF SCRATCH BUFFER DESCRIPTOR.
                                     0036   136 ;	R9 = ADDRESS OF SCRATCH STACK.
                                     0036   137 ;	R10 = RESULT PARSE DESCRIPTOR TABLE POINTER.
                                     0036   138 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     0036   139 ;	FP = BASE ADDRESS OF COMMAND WORK AREA.
                                     0036   140 ;
                                     0036   141 ; OUTPUTS:
                                     0036   142 ;
                                     0036   143 ;	IF A PROCESS NAME OR IDENTIFICATION IS SPECIFIED, THEN THAT PROCESS IS
                                     0036   144 ;	DELETED.
                                     0036   145 ;
                                     0036   146 ;	IF THE JOB IS A NONINTERACTIVE JOB, THEN THE JOB IS LOGGED OFF THE SYSTEM
                                     0036   147 ;	WITH A STATUS OF NORMAL COMPLETION. OTHERWISE ALL INDIRECT FILE LEVELS ARE
                                     0036   148 ;	UNSTACKED AND A TEST IS MADE TO DETERMINE IF AN IMAGE WAS INTERRUPTED VIA
                                     0036   149 ;	A CONTROL C/Y. IF A PREVIOUS IMAGE WAS INTERRUPTED, THEN THE CONTEXT OF THE
                                     0036   150 ;	RUN COMMAND THAT INITIATED IMAGE EXECUTION IS REMOVED FROM THE STACK AND
                                     0036   151 ;	RMS-32 IS CALLED TO CLOSE ALL OPEN IMAGE FILES. OTHERWISE NO OPERATION IS
                                     0036   152 ;	PERFORMED.
                                     0036   153 ;-
                                     0036   154  
                                     0036   155 DCL$STOP::				;STOP IMAGE EXECUTION
                                     0036   156 	GETDVAL				;GET DESCRIPTOR VALUES
            55            04     D1  0039   157 	CMPL	#PTR_K_ENDLINE,R5	;END OF LINE?
                          27     12  003C   158 	BNEQ	40$			;IF NEQ NO
         14 AB         08 AB     D1  003E   159 10$:	CMPL	PRC_L_INPRAB(R11),PRC_L_INDINPRAB(R11) ;INDIRECT LEVEL ZERO?
                          05     13  0043   160 	BEQL	20$			;IF EQL YES
                                     0045   161 	UNSTACK				;UNSTACK INDIRECT LEVEL
                          F4     11  0048   162 	BRB	10$			;
         54 AB            06     E0  004A   163 20$:	BBS	#PRC_V_MODE,PRC_W_FLAGS(R11),30$ ;IF SET, NONINTERACTIVE JOB
                          13         004E       
                          56     10  004F   164 	BSBB	TESTMODE		;TEST PREVIOUS MODE
            5D         CC AD     D0  0051   165 	MOVL	WRK_L_SAVFP(FP),FP	;RESTORE SAVED FRAME POINTER
                                     0055   166 	SHUTDOWN			;CLOSE FILES OF PREVIOUS IMAGE
                                     0058   167 	$RUNDWN_S #PSL$C_USER		;RUN DOWN PREVIOUS IMAGE
                                 05  0061   168 	RSB				;
                          FF9B'  31  0062   169 30$:	BRW	DCL$LOGOUT		;LOG PROCESS OUT
                                     0065   170  
                                     0065   171 ;
                                     0065   172 ; DELETE PROCESS
                                     0065   173 ;
                                     0065   174  
                          79     D4  0065   175 40$:	CLRL	-(R9)			;CLEAR PROCESS IDENTIFICATION
            57            58     D0  0067   176 	MOVL	R8,R7			;COPY ADDRESS OF SCRATCH DESCRIPTOR
            55            00     D1  006A   177 	CMPL	#PTR_K_COMDQUAL,R5	;COMMAND QUALIFIER SPECIFIED?
                          25     12  006D   178 	BNEQ	70$			;IF NEQ NO
                                     006F   179 	GETDVAL				;GET VALUE PARAMETERS
            52            51     7D  0072   180 	MOVQ	R1,R2			;SAVE VALUE PARAMETERS
                          52     D7  0075   181 50$:	DECL	R2			;ANY MORE CHARACTERS TO CONVERT?
