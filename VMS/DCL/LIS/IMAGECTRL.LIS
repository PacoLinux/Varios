01              SHUT DOWN IMAGE                                                                                                  (1)

                                     012B   272 	.SBTTL	SHUT DOWN IMAGE
                                     012B   273 ;+
                                     012B   274 ; DCL$SHUTDOWN - SHUT DOWM IMAGE
                                     012B   275 ;
                                     012B   276 ; THIS ROUTINE IS CALLED TO CLOSE ALL FILES OPENED BY THE JUST EXECUTED IMAGE
                                     012B   277 ; AND TO CLOSE THE IMAGE ACTIVATION FILE.
                                     012B   278 ;
                                     012B   279 ; INPUTS:
                                     012B   280 ;
                                     012B   281 ;	R11 = BASE ADDRESS OF PROCESS WORK AREA.
                                     012B   282 ;
                                     012B   283 ; OUTPUTS:
                                     012B   284 ;
                                     012B   285 ;	ALL FILES OPENED BY THE JUST EXECUTED IMAGE ARE CLOSED BY CALLING RMS-32,
                                     012B   286 ;	DATA RECORDS ARE SKIPPED IN THE INPUT STREAM, AND THE IMAGE FILE IS CLOSED.
                                     012B   287 ;
                                     012B   288 ;		R2 = NUMBER OF DATA RECORDS SKIPPED IN THE INPUT STREAM.
                                     012B   289 ;-
                                     012B   290  
                                     012B   291 DCL$SHUTDOWN::				;SHUT DOWN IMAGE
                                     012B   292 	ALLOCBUF			;ALLOCATE BUFFER AND DESCRIPTOR
            62         84 8F     9A  012E   293 10$:	MOVZBL	#MSGBUFSIZ,(R2)		;RESET SIZE OF MESSAGE BUFFER
                          00     DD  0132   294 	PUSHL	#0			;RUN DOWN ONLY IMAGE FILES
                          62     9F  0134   295 	PUSHAB	(R2)			;SET ADDRESS OF MESSAGE BUFFER DESCRIPTOR
   00000000'9F            02     FB  0136   296 	CALLS	#2,@#SYS$RMSRUNDWN	;RUNDOWN RMS-32 FILES
            EE            50     E9  013D   297 	BLBC	R0,10$			;IF RUNDOWN FAILURE CONTINUE WITH NEXT FILE
            5E       008C CE     9E  0140   298 	MOVAB	MSGBUFSIZ+8(SP),SP	;DEALLOCATE MESSAGE BUFFER AND DESCRIPTOR
                          52     D4  0145   299 	CLRL	R2			;CLEAR COUNT OF RECORDS SKIPPED
            53         14 AB     D0  0147   300 	MOVL	PRC_L_INDINPRAB(R11),R3	;GET ADDRESS OF INDIRECT RAB
            7E         10 A3     7D  014B   301 	MOVQ	RAB$W_RFA(R3),-(SP)	;SAVE RFA OF LAST COMMAND
