                          04     11  00C4   240 	BRB	5$			;
                                     00C6   241  
                                     00C6   242 DCL$RUNDOWN::				;RUN DOWN IMAGE AND INDIRECT LEVELS
            50         1E'AF     9E  00C6   243 	MOVAB	B^10$,R0		;SET EXIT HANDLER RETURN ADDRESS
         54 AB            04     A8  00CA   244 5$:	BISW	#PRC_M_DISABL,PRC_W_FLAGS(R11) ;DISABLE CONTROL Y/C AST'S
         54 AB            0B     E5  00CE   245 	BBCC	#PRC_V_YLEVEL,PRC_W_FLAGS(R11),20$ ;IF CLR, NOT AT CONTROL Y/C LEVEL
                          57         00D2       
                          50     DD  00D3   246 	PUSHL	R0			;PUSH PROPER RETURN ADDRESS
            50            6B     D0  00D5   247 	MOVL	PRC_L_SAVAP(R11),R0	;GET ADDRESS OF PREVIOUS PSL
            60            18     E1  00D8   248 	BBC	#PSL$V_CURMOD,(R0),10$	;IF CLR, PREVIOUS MODE SUPERVISOR
                          42         00DB       
            70   00000000'9F     9E  00DC   249 	MOVAB	@#EXE$EXIT_IMAGE,-(R0)	;RESET USER RETURN ADDRESS
         F8 A0       0980 8F     3C  00E3   250 	MOVZWL	#SS$_CLIFRCEXT,-8(R0)	;SET EXIT CAUSE INTO SAVED R0
            5E            5D     C3  00E9   251 	SUBL3	FP,SP,WRK_L_SAVSP(FP)	;SAVE RELATIVE ADDRESS OF TOP OF STACK
                       C4 AD         00EC       
            58         C8 AD     7D  00EE   252 	MOVQ	WRK_L_SAVAP(FP),R8	;RETRIEVE PREVIOUS ARGUMENT AND FRAME POINTERS
            6B            58     7D  00F2   253 	MOVQ	R8,PRC_L_SAVAP(R11)	;SAVE IN PROCESS WORK AREA
       FD34 CD            5D     C2  00F5   254 	SUBL	FP,WRK_L_RSLNXT(FP)	;CONVERT PARSE POINTER TO RELATIVE ADDRESS
            57         08 A0     9E  00FA   255 	MOVAB	8(R0),R7		;GET ADDRESS OF END OF ARGUMENT LIST + 4
            57            5D     C2  00FE   256 	SUBL	FP,R7			;CALCULATE LENGTH OF CALL FRAME AND ARGLIST
            5E            57     C2  0101   257 	SUBL	R7,SP			;CALCULATE NEW TOP OF STACK ADDRESS
