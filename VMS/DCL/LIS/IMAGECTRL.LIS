                                     00C0   219 	.SBTTL	RUN DOWN IMAGE AND INDIRECT LEVELS
                                     00C0   220 ;+
                                     00C0   221 ; DCL$RUNDOWN - RUN DOWN IMAGE AND INDIRECT LEVELS
                                     00C0   222 ;
                                     00C0   223 ; THIS SUBROUTINE IS CALLED TO CHECK WHETHER INDIRECT LEVELS SHOULD BE RUN DOWN
                                     00C0   224 ; AND TO CLOSE RMS-32 FILES AND RUN DOWN THE PREVIOUS IMAGE.
                                     00C0   225 ;
                                     00C0   226 ; INPUTS:
                                     00C0   227 ;
                                     00C0   228 ;	NONE.
                                     00C0   229 ;
                                     00C0   230 ; OUTPUTS:
                                     00C0   231 ;
                                     00C0   232 ;	IF THE CURRENT LEVEL IS CONTROL Y/C, THEN ALL INDIRECT FILES ARE UNSTACKED.
                                     00C0   233 ;	IF THE PREVIOUS MODE WAS USER, THEN THE USER IMAGE EXIT HANDLERS ARE
                                     00C0   234 ;	EXECUTED. THE PREVIOUS IMAGE IS ALWAYS RUNDOWN.
                                     00C0   235 ;-
                                     00C0   236 	.ENABL	LSB
                                     00C0   237  
                                     00C0   238 DCL$RUNDWNI::				;RUN DOWN BUT PRESERVE INDIRECT LEVEL
