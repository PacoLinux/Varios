0001    	SUBROUTINE	LINCHK(LUN,COUNT)
0002    	BYTE		LUN
0003    	INTEGER*2	COUNT
         
        C
        C	INCLUDE FILES:
        C			NONE
         
        C
        C	OWN STORAGE:
        C
0004    	INTEGER*2	LINES
         
0005    	DATA		LINES/56/
         
0006    	LINES	= LINES + COUNT
         
0007    	IF(LINES .LT. 56)  RETURN
         
0008    	CALL TOF(LUN)
         
0009    	LINES	= COUNT
         
0010    	RETURN
         
         
0011    	ENTRY		FRCTOF(LUN)
         
0012    	CALL TOF(LUN)
         
0013    	LINES	= 0
         
0014    	RETURN
0015    	END

