0086    	FUNC	= IAND(REG(MTCS1),'3E'X)
0087    	OPERATION = 'INVALID'
         
0088    	IF(FUNC .EQ. '00'O)  OPERATION = 'NO OP'
0089    	IF(FUNC .EQ. '02'O)  OPERATION = 'REWIND OFF-LINE'
0090    	IF(FUNC .EQ. '06'O)  OPERATION = 'REWIND'
0091    	IF(FUNC .EQ. '10'O)  OPERATION = 'DRIVE CLEAR'
0092    	IF(FUNC .EQ. '24'O)  OPERATION = 'ERASE'
0093    	IF(FUNC .EQ. '26'O)  OPERATION = 'WRITE TAPE MARK'
0094    	IF(FUNC .EQ. '30'O)  OPERATION = 'SPACE FORWARD'
0095    	IF(FUNC .EQ. '32'O)  OPERATION = 'SPACE REVERSE'
0096    	IF(FUNC .EQ. '50'O)  OPERATION = 'WRITE CHECK FORWARD'
0097    	IF(FUNC .EQ. '56'O)  OPERATION = 'WRITE CHECK REVERSE'
0098    	IF(FUNC .EQ. '60'O)  OPERATION = 'WRITE FORWARD'
0099    	IF(FUNC .EQ. '70'O)  OPERATION = 'READ FORWARD'
0100    	IF(FUNC .EQ. '76'O)  OPERATION = 'READ REVERSE'
         
0101    	CALL LINCHK(LUN,1)
         
0102    	WRITE(LUN,601) FUNC,OPERATION
0103     601	FORMAT(' ',T40,'FUNCTION = O''',O2,''' = ',A)
         
0104    	CALL DEVREG(LUN,'MTDS',REG(MTDS),MTDS_BITS(0))
         
0105    	CALL DEVREG(LUN,'MTER',REG(MTER),MTER_BITS(0))
         
0106    	CALL DEVREG(LUN,'MTMR',REG(MTMR),'0')
         
0107    	CALL DEVREG (LUN, 'MTAS', REG(MTAS),'0')
        	
0108    	CALL DEVREG(LUN,'MTFC',REG(MTFC),'0')
         
0109    	CALL DEVREG(LUN,'MTDT',REG(MTDT),'0')
         
0110    	CALL DEVREG(LUN,'MTCK',REG(MTCK),'0')
         
0111    	CALL DEVREG(LUN,'MTSN',REG(MTSN),'0')
         
0112    	SLV SEL	= IAND(REG(MTTC),'03'X)
0113    	FMT SEL	= IAND(REG(MTTC),2**7+2**6+2**5+2**4) / 2**4
         
0114    	MODE	= 'INVALID FMT'
         
0115    	IF(FMTSEL .EQ. '0C'X)  MODE = 'NORMAL MODE'
0116    	IF(FMTSEL .EQ. '0D'X)  MODE = 'CORE DUMP'
0117    	IF(FMTSEL .EQ. '0E'X)  MODE = '15 MODE'
0118    	DEN SEL	= IAND(REG(MTTC),2**10+2**9+2**8) / 2**8
         
0119    	CALL LINCHK(LUN,3+BITCNT(IAND(REG(MTTC),2**4)))
         
0120    	WRITE(LUN,751)  REG(MTTC),DENSEL,DENSITY(DEN SEL),FMTSEL,MODE
0121     751	FORMAT(' ',T10,'MTTC',T23,Z8,T40,'DENSITY = ',I2,'. = ',A/
        	1	' ',T40,'FORMAT SELECT =',I3,'. = ',A)
         
0122    	IF( IAND(REG(MTTC), 2**4) .EQ. 2**4
        	1	.AND.

TE16            22-Aug-1978 02:51:29	VAX-11 FORTRAN IV-PLUS V1.0-2      Page 4   
					TE16.FOR.11         

        	2		DEN SEL .NE. 4)
        	3					WRITE(LUN,761)
0123     761						FORMAT(' ',T40,'EVEN PARITY')
         
0124    	WRITE(LUN,771)  SLV SEL
0125     771	FORMAT(' ',T40,'SLAVE SELECT =',I2,'.')
         
0126    	CALL MBAREG(LUN,REG)
         
0127    	RETURN
         
         
0128    	ENTRY		B_TE16(LUN,REG)
         
0129    	CALL LINCHK(LUN,3)
0130    	WRITE (LUN,800)TITLE
0131    800	FORMAT ('0',T10,A)
0132    	CALL CRYPTK(LUN,'        ',REG(MTCS1),10)
         
0133    	RETURN
         
0134    	ENTRY		C_TE16(LUN,REG)
         
0135    	CALL LINCHK(LUN,1)
0136    	CALL CRYPTK(LUN,'TE16    ',REG(MTCS1),10)
         
0137    	RETURN
         
0138    	END

