                                     00DE 15200 ;++
                                     00DE 15300 ; FUNCTIONAL DESCRIPTION:
                                     00DE 15400 ;
                                     00DE 15500 ;	THIS ROUTINE USES A 5X7 ARRAY OF BITS TO GENERATE
                                     00DE 15600 ;	CHARACTERS ON THE PRINTER.  THE ROUTINE IS USED TO GENERATE
                                     00DE 15700 ;	THE FLAG PAGE.
                                     00DE 15800 ;
                                     00DE 15900 ; CALLING SEQUENCE:
                                     00DE 16000 ;
                                     00DE 16100 ;	BSB/JSB	SMB$CHRGEN
                                     00DE 16200 ;
                                     00DE 16300 ; INPUT PARAMETERS:
                                     00DE 16400 ;
                                     00DE 16500 ;	R2 CONTAINS THE COUNT OF CHARACTER TO OUTPUT
                                     00DE 16600 ;	R3 CONTAINS THE ADDRESS
                                     00DE 16700 ;	R4 CONTAINS:
                                     00DE 16800 ;
                                     00DE 16900 ;		+--------+--------+--------+--------+
                                     00DE 17000 ;		! SPACING! HEIGTH ! WIDTH  ! SPACOUT!
                                     00DE 17100 ;		+--------+--------+--------+--------+
                                     00DE 17200 ; WHERE:
                                     00DE 17300 ;	SPACOUT IS THE NUMBER OF LEADING SPACES ON EACH LINE
                                     00DE 17400 ;	WIDTH   IS THE NUMBER OF TIMES TO REPEAT A CHARACTER ON A ROW
                                     00DE 17500 ;	HEIGTH  IS THE NUMBER OF TIMES EACH LINE IS PRINTED
                                     00DE 17600 ;	SPACING IS THE NUMBER IF BLANK CHARACTERS BETWEEN BIG CHARACTERS
                                     00DE 17700 ;
                                     00DE 17800 ;
                                     00DE 17900 ; IMPLICIT INPUTS:
                                     00DE 18000 ;
                                     00DE 18100 ;	R11 CONTAINS THE ADDRESS OF IMPURE DATA BLOCK
                                     00DE 18200 ;
                                     00DE 18300 ; OUTPUT PARAMETERS:
                                     00DE 18400 ;
                                     00DE 18500 ;	THE WIDE LINE IS PRINTED
                                     00DE 18600 ;
                                     00DE 18700 ; IMPLICIT OUTPUTS:
                                     00DE 18800 ;
                                     00DE 18900 ;	NONE
                                     00DE 19000 ;
                                     00DE 19100 ; COMPLETION CODES:
                                     00DE 19200 ;
                                     00DE 19300 ;	NONE
                                     00DE 19400 ;
                                     00DE 19500 ; SIDE EFFECTS:
                                     00DE 19600 ;
                                     00DE 19700 ;	NONE
                                     00DE 19800 ;
                                     00DE 19900 ;--
                                     00DE 20000  
                                     00DE 20100  
                                     00DE 20200 
                                     00DE 20300 SMB$CHRGEN::				;PRINT A WIDE LINE
                     01FC 8F     BB  00DE 20400 	PUSHR	#^M<R2,R3,R4,R5,R6,R7,R8> ; SAVE NON-VOLATILE REGISTERS
                          58     D4  00E2 20500 	CLRL	R8			;ZERO ROW COUNTER
            56            6E     7D  00E4 20600 10$:	MOVQ	COUNT(SP),R6		;GET COUNT TO R6, LINE ADDRESS TO R7
            50         2B AB     D0  00E7 20700 	MOVL	SD_A_TBUFADR(R11),R0	;BASE ADDRESS OF LINE BUFFER
