            51            67     9A  0120 22800 70$:	MOVZBL	(R7),R1			;GET THE CHARACTER
            64            53     E0  0123 22900 	BBS	R3,(R4),80$		;BR IF NO SPACE THIS TIME
                          03         0126       
            51            20     9A  0127 23000 	MOVZBL	#^A/ /,R1		;PRINT SPACES
            52         0A AE     9A  012A 23100 80$:	MOVZBL	WIDTH(SP),R2		;GET NUMBER OF CHARACTERS PER COLUMN
                          3C     10  012E 23200 	BSBB	200$			;OUTPUT CHARACTERS TO THE LINE
                          53     D6  0130 23300 	INCL	R3			;ADJUST BIT NUMBER UP 1
            EB            55     F5  0132 23400 	SOBGTR	R5,70$			;REPEAT FOR 5 TIMES-NUMBER COLUMNS PER
