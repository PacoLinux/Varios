            66       5003 8F     B0  004A 13810 	MOVW	#FAB$C_BID+<FAB$C_BLN@8>,FAB$B_BID(R6) ;CREATE FAB
                                     004F 13815 	ASSUME 	RAB$B_BID+1 EQ RAB$B_BLN
            67       4401 8F     B0  004F 13820 	MOVW	#RAB$C_BID+<RAB$C_BLN@8>,RAB$B_BID(R7) ;CREATE RAB
                                     0054 13825 	ASSUME	NAM$B_BID+1 EQ NAM$B_BLN
            68       3802 8F     B0  0054 13830 	MOVW	#NAM$C_BID+<NAM$C_BLN@8>,NAM$B_BID(R8) ;CREATE NAM BLK
                                     0059 13835 
         28 A6            58     D0  0059 14200 	MOVL	R8,FAB$L_NAM(R6)	;SET NAME BLOCK ADDRESS IN FAB
         3C A7            56     D0  005D 14300 	MOVL	R6,RAB$L_FAB(R7)	;SET FAB ADDRESS IN RAB
         20 A7       0100 8F     B0  0061 14400 	MOVW	#SMB$K_LBUFSIZ,RAB$W_USZ(R7) ;SET RECORD BUFFER SIZE
         24 A7       01CA CB     DE  0067 14450 	MOVAL	SD_T_LBUF(R11),RAB$L_UBF(R7) ;SET RECORD BUFFER ADDR
                                     006D 14475 	SETBIT	RAB$V_RAH,RAB$L_ROP(R7)	;USE READ-AHEAD
                                     0072 14500 
       00C4 CB            0C     9A  0072 14600 	MOVZBL	#QIO$_NARGS,SD_G_QIOBLK(R11) ;SET QIO BLOCK LENGTH
       00D0 CB       0000'8F     3C  0077 14700 	MOVZWL	#IO$_WRITELBLK,SD_G_QIOBLK+QIO$_FUNC(R11) ;SET I/O FUNCTION
                       02 AB     B4  007E 15800 	CLRW	SD_B_ERR_FLAGS(R11)	;CLEAR BOTH SETS OF FLAGS
                                     0081 16100 
                                     0081 16200 	ASSUME	STATE$_IDLE EQ 0
                                     0081 16300 
                       04 AB     94  0081 16400 	CLRB	SD_B_STATE(R11)		;SET INITIAL STATE TO IDLE
