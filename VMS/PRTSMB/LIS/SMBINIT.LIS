                                     000E  7400 	.SBTTL	SYMBIONT INITIALIZATION ROUTINE
                                     000E  7500 ;++
                                     000E  7600 ; FUNCTIONAL DESCRIPTION:
                                     000E  7700 ;
                                     000E  7800 ;	THIS ROUTINE PERFORMS ALL ONE TIME FUNCTIONS FOR THE
                                     000E  7900 ;	PRINT SYMBIONT.
                                     000E  8000 ;
                                     000E  8100 ; CALLING SEQUENCE:
                                     000E  8200 ;
                                     000E  8300 ;	MAIN ENTRY POINT OF SYMBIONT
                                     000E  8400 ;
                                     000E  8500 ; INPUT PARAMETERS:
                                     000E  8600 ;
                                     000E  8700 ;	NONE
                                     000E  8800 ;
                                     000E  8900 ; IMPLICIT INPUTS:
                                     000E  9000 ;
                                     000E  9100 ;	NONE
                                     000E  9200 ;
                                     000E  9300 ; OUTPUT PARAMETERS:
                                     000E  9400 ;
                                     000E  9500 ;	R11 CONTAINS THE ADDRESS OF THE IMPURE DATA BLOCK
                                     000E  9600 ;	INIT DONE MESSAGE SENT TO SYMBIONT MANAGER
                                     000E  9700 ;
                                     000E  9800 ; IMPLICIT OUTPUTS:
                                     000E  9900 ;
                                     000E 10000 ;	CHANNEL ASSIGNED TO SYMBIONT MANAGER MAILBOX
                                     000E 10100 ;	MAILBOX CREATED FOR RECEIPT OF MANAGER MESSAGES
                                     000E 10200 ;
                                     000E 10300 ; COMPLETION CODES:
                                     000E 10400 ;
                                     000E 10500 ;	NONE
                                     000E 10600 ;
                                     000E 10700 ; SIDE EFFECTS:
                                     000E 10800 ;
                                     000E 10900 ;	THIS ROUTINE DISPATCHES DIRECTLY TO THE SYMBIONT IDLE LOOP
                                     000E 11000 ;
                                     000E 11100 ;--
                                     000E 11200  
                                     000E 11300  
                                     000E 11400  
                                     000E 11500 SMB_START:				;SYMBIONT INITIAL ENTRY
                               0000  000E 11600 	.WORD	0			;ENTRY MASK
                                     0010 11700 
