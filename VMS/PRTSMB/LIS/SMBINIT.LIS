            57       0148 CB     DE  0040 13700 	MOVAL	SD_G_RAB(R11),R7	;GET ADDRESS OF RAB
            58       018C CB     DE  0045 13800 	MOVAL	SD_G_NAM(R11),R8	;GET ADDRESS OF NAM BLK
                                     004A 13801 
