                                     0084 17100 		SD_W_JBCCHAN(R11)	;MAILBOX-CHANNEL NUMBER STORED HERE
            11            50     E8  0094 17200 	BLBS	R0,10$			;BR IF NO ERROR
                                     0097 17210 	SIGNAL	JBC$_MBASGN,#0,R0	;SIGNAL THE ERROR
                          2D     11  00A6 17220 	BRB	20$			;EXIT
                                     00A8 17300 
                                     00A8 17400 ;
                                     00A8 17500 ; CREATE SYMBIONT'S MAILBOX
                                     00A8 17600 ;
                                     00A8 17700 
                                     00A8 17710 10$:
                                     00A8 17800 	$CREMBX_S 	-		;CREATE A MAIL BOX FOR COMMANDS
                                     00A8 17900 		PROMSK = #0,-		; PROTECTION
                                     00A8 18000 		MAXMSG = #SIM$K_SIZE,-	;MAXIMUM MESSAGE SIZE
                                     00A8 18100 		BUFQUO = #2*SIM$K_SIZE,-; 2 MESSAGES MAX
                                     00A8 18200 		CHAN   = SD_W_MBCHAN(R11) ; CHANNEL OF CREATED MAILBOX GOES HERE
            18            50     E8  00C3 18300 	BLBS	R0,30$			;BR IF NO ERROR
                                     00C6 18310 	SIGNAL	JBC$_SYMBCRE,#0,R0	;SIGNAL THE ERROR
