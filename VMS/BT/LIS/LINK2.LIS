                                     0008    63 ;+
                                     0008    64 ; BACKTRANSLATOR LINK/RSX
                                     0008    65 ;
                                     0008    66 ; CONVERTS DCL COMMAND TO TKB INDIRECT COMMAND FILE
                                     0008    67 ;
                                     0008    68 ;
                                     0008    69 ; INPUTS:
                                     0008    70 ;  THE INPUTS ARE RETURNED FROM THE DCL RESULT PARSER AS RESPONSES
                                     0008    71 ;  TO THE VARIOUS REQUEST DESCRIPTORS THAT ARE GIVEN TO IT.
                                     0008    72 ;
                                     0008    73 ;
                                     0008    74 ; OUTPUTS:
                                     0008    75 ;  THE OUTPUT CONSISTS OF THE INDIRECT COMMAND FILE 'ATLNK.CMD;1'.
                                     0008    76 ;  THE FILE CONTAINS THE APPROPRIATE COMMANDS TO ACCOMPLISH THE LINK.
                                     0008    77 ;  THE CALLER OPENS THE IMAGE TKB AND INVOKES IT GIVING @ATLNK
                                     0008    78 ;  IF NO ERROR OCCURS.
                                     0008    79 ;
                                     0008    80 ;-
                                     0008    81 
                                     0008    82 	PURE
                                 00000008       	.PSECT	PURE_	BYTE,RD,EXE,NOWRT
                                     0008    83 
