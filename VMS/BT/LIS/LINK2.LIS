                          FFF5'  30  0008    85 	BSBW	COMMAND_GEN1		;GENERATE COMMAND LINE OF TKB FILE
                                     000B    86 	$CREATE	FAB=W^AFAB		;CREATE THE INDIRECT FILE 'ATLNK.CMD'
                     0000'CF     DF  000B       		PUSHAL	W^AFAB
   00000000'9F            01     FB  000F       		CALLS	#$$.TMP1,@#SYS$CREATE
            44            50     E9  0016    87 	BLBC	R0,40$			;BRANCH IF ERROR
            59       0050'CF     9E  0019    88 	MOVAB	W^ARAB,R9		;GET A POINTER TO THE RAB
                                     001E    89 	$CONNECT RAB=R9			;SET UP THE RAB
                          59     DD  001E       		PUSHL	R9
   00000000'9F            01     FB  0020       		CALLS	#$$.TMP1,@#SYS$CONNECT
            33            50     E9  0027    90 	BLBC	R0,40$			;BRANCH IF ERROR
                                     002A    91 	$RAB_STORE RAB=R9,RSZ=W^RESULT_DESC,RBF=@W^RESULT_DESC+4
         28 A9       0004'DF     DE  002A       				MOVAL @W^RESULT_DESC+4,RAB$L_RBF(R9)
         22 A9       0000'CF     B0  0030       				MOVW W^RESULT_DESC,RAB$W_RSZ(R9)
                                     0036    92 	$PUT	RAB=R9			;PUT THE COMMAND LINE INTO THE FILE
                          59     DD  0036       		PUSHL	R9
   00000000'9F            01     FB  0038       		CALLS	#$$.TMP1,@#SYS$PUT
