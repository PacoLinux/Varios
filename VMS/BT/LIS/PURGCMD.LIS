                                     0070    79 ;	.SBTTL PURGE
                                     0070    80 ;+
                                     0070    81 ; BACKTRANSLATOR PURGE
                                     0070    82 ;
                                     0070    83 ; CONVERTS DCL COMMAND TO COMPATIBILITY MODE IMAGE COMMAND
                                     0070    84 ;
                                     0070    85 ;
                                     0070    86 ; INPUTS:
                                     0070    87 ;  THE INPUTS ARE RETURNED FROM THE DCL RESULT PARSER AS RESPONSES
                                     0070    88 ;  TO THE VARIOUS REQUEST DESCRIPTORS THAT ARE GIVEN TO IT.
                                     0070    89 ;
                                     0070    90 ;
                                     0070    91 ; OUTPUTS:
                                     0070    92 ;  THE OUTPUT CONSISTS OF TWO STRINGS OR AN ERROR MESSAGE AND ABORT.
                                     0070    93 ;  THE CALLER OPENS THE IMAGE AND INVOKES THE COMPATIBILITY EMULATOR
                                     0070    94 ;  IF NO ERROR OCCURS.
                                     0070    95 ;
                                     0070    96 ;  1- FILENAME STRING OF COMPATIBILITY MODE IMAGE TO BE ACTIVATED.
                                     0070    97 ;
                                     0070    98 ;  2- COMMAND LINE TO BE GIVEN TO THE IMAGE.
                                     0070    99 ;
                                     0070   100 ;-
                                     0070   101 
                                     0070   102 	PURE
                                 00000000       	.PSECT	PURE_	BYTE,RD,EXE,NOWRT
                                     0000   103 
                                     0000   104 PURG::
                                     0000   105 	$SETSTRING	</PU>,TYPE=INLINE ;ALWAYS TURN ON /UN OPTION
                          FFFD'  30  0000       	BSBW	SETSTRING
                                 01  0003       	.BYTE	$$$FLAGS
                                 00  0004       	.BYTE	0
                                 03  0005       	.BYTE	$$CNT
                           55 50 2F  0006       	.ASCII	\/PU\
                                 FA  0009   106 	CALLG	W^QUAL_ARGLIST,-	;PROCESS COMMAND QUALIFIER OPTIONS
         08 BC       002C'CF         000A   107 		@CLI$A_UTILSERV(AP)
                                 FA  000F   108 10$:	CALLG	W^INPUT_ARGLIST,-	;PROCESS INPUTS
         08 BC       0000'CF         0010   109 		@CLI$A_UTILSERV(AP)
            59       0018'CF     7D  0015   110 	MOVQ	W^CLI$Q_RQDESC+INPUT_DESC,R9 ;PICK UP STRING DESCRIPTOR
                          FFE3'  30  001A   111 	BSBW	INSERTARG		;SAVE IT AS PRIMARY OUTPUT
                     0000'CF     97  001D   112 	DECB	W^PARAMLOC		;SET UP TO PUT NEXT SPEC IN NEXT SPOT
                                 E0  0021   113 	BBS	#CLI$V_CONCATINP,-	;BRANCH IF MORE INPUTS
       0013'CF            01         0022   114 		W^CLI$B_RQSTAT+INPUT_DESC,10$
                          E8         0026       
                                     0027   115 	DEFPROG	PIP			;RETURN DEFAULT PROGRAM TO RUN
            50            01     D0  0027       	MOVL	#PROG_PIP,R0
                                 05  002A   116 	RSB				;RETURN TO GENERATE COMMAND TEXT
                                     002B   117 
                                     002B   118 PURG_KEEP:
                                     002B   119 	$GETDECNUM	<:>,FLAGS=<BTR$M_VALREQ> ;GET THE NUMBER OF VERSIONS TO KEEP
                               0FFC  002B       	.WORD	SAVE_REG_MASK
                          FFD0'  30  002D       	BSBW	GETSTRING
