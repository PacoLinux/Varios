                                     0090 10700 ;
                                     0090 10800 ; FUNCTIONAL DESCRIPTION:
                                     0090 10900 ;
                                     0090 11000 ;	CALLS SYS$PUTMSG TO GET THE ERROR MESSAGE TEXT, WHICH CALLS
                                     0090 11100 ;	THE ACTION ROUTINE WHICH WILL OUTPUT THE ERROR MESSAGE TO:
                                     0090 11200 ;
                                     0090 11300 ;		1) OPCOM - IF A JOB CARD HAS NOT BEEN READ YET, THE
                                     0090 11400 ;			ERROR MESSAGE GOES TO THE OPERATOR TERMINALS
                                     0090 11500 ;
                                     0090 11600 ;		2) [SYSMGR]INPBATCH.ERR - IF A JOB CARD HAS BEEN READ,
                                     0090 11700 ;			BUT THE OUPUT FILE HASN'T BEEN CREATED YET, THE
                                     0090 11800 ;			ERROR MESSAGE WILL BE WRITTEN TO THIS FILE
                                     0090 11850 ;
                                     0090 11855 ;		3) [user's dir]INPBATCH.COM - IF THE OUTPUT COMMAND
                                     0090 11860 ;			FILE WAS CREATED, THE ERROR MESSAGE WILL BE
                                     0090 11865 ;			WRITTEN TO THIS FILE
                                     0090 11900 ;
                                     0090 12000 ;	IF THE MESSAGE(S) WERE WRITTEN TO A FILE, IT IS ENTERED IN THE
                                     0090 12100 ;	SYS$PRINT QUEUE TO BE PRINTED WITH THE USER'S NAME.  THE CALL
                                     0090 12200 ;	FRAMES ARE THEN UNWOUND AND THE IMAGE RESTARTED AT INP$GET_JOB.
                                     0090 12300 ;
                                     0090 12400 ; CALLING SEQUENCE:
                                     0090 12500 ;
                                     0090 12600 ;	CALLED AS A CONDITION HANDLER
                                     0090 12700 ;
                                     0090 12800 ; INPUT PARAMETERS:
                                     0090 12900 ;
                                     0090 13000 ;	STANDARD CONDITION HANDLER PARAMETERS
                                     0090 13100 ;
                                     0090 13200 ; IMPLICIT INPUTS:
                                     0090 13300 ;
                                     0090 13400 ;	NONE
                                     0090 13500 ;
                                     0090 13600 ; OUTPUT PARAMETERS:
                                     0090 13700 ;
                                     0090 13800 ;	NONE
                                     0090 13900 ;
                                     0090 14000 ; IMPLICIT OUTPUTS:
                                     0090 14100 ;
                                     0090 14200 ;	ERROR MESSAGES SENT EITHER TO OPERATOR OR USER
                                     0090 14300 ;
                                     0090 14400 ; ROUTINE VALUE:
                                     0090 14500 ;
                                     0090 14600 ;	NONE
                                     0090 14700 ;
                                     0090 14800 ; SIDE EFFECTS:
                                     0090 14900 ;
                                     0090 15000 ;	CALL FRAMES UNWOUND AND PROCESS RESTARTED AT INP$GET_JOB
                                     0090 15100 ;
                                     0090 15200 ;--
