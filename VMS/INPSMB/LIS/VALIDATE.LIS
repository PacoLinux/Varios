                                     0021    89 	.SBTTL	VALIDATE_USER - subroutine to lookup username
                                     0021    90 
                                     0021    91 ;++
                                     0021    92 ;
                                     0021    93 ; FUNCTIONAL DESCRIPTION:
                                     0021    94 ;
                                     0021    95 ;	Routine to lookup the username in the SYSUAF file, optionally 
                                     0021    96 ;	validate the password, and return the UAF record.
                                     0021    97 ;
                                     0021    98 ; CALLING SEQUENCE:
                                     0021    99 ;
                                     0021   100 ;	CALLS OR CALLG
                                     0021   101 ;
                                     0021   102 ; INPUTS:
                                     0021   103 ;
                                     0021   104 ;	NAMEDSC - address of the string descriptor for the username string
                                     0021   105 ;	PWDDSC - address of the string descriptor for the password string
                                     0021   106 ;	BUFDSC - address of the buffer descriptor for the user's buffer
                                     0021   107 ;		 to receive the UAF record.
                                     0021   108 ;
                                     0021   109 ; IMPLICIT INPUTS:
                                     0021   110 ;
                                     0021   111 ;	none
                                     0021   112 ;
                                     0021   113 ; OUTPUTS:
                                     0021   114 ;
                                     0021   115 ;	The buffer described by BUFDSC receives the user's record if
                                     0021   116 ;	the username string is found and the password is valid.  If
                                     0021   117 ;	the username string is not found, then the default record is
                                     0021   118 ;	returned.
                                     0021   119 ;
                                     0021   120 ; IMPLICIT OUTPUTS:
                                     0021   121 ;
                                     0021   122 ;	none
                                     0021   123 ;
                                     0021   124 ; ROUTINE VALUE:
                                     0021   125 ;
                                     0021   126 ;	UAF$_NORMAL - username and password ok
                                     0021   127 ;	UAF$_INVPWD - password not valid
                                     0021   128 ;	UAF$_INVUSR - Username not valid
                                     0021   129 ;	RMS error code - error on UAF file
                                     0021   130 ;
                                     0021   131 ; SIDE EFFECTS:
                                     0021   132 ;
                                     0021   133 ;	none
                                     0021   134 ;--
                                     0021   135 
                                     0021   136 
                                     0021   137 ;
                                     0021   138 ; Register Usage:
                                     0021   139 ;	R4 - username string address
                                     0021   140 ;	R5 - user buffer address
                                     0021   141 ;	R6 - FAB address
                                     0021   142 ;	R7 - RAB address
                                     0021   143 ;
                                     0021   144 	.ENABL	LSB
                                     0021   145  
VALIDATE_USER   - routine to validate user and return UAF recor  21-AUG-1978 21:41:39   VAX-11 MACRO X0.3-11               Page   4
