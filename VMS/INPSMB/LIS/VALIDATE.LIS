                                     0021   146 LGI$SEARCHUSER::			; find the user authorization record
                               00FC  0021   147 	.WORD	^M<R2,R3,R4,R5,R6,R7>	; entry save amsk
                          7E     D4  0023   148 	CLRL	-(SP)			; set search only flag
                          04     11  0025   149 	BRB	5$			; enter common code
                                     0027   150  
                                     0027   151 LGI$VALIDATE::				; find record and check password
                               00FC  0027   152 	.WORD	^M<R2,R3,R4,R5,R6,R7>	; save mask
                          01     DD  0029   153 	PUSHL	#1			; set validate flag
            56         B0 AE     9E  002B   154 5$:	MOVAB	-FAB$K_BLN(SP),R6	; allocate a FAB in the stack
            57         BC A6     9E  002F   155 	MOVAB	-RAB$K_BLN(R6),R7	; then a RAB
            5E            57     D0  0033   156 	MOVL	R7,SP			; now create the space
            6E            00     2C  0036   157 	MOVC5	#0,(SP),#0,#FAB$K_BLN+RAB$K_BLN,(SP) ; zero FAB and RAB
       0094 8F            00         0039       
                          6E         003D       
                                     003E   158 	ASSUME	FAB$B_BLN EQ FAB$B_BID+1 ; check for byte adjacency
                                 B0  003E   159 	MOVW	#FAB$K_BLN@8!FAB$C_BID,- ; set block length and block ID-
            66       5003 8F         003F   160 		FAB$B_BID(R6)		; values into proper fields of FAB
         2C A6         BA AF     DE  0043   161 	MOVAL	UAFNAME,FAB$L_FNA(R6)	; insert file name address
         34 A6            06     90  0048   162 	MOVB	#UAFSIZE,FAB$B_FNS(R6)	; insert file name size
         30 A6         B7 AF     DE  004C   163 	MOVAL	DEFNAME,FAB$L_DNA(R6)	; insert default file name address
         35 A6            0F     90  0051   164 	MOVB	#DEFSIZE,FAB$B_DNS(R6)	; insert default name size
                                     0055   165 	SETBIT	FAB$V_GET,FAB$B_FAC(R6)	; get access only
                                     0059   166 	ASSUME	RAB$B_BLN EQ RAB$B_BID+1 ; check for byte adjacency
                                 B0  0059   167 	MOVW	#RAB$K_BLN@8!RAB$C_BID,- ; set block length and block ID-
            67       4401 8F         005A   168 		RAB$B_BID(R7)		; values into proper fields of RAB
                                     005E   169 	ASSUME	RAB$C_SEQ EQ 0		; check that zero RAB is sequential I/O
                                     005E   170 ; ***	MOVB	#RAB$C_SEQ,RAB$B_RAC(R7) ; sequential access
         37 A7            0A     90  005E   171 	MOVB	#10,RAB$B_MBC(R7)	; multi-block reads
            55         0C AC     D0  0062   172 	MOVL	BUFDSC(AP),R5		; get user descriptor address
         20 A7            65     B0  0066   173 	MOVW	(R5),RAB$W_USZ(R7)	; user buffer size
            55         04 A5     D0  006A   174 	MOVL	4(R5),R5		; save buffer address in R5
         24 A7            55     D0  006E   175 	MOVL	R5,RAB$L_UBF(R7)	; put user buffer address in rab
         3C A7            66     DE  0072   176 	MOVAL	(R6),RAB$L_FAB(R7)	; insert FAB address
            54         04 AC     D0  0076   177 	MOVL	NAMEDSC(AP),R4		; get username descriptor addr
            54         04 A4     D0  007A   178 	MOVL	4(R4),R4		; get username string address
                                     007E   179 
                                     007E   180 ;
                                     007E   181 ; Open the File and connect RAB
                                     007E   182 ;
                                     007E   183 
                                     007E   184 	$OPEN	FAB=(R6)
            71            50     E9  0087   185 	BLBC	R0,EXIT			; exit on error
                                     008A   186 	$CONNECT RAB=(R7)		; connect RAB to FAB
            65            50     E9  0093   187 	BLBC	R0,EXIT			; exit on error
                                     0096   188 
                                     0096   189 ;
                                     0096   190 ; Check for DEFAULT user name and deny access if a match occurs.
                                     0096   191 ;
                                     0096   192 
            64         04 BC     2D  0096   193 	CMPC5	@NAMEDSC(AP),(R4),#^A\ \,#12,DEFUSER
            0C            20         009A       
                     FF76 CF         009C       
                          41     13  009F   194 	BEQL	50$
                                     00A1   195 
                                     00A1   196 20$:	$GET	RAB=(R7)		; get next record
            2C            50     E9  00AA   197 	BLBC	R0,40$			; exit loop on error
                                     00AD   198 
VALIDATE_USER   - routine to validate user and return UAF recor  21-AUG-1978 21:41:39   VAX-11 MACRO X0.3-11               Page   5
