                                     00AD   199 ;
                                     00AD   200 ; Check the supplied username 
                                     00AD   201 ;
                                     00AD   202 
            64         04 BC     2D  00AD   203 	CMPC5	@NAMEDSC(AP),(R4),#32,#12,UAF$T_USERNAME(R5)
            0C            20         00B1       
                          65         00B3       
                          EB     12  00B4   204 	BNEQ	20$			; look again if no match
                       FC AD     D5  00B6   205 	TSTL	-4(FP)			; is this a validate of search?
                          10     13  00B9   206 	BEQL	25$			; br if search only
                       08 AC     DD  00BB   207 	PUSHL	PWDDSC(AP)		; push argument
   00000000'EF            01     FB  00BE   208 	CALLS	#1,LGI$HPWD		; mash password
         0C A5            50     D1  00C5   209 	CMPL	R0,UAF$L_PWD(R5)	; password's match?
                          05     12  00C9   210 	BNEQ	30$			; branch if bad password
            50            01     D0  00CB   211 25$:	MOVL	#UAF$_NORMAL,R0		; good return
                          2B     11  00CE   212 	BRB	EXIT
            50   FFFFFFFC 8F     D0  00D0   213 30$:	MOVL	#UAF$_INVPWD,R0		; bad password specified
                          22     11  00D7   214 	BRB	EXIT
                                     00D9   215 
                                     00D9   216 40$:
                                     00D9   217 
                                     00D9   218 ;
                                     00D9   219 ; Error on get.  If its EOF then user not found.  Load default
                                     00D9   220 ; record into user buffer.
                                     00D9   221 ;
                                     00D9   222 
   00000000'8F            50     D1  00D9   223 	CMPL	R0,#RMS$_EOF		; was this EOF?
                          19     12  00E0   224 	BNEQ	EXIT			; exit with error if not
                                     00E2   225 50$:	$REWIND	RAB=(R7)		; rewind to default record
                                     00EB   226 	$GET	RAB=(R7)
            50   FFFFFFFE 8F     D0  00F4   227 	MOVL	#UAF$_INVUSR,R0		; load status code
                                     00FB   228 EXIT:
                          50     DD  00FB   229 	PUSHL	R0			; save status
                                     00FD   230 	$DISCONNECT	RAB=(R7)	; clean up shop
                                     0106   231 	$CLOSE		FAB=(R6)	;
                          50   8ED0  010F   232 	POPL	R0
                                 04  0112   233 	RET				; return
                                     0113   234 
                                     0113   235 	.END
