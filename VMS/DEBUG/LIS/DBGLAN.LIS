;	3791	GLOBAL ROUTINE dbg$set_lang (lang_str_desc, language_module) : NOVALUE =
;	3792	
;	3793	!++
;	3794	! Functional description:
;	3795	!	This routine loads the pointers to the current parsing tables
;	3796	!	with those of the new language.  The reduction routine and lexeme
;	3797	!	collection routine addresses are also changed.
;	3798	!
;	3799	! Formal parameters:
;	3800	!	lang_str_desc	- The address of a string descriptor that holds
;	3801	!			  the new language name, or 0.
;	3802	!	language_module	- If "lang_str_desc" is zero this parameter holds the
;	3803	!			  language and module index number as defined in DBGGEN.REQ.
;	3804	!			  MACRO - 0
;	3805	!			  FORTRAN - 1
;	3806	!			  BLISS - 2
;	3807	!
;	3808	! Implicit inputs:
;	3809	!	none
;	3810	!
;	3811	! Outputs:
;	3812	!	none
;	3813	!
;	3814	! Implicit outputs:
;	3815	!	The pointers to the parsing tables and to the action routines
;	3816	!	are changed.
;	3817	!
;	3818	! Routine value:
;	3819	!	novalue
;	3820	!
;	3821	! Side effects:
;	3822	!	Signal if language name is not recognized or not yet supported.
;	3823	!--
;	3824	
;	3825		BEGIN
;	3826	
;	3827		LITERAL
;	3828			max_lang = 1;
;	3829	
;	3830		LOCAL
;	3831			count;
;	3832	
;	3833		IF (.lang_str_desc NEQ 0)		! Was routine called with descriptor?
;	3834		THEN
;	3835			BEGIN
;	3836			count = dbg$tran_lexeme ( .lang_str_desc, language_table);
;	3837			IF (.count EQL alpha_str_token)
;	3838			THEN	signal (dbg$_nosuchlan, 1, .lang_str_desc);
;	3839			IF (.count GTR max_lang)
;	3840			THEN
;	3841			    BEGIN
;	3842				signal (dbg$_notimplan, 1, dbg$language(.count));
;	3843				RETURN;
;	3844			    END;
;	3845			END

