MACRO		RND$T_NAME	= 2,0,0,0%; 		!NAME OF REMOTE NODE 
LITERAL	RND$S_NAME	= 6; 
MACRO		RND$W_NUMBER	= 8,0,16,0%; 		!NUMBER OF REMOTE NODE 
MACRO		RND$B_SECLDR	= 10,0,8,0%; 		!COUNT OF NEXT FIELD 
MACRO		RND$T_SECLDR	= 11,0,0,0%; 	!NAME OF SECONDARY LOADER FILE 
LITERAL	RND$S_SECLDR	= 13; 
MACRO		RND$B_TERLDR	= 24,0,8,0%; 		!COUNT OF NEXT FIELD 
