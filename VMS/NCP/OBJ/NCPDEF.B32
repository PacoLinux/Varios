! 
!****ALL "T" FIELDS AND THEIR PRECEDING COUNTS MUST BE KEPT 
!***CONTINUOUS 
!****IN ADDITION THE FIRST BYTE OF EACH RECORD MUST BE THE USE 
!****BYTE AND THE IDENT BYTE MUST BE THE LAST 
! 
!...$LNDDEF		!LOCAL NODE DATABLOCK 
LITERAL	LND$C_FILE	= 0; 			!AN INDEX TO THIS TYPE OF RECORD 
MACRO		LND$B_USE	= 0,0,8,0%; 		!RECORD IN USE BYTE 
MACRO		LND$B_NAME	= 1,0,8,0%; 		!COUNT FOR NEXT FIELD 
MACRO		LND$T_NAME	= 2,0,0,0%; 		!NAME OF LOCAL NODE 
LITERAL	LND$S_NAME	= 6; 
MACRO		LND$W_NUMBER	= 8,0,16,0%; 		!NODE NUMBER OF LOCAL NODE 
MACRO		LND$B_MAXLINKS	= 10,0,8,0%; 	!MAX NUMBER OF LOGICAL LINKS ALLOWED 
MACRO		LND$B_MAXNODES	= 11,0,8,0%; 	!MAX NODES ACTIVE TO LOCAL NODE 
MACRO		LND$B_IDENT	= 12,0,8,0%; 		!HOLDS RECORD TYPE  FOR THIS RECORD 
LITERAL	LND$C_SIZE	= 13; 
LITERAL	LND$K_SIZE	= 13; 	 

!...$RNDDEF		!REMOTE NODE DATABLOCK 
LITERAL	RND$C_FILE	= 1; 			!INDEX FOR THIS KIND OF FILE 
MACRO		RND$B_USE	= 0,0,8,0%; 		!RECORD ACTIVE FIELD 
