                                     0012    97 ;
                                     0012    98 ;ROUTINE WHICH ACTUALLY STARTS ACP
                                     0012    99 ;
                                     0012   100 GO:
                               004C  0012   101 	.WORD	^M<R2,R3,R6>
                                     0014   102 ;
                                     0014   103 ;CHECK TO SEE IF ACP IS MARKED FOR DISMOUNT
                                     0014   104 ;
            53   00000000'EF     DE  0014   105 	MOVAL	NET$UCB0,R3		;GET UCB ADDRESS
         34 A3   00000000'8F     E1  001B   106 	BBC	#DEV$V_DMT,UCB$L_DEVCHAR(R3),40$	;DEV MARKED FOR DISMOUNT
                          08         0023       
            50   0000006C 8F     D0  0024   107 	MOVL	#SS$_DEVMOUNT,R0	;NOTE MARK FOR DISMOUNT
                                 04  002B   108 	RET
                                     002C   109 40$:
                                     002C   110 ;
                                     002C   111 ;BUILD THE ACP QUEUE BLOCK(AQB)
                                     002C   112 ;
            51            18     9A  002C   113 	MOVZBL	#AQB$C_LENGTH,R1	;LENGTH OF AQB BLOCK
                 00000000'EF     16  002F   114 	JSB	EXE$ALONONPAGED		;GET A CHUNK OF STORAGE
            03            50     E8  0035   115 	BLBS	R0,50$			;BR ON SUCCESSFUL ALLOCATION
                          0098   31  0038   116 	BRW	77$			;ERROR ON ALLOCATION
                                     003B   117 50$:
                                     003B   118 
                                     003B   119 ;
                                     003B   120 ;FILL IN THE AQB
                                     003B   121 ;
         08 A2            18     9B  003B   122 	MOVZBW	#AQB$C_LENGTH,AQB$W_SIZE(R2)	;RECORD SIZE OF AQB
         0A A2            03     90  003F   123 	MOVB	#DYN$C_AQB,AQB$B_TYPE(R2);NOTE TYPE OF BLOCK
            62            52     D0  0043   124 	MOVL	R2,AQB$L_ACPQFL(R2)	;SET QUEUE FORWARD LINK
         04 A2            52     D0  0046   125 	MOVL	R2,AQB$L_ACPQBL(R2)	;SET QUEUE BACK LINK
         0B A2            01     90  004A   126 	MOVB	#1,AQB$B_MNTCNT(R2)		;INITIALIZE MOUNT COUNT
         14 A2            09     88  004E   127 	BISB	#AQB$M_UNIQUE!AQB$M_CREATING,AQB$B_STATUS(R2);NOTE STATUS OF ACP
                          52     DD  0052   128 	PUSHL	R2			;SAVE AQB ADDRESS
                                     0054   129 ;
                                     0054   130 ;BUILD VCB
                                     0054   131 ;
            51         54 8F     9A  0054   132 	MOVZBL	#VCB$C_LENGTH,R1	;RECORD LENGTH OF BLOCK
                 00000000'EF     16  0058   133 	JSB	EXE$ALONONPAGED		;ALLOCATE THE STORAGE
            69            50     E9  005E   134 	BLBC	R0,76$			;DEALLOCATE AQB ON ALLOCATION FAILURE
                                     0061   135 ;
                                     0061   136 75$:
         0A A2            11     90  0061   137 	MOVB	#DYN$C_VCB,VCB$B_TYPE(R2);SET STRUCTURE TYPE
         08 A2         54 8F     9B  0065   138 	MOVZBW	#VCB$C_LENGTH,VCB$W_SIZE(R2)	;SET SIZE OF VCB
         0C A2            01     B0  006A   139 	MOVW	#1,VCB$W_TRANS(R2)	;INITIALIZE TRANSACTION COUNT
         10 A2            6E     D0  006E   140 	MOVL	(SP),VCB$L_AQB(R2)	;ADD LINK TO AQB
            53   00000000'EF     DE  0072   141 	MOVAL	NET$UCB0,R3		;ADDRESS OF NETWORK UCB
         30 A3            52     D0  0079   142 	MOVL	R2,UCB$L_VCB(R3)	;LINK TO VCB
         58 A3       0200 8F     A8  007D   143 	BISW	#UCB$M_MOUNTING,UCB$W_STS(R3)	;NOTE DEVICE IS MOUNTING
                                     0083   144 ;
                                     0083   145 ;STARTUP THE ACP
