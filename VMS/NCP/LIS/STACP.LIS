                                     0083   153 			STSFLG=#0,-	;DISABLE RESOURCE WAIT
                                     0083   154 			UIC=UIC
            19            50     E8  00BB   155 	BLBS	R0,80$			;BR IF SUCCESSFUL
            56            50     D0  00BE   156 	MOVL	R0,R6			;SAVE ERROR STATUS
                                     00C1   157 ;
                                     00C1   158 ;DEALLOCATE THE AQB AND VCB
                                     00C1   159 ;
            50            52     D0  00C1   160 	MOVL	R2,R0			;ADDRESS VCB
                 00000000'EF     16  00C4   161 	JSB	EXE$DEANONPAGED		;RETURN BLOCK
                                     00CA   162 76$:
                          50   8ED0  00CA   163 	POPL	R0			;ADDRESS AQB
                 00000000'EF     16  00CD   164 	JSB	EXE$DEANONPAGED		;RETURN AQB
                                     00D3   165 77$:
            50            56     D0  00D3   166 	MOVL	R6,R0			;RESTORE ERROR STATUS
                                 04  00D6   167 	RET
                                     00D7   168 ;
                                     00D7   169 ;PLACE PID INTO AQB
                                     00D7   170 80$:
                                     00D7   171 
                          52   8ED0  00D7   172 	POPL	R2			;RESTORE ADDRESS OF AQB
         0C A2       0000'CF     D0  00DA   173 	MOVL W^PIDADR,AQB$L_ACPPID(R2)
                                     00E0   174 ;
                                     00E0   175 ;LINK THE AQB INTO THE AQB LIST
                                     00E0   176 ;
                                     00E0   177 
       0000'CF            00     FB  00E0   178 	CALLS	#0,W^LOCK_IODB	;LOCK THE IO DATABASE
            51   00000000'EF     9E  00E5   179 	MOVAB	L^IOC$GL_AQBLIST,R1		;GET ADDRESS OF LIST HEAD
         10 A2            61     D0  00EC   180 	MOVL	(R1),AQB$L_LINK(R2)	;MAKE FORWARD LINK FROM AQB
            61            52     D0  00F0   181 	MOVL	R2,(R1)			;MAKE THIS AQB HEAD OF LIST
       0000'CF            00     FB  00F3   182 	CALLS	#0,W^UNLOCK_IODB		;UNLOCK DATABASE
            50            01     D0  00F8   183 	MOVL	#1,R0			;RETURN SUCCESS
                                 04  00FB   184 100$:	RET
                                     00FC   185 	.END	
