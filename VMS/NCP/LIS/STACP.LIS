                                     0083   148 			IMAGE=IMAGE_NAME,-
                                     0083   149 			INPUT=INPUT_DESC,-
                                     0083   150 			QUOTA=QLIST,-
                                     0083   151 			BASPRI=#8,-
                                     0083   152 			PRCNAM=PROCESS_NAME,-
