POSTEF          POST EVENT FLAGS ;                               21-AUG-1978 20:05:55   VAX-11 MACRO X0.3-11               Page   3
X01             SCH$POSTEF - POST EVENT FLAG                                                                                     (1)

                                     0000  7400 	.SBTTL	SCH$POSTEF - POST EVENT FLAG 
                                     0000  7500 
                                     0000  7600 ;++
                                     0000  7700 ; FUNCTIONAL DESCRIPTION:
                                     0000  7800 ;	POST EVENT FLAG WILL SET AN EVENT FLAG SPECIFIED BY NUMBER
                                     0000  7900 ;	FOR A PROCESS SPECIFIED BY PROCESS ID(PID) AND CAUSE ANY
                                     0000  8000 ;	NECESSARY RESCHEDULING IF THIS EVENT SATISFIES A WAIT 
                                     0000  8100 ;	CONDITION FOR THE PROCESS.
                                     0000  8200 ;
                                     0000  8300 ; CALLING SEQUENCE:
                                     0000  8400 ;
                                     0000  8500 ;	JSB	SCH$POSTEF		(ALSO BSB)
                                     0000  8600 ;
                                     0000  8700 ; INPUT PARAMETERS:
                                     0000  8800 ;	R1 - PROCESS IDENTIFICATION (PID)
                                     0000  8900 ;	R2 - PRIORITY INCREMENT CLASS NUMBER
                                     0000  9000 ;	R3 - EVENT FLAG NUMBER
                                     0000  9100 ;
                                     0000  9200 ; IMPLICIT INPUTS:
                                     0000  9300 ;	NONE
                                     0000  9400 ;
                                     0000  9500 ; OUTPUT PARAMETERS:
                                     0000  9600 ;	R0 - COMPLETION STATUS CODE
                                     0000  9700 ;	R4 - PCB ADDRESS OF PROCESS SPECIFIED BY PID
                                     0000  9800 ;
                                     0000  9900 ; IMPLICIT OUTPUTS:
                                     0000 10000 ;	EVENT FLAG BIT SELECTED BY PID AND EVENT FLAG NUMBER
                                     0000 10100 ;	IS SET.
                                     0000 10200 ;
                                     0000 10300 ; COMPLETION CODES:
                                     0000 10400 ;	SS$_NORMAL  -  NORMAL SUCCESSFUL COMPLETION
                                     0000 10500 ;	SS$_NONEXPR -  NON-EXISTENT PROCESS
                                     0000 10600 ;
                                     0000 10700 ; SIDE EFFECTS:
                                     0000 10800 ;	CAN CAUSE RESCHEDULING OF ONE OR MORE PROCESSES.
                                     0000 10900 ;--
                                     0000 11000 
                                     0000 11100 SCH$POSTEF::					; POST EVENT FLAG ENTRY
            50            51     3C  0000 11200 	MOVZWL	R1,R0				; GET INDEX TO PCB ADDRESS
                                     0003 11300 	DSBINT	#IPL$_SYNCH			; BLOCK SYSTEM EVENTS
            54       0000'DF40   D0  0009 11400 	MOVL	@W^SCH$GL_PCBVEC[R0],R4		; AND GET PCB ADDRESS
         60 A4            51     D1  000F 11500 	CMPL	R1,PCB$L_PID(R4)		; CHECK FOR PID MATCH
                          2A     12  0013 11600 	BNEQ	NONEXPR				; PROCESS IS GONE
            02            05     EF  0015 11700 	EXTZV	#5,#2,R3,R1			; GET CLUSTER NUMBER
            51            53         0018       
            53   FFFFFFE0 8F     CA  001A 11800 	BICL	#^C<^X1F>,R3			; EXTRACT EVENT NUMBER
            50         50 A441   DE  0021 11900 	MOVAL	PCB$L_EFCS(R4)[R1],R0		; GET CLUSTER OR PTR ADDR
            1D            51     F5  0026 12000 	SOBGTR	R1,COMMON			; CHECK FOR COMMON
                                     0029 12100 LOCAL:						; POST LOCAL EF
            60            53     E2  0029 12200 	BBSS	R3,(R0),10$			; SET EVENT FLAG
                          00         002C       
            51         32 A4     9A  002D 12400 10$:	MOVZBL	PCB$B_WEFC(R4),R1		; GET NUMBER OF WAIT CLUSTER
            51         50 A441   DE  0031 12425 	MOVAL	PCB$L_EFCS(R4)[R1],R1	; AND COMPUTE ITS ADDRESS
                          46     10  0036 12450 	BSBB	CHKWAIT				; IS WAIT SATISFIED?
            50            01     3C  0038 12500 EXITN:	MOVZWL	#SS$_NORMAL,R0			; SET NORMAL STATUS
                                     003B 12600 EXITEN:	ENBINT					; 
                                     003E 12700 EXIT:						;
POSTEF          POST EVENT FLAGS ;                               21-AUG-1978 20:05:55   VAX-11 MACRO X0.3-11               Page   4
X01             SCH$POSTEF - POST EVENT FLAG                                                                                     (1)

                                 05  003E 12800 	RSB					; RETURN
                                     003F 12900 
                                     003F 13000 NONEXPR:					; NONEXISTENT PROCESS
            50       08E8 8F     3C  003F 13100 	MOVZWL	#SS$_NONEXPR,R0			; SET NONEXISTENT CODE
                          F5     11  0044 13200 	BRB	EXITEN				; AND EXIT ENABLING
                                     0046 13300 
                                     0046 13400 COMMON:						; COMMON EVENT POST
                          54     DD  0046 13500 	PUSHL	R4				; SAVE R4
            7E            55     7D  0048 13550 	MOVQ	R5,-(SP)			; SAVE R5,R6
            60            14     C1  004B 13600 	ADDL3	#CEB$L_WQFL,(R0),R5		; POINT TO HEAD OF WAIT QUEUE
                          55         004E       
                          25     18  004F 13700 	BGEQ	20$				; EXIT IF NOT ASSIGNED
            54            65     D0  0051 13800 	MOVL	(R5),R4				; GET HEAD OF WAIT QUEUE
         FC A5            53     E2  0054 13900 	BBSS	R3,<CEB$L_EFC-CEB$L_WQFL>(R5),10$	; SET EVENT FLAG
                          00         0058       
            55            54     D1  0059 14000 10$:	CMPL	R4,R5				; CHECK FOR END OF WAIT QUEUE
                          18     13  005C 14100 	BEQL	20$				; DONE
            56            64     D0  005E 14200 	MOVL	(R4),R6				; SAVE REAL FLINK
            50         FC A5     DE  0061 14300 	MOVAL	<CEB$L_EFC-CEB$L_WQFL>(R5),R0	; POINT TO EVENT FLAG MASK
            51         32 A4     9A  0065 14305 	MOVZBL	PCB$B_WEFC(R4),R1		; GET WAIT CLUSTER NUMBER
            10         50 A441   C1  0069 14310 	ADDL3	PCB$L_EFCS(R4)[R1],#CEB$L_EFC,R1; AND COMPUTE CLUSTER ADDRESS
                          51         006E       
                          0D     10  006F 14400 	BSBB	CHKWAIT				; CHECK FOR WAIT SATISFIED
            54            56     D0  0071 14500 	MOVL	R6,R4				; NEXT IN WAIT QUEUE
                          E3     11  0074 14600 	BRB	10$				; CONTINUE
            55            8E     7D  0076 14700 20$:	MOVQ	(SP)+,R5			; RESTORE R5,R6
            54            8E     D0  0079 14750 	MOVL	(SP)+,R4			; AND R4
                          BA     11  007C 14800 	BRB	EXITN				; AND EXIT
POSTEF          POST EVENT FLAGS ;                               21-AUG-1978 20:05:55   VAX-11 MACRO X0.3-11               Page   5
X01             CHKWAIT - CHECK FOR WAIT CONDITION SATISFIED                                                                     (1)

                                     007E 15000 	.SBTTL	CHKWAIT - CHECK FOR WAIT CONDITION SATISFIED
                                     007E 15100 
                                     007E 15200 ;++
                                     007E 15300 ; FUNCTIONAL DESCRIPTION:
                                     007E 15400 ;	CHKWAIT CHECKS THE WAIT CONDITION SPECIFIED IN THE PCB
                                     007E 15500 ;	AND RESTARTS THE PREOCESS IF IT IS SATISFIED.
                                     007E 15600 ;
                                     007E 15700 ; CALLING SEQUENCE:
                                     007E 15800 ;	JSB/BSB	CHKWAIT
                                     007E 15900 ;
                                     007E 16000 ; INPUT PARAMETERS:
                                     007E 16100 ;	R0 - EVENT FLAG CLUSTER ADDRESS
                                     007E 16200 ;	R2 - PRIORITY INCREMENT CLASS
                                     007E 16300 ;	R4 - PCB ADDRESS
                                     007E 16400 ;
                                     007E 16500 ; SIDE EFFECTS:
                                     007E 16600 ;	IF THE WAIT CONDITION IS SATISFIED, THE PROCESS WILL RESUME
                                     007E 16700 ;	EXECUTION.
                                     007E 16800 ;
                                     007E 16900 ;--
                                     007E 17000 
                                     007E 17100 CHKWAIT:					; CHECK FOR WAIT SATISFIED
            51            50     D1  007E 17105 	CMPL	R0,R1				; IS PROCESS WAITING FOR THIS
                          10     12  0081 17110 	BNEQ	20$				; CLUSTER? BR IF NOT
            60         4C A4     CB  0083 17200 	BICL3	PCB$L_EFWM(R4),(R0),R3		; TEST WITH MASK
                          53         0087       
                          09     13  0088 17300 	BEQL	20$				; WAIT NOT SATISFIED
         28 A4            0D     E0  008A 17400 	BBS	#PCB$V_WALL,PCB$L_STS(R4),30$	; CHK FOR WAIT ANY
                          05         008E       
                                     008F 17500 ;
                                     008F 17600 ;	AT THIS POINT:
                                     008F 17700 ;			R2 - PRIORITY INCREMENT CLASS NUMBER
                                     008F 17800 ;			R4 - PCB ADDRESS OF PROCESS TO ACTIVATE
                                     008F 17900 ;
                                     008F 18000 10$:	RPTEVT	EVENT				; REPORT EVENT SET
                                 05  0093 18100 20$:	RSB					; RETURN
            53            53     D2  0094 18200 30$:	MCOML	R3,R3				; INVERT MASKED EVENTS
            53         4C A4     D1  0097 18300 	CMPL	PCB$L_EFWM(R4),R3		; CHECK FOR ALL
                          F2     13  009B 18400 	BEQL	10$				; WAIT NOW SATISFIED
                                 05  009D 18500 	RSB					; RETURN
                                     009E 18600 
                                     009E 18700 	.END
