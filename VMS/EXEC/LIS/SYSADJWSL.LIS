X0117           EXTRADYNWS - CALCULATE EXTRA DYNAMIC WORKING SE                                                                  (5)

                                     0074   100 	.SBTTL	EXTRADYNWS - CALCULATE EXTRA DYNAMIC WORKING SET COUNT
                                     0074   200 
                                     0074   300 ;
                                     0074   400 ; FUNCTIONAL DESCRIPTION:
                                     0074   500 ;
                                     0074   600 ;	THIS ROUTINE CALCULATES THE EXTRA NUMBER OF DYNAMIC WORKING SET
                                     0074   700 ; ENTRIES AVAILABLE BEYOND THE NUMBER GUARANTEED BY WSFLUID
                                     0074   800 ;
                                     0074   900 ; CALLING SEQUENCE:
                                     0074  1000 ;
                                     0074  1100 ;	BSBW	MMG$EXTRADYNWS
                                     0074  1200 ;
                                     0074  1300 ; INPUTS:
                                     0074  1400 ;
                                     0074  1500 ;	R5 = PROCESS HEADER ADDRESS
                                     0074  1600 ;	IPL = AT LEAST ASTDEL
                                     0074  1700 ;
                                     0074  1800 ; OUTPUTS:
                                     0074  1900 ;
                                     0074  2000 ;	R1 = EXTRA DYNAMIC WORKING SET COUNT
                                     0074  2050 ;	R0,R2,R3 PRESERVED
                                     0074  2100 ;	PHD$W_EXTDYNWS = EXTRA DYNAMIC WORKING SET COUNT
                                     0074  2200 ;
                                     0074  2210 ;
                                     0074  2220 ; **********************************************************************
                                     0074  2230 ;
                                     0074  2240 ; **************** THE FOLLOWING CODE MUST BE RESIDENT *****************
                                     0074  2250 ;
                                 00000074  2260 	.PSECT	$MMGCOD
                                     0074  2270 ;
                                     0074  2280 ; **********************************************************************
                                     0074  2290 ;
                                     0074  2300 
                                     0074  2400 MMG$EXTRADYNWS::
         72 A5         6C A5     A3  0074  2500 	SUBW3	PHD$W_PTCNTLCK(R5),PHD$W_PTCNTMAX(R5),R1 ;COUNT OF PAGE TABLES
                          51         0079       
                                     007A  2600 					;WHICH ARE NOT LOCKED DOWN
         74 A5            51     B1  007A  2700 	CMPW	R1,PHD$W_WSFLUID(R5)	;MINIMIZE WITH FLUID COUNT
                          04     1B  007E  2800 	BLEQU	10$			;BRANCH IF SMALLER
            51         74 A5     3C  0080  2900 	MOVZWL	PHD$W_WSFLUID(R5),R1	;USE FLUID, IT IS SMALLER
            51         74 A5     A0  0084  3000 10$:	ADDW	PHD$W_WSFLUID(R5),R1	;ADD IN FLUID FOR DATA & I STREAM PAGES
            7E            51     3C  0088  3100 	MOVZWL	R1,-(SP)		;SAVE IT
         12 A5         0E A5     A3  008B  3200 	SUBW3	PHD$W_WSDYN(R5),PHD$W_WSLAST(R5),R1 ;COUNT-1 OF DYNAMIC WORKING SET LIST
                          51         0090       
                          51     B6  0091  3300 	INCW	R1			;COUNT OF DYNAMIC WORKING SET LIST
            51         6C A5     A2  0093  3400 	SUBW	PHD$W_PTCNTLCK(R5),R1	;TAKE AWAY THE PAGE TABLES
                                     0097  3500 					;WHICH HAVE LOCKED PAGES
            51            51     3C  0097  3600 	MOVZWL	R1,R1
            51            8E     C2  009A  3700 	SUBL	(SP)+,R1		;DIFFERENCE IN EXTRA DYN WS COUNT
         76 A5            51     B0  009D  3800 	MOVW	R1,PHD$W_EXTDYNWS(R5)	;SAVE IT IN PROCESS HEADER
                                 05  00A1  3900 	RSB
                                     00A2 20500 
                                     00A2 20600 
                                     00A2 20700 	.END
