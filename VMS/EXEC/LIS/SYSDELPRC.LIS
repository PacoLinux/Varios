                                     003D  7500 ; FUNCTIONAL DESCRIPTION: DELETE EXECUTES AS THE RESULT OF A KERNEL
                                     003D  7550 ;	AST INITIATED BY THE DELETE PROCESS SYSTEM SERVICE.  IT PERFORMS
                                     003D  7600 ;	THE ACTIONS NECESSARY TO DELETE A PROCESS AND RETURN ITS RESOURCES.
                                     003D  7650 ;	TERMINATION MESSAGES ARE SENT TO THE TERMINATION MAILBOX UNIT
                                     003D  7700 ;	SPECIFIED WHEN THE PROCESS WAS CREATED AND TO THE SYMBIONT
                                     003D  7750 ;	MANAGER IF ACCOUNTING IS NOT DISABLED FOR THIS PROCESS.
                                     003D  7800 ;
                                     003D  7850 ; CALLING SEQUENCE:
                                     003D  7900 ;	SETIPL	#IPL$_ASTDEL
                                     003D  7950 ;	JSB	DELETE
                                     003D  8000 ;
                                     003D  8050 ; INPUT PARAMETERS:
                                     003D  8100 ;	R4 - PCB ADDRESS
                                     003D  8150 ;	R5 - AST CONTROL BLOCK ADDRESS
                                     003D  8200 ;
                                     003D  8250 ; OUTPUT PARAMETERS:
                                     003D  8300 ;	NONE
                                     003D  8350 ;
                                     003D  8400 ; IMPLICIT INPUTS:
                                     003D  8450 ;	PCB OF CURRENT PROCESS (LOCATED VIA SCH$GL_CURPCB)
                                     003D  8500 ;	PHD OF CURRENT PROCESS
                                     003D  8550 ;
                                     003D  8600 ; IMPLICIT OUTPUTS:
                                     003D  8650 ;	NONE
                                     003D  8700 ;
                                     003D  8750 ; SIDE EFFECTS:
                                     003D  8800 ;	NONE
                                     003D  8850 ;--
                                     003D  8900 
                                 02  003D  8950 DOREI:	REI				; FOR OUTWARD MODE CHANGE
                                     003E  9000 
                                     003E  9050 DELETE:					; PERFORM DELETE OPERATIONS
            50            55     D0  003E  9100 	MOVL	R5,R0			; SET ADDRESS FOR ACB RELEASE
                          FFBC'  30  0041  9150 	BSBW	EXE$DEANONPAGED		; DEALLOCATE AST CONTROL BLOCK
                                     0044  9200 
                          FFB9'  30  0044  9250 	BSBW	SCH$NEWLVL		; COMPUTE NEW AST LEVEL
                                     0047  9300 	SETIPL	#0			; AND DROP IPL
         28 A4            0A     E5  004A  9350 	BBCC	#PCB$V_SSRWAIT,PCB$L_STS(R4),10$	; ENABLE RESOURCE WAIT
                          00         004E       
            5E         80 AE     9E  004F  9400 10$:	MOVAB	-128(SP),SP		; ALLOCATE STRING BUFFER
            6E         80 8F     9A  0053  9450 20$:	MOVZBL	#128,(SP)		; SET COUNT FOR STRING BUFFER
         04 AE         08 AE     DE  0057  9500 	MOVAL	8(SP),4(SP)		; FORM ADDRESS PART OF DESCRIPTOR
                          02     DD  005C  9550 	PUSHL	#2			; LAST CHANCE RMS CLEANUP
