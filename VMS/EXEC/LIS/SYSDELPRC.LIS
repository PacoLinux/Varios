03              EXE$DELPRC - DELETE PROCESS SYSTEM SERVICE                                                                       (1)

                                     0000  4050 	.SBTTL	EXE$DELPRC - DELETE PROCESS SYSTEM SERVICE
                                     0000  4100 ;++
                                     0000  4150 ; FUNCTIONAL DESCRIPTION:
                                     0000  4200 ;	EXE$DELPRC IMPLEMENTS THE DELETE PROCESS SYSTEM SERVICE WHICH CAUSES
                                     0000  4250 ;	A PROCESS TO BE DELETED FROM THE SYSTEM.  IF THE SPECIFIED
                                     0000  4300 ;	PROCESS EXISTS, A SPECIAL KERNEL AST IS QUEUED FOR IT TO
                                     0000  4350 ;	PERFORM THE DELETION PROCESSING IN THE CONTEXT OF THE TARGET
                                     0000  4400 ;	PROCESS.  A PROCESS THEREFORE DELETES ITSELF.
                                     0000  4450 ;
                                     0000  4500 ; CALLING SEQUENCE:
                                     0000  4550 ;	CALLG	ARGLIST,EXE$DELPRC
                                     0000  4600 ;
                                     0000  4650 ; INPUT PARAMETERS:
                                     0000  4700 ;	PIDADR(AP)  - ADDRESS OF THE PROCESS IDENTIFICATION OF THE
                                     0000  4750 ;		      PROCESS TO BE DELETED.
                                     0000  4800 ;	PRCNAM(AP) - ADDRESS OF STRING DESCRIPTOR FOR PROCESS LOGICAL NAME 
                                     0000  4850 ;		      STRING.
                                     0000  4900 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0000  4950 ;
                                     0000  5000 ; IMPLICIT INPUTS:
                                     0000  5050 ;	CURRENT PROCESS PCB AND PHD
                                     0000  5100 ;	PCB OF TARGET PROCESS
                                     0000  5150 ;
                                     0000  5200 ; OUTPUT PARAMETERS:
                                     0000  5250 ;	R0 - COMPLETION STATUS CODE
                                     0000  5300 ;	@PIDADR(AP) - PROCESS IDENTIFICATION OF DELETED PROCESS
                                     0000  5350 ;
                                     0000  5400 ; IMPLICIT OUTPUTS:
                                     0000  5450 ;	NONE
                                     0000  5500 ;
                                     0000  5550 ; COMPLETION CODES:
                                     0000  5600 ;	SS$_NORMAL   -   SUCCESSFUL COMPLETION 
                                     0000  5650 ;	SS$_NONEXPR  -   NONEXISTENT PROCESS
                                     0000  5700 ;	SS$_NOPRIV   -   NO PRIVILEGE, TARGET PROCESS IS NOT A SUBPROCESS
                                     0000  5750 ;			 AND ISSUING PROCESS DOES NOT HAVE PROCESS CONTROL
                                     0000  5800 ;			 PRIVILEGE FOR GROUP OR WORLD.
                                     0000  5850 ;	SS$_INSFMEM  -	 INSUFFICIENT DYNAMIC MEMORY AVAILABLE FOR SERVICE
                                     0000  5900 ;			 AND RESOURCE WAIT MODE DISABLED.
                                     0000  5950 ;
                                     0000  6000 ; SIDE EFFECTS:
                                     0000  6050 ;	A SPECIAL KERNEL AST WILL BE ENQUEUED FOR THE PROCESS TO BE
                                     0000  6100 ;	DELETED WHICH MAY CAUSE RESCHEDULING TO OCCUR.
                                     0000  6150 ;
                                     0000  6200 ;--
                                     0000  6250 
                                     0000  6300 
                               00FC' 0000  6350 	.ENTRY	EXE$DELPRC,^M<R2,R3,R4,R5,R6,R7>	; SAVE R2-R7
                          FFFB'  30  0002  6400 	BSBW	EXE$NAMPID		; CONVERT NAME/PID TO PCB ADDRESS
            31            50     E9  0005  6450 	BLBC	R0,RETURN		; EXIT IF ERROR
         28 A4            01     E2  0008  6500 	BBSS	#PCB$V_DELPEN,PCB$L_STS(R4),RETN	; DONE IF MARKED FOR DELETE
                          29         000C       
                                     000D  6550 	SETIPL	#0			; DROP IPL BEFORE ALLOCATE REQUEST
                          51     DD  0010  6600 	PUSHL	R1			; AND SAVE PID OF TARGET PROCESS
                                     0012  6650 	$RESUME_G	(AP)		; RESUME PROCESS
                          FFE4'  30  0019  6700 	BSBW	EXE$ALLOCIRP		; ALLOCATE AST PACKET
            1A            50     E9  001C  6750 	BLBC	R0,RETURN		; RETURN ERROR IF NONE
            55            52     D0  001F  6800 	MOVL	R2,R5			; SET ADDRESS FOR ENQUEUE
