                                     02E7 21700 ;++
                                     02E7 21750 ; FUNCTIONAL DESCRIPTION:
                                     02E7 21800 ;	RETQUOTA IS ENTERED VIA A SPECIAL KERNEL AST DIRECTED TO THE
                                     02E7 21850 ;	CREATOR OF A SUB-PROCESS.  THE AST CONTROL BLOCK CONTAINS THE
                                     02E7 21900 ;	UNUSED QUOTAS TO BE RETURNED TO THE CREATOR.
                                     02E7 21950 ;
                                     02E7 22000 ; INPUT PARAMETERS:
                                     02E7 22050 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     02E7 22100 ;	R5 - POINTER TO AST CONTROL BLOCK
                                     02E7 22150 ;	BYTCNT(R5) - BUFFERED I/O BYTE COUNT QUOTA
                                     02E7 22200 ;	CPULIM(R5) - CPU TIME LIMIT REMAINING
                                     02E7 22250 ;	FILLM(R5)  - OPEN FILE QUOTA
                                     02E7 22300 ;	PRCLM(R5)  - SUB-PROCESS QUOTA
                                     02E7 22350 ;	TQLM(R5)   - TIMER QUEUE ENTRY QUOTA
                                     02E7 22400 ;	PGFLQUOTA(R5) - PAGING FILE QUOTA
                                     02E7 22450 ;
                                     02E7 22500 ; OUTPUT PARAMETERS:
                                     02E7 22550 ;	PHD$L_CPUTIM, PHD$L_CPULIM, PHD$W_FILLM, PHD$W_PRCLM, PHD$W_TQLM,
                                     02E7 22600 ;	PHD$L_PGFLQUOTA AND PCB$W_BYTLM HAVE THE CORRESPONDING REMAINING
                                     02E7 22650 ;	QUOTA VALUES ADDED TO THEM.
                                     02E7 22700 ;
                                     02E7 22750 ; SIDE EFFECTS:
                                     02E7 22800 ;	THE AST CONTROL BLOCK LOCATED VIA R5 IS RELEASED TO THE NON-PAGED
                                     02E7 22850 ;	DYNAMIC MEMORY POOL.
                                     02E7 22900 ;
                                     02E7 22950 ; ENVIRONMENT:
                                     02E7 23000 ;	IPL$_ASTDEL  -  AST DELIVERY IPL WHILE CARRYING POINTER TO 
                                     02E7 23050 ;			AST CONTROL BLOCK.
