                                     0000  6600 	.SBTTL	EXE$FORCEX - FORCE EXIT SYSTEM SERVICE
                                     0000  6700 
                                     0000  6800 ;++
                                     0000  6900 ; FUNCTIONAL DESCRIPTION:
                                     0000  7000 ;
                                     0000  7100 ; CALLING SEQUENCE:
                                     0000  7200 ;	CALLG	ARGLIST,EXE$FORCEX
                                     0000  7300 ;
                                     0000  7400 ; INPUT PARAMETERS:
                                     0000  7500 ;	PIDADR(AP) - ADDRESS OF PID (WRITTEN)
                                     0000  7600 ;	PRCNAM(AP) - ADDRESS OF PROCESS LOGICAL NAME
                                     0000  7700 ;	CODE(AP) - COMPLETION CODE TO BE USED FOR EXIT CALL
                                     0000  7750 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0000  7800 ;
                                     0000  7900 ; IMPLICIT INPUTS:
                                     0000  8000 ;	NONE
                                     0000  8100 ;
                                     0000  8200 ; OUTPUT PARAMETERS:
                                     0000  8300 ;	@PIDADR(AP) - PID OF PROCESS FOR WHICH EXIT WAS FORCED
                                     0000  8400 ;	R0 - COMPLETION STATUS
                                     0000  8500 ;
                                     0000  8600 ; IMPLICIT OUTPUTS:
                                     0000  8700 ;	NONE
                                     0000  8800 ;
                                     0000  8900 ; COMPLETION CODES:
                                     0000  9000 ;	SS$_NORMAL  -  SUCCESSFUL COMPLETION
                                     0000  9100 ;	SS$_INSFMEM -  INSUFFICIENT DYNAMIC MEMORY
                                     0000  9200 ;	SS$_NONEXPR -  NON-EXISTENT PROCESS
                                     0000  9300 ;	SS$_NOPRIV  -  INSUFFICIENT PRIVILEGE TO CONTROL DETACHED PROCESS
                                     0000  9400 ;
                                     0000  9500 ; SIDE EFFECTS:
                                     0000  9600 ;	NONE
                                     0000  9700 ;
                                     0000  9800 ;--
                                     0000  9900 
                                     0000 10000 EXE$FORCEX::				; FORCE EXIT SYSTEM SERVICE
                               003C  0000 10100 	.WORD	^M<R2,R3,R4,R5>		; SAVE REGISTERS R2-R5
                          FFFB'  30  0002 10200 	BSBW	EXE$NAMPID		; CONVERT NAME-PID PAIR
                                     0005 10250 	SETIPL	#0			; DROP IPL
            2B            50     E9  0008 10300 	BLBC	R0,20$			; EXIT IF ERROR
         28 A4            02     E2  000B 10350 	BBSS	#PCB$V_FORCPEN,PCB$L_STS(R4),10$; FORCE EXIT ALREADY PENDING
                          23         000F       
                          51     DD  0010 10400 	PUSHL	R1			; SAVE PID
                          FFEB'  30  0012 10500 	BSBW	EXE$ALLOCIRP		; ALLOCATE AST CONTROL BLOCK
            1E            50     E9  0015 10600 	BLBC	R0,20$			; EXIT IF ERROR
         0B A2            03     90  0018 10700 	MOVB	#3,ACB$B_RMOD(R2)	; SET ACCESS MODE
         0C A2            8E     D0  001C 10800 	MOVL	(SP)+,ACB$L_PID(R2)	; SET PID OF DESTINATION
         10 A2         37'AF     9E  0020 10900 	MOVAB	B^DOEXIT,ACB$L_AST(R2)	; AND ADDRESS OF AST ROUTINE
         14 A2         0C AC     D0  0025 11000 	MOVL	CODE(AP),ACB$L_ASTPRM(R2); SET CODE FOR EXIT CALL
            55            52     D0  002A 11100 	MOVL	R2,R5			; AST CONTROL BLOCK ADDRESS FOR QAST
            52            02     9A  002D 11200 	MOVZBL	#PRI$_RESAVL,R2		; SET PRIORITY INCREMENT CLASS
                          FFCD'  30  0030 11300 	BSBW	SCH$QAST		; QUEUE ACB FOR TARGET PROCESS
            50            01     3C  0033 11400 10$:	MOVZWL	#SS$_NORMAL,R0		; SET NORMAL COMPLETION STATUS
                                 04  0036 11600 20$:	RET				; AND RETURN TO CALLER
                                     0037 11700 
                                     0037 11800 
