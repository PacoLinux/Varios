                                     00AB 31600 	.SBTTL	SCH$CHSE - CHANGE STATE TO EXECUTABLE
                                     00AB 31700 ;++
                                     00AB 31800 ; FUNCTIONAL DESCRIPTION:
                                     00AB 31900 ;	SCH$CHSE CHANGES THE STATE OF A PROCESS, AS REPRESENTED BY
                                     00AB 32000 ;	ITS PCB, TO AN EXECUTABLE STATE.  THE RESCHEDULING INTERRUPT
                                     00AB 32100 ;	WILL BE TRIGGERED IF THE PROCESS IS RESIDENT AND HAS A PRIOITY
                                     00AB 32200 ;	GREATER THAN THAT OF THE CURRENTLY EXECUTING PROCESS.  A 
                                     00AB 32300 ;	PRIORITY INCREMENT CLASS NUMBER SUPPLIED AS A REGISTER CONTAINED
                                     00AB 32400 ;	ARGUMENT IS USED TO COMPUTE THE NEW PROCESS PRIORITY FROM ITS
                                     00AB 32500 ;	BASE PRIORITY.
                                     00AB 32600 ;
                                     00AB 32700 ; CALLING SEQUENCE:
                                     00AB 32800 ;	BSB/JSB	SCH$CHSE	
                                     00AB 32900 ;
                                     00AB 33000 ; INPUT PARAMETERS:
                                     00AB 33100 ;	R0 - NEW PRIORITY	(SCH$CHSEP ONLY)
                                     00AB 33200 ;	R2 - PRIORITY INCREMENT CLASS NUMBER   (SCH$CHSE ONLY)
                                     00AB 33300 ;		0 => NO INCREMENT
                                     00AB 33400 ;		1 => NON-TERMINAL I/O COMPLETION
                                     00AB 33500 ;		2 => RESOURCE AVAILABILITY
                                     00AB 33600 ;		3 => TERMINAL OUTPUT COMPLETION
                                     00AB 33700 ;		4 => TERMINAL INPUT COMPLETION
                                     00AB 33800 ;	R4 - PCB ADDRESS
                                     00AB 33900 ;
                                     00AB 34000 ; IMPLICIT INPUTS:
                                     00AB 34100 ;	SCH$AQ_COMT - COMPUTE QUEUE HEADERS FOR COM,COMO STATES
                                     00AB 34200 ;	SCH$GB_PRI  - CURRENT PROCESS PRIORITY.
                                     00AB 34300 ;
                                     00AB 34400 ;
                                     00AB 34500 ; OUTPUT PARAMETERS:
                                     00AB 34600 ;	R2 - R2 , PRIORITY INCREMENT CLASS NUMBER IF SCH$CHSE. (PRESERVED)
                                     00AB 34700 ;	R3 - R3 (PRESERVED)
                                     00AB 34800 ;
