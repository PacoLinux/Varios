                          B8         00B1       
            51       0000'8F     3C  00B2   139 80$:	MOVZWL	#SS$_EXQUOTA,R1		;SET EXCEEDED QUOTA STATUS
            50            01     3C  00B7   140 	MOVZWL	#RSN$_ASTWAIT,R0	;SET AST WAIT RESOURCE WAIT NUMBER
