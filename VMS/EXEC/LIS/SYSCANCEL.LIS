            1A            50     E8  00C6   145 	BLBS	R0,100$			;IF LBS SUCCESSFUL ALLOCATION
            51       0000'8F     3C  00C9   146 	MOVZWL	#SS$_INSFMEM,R1		;SET INSUFFICIENT MEMORY STATUS
            50            03     3C  00CE   147 	MOVZWL	#RSN$_NPDYNMEM,R0	;SET NONPAGED DYNAMIC MEMORY RESOURCE NUMBER
         28 A4            0A     E0  00D1   148 90$:	BBS	#PCB$V_SSRWAIT,PCB$L_STS(R4),95$ ;IF SET, NO WAIT
