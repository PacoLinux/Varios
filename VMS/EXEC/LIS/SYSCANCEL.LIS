            50         0A A6     3C  0090   130 	MOVZWL	CCB$W_IOC(R6),R0	;GET OUTSTANDING I/O COUNT
            50         04 A6     C8  0094   131 	BISL	CCB$L_WIND(R6),R0	;OUTSTANDING I/O OR FILE ACTIVITY?
                          D0     13  0098   132 	BEQL	40$			;IF EQL NO
         34 A5            13     E1  009A   133 	BBC	#DEV$V_MNT,UCB$L_DEVCHAR(R5),40$ ;IF CLR, DEVICE DISMOUNTED
                          CB         009E       
