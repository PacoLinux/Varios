01              EXE$DLCEFC - DELETE COMMON EVENT CLUSTER                                                                         (1)

                                     00F9 27500 	.SBTTL	EXE$DLCEFC - DELETE COMMON EVENT CLUSTER
                           00000004  00F9 27600 NAME=4					; NAME DISPLACEMENT IN ARGLIST
                                     00F9 27700 
                                     00F9 27800 
                                     00F9 27900 ;++
                                     00F9 28000 ; FUNCTIONAL DESCRIPTION:
                                     00F9 28100 ;	EXE$DLCEFC IMPLEMENTS THE DELETE COMMON EVENT CLUSTER SYSTEM
                                     00F9 28200 ;	SERVICE WHICH PERMITS A PERMANENT COMMON EVENT CLUSTER TO BE
                                     00F9 28300 ;	MARKED FOR DELETION WHEN ITS REFERENCE COUNT BECOMES ZERO.
                                     00F9 28400 ;
                                     00F9 28500 ; CALLING SEQUENCE:
                                     00F9 28600 ;	CALLG	ARGLIST,EXE$DLCEFC
                                     00F9 28700 ;
