         24 A2         0C AC     B0  007C 23400 	MOVW	PROT(AP),CEB$W_PROT(R2) ; AND PROTECTION MASK
            50            6E     9A  0081 23500 	MOVZBL	(SP),R0			; GET STRING COUNT
