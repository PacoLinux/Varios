                                     0137 34300 	.SBTTL	LOCLUST	- LOCATE CLUSTER GIVEN NAME
                                     0137 34400 
                           00000008  0137 34500 NAME=8					; NAME DISPLACEMENT IN ARGLIST
                                     0137 34600 ;++
                                     0137 34700 ; FUNCTIONAL DESCRIPTION:
                                     0137 34800 ;	LOCLUST SEARCHES THE LIST OF COMMON EVENT BLOCKS FOR THE 
                                     0137 34900 ;	NAMED CLUSTER AND RETURNS THE ADDRESS OF THE CLUSTER.
                                     0137 35000 ;	IF NO ERROR CONDITIONS ARE ENCOUNTERED, LOCLUST RETURNS WITH
                                     0137 35100 ;	THE CEB MUTEX LOCKED FOR WRITE.  IN THE EVENT OF ERROR, THE
                                     0137 35200 ;	MUTEX IS UNLOCKED AND A RET IS ISSUED TO EXIT THE SERVICE.
                                     0137 35300 ;
                                     0137 35400 ; CALLING SEQUENCE:
                                     0137 35500 ;	BSB/JSB	LOCLUST
                                     0137 35600 ;
                                     0137 35700 ; INPUT PARAMETERS:
                                     0137 35800 ;	04(SP) - 16 BYTE AREA INTO WHICH NAME WILL BE MOVED
                                     0137 35900 ;	R3 - ADDRESS OF NAME STRING
                                     0137 35950 ;	R4 - PCB ADDRESS 
                                     0137 36000 ;
                                     0137 36100 ; OUTPUT PARAMETERS:
                                     0137 36200 ;	R0 - COMPLETION STATUS
                                     0137 36300 ;	R2 - ADDRESS OF CEB
                                     0137 36400 ;		ZERO IF NAME NOT FOUND
                                     0137 36500 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0137 36600 ;
                                     0137 36700 ; COMPLETION CODES:
                                     0137 36800 ;
                                     0137 36900 ;	SS$_ACCVIO   -  LOGICAL NAME INACCESSIBLE
                                     0137 37000 ;	SS$_IVLOGNAM -  COUNT FOR LOGICAL NAME NOT 1-15
                                     0137 37100 ;	SS$_NOPRIV   -  PRIVILEGE VIOLATION 
                                     0137 37200 ;
                                     0137 37300 ;--
                                     0137 37400 
                                     0137 37500 LOCLUST:				; LOCATE CLUSTER
                                     0137 37700 	IFNORD	#8,(R3),30$		; TEST DESCRIPTOR READABILITY
            51            63     3C  013D 37800 	MOVZWL	(R3),R1			; GET COUNT FROM DESCRIPTOR
                          6F     13  0140 37900 	BEQL	50$			; ILLEGAL STRING LENGTH
            53         04 A3     D0  0142 38000 	MOVL	4(R3),R3		; GET ADDRESS OF STRING
            0F            51     D1  0146 38100 	CMPL	R1,#15			; CHECK LENGTH
                          66     14  0149 38200 	BGTR	50$			; ERROR IF >15
                                     014B 38300 	IFNORD	R1,(R3),30$		; CHECK READABILITY OF STRING
         04 AE            51     90  0151 38400 	MOVB	R1,4(SP)		; SET COUNT IN STRING
                          32     BB  0155 38500 	PUSHR	#^M<R1,R4,R5>		; SAVE REGISTERS FOR STRING MOVE
            63            51     28  0157 38600 	MOVC3	R1,(R3),17(SP)		; COPY STRING TO STACK
                       11 AE         015A       
                          32     BA  015C 38700 	POPR	#^M<R1,R4,R5>		; RESTORE REGISTERS
            01            51     C1  015E 38800 	ADDL3	R1,#1,-(SP)		; SAVE STRING COUNT
                          7E         0161       
                          C6     10  0162 38900 	BSBB	CEBLOCK			; LOCK CEB LIST FOR WRITE
                          02     BA  0164 39000 	POPR	#^M<R1>			; RESTORE COUNT
            50   00000000'EF     DE  0166 39100 	MOVAL	SCH$GQ_CEBHD,R0	; GET ADDRESS OF CEB LIST
            52            50     D0  016D 39200 	MOVL	R0,R2			; AND POINTER TO HEAD OF LIST
            53         04 AE     DE  0170 39300 	MOVAL	4(SP),R3		; SET ADDRESS OF NAME STRING
            52            62     D0  0174 39400 10$:	MOVL	(R2),R2			; FLINK ONWARD
            52            50     D1  0177 39500 	CMPL	R0,R2			; CHECK FOR END OF LIST
