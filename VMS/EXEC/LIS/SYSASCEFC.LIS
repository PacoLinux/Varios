01              RETQUOTA - RETURN CEF QUOTA                                                                                      (1)

                                     0000  7100 	.SBTTL	RETQUOTA - RETURN CEF QUOTA
                                     0000  7200 ;+
                                     0000  7300 ; FUNCTIONAL DESCRIPTION:
                                     0000  7400 ;	RETQUOTA RETURNS THE QUOTA TO THE ORIGINAL CREATOR OF THE
                                     0000  7500 ;	COMMON EVENT CLUSTER.  IF THE PROCESS IDENTIFIED AS THE CLUSTER
                                     0000  7600 ;	CREATOR HAS BEEN DELETED NO OPERATION IS PERFORMED.
                                     0000  7700 ;
                                     0000  7705 ; CALLING SEQUENCE:
                                     0000  7710 ;	BSB/JSB	RETQUOTA
                                     0000  7715 ;
                                     0000  7800 ; INPUT PARAMETERS:
                                     0000  7802 ;	R0 - ADDRESS OF COMMON EVENT BLOCK
                                     0000  7805 ;	R2 - PROCESS INDEX OF CREATOR
                                     0000  7810 ;
                                     0000  7815 ; OUTPUT PARAMETERS:
                                     0000  7820 ;	R0 - R0 (PRESERVED)
                                     0000  7825 ;
                                     0000  7830 ;--
                                     0000  7835 
                                     0000  7840 RETQUOTA:				; RETURN QUOTA
                                     0000  7845 	DSBINT	#IPL$_SYNCH		; BLOCK SYSTEM EVENTS
            51       0000'DF42   D0  0006  7850 	MOVL	@W^SCH$GL_PCBVEC[R2],R1	; CONVERT TO PCB ADDRESS
         0C A0         60 A1     D1  000C  7855 	CMPL	PCB$L_PID(R1),CEB$L_PID(R0)	; CHECK FOR EXISTENCE
                          03     12  0011  7860 	BNEQ	10$			; NO, IGNORE QUOTA
                       4A A1     B6  0013  7865 	INCW	PCB$W_TQCNT(R1)		; GIVE BACK QUOTA
                                     0016  7870 10$:	ENBINT				; ENABLE SYSTEM EVENTS
                                 05  0019  7875 	RSB				; RETURN
                                 00000000  7900 	.PSECT	Y$EXEPAGED,BYTE		; PAGED PSECT
SYSASCEFC       - ASSOCIATE, DISASSOCIATE AND DELETE COMMON EVE  21-AUG-1978 20:18:40   VAX-11 MACRO X0.3-11               Page   4
01              EXE$DACEFC - DISASSOCIATE COMMON EVENT CLUSTER                                                                   (1)

                                     0000  8100 	.SBTTL	EXE$DACEFC - DISASSOCIATE COMMON EVENT CLUSTER
                                     0000  8200 
                                     0000  8300 ;++
                                     0000  8400 ; FUNCTIONAL DESCRIPTION:
                                     0000  8500 ;	EXE$DACEFC IMPLEMENTS THE DISASSOCIATE COMMON EVENT CLUSTER
                                     0000  8600 ;	SYSTEM SERVICE.  THE ISSUING PROCESS IS DISASSOCIATED WITH
                                     0000  8700 ;	THE CLUSTER SELECTED BY THE SPECIFIED EVENT FLAG NUMBER AND
                                     0000  8800 ;	THE CLUSTER CONTROL BLOCK (CEB) WILL BE RELEASED IF THE REFERENCE
                                     0000  8900 ;	COUNT BECOMES ZERO AND IT IS NOT MARKED PERMANENT.
                                     0000  9000 ;
                                     0000  9100 ; CALLING SEQUENCE:
                                     0000  9200 ;	CALLG	ARGLIST,EXE$DACEFC
                                     0000  9300 ;
                                     0000  9400 ; INPUT PARAMETERS:
                                     0000  9500 ;	EFN(AP) - EVENT FLAG NUMBER TO SELECT CLUSTER TO DISASSOCIATE.
                                     0000  9550 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0000  9600 ;
                                     0000  9700 ; OUTPUT PARAMETERS:
                                     0000  9800 ;	R0 - COMPLETION STATUS
                                     0000  9900 ;
                                     0000 10000 ; COMPLETION CODES:
                                     0000 10100 ;	SS$_NORMAL  -  SUCCESSFUL COMPLETION
                                     0000 10200 ;	SS$_ILLEFC  -  ILLEGAL EVENT FLAG NUMBER FOR COMMON EVENT CLUSTER
                                     0000 10300 ;		       MUST BE 64-127
                                     0000 10400 ;
                                     0000 10500 ; SIDE EFFECTS:
                                     0000 10600 ; 	IF THE REFERENCE COUNT FOR THE SELECTED COMMON EVENT CLUSTER
                                     0000 10700 ;	BECOMES ZERO AND IT IS NOT MARKED AS PERMANENT, IT WILL BE
                                     0000 10800 ;	DELETED.
                                     0000 10900 ;
                                     0000 11000 ;--
                                     0000 11100 
                                     0000 11200 EXE$DACEFC::				; DISASSOCIATE COMMON EVNET CLUSTER
                               003C  0000 11300 	.WORD	^M<R2,R3,R4,R5>		; SAVE REGISTERS R2-R5
                          0E     10  0002 11400 	BSBB	GETCLUSTER		; GET CLUSTER INDEX AND VALIDATE
                       58 A445   DD  0004 11500 	PUSHL	PCB$L_EFC2P(R4)[R5]	; PUSH ADDRESS OF CEB FOR THIS CLUSTER
                       58 A445   D4  0008 11600 	CLRL	PCB$L_EFC2P(R4)[R5]	; CLEAR POINTER
                          011B   30  000C 11700 	BSBW	CEBLOCK			; LOCK MUTEX FOR WRITE
                          00A2   31  000F 11800 	BRW	DISASOC			; DISASSOCIATE CLUSTER
SYSASCEFC       - ASSOCIATE, DISASSOCIATE AND DELETE COMMON EVE  21-AUG-1978 20:18:40   VAX-11 MACRO X0.3-11               Page   5
01              GETCLUSTER - GET AND VERIFY CLUSTER NUMBER                                                                       (1)

                                     0012 12000 	.SBTTL	GETCLUSTER - GET AND VERIFY CLUSTER NUMBER
                                     0012 12100 
                                     0012 12200 ;++
                                     0012 12300 ; FUNCTIONAL DESCRIPTION:
                                     0012 12400 ;	GETCLUSTER EXTRACTS THE CLUSTER NUMBER FROM THE EFN ARGUMENT,
                                     0012 12500 ;	VALIDATES IT AND SUBTRACTS THE LOCAL EVENT CLUSTER BIAS TO
                                     0012 12600 ;	FORM A COMMON EVENT CLUSTER INDEX.  IF THE CLUSTER NUMBER
                                     0012 12700 ;	IS NOT A VALID COMMON EVENT CLUSTER, AN EXIT IS MADE FROM
                                     0012 12800 ;	THE SERVICE BY ISSUING A RET
                                     0012 12900 ;
                                     0012 13000 ; CALLING SEQUENCE:
                                     0012 13100 ;	BSB/JSB	GETCLUSTER
                                     0012 13200 ;
                                     0012 13300 ; INPUT PARAMETERS:
                                     0012 13400 ;	EFN(AP) - EVENT FLAG NUMBER
                                     0012 13450 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0012 13500 ;
                                     0012 13600 ; OUTPUT PARAMETERS:
                                     0012 13800 ;	R5 - COMMON EVENT CLUSTER INDEX
                                     0012 13900 ;
                                     0012 14000 ; COMPLETION CODES:	(ON ERROR RETURN ONLY)
                                     0012 14100 ;	SS$_ILLEFC - ILLEGAL COMMON EVENT CLUSTER
                                     0012 14200 ;
                                     0012 14300 ;--
                                     0012 14400 
                                     0012 14500 GETCLUSTER:				; GET COMMON EVENT CLUSTER INDEX
            03            05     EE  0012 14600 	EXTV	#5,#3,EFN(AP),R5	; EXTRACT CLUSTER FROM EFN
            55         04 AC         0015       
            55            02     C2  0018 14700 	SUBL	#2,R5			; SUBTRACT BIAS
                          01     19  001B 14800 	BLSS	10$			; ILLEGAL CLUSTER NUMBER
                                 05  001D 15000 	RSB				; SUCCESSFUL RETURN
                                     001E 15100 
            50       00EC 8F     3C  001E 15200 10$:	MOVZWL	#SS$_ILLEFC,R0		; SET ERROR CODE
                                 04  0023 15300 	RET				; AND EXIT SERVICE CALL
                                     0024 15400 
                                     0024 15500 
SYSASCEFC       - ASSOCIATE, DISASSOCIATE AND DELETE COMMON EVE  21-AUG-1978 20:18:40   VAX-11 MACRO X0.3-11               Page   6
01              EXE$ASCEFC - ASSOCIATE AND CREATE COMMON EVENT                                                                   (1)

                                     0024 15700 	.SBTTL	EXE$ASCEFC - ASSOCIATE AND CREATE COMMON EVENT CLUSTER
                                     0024 15800 
                                     0024 15900 ;++
                                     0024 16000 ; FUNCTIONAL DESCRIPTION:
                                     0024 16100 ;	EXE$ASCEFC IMPLEMENTS THE ASSOCIATE COMMON EVENT CLUSTER SYSTEM 
                                     0024 16200 ;	SERVICE.  IF THE NAMED COMMON EVENT CLUSTER DOES NOT ALREADY
                                     0024 16300 ;	EXIST, IT WILL BE CREATED.  IF IT ALREADY EXISTS, THE PROCESS
                                     0024 16400 ;	WILL BE ASSOCIATED WITH IT, PROTECTION PERMITTING.
                                     0024 16500 ;
                                     0024 16600 ; CALLING SEQUENCE:
                                     0024 16700 ;	CALLG	ARGLIST,EXE$ASCEFC
                                     0024 16800 ;
                                     0024 16900 ; INPUT PARAMETERS:
                                     0024 17000 ;	EFN(AP)  - EVENT FLAG NUMBER TO SPECIFY CLUSTER NUMBER
                                     0024 17100 ;	NAME(AP) - ADDRESS OF LOGICAL NAME FOR CLUSTER
                                     0024 17200 ;	PROT(AP) - PROTECTION MASK FOR CLUSTER IF CREATING
                                     0024 17300 ;	PERM(AP) - INDICATOR TO SPECIFY CREATION OF PERMANENT CLUSTER
                                     0024 17350 ;	R4 - PCB ADDRESS
                                     0024 17400 ;
                                     0024 17500 ; IMPLICIT INPUTS:
                                     0024 17600 ;	PCB OF CURRENT PROCESS
                                     0024 17700 ;
                                     0024 17800 ; OUTPUT PARAMETERS:
                                     0024 17900 ;	R0 - COMPLETION STATUS CODE
                                     0024 18000 ;
                                     0024 18100 ; IMPLICIT OUTPUTS:
                                     0024 18200 ;	PCB$L_EFC2P - POINTER TO COMMON EVENT CONTROL BLOCK IF CLUSTER 2
                                     0024 18300 ;	PCB$L_EFC3P - POINTER TO COMMON EVENT CONTROL BLOCK IF CLUSTER 3
                                     0024 18400 ;
                                     0024 18500 ; COMPLETION CODES:
                                     0024 18600 ;	SS$_NORMAL   -  SUCCESSFUL COMPLETION
                                     0024 18700 ;	SS$_EXQUOTA  -  QUOTA FOR COMMON EVENT BLOCK CREATION EXHAUSTED 
                                     0024 18800 ;	SS$_ILLEFC   -  ILLEGAL CLUSTER NUMBER (NOT 0,1,2,3)
                                     0024 18900 ;	SS$_IVLOGNAM -  INVALID LOGICAL NAME.  STRING LENGTH NOT 1-15.
                                     0024 19000 ;	SS$_NOPRIV   -  PRIVILEGE VIOLATION
                                     0024 19100 ;
                                     0024 19200 ; SIDE EFFECTS:
                                     0024 19300 ;	AN IMPLIED DISASSOCIATE IS PERFORMED ON THE SPECIFIED CLUSTER
                                     0024 19400 ;	AND THE PREVIOUSLY ASSOCIATED COMMON EVENT CLUSTER MAY BE
                                     0024 19500 ;	DELETED IF IT IS NOT MARKED PERMANENT AND ITS REFERENCE COUNT
                                     0024 19600 ;	BECOMES ZERO.
                                     0024 19700 ;
                                     0024 19800 ;--
                                     0024 19900 
                                     0024 20000 	.ENABL	LSB
                                     0024 20100 EXE$ASCEFC::				; ASSOCIATE COMMON EVENT CLUSTER
                               003C  0024 20200 	.WORD	^M<R2,R3,R4,R5>		; REGISTERS R2-R5
                          EA     10  0026 20300 	BSBB	GETCLUSTER		; GET CLUSTER INDEX AND VALIDATE
            53         08 AC     D0  0028 20400 	MOVL	NAME(AP),R3		; GET POINTER TO CLUSTER NAME
                       10 AC     DD  002C 20500 	PUSHL	PERM(AP)		; SAVE PERMANENT INDICATOR
            09            6E     E9  002F 20600 	BLBC	(SP),10$		; IS PERMANENT INDICATOR SET?
                                     0032 20700 	IFPRIV	PRMCEB,10$		; CONTINUE IF PRIVILEGED
            50            24     3C  0037 20800 	MOVZWL	#SS$_NOPRIV,R0		; SET ERROR CODE
                                 04  003A 20900 	RET				; AND EXIT SERVICE
            5E            10     C2  003B 21000 10$:	SUBL	#16,SP			; ALLOCATE SPACE FOR NAME ON STACK
                          00F6   30  003E 21100 	BSBW	LOCLUST			; LOCATE CLUSTER
                          52     D5  0041 21200 	TSTL	R2			; WAS NAME FOUND
