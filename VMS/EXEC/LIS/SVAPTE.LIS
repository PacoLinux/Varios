                                     0000   100 	.SBTTL	SVAPTECHK - SYSTEM VIRTUAL ADDRESS OF PAGE TABLE ENTRY
                                     0000   200 ;++
                                     0000   300 ; FUNCTIONAL DESCRIPTION:
                                     0000   400 ;
                                     0000   500 ;	GIVEN A PROCESS OR SYSTEM VIRTUAL ADDRESS IN R2, RETURN THE
                                     0000   600 ;	SYSTEM VIRTUAL ADDRESS OF THE CORRESPONDING PAGE TABLE ENTRY
                                     0000   700 ;	IF THE ADDRESS IS OFF THE END OF ITS PAGE TABLE, BUGCHK
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;
                                     0000  1100 ;	BSBW	MMG$SVAPTECHK
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;
                                     0000  1500 ;	R2 = VIRTUAL ADDRESS (P0, P1, OR SYSTEM SPACE)
                                     0000  1600 ;	R4 = PROCESS CONTROL BLOCK ADDRESS
                                     0000  1700 ;	R5 = PROCESS HEADER ADDRESS
                                     0000  1800 ;	IPL = SYNCH (TO AVOID HAVING PROCESS PAGE TABLE MOVE)
                                     0000  1900 ;
                                     0000  2000 ; IMPLICIT INPUTS:
                                     0000  2100 ;
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; OUTPUT PARAMETERS:
                                     0000  2500 ;
                                     0000  2600 ;	R2 PRESERVED
                                     0000  2700 ;	R3 = SYSTEM VIRTUAL ADDRESS OF THE PAGE TABLE ENTRY
                                     0000  2800 ;
                                     0000  2900 ; IMPLICIT OUTPUTS:
                                     0000  3000 ;
                                     0000  3100 ;	NONE
                                     0000  3200 ;
                                     0000  3300 ; COMPLETION CODES:
                                     0000  3400 ;
                                     0000  3500 ;	NONE
                                     0000  3600 ;
                                     0000  3700 ; SIDE EFFECTS:
                                     0000  3800 ;
                                     0000  3900 ;	NONE
                                     0000  4000 ;
                                     0000  4100 ;--
                                     0000  4200 MMG$SVAPTECHK::
            52            1F     E1  0000  4300 	BBC	#31,R2,MMG$PTEADRCHK	;HANDLE PROCESS SPACE VIA PTEADRCHK
