                                     0019   100 	.SBTTL	PTEADR, PTEINDX - PAGE TABLE ENTRY ADDRESS OR INDEX
                                     0019   200 ;++
                                     0019   300 ; FUNCTIONAL DESCRIPTION:
                                     0019   400 ;
                                     0019   500 ;	GIVEN A VIRTUAL ADDRESS (SYSTEM OR PROCESS), CALCULATE
                                     0019   600 ;		1) THE SYSTEM VIRTUAL ADDRESS OF THE PAGE TABLE ENTRY
                                     0019   700 ;	OR
                                     0019   800 ;		2) THE LONG WORD INDEX FROM THE BASE OF THE PROCESS/SYSTEM
                                     0019   900 ;		HEADER TO THE PAGE TABLE ENTRY
                                     0019  1000 ;
                                     0019  1100 ; CALLING SEQUENCE:
                                     0019  1200 ;
                                     0019  1300 ;	BSBW	MMG$PTEREF		;FAULT PTE, RETURN SYS VIRT ADR OF PTE
                                     0019  1400 ;					;RETURN IPL=SYNCH, R0 = SYSTEM STATUS
                                     0019  1500 ;	BSBW	MMG$PTEADRCHK		;RETURN SYSTEM VIRTUAL ADDRESS OF PTE
                                     0019  1600 ;					;BUGCHK IF LENGTH VIOLATION
                                     0019  1700 ;	BSBW	MMG$PTEINDX		;RETURN LONG WORD INDEX TO PTE
                                     0019  1800 ;					;R0 = SYSTEM STATUS CODE
                                     0019  1900 ;	BSBW	MMG$PTEINDXCHK		;RETURN LONG WORD INDEX TO PTE
                                     0019  2000 ;					;BUGCHK IF LENGTH VIOLATION
                                     0019  2100 ;
                                     0019  2200 ; INPUT PARAMETERS:
                                     0019  2300 ;
                                     0019  2400 ;	R2 = PROCESS VIRTUAL ADDRESS (P0 OR P1 SPACE)
                                     0019  2500 ;	R4 = PROCESS CONTROL BLOCK ADDRESS
                                     0019  2600 ;	R5 = PROCESS HEADER ADDRESS
                                     0019  2700 ;		THIS MAY BE EITHER THE SYSTEM SPACE OR P1 SPACE ADDRESS
                                     0019  2800 ;		OF THE PROCESS HEADER, BUT IF IT IS THE SYSTEM SPACE ADDRESS
                                     0019  2900 ;		THE CALLING IPL MUST BE AT SYNCH TO PREVENT SWAPPING
                                     0019  3000 ;	IPL LEQ SYNCH - IF REQUESTING THE ADDRESS OF THE PAGE TABLE ENTRY
                                     0019  3100 ;		OR IF THE SYSTEM SPACE ADDRESS OF THE PROCESS HEADER IS
                                     0019  3200 ;		SPECIFIED, IPL MUST BE AT SYNCH TO PREVENT SWAPPING
                                     0019  3300 ;
                                     0019  3400 ;  OR
                                     0019  3500 ;
                                     0019  3600 ;	R2 = SYSTEM SPACE VIRTUAL ADDRESS
                                     0019  3700 ;	R4 = SYSTEM PROCESS CONTROL BLOCK ADDRESS
                                     0019  3800 ;	R5 = SYSTEM ADDRESS (NO P1 SPACE EQUIVALENT) OF SYSTEM PROCESS HEADER
                                     0019  3900 ;	IPL NOT REQUIRED TO BE AT SYNCH SINCE THE SYSTEM PAGE TABLE IS
                                     0019  4000 ;		NOT EVER MOVED.
                                     0019  4100 ;
                                     0019  4200 ; IMPLICIT INPUTS:
                                     0019  4300 ;
                                     0019  4400 ;	NONE
                                     0019  4500 ;
                                     0019  4600 ; OUTPUT PARAMETERS:
                                     0019  4700 ;
                                     0019  4800 ;	R0 = SYSTEM STATUS CODE
                                     0019  4900 ;	R2 PRESERVED
                                     0019  5000 ;	R3 = SYSTEM VIRTUAL ADDRESS OF PAGE TABLE ENTRY OR LONG WORD
                                     0019  5100 ;		INDEX TO PAGE TABLE ENTRY ACCORDING TO THE ENTRY POINT
                                     0019  5200 ;
                                     0019  5300 ; IMPLICIT OUTPUTS:
                                     0019  5400 ;
                                     0019  5500 ;	NONE
                                     0019  5600 ;
                                     0019  5700 ; COMPLETION CODES:
