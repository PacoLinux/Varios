X0103           PURGWSSCN - Scan Working Set to Purge Pages                                                                      (4)

                                     0040   100 	.SBTTL	PURGWSSCN - Scan Working Set to Purge Pages
                                     0040   200 ;++
                                     0040   300 ; FUNCTIONAL DESCRIPTION:
                                     0040   400 ;
                                     0040   500 ;	This routine is called to do a scan of the working set list
                                     0040   600 ; for pages in the specified virtual address range and delete those
                                     0040   700 ; pages from the working set list.
                                     0040   800 ;
                                     0040   900 ; CALLING SEQUENCE:
                                     0040  1000 ;
                                     0040  1100 ;	BSBW	MMG$PURGWSSCN
                                     0040  1200 ;
                                     0040  1300 ; INPUT PARAMETERS:
                                     0040  1400 ;
                                     0040  1500 ;	R0 = Access mode in low byte, flags in rest
                                     0040  1600 ;	R2 = STARTVA
                                     0040  1700 ;	R4 = Process Control Block
                                     0040  1800 ;	R5 = Process Header Address (P1 space ok)
                                     0040  1900 ;	R6 = Number of pages - 1 remaining in range of pages
                                     0040  2000 ;	R7 = +^X200 if going forward in the address space
                                     0040  2010 ;	   = -^X200 if going backwards in the address space
                                     0040  2020 ;	R8 = 0 if scanning only for global pages in the specified range
                                     0040  2025 ;	     Used by MMG$DELPAG when a GLOBAL page is found in the range
                                     0040  2030 ;	   = 1 if scanning for any pages in the specified range
                                     0040  2065 ;	     Used by the $PURGWS System Service
                                     0040  2100 ;
                                     0040  2200 ; IMPLICIT INPUTS:
                                     0040  2300 ;
                                     0040  2400 ;	NONE
                                     0040  2500 ;
                                     0040  2600 ; OUTPUT PARAMETERS:
                                     0040  2700 ;
                                     0040  2800 ;	R0,R2 PRESERVED
                                     0040  2900 ;
                                     0040  3000 ; IMPLICIT OUTPUTS:
                                     0040  3100 ;
                                     0040  3200 ;	NONE
                                     0040  3300 ;
                                     0040  3400 ; COMPLETION CODES:
                                     0040  3500 ;
                                     0040  3600 ;	NONE
                                     0040  3700 ;
                                     0040  3800 ; SIDE EFFECTS:
                                     0040  3900 ;
                                     0040  4000 ;	NONE
                                     0040  4100 ;
                                     0040  4200 ;--
SYSPURGWS       - Purge Working Set System Service               21-AUG-1978 20:38:01   VAX-11 MACRO X0.3-11               Page   7
X0103           PURGWSSCN - Scan Working Set to Purge Pages                                                                      (5)

                                     0040   100 ;
                                     0040   200 ; **********************************************************************
                                     0040   300 ;
                                     0040   400 ; **************** THE FOLLOWING CODE MUST BE RESIDENT *****************
                                     0040   500 ;
                                 00000000   600 	.PSECT	$MMGCOD
                                     0000   700 ;
                                     0000   800 ; **********************************************************************
                                     0000   900 ;
                                     0000   910 	.ENABL	LSB
                                     0000   920 
                                     0000   930 PURGWS_EMPTY:
                          00B2   31  0000   940 	BRW	80$			;No pages to be purged
                                     0003   950 
                                     0003  1000 MMG$PURGWSSCN::
                                     0003  1100 	DSBINT	#IPL$_SYNCH		;Save called IPL, raise to SYNCH
            7E            12     DB  0003       		MFPR	S^#PR$_IPL,-(SP)
            12            07     DA  0006       		MTPR	#IPL$_SYNCH,S^#PR$_IPL
                                     0009  1200 					;no swapping while scanning the WSL
            51         0C A5     3C  0009  1250 	MOVZWL	PHD$W_WSLOCK(R5),R1	;Start scanning with locked pages
                                     000D  1260 					;if this is a GLOBAL scan
            7E         38 A4     3C  000D  1300 	MOVZWL	PCB$W_GPGCNT(R4),-(SP)	;Count of GLOBAL pages in WSL
            10            58     E9  0011  1310 	BLBC	R8,5$			;Branch if scanning for GLOBAL pages
            51         0E A5     3C  0014  1320 	MOVZWL	PHD$W_WSDYN(R5),R1	;Don't consider the locked pages
                                     0018  1330 					;when scanning for all page types
            6E         3A A4     A0  0018  1340 	ADDW	PCB$W_PPGCNT(R4),(SP)	;Calculate number of WSL entries
            51         08 A5     A3  001C  1350 	SUBW3	PHD$W_WSLIST(R5),R1,R3	;Number of entries in locked area
                          53         0020       
            6E            53     A2  0021  1360 	SUBW	R3,(SP)			;Entry count = GPGCNT + PPGCNT - locked
                          DA     13  0024  1390 5$:	BEQL	PURGWS_EMPTY		;Branch if no entries to scan
                          05     BB  0026  1500 	PUSHR	#^M<R0,R2>		;Save STARTVA, access mode
            57            1F     E0  0028  1600 	BBS	#31,R7,10$		;Branch if going backwards
                          0B         002B       
            52         F7 8F     78  002C  1700 	ASHL	#-9,R2,R3		;Low virtual page number
                          53         0030       
            53            56     C1  0031  1800 	ADDL3	R6,R3,R0		;High virtual page number
                          50         0034       
                          09     11  0035  1900 	BRB	20$
            52         F7 8F     78  0037  2000 10$:	ASHL	#-9,R2,R0		;High virtual page number
                          50         003B       
            50            56     C3  003C  2100 	SUBL3	R6,R0,R3		;Low virtual page number
                          53         003F       
                          09     BB  0040  2200 20$:	PUSHR	#^M<R0,R3>		;Push high VPN, low VPN
                                     0042  2300 ;
                                     0042  2400 ; 0(SP)  = High virtual page number
                                     0042  2500 ; 4(SP)  = Low virtual page number
                                     0042  2600 ; 8(SP)  = Access mode
                                     0042  2700 ; 12(SP) = Saved STARVA
                                     0042  2800 ; 16(SP) = Count of pages to scan for
                                     0042  2900 ; 20(SP) = Saved caller's IPL
                                     0042  3000 ;
            52            6541   D0  0042  3400 24$:	MOVL	(R5)[R1],R2		;Fetch next WSLE
            5C            52     E9  0046  3500 	BLBC	R2,60$			;Branch if not valid
            07            58     E9  0049  3510 	BLBC	R8,26$			;Branch if just scanning for GLOBALS
            52            30     93  004C  3520 	BITB	#<WSL$M_WSLOCK ! WSL$M_PFNLOCK>,R2 ;All page types,
                          09     13  004F  3525 	BEQL	28$			;but no locked ones
                          4D     11  0051  3530 	BRB	50$
SYSPURGWS       - Purge Working Set System Service               21-AUG-1978 20:38:01   VAX-11 MACRO X0.3-11               Page   8
