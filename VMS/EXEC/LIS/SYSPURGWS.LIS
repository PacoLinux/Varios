                               FEFF  00AC       		.WORD	^XFEFF
                               0004' 00AE       		.IIF IDN <FATAL>,<FATAL> , .WORD	BUG$_WSLPAGCNT!4
            5E            08     C0  00B0  6100 70$:	ADDL	#2*4,SP			;Clean off page numbers
                          05     BA  00B3  6200 	POPR	#^M<R0,R2>		;Restore R0 = access mode, R2 = STARTVA
            5E            04     C0  00B5  6300 80$:	ADDL	#1*4,SP			;Clean off page count
                                     00B8  6400 	ENBINT				;Restore caller's IPL
            12            8E     DA  00B8       		MTPR	(SP)+,S^#PR$_IPL
                                 05  00BB  6500 	RSB				;and exit to it
                                     00BC  6510 ;
                                     00BC  6520 ; FREWSLX needed to allocate page file and couldn't
                                     00BC  6530 ;
            DF            58     E8  00BC  6540 90$:	BLBS	R8,40$			;Leave it in working set if $PURGWS
                                     00BF  6600 ;
                                     00BF  6700 ; Error in unlocking a locked global page or
                                     00BF  6750 ; in freeing page from the working set list.
                                     00BF  6800 ;
                                     00BF  6900 100$:	BUG_CHECK PURGWSSCN,FATAL
                               FEFF  00BF       		.WORD	^XFEFF
