            6E            50     D1  0067  4100 	CMPL	R0,(SP)			;Below the high address?
                          34     14  006A  4200 	BGTR	50$			;Branch if no
                          51     DD  006C  4300 32$:	PUSHL	R1			;Save the working set list index
                          FF8F'  30  006E  4400 	BSBW	MMG$SVAPTECHK		;Get the SVAPTE in R3
            51            6E     D0  0071  4420 	MOVL	(SP),R1			;Recover WSLX
            52            30     93  0074  4800 	BITB	#<WSL$M_WSLOCK ! WSL$M_PFNLOCK>,R2 ;Is this entry locked?
                          1F     13  0077  4820 	BEQL	35$			;Branch if not
            50         0C AE     9A  0079  4840 	MOVZBL	<8+4>(SP),R0		;Get access mode to check
            02            17     ED  007D  4860 	CMPZV	#PTE$V_OWN,#PTE$S_OWN,(R3),R0 ;Legal to unlock the page?
            50            63         0080       
                          1A     19  0082  4880 	BLSS	40$			;Branch if not, just skip it.
                          58     DD  0084  4920 	PUSHL	R8			;Save registsers
                          53     DD  0086  4930 	PUSHL	R3			;altered by unlock WSLE logic
                          FF75'  30  0088  4950 	BSBW	MMG$ULKGBLWSLE		;Unlock WSLE for GLOBAL page
            31            50     E9  008B  4960 	BLBC	R0,100$			;Error status should not be returned
                     0108 8F     BA  008E  4970 	POPR	#^M<R3,R8>		;Recover saved registers
            51         0E A5     3C  0092  4980 	MOVZWL	PHD$W_WSDYN(R5),R1	;New WSLX for this page
                          6E     D7  0096  4990 	DECL	(SP)			;Continue scanning from original wslx
                          FF65'  30  0098  5000 35$:	BSBW	MMG$FREWSLX		;Delete working set list entry
            1E            50     E9  009B  5100 	BLBC	R0,90$			;Branch if need to wait for page file
                          02     BA  009E  5200 40$:	POPR	#^M<R1>			;Recover WSLX to continue scan from
                       10 AE     D7  00A0  5300 50$:	DECL	16(SP)			;Count another page found
                          0B     15  00A3  5400 	BLEQ	70$			;Branch if no more to look for
            01         12 A5     3D  00A5  5500 60$:	ACBW	PHD$W_WSLAST(R5),#1,R1,24$ ;Next working set list index
            FF96          51         00A9       
