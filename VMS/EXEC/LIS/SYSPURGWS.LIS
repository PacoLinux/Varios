            03            01     EC  0053  3600 26$:	CMPV	#WSL$V_PAGTYP,#WSL$S_PAGTYP,R2,#PFN$C_GLOBAL ;GLOBAL page?
            02            52         0056       
                          4B     19  0058  3700 	BLSS	60$			;Branch if not
            52         F7 8F     78  005A  3800 28$:	ASHL	#-9,R2,R0		;Get virtual page number
                          50         005E       
                          0B     19  005F  3850 	BLSS	32$			;If system address, then discard it.
                                     0061  3860 					;It is a page table page which is not
