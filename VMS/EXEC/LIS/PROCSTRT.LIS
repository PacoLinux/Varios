                                     0276   290 ; EXE$CATCH_ALL - CATCH ALL CONDITION HANDLER
                                     0276   291 ;
                                     0276   292 ; THIS ROUTINE IS ENTERED AS THE RESULT OF AN UNFIELDED OR IMPROPERLY HANDLED
                                     0276   293 ; EXCEPTION CONDITION OR SOFTWARE SIGNAL.
                                     0276   294 ;
                                     0276   295 ; INPUTS:
                                     0276   296 ;
                                     0276   297 ;	CHF$L_MCHARGLST(AP) = ADDRESS OF MECHANISM ARGUMENT LIST.
                                     0276   298 ;	CHF$L_SIGARGLST(AP) = ADDRESS OF CONDITION ARGUMENT LIST.
                                     0276   299 ;
                                     0276   300 ; OUTPUTS:
                                     0276   301 ;
                                     0276   302 ;	A MESSAGE IS ISSUED USING THE SYS$PUTMSG SYSTEM SERVICE AND A TEST IS
                                     0276   303 ;	MADE ON THE CONDITION NAME TO DETERMINE IF THE IMAGE SHOULD BE ALLOWED
                                     0276   304 ;	TO CONTINUE EXECUTION. THE FOLLOWING CONDITIONS CAUSE A FORCED IMAGE
                                     0276   305 ;	EXIT:
                                     0276   306 ;
                                     0276   307 ;		1. ANY ENTRY TO THIS ROUTINE VIA THE LAST CHANCE VECTOR.
                                     0276   308 ;
                                     0276   309 ;		2. THE CONDITION NAME HAS A SEVERITY OF "SEVERE ERROR".
                                     0276   310 ;
                                     0276   311 ;	IF A FORCED IMAGE EXIT IS PERFORMED, THEN A SUMMARY OF THE CONDITION
                                     0276   312 ;	ARGUMENTS AND FINAL REGISTERS ARE WRITTEN TO SYS$OUTPUT.
                                     0276   313 ;-
                                     0276   314  
                               0000' 0276   315 	.ENTRY	EXE$CATCH_ALL,^M<>
                          00     DD  0278   316 	PUSHL	#0			; SET EXCEPTION NAME FLAG FALSE
