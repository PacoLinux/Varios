                       06 A2     B5  0282   320 	TSTW	CHF$L_SIG_NAME+2(R2)	; POSSIBLY SYSTEM EXCEPTION NAME?
                          1C     12  0285   321 	BNEQ	20$			; IF NEQ NO
                       08 AE     D6  0287   322 	INCL	8(SP)			; SET EXCEPTION NAME FLAG TRUE
            51            17     9A  028A   323 	MOVZBL	#EXSIZE,R1		; SET LOOP COUNT
            50       FDC9 CF     9E  028D   324 	MOVAB	EXTABLE,R0		; GET ADDRESS OF EXCEPTION TABLE
            7E            80     3C  0292   325 10$:	MOVZWL	(R0)+,-(SP)		; GET NEXT HARDWARE EXCEPTION CODE
                          03     ED  0295   326 	CMPZV	#STS$V_CODE,#STS$S_CODE,- ; CONDITION VALUE HARDWARE CODE?
         04 A2            0C         0297   327 		CHF$L_SIG_NAME(R2),(SP)+ ;
                          8E         029A       
                          09     13  029B   328 	BEQL	30$			; IF EQL YES
            F2            51     F5  029D   329 	SOBGTR	R1,10$			; ANY MORE TO COMPARE?
                       08 AE     D4  02A0   330 	CLRL	8(SP)			; SET EXCEPTION NAME FALG FALSE
            62            02     C2  02A3   331 20$:	SUBL	#2,(R2)			; ADJUST LENGTH OF ARGUMENT LIST
                          00     DD  02A6   332 30$:	PUSHL	#0			; CLEAR ADDRESS OF FACILITY NAME DESCRIPTOR
                          00     DD  02A8   333 	PUSHL	#0			; CLEAR ADDRESS OF ACTION ROUTINE
                          62     9F  02AA   334 	PUSHAB	(R2)			; SET ADDRESS OF MESSAGE VECTOR
   00000000'9F            03     FB  02AC   335 	CALLS	#3,@#SYS$PUTMSG		; OUTPUT MESSAGE
