                                     0311   384 	.SBTTL	CREATELOG - CREATE LOGICAL NAME
                                     0311   385 ;++
                                     0311   386 ; FUNCTIONAL DESCRIPTION:
                                     0311   387 ;	CREATELOG CALLS THE CREATE LOGICAL NAME SYSTEM SERVICE
                                     0311   388 ;	TO CREATE THE LOGICAL NAME EQUIVALENCES GIVEN IN THE QUOTA
                                     0311   389 ;	BUFFER.
                                     0311   390 ;
                                     0311   391 ; CALLING SEQUENCE:
                                     0311   392 ;	BSBW	CREATELOG
                                     0311   393 ;	.BYTE	DESCRIPTOR-DESCBASE
                                     0311   394 ;	.BYTE	PQB$T_<OFFSET>
                                     0311   395 ;
                                     0311   396 ; INPUT PARAMETERS:
                                     0311   397 ;	R6 - ADDRESS OF PROCESS QUOTA BLOCK
                                     0311   398 ;	@(SP) - BYTE OFFSET FROM DESCRIPTOR BASE TO DESCRIPTOR
                                     0311   399 ;	1+@(SP) - PROCESS QUOTA BLOCK OFFSET
                                     0311   400 ;
                                     0311   401 ; OUTPUT PARAMETERS:
                                     0311   402 ;	NONE
                                     0311   403 ;
                                     0311   404 ; IMPLICIT INPUTS:
                                     0311   405 ;	NONE
                                     0311   406 ;
                                     0311   407 ; IMPLICIT OUTPUTS:
                                     0311   408 ;	CREATION OF SPECIFIED LOGICAL NAMES
                                     0311   409 ;
                                     0311   410 ;--
                                     0311   411 
                                     0311   412 CREATELOG:				; CREATE LOGICAL NAME
            52            6E     D0  0311   413 	MOVL	(SP),R2			; GET ARGUMENT POINTER
            6E            02     C0  0314   414 	ADDL	#2,(SP)			; CORRECT RETURN PC VALUE
            53            82     9A  0317   415 	MOVZBL	(R2)+,R3		; GET DESCRIPTOR OFFSET
            53       FD0B CF43   9E  031A   416 	MOVAB	DESCBASE[R3],R3		; COMPUTE ADDRESS OF DESCRIPTOR
            52            62     9A  0320   417 	MOVZBL	(R2),R2			; GET PROCESS QUOTA BLOCK OFFSET
            52            6642   DE  0323   418 	MOVAL	(R6)[R2],R2		; COMPUTE ADDRESS OF STRING
            51            82     9A  0327   419 	MOVZBL	(R2)+,R1		; GET STRING LENGTH
                          18     13  032A   420 	BEQL	10$			; NULL, IGNORE
            7E            51     7D  032C   421 	MOVQ	R1,-(SP)		; STACK STRING DESCRIPTOR
            52            5E     D0  032F   422 	MOVL	SP,R2			; SAVE DESCRIPTOR ADDRESS
                                     0332   423 	$CRELOG_S	#2,(R3),(R2),#1	; CREATE NAME
