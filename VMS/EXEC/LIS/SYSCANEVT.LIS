01              CANCEL TIMER REQUEST                                                                                             (1)

                                     0000    54 	.SBTTL	CANCEL TIMER REQUEST
                                     0000    55 ;+
                                     0000    56 ; EXE$CANTIM - CANCEL TIMER REQUEST
                                     0000    57 ;
                                     0000    58 ; THIS SERVICE PROVIDES THE CAPABILITY TO CANCEL ALL OR A SPECIFIED SUBSET
                                     0000    59 ; OF TIMERS THAT WERE PREVIOUSLY ESTABLISHED.
                                     0000    60 ;
                                     0000    61 ; INPUTS:
                                     0000    62 ;
                                     0000    63 ;	REQIDT(AP) = REQUEST IDENTIFICATION (ZERO IMPLIES ALL).
                                     0000    64 ;	ACMODE(AP) = ACCESS MODE (ALL EQUAL AND HIGHER ACCESS MODES).
                                     0000    65 ;
                                     0000    66 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    67 ;
                                     0000    68 ; OUTPUTS:
                                     0000    69 ;
                                     0000    70 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    71 ;
                                     0000    72 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0000    73 ;-
                                     0000    74  
                                     0000    75 	.ENABL	LSB
                               003C' 0000    76 	.ENTRY	EXE$CANTIM,^M<R2,R3,R4,R5>
            02            00     EF  0002    77 	EXTZV	#0,#2,ACMODE(AP),R0	;GET SPECIFIED ACCESS MODE
            50         08 AC         0005       
                 00000000'EF     16  0008    78 	JSB	EXE$MAXACMODE		;MAXIMIZE ACCESS MODE WITH PREVIOUS MODE
            55         60 A4     D0  000E    79 	MOVL	PCB$L_PID(R4),R5	;SET PROCESS ID
            54            00     9A  0012    80 	MOVZBL	#TQE$C_TMSNGL,R4	;SET TYPE OF ENTRY
            53         04 AC     D0  0015    81 	MOVL	REQIDT(AP),R3		;SET REQUEST IDENTIFICATION
            52            50     D0  0019    82 	MOVL	R0,R2			;SET CANCELLATION ACCESS MODE
                          0E     11  001C    83 	BRB	10$			;FINISH IN COMMON CODE
SYSCANEVT       - SYSTEM SERVICES TO CANCEL EVENTS               21-AUG-1978 20:21:50   VAX-11 MACRO X0.3-11               Page   3
01              CANCEL WAKEUP                                                                                                    (1)

                                     001E    85 	.SBTTL	CANCEL WAKEUP
                                     001E    86 ;+
                                     001E    87 ; EXE$CANWAK - CANCEL WAKEUP
                                     001E    88 ;
                                     001E    89 ; THIS SERVICE PROVIDES THE CAPABILITY TO CANCEL ALL WAKE UP REQUESTS FOR
                                     001E    90 ; A SPECIFIED PROCESS.
                                     001E    91 ;
                                     001E    92 ; INPUTS:
                                     001E    93 ;
                                     001E    94 ;	PIDADR(AP) = ADDRESS OF PROCESS IDENTIFICATION.
                                     001E    95 ;	LOGNAM(AP) = ADDRESS OF PROCESS NAME STRING DESCRIPTOR.
                                     001E    96 ;
                                     001E    97 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     001E    98 ;
                                     001E    99 ; OUTPUTS:
                                     001E   100 ;
                                     001E   101 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO CANCEL WAKE UP REQUESTS.
                                     001E   102 ;
                                     001E   103 ;		R0 = SS$_ACCVIO - PROCESS NAME STRING OR PROCESS NAME
                                     001E   104 ;			STRING DESCRIPTOR CANNOT BE READ BY CALLING ACCESS
                                     001E   105 ;			MODE, OR PROCESS IDENTIFICATION CANNOT BE WRITTEN
                                     001E   106 ;			BY CALLING ACCESS MODE.
                                     001E   107 ;
                                     001E   108 ;		R0 = SS$_IVLOGNAM - ZERO OR GREATER THAN MAXIMUM LENGTH
                                     001E   109 ;			PROCESS NAME STRING SPECIFIED.
                                     001E   110 ;
                                     001E   111 ;		R0 = SS$_NONEXPR - NONEXISTENT PROCESS OF INVALID PROCESS
                                     001E   112 ;			IDENTIFICATION SPECIFIED.
                                     001E   113 ;
                                     001E   114 ;		R0 = SS$_NOPRIV - PROCESS DOES NOT HAVE PRIVILEGE TO WAKE
                                     001E   115 ;			SPECIFIED PROCESS.
                                     001E   116 ;
                                     001E   117 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     001E   118 ;
                                     001E   119 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     001E   120 ;-
                                     001E   121  
                               003C' 001E   122 	.ENTRY	EXE$CANWAK,^M<R2,R3,R4,R5>
                          FFDD'  30  0020   123 	BSBW	EXE$NAMPID		;GET TARGET PROCESS ID
            13            50     E9  0023   124 	BLBC	R0,20$			;IF LBC INVALID ARGUMENT
            54            02     9A  0026   125 	MOVZBL	#TQE$C_WKSNGL,R4	;SET TYPE OF ENTRY
            55            51     D0  0029   126 	MOVL	R1,R5			;SET TARGET PROCESS ID
                                     002C   127 10$:	SETIPL	#IPL$_TIMER		;RAISE IPL TO TIMER LEVEL
                          FFCA'  30  0033   128 	BSBW	EXE$RMVTIMQ		;REMOVE ENTRIES FROM TIMER QUEUE
            50            01     3C  0036   129 	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION STATUS
                                     0039   130 20$:	SETIPL	#0			;ALLOW INTERRUPTS
                                 04  003C   131 	RET				;
                                     003D   132 	.DSABL	LSB
                                     003D   133  
                                     003D   134 	.END
