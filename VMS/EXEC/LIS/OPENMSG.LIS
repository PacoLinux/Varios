                                     0114   165 ; CONNECT_RAB - CONNECT RECORD STREAM
                                     0114   166 ;
                                     0114   167 ; THIS ROUTINE IS CALLED TO CONNECT A RECORD STREAM TO A RAB.
                                     0114   168 ;
                                     0114   169 ; INPUTS:
                                     0114   170 ;
                                     0114   171 ;	R2 = FILE IFI NUMBER.
                                     0114   172 ;	R3 = ADDRESS OF RAB.
                                     0114   173 ;
                                     0114   174 ; OUTPUTS:
                                     0114   175 ;
                                     0114   176 ;	THE SPECIFIED RAB IS CONNECTED AND IMPLIED CARRIAGE CONTROL IS INSERTED
                                     0114   177 ;	IN THE RESULTANT ISI NUMBER.
                                     0114   178 ;
                                     0114   179 ;	NOTE ALL ERRORS ARE IGNORED.
                                     0114   180 ;
                                     0114   181  
                                     0114   182 CONNECT_RAB:				;CONNECT RAB
         02 A9            52     B0  0114   183 	MOVW	R2,FAB$W_IFI(R9)	;INSERT IFI NUMBER
                                     0118   184 	$CONNECT RAB=(R3)		;CONNECT RECORD STREAM
                                 05  0121   185 	RSB				;
                                     0122   186  
                                     0122   187 ;
                                     0122   188 ; CREATE_FILE
                                     0122   189 ;
                                     0122   190 ; THIS ROUTINE IS CALLED TO CREATE A FILE AND RETURN THE RESULTANT IFI.
                                     0122   191 ;
                                     0122   192 ; INPUTS:
                                     0122   193 ;
                                     0122   194 ;	R0 = ADDRESS OF FILE NAME COUNTED STRING.
                                     0122   195 ;
                                     0122   196 ; OUTPUTS:
                                     0122   197 ;
                                     0122   198 ;	R4 = RESULTANT FILE IFI NUMBER.
                                     0122   199 ;
                                     0122   200 ;	NOTE ALL ERRORS ARE IGNORED.
                                     0122   201 ;
                                     0122   202  
                                     0122   203 CREATE_FILE:				;CREATE FILE
         34 A9            80     90  0122   204 	MOVB	(R0)+,FAB$B_FNS(R9)	;INSERT FILENAME STRING SIZE
         2C A9            60     9E  0126   205 	MOVAB	(R0),FAB$L_FNA(R9)	;INSERT FILE NAME ADDRESS
