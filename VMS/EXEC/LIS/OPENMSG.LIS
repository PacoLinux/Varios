                                     0025    54 ; EXE$OPEN_MSG - OPEN FILES FOR SYSTEM OUTPUT AND ERROR
                                     0025    55 ;
                                     0025    56 ; THIS ROUTINE IS CALLED TO OPEN FILES FOR SYSTEM OUTPUT AND ERROR.
                                     0025    57 ;
                                     0025    58 ; INPUTS:
                                     0025    59 ;
                                     0025    60 ;	NONE.
                                     0025    61 ;
                                     0025    62 ; OUTPUTS:
                                     0025    63 ;
                                     0025    64 ;	RAB'S ARE ALLOCATED ON THE STACK FOR THE FILES 'SYS$OUTPUT' AND 
                                     0025    65 ;	'SYS$ERROR', AND THE FILE ISI VALUES ARE OBTAINED EITHER BY ACTUALLY
                                     0025    66 ;	OPENING THE RESPECTIVE FILES OR BY LOGICAL NAME TRANSLATION.
                                     0025    67 ;
                                     0025    68 ;	R10 = ADDRESS OF ALLOCATED 'SYS$OUTPUT' RAB.
                                     0025    69 ;	R11 = ADDRESS OF ALLOCATED 'SYS$ERROR' RAB.
                                     0025    70 ;-
                                     0025    71  
                                     0025    72 EXE$OPEN_MSG::				;OPEN MESSAGE FILES FOR SYSTEM OUTPUT AND ERROR
                          50   8ED0  0025    73 	POPL	R0			;SAVE RETURN ADDRESS
            5A         BC AE     9E  0028    74 	MOVAB	-RAB$C_BLN(SP),R10	;ALLOCATE SPACE FOR 'SYS$OUTPUT' RAB
            5B       FF78 CE     9E  002C    75 	MOVAB	-2*RAB$C_BLN(SP),R11	;ALLOCATE SPACE FOR 'SYS$ERROR' RAB
            5E            5B     D0  0031    76 	MOVL	R11,SP			;SET NEW STACK ADDRESS
                          50     DD  0034    77 	PUSHL	R0			;RESTORE RETURN ADDRESS
            5E         B0 AE     9E  0036    78 	MOVAB	-FAB$C_BLN(SP),SP	;ALLOCATE SPACE FOR FAB
            59            5E     D0  003A    79 	MOVL	SP,R9			;SAVE ADDRESS OF FAB
            69            00     2C  003D    80 	MOVC5	#0,(R9),#0,#FAB$C_BLN,(R9) ;CLEAR FAB
       0050 8F            00         0040       
                          69         0044       
         01 A9         50 8F     90  0045    81 	MOVB	#FAB$C_BLN,FAB$B_BLN(R9) ;SET LENGTH OF FAB
            69            03     90  004A    82 	MOVB	#FAB$C_BID,FAB$B_BID(R9) ;SET IDENTIFICATION OF FAB
         35 A9         BA AF     90  004D    83 	MOVB	FILTYP,FAB$B_DNS(R9)	;SET DEFAULT NAME STRING SIZE
         30 A9         B6 AF     9E  0052    84 	MOVAB	FILTYP+1,FAB$L_DNA(R9)	;SET DEFAULT NAME STRING ADDRESS
         36 A9       0100 8F     B0  0057    85 	MOVW	#BUFSIZ,FAB$W_MRS(R9)	;SET MAXIMUM RECORD SIZE
         16 A9            01     90  005D    86 	MOVB	#FAB$M_PUT,FAB$B_FAC(R9) ;SET FILE ACCESS MODE
                                     0061    87  
                                     0061    88 	ASSUME	FAB$M_SQO LE 255
                                     0061    89  
         04 A9         40 8F     9A  0061    90 	MOVZBL	#FAB$M_SQO,FAB$L_FOP(R9) ;SET FILE OPEN OPTIONS
            6A            00     2C  0066    91 	MOVC5	#0,(R10),#0,#RAB$C_BLN,(R10) ;CLEAR OUTPUT FILE RAB
       0044 8F            00         0069       
                          6A         006D       
            6A            01     90  006E    92 	MOVB	#RAB$C_BID,RAB$B_BID(R10) ;SET RAB BLOCK IDENTIFICATION
         01 AA         44 8F     90  0071    93 	MOVB	#RAB$C_BLN,RAB$B_BLN(R10) ;SET RAB BLOCK LENGTH
         3C AA            59     D0  0076    94 	MOVL	R9,RAB$L_FAB(R10)	;SET ADDRESS OF FAB
            6A       0044 8F     28  007A    95 	MOVC	#RAB$C_BLN,(R10),(R11)	;DUPLICATE OUTPUT FILE RAB
                          6B         007F       
                                     0080    96  
                                     0080    97 	ASSUME	RAB$V_CCO EQ 31
                                     0080    98  
         07 AB         80 8F     90  0080    99 	MOVB	#RAB$M_CCO@-24,RAB$L_ROP+3(R11) ;SET CANCEL CONTROL O
                       88 AF     9F  0085   100 	PUSHAB	ISINAM+1		;BUILD ISI NAME STRING DESCRIPTOR
            7E         84 AF     9A  0088   101 	MOVZBL	ISINAM,-(SP)		;
            58            5E     D0  008C   102 	MOVL	SP,R8			;SAVE ADDRESS OF ISI NAME STRING DESCRIPTOR
                          7E     7C  008F   103 	CLRQ	-(SP)			;CLEAR STORAGE FOR EQUIVALENCE NAME
