            55            54     B1  00D3   126 	CMPW	R4,R5			;'SYS$OUTPUT' AND 'SYS$ERROR' IFI'S MATCH?
                          0A     13  00D6   127 	BEQL	20$			;IF EQL YES
                                     00D8   128  
                                     00D8   129 ;
                                     00D8   130 ; 'SYS$ERROR' AND 'SYS$OUTPUT' ARE NOT THE SAME FILE
                                     00D8   131 ;
                                     00D8   132  
            52            54     D0  00D8   133 	MOVL	R4,R2			;SET 'SYS$OUTPUT' IFI
            53            5A     D0  00DB   134 	MOVL	R10,R3			;SET 'SYS$OUTPUT' RAB
                          34     10  00DE   135 	BSBB	CONNECT_RAB		;CONNECT RECORD STREAM
                          05     11  00E0   136 	BRB	30$			;
                                     00E2   137  
                                     00E2   138 ;
                                     00E2   139 ; 'SYS$ERROR' AND 'SYS$OUTPUT' ARE SAME FILE
                                     00E2   140 ;
                                     00E2   141  
         02 AA         02 AB     B0  00E2   142 20$:	MOVW	RAB$W_ISI(R11),RAB$W_ISI(R10) ;COPY ISI NUMBER
                                     00E7   143  
                                     00E7   144 ;
                                     00E7   145 ; CREATE LOGICAL NAME EQUIVALENCED TO 'SYS$ERROR' AND 'SYS$OUTPUT' ISI NUMBERS
                                     00E7   146 ;
                                     00E7   147  
            67       011B 8F     B0  00E7   148 30$:	MOVW	#ESCAPE,(R7)		;INSERT SPECIAL ESCAPE PREFIX
         02 A7         02 AA     B0  00EC   149 	MOVW	RAB$W_ISI(R10),2(R7)	;INSERT 'SYS$OUTPUT' ISI NUMBER
         04 A7         02 AB     B0  00F1   150 	MOVW	RAB$W_ISI(R11),4(R7)	;INSERT 'SYS$ERROR' ISI NUMBER
                                     00F6   151 	$CRELOG_S #LOG$C_PROCESS,(R8),-	;CREATE LOGICAL NAME
                                     00F6   152 		(R6),#PSL$C_USER	;
                                     0105   153  
                                     0105   154 ;
                                     0105   155 ; INSERT ISI NUMBERS IN RABS AND RETRIEVE ERROR/OUTPUT EQUIVALENCE INDICATOR
                                     0105   156 ;
                                     0105   157  
         02 AA         02 A7     B0  0105   158 40$:	MOVW	2(R7),RAB$W_ISI(R10)	;INSERT 'SYS$OUTPUT' ISI NUMBER
         02 AB         04 A7     B0  010A   159 	MOVW	4(R7),RAB$W_ISI(R11)	;INSERT 'SYS$ERROR' ISI NUMBER
