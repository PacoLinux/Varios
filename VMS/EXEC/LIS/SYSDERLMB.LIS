SYSDERLMB       - DECLARE ERROR LOG MAILBOX YSSTEM SERVICE       21-AUG-1978 20:27:52   VAX-11 MACRO X0.3-11               Page   3
01              SYSDERLMBX - DECLARE ERROR LOG MAILBOX                                                                           (1)

                                     0000  6600 	.SBTTL	SYSDERLMBX - DECLARE ERROR LOG MAILBOX
                                     0000  6700 ;++
                                     0000  6800 ; FUNCTIONAL DESCRIPTION:
                                     0000  6900 ;	EXE$DERLMBX SETS THE ERRORLOG MAILBOX DESCRIPTOR TO POINT TO A 
                                     0000  7000 ;	SPECIFIED MAILBOX UNIT.  ONLY ONE PROCESS MAY ASSIGN A MAILBOX UNIT
                                     0000  7100 ;	TO THE ERROR LOG AT ONE TIME.  IF THE ERROR LOG MAILBOX IS ALREADY
                                     0000  7200 ;	ASSIGNED TO ANOTHER PROCESS, THE ERROR STATUS CODE SS$_DEVALLOC
                                     0000  7300 ;	IS RETURNED.  WHEN A PROCESS IS FINISHED WITH THE ERROR LOG MAILBOX
                                     0000  7400 ;	IT SHOULD ASSIGN A UNIT NUMBER OF 0(ZERO).  EXE$RUNDWN WILL DO THIS
                                     0000  7500 ;	IN CASE THE IMAGE NEGLECTS TO EXIT CORRECTLY.
                                     0000  7600 ;
                                     0000  7700 ; CALLING SEQUENCE:
                                     0000  7800 ;	CALLG	ARGLIST,EXE$DERLMBX
                                     0000  7900 ;
                                     0000  8000 ; INPUT PARAMETERS:
                                     0000  8100 ;	MBXUNT(AP) - MAILBOX UNIT NUMBER
                                     0000  8150 ;	R4 - PCB ADDRESS OF CURENT PROCESS
                                     0000  8200 ;
                                     0000  8300 ; IMPLICIT INPUTS:
                                     0000  8400 ;	PCB AND PROCESS HEADER OF CURRENT PROCESS
                                     0000  8500 ;
                                     0000  8600 ; OUTPUT PARAMETERS:
                                     0000  8700 ;	R0 - COMPLETION STATUS CODE
                                     0000  8800 ;
                                     0000  8900 ; IMPLICIT OUTPUTS:
                                     0000  9000 ;	NONE
                                     0000  9100 ;
                                     0000  9200 ; COMPLETION CODES:
                                     0000  9300 ;	SS$_NORMAL   - NORMAL SUCCESSFUL COMPLETION
                                     0000  9400 ;	SS$_NOPRIV   - PROCESS DOES NOT HAVE DIAGNOSTIC PRIVILEGE
                                     0000  9500 ;	SS$_DEVALLOC - ERROR LOG MAILBOX UNIT ALREADY DECLARED BY ANOTHER
                                     0000  9600 ;		       PROCESS.
                                     0000  9700 ;
                                     0000  9800 ; SIDE EFFECTS:
                                     0000  9900 ;	NONE
                                     0000 10000 ;
                                     0000 10100 ;--
                                     0000 10200 
                                     0000 10300 
                                     0000 10400 EXE$DERLMB::				;
                               003C  0000 10450 	.WORD	^M<R2,R3,R4,R5>		; SAVE R2-R5
                                     0002 10500 
            50            24     3C  0002 10600 	MOVZWL	#SS$_NOPRIV,R0		; ASSUME NO PRIVILEGE
                                     0005 10700 	IFNPRIV	DIAGNOSE,EXIT		; EXIT WITH ERROR STATUS IF NO PRIVILEGE
            50       0840 8F     3C  000A 10800 	MOVZWL	#SS$_DEVALLOC,R0	; ASSUME "DEVICE" ALLOCATED
            55       0000'CF     9E  000F 10900 	MOVAB	W^EXE$GQ_ERLMBX,R5	; SET ADDRESS OF ERRORLOG MAILBOX DESCRIPTOR
                                     0014 11000 	DSBINT	#IPL$_SYNCH		; INHIBIT SYSTEM EVENT REPORTING
                          65     B5  001A 11100 	TSTW	UNIT(R5)		; TEST FOR ALREADY ASSIGNED
                          07     13  001C 11200 	BEQL	10$			; NOT ASSIGNED
         04 A5         60 A4     D1  001E 11300 	CMPL	PCB$L_PID(R4),PID(R5)	; CHECK FOR CURRENT PROCESS
                          11     12  0023 11400 	BNEQ	EXITEN			; NO, EXIT WITH DEVALLOC ERROR
         04 A5         60 A4     D0  0025 11500 10$:	MOVL	PCB$L_PID(R4),PID(R5)	; SET PID OF OWNER
            65         04 AC     B0  002A 11550 	MOVW	MBXUNT(AP),UNIT(R5)	; SET ERRORLOG MAILBOX UNIT NUMBER
                          03     12  002E 11600 	BNEQ	20$			; BR IF ACTUAL UNIT
                       04 A5     D4  0030 11700 	CLRL	PID(R5)			; ZAP PID OF MAILBOX OWNER
            50            01     D0  0033 11800 20$:	MOVL	#SS$_NORMAL,R0		; SET NORMAL COMPLETION STATUS
                                     0036 11900 EXITEN:	ENBINT				; ENABLE SYSTEM EVENT REPORTING
