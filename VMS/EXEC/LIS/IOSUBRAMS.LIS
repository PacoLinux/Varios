                                     011C 25500 	.SBTTL	UPDATE TRANSFER PARAMETERS
                                     011C 25600 ;+
                                     011C 25700 ; IOC$UPDATRANSP - UPDATE TRANSFER PARAMETERS
                                     011C 25800 ;
                                     011C 25900 ; THIS ROUTINE IS CALLED TO UPDATE THE TRANSFER PARAMETERS AFTER A DISK ERROR
                                     011C 26000 ; HAS BEEN DISCOVERED BUT GOOD DATA WAS TRANSFERED.
                                     011C 26100 ;
                                     011C 26200 ; INPUTS:
                                     011C 26300 ;
                                     011C 26400 ;	R0 = NUMBER OF BYTES OF DATA THAT WERE TRANSFERED (MUST BE A MULTIPLE
                                     011C 26500 ;		OF 512 BYTES).
                                     011C 26600 ;	R5 = DEVICE UNIT UCB ADDRESS.
                                     011C 26700 ;
                                     011C 26800 ;	UCB$W_BCNT(R5) = LENGTH OF TRANSFER IN BYTES.
                                     011C 26900 ;	UCB$W_DA(R5) = CURRENT SECTOR AND TRACK ADDRESS.
                                     011C 27000 ;	UCB$W_DC(R5) = CURRENT CYLINDER ADDRESS.
                                     011C 27100 ;	UCB$L_SVAPTE(R5) = SYSTEM VIRTUAL ADDRESS OF PAGE TABLE THAT MAPS
                                     011C 27200 ;		THE TRANSFER.
                                     011C 27300 ;
                                     011C 27400 ; OUTPUTS:
                                     011C 27500 ;
                                     011C 27600 ;	THE NUMBER OF BYTES REMAINING TO BE TRANSFERED, THE SYSTEM VIRTUAL
                                     011C 27700 ;	ADDRESS OF THE NEXT PTE, AND THE CURRENT DISK ADDRESS OF THE TRANSFER
                                     011C 27800 ;	ARE UPDATED.
                                     011C 27900 ;
                                     011C 28000 ;	R3 IS PRESERVED ACROSS CALL.
                                     011C 28100 ;-
                                     011C 28200  
