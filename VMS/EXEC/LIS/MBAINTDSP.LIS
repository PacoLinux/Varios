                                     0000    36 	.SBTTL	MASSBUS ADAPTER INTERRUPT DISPATCHER
                                     0000    37 ;+
                                     0000    38 ; MBA$INT - MASSBUS ADAPTER INTERRUPT DISPATCHER
                                     0000    39 ;
                                     0000    40 ; THIS ROUTINE IS ENTERED VIA A JSB INSTRUCTION WHEN AN INTERRUPT OCCURS
                                     0000    41 ; ON A MASSBUS ADAPTER. THE STATE OF THE STACK ON ENTRY IS:
                                     0000    42 ;
                                     0000    43 ;	00(SP) = ADDRESS OF IDB ADDRESS.
                                     0000    44 ;	04(SP) = SAVED R2.
                                     0000    45 ;	08(SP) = SAVED R3.
                                     0000    46 ;	12(SP) = SAVED R4.
                                     0000    47 ;	16(SP) = SAVED R5.
                                     0000    48 ;	20(SP) = INTERRUPT PC.
                                     0000    49 ;	24(SP) = INTERRUPT PSL.
                                     0000    50 ;
                                     0000    51 ; INTERRUPT DISPATCHING OCCURS AS FOLLOWS:
                                     0000    52 ;
                                     0000    53 ;	IF THE INTERRUPTING ADAPTER IS CURRENTLY OWNED AND THE OWNER UNIT
                                     0000    54 ;	IS EXPECTING AN INTERRUPT, THEN THAT UNIT IS DISPATCHED FIRST. ALL
                                     0000    55 ;	OTHER UNITS ARE DISPATCHED BY READING THE ATTENTION SUMMARY REG-
                                     0000    56 ;	ISTER AND SCANNING FOR UNITS THAT HAVE ATTENTION SET. AS EACH UNIT
                                     0000    57 ;	IS FOUND, ITS ATTENTION SUMMARY BIT IS CLEARED AND THEN A TEST IS
                                     0000    58 ;	MADE TO DETERMINE IF AN INTERRUPT IS EXPECTED ON THE UNIT. IF YES,
                                     0000    59 ;	THEN THE DRIVER IS CALLED AT ITS INTERRUPT RETURN ADDRESS. ELSE
                                     0000    60 ;	THE DRIVER IS CALLED AT ITS UNSOLICITED INTERRUPT ADDRESS. AS EACH
                                     0000    61 ;	CALL TO THE DRIVER RETURNS, THE ATTENTION SUMMARY REGISTER IS RE-
                                     0000    62 ;	READ AND AN ATTEMPT IS MADE TO FIND ANOTHER UNIT TO DISPATCH. WHEN
                                     0000    63 ;	NO UNITS REQUESTING ATTENTION REMAIN, THE INTERRUPT IS DISMISSED.
                                     0000    64 ;-
                                     0000    65  
                                 00000000    66 	.PSECT	$$$115_DRIVER
                                     0000    67 MBA$INT::				;MASSBUS ADAPTER INTERRUPT DISPATCHER
            53         00 BE     D0  0000    68 	MOVL	@(SP),R3		;GET ADDRESS OF IDB
            54            63     D0  0004    69 	MOVL	IDB$L_CSR(R3),R4	;GET ADDRESS OF CONFIGURATION STATUS REGISTER
            55         04 A3     D0  0007    70 	MOVL	IDB$L_OWNER(R3),R5	;GET OWNER UNIT UCB ADDRESS
                          09     13  000B    71 	BEQL	10$			;IF EQL NO OWNER
            52         74 A5     9A  000D    72 	MOVZBL	UCB$B_SLAVE(R5),R2	;GET OWNER SLAVE CONTROLLER NUMBER
         58 A5            01     E0  0011    73 	BBS	#UCB$V_INT,UCB$W_STS(R5),20$ ;IF SET, INTERRUPT EXPECTED
                          21         0015       
            53         00 BE     D0  0016    74 10$:	MOVL	@(SP),R3		;RETRIEVE ADDRESS OF IDB
            54            63     D0  001A    75 	MOVL	IDB$L_CSR(R3),R4	;RETRIEVE MBA CONFIGURATION REGISTER ADDRESS
         08 A4            00     D2  001D    76 	MCOML	#0,MBA$L_SR(R4)		;CLEAR ALL MBA STATUS BITS
            52       0410 C4     D0  0021    77 	MOVL	MBA$L_AS(R4),R2		;READ ATTENTION SUMMARY REGISTER
            08            00     EA  0026    78 	FFS	#0,#8,R2,R2		;FIND FIRST UNIT REQUESTING ATTENTION
            52            52         0029       
                          0A     12  002B    79 	BNEQ	20$			;IF NEQ UNIT FOUND
            5E            04     C0  002D    80 	ADDL	#4,SP			;REMOVE IDB ADDRESS FROM STACK
            52            8E     7D  0030    81 	MOVQ	(SP)+,R2		;RESTORE REGISTERS
            54            8E     7D  0033    82 	MOVQ	(SP)+,R4		;
                                 02  0036    83 	REI				;
            01            52     78  0037    84 20$:	ASHL	R2,#1,MBA$L_AS(R4)	;CLEAR ATTENTION SUMMARY BIT
                     0410 C4         003A       
            55         10 A342   D0  003D    85 	MOVL	IDB$L_UCBLST(R3)[R2],R5	;GET ADDRESS OF UCB OR INTERRUPT DISPATCHER
                          D2     13  0042    86 	BEQL	10$			;IF EQL NONE DEFINED
            24            55     E8  0044    87 	BLBS	R5,40$			;IF LBS INTERRUPT DISPATCHER ADDRESS
         58 A5            01     E5  0047    88 	BBCC	#UCB$V_INT,UCB$W_STS(R5),30$ ;IF CLR, INTERRUPT NOT EXPECTED
                          09         004B       
MBAINTDSP       - MASSBUS ADAPTER INTERRUPT DISPATCHER           21-AUG-1978 20:55:18   VAX-11 MACRO X0.3-11               Page   3
01              MASSBUS ADAPTER INTERRUPT DISPATCHER                                                                             (1)

            53         10 A5     7D  004C    89 	MOVQ	UCB$L_FR3(R5),R3	;RESTORE DRIVER CONTEXT
                       0C B5     16  0050    90 	JSB	@UCB$L_FPC(R5)		;CALL DRIVER AT INTERRUPT RETURN ADDRESS
                          C1     11  0053    91 	BRB	10$			;
            53         24 A5     D0  0055    92 30$:	MOVL	UCB$L_DDB(R5),R3	;GET ADDRESS OF DDB
            53         0C A3     D0  0059    93 	MOVL	DDB$L_DDT(R3),R3	;GET ADDRESS OF DDT
            53         04 A3     C1  005D    94 	ADDL3	DDT$L_UNSOLINT(R3),R3,R2 ;CALCULATE ADDRESS OF UNSOLICITED INTERRUPT ENTRY
                          52         0061       
                          03     19  0062    95 	BLSS	35$			;IF LSS SYSTEM ADDRESS
            52            53     C2  0064    96 	SUBL	R3,R2			;SUBTRACT OUT BASE ADDRESS
                          62     16  0067    97 35$:	JSB	(R2)			;CALL UNSOLICITED INTERRUPT ROUTINE
                          AB     11  0069    98 	BRB	10$			;
                          7E     DC  006B    99 40$:	MOVPSL	-(SP)			;READ CURRENT PSL
                          75     16  006D   100 	JSB	-(R5)			;CALL SLAVE CONTROLLER INTERRUPT DISPATCHER
                          A5     11  006F   101 	BRB	10$			;
