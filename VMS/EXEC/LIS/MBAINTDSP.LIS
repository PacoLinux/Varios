                                     0071   103 	.SBTTL	MASSBUS ADAPTER INITIALIZATION
                                     0071   104 ;+
                                     0071   105 ; MBA$INITIAL - MASSBUS ADAPTER INITIALIZATION
                                     0071   106 ;
                                     0071   107 ; THIS ROUTINE IS CALLED VIA A JSB INSTRUCTION AT SYSTEM STARTUP AND AFTER
                                     0071   108 ; A POWER RECOVERY RESTART TO ALLOW INITIALIZATION OF MASSBUS ADAPTERS.
                                     0071   109 ; 
                                     0071   110 ; INPUTS:
                                     0071   111 ;
                                     0071   112 ;	R4 = CSR ADDRESS OF MASSBUS ADAPTER.
                                     0071   113 ;	R5 = ADDRESS OF ADAPTER IDB.
                                     0071   114 ;
                                     0071   115 ;	ALL INTERRUPTS ARE LOCKED OUT.
                                     0071   116 ;
                                     0071   117 ; OUTPUTS:
                                     0071   118 ;
                                     0071   119 ;	THE MASSBUS ADAPTER IS INITIALIZED AND INTERRUPTS ARE ENABLED.
                                     0071   120 ;-
                                     0071   121  
                                     0071   122 MBA$INITIAL::				;MASSBUS ADAPTER INITIALIZATION
         04 A4            01     D0  0071   123 	MOVL	#MBA$M_CR_INIT,MBA$L_CR(R4) ;INITIALIZE MASSBUS ADAPTER
         04 A4            04     D0  0075   124 	MOVL	#MBA$M_CR_IE,MBA$L_CR(R4) ;ENABLE INTERRUPTS
                                 05  0079   125 	RSB				;
                                     007A   126  
                                     007A   127 	.END
