                                     0020   100 	.SBTTL	GETPTIPAG - Get Page Table Info for Single Page
                                     0020   200 
                                     0020   300 ;++
                                     0020   400 ; FUNCTIONAL DESCRIPTION:
                                     0020   500 ;
                                     0020   600 ;
                                     0020   700 ; CALLING SEQUENCE:
                                     0020   800 ;
                                     0020   900 ;	BSBW	MMG$GETPTIPAG
                                     0020  1000 ;
                                     0020  1100 ;
                                     0020  1200 ; INPUT PARAMETERS:
                                     0020  1300 ;
                                     0020  1400 ;	R0  = Access Mode for page ownership check
                                     0020  1500 ;	R2  = Virtual Address
                                     0020  1600 ;	R4  = Current PCB address
                                     0020  1700 ;	R5  = Process Header Address - P1 or System Space
                                     0020  1800 ;	R6  = Count - 1 of pages to be processed including this one
                                     0020  1900 ;	R7  = +^X200 if going forward in the address space
                                     0020  2000 ;	    = -^X200 if going backwards in the address space
                                     0020  2100 ;	R8  = Page table information mask, specifying the criteria
                                     0020  2150 ;	      against which the pages are tested.
                                     0020  2200 ;	R9  = count - 1 of pages found so far that satisfy the criteria
                                     0020  2400 ;	R10 = threshhold page count - 1.  Once a run of pages is found
                                     0020  2410 ;	      that meets the criteria, a further check is made to see if
                                     0020  2420 ;	      the range of pages is large enough.
                                     0020  2500 ;
                                     0020  2600 ;	IPL = ASTDEL
                                     0020  2700 ;
                                     0020  2800 ; IMPLICIT INPUTS:
                                     0020  2900 ;	NONE
                                     0020  3000 ;
                                     0020  3100 ; OUTPUT PARAMETERS:
                                     0020  3200 ;
                                     0020  3300 ;	R0 = Status Code
                                     0020  3400 ;	R2   Preserved
                                     0020  3500 ;
                                     0020  3600 ; IMPLICIT OUTPUTS:
                                     0020  3700 ;	NONE
                                     0020  3800 ;
                                     0020  3900 ; COMPLETION CODES:
                                     0020  4000 ;
                                     0020  4100 ;	SS$_NORMAL			;Successful Completion
                                     0020  4200 ;	SS$_PAGOWNVIO			;Page Owner Violation
                                     0020  4300 ;	SS$_LENVIO			;Length Violation
                                     0020  4400 ;	SS$_ACCVIO			;Access Violation
                                     0020  4500 ;
                                     0020  4600 ; SIDE EFFECTS:
                                     0020  4700 ;
                                     0020  4800 ;	NONE
                                     0020  4900 ;
                                     0020  5000 ;--
                                     0020  5100 ;
                                     0020  5200 ; **********************************************************************
                                     0020  5300 ;
                                     0020  5400 ; **************** THE FOLLOWING CODE MUST BE RESIDENT *****************
                                     0020  5500 ;
SYSGETPTI       - Get Page Table Information System Service      21-AUG-1978 20:32:39   VAX-11 MACRO X0.3-11               Page   6
X0100           GETPTIPAG - Get Page Table Info for Single Page                                                                  (4)

                                 00000000  5600 	.PSECT	$MMGCOD
                                     0000  5700 ;
                                     0000  5800 ; **********************************************************************
                                     0000  5900 ;
                                     0000  6000 MMG$GETPTIPAG::
                                     0000  6100 	SAVIPL				;Save caller's IPL
                          50     DD  0003  6200 	PUSHL	R0			;Save access mode
                          FFF8'  30  0005  6300 	BSBW	MMG$PTEREF		;Reference PTE, return SVAPTE
                                     0008  6400 					;return at IPL=SYNCH
            69            50     E9  0008  6500 	BLBC	R0,90$			;Branch if Length Violation
            50            0C     3C  000B  6600 	MOVZWL	#SS$_ACCVIO,R0		;In case this page is not accessable
            51            63     D0  000E  6700 	MOVL	(R3),R1			;Fetch the page table entry
                          61     13  0011  6800 	BEQL	90$			;Branch if deleted, access violation
            50            6E     D0  0013  6900 	MOVL	(SP),R0			;Access Mode
                          FFE7'  30  0016  7000 	BSBW	MMG$PAGETYPE		;Check access, return page type
            58            50     E9  0019  7100 	BLBC	R0,90$			;Branch if Page Owner Violation
            16            00     EF  001C  7200 	EXTZV	#PTE$V_PGFLVB,#PTE$S_PGFLVB,(R3),R0 ;PFN, PGFLVB, GPTX
            50            63         001F       
            7E            57     7D  0021  7300 	MOVQ	R7,-(SP)		;Push R8, reserve a scratch location
            20            00     EA  0024  7400 20$:	FFS	#0,#32,R8,(SP)		;Convert next bit to its bit number
            6E            58         0027       
                          09     13  0029  7500 	BEQL	40$			;Branch if no more bits
            58            6E     E5  002B  7600 	BBCC	(SP),R8,30$		;Shut off the bit
                          00         002E       
                          49     10  002F  7700 30$:	BSBB	100$			;See if this page has desired attribute
            F0            50     E9  0031  7800 	BLBC	R0,20$			;Branch if not, check next attribute
                     0102 8F     BA  0034  7900 40$:	POPR	#^M<R1,R8>		;Clean off scratch loc, restore R8
            59            50     C0  0038  7950 	ADDL	R0,R9			;If match found, count it
                          0F     19  003B  8000 	BLSS	60$			;Branch if haven't found first page
            08            50     E9  003D  8050 	BLBC	R0,50$			;Branch if end of "run"
                          0A     12  0040  8100 	BNEQ	60$			;Branch if not first page of "run"
         00'AD            52     D0  0042  8150 	MOVL	R2,B^CDL$L_SVSTARTVA(FP) ;Save address of first page
                          04     11  0046  8200 	BRB	60$
            56            1F     E2  0048  8250 50$:	BBSS	#31,R6,60$		;Indicate end of run
                          00         004B       
            50            01     3C  004C  8300 60$:	MOVZWL	#SS$_NORMAL,R0		;Set success indication
                          56     D5  004F  8350 	TSTL	R6			;Check for end of range or end of "run"
                          21     14  0051  8400 	BGTR	90$			;Branch if not
                          59     D5  0053  8450 	TSTL	R9			;At end range or end "run"
                          1A     19  0055  8500 	BLSS	80$			;Branch if no "run" found
            5A            59     D1  0057  8550 	CMPL	R9,R10			;Is the "run" large enough?
                          0B     19  005A  8600 	BLSS	70$			;Branch if not, start over
                                     005C  8610 ;
                                     005C  8620 ; Found a large enough range, return it
                                     005C  8630 ;
                          56     D5  005C  8650 	TSTL	R6			;At end of range?
                          14     13  005E  8700 	BEQL	90$			;Branch if yes, this VA is in the range
                          56     D4  0060  8750 	CLRL	R6			;Force end of range
            52            57     C2  0062  8800 	SUBL	R7,R2			;and back off 1 page
                          0D     11  0065  8850 	BRB	90$
                                     0067  8860 ;
                                     0067  8870 ; This "run" was too small, start looking for another
                                     0067  8880 ;
            59            01     CE  0067  8900 70$:	MNEGL	#1,R9			;No pages in the "run"
            1E            00     EF  006A  8950 	EXTZV	#0,#30,R6,R6		;Restore count - 1 of pages left in range
            56            56         006D       
                          03     12  006F  9000 	BNEQ	90$			;Branch if still some to do
