                                     0000  4480 ; **********************************************************************
                                     0000  4490 ;
                               07FC' 0000  4600 	.ENTRY	EXE$GETPTI,^M<R2,R3,R4,R5,R6,R7,R8,R9,R10>
                                     0002  4700 
            56   00000000'EF     DE  0002  4800 	MOVAL	L^MMG$GETPTIPAG,R6	;Per-page subroutine
            58         10 AC     D0  0009  4900 	MOVL	MASK(AP),R8		;Type of information desired
            59            01     CE  000D  5000 	MNEGL	#1,R9			;No pages found yet
         14 AC            01     C3  0010  5010 	SUBL3	#1,PAGCNT(AP),R10	;Minimum page count - 1
                          5A         0014       
                          02     18  0015  5020 	BGEQ	10$			;Branch if specified
                          5A     D4  0017  5030 	CLRL	R10			;Default to 1 page
            5E            00'    C2  0019  5100 10$:	SUBL	S^#CDL$C_LENGTH,SP	;Reserve scratch area
                          FFE1'  30  001C  5200 	BSBW	MMG$CRECOM1		;Common Create code
