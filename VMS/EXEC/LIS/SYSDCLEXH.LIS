                                     0052   134 	.SBTTL	GET EXIT CONTROL BLOCK PARAMETERS
                                     0052   135 ;
                                     0052   136 ; SUBROUTINE TO GET EXIT CONTROL BLOCK PARAMETERS
                                     0052   137 ;
                                     0052   138  
                                     0052   139 GETPARM:				;
                          50     DC  0052   140 	MOVPSL	R0			;READ CURRENT PSL
            02            16     EF  0054   141 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R0,R0 ;EXTRACT PREVIOUS MODE FIELD
            50            50         0057       
                          50     D7  0059   142 	DECL	R0			;SUBTRACT OUT EXECUTIVE MODE BIAS
                          0D     19  005B   143 	BLSS	10$			;IF LSS CALL FROM KERNEL MODE
            50   00000000'9F40   DE  005D   144 	MOVAL	@#CTL$GL_THEXEC[R0],R0	;GET ADDRESS OF EXIT HANDLER LISTHEAD
            51         04 AC     D0  0065   145 	MOVL	DESBLK(AP),R1		;GET ADDRESS OF EXIT CONTROL BLOCK
                                 05  0069   146 	RSB				;
                                     006A   147 10$:	BUG_CHECK IVSSRVRQST		;INVALID SYSTEM SERVICE REQUEST
            50       0174 8F     3C  006E   148 	MOVZWL	#SS$_IVSSRQ,R0		;SET INVALID SYSTEM SERVICE REQUEST
                                 04  0073   149 	RET				;
                                     0074   150  
                                     0074   151 	.END
