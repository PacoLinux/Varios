01              CANCEL EXIT HANDLER                                                                                              (1)

                                     0000    41 	.SBTTL	CANCEL EXIT HANDLER
                                     0000    42 ;+
                                     0000    43 ; EXE$CANEXH - CANCEL EXIT HANDLER
                                     0000    44 ;
                                     0000    45 ; THIS SERVICE PROVIDES THE CAPABILITY TO DELETE AN EXIT CONTROL BLOCK OR
                                     0000    46 ; ALL EXIT CONTROL BLOCKS FROM THE EXIT HANDLER LIST FOR THE CALLING ACCESS
                                     0000    47 ; MODE.
                                     0000    48 ;
                                     0000    49 ; INPUTS:
                                     0000    50 ;
                                     0000    51 ;	DESBLK(AP) = ADDRESS OF EXIT CONTROL BLOCK (ZERO IMPLIES ALL).
                                     0000    52 ;
                                     0000    53 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    54 ;
                                     0000    55 ; OUTPUTS:
                                     0000    56 ;
                                     0000    57 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO CANCEL EXIT HANDLER.
                                     0000    58 ;
                                     0000    59 ;		R0 = SS$_ACCVIO - FIRST LONGWORD OF EXIT CONTROL BLOCK
                                     0000    60 ;			CANNOT BE READ BY THE CALLING ACCESS MODE, FIRST
                                     0000    61 ;			LONGWORD OF A EXIT CONTROL BLOCK IN THE LIST CANNOT
                                     0000    62 ;			BE READ BY THE CALLING ACCESS MODE, OR THE FIRST
                                     0000    63 ;			LONGWORD OF THE PREVIOUS EXIT CONTROL BLOCK CANNOT
                                     0000    64 ;			BE WRITTEN.
                                     0000    65 ;
                                     0000    66 ;		R0 = SS$_NOHANDLER - SPECIFIED HANDLER CANNOT BE FOUND IN
                                     0000    67 ;			EXIT HANLDER CONTROL BLOCK LIST.
                                     0000    68 ;
                                     0000    69 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    70 ;
                                     0000    71 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0000    72 ;-
                                     0000    73  
                                     0000    74 	.ENABL	LSB
                                 00000000    75 	.PSECT	Y$EXEPAGED
                               001C' 0000    76 	.ENTRY	EXE$CANEXH,^M<R2,R3,R4>
                          4E     10  0002    77 	BSBB	GETPARM			;GET EXIT CONTROL BLOCK PARAMETERS
                          3B     13  0004    78 	BEQL	30$			;IF EQL CANCEL ALL EXIT HANDLERS
            52            50     D0  0006    79 	MOVL	R0,R2			;COPY ADDRESS OF LISTHEAD
            53            52     D0  0009    80 10$:	MOVL	R2,R3			;SAVE ADDRESS OF PREVIOUS ENTRY
                                     000C    81 	IFNORD	#4,(R3),50$		;CAN NEXT EXIT CONTROL BLOCK ADDRESS BE READ?
            52            63     D0  0012    82 	MOVL	(R3),R2			;GET ADDRESS OF NEXT EXIT CONTROL BLOCK
                          35     13  0015    83 	BEQL	60$			;IF EQL END OF LIST
            52            51     D1  0017    84 	CMPL	R1,R2			;EXIT CONTROL BLOCK ADDRESS MATCH?
                          ED     12  001A    85 	BNEQ	10$			;IF NEQ NO
                                     001C    86 	IFNORD	#4,(R2),50$		;CAN NEXT EXIT CONTROL BLOCK ADDRESS BE READ?
            53            50     D1  0022    87 	CMPL	R0,R3			;PREVIOUS EXIT CONTROL BLOCK LISTHEAD?
                          06     13  0025    88 	BEQL	20$			;IF EQL YES
                                     0027    89 	IFNOWRT	#4,(R3),50$		;CAN PREVIOUS EXIT CONTROL BLOCK BE WRITTEN?
            63            62     D0  002D    90 20$:	MOVL	(R2),(R3)		;REMOVE EXIT CONTROL BLOCK FROM LIST
                          12     11  0030    91 	BRB	40$			;
SYSDCLEXH       - SYSTEM SERVICES CANCEL AND DECLARE EXIT HANDL  21-AUG-1978 20:26:41   VAX-11 MACRO X0.3-11               Page   3
01              DECLARE EXIT HANDLER                                                                                             (1)

                                     0032    93 	.SBTTL	DECLARE EXIT HANDLER
                                     0032    94 ;+
                                     0032    95 ; EXE$DCLEXH - DECLARE EXIT HANDLER
                                     0032    96 ;
                                     0032    97 ; THIS SERVICE PROVIDES THE CAPABILITY TO INSERT AN EXIT CONTROL BLOCK AT
                                     0032    98 ; THE FRONT OF THE EXIT HANDLER LIST FOR THE CALLING ACCESS MODE.
                                     0032    99 ;
                                     0032   100 ; INPUTS:
                                     0032   101 ;
                                     0032   102 ;	DESBLK(AP) = ADDRESS OF EXIT CONTROL BLOCK.
                                     0032   103 ;
                                     0032   104 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0032   105 ;
                                     0032   106 ; OUTPUTS:
                                     0032   107 ;
                                     0032   108 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO DECLARE EXIT HANDLER.
                                     0032   109 ;
                                     0032   110 ;		R0 = SS$_ACCVIO - FIRST LONGWORD OF EXIT CONTROL BLOCK
                                     0032   111 ;			CANNOT BE WRITTEN BY CALLING ACCESS MODE.
                                     0032   112 ;
                                     0032   113 ;		R0 = SS$_NOHANDLER - NO EXIT CONTROL BLOCK ADDRESS SPECIFIED.
                                     0032   114 ;
                                     0032   115 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0032   116 ;
                                     0032   117 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0032   118 ;-
                                     0032   119  
                               001C' 0032   120 	.ENTRY	EXE$DCLEXH,^M<R2,R3,R4>
                          1C     10  0034   121 	BSBB	GETPARM			;GET EXIT CONTROL BLOCK PARAMETERS
                          14     13  0036   122 	BEQL	60$			;IF EQL NO EXIT CONTROL BLOCK SPECIFIED
                                     0038   123 	IFNOWRT	#4,(R1),50$		;CAN FIRST LONGWORD BE WRITTEN?
            61            60     D0  003E   124 	MOVL	(R0),(R1)		;LINK OLD FIRST TO NEW FIRST
            60            51     D0  0041   125 30$:	MOVL	R1,(R0)			;SET ADDRESS OF NEW FIRST ENTRY
            50            01     3C  0044   126 40$:	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION STATUS
                                 04  0047   127 	RET				;
            50            0C     3C  0048   128 50$:	MOVZWL	#SS$_ACCVIO,R0		;SET ACCESS VIOLATION
                                 04  004B   129 	RET				;
            50       08F8 8F     3C  004C   130 60$:	MOVZWL	#SS$_NOHANDLER,R0	;SET NO HANLDER STATUS
                                 04  0051   131 	RET				;
                                     0052   132 	.DSABL	LSB
SYSDCLEXH       - SYSTEM SERVICES CANCEL AND DECLARE EXIT HANDL  21-AUG-1978 20:26:41   VAX-11 MACRO X0.3-11               Page   4
