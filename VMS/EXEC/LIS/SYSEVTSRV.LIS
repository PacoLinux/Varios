01              EXE$CLREF - CLEAR EVENT FLAG SERVICE                                                                             (1)

                                     0000  6600 	.SBTTL	 EXE$CLREF - CLEAR EVENT FLAG SERVICE 
                                     0000  6700 
                                     0000  6800 ;++
                                     0000  6900 ; FUNCTIONAL DESCRIPTION:
                                     0000  7000 ;	THE CLEAR EVENT FLAG SYSTEM SERVICE CLEARS THE SPECIFIED 
                                     0000  7100 ;	EVENT FLAG.  NO RESCHEDULING IS EVER NECESSARY AFTER CLEARING
                                     0000  7200 ;	AN EVENT FLAG.
                                     0000  7300 ;
                                     0000  7400 ;
                                     0000  7500 ; CALLING SEQUENCE:
                                     0000  7600 ;	CALL	ARGLIST,EXE$CLREF
                                     0000  7700 ;
                                     0000  7800 ;
                                     0000  7900 ; INPUT PARAMETERS:
                                     0000  8000 ;	04(AP) - EVENT FLAG NUMBER TO BE CLEARED
                                     0000  8050 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0000  8100 ;
                                     0000  8200 ; IMPLICIT INPUTS:
                                     0000  8400 ;
                                     0000  8500 ; OUTPUT PARAMETERS:
                                     0000  8600 ;	R0 - COMPLETION STATUS CODE, ERROR IF BIT 0 IS SET.
                                     0000  8700 ;
                                     0000  8800 ;
                                     0000  8900 ; IMPLICIT OUTPUTS:
                                     0000  9000 ;	THE EVENT FLAG SPECIFIED WITHIN THE CLUSTER SPECIFIED
                                     0000  9100 ;	IS CLEARED.
                                     0000  9200 ;
                                     0000  9300 ; COMPLETION CODES:
                                     0000  9400 ;	SS$_WASCLR - EVENT FLAG WAS ALREADY CLEAR
                                     0000  9500 ;	SS$_WASSET - EVENT FLAG WAS SET
                                     0000  9600 ;	SS$_ILLEFC - ILLEGAL CLUSTER NUMBER (I.E. 4 - 7)
                                     0000  9700 ;	SS$_UNASEFC - UNASSIGNED CLUSTER NUMBER
                                     0000  9800 ;
                                     0000  9900 ; SIDE EFFECTS:
                                     0000 10000 ;	NONE
                                     0000 10100 ;
                                     0000 10200 ;--
                                     0000 10300 
                                     0000 10400 
                                     0000 10500 
                                     0000 10600 EXE$CLREF::				; CLEAR EVENT FLAG SERVICE
                               001C  0000 10700 	.WORD	^M<R2,R3,R4>		; ENTRY MASK SAVING R2,R3,R4
            53         04 AC     9A  0002 10800 	MOVZBL	EFN(AP),R3		; GET EVENT FLAG NUMBER
                          38     10  0006 10900 	BSBB	SCH$CLREF		; CALL INTERNAL ROUTINE
            08            50     E9  0008 10905 	BLBC	R0,10$			; BR IF SOME KIND OF ERROR
            5D         0C AD     D0  000B 10910 	MOVL	12(FP),FP		; RESTORE FRAME POINTER
            5E            00'    C0  000F 10915 	ADDL	S^#EXE$C_CMSTKSZ,SP	; CLEAN STACK BACK TO PC,PSL
                                 02  0012 10920 	REI				; RETURN
                                 04  0013 11000 10$:	RET				; AND RETURN TO CALLER
                                     0014 11100 
                                     0014 11200 
SYSEVTSRV       - EVENT FLAG SERVICES ;                          21-AUG-1978 20:29:51   VAX-11 MACRO X0.3-11               Page   4
01              SCH$GETEFC - GET ADDRESS OF EVENT FLAG CLUSTER                                                                   (1)

                                     0014 11400 	.SBTTL	SCH$GETEFC - GET ADDRESS OF EVENT FLAG CLUSTER
                                     0014 11500 ;++
                                     0014 11600 ; FUNCTION:
                                     0014 11700 ;	SCH$GETEFC COMPUTES THE ADDRESS OF THE EVENT FLAG CLUSTER
                                     0014 11800 ;	SELECTED BY THE EVENT FLAG NUMBER IN R3.  THE EVENT FLAG CLUSTER
                                     0014 11900 ;	ADDRESS IS RETURNED IN R2.
                                     0014 12000 ;
                                     0014 12100 ; CALLING SEQUENCE:
                                     0014 12200 ;	BSBB/BSBW/JSB	SCH$GETEFC
                                     0014 12300 ;
                                     0014 12400 ; INPUT PARAMETERS:
                                     0014 12500 ;	R3 - EVENT FLAG NUMBER
                                     0014 12550 ;	R4 - PCB ADDRESS
                                     0014 12600 ;
                                     0014 13100 ; OUTPUT PARAMETERS:
                                     0014 13200 ;	R0 - ERROR STATUS CODE
                                     0014 13300 ;	R2 - ADDRESS OF EVENT FLAG CLUSTER
                                     0014 13350 ;	R3 - EVENT FLAG NUMBER WITHIN CLUSTER
                                     0014 13400 ;
                                     0014 13500 ; COMPLETION CODES:
                                     0014 13600 ;	SS$_NORMAL  -  NORMAL COMPLETION STATUS
                                     0014 13700 ;	SS$_ILLEFC  -  ILLEGAL CLUSTER NUMBER
                                     0014 13800 ;	SS$_UNASEFC -  UNASSIGNED CLUSTER NUMBER
                                     0014 13900 ;
                                     0014 14000 ;--
                                     0014 14100 
                                     0014 14200 SCH$GETEFC:				; GET ADDRESS OF EVENT FLAG CLUSTER
            50            01     3C  0014 14250 	MOVZWL	#SS$_NORMAL,R0		; ASSUME NORMAL COMPLETION
            03            05     EE  0017 14400 	EXTV	#5,#3,R3,R1		; EXTRACT CLUSTER NUMBER
            51            53         001A       
                          1C     19  001C 14500 	BLSS	30$			; ILLEGAL WITH 4,5,6,7
            52         50 A441   DE  001E 14800 	MOVAL	PCB$L_EFCS(R4)[R1],R2	; GET ADDR OF EF VECTOR
            53   FFFFFFE0 8F     CA  0023 14850 	BICL	#^C<^X1F>,R3		; EXTRACT EVENT NUMBER WITHIN CLUSTER
            01            51     F5  002A 14900 	SOBGTR	R1,10$			; BR IF COMMON
                                 05  002D 14950 	RSB				; RETURN WITH SUCCESS
            62            10     C1  002E 15000 10$:	ADDL3	#CEB$L_EFC,(R2),R2	; GET POINTER TO CEB EVENT MASK
                          52         0031       
                          05     19  0032 15100 	BLSS	20$			; ASSIGNED
            50       0234 8F     3C  0034 15200 	MOVZWL	#SS$_UNASEFC,R0		; SET ERROR CODE FOR UNASSIGNED
                                 05  0039 15300 20$:	RSB				; AND EXIT
            50       00EC 8F     3C  003A 15400 30$:	MOVZWL	#SS$_ILLEFC,R0		; SET ERROR CODE FOR ILLEGAL EFC
                                 05  003F 15500 	RSB				; AND EXIT
SYSEVTSRV       - EVENT FLAG SERVICES ;                          21-AUG-1978 20:29:51   VAX-11 MACRO X0.3-11               Page   5
01              SCH$CLREF - CLEAR EVENT FLAG (INTERNAL FORM)                                                                     (1)

                                     0040 15800 	.SBTTL	SCH$CLREF - CLEAR EVENT FLAG (INTERNAL FORM)
                                     0040 15900 ;++
                                     0040 16000 ;	SCH$CLREF - CLEAR EVENT FLAG, INTERNAL FORM
                                     0040 16100 ;
                                     0040 16200 ; CALLING SEQUENCE:
                                     0040 16300 ;	JSB/BSB SCH$CLREF
                                     0040 16400 ;
                                     0040 16500 ; INPUT:
                                     0040 16600 ;	R3 - EVENT FLAG NUMBER, ZERO FILLED.
                                     0040 16700 ;
                                     0040 16800 ; OUTPUT:
                                     0040 16900 ;	R0 - COMPLETION STATUS, ERROR IF BIT 0 CLEAR.
                                     0040 17000 ; COMPLETION CODES:
                                     0040 17100 ;	SS$_WASCLR - EVENT FLAG WAS ALREADY CLEAR
                                     0040 17200 ;	SS$_WASSET - EVENT FLAG WAS SET
                                     0040 17300 ;	SS$_ILLEFC - ILLEGAL CLUSTER NUMBER
                                     0040 17400 ;	SS$_UNASEFC - UNASSIGNED CLUSTER NUMBER
                                     0040 17500 ;--
                                     0040 17600 
                                     0040 17700 
                                     0040 17800 SCH$CLREF::				; CLEAR EVENT FLAG INTERNAL 
                          D2     10  0040 17900 	BSBB	SCH$GETEFC		; GET ADDRESS OF EVENT FLAG CLUSTER
            07            50     E9  0042 18000 	BLBC	R0,10$			; EXIT IF ERROR
                                     0045 18200 	ASSUME	SS$_WASCLR EQ SS$_NORMAL
                                     0045 18250 					; SS$_NORMAL RETURNED BY SCH$GETEFC
            62            53     E5  0045 18300 	BBCC	R3,(R2),10$		; CLEAR AND TEST FLAG
                          03         0048       
            50            09     D0  0049 18400 	MOVL	#SS$_WASSET,R0		; REPORT STATUS
                                 05  004C 18500 10$:	RSB				; RETURN TO CALLER
SYSEVTSRV       - EVENT FLAG SERVICES ;                          21-AUG-1978 20:29:51   VAX-11 MACRO X0.3-11               Page   6
01              EXE$READEF - READ EVENT FLAG SYTEM SERVICE                                                                       (1)

                                     004D 18700 	.SBTTL	EXE$READEF - READ EVENT FLAG SYTEM SERVICE
                                     004D 18800 ;++
                                     004D 18900 ; FUNCTIONAL DESCRIPTION:
                                     004D 19000 ;	EXE$READEF IMPLEMENTS THE READ EVENT FLAG SYSTEM SERVICE WHICH
                                     004D 19100 ;	RETURNS THE STATE OF THE EVENT FLAG CLUSTER SELECTED BY THE
                                     004D 19200 ;	SPECIFIED EVENT FLAG NUMBER.
                                     004D 19300 ;
                                     004D 19400 ; CALLING SEQUENCE:
                                     004D 19500 ;	CALLG	ARGLIST,EXE$READEF
                                     004D 19600 ;
                                     004D 19700 ; INPUT PARAMETERS:
                                     004D 19800 ;	04(AP) - EVENT FLAG NUMBER TO SELECT PROPER CLUSTER
                                     004D 19900 ;	08(AP) - ADDRESS TO STORE STATE OF EVENT FLAG CLUSTER
                                     004D 20000 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     004D 20300 ;
                                     004D 20400 ; OUTPUT PARAMETERS:
                                     004D 20500 ;	R0 - COMPLETION STATUS CODE
                                     004D 20600 ;	@08(AP) - STATE OF 32 EVENTS IN SPECIFIED CLUSTER
                                     004D 20700 ;
                                     004D 20800 ; COMPLETION CODES:
                                     004D 20900 ;	SS$_WASSET  -  SPECIFIC EVENT FLAG IS CURRENTLY SET
                                     004D 21000 ;	SS$_WASCLR  -  SPECIFIC EVENT FLAG IS CURRENTLY CLEAR
                                     004D 21100 ;	SS$_ILLEFC  -  ILLEGAL EVENT FLAG CLUSTER NUMBER
                                     004D 21200 ;	SS$_UNASEFC -  UNASSIGNED EVENT FLAG CLUSTER
                                     004D 21300 ;	SS$_ACCVIO  -  ACCESS VIOLATION ON WRITE DESTINATION
                                     004D 21400 ;
                                     004D 21500 ;--
                                     004D 21600 
                                     004D 21700 EXE$READEF::				; READ EVENT FLAG SYSTEM SERVICE
                               001C  004D 21800 	.WORD	^M<R2,R3,R4>		; ENTRY MASK SAVING R2,R3,R4
            53         04 AC     9A  004F 21900 	MOVZBL	EFN(AP),R3		; GET EVENT FLAG NUMBER 
                          BF     10  0053 22000 	BSBB	SCH$GETEFC		; GET EFC ADDRESS AND CONTENT
            18            50     E9  0055 22100 	BLBC	R0,20$			; NO STORE IF ERROR
            51         08 AC     D0  0058 22200 	MOVL	MASKP(AP),R1		; GET ADDRESS OF DESTINATION
                                     005C 22300 	IFNOWRT	#4,(R1),10$		; TEST FOR WRITABLE
            61            62     D0  0062 22400 	MOVL	(R2),(R1)		; STORE EVENT FLAG BIT VECTOR
            61            53     E1  0065 22600 	BBC	R3,(R1),20$		; EXIT IF CLEAR
                          07         0068       
            50            09     3C  0069 22700 	MOVZWL	#SS$_WASSET,R0		; RETURN SET STATUS CODE
                                 04  006C 22800 	RET				; AND RETURN
            50            0C     3C  006D 22900 10$:	MOVZWL	#SS$_ACCVIO,R0		; SET ACCESS VIOLATION
                                 04  0070 23000 20$:	RET				; AND RETURN
                                     0071 23100 
SYSEVTSRV       - EVENT FLAG SERVICES ;                          21-AUG-1978 20:29:51   VAX-11 MACRO X0.3-11               Page   7
01              EXE$SETEF - SET EVENT FLAG SERVICE                                                                               (1)

                                     0071 23300 	.SBTTL	EXE$SETEF - SET EVENT FLAG SERVICE
                                     0071 23400 ;++
                                     0071 23500 ; FUNCTIONAL DESCRIPTION:
                                     0071 23600 ;	EXE$SETEF IMPLEMENTS THE SET EVENT FLAG SERVICE WHICH SETS
                                     0071 23700 ;	THE SPECIFIED EVENT FLAG AND CAUSES ANY APPROPRIATE RESCHEDULING.
                                     0071 23800 ;
                                     0071 23900 ; CALLING SEQUENCE:
                                     0071 24000 ;	CALLG	EXE$SETEF
                                     0071 24100 ;
                                     0071 24200 ; INPUT PARAMETERS:
                                     0071 24300 ;	04(AP) - EVENT FLAG NUMBER
                                     0071 24350 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0071 24400 ;
                                     0071 24500 ; IMPLICIT INPUTS:
                                     0071 24600 ;	CURRENT PCB LOCATED VIA SCH$GL_CURPCB
                                     0071 24700 ;
                                     0071 24800 ; OUTPUT PARAMETERS:
                                     0071 24900 ;	R0 - COMPLETION STATUS CODE
                                     0071 25000 ;
                                     0071 25100 ; COMPLETION CODES:
                                     0071 25200 ;	SS$_WASCLR  -  SPECIFIED EVENT WAS CLEAR INITIALLY
                                     0071 25300 ;	SS$_WASSET  -  SPECIFIED EVENT WAS SET INITIALLY
                                     0071 25400 ;	SS$_ILLEFC  -  ILLEGAL CLUSTER NUMBER
                                     0071 25500 ;	SS$_UNASEFC -  UNASSIGNED EVENT FLAG CLUSTER NUMBER
                                     0071 25600 ;
                                     0071 25700 ;--
                                     0071 25800 
                                     0071 25900 EXE$SETEF::				; SET EVENT FLAG SYSTEM SERVICE
                               003C  0071 26000 	.WORD	^M<R2,R3,R4,R5>		; ENTRY MASK SAVING R2,R3,R4,R5
            53         04 AC     9A  0073 26100 	MOVZBL	EFN(AP),R3		; GET EVENT FLAG NUMBER
                          9B     10  0077 26200 	BSBB	SCH$GETEFC		; GET EFC ADDRESS AND VALIDATE
            23            50     E9  0079 26300 	BLBC	R0,20$			; EXIT IF ERROR IN EFN
            55            09     D0  007C 26500 	MOVL	#SS$_WASSET,R5		; ASSUME SET
            62            53     E2  007F 26600 	BBSS	R3,(R2),10$		; SET EVENT FLAG
                          03         0082       
            55            01     D0  0083 26700 	MOVL	#SS$_WASCLR,R5		; FLAG WAS CLEAR, SET STATUS
            51         60 A4     D0  0086 27000 10$:	MOVL	PCB$L_PID(R4),R1	; AND PROCESS ID (PID)
            52            01     D0  008A 27100 	MOVL	#PRI$_IOCOM,R2		; SET PRIORITY INCREMENT CLASS
            53         04 AC     D0  008D 27200 	MOVL	EFN(AP),R3		; AND RESTORE EVENT FLAG NUMBER
                          FF6C'  30  0091 27300 	BSBW	SCH$POSTEF		; POST EVENT FLAG
            50            55     D0  0094 27400 	MOVL	R5,R0			; RESTORE STATUS
            5D         0C AD     D0  0097 27405 	MOVL	12(FP),FP		; RESTORE FP
            5E            00'    C0  009B 27410 	ADDL	S^#EXE$C_CMSTKSZ,SP	; CLEAN STACK BACK TO PC,PSL
                                 02  009E 27415 	REI				; AND RETURN
                                 04  009F 27500 20$:	RET				; RETURN
                                     00A0 27600 	.END
