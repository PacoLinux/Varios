                 00000000'9F     16  0A34   658 	JSB	@#MMG$SVAPTECHK		; GET SVA OF FIRST PHD PTE
            83            89     D0  0A3A   659 	MOVL	(R9)+,(R3)+		; MAP PROCESS HEADER FIXED PAGE
            51   00000000'9F     3C  0A3D   660 	MOVZWL	@#SWP$GW_WSLPTE,R1	; GET COUNT OF ADDED PAGES FOR WSL+PST
