            50   00000000'9F     D0  0A04   647 	MOVL	@#SWP$GL_SHELLSIZ,R0	; GET PAGES ALLOCATED FOR SHELL
            5B            6940   DE  0A0B   648 	MOVAL	(R9)[R0],R11		; COMPUTE END ADDRESS IN MAP
   00000000'9F            02     78  0A0F   649 	ASHL	#2,@#SWP$GL_SHELIO,R0	; GET I/O SIZE OF SHELL
                          50         0A16       
            5B            59     C3  0A17   650 	SUBL3	R9,R11,R1		; COMPUTE SIZE OF EXTENSION PAGES
                          51         0A1A       
            51            50     C2  0A1B   651 	SUBL	R0,R1			; LESS I/O SIZE
            51            03     9C  0A1E   652 	ROTL	#3,R1,R1		; CONVERT TO DOUBLE QUAD COUNT
                          51         0A21       
   00000000'9F            09     9C  0A22   653 	ROTL	#9,@#SWP$GL_SHELIO,R0	; COMPUTE ADDRESS AT END OF I/O TRANSFER
                          50         0A29       
                          80     7C  0A2A   654 10$:	CLRQ	(R0)+			; CLEAR PAGES
                          80     7C  0A2C   655 	CLRQ	(R0)+			; NOT READ FROM SHELL
            F9            51     F5  0A2E   656 	SOBGTR	R1,10$			; 
