01              FILL SYSTEM PTE WITH TRANSFER PTE                                                                                (1)

                                     0038    95 	.SBTTL	FILL SYSTEM PTE WITH TRANSFER PTE
                                     0038    96 ;+
                                     0038    97 ; IOC$FILSPT - FILL SYSTEM PTE WITH TRANSFER PTE
                                     0038    98 ;
                                     0038    99 ; THIS ROUTINE IS CALLED TO FILL A SYSTEM PTE WITH A TRANSFER PTE OF A
                                     0038   100 ; LOCKED DOWN BUFFER SO THAT IT MAY BE DIRECTLY ADDRESS.
                                     0038   101 ;
                                     0038   102 ; INPUTS:
                                     0038   103 ;
                                     0038   104 ;	R5 = DEVICE UNIT UCB ADDRESS.
                                     0038   105 ;
                                     0038   106 ; OUTPUTS:
                                     0038   107 ;
                                     0038   108 ;	R0 = SYSTEM VIRTUAL ADDRESS OF START OF PAGE CONTAINING THE BUFFER.
                                     0038   109 ;
                                     0038   110 ;	REGISTERS R1, R2, AND R3 ARE PRESERVED ACROSS CALL.
                                     0038   111 ;-
                                     0038   112  
                                     0038   113 IOC$FILSPT::				;FILL SYSTEM PTE WITH TRANSFER PTE
                          53     DD  0038   114 	PUSHL	R3			;SAVE REGISTERS
         64 A5            04     C5  003A   115 	MULL3	#4,UCB$L_SVPN(R5),R0	;CALCULATE BYTE OFFSET IN SYSTEM PAGE TABLE
                          50         003E       
            53         68 B5     D0  003F   116 	MOVL	@UCB$L_SVAPTE(R5),R3	;GET CONTENTS OF TRANSFER PTE
                          03     19  0043   117 	BLSS	10$			;IF LSS ENTRY IS VALID
                          FFB8'  30  0045   118 	BSBW	IOC$PTETOPFN		;GET PFN FOR INVALID PTE
            00            53     F0  0048   119 10$:	INSV	R3,#PTE$V_PFN,#PTE$S_PFN,@MMG$GL_SPTBASE[R0]
   00000000'FF40          15         004B       
                                     0052   120 					;SET SYSTEM PTE ENTRY PFN
            50            07     78  0052   121 	ASHL	#7,R0,R0		;CONVERT SVPN TO SYSTEM VIRTUAL ADDRESS
                          50         0055       
            50            1F     E2  0056   122 	BBSS	#31,R0,20$		;
                          00         0059       
                                     005A   123 20$:	INVALID	R0			;INVALIDATE TRANSLATION BUFFER
                          08     BA  005D   124 	POPR	#^M<R3>			;RESTORE REGISTERS
                                 05  005F   125 	RSB				;
                                     0060   126  
                                     0060   127 	.END
