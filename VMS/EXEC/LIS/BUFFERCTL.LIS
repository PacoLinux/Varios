                                     0000    35 ;+
                                     0000    36 ; IOC$MOVFRUSER - MOVE FROM USER BUFFER
                                     0000    37 ;
                                     0000    38 ; THIS ROUTINE IS CALLED BY AN I/O DRIVER TO MOVE A STRING FROM A USER
                                     0000    39 ; BUFFER TO AN INTERNAL BUFFER.
                                     0000    40 ;
                                     0000    41 ; INPUTS:
                                     0000    42 ;
                                     0000    43 ;	R1 = ADDRESS OF INTERNAL BUFFER.
                                     0000    44 ;	R2 = NUMBER OF BYTES TO BE MOVED.
                                     0000    45 ;	R5 = UCB ADDRESS OF DEVICE UNIT.
                                     0000    46 ;
                                     0000    47 ; OUTPUTS:
                                     0000    48 ;
                                     0000    49 ;	***TBS***
                                     0000    50 ;-
                                     0000    51  
                                 00000000    52 	.PSECT	WIONONPAGED
                                     0000    53 IOC$MOVFRUSER::				;MOVE FROM USER BUFFER
                          36     10  0000    54 	BSBB	IOC$FILSPT		;FILL SYSTEM PTE WITH PROPER RELOCATION
            50         6C A5     A8  0002    55 	BISW	UCB$W_BOFF(R5),R0	;MERGE BYTE OFFSET WITH VIRTUAL ADDRESS
                          0D     11  0006    56 	BRB	20$			;
            50       01FF 8F     B3  0008    57 10$:	BITW	#^X01FF,R0		;OVERFLOW PAGE BOUNDRY?
                          06     12  000D    58 	BNEQ	20$			;IF NEQ NO
         68 A5            04     C0  000F    59 	ADDL	#4,UCB$L_SVAPTE(R5)	;UPDATE ADDRESS OF USER PTE
                          23     10  0013    60 	BSBB	IOC$FILSPT		;FILL SYSTEM PTE WITH PROPER RELOCATION
            81            80     90  0015    61 20$:	MOVB	(R0)+,(R1)+		;MOVE BYTE TO INTERNAL BUFFER
            ED            52     F5  0018    62 	SOBGTR	R2,10$			;ANY MORE BYTES TO MOVE?
                                 05  001B    63 	RSB				;
