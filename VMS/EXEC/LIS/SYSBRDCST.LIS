                                     0000    55 ; EXE$BRDCST - TERMINAL BROADCAST SYSTEM SERVICE
                                     0000    56 ;
                                     0000    57 ; FUNCTIONAL DESCRIPTION:
                                     0000    58 ;
                                     0000    59 ; THIS IS A PRIVILEGED SYSTEM SERIVCE. PRV$_OPER
                                     0000    60 ; FOR MULTI-TERMINAL BROADCASTS. OTHERWISE, SINGLE BROADCASTS CAN BE DONE
                                     0000    61 ; IF THE REQUESTOR OWNS THE TERMINAL.
                                     0000    62 ;
                                     0000    63 ; EXE$BRDCST COPIES A STRING DESCRIPTOR SPECIFIED MESSAGE INTO SYSTEM SPACE
                                     0000    64 ; AND SETS UP THE NECESSARY TERMINAL UCB INFORMATION FOR A BREAK
                                     0000    65 ; THROUGH WRITE. THE MESSAGE IS LIMITED BY THE CURRENT PROCESS
                                     0000    66 ; BUFFERED I/O QOUTA.
                                     0000    67 ;
                                     0000    68 ; INPUTS:
                                     0000    69 ;
                                     0000    70 ;	(AP) = 2
                                     0000    71 ;	4(AP) = ADDRESS OF STRING DESCRIPTER FOR MESSAGE
                                     0000    72 ;	8(AP) = TARGET TERMINAL DESCRIPTER ADDRESS
                                     0000    73 ;		IF 8(AP) = 0 THEN ALL ALLOCATED TERMINALS
                                     0000    74 ;		OTHERWISE IT IS ASSUMED THAT 8(AP) IS THE ADDRESS
                                     0000    75 ;		OF A STRING DESC. WHICH HAS THE LOGICAL NAME OF THE TARGET
                                     0000    76 ;		TERMINAL DEVICE.
                                     0000    77 ;
                                     0000    78 ;		IF THE STRING DESCRIBES A ZERO LENGTH STRING THEN ALL
                                     0000    79 ;		TERMINALS ARE THE TARGET.
                                     0000    80 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    81 ;
                                     0000    82 ; OUTPUTS:
                                     0000    83 ;
                                     0000    84 ;	R0 = STATUS OF THE OPERATION
                                     0000    85 ;
                                     0000    86 ; COMPLETION CODES:
                                     0000    87 ;
                                     0000    88 ;	SS$_NORMAL - SUCCESSFUL OPERATION
                                     0000    89 ;	SS$_ACCVIO - MESSAGE ACCESS VIOLATION
                                     0000    90 ;	SS$_EXQUOTA - BUFFERED I/O QUOTA EXCEEDED
                                     0000    91 ;	SS$_INSFMEM - MEMORY ALLOCATION FAILURE WITHOUT RESOURCE WAIT
                                     0000    92 ;	SS$_NOPRIV - CURRENT PROCESS DOES NOT HAVE "PRV$V_DEVCHAR"
                                     0000    93 ;	SS$_DEVOFFLINE - DEVICE NOT AVAILABLE FOR BREAK THROUGH WRITE
                                     0000    94 ;--
                                     0000    95 EXE$BRDCST::				; BROADCAST SYSTEM SERVICE
                               00FC  0000    96 	.WORD	^M<R2,R3,R4,R5,R6,R7>
                                     0002    97 ;
                                     0002    98 ; ADDRESS CHECK SPECIFIED MESSAGE STRING
                                     0002    99 ;
            52         04 AC     D0  0002   100 	MOVL	4(AP),R2		; GET STRING DESCRIPTOR ADDRESS
                          012E   30  0006   101 	BSBW	ADDRCHK			; ADDRESS CHECK MESSAGE
            57            52     D0  0009   102 	MOVL	R2,R7			; SAVE ADDRESS OF THE MESSAGE DESC.
                                     000C   103 ;
                                     000C   104 ; ADDRESS CHECK TARGET TERMINAL DESC.
                                     000C   105 ;
            52         08 AC     D0  000C   106 	MOVL	8(AP),R2		; GET DESC ADDRESS
                          03     13  0010   107 	BEQL	20$			; IF EQL THEN ALL ALLOCATED TERMS
                          0122   30  0012   108 	BSBW	ADDRCHK			; ADDRESS CHECK THE TARGET TERMINAL DESC.
