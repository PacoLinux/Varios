01              NULL DEVICE FUNCTION DECISION TABLE ROUTINES                                                                     (1)

                                     0040    84 	.SBTTL	NULL DEVICE FUNCTION DECISION TABLE ROUTINES
                                     0040    85 ;+
                                     0040    86 ; READ - READ FUNCTIONS
                                     0040    87 ; WRITE - WRITE FUNCTIONS
                                     0040    88 ;
                                     0040    89 ; THE NULL DEVICE IS A SPECIAL DEVICE IN THAT ALL WRITES ARE DISCARDED AND ALL
                                     0040    90 ; READS RETURN AN END OF FILE STATUS. THERE IS NO ACTUAL DRIVER, BUT RATHER ALL
                                     0040    91 ; PROCESSING IS DONE BY FUNCTION DECISION TABLE ROUTINES.
                                     0040    92 ;
                                     0040    93 ; INPUTS:
                                     0040    94 ;
                                     0040    95 ;	R0 = SCRATCH.
                                     0040    96 ;	R1 = SCRATCH.
                                     0040    97 ;	R2 = SCRATCH.
                                     0040    98 ;	R3 = ADDRESS OF I/O REQUEST PACKET.
                                     0040    99 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0040   100 ;	R5 = ASSIGNED DEVICE UCB ADDRESS.
                                     0040   101 ;	R6 = ADDRESS OF CCB.
                                     0040   102 ;	R7 = I/O FUNCTION CODE BIT NUMBER.
                                     0040   103 ;	R8 = FUNCTION DECISION TABLE DISPATCH ADDRESS.
                                     0040   104 ;	R9 = SCRATCH.
                                     0040   105 ;	R10 = SCRATCH.
                                     0040   106 ;	R11 = SCRATCH.
                                     0040   107 ;	AP = ADDRESS OF FIRST FUNCTION DEPENDENT PARAMETER.
                                     0040   108 ;
                                     0040   109 ; OUTPUTS:
                                     0040   110 ;
                                     0040   111 ;	IF THE FUNCTION IS A READ, THEN SS$_ENDOFFILE IS RETURNED.
                                     0040   112 ;
                                     0040   113 ;	IF THE FUNCTION IS A WRITE, THEN SS$_NORMAL IS RETURNED.
                                     0040   114 ;-
                                     0040   115  
                                     0040   116 	.ENABL	LSB
                                 00000000   117 	.PSECT	Y$EXEPAGED
            50       0000'8F     3C  0000   118 READ:	MOVZWL	#SS$_ENDOFFILE,R0	;SET END OF FILE STATUS
                          05     11  0005   119 	BRB	10$			;
            50       0000'8F     3C  0007   120 WRITE:	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION
                 00000000'EF     17  000C   121 10$:	JMP	EXE$FINISHIOC		;FINISH I/O OPERATION
                                     0012   122 	.DSABL	LSB
                                     0012   123 NL_END:					;ADDRESS OF LAST LOCATION IN DRIVER
                                     0012   124  
                                     0012   125 	.END
