                                     0010   200 ;
                                     0010   300 ; FUNCTIONAL DESCRIPTION:
                                     0010   400 ;
                                     0010   500 ;	THIS ROUTINE DISMOUNTS THE INDICATED MOUNTED VOLUME LIST ENTRY.
                                     0010   600 ;	THE MTL AND LOGICAL NAME, IF IT STILL EXISTS, ARE DELETED, AND THE
                                     0010   700 ;	VOLUME SHARE COUNT IS DECREMENTED. IF THE SHARE COUNT GOES TO
                                     0010   800 ;	ZERO, THE VOLUME ITSELF IS DISMOUNTED.
                                     0010   900 ;
                                     0010  1000 ; CALLING SEQUENCE:
                                     0010  1100 ;	JSB IOC$DISMOUNT
                                     0010  1200 ;
                                     0010  1300 ; INPUT PARAMETERS:
                                     0010  1400 ;	R3 = LBC TO UNLOAD VOLUME
                                     0010  1500 ;	     LBS TO NOT UNLOAD
                                     0010  1600 ;	R4 = ADDRESS OF PROCESS PCB
                                     0010  1700 ;	R6 = ADDRESS OF MOUNTED VOLUME LIST ENTRY
                                     0010  1800 ;
                                     0010  1900 ; IMPLICIT INPUTS:
                                     0010  2000 ;	IPL  -  IPL$_ASTDEL
                                     0010  2100 ;
                                     0010  2200 ; OUTPUT PARAMETERS:
                                     0010  2300 ;	R0-R2,R6 SMASHED, OTHER REGISTERS PRESERVED
                                     0010  2400 ;
                                     0010  2500 ; IMPLICIT OUTPUTS:
                                     0010  2600 ;	NONE
                                     0010  2700 ;
                                     0010  2800 ; ROUTINE VALUE:
                                     0010  2900 ;	SS$_NORMAL,SS$_NOIOCHAN
                                     0010  3000 ;
                                     0010  3100 ; SIDE EFFECTS:
                                     0010  3200 ;	VOLUME DISMOUNTED: LOGICAL NAME & MTL DEALLOCATED, VCB GONE OR SOON
                                     0010  3300 ;	TO GO, ACP PROCESS MAY BECOME DELETED
                                     0010  3400 ;
                                     0010  3500 ;--
                                     0010  3600 
                                     0010  3700 
                                     0010  3800 IOC$DISMOUNT::
                          28     BB  0010  3900 	PUSHR	#^M<R3,R5>		; SAVE REGISTERS
            55         0C A6     D0  0012  4000 	MOVL	MTL$L_UCB(R6),R5	; GET UCB ADDRESS
            51         10 A6     D0  0016  4100 	MOVL	MTL$L_LOGNAME(R6),R1	; GET ADDRESS OF LOGICAL NAME
                          03     13  001A  4200 	BEQL	10$			; BRANCH IF NONE
                          FFE1'  30  001C  4300 	BSBW	LOG$DELETE		; DELETE THE LOGICAL NAME
                                     001F  4400 
            50            56     D0  001F  4500 10$:	MOVL	R6,R0			; GET MTL ADDRESS IN R0
                          05     18  0022  4600 	BGEQ	20$			; BRANCH IF PROCESS SPACE ADDRESS
                          FFD9'  30  0024  4700 	BSBW	EXE$DEAPAGED		; DEALLOCATE TO SYSTEM PAGED POOL
                          0E     11  0027  4800 	BRB	30$
            51         08 A6     3C  0029  4900 20$:	MOVZWL	MTL$W_SIZE(R6),R1	; GET BLOCK SIZE
            53   00000000'9F     DE  002D  5000 	MOVAL	@#CTL$GQ_ALLOCREG,R3	; AND PROCESS ALLOCATION LIST HEAD
                          FFC9'  30  0034  5100 	BSBW	EXE$DEALLOCATE		; AND DEALLOCATE TO PROCESS POOL
                                     0037  5200 
                                     0037  5300 ;
                                     0037  5400 ; NOW LOCK THE I/O DATABASE MUTEX AND DECREMENT THE VOLUME SHARE COUNT.
                                     0037  5500 ; IF IT GOES TO ZERO, MARK THE UCB FOR DISMOUNT.
                                     0037  5600 ;
                          FFC6'  30  0037  5700 30$:	BSBW	SCH$IOLOCKW		; LOCK I/O DATABASE
