01              CHECK PROCESS RESOURCE REQUEST                                                                                   (1)

                                     0000    41 	.SBTTL	CHECK PROCESS RESOURCE REQUEST
                                     0000    42  
                                     0000    43 	.ENABL	LSB
                                     0000    44 ;+
                                     0000    45 ; EXE$BUFFRQUOTA - CHECK BUFFER BYTE COUNT QUOTA AND CONDITIONALLY WAIT
                                     0000    46 ; EXE$BUFQUOPRC - CHECK PROCESS BUFFER QUOTA ONLY AND CONDITIONALLY WAIT
                                     0000    47 ; EXE$MULTIQUOTA - CHECK MULTI-UNIT RESOURCE REQUEST AND CONDITIONALLY WAIT
                                     0000    48 ; EXE$SNGLEQUOTA - CHECK SINGLE-UNIT RESOURCE REQUEST AND CONDITIONALLY WAIT
                                     0000    49 ;
                                     0000    50 ; THESE ROUTINES ARE CALLED TO CHECK IF A PROCESS HAS SUFFICIENT RESIDUAL
                                     0000    51 ; QUOTA TO GRANT A RESOURCE. IF SUFFICIENT QUOTA IS NOT AVAILABLE AND THE
                                     0000    52 ; PROCESS IS ENABLED FOR IMPLIED RESOURCE WAIT, THEN THE PROCESS IS ENTERED
                                     0000    53 ; IN A WAIT STATE TO WAIT FOR SUFFICIENT QUOTA TO BECOME AVAILABLE.
                                     0000    54 ;
                                     0000    55 ; INPUTS:
                                     0000    56 ;
                                     0000    57 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    58 ;
                                     0000    59 ; IF ENTRY AT EXE$BUFFRQUOTA OR EXE$MULTIQUOTA, THEN
                                     0000    60 ;
                                     0000    61 ;	R1 = NUMBER OF UNITS OF RESOURCE REQUESTED.
                                     0000    62 ;
                                     0000    63 ; IF ENTRY AT EXE$MULTIQUOTA OR EXE$SNGLEQUOTA, THEN
                                     0000    64 ;
                                     0000    65 ;	R2 = ADDRESS OF QUOTA WORD CONTAINING REMAINING UNITS FOR SPECIFIED
                                     0000    66 ;		RESOURCE.
                                     0000    67 ; OUTPUTS:
                                     0000    68 ;
                                     0000    69 ;	R0 LOW BIT CLEAR INDICATES CHECK FAILURE WITH CALLING IPL PRESERVED.
                                     0000    70 ;
                                     0000    71 ;		R0 = SS$_EXQUOTA = QUOTA EXCEEDED OR REQUEST GREATER THAN
                                     0000    72 ;			MAXIMUM PROCESS OR SYSTEM QUOTA.
                                     0000    73 ;
                                     0000    74 ;	R0 LOW BIT SET INDICATES SUCCESS WITH IPL SET TO AST DELIVERY
                                     0000    75 ;		LEVEL.
                                     0000    76 ;
                                     0000    77 ;		R0 = SS$_NORMAL = NORMAL COMPLETION.
                                     0000    78 ;
                                     0000    79 ;	R1 IS PRESERVED ACROSS CALL.
                                     0000    80 ;-
                                     0000    81  
                                 00000000    82 	.PSECT	AEXENONPAGED
                                     0000    83 EXE$BUFFRQUOTA::			;CHECK BUFFER BYTE COUNT QUOTA
   00000000'EF            51     B1  0000    84 	CMPW	R1,IOC$GW_MAXBUF	;REQUEST GREATER THAN SYSTEM MAXIMUM?
                          0A     1A  0007    85 	BGTRU	10$			;IF GTRU YES
                                     0009    86 EXE$BUFQUOPRC::				;CHECK PROCESS BUFFER QUOTA ONLY
            52         42 A4     DE  0009    87 	MOVAL	PCB$W_BYTCNT(R4),R2	;SET ADDRESS OF BYTE COUNT QUOTA WORD
         68 A4            51     B1  000D    88 	CMPW	R1,PCB$W_BYTLM(R4)	;REQUEST GREATER THAN PROCESS MAXIMUM?
                          07     1B  0011    89 	BLEQU	EXE$MULTIQUOTA		;IF LEQU NO
            50            1C     3C  0013    90 10$:	MOVZWL	#SS$_EXQUOTA,R0		;SET QUOTA EXCEEDED
                                 05  0016    91 	RSB				;
                                     0017    92 EXE$SNGLEQUOTA::			;CHECK SINGLE-UNIT RESOURCE REQUEST
            51            01     D0  0017    93 	MOVL	#1,R1			;SET NUMBER OF UNITS OF RESOURCE REQUESTED
                                     001A    94 EXE$MULTIQUOTA::			;CHECK MULTI-UNIT RESOURCE REQUEST
                          7E     DC  001A    95 15$:	MOVPSL	-(SP)			;READ CURRENT PSL
                                     001C    96 	DSBINT	#IPL$_SYNCH,R3		;SYNCHRONIZE ACCESS TO SYSTEM DATA BASE
            50            1C     3C  0022    97 	MOVZWL	#SS$_EXQUOTA,R0		;ASSUME QUOTA EXCEEDED
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   3
01              CHECK PROCESS RESOURCE REQUEST                                                                                   (1)

            62            51     B1  0025    98 	CMPW	R1,(R2)			;ENOUGH RESOURCE UNITS REMAINING?
                          0C     1A  0028    99 	BGTRU	30$			;IF GTRU NO
            50            01     3C  002A   100 	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION
            53            02     D0  002D   101 	MOVL	#IPL$_ASTDEL,R3		;SET TO RAISE TO AST DELIVERY LEVEL
                          8E     D5  0030   102 20$:	TSTL	(SP)+			;REMOVE PSL FROM STACK
                                     0032   103 	ENBINT	R3			;ALLOW INTERRUPTS
                                 05  0035   104 	RSB				;
         28 A4            0A     E0  0036   105 30$:	BBS	#PCB$V_SSRWAIT,PCB$L_STS(R4),20$ ;IF SET, NO WAIT
                          F5         003A       
            50            01     3C  003B   106 	MOVZWL	#RSN$_ASTWAIT,R0	;SET AST WAIT RESOURCE NUMBER
                          FFBF'  30  003E   107 	BSBW	SCH$RWAIT		;WAIT FOR AST
                          D7     11  0041   108 	BRB	15$			;
                                     0043   109 	.DSABL	LSB
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   4
01              GET PROTECTION MASK ACCORDING TO UIC ACCESS                                                                      (1)

                                     0043   111 	.SBTTL	GET PROTECTION MASK ACCORDING TO UIC ACCESS
                                     0043   112 ;+
                                     0043   113 ; EXE$GETACCESS - GET ACCESS RIGHTS ACCORDING TO UIC AND PROTECTION MASK
                                     0043   114 ;
                                     0043   115 ; THIS ROUTINE RETURNS THE ACCESS RIGHTS OF A GIVEN PROCESS PCB WITH
                                     0043   116 ; REGARDS TO A PROTECTION / UIC MASK PAIR.
                                     0043   117 ;
                                     0043   118 ; INPUTS:
                                     0043   119 ;
                                     0043   120 ;	R1 = PROTECTION MASK IN "RWED" FORM (SYSTEM,OWNER,GROUP,WORLD).
                                     0043   121 ;	R2 = UIC ASSOCIATED WITH PROTECTION MASK.
                                     0043   122 ;	R4 = PCB OF PROCESS WISHING VALIDATION.
                                     0043   123 ;
                                     0043   124 ; OUTPUTS:
                                     0043   125 ;	
                                     0043   126 ;	R0 = MASK IN "RWED" FORM (SYSTEM,OWNER,GROUP,WORLD) WHICH
                                     0043   127 ;	     CORRESPONDS TO THE ACCESS AVAILABLE TO THE PROCESS.
                                     0043   128 ;
                                     0043   129 ;	R3 IS PRESERVED ACROSS CALL.
                                     0043   130 ;-
                                     0043   131  
                                 00000000   132 	.PSECT	Y$EXEPAGED
                                     0000   133 EXE$GETACCESS::				;
            50       F000 8F     3C  0000   134 	MOVZWL	#^XF000,R0		;SET WORLD MASK
                                     0005   135  
                                     0005   136 ;
                                     0005   137 ; TEST GROUP ACCESS
                                     0005   138 ;
                                     0005   139  
            7E         22 A4     3C  0005   140 	MOVZWL	PCB$W_GRP(R4),-(SP)	;GET CURRENT PROCESS GROUP CODE
            10            10     ED  0009   141 	CMPZV	#16,#16,R2,(SP)+	;COMPARE GROUP VALUES
            8E            52         000C       
                          10     12  000E   142 	BNEQ	20$			;IF NEQ THEN NOT THE SAME GROUP
            08            0F     F0  0010   143 	INSV	#^XF,#8,#4,R0		;INSERT GROUP MASK
            50            04         0013       
                                     0015   144  
                                     0015   145 ;
                                     0015   146 ; TEST OWNER ACCESS
                                     0015   147 ;
                                     0015   148  
            52         20 A4     B1  0015   149 	CMPW	PCB$L_UIC(R4),R2	;COMPARE MEMBER VALUES
                          05     12  0019   150 	BNEQ	20$			;IF NEQ THENNOT SAME MEMBER
            04            0F     F0  001B   151 	INSV	#^XF,#4,#4,R0		;INSERT OWNER MASK
            50            04         001E       
                                     0020   152  
                                     0020   153 ;
                                     0020   154 ; CHECK SYSTEM ACCESS
                                     0020   155 ;
                                     0020   156  
         22 A4   00000000'EF     B1  0020   157 20$:	CMPW	L^EXE$GL_SYSUIC,PCB$W_GRP(R4) ;COMPARE GROUP WITH SYSTEM ACCESS
                          05     1F  0028   158 	BLSSU	30$			;IF LSSU THEN NOT SYSTEM
            00            0F     F0  002A   159 	INSV	#^XF,#0,#4,R0		;INSERT SYSTEM MASK
            50            04         002D       
                                     002F   160  
                                     002F   161 ;
                                     002F   162 ; FORM FINAL MASK
                                     002F   163 ;
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   5
01              GET PROTECTION MASK ACCORDING TO UIC ACCESS                                                                      (1)

                                     002F   164  
            50            51     AA  002F   165 30$:	BICW	R1,R0			;RETURN RESULT IN R0
                                 05  0032   166 	RSB				;
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   6
01              CHECK ACCESS PROTECTION                                                                                          (1)

                                     0033   168 	.SBTTL CHECK ACCESS PROTECTION
                                     0033   169 ;+
                                     0033   170 ; EXE$CHKCREACCES - CHECK CREATE ACCESS
                                     0033   171 ; EXE$CHKDELACCES - CHECK DELETE ACCESS
                                     0033   172 ; EXE$CHKLOGACCES - CHECK LOGICAL I/O FUNCTION ACCESS
                                     0033   173 ; EXE$CHKPHYACCES - CHECK PHYSICAL I/O FUNCTION ACCESS
                                     0033   174 ; EXE$CHKRDACCES - CHECK READ ACCESS
                                     0033   175 ; EXE$CHKWRTACCES - CHECK WRITE ACCESS
                                     0033   176 ;
                                     0033   177 ; THESE ROUTINES RETURN A TRUE OR FALSE VALUE ON ACCESS
                                     0033   178 ; GIVEN A REQUESTING PROCESS PCB AND A PROTECTION MASK / UIC PAIR.
                                     0033   179 ;
                                     0033   180 ; INPUTS:
                                     0033   181 ;
                                     0033   182 ;	R1 = PROTECTION MASK - "RWED" FOR (SYSTEM,OWNER,GROUP,WORLD).
                                     0033   183 ;	R2 = UIC ASSOCIATED WITH PROTECTION MASK.
                                     0033   184 ;	R4 = REQUESTING PCB.
                                     0033   185 ;
                                     0033   186 ; OUTPUTS:
                                     0033   187 ;
                                     0033   188 ;	R0 = SS$_NORMAL FOR ACCESS ALLOWED
                                     0033   189 ;	R0 = SS$_NOPRIV FOR ACCESS DENIED
                                     0033   190 ;
                                     0033   191 ;	R3 IS PRESERVED ACROSS CALL.
                                     0033   192 ;-
                                     0033   193  
                                     0033   194 	.ENABL	LSB
                                     0033   195 EXE$CHKDELACCES::			;CHECK DELETE ACCESS
                                     0033   196 EXE$CHKLOGACCES::			;CHECK LOGICAL I/O FUNCTION ACCESS
            7E       7777 8F     3C  0033   197 	MOVZWL	#^X7777,-(SP)		;SET COMPLEMENT OF NEEDED ACCESS
                          13     11  0038   198 	BRB	10$			;
                                     003A   199 EXE$CHKCREACCES::			;CHECK CREATE ACCESS
                                     003A   200 EXE$CHKPHYACCES::			;CHECK PHYSICAL I/O FUNCTION ACCESS
            7E       BBBB 8F     3C  003A   201 	MOVZWL	#^XBBBB,-(SP)		;SET COMPLEMENT OF NEEDED ACCESS
                          0C     11  003F   202 	BRB	10$			;
                                     0041   203 EXE$CHKRDACCES::			;CHECK READ ACCESS
            7E       EEEE 8F     3C  0041   204 	MOVZWL	#^XEEEE,-(SP)		;SET COMPLEMENT OF NEEDED ACCESS
                          05     11  0046   205 	BRB	10$			;
                                     0048   206 EXE$CHKWRTACCES::			;CHECK WRITE ACCESS
            7E       DDDD 8F     3C  0048   207 	MOVZWL	#^XDDDD,-(SP)		;SET COMPLEMENT OF NEEDED ACCESS
                          B1     10  004D   208 10$:	BSBB	EXE$GETACCESS		;GET THE PROCESS' ACCESS RIGHTS
            50            8E     CA  004F   209 	BICL	(SP)+,R0		;FORM ANSWER -- IF 0 THEN FALSE
                          04     13  0052   210 	BEQL	20$			;RETURN NO PRIVILEGE IF EQL
            50            01     9A  0054   211 	MOVZBL	#SS$_NORMAL,R0		;SET SUCCESS INDICATOR
                                 05  0057   212 	RSB				;AND RETURN
            50            24     3C  0058   213 20$:	MOVZWL	#SS$_NOPRIV,R0		;SET ERROR
                                 05  005B   214 	RSB				;AND RETURN
                                     005C   215 	.DSABL	LSB
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   7
01              INSERT ENTRY IN TIME DEPENDENT SCHEDULER QUEUE                                                                   (1)

                                     005C   217 	.SBTTL	INSERT ENTRY IN TIME DEPENDENT SCHEDULER QUEUE
                                     005C   218 ;+
                                     005C   219 ; EXE$INSTIMQ - INSERT ENTRY IN TIME DEPENDENT SCHEDULER QUEUE
                                     005C   220 ;
                                     005C   221 ; THIS ROUTINE IS CALLED TO INSERT AN ENTRY IN THE TIME DEPENDENT SCHEDULER
                                     005C   222 ; QUEUE. THE ENTRY IS THREADED INTO THE QUEUE ACCORDING TO ITS DUE TIME.
                                     005C   223 ; THE QUEUE IS ORDERED SUCH THAT THE MOST IMMINENT ENTRIES ARE AT THE FRONT
                                     005C   224 ; OF THE QUEUE.
                                     005C   225 ;
                                     005C   226 ; INPUTS:
                                     005C   227 ;
                                     005C   228 ;	R0 = LOW ORDER PART OF EXPIRATION TIME.
                                     005C   229 ;	R1 = HIGH ORDER PART OF EXPIRATION TIME.
                                     005C   230 ;	R5 = ADDRESS OF ENTRY TO INSERT IN TIME QUEUE.
                                     005C   231 ;
                                     005C   232 ;	IPL MUST BE IPL$_TIMER OR ABOVE.
                                     005C   233 ;
                                     005C   234 ; OUTPUTS:
                                     005C   235 ;
                                     005C   236 ;	SPECIFIED ENTRY IS INSERTED INTO THE TIME DEPENDENT SCHEDULER QUEUE
                                     005C   237 ;	ACCORDING TO ITS DUE TIME.
                                     005C   238 ;-
                                     005C   239  
                                     0000   240 	.PSECT
                                     0000   241 EXE$INSTIMQ::				;INSERT ENTRY IN TIME QUEUE
         18 A5            50     7D  0000   242 	MOVQ	R0,TQE$Q_TIME(R5)	;SET ABSOLUTE DUE TIME
            53       0000'CF     DE  0004   243 	MOVAL	W^EXE$GL_TQFL,R3	;GET ADDRESS OF TIME QUEUE LISTHEAD
            52            53     D0  0009   244 	MOVL	R3,R2			;COPY ADDRESS OF TIME QUEUE LISTHEAD
            52         04 A2     D0  000C   245 10$:	MOVL	TQE$L_TQBL(R2),R2	;GET ADDRESS OF NEXT ENTRY
            52            53     D1  0010   246 	CMPL	R3,R2			;END OF QUEUE?
                          0E     13  0013   247 	BEQL	20$			;IF EQL YES
         1C A2            51     D1  0015   248 	CMPL	R1,TQE$Q_TIME+4(R2)	;COMPARE HIGH ORDER PARTS OF TIME
                          F1     1F  0019   249 	BLSSU	10$			;IF LSSU NEW ENTRY MORE IMMINENT
                          06     1A  001B   250 	BGTRU	20$			;IF GTRU NEW ENTRY LESS IMMINENT
         18 A2            50     D1  001D   251 	CMPL	R0,TQE$Q_TIME(R2)	;COMPARE LOW ORDER PART OF TIME
                          E9     1F  0021   252 	BLSSU	10$			;IF LSSU NEW ENTRY MORE IMMINENT
            62            65     0E  0023   253 20$:	INSQUE	TQE$L_TQFL(R5),TQE$L_TQFL(R2) ;INSERT NEW ENTRY IN TIME QUEUE
                                 05  0026   254 	RSB				;
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   8
01              MAXIMIZE ACCESS MODE                                                                                             (1)

                                     0027   256 	.SBTTL	MAXIMIZE ACCESS MODE
                                     0027   257 ;+
                                     0027   258 ; EXE$MAXACMODE - MAXIMIZE ACCESS MODE
                                     0027   259 ;
                                     0027   260 ; THIS ROUTINE IS CALLED TO MAXIMIZE A SPECIFIED ACCESS MODE WITH THE PREVIOUS
                                     0027   261 ; MODE FIELD OF THE CURRENT PSL.
                                     0027   262 ;
                                     0027   263 ; INPUTS:
                                     0027   264 ;
                                     0027   265 ;	R0 = ACCESS MODE TO MAXIMIZE WITH PREVIOUS MODE FIELD OF PSL.
                                     0027   266 ;
                                     0027   267 ; OUTPUTS:
                                     0027   268 ;
                                     0027   269 ;	THE SPECIFIED ACCESS MODE IS MAXIMIZED WITH THE PREVIOUS MODE FIELD
                                     0027   270 ;	OF THE CURRENT PSL AND RETURNED IN REGISTER R0.
                                     0027   271 ;
                                     0027   272 ;	REGISTERS R2 AND R3 ARE PRESERVED ACROSS CALL.
                                     0027   273 ;-
                                     0027   274  
                                 0000005C   275 	.PSECT	Y$EXEPAGED
                                     005C   276 EXE$MAXACMODE::				;MAXIMIZE ACCESS MODE
                          51     DC  005C   277 	MOVPSL	R1			;READ CURRENT PSL
            02            16     ED  005E   278 	CMPZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R1,R0 ;COMPARE WITH PREVIOUS MODE
            50            51         0061       
                          05     15  0063   279 	BLEQ	10$			;IF LEQ SPECIFIED ACCESS MODE LESS PRIVILEGED
            02            16     EF  0065   280 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R1,R0 ;EXTRACT PREVIOUS MODE FIELD
            50            51         0068       
                                 05  006A   281 10$:	RSB				;
EXSUBROUT       - EXECUTIVE SUPPORT SUBROUTINES                  21-AUG-1978 19:45:52   VAX-11 MACRO X0.3-11               Page   9
01              REMOVE ENTRY FROM TIME DEPENDENT SCHEDULER QUEU                                                                  (1)

                                     006B   283 	.SBTTL	REMOVE ENTRY FROM TIME DEPENDENT SCHEDULER QUEUE
                                     006B   284 ;+
                                     006B   285 ; EXE$RMVTIMQ - REMOVE ENTRY FROM TIME DEPENDENT SCHEDULER QUEUE
                                     006B   286 ;
                                     006B   287 ; THIS ROUTINE IS CALLED TO REMOVE ONE OR MORE ENTRIES FROM THE TIME
                                     006B   288 ; DEPENDENT SCHEDULER QUEUE. ENTRIES ARE REMOVED BY TYPE, ACCESS MODE,
                                     006B   289 ; REQUEST IDENTIFICATION, AND PROCESS ID.
                                     006B   290 ;
                                     006B   291 ; INPUTS:
                                     006B   292 ;
                                     006B   293 ;	R2 = ACCESS MODE (ALL EQUAL AND HIGHER ACCESS MODES).
                                     006B   294 ;	R3 = REQUEST IDENTIFICATION (ZERO IMPLIES ALL).
                                     006B   295 ;	R4 = TYPE OF ENTRY TO REMOVE (ALL SINGLE AND REPEAT ENTRIES).
                                     006B   296 ;	R5 = PROCESS ID OF PROCESS TO REMOVE ENTRIES FOR.
                                     006B   297 ;
                                     006B   298 ;	IPL MUST BE IPL$_TIMER OR ABOVE.
                                     006B   299 ;
                                     006B   300 ;	IF SYSTEM SUBROUTINE OR WAKE REQUESTS ARE BEING REMOVED, THEN ACCESS
                                     006B   301 ;	MODE AND REQUEST IDENTIFICATION ARE NOT USED AND NEED NOT BE SUPPLIED
                                     006B   302 ;	IN THE CALLING SEQUENCE.
                                     006B   303 ;
                                     006B   304 ; OUTPUTS:
                                     006B   305 ;
                                     006B   306 ;	ALL ENTRIES OF THE SPECIFIED TYPE ARE REMOVED FROM THE TIME 
                                     006B   307 ;	DEPENDENT SCHEDULER QUEUE.
                                     006B   308 ;-
                                     006B   309  
                                 00000027   310 	.PSECT
                                     0027   311 EXE$RMVTIMQ::				;REMOVE ENTRY FROM TIME QUEUE
            50       0000'CF     DE  0027   312 10$:	MOVAL	W^EXE$GL_TQFL,R0	;GET ADDRESS OF TIMER QUEUE LISTHEAD
            51            50     D0  002C   313 	MOVL	R0,R1			;COPY LISTHEAD ADDRESS
            51            61     D0  002F   314 20$:	MOVL	TQE$L_TQFL(R1),R1	;GET ADDRESS OF NEXT ENTRY
            50            51     D1  0032   315 	CMPL	R1,R0			;END OF QUEUE?
                          5C     13  0035   316 	BEQL	80$			;IF EQL YES
            02            00     ED  0037   317 	CMPZV	#0,#2,TQE$B_RQTYPE(R1),R4 ;REQUEST TYPE MATCH?
