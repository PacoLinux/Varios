                                     0000    59 	.SBTTL	EXE$NULLPROC - NULL PROCESS 
                                     0000    60 ;++
                                     0000    61 ; FUNCTIONAL DESCRIPTION:
                                     0000    62 ;	EXE$NULLPROC CONTAINS THE CODE EXECUTED ONLY WHEN NO OTHER
                                     0000    63 ;	PROCESS REQUIRES PROCESSOR TIME.  THE CUMULATIVE EXECUTION
                                     0000    64 ;	TIME FOR THE NULL PROCESS REPRESENTS THE SYSTEM IDLE TIME.
                                     0000    65 ;	THE REASON FOR BEING IDLE IS DETERMINED BY THE NULL PROCESS
                                     0000    66 ;	AND A CORRESPONDING COUNTER INCREMENTED.  THIS SET OF COUNTERS
                                     0000    67 ;	CAN BE USED TO DETERMINE THE TIME SPENT IN EACH POSSIBLE
                                     0000    68 ;	IDLE STATE.
                                     0000    69 ;
                                     0000    70 ; CALLING SEQUENCE:
                                     0000    71 ;	NONE
                                     0000    72 ;
                                     0000    73 ; INPUT PARAMETERS:
                                     0000    74 ;	NONE
                                     0000    75 ;
                                     0000    76 ; IMPLICIT INPUTS:
                                     0000    77 ;	NONE
                                     0000    78 ;
                                     0000    79 ; OUTPUT PARAMETERS:
                                     0000    80 ;	NONE
                                     0000    81 ;
                                     0000    82 ; IMPLICIT OUTPUTS:
                                     0000    83 ;	NONE
                                     0000    84 ;
                                     0000    85 ; COMPLETION CODES:
                                     0000    86 ;	NONE
                                     0000    87 ;
                                     0000    88 ; SIDE EFFECTS:
                                     0000    89 ;	NONE
                                     0000    90 ;
                                     0000    91 ;--
                                     0000    92 
