                                     0004  4700 	.SBTTL	INITIALIZE DRIVER DATA BASE
                                     0004  4800 ;+
                                     0004  4900 ; IOC$INITDRV - INITIALIZE DRIVER DATA BASE
                                     0004  5000 ;
                                     0004  5100 ; THIS ROUTINE IS CALLED AFTER HAVING LOADED ALL OR PART OF A DEVICE DATA BASE.
                                     0004  5200 ; IT MUST BE CALLED FOR EACH UNIT THAT IS DEFINED. ITS FUNCTION IS TO USE THE
                                     0004  5300 ; INITIALIZATION TABLE IN THE DRIVER PROLOGUE TO INITIALIZE STATIC INFORMATION
                                     0004  5400 ; IN THE DRIVER DATA BASE.
                                     0004  5500 ;
                                     0004  5600 ; INPUTS:
                                     0004  5700 ;
                                     0004  5800 ;	R4 = ADDRESS OF DRIVER PROLOGUE TABLE.
                                     0004  5900 ;	R5 = ADDRESS OF DEVICE UCB.
                                     0004  6000 ;
                                     0004  6100 ; OUTPUTS:
                                     0004  6200 ;
                                     0004  6300 ;	THE DRIVER PROLOGUE TABLE IS INTERPRETED AND THE RESPECTIVE DATA BASE
                                     0004  6400 ;	IS INITIALIZED.
                                     0004  6500 ;
                                     0004  6600 ;	R0 LOW BIT CLEAR INDICATES A BAD DATA STRUCTURE.
                                     0004  6700 ;
                                     0004  6800 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0004  6900 ;-
                                     0004  7000  
                                     0004  7100 	.ENABL	LSB
                                     0004  7200 IOC$INITDRV::				;INITIALIZE DRIVER DATA BASE
            53         10 A4     3C  0004  7300 	MOVZWL	DPT$W_INITTAB(R4),R3	;GET OFFSET TO INITIALIZATION TABLE
                          04     11  0008  7350 	BRB	10$			;BR TO COMMON CODE
