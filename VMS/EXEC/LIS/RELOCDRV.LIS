                                     0072 20000 ; LOCATE - LOCATE DATA STRUCTURE TYPE
                                     0072 20100 ;
                                     0072 20200 ; INPUTS:
                                     0072 20300 ;
                                     0072 20400 ;	R5 = ADDRESS OF DEVICE UCB.
                                     0072 20500 ;
                                     0072 20600 ; OUTPUTS:
                                     0072 20700 ;
                                     0072 20800 ;	IF THE STRUCTURE TYPE CODE IS ZERO, THEN SUCCESS IS RETURNED TO THE CALLERS
                                     0072 20900 ;	CALLER. IF THE STRUCTURE TYPE CODE IS INVALID, THEN AN ERROR IS RETURNED
                                     0072 21000 ;	TO THE CALLERS CALLER. ELSE THE APPROPRIATE STRUCTURE BASE ADDRESS IS
