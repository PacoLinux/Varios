                                     0020 11000 	.SBTTL	LOCAL SUBROUTINE TO EXECUTE OPERATION CODE
                                     0020 11100 ;
                                     0020 11200 ; EXECUTE - EXECUTE OPERATION CODE
                                     0020 11300 ;
                                     0020 11400 ; THIS ROUTINE IS CALLED TO EXECUTE A SINGLE RELOCATION OPERATION CODE.
                                     0020 11500 ;
                                     0020 11600 ; INPUTS:
                                     0020 11700 ;
                                     0020 11800 ;	R1 = OPERATION CODE.
                                     0020 11900 ;	R2 = STRUCTURE FIELD BASE ADDRESS.
                                     0020 12000 ;	R3 = ADDRESS OF NEXT RELOCATION BYTE.
                                     0020 12100 ;	R4 = BASE ADDRESS OF DRIVER PROLOGUE TABLE.
                                     0020 12200 ;	R5 = ADDRESS OF DEVICE UCB.
                                     0020 12300 ;
                                     0020 12400 ; OUTPUTS:
                                     0020 12500 ;
                                     0020 12600 ;	IF AN INVALID OPERATION CODE IS SPECIFIED, A FAILURE IS RETURNED TO THE
                                     0020 12700 ;	CALLERS CALLER. ELSE THE OPERAND VALUE IS STORE IN THE SPECIFIED FIELD
                                     0020 12800 ;	IN THE SPECIFIED STRUCTURE.
                                     0020 12900 ;
                                     0020 13000  
                                     0020 13100 EXECUTE:				;EXECUTE RELOCATION OPERATION
                                     0020 13200 	CASE	R1,<-			;DISPATCH TO FETCH ROUTINE
                                     0020 13300 		10$,-			;BYTE IMMEDIATE
                                     0020 13400 		30$,-			;WORD IMMEDIATE
                                     0020 13500 		50$,-			;WORD DISPLACED
                                     0020 13600 		70$,-			;LONGWORD IMMEDIATE
                                     0020 13700 		90$,-			;FIELD IMMEDIATE
                                     0020 13800 		>
                                     002E 13900  
                                     002E 14000 ;
                                     002E 14100 ; INDIRECT OPERAND
                                     002E 14200 ;
                                     002E 14300  
            50            93     D0  002E 14400 	MOVL	@(R3)+,R0		;GET DATA LONGWORD
                                     0031 14500 	CASE	R1,<-			;DISPATCH TO STORE ROUTINE
                                     0031 14600 		20$,-			;STORE BYTE
                                     0031 14700 		40$,-			;STORE WORD
                                     0031 14800 		60$,-			;STORE WORD DISPLACED
                                     0031 14900 		80$,-			;STORE LONGWORD
                                     0031 15000 		100$,-			;STORE FILED
                                     0031 15100 		>,LIMIT=#^X80		;
                          8E     D5  0041 15200 	TSTL	(SP)+			;REMOVE RETURN FROM STACK
                          50     D4  0043 15300 	CLRL	R0			;SET FAILURE INDICATION
                                 05  0045 15400 	RSB				;
                                     0046 15500  
                                     0046 15600 ;
                                     0046 15700 ; BYTE IMMEDIATE
                                     0046 15800 ;
                                     0046 15900  
            50            83     9A  0046 16000 10$:	MOVZBL	(R3)+,R0		;GET DATA BYTE
            62            50     90  0049 16100 20$:	MOVB	R0,(R2)			;STORE BYTE
                                 05  004C 16200 	RSB				;
                                     004D 16300  
                                     004D 16400 ;
                                     004D 16500 ; WORD IMMEDIATE
                                     004D 16600 ;
