                                     0016 12200 	.SBTTL	EXE$CREPRC - CREATE PROCESS SYSTEM SERVICE
                                     0016 12300 ;++
                                     0016 12400 ; FUNCTIONAL DESCRIPTION:
                                     0016 12500 ;	EXE$CREPRC CREATES A NEW PROCESS ACCORDING TO THE 
                                     0016 12600 ;	SUPPLIED PARAMETERS.  THE NEW PROCESS MAY BE EITHER A SUB-PROCESS
                                     0016 12700 ;	OR AN INDEPENDENT, DETACHED PROCESS.
                                     0016 12800 ;
                                     0016 12900 ; CALLING SEQUENCE:
                                     0016 13000 ;	CALLG	ARGLIST,EXE$CREPRC
                                     0016 13100 ;
                                     0016 13200 ; INPUT PARAMETERS:
                                     0016 13300 ;	PIDADR(AP) - ADDRESS AT WHICH TO RETURN PID OF CREATED PROCESS
                                     0016 13400 ;	IMAGE(AP)  - ADDRESS OF IMAGE NAME STRING DESCRIPTOR
                                     0016 13500 ;	INPUT(AP)  - ADDRESS OF INPUT NAME STRING DESCRIPTOR
                                     0016 13600 ;	OUTPUT(AP) - ADDRESS OF OUTPUT NAME STRING DESCRIPTOR
                                     0016 13700 ;	ERROR(AP)  - ADDRESS OF ERROR LOGICAL NAME STRING DESCRIPTOR
                                     0016 13800 ;	PRVADR(AP) - ADDRESS OF PRIVILEGE MASK FOR CREATED PROCESS
                                     0016 13900 ;	QUOTA(AP)  - POINTER TO QUOTA BUFFER
                                     0016 14000 ;	PRCNAM(AP) - ADDRESS OF PROCESS NAME STRING DESCRIPTOR
                                     0016 14100 ;	BASPRI(AP) - BASE PRIORITY FOR CREATED PROCESS
                                     0016 14200 ;	UIC(AP)    - UIC FOR CREATED PROCESS(0 => SUB-PROCESS)
                                     0016 14300 ;	MBXUNT(AP) - MAILBOX UNIT NUMBER FOR TERMINATION MESSAGES
                                     0016 14400 ;	STSFLG(AP) - STATUS FLAG SETTINGS FOR CREATED PROCESS
                                     0016 14500 ;	R4 - ADDRESS OF CURRENT PROCESS CONTROL BLOCK
                                     0016 14600 ;
                                     0016 14700 ;			BIT	MEANING
                                     0016 14800 ;			---	-------
                                     0016 14900 ;			 0	RESOURCE WAIT DISABLE
                                     0016 15000 ;			 1	SYSTEM SERVICE FAIL EXCEPTION ENABLE
                                     0016 15100 ;			 2	PROCESS SWAP MODE
                                     0016 15200 ;			 3	ACCOUNTING MESSAGE DISABLE
                                     0016 15300 ;			 4	BATCH INDICATOR
                                     0016 15400 ;			 5	HIBERNATE BEFORE CALLING INITIAL IMAGE
                                     0016 15500 ;			 6	BYPASS LOGIN VERIFICATION FOR DETACHED
                                     0016 15600 ;				PROCESS.
                                     0016 15700 ;
                                     0016 15800 ;
                                     0016 15900 ; OUTPUT PARAMETERS:
                                     0016 16000 ;	R0  -  COMPLETION STATUS CODE
                                     0016 16100 ;	@PIDADR(AP) - PROCESS ID (PID) OF CREATED PROCESS
                                     0016 16200 ;
                                     0016 16300 ; COMPLETION CODES:
                                     0016 16400 ;	SS$_NORMAL   -   SUCCESSFUL COMPLETION
                                     0016 16500 ;	SS$_ACCVIO   -   ACCESS VIOLATION
                                     0016 16600 ;	SS$_DUPLNAM  -   DUPLICATE PROCESS NAME
                                     0016 16700 ;	SS$_EXQUOTA  -   EXCEEDED QUOTA
                                     0016 16800 ;	SS$_INSFMEM  -   INSUFFICIENT MEMORY AVAILABLE
                                     0016 16900 ;	SS$_IVLOGNAM -   INVALID LOGICAL NAME
                                     0016 17000 ;	SS$_IVQUOTAL -   INVALID QUOTA LIST
                                     0016 17100 ;	SS$_IVSTSFLG -   INVALID STATUS FLAG ARGUMENT
                                     0016 17200 ;	SS$_NOPRIV   -   NO PRIVILEGE FOR SPECIFIED OPERATION
                                     0016 17300 ;
                                     0016 17400 ; SIDE EFFECTS:
                                     0016 17500 ;	IF NO ERRORS ARE DETECTED, A NEW PROCESS WILL HAVE BEEN ACTIVATED
                                     0016 17600 ;	AND MARKED NON-RESIDENT.  THE INITIAL INSWAP FOR THIS PROCESS
                                     0016 17700 ;	WILL BE FROM THE SHELL PROCESS.  EXECUTION FOR THIS PROCESS
                                     0016 17800 ;	BEGINS IN THE ROUTINE EXE$PROCSTRT WHICH WILL MOVE THE INFORMATION
