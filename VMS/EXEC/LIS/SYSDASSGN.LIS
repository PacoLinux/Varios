                                     0000    49 	.SBTTL	DEASSIGN I/O CHANNEL
                                     0000    50 ;+
                                     0000    51 ; EXE$DASSGN - DEASSIGN I/O CHANNEL
                                     0000    52 ;
                                     0000    53 ; THIS SERVICE DEASSIGNS A PREVIOUSLY ASSIGNED I/O CHANNEL AND CLEARS THE
                                     0000    54 ; LINKAGE AND CONTROL INFORMATION IN THE CORRESPONDING CHANNEL CONTROL BLOCK.
                                     0000    55 ; IF ANY I/O IS OUTSTANDING ON THE CHANNEL IT IS CANCELLED. IF A FILE IS
                                     0000    56 ; OPEN ON THE CHANNEL IT IS CLOSED. IF A MAILBOX WAS ASSOCIATED WITH THE
                                     0000    57 ; DEVICE WHEN IT WAS ASSIGNED, THE LINKAGE TO THE MAILBOX IS CLEARED. IF THE
                                     0000    58 ; THE CHANNEL IS LAST ONE ASSIGNED TO THE DEVICE AND IT IS MARKED FOR DIS-
                                     0000    59 ; MOUNT, THEN THE DISMOUNT IS COMPLETED.
                                     0000    60 ;
                                     0000    61 ; INPUTS:
                                     0000    62 ;
                                     0000    63 ;	CHAN(AP) = NUMBER OF THE I/O CHANNEL TO DEASSIGN.
                                     0000    64 ;
                                     0000    65 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    66 ;
                                     0000    67 ; OUTPUTS:
                                     0000    68 ;
                                     0000    69 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO DEASSIGN CHANNEL.
                                     0000    70 ;
                                     0000    71 ;		R0 = SS$_IVCHAN - INVALID CHANNEL NUMBER SPECIFIED.
                                     0000    72 ;
                                     0000    73 ;		R0 = SS$_NOPRIV - SPECIFIED CHANNEL IS NOT ASSIGNED TO A
                                     0000    74 ;			DEVICE OR THE CALLER DOES NOT HAVE SUFFICIENT
                                     0000    75 ;			PRIVILEGE TO ACCESS THE CHANNEL.
                                     0000    76 ;
                                     0000    77 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    78 ;
                                     0000    79 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0000    80 ;-
                                     0000    81  
                               00FC' 0000    82 	.ENTRY	EXE$DASSGN,^M<R2,R3,R4,R5,R6,R7>
            55         04 AC     3C  0002    83 	MOVZWL	CHAN(AP),R5		;GET CHANNEL NUMBER
            50            55     D0  0006    84 	MOVL	R5,R0			;COPY I/O CHANNEL NUMBER
                 00000000'EF     16  0009    85 	JSB	IOC$VERIFYCHAN		;VERIFY CHANNEL NUMBER
            57            50     E9  000F    86 	BLBC	R0,50$			;IF LBC INVALID CHANNEL
            56            51     D0  0012    87 	MOVL	R1,R6			;COPY ADDRESS OF CCB
            57            52     D0  0015    88 	MOVL	R2,R7			;SAVE CHANNEL INDEX
                                     0018    89 	$CANCEL_S R5			;CANCEL I/O ON CHANNEL
                       04 A6     D5  0022    90 20$:	TSTL	CCB$L_WIND(R6)		;FILE ACCESSED ON CHANNEL?
                          25     13  0025    91 	BEQL	30$			;IF EQL NO
                                     0027    92 	$QIOW_S	#31,R5,#IO$_DEACCESS	;DEACCESS FILE
            07            50     E8  0042    93 	BLBS	R0,30$			;IF LBS SUCCESSFUL COMPLETION
            50       00AC 8F     B1  0045    94 	CMPW	#SS$_FILNOTACC,R0	;NO FILE ACCESSED?
                          1D     12  004A    95 	BNEQ	50$			;IF NEQ NO
                          7E     DC  004C    96 30$:	MOVPSL	-(SP)			;SAVE CURRENT PROCESSOR STATUS
                                     004E    97 	SETIPL	#IPL$_ASTDEL		;RAISE TO AST DELIVERY LEVEL
                       0A A6     B5  0051    98 	TSTW	CCB$W_IOC(R6)		;ANY I/O STILL OUTSTANDING?
                          14     13  0054    99 	BEQL	60$			;IF EQL NO
                                     0056   100 	SETIPL	#IPL$_SYNCH		;RAISE TO SYNCHRONIZATION LEVEL
            50            13     DB  0059   101 	MFPR	#PR$_ASTLVL,R0		;READ CURRENT AST LEVEL
                          06     13  005C   102 	BEQL	40$			;IF EQL AST QUEUED
            50            01     3C  005E   103 	MOVZWL	#RSN$_ASTWAIT,R0	;SET AST WAIT RESOURCE WAIT NUMBER
                          FF9C'  30  0061   104 	BSBW	SCH$RWAIT		;WAIT FOR AST
                                     0064   105 40$:	SETIPL	#0			;ALLOW INTERRUPTS
