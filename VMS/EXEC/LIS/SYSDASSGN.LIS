                       1A A5     B4  00AD   128 	CLRW	UCB$W_VPROT(R5)		;CLEAR VOLUME PROTECTION MASK
         58 A5       0800 8F     AA  00B0   129 	BICW	#UCB$M_VALID,UCB$W_STS(R5) ;CLEAR SOFTWARE VOLUME VALID
            50         24 A5     D0  00B6   130 90$:	MOVL	UCB$L_DDB(R5),R0	;GET ADDRESS OF DDB
            50         0C A0     D0  00BA   131 	MOVL	DDB$L_DDT(R0),R0	;GET ADDRESS OF DDT
                                     00BE   132 	SETIPL	UCB$B_FIPL(R5)		;RAISE TO DEVICE FORK IPL
            53         4C A5     D0  00C2   133 	MOVL	UCB$L_IRP(R5),R3	;GET CURRENT I/O PACKET ADDRESS
            52            57     D0  00C6   134 	MOVL	R7,R2			;SET CHANNEL INDEX
            50         0C A0     C1  00C9   135 	ADDL3	DDT$L_CANCEL(R0),R0,R1	;CALCULATE ADDRESS OF CANCEL ROUTINE
                          51         00CD       
                          03     19  00CE   136 	BLSS	100$			;IF LSS SYSTEM ADDRESS
            51            50     C2  00D0   137 	SUBL	R0,R1			;SUBTRACT OUT BASE ADDRESS
                          61     16  00D3   138 100$:	JSB	(R1)			;CALL CANCEL I/O ROUTINE
                                     00D5   139 	SETIPL	#IPL$_ASTDEL		;LOWER TO AST DELIVERY LEVEL
            53            55     D0  00D8   140 	MOVL	R5,R3			;COPY ADDRESS OF DEVICE UCB
            55         54 A5     D0  00DB   141 	MOVL	UCB$L_AMB(R5),R5	;GET ASSOCIATED MAILBOX UCB ADDRESS
                          0E     13  00DF   142 	BEQL	110$			;IF EQL NONE
         34 A5            00'    E1  00E1   143 	BBC	S^#DEV$V_MBX,UCB$L_DEVCHAR(R5),110$ ;IF CLR, DEVICE NOT MAILBOX
                          09         00E5       
                       54 A3     D4  00E6   144 	CLRL	UCB$L_AMB(R3)		;CLEAR ASSOCIATED MAILBOX ADDRESSS
                       50 A5     B7  00E9   145 	DECW	UCB$W_REFC(R5)		;DECREMENT MAILBOX UCB REFERENCE COUNT
                          FF11'  30  00EC   146 	BSBW	IOC$DELMBX		;CHECK FOR MAILBOX DELETION
            55            66     D0  00EF   147 110$:	MOVL	CCB$L_UCB(R6),R5	;RETRIEVE ASSIGNED DEVICE UCB ADDRESS
                          FF0B'  30  00F2   148 	BSBW	IOC$DELMBX		;CHECK FOR MAILBOX DELETION
            50            01     3C  00F5   149 120$:	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION
