                                     0021 25400 	.SBTTL	BOO$QIO - BOOTSTRAP QIO ROUTINE
                                     0021 25500 ;++
                                     0021 25600 ; FUNCTIONAL DESCRIPTION:
                                     0021 25700 ;	BOO$QIO PROVIDES THE DEVICE INDEPENDENT I/O INTERFACE FOR BOTH
                                     0021 25800 ;	READING AND WRITING THE BOOTSTRAP DEVICE.
                                     0021 25900 ;
                                     0021 26000 ; CALLING SEQUENCE:
                                     0021 26100 ;	CALLG	ARGLIST,BOO$QIO
                                     0021 26200 ;
                                     0021 26300 ; INPUT PARAMETERS:
                                     0021 26400 ;	BUF(AP)  - BUFFER ADDRESS
                           00000004  0021 26500 	BUF	 = 4
                                     0021 26600 ;	SIZE(AP) - SIZE OF BUFFER IN BYTES
                           00000008  0021 26700 	SIZE	 = 8
                                     0021 26800 ;	LBN(AP)  - LOGICAL BLOCK NUMBER
                           0000000C  0021 26900 	LBN	 = 12
                                     0021 27000 ;	FUNC(AP) - FUNCTION CODE
                                     0021 27100 ;			ACCEPTS IO$_READLBLK AND IO$_WRITELBLK
                           00000010  0021 27200 	FUNC	 = 16
                                     0021 27300 ;	MODE(AP) - ADDRESS INTERPRETATION MODE
                                     0021 27400 ;			0 => PHYSICAL, 1 => VIRTUAL
                           00000014  0021 27500 	MODE	 = 20
                                     0021 27600 ;
                                     0021 27700 ;	RPB(AP)	- ADDRESS OF RESTART PARAMETER BLOCK
                           00000018  0021 27800 	RPB	 = 24
                                     0021 27900 ;
                                     0021 28000 ; OUTPUT PARAMETERS:
                                     0021 28100 ;	R0 - COMPLETION STATUS CODE
                                     0021 28200 ;	R1 - TOTAL BYTES TRANSFERRED
                                     0021 28300 ;
                                     0021 28400 ;--
                                     0021 28500 
                               0FFC  0021 28600 BOO$QIO::	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9,R10,R11>	; PRESERVE REGISTERS
            59         18 AC     D0  0023 28700 	MOVL	RPB(AP),R9		; GET BASE ADDRESS OF RESTART PARAMETER BLK
