X01             IOGEN$CNTRL_INI                                                                                                  (1)

                                     004C 19200 	.SBTTL	IOGEN$CNTRL_INI
                                     004C 19300 ;++
                                     004C 19400 ;
                                     004C 19500 ; IOGEN$CNTRL_INI - INITIALIZE THE DEVICE CONTROLLER
                                     004C 19600 ;
                                     004C 19700 ; INPUTS - R6 = ADDR OF DDB
                                     004C 19800 ;	   R11 = ADDR OF DRIVER PROLOGUE TABLE
                                     004C 19900 ;
                                     004C 20000 ;--
                                     004C 20100 IOGEN$CNTRL_INI::
         0C AB         00'8F     91  004C 20200 	CMPB	#AT$_UBA,DPT$B_ADPTYPE(R11) ;UBA DEVICE?
                          2F     12  0051 20300 	BNEQ	20$			;BR IF NOT
                     0FFC 8F     BB  0053 20350 	PUSHR	#^M<R2,R3,R4,R5,R6,R7,R8,R9,R10,R11> ;SAVE REGISTERS
            50         04 A6     D0  0057 20400 	MOVL	DDB$L_UCB(R6),R0	;GET ADDR OF FIRST UCB
            58         20 A0     D0  005B 20500 	MOVL	UCB$L_CRB(R0),R8	;GET ADDR OF CRB
            55         1C A8     D0  005F 20600 	MOVL	CRB$L_INTD+VEC$L_IDB(R8),R5 ;SET ADDR OF IDB
            54            65     D0  0063 20700 	MOVL	IDB$L_CSR(R5),R4	;SET ADDR OF CSR
            50         0C A5     D0  0066 20800 	MOVL	IDB$L_ADP(R5),R0	;GET ADDR OF ADP
            50            60     D0  006A 20810 	MOVL	ADP$L_CSR(R0),R0	;GET ADDR OF UBA CSR
                          64     B5  006D 20820 	TSTW	(R4)			;TEST IF CONTROLLER EXISTS
         08 A0         08 A0     D0  006F 20830 	MOVL	UBA$L_SR(R0),UBA$L_SR(R0) ;TEST FOR ERROR AND CLEAR IT
                          08     12  0074 20840 	BNEQ	10$			;BR IF THERE WAS AN ERROR
            50         20 A8     D0  0076 21000 	MOVL	CRB$L_INTD+VEC$L_INITIAL(R8),R0 ;GET ADDR OF INIT ROUTINE
                          02     13  007A 21100 	BEQL	10$			;BR IF NONE
                                     007C 21102 				;R4 = ADDR OF CSR
                                     007C 21104 				;R5 = ADDR OF IDB
                                     007C 21106 				;R8 = ADDR OF CRB
                          60     16  007C 21300 	JSB	(R0)			;INIT CONTROLLER
                                     007E 21350 10$:
                     0FFC 8F     BA  007E 21400 	POPR	#^M<R2,R3,R4,R5,R6,R7,R8,R9,R10,R11> ;RESTORE REGISTERS
                                     0082 21500 20$:	
                                 05  0082 21600 	RSB
                                     0083 21610 
                                     0083 21620 	.END
