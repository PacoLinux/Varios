X01             IOGEN$LOCK_IODB/IOGEN$UNLK_IODB                                                                                  (1)

                                     0000 12800 	.SBTTL	IOGEN$LOCK_IODB/IOGEN$UNLK_IODB
                                     0000 12900 ;++
                                     0000 13000 ; IOGEN$LOCK_IODB - LOCK THE SYSTEM I/O DATABASE FOR WRITE
                                     0000 13100 ; IOGEN$UNLK_IODB - UNLOCK THE SYSTEM I/O DATABASE
                                     0000 13200 ;
                                     0000 13300 ; FUNCTIONAL DESCRIPTION:
                                     0000 13400 ;
                                     0000 13500 ;	LOCK - GETS THE I/O DATABASE MUTEX FOR WRITE
                                     0000 13600 ;	UNLOCK - RETURNS THE I/O DATABASE MUTEX AND RETURNS IPL TO 1.
                                     0000 13700 ;
                                     0000 13800 ;
                                     0000 13900 ; INPUTS:
                                     0000 14000 ;
                                     0000 14100 ;	NONE
                                     0000 14200 ;
                                     0000 14300 ; OUTPUTS:
                                     0000 14400 ;
                                     0000 14500 ;	I/O DATABASE LOCKED AND IPL RAISED OR UNLOCKED AND IPL LOWERED
                                     0000 14600 ;--
                                 00000000 14650 	.PSECT	IOGEN$CODE RD,NOWRT,EXE
                                     0000 14700 
                                     0000 14800 IOGEN$LOCK_IODB::
            50   00000000'GF     DE  0000 14900 	MOVAL	G^IOC$GL_MUTEX,R0	; GET I/O DATA BASE MUTEX
            54   00000000'GF     D0  0007 15000 	MOVL	G^SCH$GL_CURPCB,R4	; GET OWN PCB ADDRESS
                 00000000'GF     16  000E 15100 	JSB	G^SCH$LOCKW		; AND LOCK IT
                                 05  0014 15200 	RSB
                                     0015 15300 
                                     0015 15400 IOGEN$UNLK_IODB::
            50   00000000'GF     DE  0015 15500 	MOVAL	G^IOC$GL_MUTEX,R0	; GET I/O DATA BASE MUTEX
            54   00000000'GF     D0  001C 15600 	MOVL	G^SCH$GL_CURPCB,R4	; AND OWN PCB ADDRESS
                 00000000'GF     16  0023 15700 	JSB	G^SCH$UNLOCK		; AND UNLOCK IT
                                     0029 15800 	SETIPL	#0			; ALSO LOWER IPL
                                 05  002C 15900 	RSB
