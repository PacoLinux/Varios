X01             IOGEN$ALLOBLOCK                                                                                                  (1)

                                     002D 16300 	.SBTTL	IOGEN$ALLOBLOCK
                                     002D 16400 ;++
                                     002D 16500 ;
                                     002D 16600 ; IOGEN$ALLOBLOCK - ALLOCATE CONTROL BLOCKS
                                     002D 16700 ;
                                     002D 16800 ;	ALLOCATES A BLOCK OF MEMORY FROM THE NON-PAGED POOL AND
                                     002D 16900 ;	ZEROS IT.
                                     002D 17000 ;
                                     002D 17100 ; INPUTS:
                                     002D 17150 ;
                                     002D 17200 ;	R1 = SIZE OF BLOCK DESIRED
                                     002D 17300 ;
                                     002D 17400 ; OUTPUTS:
                                     002D 17450 ;
                                     002D 17500 ;	R0 = STATUS OF ALLOCATION
                                     002D 17600 ;	R1 = SIZE OF ALLOCATED BLOCK
                                     002D 17700 ;	R2 = ADDRESS OF ALLOCATED BLOCK
                                     002D 17800 ;
                                     002D 17900 ;--
                                     002D 18000 IOGEN$ALLOBLOCK::
                          38     BB  002D 18050 	PUSHR	#^M<R3,R4,R5>		;SAVE REGISTERS
                 00000000'GF     16  002F 18100 	JSB	G^EXE$ALONONPAGED	;ALLOCATE NON-PAGED POOL
            08            50     E8  0035 18200 	BLBS	R0,10$			;BR IF SUCCESS
            50       0000'8F     3C  0038 18300 	MOVZWL	#SS$_INSFMEM,R0		;SET ERROR STATUS
                          38     BA  003D 18350 	POPR	#^M<R3,R4,R5>		;RESTORE REGISTERS
                                 05  003F 18400 	RSB				;RETURN
                                     0040 18500 10$:
                          07     BB  0040 18600 	PUSHR	#^M<R0,R1,R2>		;SAVE REGISTERS
         FC AF            00     2C  0042 18700 20$:	MOVC5	#0,20$,#0,R1,(R2)	;ZERO THE BUFFER
            51            00         0046       
                          62         0048       
                          3F     BA  0049 18800 	POPR	#^M<R0,R1,R2,R3,R4,R5>	;RESTORE REGISTERS
                                 05  004B 18900 	RSB				;RETURN
                                     004C 19000 
