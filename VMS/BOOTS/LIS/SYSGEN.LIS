       0000'CF            6C     FA  2112 19700 	CALLG	(AP),W^BOO$SEARCH	; Search for parameter
            E4            50     E9  2117 19800 	BLBC	R0,30$			; Next parameter if not found
       0000'CF            6C     FA  211A 19900 	CALLG	(AP),W^BOO$SETVALUE	; Set value of parameter
                          FFDC   31  211F 20000 	BRW	30$			; Continue with next parameter
            50            01     3C  2122 20100 DONE:	MOVZWL	#1,R0			; Return success
