                                     20D0 15800 	.SBTTL	BOO$USEFILE - Use parameter file
                                     20D0 15900 ;++
                                     20D0 16000 ; Functional description:
                                     20D0 16100 ;	BOO$USEFILE reads the specified file in response to the USE
                                     20D0 16200 ;	command and merges all of the values specified in that file into
                                     20D0 16300 ;	the working copy of the parameter values.  This is accomplished
                                     20D0 16400 ;	by looking up each value specified and merging the associated 
                                     20D0 16500 ;	value.
                                     20D0 16600 ;
                                     20D0 16700 ; Calling sequence:
                                     20D0 16800 ;	CALLG	arglist,BOO$USEFILE
                                     20D0 16900 ;
                                     20D0 17000 ; Input Parameters:
                                     20D0 17100 ;	TPA$L_TOKENCNT(AP) - Length of file name string
                                     20D0 17200 ;	TPA$L_TOKENPTR(AP) - Address fo file name string
                                     20D0 17300 ; Output Parameters:
                                     20D0 17400 ;	R0 - Completion status code
                                     20D0 17500 ;
                                     20D0 17600 ;--
                                     20D0 17700 BOO$USEFILE::				;
                               03FC  20D0 17800 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>	; Entry mask
            57         10 AC     9E  20D2 17900 	MOVAB	TPA$L_TOKENCNT(AP),R7	; Set address of file name descriptor
                          DF27'  30  20D6 18000 	BSBW	BOO$FILOPEN		; Open specified file
            04            50     E8  20D9 18100 	BLBS	R0,20$			; Continue if success
            50            01     3C  20DC 18200 10$:	MOVZWL	#1,R0			; Force success
                                 04  20DF 18300 	RET				;
            56       DF1C CF     9E  20E0 18400 20$:	MOVAB	BOO$AB_PRMBUF,R6	; Set address of parameter buffer
            59            10     D0  20E5 18500 	MOVL	#16,R9			; Set size of buffer
                          DF15'  30  20E8 18600 	BSBW	BOO$READFILE		; Read file content into parameter buffer
            EE            50     E9  20EB 18700 	BLBC	R0,10$			; Exit if error
            58       DF0E CF     9E  20EE 18800 	MOVAB	BOO$AB_PRMBUF,R8	; Init pointer to parameter buffer
            68            20     28  20F3 18900 	MOVC3	#32,(R8),EXE$GT_STARTUP	; Set startup command file name
                 00000000'EF         20F6       
            58            20     C0  20FB 19000 	ADDL	#32,R8			;  and advance buffer pointer
                          68     D5  20FE 19100 30$:	TSTL	(R8)			; Check for end of list
                          20     13  2100 19200 	BEQL	DONE			; Branch if yes
         10 AC            68     9A  2102 19300 	MOVZBL	(R8),TPA$L_TOKENCNT(AP)	; Set token count for search
         14 AC         01 A8     9E  2106 19400 	MOVAB	1(R8),TPA$L_TOKENPTR(AP); And address of string
            58            10     C0  210B 19500 	ADDL	#16,R8			; Advance to value
