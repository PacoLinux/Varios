                                     0070  9400 ;++
                                     0070  9500 ; FUNCTIONAL DESCRIPTION:
                                     0070  9600 ;
                                     0070  9700 ;	This routine will load a device driver into the system pool
                                     0070  9800 ;	and if the same name driver was already loaded, it will
                                     0070  9900 ;	reinitialize all the control-blocks with info from the new
                                     0070 10000 ;	driver and unload the old driver.
                                     0070 10100 ;
                                     0070 10200 ; CALLING SEQUENCE:
                                     0070 10300 ;
                                     0070 10400 ;	CALL IOGEN$LOAD(FILE_NAME)
                                     0070 10500 ;
                                     0070 10600 ; INPUT PARAMETERS:
                                     0070 10700 ;
                                     0070 10800 ;	FILE_NAME(AP) = address of file name counted string
                                     0070 10900 ;
                                     0070 11000 ; IMPLICIT INPUTS:
                                     0070 11100 ;
                                     0070 11200 ;	NONE
                                     0070 11300 ;
                                     0070 11400 ; OUTPUT PARAMETERS:
                                     0070 11500 ;
                                     0070 11600 ;	IOGEN$GL_DPT = ADDR OF NEW DRIVER PROLOGUE TABLE
                                     0070 11700 ;
                                     0070 11800 ; IMPLICIT OUTPUTS:
                                     0070 11900 ;
                                     0070 12000 ;	NONE
                                     0070 12100 ;
                                     0070 12200 ; COMPLETION CODES:
                                     0070 12300 ;
                                     0070 12400 ;	R0 = STATUS OF LOAD OPERATION
                                     0070 12500 ;
                                     0070 12600 ; SIDE EFFECTS:
                                     0070 12700 ;
                                     0070 12800 ;	NONE
                                     0070 12900 ;
                                     0070 13000 ;--
                                 00000000 13100 	.PSECT	IOGEN$CODE	RD,EXE,NOWRT
                                     0000 13200 
                                     0000 13300 IOGEN$LOADDRIV::
                               0000  0000 13400 	.WORD	0
                                     0002 13500 ;
                                     0002 13600 ; OPEN THE DRIVER FILE FOR INPUT
                                     0002 13700 ;
            50         04 AC     D0  0002 13800 	MOVL	FILE_NAME(AP),R0	;GET ADDR OF FILE NAME STRING
            51       0000'CF     DE  0006 13900 	MOVAL	W^IN_FAB,R1		;GET ADDR OF INPUT FAB
         34 A1            80     90  000B 14000 	MOVB	(R0)+,FAB$B_FNS(R1)	;SET FILE NAME SIZE IN FAB
         2C A1            60     9E  000F 14100 	MOVAB	(R0),FAB$L_FNA(R1)	;SET FILE NAME ADDRESS IN FAB
                                     0013 14200 	$OPEN	(R1)			;OPEN THE FILE
            78            50     E9  001C 14300 	BLBC	R0,70$			;BR IF ERROR
                                     001F 14400 ;
                                     001F 14500 ; MAP THE INPUT FILE
                                     001F 14600 ;
