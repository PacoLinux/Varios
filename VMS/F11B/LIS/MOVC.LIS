                                     0100    78 ;++
                                     0100    79 ;
                                     0100    80 ; FUNCTIONAL DESCRIPTION:
                                     0100    81 ;
                                     0100    82 ;	THIS ROUTINE SIMULATES THE MOVC3 AND MOVC5 INSTRUCTIONS.
                                     0100    83 ;
                                     0100    84 ; CALLING SEQUENCE:
                                     0100    85 ;	CALL	FCP$MOVE (ARG1, ARG2, ARG3)
                                     0100    86 ;	CALL	FCP$COPY (ARG1, ARG2, ARG3, ARG4, ARG5)
                                     0100    87 ;
                                     0100    88 ; INPUT PARAMETERS:
                                     0100    89 ;	ARG1: SOURCE BYTE COUNT
                                     0100    90 ;	ARG2: SOURCE ADDRESS
                                     0100    91 ;	ARG3: DESTINATION FOR FCP$MOVE OR FILL FOR FCP$COPY
                                     0100    92 ;	ARG4: DESTINATION BYTE COUNT
                                     0100    93 ;	ARG5: DESTINATION ADDRESS
                                     0100    94 ;
                                     0100    95 ; IMPLICIT INPUTS:
                                     0100    96 ;	NONE
                                     0100    97 ;
                                     0100    98 ; OUTPUT PARAMETERS:
                                     0100    99 ;	NONE
                                     0100   100 ;
                                     0100   101 ; IMPLICIT OUTPUTS:
                                     0100   102 ;	NONE
                                     0100   103 ;
                                     0100   104 ; ROUTINE VALUE:
                                     0100   105 ;	R0 - R1: EXPECTED OUTPUT OF MOVC
                                     0100   106 ;
                                     0100   107 ; SIDE EFFECTS:
                                     0100   108 ;	NONE
                                     0100   109 ;
                                     0100   110 ;--
                                     0100   111 
                                     0100   112 	.ENABL	LSB
                                     0100   113 
                                     0100   114 FCP$MOVE::
                               003C  0100   115 	.WORD	^M<R2,R3,R4,R5>
         08 BC         04 AC     2E  0102   116 	MOVTC	ARG1(AP),@ARG2(AP),#0,NULL_TABLE,ARG1(AP),@ARG3(AP)
       FEF5 CF            00         0107       
         0C BC         04 AC         010B       
                                 04  010F   117 	RET
                                     0110   118 
                                     0110   119 FCP$COPY::
                               003C  0110   120 	.WORD	^M<R2,R3,R4,R5>
         08 BC         04 AC     2E  0112   121 	MOVTC	ARG1(AP),@ARG2(AP),ARG3(AP),NULL_TABLE,ARG4(AP),@ARG5(AP)
       FEE4 CF         0C AC         0117       
         14 BC         10 AC         011C       
                                 04  0120   122 	RET
                                     0121   123 
                                     0121   124 	.DSABL	LSB
                                     0121   125 
                                     0121   126 
                                     0121   127 
                                     0121   128 	.END
