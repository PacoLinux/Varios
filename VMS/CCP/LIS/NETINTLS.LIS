                                 00000000  6800 	.PSECT	NET_CODE,NOWRT
                                     0000  6900 	.SBTTL	LINK SERVICES RCVD FOR EXISTING LINK
                                     0000  7000 ;++
                                     0000  7100 ; FUNCTIONAL DESCRIPTION:
                                     0000  7200 ;
                                     0000  7300 ; NET$RCV_LNKSRV - A LINK SERVICES MSG HAS BEEN RCVD FOR AN EXISTING LINK.
                                     0000  7400 ;		 THE SEGMENT AND MESSAGE REQUEST COUNTS ARE UPDATED,
                                     0000  7500 ;		 AND SEGMENT TRANSMISSION OCCURS, IF NECESSARY.
                                     0000  7600 ;
                                     0000  7700 ; CALLING SEQUENCE:
                                     0000  7800 ;
                                     0000  7900 ;	ENTERED VIA CASE IN NETTRN
                                     0000  8000 ;
                                     0000  8100 ; INPUT PARAMETERS:
                                     0000  8200 ;
                                     0000  8300 ;	R5 - UCB ADDRESS
                                     0000  8400 ;
                                     0000  8500 ; IMPLICIT INPUTS:
                                     0000  8600 ;
                                     0000  8700 ;	R10 - PTR TO ACKNUM OR SEGNUM FIELD
                                     0000  8800 ;	R11 - PTR BEYOND END OF MSG
                                     0000  8900 ;
                                     0000  9000 ; SIDE EFFECTS:
                                     0000  9100 ;
                                     0000  9200 ;	SEGMENTS MAY BE TRANSMITTED TO PARTNER
                                     0000  9300 ;
                                     0000  9400 ;--
                                     0000  9500 
                                     0000  9600 NET$RCV_LNKSRV::
                          67     10  0000  9700 	BSBB	CHECK_INTLS		; PROCESS PREAMBLE AND REXMIT
                                     0002  9800 ;
                                     0002  9900 ; R2 CONTAINS ADDR OF INT/LS SSB AT THIS POINT
                                     0002 10000 ;
                                     0002 10100 ; PROCESS REMAINDER OF MESSAGE
                                     0002 10200 ;
                                     0002 10300 	BMSGOV	LNKSRV_ERROR,2		; NEED LSFLAGS BYTE AND FCVAL BYTE
            50            8A     9A  000A 10400 	MOVZBL	(R10)+,R0		; GET LSFLAGS
                                     000D 10500 ;
                                     000D 10600 ; CHECK FOR LEGAL VALUES
                                     000D 10700 ;
                                     000D 10800 
                                     000D 10900 	ASSUME	NET$V_REMLFS EQ 4
                                     000D 11000 	ASSUME	NET$S_REMLFS EQ 4
                                     000D 11100 
            50         F0 8F     93  000D 11200 	BITB	#^C<^XF>,R0		; ARE RESERVED BITS CLEAR?
                          38     12  0011 11300 	BNEQ	LNKSRV_ERROR		; IF NEQ NO
                                     0013 11400 
                                     0013 11500 	ASSUME 	NET$C_DATREQ EQ 0
                                     0013 11600 	ASSUME	NET$C_INTREQ EQ 1
                                     0013 11700 	ASSUME	NET$S_FCINT EQ 2
                                     0013 11800 
            51            8A     90  0013 11900 	MOVB	(R10)+,R1		; GET COUNT AND SET CONDITION CODES
            50            03     E0  0016 12000 	BBS	#NET$V_FCINT+1,R0,LNKSRV_ERROR ; IF BS ILLEGAL REQ CNT TYPE
                          31         0019       
            50            02     E0  001A 12100 	BBS	#NET$V_FCINT,R0,35$	; IF BS INTERRUPT REQUEST COUNTS						; FALL THROUGH FOR DA
                          12         001D       
            52         74 A5     D0  001E 12200 	MOVL	UCB$L_NT_DATSSB(R5),R2	; SET SSB ADDRESS FOR DATA SUBCHANNEL
NETINTLS        - INT/LS SUBCHANNEL PROCESSING                   22-AUG-1978 00:55:44   VAX-11 MACRO X0.3-11               Page   5
X01             LINK SERVICES RCVD FOR EXISTING LINK                                                                             (1)

                          51     95  0022 12300 	TSTB	R1			; CHECK COUNT
                          1E     13  0024 12400 	BEQL	60$			; IF EQL PROCEED
         3D A5            02     E0  0026 12500 	BBS	#UCB$V_SEGFLO,UCB$B_REMSRV(R5),40$ ; IF BS SEG FLOW - PROCEED
                          07         002A       
         3D A5            03     E1  002B 12600 	BBC	#UCB$V_MSGFLO,UCB$B_REMSRV(R5),50$ ; IF BC NO FLOW - ILLEGAL
                          0F         002F       
                                     0030 12700 					; FALL THROUGH TO CHECK FOR NEG COUNTS
                                     0030 12800 35$:
                          0D     19  0030 12900 	BLSS	50$			; IF LSS ILLEGAL
         22 A2            51     80  0032 13000 40$:	ADDB	R1,SSB$B_REQCNT(R2)	; UPDATE THE REQUEST COUNT
                          07     1D  0036 13100 	BVS	50$			; IF VS COUNT IS NG
         80 8F         22 A2     91  0038 13200 	CMPB	SSB$B_REQCNT(R2),#-128	; UNDERFLOW?
                          05     12  003D 13300 	BNEQ	60$			; IF NEQ NO - PROCEED
                                     003F 13400 50$:
            58            18     B0  003F 13500 	MOVW	#NET$C_DR_BADFC,R8	; ILLEGAL FCVAL FOR MSG TYPE
                          3B     11  0042 13600 	BRB	INTLS_TERMIN		; TERMINATE THE LINK
                                     0044 13700 ;
                                     0044 13800 ; DO THE RIGHT THING BY THE BACKPRESSURE BITS
                                     0044 13900 ;
                                     0044 14000 
                                     0044 14100 	ASSUME	NET$V_FCMOD EQ 0
                                     0044 14200 	ASSUME	NET$S_FCMOD EQ 2
                                     0044 14300 
                                     0044 14400 60$:
            0D            50     E9  0044 14500 	BLBC	R0,CHECK_FCMOD		; IF BC PROBABLY NO CHANGE
            50            01     E1  0047 14600 	BBC	#1,R0,STOP_FLOW		; IF BC STOP FLOW
                          03         004A       
                                     004B 14700 					; FALL THROUGH IF ILLEGAL VALUE
                                     004B 14800 ;
                                     004B 14900 ; MESSAGE WAS TOO SHORT OR USED A RESERVED BIT OR VALUE
                                     004B 15000 ;
                                     004B 15100 LNKSRV_ERROR:
                          FFB2'  31  004B 15200 	BRW	NET$PROT_ERROR		; PROTOCOL ERROR
                                     004E 15300 STOP_FLOW:
         3D A5            20     88  004E 15400 	BISB	#UCB$M_BACKP,UCB$B_REMSRV(R5) ; STOP FLOW
                          08     11  0052 15500 	BRB	NO_CHANGE		; FINISH UP
                                     0054 15600 CHECK_FCMOD:
            50            01     E1  0054 15700 	BBC	#1,R0,NO_CHANGE		; IF BC NO CHANGE
                          04         0057       
                                     0058 15800 					; FALL THROUGH TO START FLOW
                                     0058 15900 START_FLOW:
         3D A5            20     8A  0058 16000 	BICB	#UCB$M_BACKP,UCB$B_REMSRV(R5) ; START FLOW
                                     005C 16100 NO_CHANGE:
            52         78 A5     D0  005C 16200 	MOVL	UCB$L_NT_INTSSB(R5),R2	; GET SSB ADDRESS
                          FF9D'  30  0060 16300 	BSBW	NET$XMT_ACK		; TRY TO SEND AN ACK
                          FF9A'  30  0063 16400 	BSBW	NET$USRXMT		; TRY TO TRANSMIT FOR THE USER
                                     0066 16500 INTLS_EXIT:
                          FF97'  31  0066 16600 	BRW	NET$AST_EXIT		; GO TO COMPLETION CODE
NETINTLS        - INT/LS SUBCHANNEL PROCESSING                   22-AUG-1978 00:55:44   VAX-11 MACRO X0.3-11               Page   6
X01             LINK SERVICES RCVD FOR EXISTING LINK                                                                             (1)

                                     0069 16800 ;
                                     0069 16900 ; THIS ROUTINE PROCESSES THE INITIAL PART OF MESSAGES RECEIVED
                                     0069 17000 ; ON THE INTERRUPT/LINK SERVICES SUBCHANNEL.
                                     0069 17100 ;
                                     0069 17200 ;	OUTPUTS
                                     0069 17300 ;		R2 - INT/LS SSB ADDRESS
                                     0069 17400 CHECK_INTLS:
            52         78 A5     D0  0069 17500 	MOVL	UCB$L_NT_INTSSB(R5),R2	; POINT TO I/LS SUBCHAN BASE
                          FF90'  31  006D 17600 	BRW	NET$PRECHK		; CHECK MSG PREAMBLE AND RETURN
NETINTLS        - INT/LS SUBCHANNEL PROCESSING                   22-AUG-1978 00:55:44   VAX-11 MACRO X0.3-11               Page   7
X01             PROCESS INBOUND INTERRUPT MESSAGE                                                                                (1)

                                     0070 17800 	.SBTTL	PROCESS INBOUND INTERRUPT MESSAGE
                                     0070 17900 ;++
                                     0070 18000 ; FUNCTIONAL DESCRIPTION:
                                     0070 18100 ;
                                     0070 18200 ; NET$RCV_INTMSG - PROCESS RECEIVED INTERRUPT MESSAGE.
                                     0070 18300 ;	1. PROCESS MESSAGES PREAMBLE
                                     0070 18400 ;	2. IF NO INT MSG WAS SOLICITED, BREAK THE LOGICAL LINK
                                     0070 18500 ;	3. IF THERE IS NO MAILBOX, ACK THE SEGMENT
                                     0070 18600 ;		ELSE:
                                     0070 18700 ;		A.DELIVER THE INTERRUPT MSG TO THE USER'S MAILBOX
                                     0070 18800 ;		B. SOLICIT ANOTHER INTMSG, PIGGYBACK ACK
                                     0070 18900 ;
                                     0070 19000 ; NET$DATA_LOSS - ENTRY FOR TERMINATING LINK DUE TO RECEIVE COUNTER TO FLOW CONTROL
                                     0070 19100 ;
                                     0070 19200 ; CALLING SEQUENCE:
                                     0070 19300 ;
                                     0070 19400 ;	ENTERED VIA CASE IN NET$TRN
                                     0070 19500 ;
                                     0070 19600 ; INPUT PARAMETERS:
                                     0070 19700 ;
                                     0070 19800 ;	R5 - UCB ADDRESS
                                     0070 19900 ;
                                     0070 20000 ; SIDE EFFECTS:
                                     0070 20100 ;
                                     0070 20200 ;	INTERRUPT MESSAGE DELIVERED, IF USER HAS MAILBOX
                                     0070 20300 ;	ANOTHER INT MSG SOLICITED IF USER HAS MAILBOX
                                     0070 20400 ;
                                     0070 20500 ;--
                                     0070 20600 	.ENABL	LSB
                                     0070 20700 
                                     0070 20800 NET$RCV_INTMSG::
                          F7     10  0070 20900 	BSBB	CHECK_INTLS		; DO INITIAL PROCESSING
                                     0072 21000 ;
                                     0072 21100 ; R2 CONTAINS ADDR OF I/LS SSB AT THIS POINT
                                     0072 21200 ;
                       23 A2     97  0072 21300 	DECB	SSB$B_PNDCNT(R2)	; AM I EXPECTING AN INTMSG ON THIS LINK?
                          0B     1E  0075 21400 	BGEQU	10$			; IF GEQU YES
                                     0077 21500 NET$DATA_LOSS::
         5A A5            05     E0  0077 21600 	BBS	#UCB$V_NT_BREAK,UCB$W_DEVSTS(R5),INTLS_EXIT ; IF BS LINK IS INACTIVE
                          EA         007B       
            58            28     B0  007C 21700 	MOVW	#NET$C_DR_LOSS,R8	; DATA LOSS
                                     007F 21800 INTLS_TERMIN:
                          FF7E'  31  007F 21900 	BRW	NET$TERM_LINK		; TERMINATE THE LINK
                                     0082 22000 ;
                                     0082 22100 ; INTERRUPT MESSAGE WAS SOLICITED (POSSIBLY BY VIRTUE OF CONNECT)
                                     0082 22200 ;
                                     0082 22300 10$:
                       54 A5     D5  0082 22400 	TSTL	UCB$L_AMB(R5)		; IS THERE A MAILBOX?
                          05     12  0085 22500 	BNEQ	20$			; IF NEQ YES
                                     0087 22600 ;
                                     0087 22700 ; THERE IS NO MAILBOX - TOSS THE MESSAGE, ACK IT, AND DON'T SOLICIT ANOTHER
                                     0087 22800 ;
                          FF76'  30  0087 22900 	BSBW	NET$XMT_ACK		; SEND THE ACK
                          DA     11  008A 23000 	BRB	INTLS_EXIT		; FINISH IN COMMON CODE
                                     008C 23100 ;
                                     008C 23200 ; INTERRUPT MESSAGE IS DELIVERABLE
                                     008C 23300 ;
NETINTLS        - INT/LS SUBCHANNEL PROCESSING                   22-AUG-1978 00:55:44   VAX-11 MACRO X0.3-11               Page   8
X01             PROCESS INBOUND INTERRUPT MESSAGE                                                                                (1)

                                     008C 23400 20$:
         1A A2            0F     E2  008C 23500 	BBSS	#SSB$V_ACKP,SSB$W_RCVSEGACK(R2),30$ ; FORCE PIGGYBACKED ACK
                          00         0090       
                                     0091 23600 30$:
            57            35     3C  0091 23700 	MOVZWL	#MSG$_INTMSG,R7		; SET ATTENTION CODE
            5B            5A     C3  0094 23800 	SUBL3	R10,R11,R8		; COMPUTE NO. OF BYTES OF DATA
                          58         0097       
            00'           58     D1  0098 23900 	CMPL	R8,S^#NET$C_MAXINT	; TOO MUCH DATA?
                          AE     1A  009B 24000 	BGTRU	LNKSRV_ERROR		; IF GTRU YES
                          FF60'  30  009D 24100 	BSBW	NET$ATTENTION		; SEND THE ATTENTION
            52         78 A5     D0  00A0 24200 	MOVL	UCB$L_NT_INTSSB(R5),R2	; GET THE SSB ADDRESS
                                     00A4 24300 ;
                                     00A4 24400 ; SOLICIT ANOTHER INT MSG
                                     00A4 24500 ;
                       21 A2     96  00A4 24600 	INCB	SSB$B_FCVAL(R2)		; SOLICIT AN INT MSG
                          FF56'  30  00A7 24700 	BSBW	NET$XMT_LNKSRV		; SEND A LINK SERVICES MSG CUM ACK
                                     00AA 24750 	UPDATE	MSGRCV			; ANOTHER MESSAGE RECEIVED
                          B2     11  00B2 24800 	BRB	INTLS_EXIT		; COMPLETE PROCESSING
                                     00B4 24900 
                                     00B4 25000 	.DSABL	LSB
                                     00B4 25100  
                                     00B4 25200 	.END
