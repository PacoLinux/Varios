X01             DECLARATIONS                                                                                                     (1)

                                 00000000 15400 	.PSECT	NET_CONTROL,NOWRT,EXE
                                     0000 15500 	.SBTTL	NETWORK OPERATOR MESSAGE FORMATTING
                                     0000 15600 ;++
                                     0000 15700 ; FUNCTIONAL DESCRIPTION:
                                     0000 15800 ;
                                     0000 15900 ; NET$OPCOM_ERROR - ERROR STARTING LINE
                                     0000 16000 ; NET$OPCOM_NODE - MESSAGE IS FOR LOCAL NODE, NOT A LINE
                                     0000 16100 ; NET$OPCOM - FORMAT AND SEND OPERATOR MESSAGE ABOUT A LINE
                                     0000 16200 ;
                                     0000 16300 ; CALLING SEQUENCE:
                                     0000 16400 ;
                                     0000 16500 ;	BSB	NET$OPCOM, ETC.
                                     0000 16600 ;
                                     0000 16700 ; INPUT PARAMETERS:
                                     0000 16800 ;
                                     0000 16900 ;	R0 - ERROR CODE (NET$OPCOM_ERROR)
                                     0000 17000 ;	R6 - MESSAGE CODE
                                     0000 17100 ;	R9 - NDB ADDRESS
                                     0000 17200 ;
                                     0000 17300 ; SIDE EFFECTS:
                                     0000 17400 ;
                                     0000 17500 ;	MESSAGE SENT TO NETWORK OPERATOR
                                     0000 17600 ;
                                     0000 17700 ;--
                                     0000 17800 
                                     0000 17900 NET$OPCOM_ERROR::
            51       00BD'CF     9E  0000 18000 	MOVAB	W^ERROR_DESC,R1		; GET ADDRESS OF CONTROL STRING
       00D1'CF         04 A9     D0  0005 18100 	MOVL	NDB$L_LINE_NAME(R9),W^LINE_NAME1 ; STUFF THE LINE NAME
                          18     11  000B 18200 	BRB	COMMON1			; GO TO COMMON CODE
                                     000D 18300 
                                     000D 18400 NET$OPCOM_NODE::
            51       00AB'CF     9E  000D 18500 	MOVAB	W^NODE_DESC,R1		; GET ADDRESS OF CONTROL STRING
                          0B     11  0012 18600 	BRB	COMMON			; GO TO COMMON CODE
                                     0014 18700 NET$OPCOM::
       00A2'CF         04 A9     D0  0014 18800 	MOVL	NDB$L_LINE_NAME(R9),W^LINE_NAME ; STUFF THE LINE NAME
            51       008E'CF     9E  001A 18900 	MOVAB	W^LINE_DESC,R1		; GET ADDRESS OF CONTROL STRING
                                     001F 19000 COMMON:
            50       00BF'CF46   D0  001F 19100 	MOVL	W^MSG_ARRAY-4[R6],R0	; GET 1-ORIGIN ADDRESS OF TEXT STRING
                                     0025 19200 COMMON1:
                          3C     BB  0025 19300 	PUSHR	#^M<R2,R3,R4,R5>	; SAVE REGS
       0086'CF         50 8F     90  0027 19400 	MOVB	#TEXT_LTH,W^TEXT_DESC	; RESET THE OUTPUT BFR LTH
                                     002D 19500 	$FAO_S	CTRSTR=(R1),-		; FORMAT THE BUFFER
                                     002D 19600 		OUTLEN=W^TEXT_DESC,-	; STUFF THE BUFFER DESCRIPTOR
                                     002D 19700 		OUTBUF=W^TEXT_DESC,-	;
                                     002D 19800 		P1=R0			; THIS IS THE PARAMETER
                     0086'CF     DF  0040 19900 	PUSHAL	W^TEXT_DESC		; SET UP FOR CALL
   00000000'EF            01     FB  0044 20000 	CALLS	#1,EXE$NETSNDERL	; SEND THE MSG TO THE ERROR LOGGER
            55   00000000'EF     9E  004B 20100 	MOVAB	L^SYS$GL_OPRMBX,R5	; GET MAILBOX UCB ADDRESS
       0086'CF            2E'    C1  0052 20200 	ADDL3	S^#TEXT-MSG_BUFFER,W^TEXT_DESC,R3 ; LTH OF MBX MSG
                          53         0057       
            54       0008'CF     9E  0058 20300 	MOVAB	W^MSG_BUFFER,R4		; BUFFER ADDRESS
                 00000000'EF     16  005D 20400 	JSB	L^EXE$WRTMAILBOX	; SEND THE MESSAGE
                          3C     BA  0063 20500 	POPR	#^M<R2,R3,R4,R5>	; RESTORE REGS
                                 05  0065 20600 	RSB				; DONE
                                     0066 20700 
                                     0066 20800 	.END
