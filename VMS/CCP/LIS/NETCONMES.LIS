                                     038B 57300 ; GET_FMT - THIS ROUTINE VALIDATES OBJECT TYPES AND FORMAT TYPES
                                     038B 57400 ;
                                     038B 57500 ; INPUT PARAMETERS:
                                     038B 57600 ;
                                     038B 57700 ;	R10 - MSG PTR
                                     038B 57800 ;
                                     038B 57900 ; OUTPUT PARAMETERS:
                                     038B 58000 ;
                                     038B 58100 ;	R0 - LBS=>SUCCESS, LBC=>FAILURE
                                     038B 58200 ;	R1 - FMT TYPE
                                     038B 58300 ;	R7 - OBJTYP
                                     038B 58400 ;	R8 - FORMAT ERROR CODE IF R0 LBC
                                     038B 58500 ;
                                     038B 58600 
                                     038B 58700 GET_FMT:
            58            05     B0  038B 58800 	MOVW	#NET$C_DR_FMT,R8	; ASSUME FORMAT ERROR
                          50     D4  038E 58900 	CLRL	R0			; ASSUME FAILURE
                                     0390 59000 	BMSGOV	100$,2			; SEE IF FORMAT AND OBJECT TYPE PRESENT
            51            8A     9A  0398 59100 	MOVZBL	(R10)+,R1		; MOVE IN THE FORMAT TYPE
                          13     19  039B 59200 	BLSS	100$			; IF LSS ILLEGAL FORMAT
                          07     12  039D 59300 	BNEQ	20$			; IF NEQ NOT OBJTYPE ONLY
            57            8A     9A  039F 59400 	MOVZBL	(R10)+,R7		; GET THE OBJECT TYPE
                          0C     13  03A2 59500 	BEQL	100$			; IF EQL NG - 0 MUST HAVE DESCR
                          08     11  03A4 59600 	BRB	30$			; SUCCESS
                                     03A6 59700 20$:
            02            51     91  03A6 59800 	CMPB	R1,#NET_MAXFMT		; VALID FORMAT?
                          05     1A  03A9 59900 	BGTRU	100$			; IF GTRU NO
            57            8A     9A  03AB 60000 	MOVZBL	(R10)+,R7		; GET THE OBJECT TYPE BYTE
                                     03AE 60100 30$:
                          50     D6  03AE 60200 	INCL	R0			; INDICATE SUCCESS
                                     03B0 60300 100$:
                                 05  03B0 60400 	RSB				; DONE
                                     03B1 60500 
                                     03B1 60600 ; GET_DESC - THIS ROUTINE VALIDATES A DESCRIPTOR FIELD
                                     03B1 60700 ;
                                     03B1 60800 ; INPUT PARAMETERS:
                                     03B1 60900 ;
                                     03B1 61000 ;	R1 - FMT TYPE
                                     03B1 61100 ;	R10 - MSG PTR
                                     03B1 61200 ;
                                     03B1 61300 ; OUTPUT PARAMETERS:
                                     03B1 61400 ;
                                     03B1 61500 ;	R0 - LBS=>SUCCESS
                                     03B1 61600 ;	R0 - LBC=>FAILURE
                                     03B1 61700 ;	R6 - NO. OF CHARS IN FIELD
                                     03B1 61800 ;	R10 - MAY BE UPDATED (PAST UIC FIELDS)
                                     03B1 61900 ;
                                     03B1 62000 
                                     03B1 62100 GET_DESC:
                          50     D4  03B1 62200 	CLRL	R0			; ASSUME FAILURE
            56            10     9A  03B3 62300 	MOVZBL	#NET_MAXFMTLTH,R6	; SET MAX LTH OF A FORMAT DESCRIPTOR
                          51     97  03B6 62400 	DECB	R1			; FORMAT 1?
                          0D     13  03B8 62500 	BEQL	30$			; IF EQL YES
                                     03BA 62600 	BMSGOV	100$,4			; NEED 4 BYTES OF GROUP/USER
            56            04     C2  03C2 62700 	SUBL	#4,R6			; 4 FEWER CHARS TO GET
                          8A     D5  03C5 62800 	TSTL	(R10)+			; MOVE PTR BY 4
                                     03C7 62900 30$:
