                                     0000  6800 ; NET$GET_NAMENUM - GET NODE NAME, GIVEN THE NUMBER
                                     0000  6900 ; NET$GET_NUMNAME - GET NODE NUMBER, GIVEN THE NAME
                                     0000  7000 ; NET$GET_VERSION - GET ROUTING, COMM, AND SYSTEM VERSION
                                     0000  7100 ; NET$GET_NODBYLN - GIVEN THE LINE NAME (XMA0), GET THE NODE NAME
                                     0000  7200 ; NET$DECLARE_NAM - MAKES A PROCESS KNOWN AS ABLE TO RECEIVE INBOUND CONNECTS
                                     0000  7300 ;
                                     0000  7400 ;--
                                     0000  7500 
                                     0000  7600 NET$GET_NAMENUM::
       0000'CF            6B     B0  0000  7700 	MOVW	(R11),W^NET$GW_CTLCOUNT	; PUT BACK THE PARAMETER
                     0000'CF     9F  0005  7800 	PUSHAB	W^NET$CTL_NEXTNDB	; INIT FOR COROUTINE CALL
            53       0000'CF     D0  0009  7900 	MOVL	W^NET$GL_RES_PTR,R3	; GET DESTINATION BFR ADDR
                                     000E  8000 10$:
                          9E     16  000E  8100 	JSB	@(SP)+			; GET ADDRESS OF NEXT NDB
                          02     12  0010  8200 	BNEQ	20$			; IF NEQ NODE FOUND
                          39     11  0012  8300 	BRB	NOSUCHNODE		; NODE NOT FOUND
                                     0014  8400 20$:
         18 A9            6B     B1  0014  8500 	CMPW	(R11),NDB$W_NODEADDR(R9) ; NUMBERS MATCH?
                          F4     12  0018  8600 	BNEQ	10$			; IF NEQ NO MATCH
                          FFE3'  30  001A  8700 	BSBW	NET$CTL_MOV_NAM		; MOVE IN THE NAME
                                 04  001D  8800 	RET				; DONE
                                     001E  8900 
                                     001E  9000 NET$GET_NUMNAME::
                          FFDF'  30  001E  9100 	BSBW	NET$CTL_FNDNDB		; GET THE RIGHT NDB
                          2A     13  0021  9200 	BEQL	NOSUCHNODE		; IF EQL NO NODE
       0000'CF         18 A9     B0  0023  9300 	MOVW	NDB$W_NODEADDR(R9),W^NET$GW_CTLCOUNT ; STUFF THE NUMBER FOR RETURN
                                 04  0029  9400 	RET				; DONE
                                     002A  9500 
                                     002A  9600 NET$GET_VERSION::
                          FFD3'  30  002A  9700 	BSBW	NET$CTL_FNDNDB		; GET THE NDB
                          1E     13  002D  9800 	BEQL	NOSUCHNODE		; IF EQL NO NODE
            51         28 A9     DE  002F  9900 	MOVAL	NDB$B_RTEVER(R9),R1	; POINT TO THE ROUTING VERSION
            53       0000'CF     D0  0033 10000 	MOVL	W^NET$GL_RES_PTR,R3	; GET POINTER TO RESULT BUFFER
            5A            06     3C  0038 10100 	MOVZWL	#6,R10			; NEED 6 BYTES FOR PROTOCOL VERSIONS
                          FFC2'  30  003B 10200 	BSBW	NET$CTL_UPD_CNT		; CHECK BUFFER
            83            81     D0  003E 10300 	MOVL	(R1)+,(R3)+		; STORE 6 BYTES OF VERSIONS
            83            81     B0  0041 10400 	MOVW	(R1)+,(R3)+		; THE OTHER 2 BYTES
                          FFB9'  30  0044 10500 	BSBW	NET$CTL_MOV_PTH		; MOVE IN THE SYSTEM VERSION STRING
                                 04  0047 10600 	RET				; DONE
                                     0048 10700 
                                     0048 10800 	.ENABL	LSB
                                     0048 10900 
                                     0048 11000 NET$GET_NODBYLN::
                          FFB5'  30  0048 11100 	BSBW	NET$CTL_NDBLINE		; FIND THE NDB
                          08     12  004B 11200 	BNEQ	20$			; IF NEQ OK
                                     004D 11300 NOSUCHNODE:
       0000'CF       0000'8F     3C  004D 11400 	MOVZWL	#SS$_NOSUCHNODE,W^NET$GQ_USR_STAT ; NO SUCH NODE
                                 04  0054 11500 	RET				; DONE
                                     0055 11600 20$:
            53       0000'CF     D0  0055 11700 	MOVL	W^NET$GL_RES_PTR,R3	; SET TO MOVE IN NAME
                          FFA3'  30  005A 11800 	BSBW	NET$CTL_MOV_NAM		; DO IT
