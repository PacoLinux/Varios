0001    	PROGRAM SYSMSG
        C MAIN PROGRAM FOR MESSAGE UTILITY
        C
        C THIS PROGRAM BUILDS THE FILE MESSAGES.SYS FOR USE BY THE
        C $GETMSG SYSTEM SERVICE AND THE OBJECT FILES CONTAINING
        C THE GLOBAL DEFINITIONS OF THE MESSAGE IDENTIFIERS.
        C
        C THIS PROGRAM ACCEPTS ITS INPUT IN AN MDL LIKE LANGUAGE
        C AND CAN EXPAND THE MDL C CONSTRUCT TO SIMPLIFY PROVIDING
        C THE APPROPRIATE INPUT TO MAKE THE $GETMSG SYSTEM
        C SERVICE WORK AND PROVIDE THE GLOBAL DEFINITIONS FOR THE
        C MESSAGE CODES (ID'S).
         
0002    	INCLUDE 'SRC$:NDATACOM.FOR'
0003 *  	IMPLICIT INTEGER*4 (A-Z)
0004 *  	PARAMETER FILSIZ=204800
0005 *  	LOGICAL*1 LINE(132),NAME(17),DEFAULT(17),PREFIX(17),IDNSTR(17)
0006 *  	LOGICAL*1 FSPEC(80)
0007 *  	INTEGER*4 IDS(20000)
0008 *  	INTEGER*4 LWORDS(FILSIZ/4)
0009 *  	INTEGER*2 WORDS(FILSIZ/2)
0010 *  	LOGICAL*1 BYTES(FILSIZ)
0011 *  	LOGICAL*1 IDFLAG,OUTFLAG
0012 *  	INTEGER*4 INDEX2(128,50)
0013 *  	INTEGER*4 INDEX1(125)
0014 *  	COMMON /TEXT/ LINE,PREFIX,DEFAULT,NAME,IDNSTR,FSPEC
0015 *  	COMMON /VARS/ NUM,COL,INDEX,PASS,TPTR,CODE,ERRCNT,BINVAL,
     *  	1 SIZIDX,SIZSUBS
0016 *  	COMMON /LOGVAR/ IDFLAG,OUTFLAG
0017 *  	COMMON /IDS/ IDS
0018 *  	COMMON /MAPPED/ LWORDS
0019 *  	EQUIVALENCE (LWORDS,WORDS,BYTES)
0020 *  	EQUIVALENCE (LANGTAB,LWORDS(1))
0021 *  	EQUIVALENCE (SUBPTR,LWORDS(2))
0022 *  	EQUIVALENCE (INDEX1,LWORDS(3))
0023 *  	EQUIVALENCE (INDEX2,LWORDS(129))
0024    	LOGICAL*1 GETNUM,GETIDENT,IDENT,UNBLNK,GETLIN,NEXTFILE
         
        C START BY PROCESSING THE SYSTEM WIDE MESSAGES AND CODES
        C THIS INCLUDES ALL THE SS$_ CODES AND THEIR MESSAGES AND
        C WHATEVER OTHER 'SYSTEM WIDE' ERRORS THAT ARE PROVIDED.
        C THE INPUT FILE IS SYSMSG.MSG AND ITS CORRESPONDING OUTPUT
        C IS SYSMSG.OBJ.
         
        C THIS OPENS THE NAMED FILE AS SEQUENTIAL INPUT ON UNIT 1
         
0025    	PASS=1
0026    	INDEX=0
0027    	LANGTAB=0
0028    	IDFLAG=.TRUE.
0029    50	CALL SETUPFILE
0030    	ERRCNT=0
         
0031    60	IF (NEXTFILE().EQ..FALSE.) GOTO 2000
0032    70	DEFAULT(1)=0
0033    	GOTO 100

SYSMSG          21-Aug-1978 21:29:33	VAX-11 FORTRAN IV-PLUS V1.0-2      Page 2   
					SYSMSG.FOR.49       

         
0034    90	CALL ERROR(1,SYNTAX)
0035    100	IF (GETLIN().EQ..FALSE.) GOTO 60
0036    	IF (UNBLNK().EQ.';') GOTO 100
0037    	IF (GETIDENT().EQ..FALSE.) GOTO 90
0038    	IF (IDENT(%REF('$STRUCT'),NAME)) GOTO 200
0039    	IF (IDENT(%REF('C'),NAME)) GOTO 300
0040    	IF (IDENT(%REF('K'),NAME)) GOTO 300
0041    	IF (IDENT(%REF('V'),NAME)) GOTO 400
0042    	IF (IDENT(%REF('F'),NAME)) GOTO 100
0043    	IF (IDENT(%REF('S'),NAME)) GOTO 100
0044    	IF (IDENT(%REF('L'),NAME)) GOTO 100
0045    	IF (IDENT(%REF('M'),NAME)) GOTO 100
0046    	IF (IDENT(%REF('P'),NAME)) GOTO 100
0047    	GOTO 90
         
0048    200	CALL STRUCT
0049    	GOTO 70
         
0050    300	CALL EVALC
0051    	GOTO 100
         
0052    400	CALL SKIPV
0053    	GOTO 100
         
0054    2000	PASS=PASS+1
0055    	IF (PASS.EQ.3) GOTO 2100
0056    	CLOSE (UNIT=3)
0057    	TYPE 99,INDEX
0058    99	FORMAT(' %SYSMSG-I-number of messages in message file:',I5)
0059    	IDS(INDEX+1)=0
0060    	CALL SHELLSORT
        C	TYPE 98,(IDS(K),K=1,INDEX)
        C98	FORMAT(1X,I10)
0061    	CALL MAKINDX
0062    	SUBPTR=(SIZIDX+1)*512
0063    	CALL MAKSUBS
0064    	TPTR=(SIZIDX+1+SIZSUBS)*512+1
0065    	GOTO 50
0066    2100	OPEN (UNIT=1,NAME='EXE$:SYSMSG.MPF',TYPE='NEW',ACCESS='DIRECT',
        	1 RECORDSIZE=128)
0067    	SIZSUBS=SIZSUBS+SIZIDX
0068    	DO 2200 I=1,(TPTR+511)/512
        C	TYPE 97,I,TPTR
        C97	FORMAT(' BLOCK= ',I5,'  TOTAL BYTES =',I7)
0069    2200	WRITE(1'I) (LWORDS((I-1)*128+J),J=1,128)
0070    	TYPE 2299,ERRCNT
0071    2299	FORMAT(' %SYSMSG-I-errors detected:',I5)
0072    	CALL EXIT
0073    	END

