                                     0000    67 	.SBTTL RM$CONNECT2  -  RELATIVE-SPECIFIC CONNECT ROUTINE
                                     0000    68 ;++
                                     0000    69 ; FUNCTIONAL DESCRIPTION:
                                     0000    70 ;
                                     0000    71 ; THIS MODULE PERFORMS THE FOLLOWING FUNCTIONS REQUIRED FOR
                                     0000    72 ; CONNECTING TO RELATIVE FILES.
                                     0000    73 ;
                                     0000    74 ;	1.  PERFORMS VARIOUS VALIDITY CHECKS
                                     0000    75 ;	2.  IF CONNECT FOR BLOCK I/O ALLOCATE A BDB  AND EXIT
                                     0000    76 ;	3.  ALLOCATE BDB'S AND BUFFERS:
                                     0000    77 ;		1 MINIMUM, OTHERWISE SYSTEM DEFAULT
                                     0000    78 ;	4.  PERFORM VARIOUS IRAB INITIALIZATIONS
                                     0000    79 ;
                                     0000    80 ; CALLING SEQUENCE:
                                     0000    81 ;
                                     0000    82 ;	ENTERED VIA CASE BRANCH FROM RM$CONNECT
                                     0000    83 ;
                                     0000    84 ; INPUT PARAMETERS:
                                     0000    85 ;
                                     0000    86 ;	R11	IMPURE AREA ADDRESS
                                     0000    87 ;	R10	IFAB ADDRESS
                                     0000    88 ;	R9	IRAB ADDRESS
                                     0000    89 ;	R8	RAB ADDRESS
                                     0000    90 ;
                                     0000    91 ; IMPLICIT INPUTS:
                                     0000    92 ;
                                     0000    93 ; 	THE CONTENTS OF THE RAB AND IRAB
                                     0000    94 ;
                                     0000    95 ; OUTPUT PARAMETERS:
                                     0000    96 ;
                                     0000    97 ;	R0	STATUS
                                     0000    98 ;
                                     0000    99 ; IMPLICIT OUTPUTS:
                                     0000   100 ;
                                     0000   101 ;	SETS VARIOUS FIELDS IN THE IRAB AND IFAB.
                                     0000   102 ;
                                     0000   103 ; COMPLETION CODES:
                                     0000   104 ;
                                     0000   105 ;	THE STANDARD RMS STATUS CODE IS SET INTO R0 AND RETURN
                                     0000   106 ;	IS MADE TO USER (NOT CALLER) VIA RM$EXSUC (OR RM$EX_NOSTR
                                     0000   107 ;	AFTER CALLING RM$CCLN1 IF AN ERROR IS DETECTED, THUS 
                                     0000   108 ;	DEALLOCATING ALL IRAB - RELATED INTERNAL STRUCTURES).
                                     0000   109 ;
                                     0000   110 ; SIDE EFFECTS:
                                     0000   111 ;
                                     0000   112 ;	NONE
                                     0000   113 ;
                                     0000   114 ;--
RM2CONN         RELATIVE-SPECIFIC CONNECT                        21-AUG-1978 21:51:06   VAX-11 MACRO X0.3-11               Page   4
008             RM$CONNECT2  -  RELATIVE-SPECIFIC CONNECT ROUTI                                                                  (1)

                                     0000   116 RM$CONNECT2::
                                     0000   117 	$TSTPT CONNECT2
            68            28     E0  0006   118 	BBS	#RAB$V_EOF+ROP,(R8),ERRROP	; BRANCH IF EOF BIT SET IN ROP
                          64         0009       
                                     000A   119 ;
                                     000A   120 ;  IF OPEN OR CREATE WAS DONE WITH BRO SPECIFIED (MIXED BLOCK & RECORD I/O),
                                     000A   121 ;  USE THE BIO ROP BIT TO DETERMINE WHETHER TO CONNECT FOR BLOCK OR RECORD
                                     000A   122 ;  OPERATIONS.  NOTE: ANY SUBSEQUENT CONNECTS MUST BE OF THE SAME TYPE.
                                     000A   123 ;
         22 AA            06     E5  000A   124 	BBCC	#FAB$V_BRO,IFB$B_FAC(R10),5$	; BRANCH IF BRO NOT SPECIFIED
                          09         000E       
            68            2B     E1  000F   125 	BBC	#RAB$V_BIO+ROP,(R8),CHKRFM	; BRANCH IF BIO NOT WANTED
                          12         0012       
         22 AA            05     E3  0013   126 	BBCS	#FAB$V_BIO,IFB$B_FAC(R10),ALLBIO; SET BLOCK I/O AND BRANCH
                          05         0017       
         22 AA            05     E1  0018   127 5$:	BBC	#FAB$V_BIO,IFB$B_FAC(R10),CHKRFM; BRANCH IF NOT BLOCK I/O
                          08         001C       
                                     001D   128 ;++
                                     001D   129 ;
                                     001D   130 ; BLOCK I/O.  ALLOCATE A SINGLE BDB.
                                     001D   131 ;
                                     001D   132 ;--
                                     001D   133 RM$CONNECT_BIO::
                          FFE0'  30  001D   134 ALLBIO:	BSBW	RM$ALBDB			; ALLOCATE BDB
            3E            50     E8  0020   135 	BLBS	R0,SETNRP			; BRANCH IF O.K.
                          4C     11  0023   136 	BRB	CLEANUP				; CLEANUP ON ERROR
                                     0025   137 ;++
                                     0025   138 ;
                                     0025   139 ; RECORD I/O.  ALLOCATE BDB'S AND BUFFERS.
                                     0025   140 ;
                                     0025   141 ;--
                                     0025   142 	ASSUME	FAB$C_UDF EQ 0
                       3C AA     95  0025   143 CHKRFM:	TSTB	IFB$B_RFMORG(R10)		; RFM = UNDEFINED?
                          50     13  0028   144 	BEQL	ERRRFM				; BRANCH IF YES
            55         4A AA     9A  002A   145 	MOVZBL	IFB$B_BKS(R10),R5		; GET BUCKET SIZE IN VBNS
            55            09     9C  002E   146 	ROTL	#9,R5,R5			; AND CONVERT TO BYTES
                          55         0031       
            54            03     D0  0032   147 	MOVL	#3,R4				; INDEX FOR RELATIVE DEFAULTS
                          FFC8'  30  0035   148 	BSBW	RM$BDBALLOC			; GO ALLOCATE BDBS AND BUFFERS
            3C            50     E9  0038   149 	BLBC	R0,CLEANUP1			; GET OUT ON ERROR
                                     003B   150 ;
                                     003B   151 ; CALCULATE CELL SIZE FOR RECORDS
                                     003B   152 ;
            03         3C AA     91  003B   153 	CMPB	IFB$B_RFMORG(R10),#FAB$C_VFC	; VFC REC FORMAT?
                          05     12  003F   154 	BNEQ	60$				; BRANCH IF NOT
         42 A9         4B AA     90  0041   155 	MOVB	IFB$B_FSZ(R10),IRB$W_CSIZ(R9)	; YES-INITIALIZE RECORD SIZE
            01         3C AA     91  0046   156 60$:	CMPB	IFB$B_RFMORG(R10),#FAB$C_FIX	; FIXED REC. LEN?
                          04     13  004A   157 	BEQL	70$				; BRANCH IF YES
         42 A9            02     A0  004C   158 	ADDW2	#2,IRB$W_CSIZ(R9)		; ADD IN 2-BYTE SIZE FIELD
                       42 A9     B6  0050   159 70$:	INCW	IRB$W_CSIZ(R9)			; ADD IN CONTROL BYTE
         42 A9         4C AA     A0  0053   160 	ADDW2	IFB$W_MRS(R10),IRB$W_CSIZ(R9)	; & MAX RECORD SIZE
                                     0058   161 						; GIVING TOTAL CELL SIZE
         5C AA         42 A9     A7  0058   162 	DIVW3	IRB$W_CSIZ(R9),IFB$W_BKS_BYTES(R10),IFB$W_BKS_RECS(R10)
                       5E AA         005D       
                                     005F   163 						; COMPUTE # RECS/BKT
                          13     13  005F   164 	BEQL	ERRIFA				; BRANCH IF ZERO
                       2C A9     D6  0061   165 SETNRP:	INCL	IRB$L_NRP_VBN(R9)		; SET NRP TO VBN1 (RP = 0)
