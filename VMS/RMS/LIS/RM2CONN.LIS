            6A            3E     E0  0064   166 	BBS	#IFB$V_DAP,(R10),NTCONN		; BRANCH IF NETWORK FILE ACCESS
                          03         0067       
                          FF95'  31  0068   167 	BRW	RM$EXSUC			; EXIT WITH SUCCESS
                                     006B   168 ; ++
                                     006B   169 ; 
                                     006B   170 ;  PERFORM NETWORK CONNECT FUNCTION
                                     006B   171 ; 
                                     006B   172 ; --
                                     006B   173 NTCONN:	$PSECT_BR	NF$NETWORK
                          FFFD'  30  0000   174 	BSBW	NT$CONNECT2			; PERFORM CONNECT AT REMOTE NODE
            03            50     E9  0003   175 	BLBC	R0,10$				; BRANCH ON FAILURE
                          FFF7'  31  0006   176 	BRW	RM$EXSUC			; EXIT WITH SUCCESS
                          006B'  31  0009   177 10$:	BRW	CLEANUP1			; BRANCH AID
                                     000C   178 	$PSECT_RESTORE
RM2CONN         RELATIVE-SPECIFIC CONNECT                        21-AUG-1978 21:51:06   VAX-11 MACRO X0.3-11               Page   6
008             RM$CONNECT2  -  RELATIVE-SPECIFIC CONNECT ROUTI                                                                  (1)

                                     006E   180 ;++
                                     006E   181 ;
                                     006E   182 ; HANDLE ERRORS
                                     006E   183 ;
                                     006E   184 ;--
                                     006E   185 ;
                                     006E   186 ;  EOF ROP BIT SET BUT OPTION NOT YET IMPLEMENTED
                                     006E   187 ;
                                     006E   188 ERRROP:	$PSECT_BR	L$RMS2
                                     0000   189 	RMSERR	ROP				; INVALID RECORD OPTIONS
                                     0005   190 	$PSECT_RESTORE				; FALL THRU TO CLEANUP
                                     0071   191 ;
                                     0071   192 CLEANUP:
                                     0071   193 	$PSECT_BR	L$RMS2			; FAILED TO ALLOCATE BLOCK I/O BDB
                          FFF8'  30  0005   194 	BSBW	RM$CCLN1			; DEALLOCATE IRAB
                          FFF5'  31  0008   195 EXIT:	BRW	RM$EX_NOSTR			; AND EXIT
                                     000B   196 	$PSECT_RESTORE
                                     0074   197 ;
                                     0074   198 ;  CALCULATED THAT BUCKET HOLDS 0 RECORDS.  MUST BE BAD DATA IN FILE HEADER.
                                     0074   199 ;
                                     0074   200 ERRIFA:	$PSECT_BR	L$RMS2
         0C A8   000185D4 8F     D0  000B   201 	MOVL	#RMS$_MRS,RAB$L_STV(R8)		; STV SECONDARY ERROR CODE
                                     0013   202 	RMSERR	IFA				; ILLEGAL FILE ATTRIBUTES
                                     0018   203 						; (FALL THRU TO CLEANUP1)
                                     0018   204 	$PSECT_RESTORE
                                     0077   205 ;
                                     0077   206 ; FAILED TO ALLOCATE REQUESTED NUMBER OF BUFFERS 
                                     0077   207 ; (R0 ALREADY HAS FAILURE CODE)
                                     0077   208 ;
                                     0077   209 CLEANUP1:
                                     0077   210 	$PSECT_BR	L$RMS2	
                          FFE5'  30  0018   211 	BSBW		RM$COMCLNUP		; DEALLOCATE BDBS BUFFERS, & IRAB
                          EB     11  001B   212 	BRB		EXIT	
                                     001D   213 	$PSECT_RESTORE
                                     007A   214 ;
                                     007A   215 ;  UNDEFINED RECORD FORMAT FOR RECORD I/O
                                     007A   216 ;
                          FF83'  31  007A   217 ERRRFM:	BRW	RM$CONN_ERRRFM			; GO REPORT ERROR
                                     007D   218 
                                     007D   219 
                                     007D   220 	.END
