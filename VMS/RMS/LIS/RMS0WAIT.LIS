                                     0000    66 	.SBTTL	RMS$WAIT  -  $WAIT ROUTINE
                                     0000    67 ;++
                                     0000    68 ; FUNCTIONAL DESCRIPTION:
                                     0000    69 ;
                                     0000    70 ;  THIS MODULE PERFORMS THE FOLLOWING FUNCTIONS:
                                     0000    71 ;
                                     0000    72 ;	1. CHECKS FOR VALID IRAB (VIA ISI) AND IF NONE, ASSUMES DISCONNECT
                                     0000    73 ;	   WAS DONE,  THEREFORE LEAVES STS ALONE UNLESS IT IS ZERO, IN
                                     0000    74 ;	   WHICH CASE IT IS SET TO RMS$_SUC.
                                     0000    75 ;	2. CHECKS THAT THE RAB IS THE SAME ONE FOR LAST OPERATION,
                                     0000    76 ;	   AND IF NOT MERELY DECLARES SUCCESS IF NOT BUSY, ELSE IF BUSY SETS
                                     0000    77 ;	   FLAG IN R4 TO INDICATE DIFFERENT RAB AND CAUSES WAIT FOR SAME
                                     0000    78 ;	   EVENT FLAG.
                                     0000    79 ;	3. IF SAME RAB, TESTS STS FOR NON-ZERO, INDICATING COMPLETION.
                                     0000    80 ;	   IF ZERO, CHECKS IRAB BUSY BIT AND CAUSES WAIT IF BUSY, ELSE RETURNS
                                     0000    81 ;	   SUCCESS.
                                     0000    82 ;	4. IF STALL REQUIRED, EXITS RMS WITH STATUS = RMS$_STALL
                                     0000    83 ;
                                     0000    84 ; CALLING SEQUENCE:
                                     0000    85 ;
                                     0000    86 ;	ENTERED FROM EXEC AS A RESULT OF USER'S CALLING SYS$WAIT
                                     0000    87 ;	(E.G., BY USING THE $WAIT MACRO)
                                     0000    88 ;
                                     0000    89 ; INPUT PARAMETERS:
                                     0000    90 ;
                                     0000    91 ;	AP	USER'S ARGUMENT LIST ADDR
                                     0000    92 ;
                                     0000    93 ; IMPLICIT INPUTS:
                                     0000    94 ;
                                     0000    95 ;	THE CONTENTS OF THE RAB (STS) AND RELATED IRAB.
                                     0000    96 ;
                                     0000    97 ; OUTPUT PARAMETERS:
                                     0000    98 ;
                                     0000    99 ;	R1	DESTROYED
                                     0000   100 ;	R0	STATUS CODE
                                     0000   101 ;
                                     0000   102 ; IMPLICIT OUTPUTS:
                                     0000   103 ;
                                     0000   104 ;	THE IRAB IS UPDATED TO REFLECT THE STALL.
                                     0000   105 ;
                                     0000   106 ;	A COMPLETION AST IS QUEUED IF SPECIFIED IN THE ORIGINAL USER ARGLIST.
                                     0000   107 ;
                                     0000   108 ; COMPLETION CODES:
                                     0000   109 ;
                                     0000   110 ;	STANDARD RMS (SEE FUNCTIONAL SPEC FOR LIST) AS PER THE
                                     0000   111 ;	OPERATION WAITED UPON.
                                     0000   112 ;
                                     0000   113 ; SIDE EFFECTS:
                                     0000   114 ;
                                     0000   115 ;	NONE
                                     0000   116 ;
                                     0000   117 ;--
RMS0WAIT        WAIT FOR AYNC OPERATION COMPLETION               21-AUG-1978 22:13:58   VAX-11 MACRO X0.3-11               Page   4
005             RMS$WAIT  -  $WAIT ROUTINE                                                                                       (1)

                                     0000   119 	$ENTRY	RMS$WAIT
                                 00000000       	.PSECT	$RMS0,GBL,NOWRT
                                     0000   120 	$TSTPT	WAIT
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_WAIT
                          FFF7'  30  0006   121 	BSBW	RM$RABCHK			;VALID RAB? (EXITS IF BAD)
                                     0009   122 						;R11=IMAGE IMPURE AREA ADDR
                                     0009   123 						;R9=ISI, R8=RAB ADDR, R7=MODE
                                     0009   124 ;
                                     0009   125 ;  GET THE IRAB ADDR, CHECK FOR VALIDITY 
                                     0009   126 ;
            50            07     D0  0009   127 	MOVL	#IMP$L_IRABTBL/4,R0		;IRAB TABLE OFFSET/4
                          FFF1'  30  000C   128 	BSBW	RM$GTIADR			;GET IRAB ADDR
                          4F     13  000F   129 	BEQL	DISCONNECT			;BRANCH IF BAD (ASSUME DISCONNECT)
                           00000008  0011   130 	.IF NE $$RMSTEST&$$RMS_TBUGCHK
            0A         08 A9     91  0011   131 	CMPB	IRB$B_BID(R9),#IRB$C_BID
                          5C     12  0015   132 	BNEQ	ERRBUG				;BRANCH IF NOT A VALID IRAB
                                     0017   133 	.ENDC
                                     0017   134 ;
                                     0017   135 ;  ALSO CHECK THE STORED ARG LIST RAB FOR SAMENESS
                                     0017   136 ;
                          54     D4  0017   137 	CLRL	R4				;CLEAR FLAG ASSUMING SAME RAB
            55         18 A9     D0  0019   138 	MOVL	IRB$L_ARGLST(R9),R5		;GET OLD ARG LIST
                                     001D   139 	IFNORD	#8,(R5),DIFRAB,R7		;ASSUME DIFFERENT IF NOT READABLE
            08            57     0C  001D       		PROBER	R7,#8,(R5)
                          65         0020       
                          32     13  0021       		BEQL	DIFRAB
            58         04 A5     D1  0023   140 	CMPL	4(R5),R8			;SAME RAB?
                          2C     12  0027   141 	BNEQ	DIFRAB				;BRANCH IF NOT
                                     0029   142 ;
                                     0029   143 ;  SAME RAB.  IF STS HAS A NON-ZERO VALUE IMPLYING OPERATION DONE, JUST EXIT.
                                     0029   144 ;
            50         08 A8     D0  0029   145 	MOVL	RAB$L_STS(R8),R0		;PICK UP STATUS
                          41     12  002D   146 	BNEQ	EXIT				;BRANCH IF STATUS INDICATES DONE
            69            20     E1  002F   147 	BBC	#IRB$V_BUSY,(R9),SUCXIT		;ALL SET IF NOT BUSY
                          3A         0032       
                                     0033   148 	ASSUME	IRB$V_ASYNC EQ IFB$V_ASYNC
            69            23     E1  0033   149 CHKASY:	BBC	#IRB$V_ASYNC,(R9),SYNC_STALL	;BRANCH IF OPERATION SYNCHRONOUS
                          18         0036       
                                     0037   150 	SSB	#IRB$V_ASYNCWAIT,(R9)		;INDICATE SET EVENT FLAG REQUIRED
            69            24     E2  0037       		BBSS	#IRB$V_ASYNCWAIT,(R9),30000$
                          00         003A       
                                     003B       30000$:
            53            1E     9A  003B   151 	MOVZBL	#IMP$C_ASYEFN,R3		;SET EFN TO WAIT ON
                                     003E   152 	$CLREF_S	R3			;CLEAR IT
                          53     DD  003E       		PUSHL	R3
   00000000'GF            01     FB  0040       		CALLS	#1,G^SYS$CLREF
                                     0047   153 SETSTALL:
                                     0047   154 	RMSSUC	STALL,AP			;GET SET TO STALL
            5C       8001 8F     3C  0047       		MOVZWL	#<RMS$_STALL&^XFFFF>,AP
                          FFB1'  31  004C   155 	BRW	RM$ENBAST			;GO RE-ENABLE ASTS AND EXIT
                                     004F   156 ;
                                     004F   157 ;  WAIT UPON A SYNCHRONOUS REQUEST (MUST BE FROM AST LEVEL)
                                     004F   158 ;
                                     004F   159 SYNC_STALL:
                                     004F   160 	ASSUME	IFB$B_EFN EQ IRB$B_EFN
            53         0B A9     9A  004F   161 	MOVZBL	IRB$B_EFN(R9),R3		;SET EFN TO WAIT ON
                          F2     11  0053   162 	BRB	SETSTALL
RMS0WAIT        WAIT FOR AYNC OPERATION COMPLETION               21-AUG-1978 22:13:58   VAX-11 MACRO X0.3-11               Page   5
005             RMS$WAIT  -  $WAIT ROUTINE                                                                                       (1)

                                     0055   164 ;++
                                     0055   165 ;
                                     0055   166 ;  RAB FOR $WAIT AND ORIGINAL OPERATION ARE DIFFERENT.
                                     0055   167 ;  ALL SET IF IRAB NOT BUSY, ELSE CLEAR STS AND CAUSE WAIT.
                                     0055   168 ;
                                     0055   169 ;--
            69            20     E1  0055   170 DIFRAB:	BBC	#IRB$V_BUSY,(R9),SUCXIT	;BRANCH IF NOT BUSY
                          14         0058       
                          54     D6  0059   171 	INCL	R4			;SET FLAG FOR DIFFERENT RABS
                       08 A8     D4  005B   172 	CLRL	RAB$L_STS(R8)		;ZERO STS FOR WAIT
                          D3     11  005E   173 	BRB	CHKASY			;GO CHECK SYNC/ASYNC OPERATION & STALL
                                     0060   174 
                                     0060   175 ;++
                                     0060   176 ;
                                     0060   177 ;  BAD ISI VALUE.  ASSUME DISCONNECT HAS OCCURRED.
                                     0060   178 ;  LEAVE STS UNCHANGED UNLESS IT IS ZERO, IN WHICH CASE DECLARE SUCCESS.
                                     0060   179 ;
                                     0060   180 ;--
                                     0060   181 DISCONNECT:
            50         08 A8     D0  0060   182 	MOVL	RAB$L_STS(R8),R0	;GET STATUS
                          07     13  0064   183 	BEQL	SUCXIT			;BRANCH IF ZERO
       8001 8F            50     B1  0066   184 	CMPW	R0,#RMS$_STALL&^XFFFF	;DID USER SET A WEIRD VALUE IN HERE?
                          03     12  006B   185 	BNEQ	EXIT			;BRANCH IF NOT (O.K.)
                                     006D   186 SUCXIT:	RMSSUC				;SHOW NORMAL SUCCESS
            50            01     D0  006D       		MOVL	#1,R0
                          FF8D'  31  0070   187 EXIT:	BRW	RM$EX_NOSTR		;NO NEED TO WAIT (ALREADY DONE)
                                     0073   188 
                                     0073   189 ;++
                                     0073   190 ;
                                     0073   191 ;  INTERNAL RMS PROBLEM  -  IRAB TABLE POINTED TO AN INVALID IRAB
                                     0073   192 ;
                                     0073   193 ;--
                                     0073   194 ERRBUG:	RMSTBUG	FTL$_BADIFAB
                                     0073       	.SAVE
                                 0000006C       	.PSECT	$ABS$,ABS
                           00000000  006C       	.=0
                                 00000073       	.RESTORE
                          FF8A'  30  0073       		BSBW	RM$BUG
                                 FD  0076       		.BYTE	FTL$_BADIFAB
                                     0077   195 
                                     0077   196 
                                     0077   197 	.END
