                                     0029   140 	.SBTTL RM$GETDEVIFID  -  GET DEVICE ID AND FILE ID
                                     0029   141 ;++
                                     0029   142 ; FUNCTIONAL DESCRIPTION:
                                     0029   143 ;
                                     0029   144 ;  THIS ROUTINE RETURNS THE COUNTED DEVICE NAME STRING AND ASCII UNIT NUMBER,
                                     0029   145 ;  AS WELL AS THE FILE ID FOR THE FILE OPEN ON THE CHANNEL.
                                     0029   146 ;
                                     0029   147 ; CALLING SEQUENCE:
                                     0029   148 ;
                                     0029   149 ;	BSBW	RM$GETDVIFID
                                     0029   150 ;
                                     0029   151 ; INPUT PARAMETERS:
                                     0029   152 ;
                                     0029   153 ;	R9		IFAB ADDRESS
                                     0029   154 ;	R3		ADDRESS OF 22-BYTE BUFFER TO RETURN DEVICE NAME STRING
                                     0029   155 ;	IFB$W_CHNL	CHANNEL #
                                     0029   156 ;
                                     0029   157 ; IMPLICIT INPUTS:
                                     0029   158 ;
                                     0029   159 ;	NONE
                                     0029   160 ;
                                     0029   161 ; OUTPUT PARAMETERS:
                                     0029   162 ;
                                     0029   163 ;	R3		ADDRESS FOLLOWING FILE ID IN BUFFER IF DEVICE IS DISK
                                     0029   164 ;	R0-R2		DESTROYED
                                     0029   165 ;
                                     0029   166 ; IMPLICIT OUTPUTS:
                                     0029   167 ;
                                     0029   168 ;	THE COUNTED ASCII STRING FOR THE DEVICE NAME AND UNIT IS MOVED
                                     0029   169 ;	TO THE BUFFER PROVIDED, FOLLOWED BY THE FILE ID STARTING 16 BYTES
                                     0029   170 ;	FROM THE START OF THE BUFFER.
                                     0029   171 ;
                                     0029   172 ; COMPLETION CODES:
                                     0029   173 ;
                                     0029   174 ;	NONE
                                     0029   175 ;
                                     0029   176 ; SIDE EFFECTS:
                                     0029   177 ;
                                     0029   178 ;	NONE
                                     0029   179 ;
                                     0029   180 ;--
RM0GETDVI       GET DEVICE INFO FROM SYSTEM                      21-AUG-1978 21:25:11   VAX-11 MACRO X0.3-11               Page   6
006             RM$GETDEVIFID  -  GET DEVICE ID AND FILE ID                                                                      (1)

                                     0029   182 	$NEWPSECT	F$RMSCLOSERNDN
                                     0029       		.SAVE
                                 00000000       		.PSECT	F$RMSCLOSERNDN,GBL,NOWRT
0000000A 00000064 000003E8 00002710  0000   183 DIVTBL:	.LONG	10000,1000,100,10	; DIVISOR TABLE FOR ASCII CONVERSION
                                     0010   184 ;
                                     0010   185 RM$GETDVIFID::
                     0078 8F     BB  0010   186 	PUSHR	#^M<R3,R4,R5,R6>
                          FFE9'  30  0014   187 	BSBW	RM$GETDEVNAM			; GET THE DEVICE NAME (IN BUFFER)
                                     0017   188 						; AND UNIT # (IN R4)
                          04     BA  0017   189 	POPR	#^M<R2>				; ADR OF DEVICE NAME COUNT
                          51     DD  0019   190 	PUSHL	R1				; SAVE CCB ADDRESS
                          1F     10  001B   191 	BSBB	RM$GETDEVUNI			; GET UNIT NUMBER IN ASCII
                          02     BA  001D   192 	POPR	#^M<R1>				; RECOVER CCB ADDRESS
                                     001F   193 ;
                                     001F   194 ;  NOW GET THE FILE ID FROM THE FCB
                                     001F   195 ;  R2 = ADDRESS OF THE SPECIFIED OUTPUT BUFFER
                                     001F   196 ;
                       10 A2     D4  001F   197 	CLRL	16(R2)				; ZERO THE FILE ID
                       14 A2     B4  0022   198 	CLRW	20(R2)				;   "
            69            05     E0  0025   199 	BBS	#DEV$V_SQD,(R9),50$		; BRANCH IF MAGTAPE (NO FCB)
                          0E         0028       
            55         04 A1     D0  0029   200 	MOVL	CCB$L_WIND(R1),R5		; GET THE WCB ADDRESS
            55         18 A5     D0  002D   201 	MOVL	WCB$L_FCB(R5),R5		; GET THE FCB ADDRESS
         20 A5            06     28  0031   202 	MOVC3	#6,FCB$W_FID(R5),16(R2)		; COPY THE FILE ID TO BUFFER
                       10 A2         0035       
                     0070 8F     BA  0037   203 50$:	POPR	#^M<R4,R5,R6>
                                 05  003B   204 	RSB
RM0GETDVI       GET DEVICE INFO FROM SYSTEM                      21-AUG-1978 21:25:11   VAX-11 MACRO X0.3-11               Page   7
006             RM$GETDEVUNI - GET DEVICE UNIT NUMBER IN ASCII                                                                   (1)

                                     003C   206 	.SBTTL	RM$GETDEVUNI - GET DEVICE UNIT NUMBER IN ASCII
                                     003C   207 ;++
                                     003C   208 ; FUNCTIONAL DESCRIPTION:
                                     003C   209 ;
                                     003C   210 ;  THIS ROUTINE CONVERTS THE BINARY UNIT NUMBER INTO DECIMAL ASCII
                                     003C   211 ;  IN THE SPECIFIED OUTPUT STRING AND UPDATES THE COUNT FIELD OF
                                     003C   212 ;  THAT STRING.
                                     003C   213 ;
                                     003C   214 ; CALLING SEQUENCE:
                                     003C   215 ;
                                     003C   216 ;	BSBW	RM$GETDEVUNI
                                     003C   217 ;
                                     003C   218 ; INPUT PARAMETERS:
                                     003C   219 ;
                                     003C   220 ;	R4		DEVICE UNIT # IN BINARY
                                     003C   221 ;	R3		ADDRESS OF BUFFER TO RETURN ASCII UNIT NUMBER
                                     003C   222 ;	R2		ADR OF BYTE COUNT TO AUGMENT WITH # OF CHARACTERS RETURNED
                                     003C   223 ;
                                     003C   224 ; IMPLICIT INPUTS:
                                     003C   225 ;
                                     003C   226 ;	NONE
                                     003C   227 ;
                                     003C   228 ; OUTPUT PARAMETERS:
                                     003C   229 ;
                                     003C   230 ;	R3		ADDRESS OF NEXT BYTE IN BUFFER AFTER UNIT NUMBER
                                     003C   231 ;	R2		PRESERVED
                                     003C   232 ;	R0-R1,R4-R6	DESTROYED
                                     003C   233 ;
                                     003C   234 ; IMPLICIT OUTPUTS:
                                     003C   235 ;
                                     003C   236 ;	THE DECIMAL ASCII TRANSLATION FOR THE SPECIFIED BINARY UNIT NUMBER
                                     003C   237 ;	IS PLACED IN THE BUFFER PROVIDED AND THE COUNT OF CHARACTERS
                                     003C   238 ;	GENERATED IS ADDED TO THE BYTE ADDRESSED BY R2.
                                     003C   239 ;
                                     003C   240 ; COMPLETION CODES:
                                     003C   241 ;
                                     003C   242 ;	NONE
                                     003C   243 ;
                                     003C   244 ; SIDE EFFECTS:
                                     003C   245 ;
                                     003C   246 ;	NONE
                                     003C   247 ;
                                     003C   248 ;--
RM0GETDVI       GET DEVICE INFO FROM SYSTEM                      21-AUG-1978 21:25:11   VAX-11 MACRO X0.3-11               Page   8
006             RM$GETDEVUNI - GET DEVICE UNIT NUMBER IN ASCII                                                                   (1)

                                     003C   250 RM$GETDEVUNI::
                          55     7C  003C   251 	CLRQ	R5				; WORK SPACE
            0A            54     D1  003E   252 	CMPL	R4,#10				; FAST PATH FOR SINGLE DIGIT
                          1D     19  0041   253 	BLSS	50$				; BRANCH IF JUST ONE DIGIT
            50         BA AF     9E  0043   254 	MOVAB	DIVTBL,R0			; DIVISOR TABLE ADDRESS
            54            80     7B  0047   255 10$:	EDIV	(R0)+,R4,R1,R4			; R1=QUOIENT, R4=REMAINDER
            54            51         004A       
                          08     13  004C   256 	BEQL	30$				; BRANCH IF 0 QUOTIENT
                          56     96  004E   257 20$:	INCB	R6				; COUNT # CHARS OUTPUT
            51            30     89  0050   258 	BISB3	#^A/0/,R1,(R3)+			; ADD THE DIGIT TO THE DEVICE NAME
                          83         0053       
                          04     11  0054   259 	BRB	40$
                          56     95  0056   260 30$:	TSTB	R6				; ALREADY OUTPUT A CHAR?
                          F4     12  0058   261 	BNEQ	20$				; BRANCH IF YES, ELSE IGNORE LEAD 0
            0A         FC A0     D1  005A   262 40$:	CMPL	-4(R0),#10			; LAST DIVISOR?
                          E7     12  005E   263 	BNEQ	10$				; CONTINUE IF NOT
                          56     96  0060   264 50$:	INCB	R6				; COUNT THE LAST DIGIT
            54            30     89  0062   265 	BISB3	#^A/0/,R4,(R3)+			; ADD THE DIGIT TO THE DEVICE NAME
                          83         0065       
            62            56     80  0066   266 	ADDB2	R6,(R2)				; ADD TO THE DEVICE NAME COUNT
                                 05  0069   267 	RSB
                                     006A   268 
                                     006A   269 
                                     006A   270 	.END
