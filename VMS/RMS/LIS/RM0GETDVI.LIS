                                     0000    68 	.SBTTL RM$GETDEVNAM  -  GET DEVICE NAME AND UNIT #
                                     0000    69 ;++
                                     0000    70 ; FUNCTIONAL DESCRIPTION:
                                     0000    71 ;
                                     0000    72 ;  THIS ROUTINE RETURNS THE COUNTED GENERIC DEVICE NAME STRING
                                     0000    73 ;  AND THE BINARY UNIT NUMBER FOR THE FILE OPEN ON THE CHANNEL.
                                     0000    74 ;
                                     0000    75 ;
                                     0000    76 ; CALLING SEQUENCE:
                                     0000    77 ;
                                     0000    78 ;	BSBW	RM$GETDEVNAM
                                     0000    79 ;
                                     0000    80 ; INPUT PARAMETERS:
                                     0000    81 ;
                                     0000    82 ;	R9		IFAB ADDRESS
                                     0000    83 ;	R3		ADDRESS OF BUFFER TO RETURN DEVICE NAME STRING
                                     0000    84 ;	IFB$W_CHNL	CHANNEL #
                                     0000    85 ;
                                     0000    86 ; IMPLICIT INPUTS:
                                     0000    87 ;
                                     0000    88 ;	NONE
                                     0000    89 ;
                                     0000    90 ; OUTPUT PARAMETERS:
                                     0000    91 ;
                                     0000    92 ;	R6		UCB ADDRESS
                                     0000    93 ;	R4		UNIT # IN BINARY
                                     0000    94 ;	R3		ADDRESS FOLLOWING DEVICE NAME IN BUFFER
                                     0000    95 ;	R1		CCB ADDRESS
                                     0000    96 ;	R0,R2,R5	DESTROYED
                                     0000    97 ;
                                     0000    98 ; IMPLICIT OUTPUTS:
                                     0000    99 ;
                                     0000   100 ;	THE COUNTED ASCII STRING FOR THE GENERIC DEVICE NAME IS MOVED
                                     0000   101 ;	TO THE BUFFER PROVIDED.
                                     0000   102 ;
                                     0000   103 ; COMPLETION CODES:
                                     0000   104 ;
                                     0000   105 ;	NONE
                                     0000   106 ;
                                     0000   107 ; SIDE EFFECTS:
                                     0000   108 ;
                                     0000   109 ;	NONE
                                     0000   110 ;
                                     0000   111 ;--
