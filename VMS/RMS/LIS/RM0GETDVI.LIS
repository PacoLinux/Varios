006             RM$GETDEVNAM  -  GET DEVICE NAME AND UNIT #                                                                      (1)

                                     0000   113 RM$GETDEVNAM::
            54            53     D0  0000   114 	MOVL	R3,R4				; SAVE THE CALLER'S BUFFER ADDRESS
                          1A     10  0003   115 	BSBB	RM$GETCCB			; GET THE CCB ADDRESS INTO R5
                          51     DD  0005   116 	PUSHL	R1				; SAVE THE CCB ADDRESS
            56            61     D0  0007   117 	MOVL	CCB$L_UCB(R1),R6		; GET THE UCB ADDRESS
         24 A6            14     C1  000A   118 	ADDL3	#DDB$T_NAME,UCB$L_DDB(R6),R1	; GET ADR OF DEVICE NAME
                          51         000E       
            50            61     9B  000F   119 	MOVZBW	(R1),R0				; GET LENGTH OF DEVICE NAME
                          50     B6  0012   120 	INCW	R0				; INCREMENT TO MOVE COUNT TOO
            61            50     28  0014   121 	MOVC3	R0,(R1),(R4)			; MOVE THE DEVICE NAME STRING
                          64         0017       
                          02     BA  0018   122 	POPR	#^M<R1>				; RECOVER CCB ADDRESS
            54         48 A6     3C  001A   123 	MOVZWL	UCB$W_UNIT(R6),R4		; GET THE UNIT NUMBER
                                 05  001E   124 	RSB					; RETURN
                                     001F   125 ;++
                                     001F   126 ;
                                     001F   127 ;  SUBROUTINE TO RETURN THE CCB ADDRESS
                                     001F   128 ;
                                     001F   129 ;  INPUT:	R9	IFAB ADDRESS WITH CHANNEL IN IFB$W_CHNL
                                     001F   130 ;
                                     001F   131 ;  OUTPUT:	R1	ADDRESS OF CCB
                                     001F   132 ;		R0,R2,R3 DESTROYED
                                     001F   133 ;
                                     001F   134 ;--
                                     001F   135 RM$GETCCB::
            50         20 A9     3C  001F   136 	MOVZWL	IFB$W_CHNL(R9),R0		; SET CHANNEL #
                 00000000'9F     17  0023   137 	JMP	@#IOC$VERIFYCHAN		; GET THE CCB ADDRESS (IN R1)
                                     0029   138 						; AND RETURN
