                                     0000    70 	.SBTTL RM$DISCOMMON  -  COMMON CLEANUP ON CONNECT-DISCONNECTT ROUTINES
                                     0000    71 ;++
                                     0000    72 ; FUNCTIONAL DESCRIPTION:
                                     0000    73 ;
                                     0000    74 ; THE FOUR ENTRY POINTS OF THIS MODULE
                                     0000    75 ; PROVIDE COMMON CONNECT - DISCONNECT CLEANUP
                                     0000    76 ; FUNCTIONS. SEE DESCRIPTION OF THE ENTRY POINTS.
                                     0000    77 ;
                                     0000    78 ; CALLING SEQUENCE:
                                     0000    79 ;
                                     0000    80 ;	BSBW	RM$DISCOMMONSUC - SETS R0 TO RM$_SUC AND FALLS
                                     0000    81 ;				  THRU TO RM$DISCOMMON
                                     0000    82 ;	BSBW	RM$DISCOMMON    - RELEASE CURRENT BDB AND FALLS
                                     0000    83 ;			   	  THRU TO RM$COMCLNUP
                                     0000    84 ;	BSBW	RM$COMCLNUP     - RETURN ALL BDB'S AND BUFFERS FOR STREAM
                                     0000    85 ;			          AND FALL THRU TO RM$CCLN1
                                     0000    86 ;	BSBW	RM$CCLN1	- DEALLOCATE IRAB AND ZERO ISI
                                     0000    87 ;				  AND IRAB TABLE ENTRY
                                     0000    88 ;
                                     0000    89 ; INPUT PARAMETERS:
                                     0000    90 ;
                                     0000    91 ;	R11	IMPURE AREA ADDRESS
                                     0000    92 ;	R10	IFAB ADDRESS
                                     0000    93 ;	R9	IRAB ADDRESS
                                     0000    94 ;	R8	RAB ADDRESS
                                     0000    95 ;	R0	STATUS CODE (EXCEPT FOR ENTRY AT RM$DISCOMMONSUC)
                                     0000    96 ;
                                     0000    97 ; IMPLICIT INPUTS:
                                     0000    98 ;
                                     0000    99 ;   FOR ENTRY AT RM$DISCOMMON:
                                     0000   100 ;	IRB$L_CURBDB & OTHER RM$RELEASE IMPLICIT INPUTS
                                     0000   101 ;   FOR ENTRY AT RM$COMCLNUP:
                                     0000   102 ;	IRB$B_BCNT & BDB CHAIN
                                     0000   103 ;   FOR ENTRY AT RM$CCLN1:
                                     0000   104 ;	IRB$L_IRAB_LNK
                                     0000   105 ;
                                     0000   106 ; OUTPUT PARAMETERS:
                                     0000   107 ;
                                     0000   108 ;	R0	STATUS CODE
                                     0000   109 ;	R7	SET FROM IRB$B_MODE
                                     0000   110 ;	R9	ZEROED
                                     0000   111 ;	AP	SET FROM IRB$L_ARGLST
                                     0000   112 ;	R1-R6	DESTROYED
                                     0000   113 ;
                                     0000   114 ; IMPLICIT OUTPUTS:
                                     0000   115 ;
                                     0000   116 ;	RAB$W_ISI ZEROED
                                     0000   117 ;	RAB$L_STV POSSIBLY UPDATED
                                     0000   118 ;	IRAB, ALL BDB'S AND RELATED BUFFERS DEALLOCATED
                                     0000   119 ;
                                     0000   120 ; COMPLETION CODES:
                                     0000   121 ;
                                     0000   122 ;	STANDARD RMS. IF AN ERROR OCCURS IT WILL REPLACE
                                     0000   123 ;	THE STATUS CODE INPUT IN R0, OTHERWISE THE CODE INPUT
                                     0000   124 ;	IN R0 WILL BE USED.
                                     0000   125 ;
                                     0000   126 ; SIDE EFFECTS:
