         04 A6            53     D0  0066   297 	MOVL	R3,4(R6)		; FILL IN DESCRIPTOR ADDRESS
         38 BA         34 AA     28  006A   298 	MOVC3	FWA$Q_NODE(R10),@FWA$Q_NODE+4(R10),(R3)
                          63         006F       
                                     0070   299 					; COPY NODENAME STRING TO NCB
