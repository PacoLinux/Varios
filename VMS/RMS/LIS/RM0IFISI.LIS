RM0IFISI        IFAB AND IRAB INDEX TABLE ROUTINES               21-AUG-1978 21:25:45   VAX-11 MACRO X0.3-11               Page   3
011             RM$GTIADR  -  INDEX TO TABLE ADDRESS CONVERSION                                                                  (1)

                                     0000    69 	.SBTTL	RM$GTIADR  -  INDEX TO TABLE ADDRESS CONVERSION ROUTINE
                                     0000    70 ;++
                                     0000    71 ; FUNCTIONAL DESCRIPTION:
                                     0000    72 ;
                                     0000    73 ;  THIS SUBROUTINE CONVERTS AN IFI OR ISI TABLE INDEX VALUE
                                     0000    74 ;  INTO THE ADDRESS OF THE RELATED IFAB OR IRAB RESPECTIVELY.
                                     0000    75 ;
                                     0000    76 ; CALLING SEQUENCE:
                                     0000    77 ;
                                     0000    78 ;	BSBW	RM$GTIADR
                                     0000    79 ;
                                     0000    80 ; INPUT PARAMETERS:
                                     0000    81 ;
                                     0000    82 ;	R11	IMAGE IMPURE AREA ADDRESS
                                     0000    83 ;	R9	INDEX VALUE (IFI OR ISI)
                                     0000    84 ;	R7	MODE OF CALLER (ONLY IF IFI OR ISI INDICATES DIRECT 
                                     0000    85 ;				ACCESS TO A PROCESS PERMANENT FILE)
                                     0000    86 ;	R0	IFAB OR IRAB INDEX TABLE OFFSET / 4
                                     0000    87 ;
                                     0000    88 ; IMPLICIT INPUTS:
                                     0000    89 ;
                                     0000    90 ;	NONE
                                     0000    91 ;
                                     0000    92 ; OUTPUT PARAMETERS:
                                     0000    93 ;
                                     0000    94 ;	R9	IFAB OR IRAB ADDRESS
                                     0000    95 ;	R0	DESTROYED
                                     0000    96 ;
                                     0000    97 ;    IF IFI OR ISI < 0 AND NOT USER MODE:
                                     0000    98 ;	
                                     0000    99 ;	R11	PROCESS I/O IMPURE AREA ADDR
                                     0000   100 ;
                                     0000   101 ; IMPLICIT OUTPUTS:
                                     0000   102 ;
                                     0000   103 ;	NONE
                                     0000   104 ;
                                     0000   105 ; COMPLETION CODES:
                                     0000   106 ;
                                     0000   107 ;	Z-BIT SET IF INVALID IFI OR ISI, CLEAR OTHERWISE.
                                     0000   108 ;
                                     0000   109 ; SIDE EFFECTS:
                                     0000   110 ;
                                     0000   111 ;	NONE
                                     0000   112 ;
                                     0000   113 ;--
                                     0000   114 RM$GTIADR::
                          59     B5  0000   115 	TSTW	R9			;NON-STANDARD IFI/ISI?
                          19     15  0002   116 	BLEQ	50$			;BRANCH IF PPF FORMAT OR ZERO
            50            6B40   D0  0004   117 5$:	MOVL	(R11)[R0],R0		;GET TABLE ADDR
         20 AB            59     B1  0008   118 10$:	CMPW	R9,IMP$W_ENTPERSEG(R11)	;IN THIS TABLE SEGMENT?
                          05     1A  000C   119 	BGTRU	30$			;BRANCH IF NOT
            59            6049   D0  000E   120 	MOVL	(R0)[R9],R9		;SET IFAB/IRAB ADDR
                                     0012   121 					;NOTE: MAY BE ZERO.
                                 05  0012   122 	RSB
                                     0013   123 ;
                                     0013   124 ;  THE INPUT IFI/ISI VALUE DOES NOT MAP TO THIS SEGMENT
                                     0013   125 ;  OF THE INDEX TABLE  -  TRY THE NEXT SEGMENT
RM0IFISI        IFAB AND IRAB INDEX TABLE ROUTINES               21-AUG-1978 21:25:45   VAX-11 MACRO X0.3-11               Page   4
011             RM$GTIADR  -  INDEX TO TABLE ADDRESS CONVERSION                                                                  (1)

                                     0013   126 ;
            59         20 AB     A2  0013   127 30$:	SUBW2	IMP$W_ENTPERSEG(R11),R9	;REDUCE THE INDEX VALUE
            50            60     D0  0017   128 	MOVL	(R0),R0			;GET LINK TO NEXT TABLE SEG
                          EC     12  001A   129 	BNEQ	10$			;AND KEEP TRYING, IF ANY MORE
                                 05  001C   130 40$:	RSB				;RETURN WITH Z-BIT SET
                                     001D   131 ;
                                     001D   132 ;  A NEGATIVE ISI OR IFI HAS BEEN INPUT.
                                     001D   133 ;
                                     001D   134 ;  2 VALID CASES EXIST, DEPENDING UPON THE STATE OF BIT 14:
                                     001D   135 ;	- (BIT 14=0) - A NON-USER MODE CALLER DESIRES TO OPERATE
                                     001D   136 ;		       ON THE PPF DIRECTLY
                                     001D   137 ;	- (BIT 14=1) - A CALLER DESIRES TO OPERATE ON THE PPF INDIRECTLY
                                     001D   138 ;
                                     001D   139 ;  IF ACCESS ALLOWED SWITCH TO THE PROCESS PERMANENT FILES IFAB OR
                                     001D   140 ;  IRAB TABLE IN THE PROCESS I/O SEGMENT.
                                     001D   141 ;
                          FD     13  001D   142 50$:	BEQL	40$			;BRANCH IF ZERO IFI/ISI (ERROR)
            59            0E     E0  001F   143 	BBS	#FAB$V_PPF_IND,R9,55$	;BRANCH IF INDIRECT ACCESS
                          05         0022       
            03            57     91  0023   144 	CMPB	R7,#PSL$C_USER		;DIRECT ACCESS ATTEMPT FROM USER MODE?
                          0E     13  0026   145 	BEQL	60$			;ERROR IF USER
            5B   00000000'9F     DE  0028   146 55$:	MOVAL	@#PIO$GW_PIOIMPA,R11	;SWITCH IMPURE POINTERS
            59       FFC0 8F     AA  002F   147 	BICW2	#<<1@RAB$V_PPF_RAT>-1> \ <<1@16>-1>,R9 ;CLEAR ALL BUT TABLE INDEX
                          CE     12  0034   148 	BNEQ	5$			;AND CONTINUE IF NON-ZERO
                                     0036   149 ;
                                     0036   150 ;  USER ATTEMPTING DIRECT ACCESS TO A PROCESS PERMANENT FILE
                                     0036   151 ;  OR HAS A BAD ISI VALUE
                                     0036   152 ;
                          59     D4  0036   153 60$:	CLRL	R9			;IT'S A NO-NO
                                 05  0038   154 	RSB
RM0IFISI        IFAB AND IRAB INDEX TABLE ROUTINES               21-AUG-1978 21:25:45   VAX-11 MACRO X0.3-11               Page   5
011             RM$GTSLT  -  INDEX TABLE SLOT ALLOCATION ROUTIN                                                                  (1)

                                     0039   156 	.SBTTL RM$GTSLT  -  INDEX TABLE SLOT ALLOCATION ROUTINE
                                     0039   157 ;++
                                     0039   158 ;  FUNCTIONAL DESCRIPTION:
                                     0039   159 ;
                                     0039   160 ;  THIS ROUTINE SCANS THE IFAB OR IRAB INDEX TABLE FOR
                                     0039   161 ;  AN EMPTY SLOT AND IF FOUND SETS THE SLOT TO THE ASSOCIATED
                                     0039   162 ;  IFAB OR IRAB ADDRESS, RETURNING THE INDEX VALUE OF THE SLOT.
                                     0039   163 ;
                                     0039   164 ;  CALLING SEQUENCE:
                                     0039   165 ;
                                     0039   166 ;	BSBW	RM$GTSLT
                                     0039   167 ;
                                     0039   168 ;  INPUT PARAMETERS:
                                     0039   169 ;
                                     0039   170 ;	R11	PROCESS OR IMAGE IMPURE AREA ADDRESS
                                     0039   171 ;	R9	IFAB OR IRAB ADDRESS
                                     0039   172 ;	R5	IFAB OR IRAB INDEX TABLE START ADDRESS
                                     0039   173 ;
                                     0039   174 ;  IMPLICIT INPUTS:
                                     0039   175 ;
                                     0039   176 ;	NONE
                                     0039   177 ;
                                     0039   178 ;  OUTPUT PARAMETERS:
                                     0039   179 ;
                                     0039   180 ;	R6	TABLE INDEX VALUE (I.E., IFI OR ISI)
                                     0039   181 ;	R0	STATUS CODE
                                     0039   182 ;	R1,R2,R3,R4,R5 DESTROYED
                                     0039   183 ;
                                     0039   184 ;  IMPLICIT OUTPUTS:
                                     0039   185 ;
                                     0039   186 ;	THE ALLOCATED TABLE SLOT IS SET TO THE CONTENTS OF R9.
                                     0039   187 ;
                                     0039   188 ;  COMPLETION CODES:
                                     0039   189 ;
                                     0039   190 ;	STANDARD RMS, IN PARTICULAR, SUCCESS OR DME.
                                     0039   191 ;
                                     0039   192 ;  SIDE EFFECTS:
                                     0039   193 ;
                                     0039   194 ;	NONE
                                     0039   195 ;
                                     0039   196 ;--
                                 00000000   197 	.PSECT	F$RMSOPEN0,GBL,NOWRT
                                     0000   198 
                                     0000   199 RM$GTSLT::
                          56     D4  0000   200 	CLRL	R6			;BUILD INDEX VALUE HERE
            51            55     D0  0002   201 10$:	MOVL	R5,R1			;LEAVE R5 POINTING TO LINK
            50         20 AB     3C  0005   202 	MOVZWL	IMP$W_ENTPERSEG(R11),R0	;# ENTRIES PER TABLE SEGMENT
                          81     D5  0009   203 15$:	TSTL	(R1)+			;SKIP LINK
                          56     D6  000B   204 20$:	INCL	R6			;BUMP INDEX
                          81     D5  000D   205 	TSTL	(R1)+			;ZERO SLOT?
                          2B     13  000F   206 	BEQL	50$			;BRANCH IF YES - GO USE IT
            F7            50     F5  0011   207 	SOBGTR	R0,20$			;KEEP SCANNING SEGMENT
                                     0014   208 ;
                                     0014   209 ;  NO FREE SLOTS THIS TABLE SEGMENT  -  TRY NEXT IF ANY
                                     0014   210 ;
                          65     D5  0014   211 	TSTL	(R5)			;ANOTHER SEGMENT?
                          05     13  0016   212 	BEQL	30$			;BRANCH IF NONE
RM0IFISI        IFAB AND IRAB INDEX TABLE ROUTINES               21-AUG-1978 21:25:45   VAX-11 MACRO X0.3-11               Page   6
011             RM$GTSLT  -  INDEX TABLE SLOT ALLOCATION ROUTIN                                                                  (1)

            55            65     D0  0018   213 	MOVL	(R5),R5			;UPDATE LINK
                          E5     11  001B   214 	BRB	10$
RM0IFISI        IFAB AND IRAB INDEX TABLE ROUTINES               21-AUG-1978 21:25:45   VAX-11 MACRO X0.3-11               Page   7
011             RM$GTSLT  -  INDEX TABLE SLOT ALLOCATION ROUTIN                                                                  (1)

                                     001D   216 ;
                                     001D   217 ;  NO FREE SLOTS IN EXISTING SEGMENTS.
                                     001D   218 ;  IF PROCESS I/O SEGMENT, RETURN AN ERROR, OTHERWISE
                                     001D   219 ;  ADD A SEGMENT TO THE TABLE.
                                     001D   220 ;
                                     001D   221 	ASSUME	IMP$W_RMSSTATUS EQ 0
                                     001D   222 	ASSUME	IMP$V_IIOS EQ 0
            2D            6B     E9  001D   223 30$:	BLBC	(R11),ERRDME		;BRANCH IF PIO SEGMENT
            51   00000000'9F     DE  0020   224 	MOVAL	@#PIO$GL_IIOFSPLH,R1	;IMPURE AREA ADDR
            52         20 AB     3C  0027   225 	MOVZWL	IMP$W_ENTPERSEG(R11),R2	;# ENTRIES
                          52     D6  002B   226 	INCL	R2			;+1 = # LONGWORDS
            52            02     78  002D   227 	ASHL	#2,R2,R2		;= # BYTES REQUIRED
                          52         0030       
                          FFCC'  30  0031   228 	BSBW	RM$GETSPC_ALT		;GET THIS MUCH SPACE
            15            50     E9  0034   229 	BLBC	R0,RETURN		;BRANCH IF NO SPACE
                                     0037   230 ;
                                     0037   231 ;  LINK IN THE NEW SEGMENT AND GO BACK AND USE IT
                                     0037   232 ;
            65            51     D0  0037   233 	MOVL	R1,(R5)			;UPDATE THE OLD LINK
                          CD     11  003A   234 	BRB	15$			;AND GO USE
                                     003C   235 ;
                                     003C   236 ;
                                     003C   237 ;  FOUND A FREE SLOT  
                                     003C   238 ;  STORE THE IFAB OR IRAB ADDRESS IN THE SLOT AND RETURN
                                     003C   239 ;
            71            59     D0  003C   240 50$:	MOVL	R9,-(R1)		;STORE STRUCTURE ADDRESS
                       22 AB     B6  003F   241 	INCW	IMP$W_NUM_IFABS(R11)	;COUNT # IFABS IN USE
                                     0042   242 ;
                                     0042   243 ;  FLAG THE IFI OR ISI VALUE IF THIS IS THE PROCESS I/O SEGMENT
                                     0042   244 ;
                                     0042   245 	ASSUME	IMP$W_RMSSTATUS EQ 0
                                     0042   246 	ASSUME	IMP$V_IIOS EQ 0
            04            6B     E8  0042   247 	BLBS	(R11),60$		;BRANCH IF IMAGE I/O SEGMENT
                                     0045   248 	SSB	#15,R6			;SET PIO SEGMENT FLAG
            56            0F     E2  0045       		BBSS	#15,R6,30000$
                          00         0048       
                                     0049       30000$:
                                     0049   249 60$:	RMSSUC
            50            01     D0  0049       		MOVL	#1,R0
                                 05  004C   250 RETURN:	RSB
                                     004D   251 ;
                                     004D   252 ;
                                     004D   253 ;
                                     004D   254 ;  COULDN'T ALLOCATE ANOTHER PAGE FOR NEW IRAB OR IFAB
                                     004D   255 ;  TABLE SEGMENT BECAUSE IN PROCESS I/O SEGMENT
                                     004D   256 ;
                                     004D   257 ERRDME:	$PSECT_BR	L$RMS0
                          FFB0'  31  004D       		BRW	L$RMS00
                                     0050       		.SAVE
                                 00000000       		.PSECT	L$RMS0,GBL,NOWRT
                                     0000       L$RMS00:
                                     0000   258 	RMSERR	DME
            50       84D4 8F     3C  0000       		MOVZWL	#<RMS$_DME&^XFFFF>,R0
                                 05  0005   259 	RSB
                                     0006   260 	$PSECT_RESTORE
                                 00000050       		.RESTORE
                                     0050   261 
