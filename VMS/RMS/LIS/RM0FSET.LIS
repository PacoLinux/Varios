                                     003A   167 ;
                                     003A   168 ;  ERROR RETURNS
                                     003A   169 ;
                                     003A   170 ERRIFI:	$PSECT_BR	L$RMS0
                          FFC3'  31  003A       		BRW	L$RMS00
                                     003D       		.SAVE
                                 00000000       		.PSECT	L$RMS0,GBL,NOWRT
                                     0000       L$RMS00:
                                     0000   171 	RMSERR	IFI			;INVALID IFI VALUE
            50       8564 8F     3C  0000       		MOVZWL	#<RMS$_IFI&^XFFFF>,R0
                          FFF8'  31  0005   172 ERROR:	BRW	RM$EX_NOSTR
                                     0008   173 	$PSECT_RESTORE
                                 0000003D       		.RESTORE
                                     003D   174 ;
                                     003D   175 ERRACT:	$PSECT_BR	L$RMS0
                          FFC8'  31  003D       		BRW	L$RMS01
                                     0040       		.SAVE
                                 00000008       		.PSECT	L$RMS0,GBL,NOWRT
                                     0008       L$RMS01:
                                     0008   176 	RMSERR	ACT			;STREAM ALREADY ACTIVE
            50       825A 8F     3C  0008       		MOVZWL	#<RMS$_ACT&^XFFFF>,R0
                          F6     11  000D   177 	BRB	ERROR
                                     000F   178 	$PSECT_RESTORE
                                 00000040       		.RESTORE
                                     0040   179 ;
                                     0040   180 ;  INTERNAL RMS PROBLEM  -  IFAB TABLE POINTED TO AN INVALID IFAB!
                                     0040   181 ;
                                     0040   182 ERRBUG:	RMSTBUG	FTL$_BADIFAB
                                     0040       	.SAVE
                                 00000068       	.PSECT	$ABS$,ABS
                           00000000  0068       	.=0
                                 00000040       	.RESTORE
                          FFBD'  30  0040       		BSBW	RM$BUG
                                 FD  0043       		.BYTE	FTL$_BADIFAB
                                     0044   183 
                                     0044   184 
                                     0044   185 	.END
