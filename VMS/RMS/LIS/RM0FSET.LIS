                                     0000    63 	.SBTTL RM$FSET  -  COMMON SETUP FOR FAB FUNCTION ROUTINE
                                     0000    64 ;++
                                     0000    65 ; FUNCTIONAL DESCRIPTION:
                                     0000    66 ;
                                     0000    67 ;  THIS ROUTINE PERFORMS COMMON SETUP FOR A FAB FUNCTION CALL
                                     0000    68 ;  INCLUDING THE FOLLOWING:
                                     0000    69 ;
                                     0000    70 ;	1. CALL RM$FABCHK TO CHECK ARGLIST, SET BASE REGS,
                                     0000    71 ;	   AND ZERO STS AND STV FIELDS IN FAB
                                     0000    72 ;	2. CHECK FOR VALID IFI AND SET IFAB ADDR
                                     0000    73 ;	3. CHECK FOR STREAM IDLE AND SET TO BUSY
                                     0000    74 ;	4. STORE THE ARGLIST ADDR AND CALLER'S MODE IN THE IFAB
                                     0000    75 ;	5. SAVE SP ENTRY VALUE IN IMP$L_SAVED_SP
                                     0000    76 ;
                                     0000    77 ;
                                     0000    78 ; CALLING SEQUENCE:
                                     0000    79 ;
                                     0000    80 ;	BSBW	RM$FSET
                                     0000    81 ;
                                     0000    82 ;  ALTERNATE ENTRY AT RM$FSET_ALT TO PERFORM FUNCTIONS 3, 4, & 5 ONLY
                                     0000    83 ;	R7, R8, R9, AND R11 MUST BE SET AS PER OUTPUT PRIOR TO CALL.
                                     0000    84 ;
                                     0000    85 ;
                                     0000    86 ; INPUT PARAMETERS:
                                     0000    87 ;
                                     0000    88 ;	SP	STACK POINTER
                                     0000    89 ;	AP	ARGUMENT LIST ADDR
                                     0000    90 ;
                                     0000    91 ; IMPLICIT INPUTS:
                                     0000    92 ;
                                     0000    93 ;	THE CONTENTS OF THE FAB
                                     0000    94 ;
                                     0000    95 ; OUTPUT PARAMETERS:
                                     0000    96 ;
                                     0000    97 ;	R11	IMPURE AREA ADDRESS
                                     0000    98 ;	R10	IFAB ADDRESS
                                     0000    99 ;	R9	IFAB ADDRESS
                                     0000   100 ;	R8	FAB ADDRESS
                                     0000   101 ;	R7	CALLER'S MODE
                                     0000   102 ;	R0 THRU R5 DESTROYED
                                     0000   103 ;
                                     0000   104 ; IMPLICIT OUTPUTS:
                                     0000   105 ;
                                     0000   106 ;	IMP$L_SAVED_SP IS SET TO VALUE OF SP+4
                                     0000   107 ;
                                     0000   108 ; COMPLETION CODES:
                                     0000   109 ;
                                     0000   110 ;	NONE.  IF AN ERROR IS DETECTED RETURNS TO USER (NOT CALLER)
                                     0000   111 ;	AFTER APPROPRIATE CLEANUP.  THE USER RECEIVES A STANDARD
                                     0000   112 ;	RMS ERROR CODE, IN PARTICULAR, IFI AND THOSE RETURNED BY
                                     0000   113 ;	RM$FABCHK.
                                     0000   114 ;
                                     0000   115 ; SIDE EFFECTS:
                                     0000   116 ;
                                     0000   117 ;	NONE
                                     0000   118 ;
                                     0000   119 ;--
RM0FSET         SETUP FOR A FAB FUNCTION                         21-AUG-1978 21:23:12   VAX-11 MACRO X0.3-11               Page   4
007             RM$FSET  -  COMMON SETUP FOR FAB FUNCTION ROUTI                                                                  (1)

                                     0000   121 RM$FSET::
                          FFFD'  30  0000   122 	BSBW	RM$FABCHK		;VALID FAB?
                                     0003   123 					;RETURNS ONLY IF O.K.
                                     0003   124 					;NOTE: SETS R11 TO IMPURE ADDR
                                     0003   125 					;     R9 TO IFI
                                     0003   126 					;     R8 TO FAB ADDR
                                     0003   127 					;     R7 TO CALLER'S MODE
                                     0003   128 ;
                                     0003   129 ;  GET THE IFAB ADDRESS AND CHECK FOR A VALID IFAB
                                     0003   130 ;
            50            06     D0  0003   131 	MOVL	#IMP$L_IFABTBL/4,R0	;IFAB TABLE OFFSET DIVIDED BY 4
                          FFF7'  30  0006   132 	BSBW	RM$GTIADR		;GET IFAB ADDR
                          2F     13  0009   133 	BEQL	ERRIFI			;BRANCH IF BAD
                                     000B   134 	ASSUME	IFB$B_BID EQ IFB$B_BLN-1
                           00000008  000B   135 	.IF NE $$RMSTEST&$$RMS_TBUGCHK
       180B 8F         08 A9     B1  000B   136 	CMPW	IFB$B_BID(R9),#IFB$C_BID+<<IFB$C_BLN/4>*256>
                          2D     12  0011   137 	BNEQ	ERRBUG			;BRANCH IF NOT A VALID IFAB
                                     0013   138 	.ENDC
                                     0013   139 ;
                                     0013   140 ;
                                     0013   141 ;  ALTERNATE ENTRY FROM FSETI HERE
                                     0013   142 ;
                                     0013   143 ;  SET BUSY, CHECKING IF ALREADY ACTIVE
                                     0013   144 ;  STORE CALLER'S MODE AND ARGLIST ADDR IN IFAB
                                     0013   145 ;
                                     0013   146 RM$FSET_ALT::
            69            20     E2  0013   147 	BBSS	#IFB$V_BUSY,(R9),ERRACT
                          26         0016       
            68            1E     E0  0017   148 	BBS	#FAB$V_PPF_IND+<FAB$W_IFI*8>,(R8),CHKIND ;BRANCH IF INDIRECT PPF
                          19         001A       
                                     001B   149 	CSB	#IFB$V_PPF_IMAGE,(R9)	;MAKE SURE INDIRECT BIT OFF
            69            22     E5  001B       		BBCC	#IFB$V_PPF_IMAGE,(R9),30000$
                          00         001E       
                                     001F       30000$:
         0A A9            57     90  001F   150 SETMOD:	MOVB	R7,IFB$B_MODE(R9)	;SAVE CALLER'S MODE
         18 A9            5C     D0  0023   151 	MOVL	AP,IFB$L_ARGLST(R9)	;SAVE POINTER TO ARGLIST
            5A            59     D0  0027   152 	MOVL	R9,R10			;COPY IFAB ADDR
            5E            04     C1  002A   153 	ADDL3	#4,SP,IMP$L_SAVED_SP(R11)	;SAVE STACK ENTRY VALUE
                       14 AB         002D       
                                     002F   154 						; (LESS RETURN PC)
                           00000008  002F   155 	.IF NE $$RMSTEST&$$RMS_TBUGCHK
         24 A9            58     D0  002F   156 	MOVL	R8,IFB$L_LAST_FAB(R9)	;SAVE ADDR THIS FAB
                                     0033   157 	.ENDC
                                 05  0033   158 	RSB
                                     0034   159 ;
                                     0034   160 ;  THE IFI VALUE INDICATES INDIRECT PROCESSING OF A PROCESS PERMANENT FILE
                                     0034   161 ;
                                     0034   162 ;  SET PPF_IMAGE FLAG
                                     0034   163 ;
                                     0034   164 CHKIND:	SSB	#IFB$V_PPF_IMAGE,(R9)	;SET INDIRECT OPERATION FLAG
            69            22     E2  0034       		BBSS	#IFB$V_PPF_IMAGE,(R9),30001$
                          00         0037       
                                     0038       30001$:
                          E5     11  0038   165 	BRB	SETMOD				; AND CONTINUE
RM0FSET         SETUP FOR A FAB FUNCTION                         21-AUG-1978 21:23:12   VAX-11 MACRO X0.3-11               Page   5
