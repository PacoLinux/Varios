NT0ERASE        NETWORK ERASE FILE                               21-AUG-1978 22:21:56   VAX-11 MACRO X0.3-11               Page   3
007             NT$ERASE - PERFORM NETWORK ERASE FUNCTION                                                                        (3)

                                     0000    60 	.SBTTL	NT$ERASE - PERFORM NETWORK ERASE FUNCTION
                                     0000    61 
                                     0000    62 ;++
                                     0000    63 ; FUNCTIONAL DESCRIPTION:
                                     0000    64 ;
                                     0000    65 ;	NT$ERASE ENGAGES IN A DAP DIALOGUE WITH THE REMOTE FAL TO ERASE
                                     0000    66 ;	THE SPECIFIED FILE.
                                     0000    67 ;
                                     0000    68 ; CALLING SEQUENCE:
                                     0000    69 ;
                                     0000    70 ;	BSBW	NT$ERASE
                                     0000    71 ;
                                     0000    72 ; INPUT PARAMETERS:
                                     0000    73 ;
                                     0000    74 ;	R11	IMPURE AREA ADDRESS
                                     0000    75 ;	R10	FWA ADDRESS
                                     0000    76 ;	R9	IFAB ADDRESS
                                     0000    77 ;	R8	FAB ADDRESS
                                     0000    78 ;
                                     0000    79 ; IMPLICIT INPUTS:
                                     0000    80 ;
                                     0000    81 ;
                                     0000    82 ; OUTPUT PARAMETERS:
                                     0000    83 ;
                                     0000    84 ;	R0	STATUS CODE (RMS)
                                     0000    85 ;	R1-R7	DESTROYED
                                     0000    86 ;	AP	DESTROYED
                                     0000    87 ;
                                     0000    88 ; IMPLICIT OUTPUTS:
                                     0000    89 ;
                                     0000    90 ;	NONE
                                     0000    91 ;
                                     0000    92 ; COMPLETION CODES:
                                     0000    93 ;
                                     0000    94 ;	STANDARD RMS COMPLETION CODES.
                                     0000    95 ;
                                     0000    96 ; SIDE EFFECTS:
                                     0000    97 ;
                                     0000    98 ;	NONE
                                     0000    99 ;
                                     0000   100 ;--
                                     0000   101 
                                     0000   102 NT$ERASE::				; CONTROL POINT
                                     0000   103 	$TSTPT	NTERASE			;
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_NTERASE
            57         28 A9     D0  0006   104 	MOVL	IFB$L_NWA_PTR(R9),R7	; GET ADDRESS OF NWA
                          FFF3'  30  000A   105 	BSBW	NT$EXCH_CNF_ALT		; EXCHANGE CONFIGURATION MESSAGES
                                     000D   106 					;  WITH FAL
            32            50     E9  000D   107 	BLBC	R0,90$			; BRANCH ON FAILURE
            54       0144 C7     D0  0010   108 	MOVL	NWA$Q_BUFFER+4(R7),R4	; GET ADDRESS OF BUFFER
            55            54     D0  0015   109 	MOVL	R4,R5			; INITIALIZE NEXT BYTE POINTER
            85            03     90  0018   110 	MOVB	#DAP$K_ACC_MSG,(R5)+	; DAP MESSAGE TYPE FIELD
                          85     94  001B   111 	CLRB	(R5)+			; DAP FLAGS FIELD
            85            04     90  001D   112 	MOVB	#DAP$K_ERASE,(R5)+	; ACCFUNC FIELD
            85            01     90  0020   113 	MOVB	#DAP$M_NONFATAL,(R5)+	; ACCOPT FIELD
                          FFDA'  30  0023   114 	BSBW	NT$GET_FILESPEC		; STORE FILESPEC AS A COUNTED
                                     0026   115 					;  ASCII STRING
NT0ERASE        NETWORK ERASE FILE                               21-AUG-1978 22:21:56   VAX-11 MACRO X0.3-11               Page   4
007             NT$ERASE - PERFORM NETWORK ERASE FUNCTION                                                                        (3)

            55            54     C2  0026   116 	SUBL2	R4,R5			; CALCULATE SIZE OF THE MESSAGE
                          FFD4'  30  0029   117 	BSBW	NT$TRANSMIT_ALT		; SEND ACCESS MESSAGE TO FAL
            13            50     E9  002C   118 	BLBC	R0,90$			; BRANCH ON FAILURE
            55       0140 C7     D0  002F   119 	MOVL	NWA$Q_BUFFER(R7),R5	; GET RCV BUFFER SIZE
                                     0034   120 	SSB	#DAP$K_CMP_MSG,DAP$L_MSG_MASK(R7)
         0C A7            07     E2  0034       		BBSS	#DAP$K_CMP_MSG,DAP$L_MSG_MASK(R7),30000$
                          00         0038       
                                     0039       30000$:
                                     0039   121 					; EXPECT RESPONSE OF ACCESS COMPLETE MSG
                          FFC4'  30  0039   122 	BSBW	NT$RECEIVE_ALT		; GET REPLY FROM FAL
            03            50     E9  003C   123 	BLBC	R0,90$			; BRANCH ON FAILURE
                                     003F   124 	RMSSUC				; RETURN SUCCESS
            50            01     D0  003F       		MOVL	#1,R0
                                 05  0042   125 90$:	RSB				; EXIT WITH RMS CODE IN R0
                                     0043   126 	.END
