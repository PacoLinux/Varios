                                     0000    60 	.SBTTL	RMS$GET  -  COMMON $GET SETUP AND DISPATCH ROUTINE
                                     0000    61 ;++
                                     0000    62 ; FUNCTIONAL DESCRIPTION:
                                     0000    63 ;
                                     0000    64 ; THIS ROUTINE PERFORMS COMMON RAB FUNCTION SETUP FOLLOWED
                                     0000    65 ; BY DISPATCH TO ORGANIZATION-DEPENDENT $GET CODE
                                     0000    66 ;
                                     0000    67 ; CALLING SEQUENCE:
                                     0000    68 ;
                                     0000    69 ;	ENTERED FROM EXEC AS A RESULT OF USER'S CALLING SYS$GET
                                     0000    70 ;	(E.G., BY USING THE $GET MACRO)
                                     0000    71 ;
                                     0000    72 ; INPUT PARAMETERS:
                                     0000    73 ;
                                     0000    74 ;	AP	USER'S ARGUMENT LIST ADDR
                                     0000    75 ;
                                     0000    76 ; IMPLICIT INPUTS:
                                     0000    77 ;
                                     0000    78 ;	THE CONTENTS OF THE RAB AND RELATED IRAB AND IFAB.
                                     0000    79 ;
                                     0000    80 ; OUTPUT PARAMETERS:
                                     0000    81 ;
                                     0000    82 ;	R1	DESTROYED
                                     0000    83 ;	R0	STATUS CODE
                                     0000    84 ;
                                     0000    85 ; IMPLICIT OUTPUTS:
                                     0000    86 ;
                                     0000    87 ;	VARIOUS FIELDS OF THE RAB ARE FILLED IN TO REFLECT
                                     0000    88 ;	THE STATUS OF THE $GET OPERATION. (SEE RMS FUNCTIONAL
                                     0000    89 ; 	SPEC FOR A COMPLETE LIST.)
                                     0000    90 ;
                                     0000    91 ;	THE IRAB IS SIMILARLY UPDATED.
                                     0000    92 ;
                                     0000    93 ;	A COMPLETION AST IS QUEUED IF SPECIFIED IN THE USER ARGLIST.
                                     0000    94 ;
                                     0000    95 ; COMPLETION CODES:
                                     0000    96 ;
                                     0000    97 ;	STANDARD RMS (SEE FUNCTIONAL SPEC FOR LIST).
                                     0000    98 ;
                                     0000    99 ; SIDE EFFECTS:
                                     0000   100 ;
                                     0000   101 ;	NONE
                                     0000   102 ;
                                     0000   103 ;--
                                     0000   104 	$ENTRY	RMS$GET
                                 00000000       	.PSECT	$RMS0,GBL,NOWRT,QUAD
                                     0000   105 	$TSTPT	GET
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_GET
                                     0006   106 	$RABSET	FAC=IFB$V_GET	;DO COMMON SETUP
                          FFF7'  30  0006       		BSBW	RM$RSET
                                 01  0009       		.BYTE	<0 @ 5>!<0 @ 4>!<0 @ 3>!IFB$V_GET
                                     000A   107 				; RETURNS TO USER ON ERROR
                                     000A   108 ;
                                     000A   109 ; DISPATCH TO ORG-DEPENDENT CODE
                                     000A   110 ;
                                     000A   111 	CASE	TYPE=B, SRC=IFB$B_ORGCASE(R10),-
                                     000A   112 		DISPLIST=<RM$GET1,RM$GET2,RM$GET3>	; SEQ,REL,IDX ROUTINES
RMS0GET         DISPATCH FOR GET OPERATION                       21-AUG-1978 22:03:25   VAX-11 MACRO X0.3-11               Page   4
005             RMS$GET  -  COMMON $GET SETUP AND DISPATCH ROUT                                                                  (1)

            00         23 AA     8F  000A       	CASEB	IFB$B_ORGCASE(R10),#0,S^#<<30001$-30000$>/2>-1
                          02'        000E       
                                     000F       30000$:
                               FFF1' 000F       	.WORD	RM$GET1-30000$
                               FFF1' 0011       	.WORD	RM$GET2-30000$
                               FFF1' 0013       	.WORD	RM$GET3-30000$
                                     0015       30001$:
                           00000008  0015   113 	.IF NE $$RMSTEST&$$RMS_TBUGCHK
                          FFE8'  31  0015   114 	BRW	RM$ERRORG
                                     0018   115 	.ENDC
                                     0018   116 	.END
