                                     0000    63 	.SBTTL RMS$MODIFY  -  $MODIFY ROUTINE
                                     0000    64 
                                     0000    65 ;++
                                     0000    66 ; FUNCTIONAL DESCRIPTION:
                                     0000    67 ;
                                     0000    68 ;  THIS ROUTINE PERFORMS THE $MODIFY PROCESSING.
                                     0000    69 ;  IT HAS TWO MAIN FUNCTIONS:
                                     0000    70 ;
                                     0000    71 ;	1. TO REWRITE MODIFIED FILE ATTRIBUTES
                                     0000    72 ;	2. TO PROVIDE AN 'ESCAPE' MECHANISM TO PERFORM NON-STANDARD
                                     0000    73 ;	   RMS FUNCTIONS.
                                     0000    74 ;
                                     0000    75 ;
                                     0000    76 ; CALLING SEQUENCE:
                                     0000    77 ;
                                     0000    78 ;	ENTERED FROM EXEC AS A RESULT OF USER'S CALLING SYS$MODIFY
                                     0000    79 ;	(E.G., BY USING THE $MODIFY MACRO).
                                     0000    80 ;
                                     0000    81 ; INPUT PARAMETERS:
                                     0000    82 ;
                                     0000    83 ;	AP	USER'S ARGUMENT LIST ADDR
                                     0000    84 ;
                                     0000    85 ; IMPLICIT INPUTS:
                                     0000    86 ;
                                     0000    87 ;	THE CONTENTS OF THE FAB AND POSSIBLE RELATED USER INTERFACE
                                     0000    88 ;	BLOCKS.
                                     0000    89 ;
                                     0000    90 ; OUTPUT PARAMETERS:
                                     0000    91 ;
                                     0000    92 ;	R0	STATUS CODE
                                     0000    93 ;	R1	DESTROYED
                                     0000    94 ;
                                     0000    95 ; IMPLICIT OUTPUTS:
                                     0000    96 ;
                                     0000    97 ;	THE IFAB AND ALL RELATED INTERNAL RMS STRUCTURES ARE MODIFIED
                                     0000    98 ;	AS PER THE REQUREMENTS OF THE OPERATION.
                                     0000    99 ;	FAB$L_STS AND FAB$L_STV
                                     0000   100 ;
                                     0000   101 ;	A COMPLETION AST IS QUEUED IF SO SPECIFIED BY THE USER.
                                     0000   102 ;
                                     0000   103 ; COMPLETION CODES:
                                     0000   104 ;
                                     0000   105 ;	STANDARD RMS (SEE FUNCTIONAL SPEC FOR LIST).
                                     0000   106 ;
                                     0000   107 ; SIDE EFFECTS:
                                     0000   108 ;
                                     0000   109 ;	DEPENDENT UPON THE TYPE OF MODIFY
                                     0000   110 ;
                                     0000   111 ;--
RMS0MODFY       MODIFY FUNCTION                                  21-AUG-1978 22:06:26   VAX-11 MACRO X0.3-11               Page   4
