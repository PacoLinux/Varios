                                     0003   116 ;
                                     0003   117 ;  CHECK FOR MODIFY OF 'ESCAPE' TYPE AND BRANCH IF BIT SET
                                     0003   118 ;
            68            3B     E0  0003   119 	BBS	#FAB$V_ESC+FOP,(R8),ESCAPE	;BRANCH IF 'ESCAPE'
                          08         0006       
