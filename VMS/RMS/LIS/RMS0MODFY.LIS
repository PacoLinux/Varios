                          FFF1'  31  000C   125 MODXIT:	BRW	RM$EXRMS
RMS0MODFY       MODIFY FUNCTION                                  21-AUG-1978 22:06:26   VAX-11 MACRO X0.3-11               Page   5
002             PROCESS 'ESCAPE' SEQUENCE                                                                                        (1)

                                     000F   127 	.SBTTL	PROCESS 'ESCAPE' SEQUENCE
                                     000F   128 ;
                                     000F   129 ;++
                                     000F   130 ; FUNCTIONAL DESCRIPTION:
                                     000F   131 ;
                                     000F   132 ;  THE ESC BIT WAS SET IN FOP INDICATING THAT THE CALLER DESIRES
                                     000F   133 ;  TO EXECUTE ONE OF THE 'ESCAPE SEQUENCES', OTHERWISE KNOWN AS
                                     000F   134 ;  'BACK DOORS' OR 'KLUDGES', THAT IS, WAYS OF TRICKING RMS INTO
                                     000F   135 ;  THINKING THAT THE SITUATION IS OTHER THAN RMS'S CURRENT VIEW OF IT.
                                     000F   136 ;  THESE WILL, HOPEFULLY, REMAIN FEW IN NUMBER.  IMPLEMENTING THESE
                                     000F   137 ;  AS A SERVICE IS NECESSARY DUE TO THE REQUIREMENT FOR EXEC MODE
                                     000F   138 ;  PRIVILEGES AND ADDITIONALLY GIVES US A HANDLE ON THE EXTENT OF THE
                                     000F   139 ;  CANCER.  IMPROPER USE OF AN ESCAPE SEQUENCE CAN BLOW RMS OUT OF THE
                                     000F   140 ;  WATER.
                                     000F   141 ;
                                     000F   142 ; CALLIING SEQUENCE:
                                     000F   143 ;
                                     000F   144 ;	AS ABOVE.
                                     000F   145 ;
                                     000F   146 ; INPUT PARAMETERS:
                                     000F   147 ;
                                     000F   148 ;	FAB$V_CTX	'RME' CODE DEFINING WHICH ESCAPE SEQUENCE IS DESIRED.
                                     000F   149 ;	OTHER FAB FIELDS	AS REQUIRED BY PARTICULAR ESCAPE SEQUENCE
                                     000F   150 ;
                                     000F   151 ; OUTPUT PARAMETERS:
                                     000F   152 ;
                                     000F   153 ;	STS,STV
                                     000F   154 ;
                                     000F   155 ; 	OTHER FAB & IFAB FIELDS			"
                                     000F   156 ;
                                     000F   157 ; COMPLETION CODES:
                                     000F   158 ;
                                     000F   159 ;	STANDARD RMS.
                                     000F   160 ;
                                     000F   161 ; SIDE EFFECTS:
                                     000F   162 ;
                                     000F   163 ;	UNKNOWN, POTENTIALLY FATAL IF MISUSED.
                                     000F   164 ;
                                     000F   165 ;--
                                     000F   166 ESCAPE:	$TSTPT	MODIFY2
                 00000200'9F     D6  000F       		INCL	@#PIO$GL_FMLH+512+TPT$L_MODIFY2
                                     0015   167 	RMSERR	IAL				;ANTICIPATE ERROR
            50       854C 8F     3C  0015       		MOVZWL	#<RMS$_IAL&^XFFFF>,R0
            01         18 A8     D1  001A   168 	CMPL	FAB$L_CTX(R8),#RME$C_SETRFM	;KNOWN 'ESCAPE' TYPE?
                          EC     12  001E   169 	BNEQ	MODXIT				;BRANCH IF NOT
RMS0MODFY       MODIFY FUNCTION                                  21-AUG-1978 22:06:26   VAX-11 MACRO X0.3-11               Page   6
002             PROCESS 'ESCAPE' SEQUENCE                                                                                        (1)

                                     0020   171 ;++
                                     0020   172 ;  ESCAPE TYPE ONE  -  SET RFM
                                     0020   173 ;
                                     0020   174 ;  INPUTS:
                                     0020   175 ;
                                     0020   176 ;	RFM,MRS, AND FSZ (IF VFC)
                                     0020   177 ;
                                     0020   178 ;  OUTPUTS:
                                     0020   179 ;
                                     0020   180 ;	RELATED IFAB FIELDS ARE CHANGED TO VALUES SPECIFIED BY INPUTS.
                                     0020   181 ;
                                     0020   182 ;  NOTES:
                                     0020   183 ;
                                     0020   184 ;	1.  USER IS RESPONSIBLE FOR SAVING THE PREVIOUS CONTENTS OF THE
                                     0020   185 ;	    RFM, MRS, AND FSZ FIELDS IF NEEDED FOR LATER RESTORE
                                     0020   186 ;
                                     0020   187 ;	2.  IF THE FILE IS ACCESSED FOR PUT, FINAL ATTRIBUTES WRITTEN
                                     0020   188 ;	    TO THE FILE ON CLOSE WILL BE THOSE CURRENTLY IN EFFECT
                                     0020   189 ;
                                     0020   190 ;	3.  THERE ARE NO DEFAULT VALUES FOR ANY OF THE INPUT FIELDS
                                     0020   191 ;
                                     0020   192 ;	4.  IF SETTING RFM TO UDF AND NOT BLOCK I/O ACCESSED, RESULTS
                                     0020   193 ;	    ARE UNPREDICTABLE
                                     0020   194 ;
                                     0020   195 ;	5.  IF SETTING RFM TO FIX AND MRS IS 0, AN ERROR IS GENERATED
                                     0020   196 ;	    BUT FURTHER RMS CALLS WILL PRODUCE UNPREDICTABLE RESULTS
                                     0020   197 ;--
                                     0020   198 	RMSERR	RFM				;ANTICIPATE PROBLEMS
            50       8664 8F     3C  0020       		MOVZWL	#<RMS$_RFM&^XFFFF>,R0
            03         1F A8     91  0025   199 	CMPB	FAB$B_RFM(R8),#FAB$C_MAXRFM	;WITHIN RANGE?
                          E1     1A  0029   200 	BGTRU	MODXIT				;BRANCH IF NOT
                                     002B   201 	ASSUME	FAB$C_VFC EQ FAB$C_MAXRFM
                          05     12  002B   202 	BNEQ	10$				;BRANCH IF NOT VFC FORMAT
         4B A9         3F A8     90  002D   203 	MOVB	FAB$B_FSZ(R8),IFB$B_FSZ(R9)	;SET FSZ
         3C A9         1F A8     90  0032   204 10$:	MOVB	FAB$B_RFM(R8),IFB$B_RFMORG(R9)	;SET RFM
         4C A9         36 A8     B0  0037   205 	MOVW	FAB$W_MRS(R8),IFB$W_MRS(R9)	;SET MRS
            01         1F A8     91  003C   206 	CMPB	FAB$B_RFM(R8),#FAB$C_FIX	;FIXED RFM?
                          0C     12  0040   207 	BNEQ	20$				;BRANCH IF NOT
                                     0042   208 	RMSERR	MRS				;ANTICIPATE PROBLEM
            50       85D4 8F     3C  0042       		MOVZWL	#<RMS$_MRS&^XFFFF>,R0
         3E A9         36 A8     B0  0047   209 	MOVW	FAB$W_MRS(R8),IFB$W_LRL(R9)	;SET LRL
                          BE     13  004C   210 	BEQL	MODXIT				;BRANCH IF ZERO (ERROR)
                                     004E   211 20$:	RMSSUC
            50            01     D0  004E       		MOVL	#1,R0
                          B9     11  0051   212 	BRB	MODXIT
                                     0053   213 	.END
