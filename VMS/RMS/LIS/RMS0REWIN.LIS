                                     0044   156 ;  RELATIVE FILE ORG REWIND
                                     0044   157 ;
                                     0044   158 ;***************************************************************************
                                     0044   159 	ASSUME	 IRB$L_NRP_OFF EQ IRB$L_NRP_VBN+4
                       2C A9     7C  0044   160 RWREL:	CLRQ	IRB$L_NRP_VBN(R9)		; RESET NRP
            6A            18     E0  0047   161 	BBS	#DEV$V_FOR,IFB$L_PRIM_DEV(R10),10$	; BRANCH IF NON-FILE-STRUC.
                          03         004A       
                       2C A9     96  004B   162 	INCB	IRB$L_NRP_VBN(R9)		; NRP = VBN 1
                                     004E   163 10$:
                                     004E   164 	ASSUME	IRB$L_RP_OFF EQ IRB$L_RP_VBN+4
                       34 A9     7C  004E   165 	CLRQ	IRB$L_RP_VBN(R9)
                                     0051   166 ;
                                     0051   167 ;  CLEAR ALL IRAB BOOKKEEPING BITS EXCEPT BUSY, ASYNC, AND ASYNCWAIT
                                     0051   168 ;
         04 A9   FFFFFBE6 8F     CA  0051   169 DONE:	BICL	#BKPMASK,IRB$L_BKPBITS(R9)
                                     0059   170 	RMSSUC
            50            01     D0  0059       		MOVL	#1,R0
                          FFA1'  31  005C   171 EXIT:	BRW	RM$EXRMS			; RETURN TO CALLER
                                     005F   172 ;++
                                     005F   173 ;
                                     005F   174 ;  PERFORM NETWORK REWIND FUNCTION
                                     005F   175 ;
                                     005F   176 ;--
                                     005F   177 NTRWND:	$PSECT_BR	NF$NETWORK
                          FF9E'  31  005F       		BRW	NF$NETWORK0
                                     0062       		.SAVE
                                 00000000       		.PSECT	NF$NETWORK,GBL,NOWRT
                                     0000       NF$NETWORK0:
                          FFFD'  30  0000   178 	BSBW	NT$REWIND			; REWIND THE FILE
            03            50     E9  0003   179 	BLBC	R0,10$				; BRANCH ON ERROR
                          0048'  31  0006   180 	BRW	DONE				; REJOIN MAINLINE
                          FFF4'  31  0009   181 10$:	BRW	RM$EXRMS			; RETURN TO CALLER
                                     000C   182 	$PSECT_RESTORE
                                 00000062       		.RESTORE
                                     0062   183 ;
                                     0062   184 ;
                                     0062   185 ;  HANDLE ERROR
                                     0062   186 ;
                                     0062   187 ERRIOP:	$PSECT_BR	L$RMS0
                          FF9B'  31  0062       		BRW	L$RMS01
                                     0065       		.SAVE
                                 00000000       		.PSECT	L$RMS0,GBL,NOWRT
                                     0000       L$RMS01:
                                     0000   188 	RMSERR	IOP				; CAN'T REWIND UNIT REC DEV. OR
            50       8574 8F     3C  0000       		MOVZWL	#<RMS$_IOP&^XFFFF>,R0
                                     0005   189 						; PROCESS PERM. FILE IF INDIRECTLY
                                     0005   190 						; CONNECTED
                          FFF8'  31  0005   191 	BRW	RM$EXRMS
                                     0008   192 	$PSECT_RESTORE
                                 00000065       		.RESTORE
                                     0065   193 	.END
