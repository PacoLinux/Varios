                                     0000    69 	.SBTTL	RMS$REWIND  -  $REWIND FILE ROUTINE
                                     0000    70 ;++
                                     0000    71 ; FUNCTIONAL DESCRIPTION:
                                     0000    72 ;
                                     0000    73 ; THIS ROUTINE PERFORMS COMMON RAB FUNCTION SETUP FOLLOWED BY AN
                                     0000    74 ; IMPLICIT $FREE AND $FLUSH AND THEN BY DISPATCH TO ORGANIZATION-DEPENDENT
                                     0000    75 ; $REWIND CODE
                                     0000    76 ;
                                     0000    77 ; CALLING SEQUENCE:
                                     0000    78 ;
                                     0000    79 ;	ENTERED FROM EXEC AS A RESULT OF USER'S CALLING SYS$REWIND
                                     0000    80 ;	(E.G., BY USING THE $REWIND MACRO)
                                     0000    81 ;
                                     0000    82 ; INPUT PARAMETERS:
                                     0000    83 ;
                                     0000    84 ;	AP	USER'S ARGUMENT LIST ADDR
                                     0000    85 ;
                                     0000    86 ; IMPLICIT INPUTS:
                                     0000    87 ;
                                     0000    88 ;	THE CONTENTS OF THE RAB AND RELATED IRAB AND IFAB.
                                     0000    89 ;
                                     0000    90 ; OUTPUT PARAMETERS:
                                     0000    91 ;
                                     0000    92 ;	R1	DESTROYED
                                     0000    93 ;	R0	STATUS CODE
                                     0000    94 ;
                                     0000    95 ; IMPLICIT OUTPUTS:
                                     0000    96 ;
                                     0000    97 ;	ALL LOCKED RECORDS FOR THE STREAM ARE FREED, AND THE STREAM IS
                                     0000    98 ;	REPOSITIONED AT THE START OF THE FILE.  THE FILE ATTRIBUTES ARE ALSO
                                     0000    99 ;	REWRITTEN.
                                     0000   100 ;	FOR THE SEQUENTIAL ORG., ALL BDB'S AND BUFFERS ARE RELEASED.
                                     0000   101 ;	RAB$L_STS AND RAB$L_STV REFLECT THE STATUS OF THE REWIND.
                                     0000   102 ;
                                     0000   103 ;	A COMPLETION AST IS QUEUED IF SPECIFIED IN THE USER ARGLIST.
                                     0000   104 ;
                                     0000   105 ; COMPLETION CODES:
                                     0000   106 ;
                                     0000   107 ;	STANDARD RMS (SEE FUNCTIONAL SPEC FOR LIST).
                                     0000   108 ;
                                     0000   109 ; SIDE EFFECTS:
                                     0000   110 ;
                                     0000   111 ;	NONE
                                     0000   112 ;
                                     0000   113 ;--
RMS0REWIN       FILE REWIND OPERATION                            21-AUG-1978 22:09:55   VAX-11 MACRO X0.3-11               Page   4
