                                     0000   115 	$ENTRY	RMS$REWIND
                                 00000000       	.PSECT	F$RMSCLOSE0,GBL,NOWRT
                                     0000   116 	$TSTPT	REWIND
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_REWIND
                                     0006   117 	$RABSET			;DO COMMON SETUP WITH NO SPECIFIC CHECKS
                          FFF7'  30  0006       		BSBW	RM$RSET
                                 00  0009       		.BYTE	<0 @ 5>!<0 @ 4>!<0 @ 3>!0
                                     000A   118 				; RETURNS TO USER ON ERROR
            6A            3E     E0  000A   119 	BBS	#IFB$V_DAP,(R10),NTRWND		; BRANCH IF NETWORK OPERATION
                          51         000D       
            6A            2D     E1  000E   120 	BBC	#IFB$V_SQO,(R10),5$		; BRANCH IF SQO OFF
                          04         0011       
            69            22     E0  0012   121 	BBS	#IRB$V_PPF_IMAGE,(R9),ERRIOP	; BRANCH IF INDIRECT PPF
                          4C         0015       
            6A            0E     E1  0016   122 5$:	BBC	#DEV$V_FOD,IFB$L_PRIM_DEV(R10),ERRIOP	; OR IF UNIT REC DEVICE
                          48         0019       
            6A            33     E0  001A   123 	BBS	#IFB$V_NORECLK,(R10),10$	; OMIT RECORD UNLOCK IF NO LOCKING
                          03         001D       
                          FFDF'  30  001E   124 	BSBW	RM$UNLOCKALL			; FREE ANY LOCKED RECORDS
                                     0021   125 						; (IGNORE POSSIBLE ERROR)
                                     0021   126 10$:
                                     0021   127 ;
                                     0021   128 ; DISPATCH TO ORG-DEPENDENT CODE
                                     0021   129 ;
                                     0021   130 	CASE	TYPE=B, SRC=IFB$B_ORGCASE(R10),-	;AND DISPATCH
                                     0021   131 		DISPLIST=<RWSEQ,RWREL>
            00         23 AA     8F  0021       	CASEB	IFB$B_ORGCASE(R10),#0,S^#<<30001$-30000$>/2>-1
                          01'        0025       
                                     0026       30000$:
                               0004' 0026       	.WORD	RWSEQ-30000$
                               001E' 0028       	.WORD	RWREL-30000$
                                     002A       30001$:
                                     002A   132 ;
                                     002A   133 ;  INDEX FILE REWIND FALLS THRU
                                     002A   134 ;  !!!!! CODE MISSING !!!!!
                                     002A   135 ;
                                     002A   136 ;****************************************************************************
                                     002A   137 ;
                                     002A   138 ;  SEQUENTIAL FILE ORG REWIND
                                     002A   139 ;
                                     002A   140 ;****************************************************************************
                          FFD3'  30  002A   141 RWSEQ:	BSBW	RM$FLUSH			; WRITE AND RELEASE ANY BDBS
                                     002D   142 						; REWRITING FILE ATTRIBUTES
            2C            50     E9  002D   143 	BLBC	R0,EXIT				; GET OUT ON ERROR
                       42 A9     B4  0030   144 	CLRW	IRB$W_CSIZ(R9)			; RESET TO NO CURRENT RECORD
            6A            05     E1  0033   145 	BBC	#DEV$V_SQD,IFB$L_PRIM_DEV(R10),RWREL	; BRANCH IF NOT MAGTAPE
                          0D         0036       
                                     0037   146 ;
                                     0037   147 ;  MAGTAPE SUPPORT
                                     0037   148 ;
         44 AA            01     CE  0037   149 	MNEGL	#1,IFB$L_EBK(R10)		; SET EOF BLOCK TO MAX VALUE
                       48 AA     B4  003B   150 	CLRW	IFB$W_FFB(R10)			; RESET EOF OFFSET
                          FFBF'  30  003E   151 	BSBW	RM$REWIND_MT			; DO REWIND
            18            50     E9  0041   152 	BLBC	R0,EXIT				; GET OUT ON ERROR
                                     0044   153 						; ELSE FALL THRU
                                     0044   154 ;***************************************************************************
                                     0044   155 ;
