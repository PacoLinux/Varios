NT0DISCON       NETWORK DISCONNECT STREAM                        21-AUG-1978 22:21:14   VAX-11 MACRO X0.3-11               Page   3
007             NT$DISCONNECT - PERFORM NETWORK DISCONNECT FUNC                                                                  (3)

                                     0000    60 	.SBTTL	NT$DISCONNECT - PERFORM NETWORK DISCONNECT FUNCTION
                                     0000    61 
                                     0000    62 ;++
                                     0000    63 ; FUNCTIONAL DESCRIPTION:
                                     0000    64 ;
                                     0000    65 ;	NT$DISCONNECT ENGAGES IN A DAP DIALOGUE WITH THE REMOTE FAL TO
                                     0000    66 ;	DISCONNECT THE RECORD STREAM.
                                     0000    67 ;
                                     0000    68 ; CALLING SEQUENCE:
                                     0000    69 ;
                                     0000    70 ;	BSBW	NT$DISCONNECT
                                     0000    71 ;
                                     0000    72 ; INPUT PARAMETERS:
                                     0000    73 ;
                                     0000    74 ;	R11	IMPURE AREA ADDRESS
                                     0000    75 ;	R10	IFAB ADDRESS
                                     0000    76 ;	R9	IRAB ADDRESS
                                     0000    77 ;	R8	RAB ADDRESS
                                     0000    78 ;
                                     0000    79 ; IMPLICIT INPUTS:
                                     0000    80 ;
                                     0000    81 ;	NONE
                                     0000    82 ;
                                     0000    83 ; OUTPUT PARAMETERS:
                                     0000    84 ;
                                     0000    85 ;	R0	STATUS CODE (RMS)
                                     0000    86 ;	R1-R7	DESTROYED
                                     0000    87 ;	AP	DESTROYED
                                     0000    88 ;
                                     0000    89 ; IMPLICIT OUTPUTS:
                                     0000    90 ;
                                     0000    91 ;	NONE
                                     0000    92 ;
                                     0000    93 ; COMPLETION CODES:
                                     0000    94 ;
                                     0000    95 ;	STANDARD RMS COMPLETION CODES.
                                     0000    96 ;
                                     0000    97 ; SIDE EFFECTS:
                                     0000    98 ;
                                     0000    99 ;	NONE
                                     0000   100 ;
                                     0000   101 ;--
                                     0000   102 
                                     0000   103 NT$DISCONNECT::				; CONTROL POINT
                                     0000   104 	$TSTPT	NTDISCON		;
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_NTDISCON
            57         28 AA     D0  0006   105 	MOVL	IFB$L_NWA_PTR(R10),R7	; GET ADDRESS OF NWA
            67            21     E1  000A   106 	BBC	#DAP$V_VAXVMS,DAP$Q_DCODE_FLG(R7),10$
                          2D         000D       
                                     000E   107 					; SEND DISCONNECT IFF PARTNER IS VAX/VMS
            54       0144 C7     D0  000E   108 	MOVL	NWA$Q_BUFFER+4(R7),R4	; GET ADDRESS OF BUFFER
            55            54     D0  0013   109 	MOVL	R4,R5			; INITIALIZE NEXT BYTE POINTER
            85            07     90  0016   110 	MOVB	#DAP$K_CMP_MSG,(R5)+	; DAP MESSAGE TYPE FIELD
                          85     94  0019   111 	CLRB	(R5)+			; FLAGS FIELD
            85            04     90  001B   112 	MOVB	#DAP$K_DISCON,(R5)+	; CMPFUNC FIELD
            55            54     C2  001E   113 	SUBL2	R4,R5			; CALCULATE SIZE OF THE MESSAGE
                          FFDC'  30  0021   114 	BSBW	NT$TRANSMIT		; SEND ACCESS COMPLETE MESSAGE TO FAL
NT0DISCON       NETWORK DISCONNECT STREAM                        21-AUG-1978 22:21:14   VAX-11 MACRO X0.3-11               Page   4
007             NT$DISCONNECT - PERFORM NETWORK DISCONNECT FUNC                                                                  (3)

            17            50     E9  0024   115 	BLBC	R0,90$			; BRANCH ON FAILURE
            67            20     E1  0027   116 	BBC	#DAP$V_AFTER_V41,DAP$Q_DCODE_FLG(R7),10$
                          10         002A       
                                     002B   117 					; EXPECT ACKNOWLEDGE OF DISCONNECT
                                     002B   118 					;  IFF PARTNER IMPLEMENTED TO DAP
                                     002B   119 					;  AFTER V4.1
            55       0140 C7     D0  002B   120 	MOVL	NWA$Q_BUFFER(R7),R5	; GET RCV BUFFER SIZE
                                     0030   121 	SSB	#DAP$K_ACK_MSG,DAP$L_MSG_MASK(R7)
         0C A7            06     E2  0030       		BBSS	#DAP$K_ACK_MSG,DAP$L_MSG_MASK(R7),30000$
                          00         0034       
                                     0035       30000$:
                                     0035   122 					; EXPECT RESPONSE OF ACKNOWLEDGE MESSAGE
                          FFC8'  30  0035   123 	BSBW	NT$RECEIVE		; GET REPLY FROM FAL
            03            50     E9  0038   124 	BLBC	R0,90$			; BRANCH ON FAILURE
                                     003B   125 10$:	RMSSUC				; RETURN SUCCESS
            50            01     D0  003B       		MOVL	#1,R0
                                 05  003E   126 90$:	RSB				; EXIT WITH RMS CODE IN R0
                                     003F   127 	.END
