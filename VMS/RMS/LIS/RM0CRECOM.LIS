                                     0014    89 	.SBTTL RM$CREATECOM  -  PERFORM CREATE FILE FUNCTION
                                     0014    90 ;++
                                     0014    91 ; FUNCTIONAL DESCRIPTION:
                                     0014    92 ;
                                     0014    93 ;	THIS ROUTINE SETS UP THE FIB FROM THE
                                     0014    94 ;	VARIOUS USER OPTIONS, DIRECTORY ID AND
                                     0014    95 ;	ALLOCATION INFORMATION, BUILDS THE 
                                     0014    96 ;	ATTRIBUTE LIST TO WRITE THE USER RECORD
                                     0014    97 ;	AND OTHER ATTRIBUTES, BUILDS
                                     0014    98 ;	THE QIO PARAMETER LIST ON THE STACK 
                                     0014    99 ;	ISSUES THE QIO TO THE ACP TO PERFORM
                                     0014   100 ;	THE CREATE (OR ACCESS IF 'CIF' BIT SET),
                                     0014   101 ;	INITIALIZES THE HBK FIELD OF THE IFAB,
                                     0014   102 ;	AND FINALLY CALLS RM$FILLNAM TO RETURN
                                     0014   103 ; 	THE RESULTANT NAME STRING.
                                     0014   104 ;
                                     0014   105 ; CALLING SEQUENCE:
                                     0014   106 ;
                                     0014   107 ;	BSBW	RM$CREATECOM
                                     0014   108 ;
                                     0014   109 ; INPUT PARAMETERS:
                                     0014   110 ;
                                     0014   111 ;	R11	IMPURE AREA ADDR
                                     0014   112 ;	R10	FWA ADDRESS
                                     0014   113 ;	R9	IFAB ADDRESS
                                     0014   114 ;	R8	FAB ADDRESS
                                     0014   115 ;
                                     0014   116 ; IMPLICIT INPUTS:
                                     0014   117 ;
                                     0014   118 ;	THE CONTENTS OF THE PARAMETER BLOCKS LISTED
                                     0014   119 ;	ABOVE, ESPECIALLY:
                                     0014   120 ;
                                     0014   121 ;	IFB$L_PRIM_DEV
                                     0014   122 ;	FWA$Q_DIR
                                     0014   123 ;	FWA$Q_NAME
                                     0014   124 ;	FWA$L_ATRLADR
                                     0014   125 ;	IFB$L_CHNL
                                     0014   126 ;	IFB$B_FAC
                                     0014   127 ;	IFB$L_RFMORG THRU IFB$C_FHAEND
                                     0014   128 ;	FAB$L_FOP
                                     0014   129 ;	FAB$L_ALQ
                                     0014   130 ;	FAB$L_XAB
                                     0014   131 ;	FAB$L_NAM
                                     0014   132 ;
                                     0014   133 ;
                                     0014   134 ; OUTPUT PARAMETERS:
                                     0014   135 ;
                                     0014   136 ;	R0	STATUS CODE
                                     0014   137 ;	R6	FIB ADDR
                                     0014   138 ;	R1-R5,R7,AP DESTROYED
                                     0014   139 ;
                                     0014   140 ; IMPLICIT OUTPUTS:
                                     0014   141 ;
                                     0014   142 ;	IFB$V_ACCESSED SET
                                     0014   143 ;	NAM$L_RSL SET TO LENGTH OF RESULTANT STRING, IF ANY
                                     0014   144 ;	IFB$L_IOS
                                     0014   145 ;	IFB$L_HBK
