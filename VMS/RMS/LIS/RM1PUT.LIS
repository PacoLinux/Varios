                          05     11  000C   185 	BRB	ERROR
                                     000E   186 	$PSECT_RESTORE
                                 0000007B       		.RESTORE
                                     007B   187 ERRSQO:	$PSECT_BR	L$RMS1
                          FF90'  31  007B       		BRW	L$RMS12
                                     007E       		.SAVE
                                 0000000E       		.PSECT	L$RMS1,GBL,NOWRT
                                     000E       L$RMS12:
                                     000E   188 
                                     000E   189 	RMSERR	SQO				; SEQ OUTPUT ONLY AND USER
            50       86C4 8F     3C  000E       		MOVZWL	#<RMS$_SQO&^XFFFF>,R0
