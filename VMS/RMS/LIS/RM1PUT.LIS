                                     0000    66 	.SBTTL RM$PUT1 - HIGH LEVEL SEQUENTIAL $PUT
                                     0000    67 ;++
                                     0000    68 ; FUNCTIONAL DESCRIPTION:
                                     0000    69 ;
                                     0000    70 ; THIS MODULE PERFORMS THE FOLLOWING FUNCTIONS:
                                     0000    71 ;
                                     0000    72 ;	1.  CALLS RM$PUTSETUP1 TO PERFORM VARIOUS SETUPS
                                     0000    73 ;	2.  INITIALIZES THE CURRENT RECORD SIZE TO ZERO
                                     0000    74 ;	3.  VERIFIES THAT RAC = SEQUENTIAL OR KEY (IF RFM=FIX)
                                     0000    75 ;	4.  IF DEVICE IS UNIT RECORD CALLS RM$PUT_UNIT_REC
                                     0000    76 ;	    OTHERWISE, VERIFIES POSITIONING AT EOF
                                     0000    77 ;	    AND CALLS RM$PUT_BLK_DEV UNLESS RAC=KEY IN WHICH CASE CALLS
                                     0000    78 ;	    RM$UPDATE_ALT
                                     0000    79 ;
                                     0000    80 ;
                                     0000    81 ; CALLING SEQUENCE:
                                     0000    82 ;
                                     0000    83 ; ENTERED VIA CASE BRANCH FROM RM$PUT AT RM$PUT1.
                                     0000    84 ;
                                     0000    85 ; INPUT PARAMETERS:
                                     0000    86 ;
                                     0000    87 ;	R11	IMPURE AREA ADDRESS
                                     0000    88 ;	R10	IFAB ADDR
                                     0000    89 ;	R9	IRAB ADDR
                                     0000    90 ;	R8	RAB ADDR
                                     0000    91 ;
                                     0000    92 ; IMPLICIT INPUTS:
                                     0000    93 ;
                                     0000    94 ;	THE CONTENTS OF THE RAB AND RELATED IRAB AND IFAB.
                                     0000    95 ;
                                     0000    96 ; OUTPUT PARAMETERS:
                                     0000    97 ;
                                     0000    98 ;	R7 THRU R1	DESTROYED
                                     0000    99 ;	R0		STATUS
                                     0000   100 ;
                                     0000   101 ; IMPLICIT OUTPUTS:
                                     0000   102 ;
                                     0000   103 ;	VARIOUS FIELDS OF THE RAB ARE FILLED IN TO REFLECT
                                     0000   104 ;	THE STATUS OF THE OPERATION (SEE FUNCTIONAL SPEC
                                     0000   105 ;	FOR DETAILS).
                                     0000   106 ;
                                     0000   107 ;	THE IRAB IS SIMILARLY UPDATED.
                                     0000   108 ;
                                     0000   109 ; COMPLETION CODES:
                                     0000   110 ;
                                     0000   111 ;	STANDARD RMS (SEE FUNCTIONAL SPEC).
                                     0000   112 ;
                                     0000   113 ; SIDE EFFECTS:
                                     0000   114 ;
                                     0000   115 ;	NONE
                                     0000   116 ;
                                     0000   117 ;--
RM1PUT          SEQUENTIAL SPECIFIC PUT                          21-AUG-1978 21:44:27   VAX-11 MACRO X0.3-11               Page   4
012             RM$PUT1 - HIGH LEVEL SEQUENTIAL $PUT                                                                             (1)

                                     0000   119 RM$PUT1::
                                     0000   120 	$TSTPT	PUT1
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_PUT1
                          FFF7'  30  0006   121 	BSBW	RM$PUTSETUP1			; PERFORM VARIOUS PUT SETUPS
                       42 A9     B4  0009   122 	CLRW	IRB$W_CSIZ(R9)			; INDICATE NO CURRENT RECORD
            53            50     E9  000C   123 	BLBC	R0,EXIT
                                     000F   124 ;
                                     000F   125 ; VERIFY THAT RECORD ACCESS MODE IS SEQUENTIAL
                                     000F   126 ;
                                     000F   127 	ASSUME	RAB$C_SEQ EQ 0
                       1E A8     95  000F   128 	TSTB	RAB$B_RAC(R8)
                          10     12  0012   129 	BNEQ	CHKRRN
                                     0014   130 	ASSUME	DEV$V_REC EQ 0
            03            6A     E9  0014   131 	BLBC	IFB$L_PRIM_DEV(R10),BLKDEV	; BRANCH IF NOT UNIT RECORD DEVICE
                                     0017   132 ;
                                     0017   133 ; $PUT TO A UNIT RECORD DEVICE
                                     0017   134 ;
                          FFE6'  31  0017   135 	BRW	RM$PUT_UNIT_REC
                                     001A   136 ;
                                     001A   137 ;  SEQUENTIAL $PUT TO A BLOCK DEVICE
                                     001A   138 ;  MUST BE POSITIONED AT EOF
                                     001A   139 ;
                          FFE3'  30  001A   140 BLKDEV:	BSBW	RM$CHKEOF1			; GET EOF BIT SET IF AT END
            69            21     E1  001D   141 	BBC	#IRB$V_EOF,(R9),ERRNEF		; BRANCH IF NOT EOF
                          44         0020       
                          FFDC'  31  0021   142 PUTBLK:	BRW	RM$PUT_BLK_DEV			; DO THE PUT
                                     0024   143 ;
                                     0024   144 ;  CODE TO PERFORM RANDOM MODE $PUT BY RELATIVE RECORD #
                                     0024   145 ;
                                     0024   146 CHKRRN:
            01         1E A8     91  0024   147 	CMPB	RAB$B_RAC(R8),#RAB$C_KEY	; KEYED ACCESS?
                          4E     12  0028   148 	BNEQ	ERRRAC				; NO - IT'S A PROBLEM
            6A            1C     E1  002A   149 	BBC	#DEV$V_RND,IFB$L_PRIM_DEV(R10),ERRRAC	;BRANCH IF NOT DISK
                          4A         002D       
            6A            2D     E0  002E   150 	BBS	#IFB$V_SQO,(R10),ERRSQO		;BRANCH IF SQO SET
                          49         0031       
                          FFCB'  30  0032   151 	BSBW	RM$SEQKEY			;CONVERT RRN TO RFA
            2A            50     E9  0035   152 	BLBC	R0,EXIT				;GET OUT ON ERRORS
            68            24     E0  0038   153 	BBS	#RAB$V_UIF+ROP,(R8),10$		;BRANCH IF PUT ANYWHERE IS OK
                          14         003B       
            6A            18     E0  003C   154 	BBS	#DEV$V_FOR,IFB$L_PRIM_DEV(R10),10$	; BRANCH IF FOREIGN DEVICE
                          10         003F       
         44 AA         10 A8     D1  0040   155 	CMPL	RAB$W_RFA(R8),IFB$L_EBK(R10)	;CHECK FOR ATTEMPT TO PUT
                                     0045   156 						;BEYOND EOF
                          2E     1F  0045   157 	BLSSU	ERRNEF1				;BRANCH IF BEFORE EOF (ERROR)
                          07     1A  0047   158 	BGTRU	10$				;BRANCH IF BEYOND
         48 AA         14 A8     B1  0049   159 	CMPW	RAB$W_RFA+4(R8),IFB$W_FFB(R10)	;IN EOF BLK - CHECK OFFSET
                          25     1F  004E   160 	BLSSU	ERRNEF1				;BRANCH IF BEFORE EOF
         42 A9            56     B0  0050   161 10$:	MOVW	R6,IRB$W_CSIZ(R9)		;SET CURRENT RECORD SIZE
            51            56     D0  0054   162 	MOVL	R6,R1				;RESTORE RECORD SIZE
         34 A9         10 A8     7D  0057   163 	MOVQ	RAB$W_RFA(R8),IRB$L_RP_VBN(R9)	;SET RP FROM RFA
                       3A A9     B4  005C   164 	CLRW	IRB$L_RP_OFF+2(R9)		; INSURE VALID LONG WORD OFFSET
                          FF9E'  31  005F   165 	BRW	RM$UPDATE_ALT			;GO DO THE RANDOM PUT
                          FF9B'  31  0062   166 EXIT:	BRW	RM$EXRMS			; RETURN TO THE USER
