                                     00C5   227 ;++
                                     00C5   228 ; RMS RECEIVED A DAP STATUS MESSAGE FROM PARTNER (FAL).
                                     00C5   229 ; IF THE DAP STATUS CODE CAN BE MAPPED INTO A STANDARD (NON-NETWORK)
                                     00C5   230 ; RMS COMPLETION CODE:
                                     00C5   231 ;    A)	STS OF FAB/RAB = MAPPED RMS CODE
                                     00C5   232 ;    B) STV OF FAB/RAB = 0
                                     00C5   233 ; ELSE:
                                     00C5   234 ;    A) STS OF FAB/RAB = RMS$_SUP, RMS$_NET, OR RMS$_BUG_DAP
                                     00C5   235 ;    B) STV OF FAB/RAB = DAP STATUS CODE
                                     00C5   236 ;--
                                     00C5   237 
                                     00C5   238 STATUS_RETURN:				; CONTROL POINT
            04            0C     EF  00C5   239 	EXTZV	#12,#4,DAP$W_STSCODE(R7),R2 ; GET MACCODE FIELD
            52       0098 C7         00C8       
                                     00CC   240 	$CASEB	SELECTOR=R2,DISPL=<-	; STATUS RETURNED BY PARTNER:
                                     00CC   241 		FILE_ACCESS-		;  FILE OPERATION PENDING
                                     00CC   242 		FILE_ACCESS-		;  FILE OPERATION SUCCESSFUL
                                     00CC   243 		UNSUPPORT-		;  REQUEST NOT SUPPORTED
                                     00CC   244 		PROTOCOL-		;  UNDEFINED VALUE
                                     00CC   245 		FILE_ACCESS-		;  ERROR RELATED TO OPENING A FILE
                                     00CC   246 		FILE_ACCESS-		;  ERROR RELATED TO FILE TRANSFER
                                     00CC   247 		PROTOCOL-		;  UNDEFINED VALUE
                                     00CC   248 		FILE_ACCESS-		;  ERROR RELATED TO CLOSING A FILE
                                     00CC   249 ;		PROTOCOL-		;  INCORRECT MESSAGE FORMAT
                                     00CC   250 ;		PROTOCOL-		;  INVALID FIELD VALUE
                                     00CC   251 ;		PROTOCOL-		;  UNEXPECTED MESSAGE FOR STATE
                                     00CC   252 	>				;
            00            52     8F  00CC       	CASEB	R2,#0,#<$$COUNT-1>
                          07         00CF       
                                     00D0       30001$:
                               001E' 00D0       	.WORD	FILE_ACCESS-30001$
                               001E' 00D2       	.WORD	FILE_ACCESS-30001$
                               0017' 00D4       	.WORD	UNSUPPORT-30001$
                               0010' 00D6       	.WORD	PROTOCOL-30001$
                               001E' 00D8       	.WORD	FILE_ACCESS-30001$
                               001E' 00DA       	.WORD	FILE_ACCESS-30001$
                               0010' 00DC       	.WORD	PROTOCOL-30001$
                               001E' 00DE       	.WORD	FILE_ACCESS-30001$
