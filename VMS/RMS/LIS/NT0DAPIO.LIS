                                     0000    63 	.SBTTL	NT$TRANSMIT - TRANSMIT DAP MESSAGE
                                     0000    64 
                                     0000    65 ;++
                                     0000    66 ; FUNCTIONAL DESCRIPTION:
                                     0000    67 ;
                                     0000    68 ;	NT$TRANSMIT TRANSMITS THE SPECIFIED DAP MESSAGE TO THE REMOTE FAL.
                                     0000    69 ;
                                     0000    70 ; CALLING SEQUENCE:
                                     0000    71 ;
                                     0000    72 ;	BSB/JSB	NT$TRANSMIT	(IFAB ADDRESS IN R10)
                                     0000    73 ;	BSB/JSB	NT$TRANSMIT_ALT	(IFAB ADDRESS IN R9)
                                     0000    74 ;
                                     0000    75 ; INPUT PARAMETERS:
                                     0000    76 ;
                                     0000    77 ;	R11	IMPURE AREA ADDRESS
                                     0000    78 ;	R10	IFAB ADDRESS
                                     0000    79 ;	R9	IFAB/IRAB ADDRESS
                                     0000    80 ;	R8	FAB/RAB ADDRESS
                                     0000    81 ;	R7	NWA (=DAP) ADDRESS
                                     0000    82 ;	R5	BUFFER SIZE
                                     0000    83 ;	R4	BUFFER ADDRESS
                                     0000    84 ;
                                     0000    85 ; IMPLICIT INPUTS:
                                     0000    86 ;
                                     0000    87 ;	NONE
                                     0000    88 ;
                                     0000    89 ; OUTPUT PARAMETERS:
                                     0000    90 ;
                                     0000    91 ;	R0	STATUS CODE (RMS)
                                     0000    92 ;	R1-R3	DESTROYED
                                     0000    93 ;	AP	DESTROYED
                                     0000    94 ;
                                     0000    95 ; IMPLICIT OUTPUTS:
                                     0000    96 ;
                                     0000    97 ;	NONE
                                     0000    98 ;
                                     0000    99 ; COMPLETION CODES:
                                     0000   100 ;
                                     0000   101 ;	STANDARD RMS COMLETION CODES.
                                     0000   102 ;
                                     0000   103 ; SIDE EFFECTS:
                                     0000   104 ;
                                     0000   105 ;	NONE
                                     0000   106 ;
                                     0000   107 ;--
                                     0000   108 
