                                     0018   145 ;++
                                     0018   146 ;
                                     0018   147 ;  ENTRY POINT FOR $DISPLAY SERVICE
                                     0018   148 ;
                                     0018   149 ;--
                                     0018   150 	$ENTRY	RMS$DISPLAY
                                 00000018       	.PSECT	F$RMSDISPLAY0,GBL,NOWRT
                                     0018   151 	$TSTPT	DISPLAY
                 00000200'9F     D6  0018       		INCL	@#PIO$GL_FMLH+512+TPT$L_DISPLAY
                          FFDF'  30  001E   152 	BSBW	RM$FSET				; DO COMMON SETUP
                                     0021   153 						; NOTE: DOES NOT RETURN ON ERROR
                          FFDC'  30  0021   154 	BSBW	RM$CHK_IDLE			; CHECK ALL STREAMS INACTIVE
                                     0024   155 						; NOTE: DOES NOT RETURN ON ERROR
            6A            0D     E0  0024   156 	BBS	#DEV$V_NET,IFB$L_PRIM_DEV(R10),RM$ERRSUP
                          57         0027       
                                     0028   157 						; BRANCH IF NETWROK FUNCTION
                                     0028   158 	RMSSUC					; ANTICIPATE SUCCESS
            50            01     D0  0028       		MOVL	#1,R0
            6A            03     E1  002B   159 	BBC	#DEV$V_DIR,IFB$L_PRIM_DEV(R10),DSPXIT
                          4D         002E       
                                     002F   160 						; BRANCH IF NOT FILE STRUCTURED
                                     002F   161 ;
                                     002F   162 ;  ALLOCATE ATTRIBUTE LIST WORK AREA
                                     002F   163 ;
            52         60 8F     9A  002F   164 	MOVZBL	#C_MAXATTR*8,R2			; SIZE FOR ATTR. LIST WORK AREA
                          FFCA'  30  0033   165 	BSBW	RM$GETSPC1			; ALLOCATE ATTR. LIST WORK AREA
            43            50     E9  0036   166 	BLBC	R0,DSPXIT			; GET OUT ON ERRORS
            55            51     D0  0039   167 	MOVL	R1,R5				; SAVE ATTR. LIST AREA ADDR
                          55     DD  003C   168 	PUSHL	R5				;      "
                                     003E   169 ;
                                     003E   170 ;  !!!  \NOTE: MUST ADD CODE TO HANDLE INDEXED FILE ORG.\ !!!
                                     003E   171 ;
                                     003E   172 ;  PROCESS ALLOCATION XAB, IF ANY.
                                     003E   173 ;
            5C         BF AF     9E  003E   174 	MOVAB	ALL_XAB_ARGS,AP			; XAB SCAN ARG. LIST ADDR
                          FFBB'  30  0042   175 	BSBW	RM$XAB_SCAN			; PROCESS ANY ALLOCATION XAB
            27            50     E9  0045   176 	BLBC	R0,DSPCLN			; GET OUT ON ERROR
                                     0048   177 ;++!!! TEMPORARY CODE !!!
                          54     D5  0048   178 	TSTL	R4				; ANY XAB PROCESSED?
                          08     13  004A   179 	BEQL	9$				; BRANCH IF NONE
                          34     10  004C   180 	BSBB	RM$READ_ATTR			; GO READ ATTRIBUTES
            1E            50     E9  004E   181 	BLBC	R0,DSPCLN			; GET OUT ON ERROR
            55            6E     D0  0051   182 	MOVL	(SP),R5				; RESTORE ATTR LIST WORK AREA ADDR
                                     0054   183 9$:
                                     0054   184 ;--!!! END OF TEMPORARY CODE !!!
                                     0054   185 ;
                                     0054   186 ;  PROCESS OTHER XABS, IF ANY
                                     0054   187 ;
            5C         B0 AF     9E  0054   188 	MOVAB	DSP_XAB_ARGS,AP			; XAB SCAN ARG. LIST ADDR
                          FFA5'  30  0058   189 	BSBW	RM$XAB_SCAN			; PROCESS THE XABS
            11            50     E9  005B   190 	BLBC	R0,DSPCLN			; GET OUT ON ERROR
            6E            55     D1  005E   191 	CMPL	R5,(SP)				; ANY NEED TO READ ATTRIBUTES?
                          0C     13  0061   192 	BEQL	DSPCLN				; BRANCH IF NOT
                          1D     10  0063   193 	BSBB	RM$READ_ATTR			; READ FILE ATTRIBUTES
            07            50     E9  0065   194 	BLBC	R0,DSPCLN			; GET OUT ON ERROR
            5C         A6 AF     9E  0068   195 	MOVAB	DSP_XAB_ARGS1,AP		; FHC & ALLOCATION XAB SCAN ARGS
                          FF91'  30  006C   196 	BSBW	RM$XAB_SCAN			; PROCESS 2ND HALF OF ALL OR FHC XAB
