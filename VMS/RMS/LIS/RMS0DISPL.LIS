003             RM$READ_ATTR  -  SUBROUTINE TO READ FILE ATTRIB                                                                  (1)

                                     0082   219 	.SBTTL	RM$READ_ATTR  -  SUBROUTINE TO READ FILE ATTRIBUTES
                                     0082   220 ;++
                                     0082   221 ;  FUNCTIONAL DESCRIPTION:
                                     0082   222 ;
                                     0082   223 ;	THIS ROUTINE PERFORMS AN IO$_ACCESS QIO TO READ THE FILE ATTRIBUTES
                                     0082   224 ;
                                     0082   225 ;  INPUTS:
                                     0082   226 ;
                                     0082   227 ;	R11	IMPURE AREA ADDR
                                     0082   228 ;	R10	IFAB ADDR
                                     0082   229 ;	R9	IFAB ADDR
                                     0082   230 ;	R8	FAB ADDR
                                     0082   231 ;	R5	ATTRIBUTE LIST END ADDR (A ZERO LONGWORD WILL BE STORE HERE)
                                     0082   232 ;	4(SP)	ATTRIBUTE LIST START ADDR
                                     0082   233 ;
                                     0082   234 ;  OUTPUTS:
                                     0082   235 ;	
                                     0082   236 ;	R0		STATUS
                                     0082   237 ;	R1-R5,AP	DESTROYED
                                     0082   238 ;
                                     0082   239 ;--
                                     0082   240 RM$READ_ATTR::
                          65     D4  0082   241 	CLRL	(R5)				; FLAG END OF ATTR. LIST
                                     0084   242 ;
                                     0084   243 ;  ALLOCATE AND INITIALIZE FIB
                                     0084   244 ;
            52            0A     D0  0084   245 	MOVL	#FIB$C_ACCDATA,R2		; MINIMUM SIZE FIB TO ALLOCATE
                          FF76'  30  0087   246 	BSBW	RM$GETSPC1			; ALLOCATE FIB
            1C            50     E9  008A   247 	BLBC	R0,10$				; GET OUT ON ERROR
                          51     DD  008D   248 	PUSHL	R1				; BUILD FIB DESCRIPTOR (ADDR)
                          0A     DD  008F   249 	PUSHL	#FIB$C_ACCDATA			;	"		(LEN)
                                     0091   250 	SSB	#FIB$V_PRSRV_ATR,FIB$L_ACCTL(R1); SPECIFY REAL ATTRIBUTES (BLK)
            61            11     E2  0091       		BBSS	#FIB$V_PRSRV_ATR,FIB$L_ACCTL(R1),30000$
                          00         0094       
                                     0095       30000$:
                                     0095   251 ;
                                     0095   252 ;  PUSH P6 AND P5 QIO PARAMETERS ON THE STACK AND DO THE ACCESS QIO FUNCTION
                                     0095   253 ;
                          00     DD  0095   254 	PUSHL	#0				; P6 = 0
                       10 AE     DD  0097   255 	PUSHL	16(SP)				; P5 = ATTR. LIST ADDR
            50            32     9A  009A   256 	MOVZBL	#IO$_ACCESS,R0			; I/O FUNCTION CODE
                          FF60'  30  009D   257 	BSBW	RM$FCPFNC_ALT5			; READ ATTRIBUTES
                          14     BA  00A0   258 	POPR	#^M<R2,R4>			; GET FIB SIZE AND ADDRESS
                          50     DD  00A2   259 	PUSHL	R0				; SAVE STATUS CODE
                          FF59'  30  00A4   260 	BSBW	RM$RETSPC1			; DEALLOCATE FIB
                          01     BA  00A7   261 	POPR	#^M<R0>				; RESTORE STATUS
                                 05  00A9   262 10$:	RSB
                                     00AA   263 
                                     00AA   264 
                                     00AA   265 	.END
