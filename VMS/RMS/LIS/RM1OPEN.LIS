                                     0000    68 	.SBTTL RM$OPEN1  -  SEQUENTIAL OPEN ROUTINE
                                     0000    69 
                                     0000    70 ;++
                                     0000    71 ; FUNCTIONAL DESCRIPTION:
                                     0000    72 ;
                                     0000    73 ;  THIS ROUTINE PERFORMS ALL OF THE FILE OPEN FUNCTIONS
                                     0000    74 ;  THAT ARE SPECIFIC TO THE SEQUENTIAL FILE ORGANIZATION,
                                     0000    75 ;  INCLUDING:
                                     0000    76 ;
                                     0000    77 ;	1. CHECKING THAT WRITE-SHARING HAS NOT BEEN SPECIFIED
                                     0000    78 ;	2. FORCING VAR LENGTH AND RAT=FAB$B_RAT FOR UNIT RECORD DEVICES
                                     0000    79 ;	3. SETTING THE BLS FIELD OF THE FAB TO THE DEVICE BUFFER SIZE
                                     0000    80 ;	4. PERFORMING MAGTAPE FILE POSITIONING
                                     0000    81 ;	5. SETTING THE TWO CLOSE OPTION BITS AS REQUIRED
                                     0000    82 ;	6. RETURNING TO THE COMMON OPEN ROUTINE
                                     0000    83 ;
                                     0000    84 ; CALLING SEQUENCE:
                                     0000    85 ;
                                     0000    86 ;  ENTERED VIA CASE BRANCH FROM RMS$OPEN
                                     0000    87 ;  RETURNS BY JUMPING TO RM$COPRTN
                                     0000    88 ;
                                     0000    89 ; INPUT PARAMETERS:
                                     0000    90 ;
                                     0000    91 ;	R11	IMPURE AREA ADDRESS
                                     0000    92 ;	R9	IFAB ADDRESS
                                     0000    93 ;	R8	FAB ADDRESS
                                     0000    94 ;
                                     0000    95 ; IMPLICIT INPUTS:
                                     0000    96 ;
                                     0000    97 ;	THE CONTENTS OF THE FAB
                                     0000    98 ;
                                     0000    99 ; OUTPUT PARAMETERS:
                                     0000   100 ;
                                     0000   101 ;	R0	STATUS CODE
                                     0000   102 ;	R1 THRU R5 DESTROYED
                                     0000   103 ;
                                     0000   104 ; IMPLICIT OUTPUTS:
                                     0000   105 ;
                                     0000   106 ;	VARIOUS FIELDS IN THE IFAB AND FAB ARE INITIALIZED
                                     0000   107 ;
                                     0000   108 ; COMPLETION CODES:
                                     0000   109 ;
                                     0000   110 ;	STANDARD RMS IN PARTICULAR, SUCCESS AND SHR.
                                     0000   111 ;
                                     0000   112 ; SIDE EFFECTS:
                                     0000   113 ;
                                     0000   114 ;	NONE
                                     0000   115 ;
                                     0000   116 ;--
RM1OPEN         SEQUENTIAL-SPECIFIC OPEN                         21-AUG-1978 21:43:42   VAX-11 MACRO X0.3-11               Page   4
012             RM$OPEN1  -  SEQUENTIAL OPEN ROUTINE                                                                             (1)

                                     0000   118 RM$OPEN1::
                                     0000   119 	$TSTPT	OPEN1
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_OPEN1
                                     0006   120 ;
                                     0006   121 ;  CHECK THAT RECORD LOCKING NOT REQUIRED
                                     0006   122 ;
            69            33     E0  0006   123 	BBS	#IFB$V_NORECLK,(R9),2$		;BRANCH IF NO LOCKING REQUIRED
                          05         0009       
         17 A8            06     E1  000A   124 	BBC	#FAB$V_UPI,FAB$B_SHR(R8),ERRSHR	;ERROR UNLESS USER PROVIDING
                          69         000E       
                                     000F   125 						; OWN INTERLOCKING
                                     000F   126 2$:
                                     000F   127 ;
                                     000F   128 ;  IF A UNIT RECORD DEVICE, FORCE VARIABLE LENGTH RECORD FORMAT
                                     000F   129 ;  AND RECORD ATTRIBUTES FROM FAB
                                     000F   130 ;
                                     000F   131 	ASSUME	FAB$C_SEQ EQ 0
                                     000F   132 	ASSUME	DEV$V_REC EQ 0
            2C            69     E9  000F   133 	BLBC	IFB$L_PRIM_DEV(R9),10$		;BRANCH IF NOT UNIT REC.
         3C A9            02     90  0012   134 	MOVB	#FAB$C_VAR,IFB$B_RFMORG(R9)	;FORCE TO VARIABLE
         3D A9         1E A8     90  0016   135 	MOVB	FAB$B_RAT(R8),IFB$B_RAT(R9)	;AND SET RAT FROM FAB
         1E A8         F8 8F     8B  001B   136 	BICB3	#255 \ <FAB$M_CR!FAB$M_FTN!FAB$M_PRN>,FAB$B_RAT(R8),R1
                          51         0020       
                                     0021   137 						;CLEAR ALL BIT CARRIAGE CONTROL
                          1B     13  0021   138 	BEQL	10$				;BRANCH IF NONE SPECED
            51            02     E1  0023   139 	BBC	#FAB$V_PRN,R1,3$		;BRANCH IF PRN NOT SPECED
                          0A         0026       
            03         1F A8     91  0027   140 	CMPB	FAB$B_RFM(R8),#FAB$C_VFC	;RFM = VFC?
                          4E     12  002B   141 	BNEQ	ERRRAT				;BRANCH IF NOT
         3C A9            03     90  002D   142 	MOVB	#FAB$C_VFC,IFB$B_RFMORG(R9)	;CHANGE RFM TO VFC
                                     0031   143 	ASSUME 	FAB$V_FTN EQ FAB$V_CR-1
                                     0031   144 	ASSUME	FAB$V_PRN EQ FAB$V_CR+1
            03            00     EA  0031   145 3$:	FFS	#FAB$V_FTN,#3,R1,R0		;FIND THE FIRST BIT SET
            50            51         0034       
                                     0036   146 	CSB	R0,R1				;TURN IT OFF
            51            50     E5  0036       		BBCC	R0,R1,30000$
                          00         0039       
                                     003A       30000$:
                          51     95  003A   147 	TSTB	R1				;ANOTHER BIT SET
                          3D     12  003C   148 	BNEQ	ERRRAT				;BRANCH IF SO
                                     003E   149 ;
                                     003E   150 ;  SET BLS EQUAL TO DEVICE BUFFER SIZE
                                     003E   151 ;
         3C A8         5A A9     B0  003E   152 10$:	MOVW	IFB$W_ASDEVBSIZ(R9),FAB$W_BLS(R8)
            69            06     E0  0043   153 	BBS	#DEV$V_SPL,IFB$L_PRIM_DEV(R9),15$	; BRANCH IF DEVICE SPOOLED
                          05         0046       
         3C A8         38 A9     B0  0047   154 	MOVW	IFB$W_DEVBUFSIZ(R9),FAB$W_BLS(R8)	; ELSE RETURN REAL BLOCK SIZE
                                     004C   155 15$:
                                     004C   156 ;
                                     004C   157 ;  IF DEVICE IS MAGTAPE, THEN ASSUME EOF AT MAX VBN, BYTE 0
                                     004C   158 ;  UNLESS FILE IS WRITE ACCESSED, IN WHICH CASE 
                                     004C   159 ;	LOOK AT NEF AND IF CLEAR THEN SET "AT EOF" FLAG
                                     004C   160 ;		AND DECLARE EOF TO BE AT VBN 1, BYTE 0
                                     004C   161 ;	ELSE UTILIZE SAME POSITIONING AS FOR READ ACCESS.
                                     004C   162 ;
            69            05     E1  004C   163 	BBC	#DEV$V_SQD,IFB$L_PRIM_DEV(R9),50$	;BRANCH IF NOT MAGTAPE
                          1B         004F       
RM1OPEN         SEQUENTIAL-SPECIFIC OPEN                         21-AUG-1978 21:43:42   VAX-11 MACRO X0.3-11               Page   5
012             RM$OPEN1  -  SEQUENTIAL OPEN ROUTINE                                                                             (1)

         44 A9            01     CE  0050   164 	MNEGL	#1,IFB$L_EBK(R9)		;ASSUME NOT WRITE ACCESSED
            69            30     E1  0054   165 	BBC	#IFB$V_WRTACC,(R9),20$		;BRANCH IF TRUE
                          0C         0057       
            68            2A     E0  0058   166 	BBS	#FAB$V_NEF+FOP,(R8),20$		;OR IF NEF SET
                          08         005B       
         44 A9            01     D0  005C   167 	MOVL	#1,IFB$L_EBK(R9)		;SET EOF TO VBN 1
                                     0060   168 	SSB	#IFB$V_EOF,(R9)			;AND SET AT EOF FLG
            69            21     E2  0060       		BBSS	#IFB$V_EOF,(R9),30001$
                          00         0063       
                                     0064       30001$:
                       48 A9     B4  0064   169 20$:	CLRW	IFB$W_FFB(R9)			;ALWAYS USE BYTE 0 FOR MT
                                     0067   170 	SSB	#IFB$V_ANSI_D,(R9)		;SET ANSI FLAG
            69            26     E2  0067       		BBSS	#IFB$V_ANSI_D,(R9),30002$
                          00         006A       
                                     006B       30002$:
                                     006B   171 ;
                                     006B   172 ;  RETURN TO COMMON OPEN SEQUENCE
                                     006B   173 ;
            69            3E     E0  006B   174 50$:	BBS	#IFB$V_DAP,(R9),NTOPEN		; BRANCH IF NETWORK FILE OPEN
                          06         006E       
                                     006F   175 						; NOTE: NOT TASK-TO-TASK OPEN
                                     006F   176 	RMSSUC					; SHOW SUCCESS
            50            01     D0  006F       		MOVL	#1,R0
                          FF8B'  31  0072   177 OPNRET:	BRW	RM$COPRTN
                                     0075   178 
                                     0075   179 ;++
                                     0075   180 ;
                                     0075   181 ;  PERFFORM NETWORK OPEN FUNCTION
                                     0075   182 ;
                                     0075   183 ;--
                                     0075   184 NTOPEN:	$PSECT_BR	NF$NETWORK
                          FF88'  31  0075       		BRW	NF$NETWORK0
                                     0078       		.SAVE
                                 00000000       		.PSECT	NF$NETWORK,GBL,NOWRT
                                     0000       NF$NETWORK0:
                                     0000   185 ;
                                     0000   186 ;  COPY RECORD FORMAT FROM FAB TO IFAB. THIS IS TO COUNTER-ACT THE
                                     0000   187 ;  ABOVE FORCE OF VARIABLE LENGTH IF DEVICE IS RECORD ORIENTED.
                                     0000   188 ;
         3C A9         1F A8     90  0000   189 	MOVB	FAB$B_RFM(R8),IFB$B_RFMORG(R9)
                          FFF8'  30  0005   190 	BSBW	NT$OPEN1		; OPEN FILE ON REMOTE NODE
                          FFF5'  31  0008   191 	BRW	RM$COPRTN		; JOIN COMMON CODE
                                     000B   192 	$PSECT_RESTORE
                                 00000078       		.RESTORE
                                     0078   193 
                                     0078   194 ;
                                     0078   195 ;  WRITE SHARING SPECIFIED ON SEQUENTIAL FILE
                                     0078   196 ;  -  RETURN WITH ERROR CODE
                                     0078   197 ;
                                     0078   198 ERRSHR:	$PSECT_BR	L$RMS1
                          FF85'  31  0078       		BRW	L$RMS11
                                     007B       		.SAVE
                                 00000000       		.PSECT	L$RMS1,GBL,NOWRT
                                     0000       L$RMS11:
                                     0000   199 	RMSERR	SHR
            50       86B4 8F     3C  0000       		MOVZWL	#<RMS$_SHR&^XFFFF>,R0
                          05     11  0005   200 	BRB	OPNRET1
