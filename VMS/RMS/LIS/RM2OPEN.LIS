                                     0000    65 	.SBTTL RM$OPEN2  -  PROCESS RELATIVE FILE PROLOG
                                     0000    66 ;++
                                     0000    67 ; FUNCTIONAL DESCRIPTION:
                                     0000    68 ;
                                     0000    69 ;	THIS ROUTINE PERFORMS THE FILE OPEN FUNCTIONS THAT ARE
                                     0000    70 ;	SPECIFIC TO THE RELATIVE FILE ORGANIZATION, INCLUDING:
                                     0000    71 ;
                                     0000    72 ;	1  -  VERIFY INTER-PROCESS RECORD LOCKING NOT SPECIFIED
                                     0000    73 ;	      SINCE NOT YET IMPLEMENTED
                                     0000    74 ;	2  -  READING IN THE PROLOG AND SETTING THE EBK,DVBN,
                                     0000    75 ;	      AND MRN IFAB FIELDS BASED UPON ITS CONTENTS.
                                     0000    76 ;	3  -  SETTING THE MRN FAB FIELD.
                                     0000    77 ;
                                     0000    78 ; CALLING SEQUENCE:
                                     0000    79 ;
                                     0000    80 ;	ENTERED VIA CASE BRANCH FROM RMS$OPEN. RETURNS BY 
                                     0000    81 ;	JUMPING TO RM$COPRTN.
                                     0000    82 ;
                                     0000    83 ; INPUT PARAMETERS:
                                     0000    84 ;
                                     0000    85 ;	R11	IMPURE AREA ADDRESS
                                     0000    86 ;	R9	IFAB ADDRESS
                                     0000    87 ;	R8	FAB ADDRESS
                                     0000    88 ;
                                     0000    89 ; IMPLICIT INPUTS:
                                     0000    90 ;
                                     0000    91 ;	THE CONTENTS OF THE IFAB
                                     0000    92 ;
                                     0000    93 ; OUTPUT PARAMETERS:
                                     0000    94 ;
                                     0000    95 ;	R0		STATUS CODE
                                     0000    96 ;	R10		IFAB ADDR
                                     0000    97 ;	R1-R5,AP	DESTROYED
                                     0000    98 ;
                                     0000    99 ; IMPLICIT OUTPUTS:
                                     0000   100 ;
                                     0000   101 ;	VARIOUS FIELDS IN THE IFAB AND FAB ARE INITIALIZED.
                                     0000   102 ;
                                     0000   103 ; COMPLETION CODES:
                                     0000   104 ;
                                     0000   105 ;	STANDARD RMS, IN PARTICULAR SUC,PLG,SHR,RPL, AND VER.
                                     0000   106 ;
                                     0000   107 ; SIDE EFFECTS:
                                     0000   108 ;
                                     0000   109 ;	MAY WAIT QUITE SOME TIME FOR PROLOG TO BECOME
                                     0000   110 ;	FREE INITIALLY. LEAVES PROLOG LOCKED.
                                     0000   111 ;
                                     0000   112 ;--
