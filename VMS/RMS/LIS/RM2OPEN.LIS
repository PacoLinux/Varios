                                     0000   114 RM$OPEN2::
            69            3E     E0  0000   115 	BBS	#IFB$V_DAP,(R9),NTOPEN		; BRANCH IF NETWORK FILE OPEN
                          52         0003       
                                     0004   116 ;
                                     0004   117 ; CHECK RECORD LOCKING NOT REQUIRED
                                     0004   118 ;
            69            33     E0  0004   119 	BBS	#IFB$V_NORECLK,(R9),10$		; BRANCH IF NO RECORD LOCKING
                          06         0007       
                                 93  0008   120 	BITB	#<FAB$M_GET!FAB$M_PUT!FAB$M_UPD!FAB$M_DEL>,-
         17 A8            0F         0009   121 		FAB$B_SHR(R8)			; ANY SHARING?
                          4B     12  000C   122 	BNEQ	ERRSHR				; BRANCH IF YES
                                     000E   123 						; CAN'T DO CROSS PROCESS LOCKING
                                     000E   124 10$:
                                     000E   125 ;
                                     000E   126 ; READ AND PROCESS PROLOG
                                     000E   127 ;
            5A            59     D0  000E   128 	MOVL	R9,R10				; SET IFAB ADDR
                                     0011   129 	$CACHE	VBN=#1,SIZE=#512,FLAGS=LOCK,ERR=ERRRPL
                                     0022   130 						; READ THE PROLOG
                                     0022   131 						; (R5=BUFFER ADDR)
                          FFDB'  30  0022   132 	BSBW	RM$CHKSUM			; VALIDATE ITS CHECKSUM
            2B            50     E9  0025   133 	BLBC	R0,EXIT				; GET OUT ON ERROR
            01         74 A5     B1  0028   134 	CMPW	PLG$W_VER_NO(R5),#PLG$C_VER_NO	; SUPPORTED VERSION?
                          31     12  002C   135 	BNEQ	ERRPLV				; BRANCH IF NOT
                                     002E   136 ;
                                     002E   137 ; SET UP IFAB VALUES
                                     002E   138 ;
         44 A9         70 A5     D0  002E   139 	MOVL	PLG$L_EOF(R5),IFB$L_EBK(R9)	; COPY EOF VBN
         58 A9         68 A5     3C  0033   140 	MOVZWL	PLG$W_DVBN(R5),IFB$L_DVBN(R9)	; COPY VBN OF FIRST DATA BUCKET
         54 A9         6C A5     D0  0038   141 	MOVL	PLG$L_MRN(R5),IFB$L_MRN(R9)	; COPY MAX. RECORD NUMBER
                       48 A9     B4  003D   142 	CLRW	IFB$W_FFB(R9)			; SET BLK OFFSET=0
                                     0040   143 ;
                                     0040   144 ; SET MRN IN FAB AND BKS SIZE IN BYTES IN IFAB
                                     0040   145 ;
         38 A8         54 A9     D0  0040   146 SET:	MOVL	IFB$L_MRN(R9),FAB$L_MRN(R8)	; SET MRN
            50         4A A9     9A  0045   147 	MOVZBL	IFB$B_BKS(R9),R0		; GET BUCKET SIZE
                          17     13  0049   148 	BEQL	ERRIFA				; BRANCH IF 0
                                     004B   149 	ASSUME	IFB$W_BKS_RECS EQ IFB$W_BKS_BYTES+2
            50            09     9C  004B   150 	ROTL	#9,R0,IFB$W_BKS_BYTES(R10)	; COMPUTE BUCKET SIZE IN BYTES
                       5C AA         004E       
                                     0050   151 						; (ALSO ZEROES IFB$W_BKS_RECS)
                                     0050   152 	RMSSUC					; SHOW SUCCESS
                          FFAA'  31  0053   153 EXIT:	BRW	RM$COPRTN			; & REJOIN COMMON OPEN CODE
                                     0056   154 						; NOTE: THE BDB WILL
                                     0056   155 						; BE RELEASED THERE
                                     0056   156 ;++
                                     0056   157 ;
                                     0056   158 ;  PERFORM NETWORK OPEN FUNCTION
                                     0056   159 ;
                                     0056   160 ;--
                                     0056   161 NTOPEN:	$PSECT_BR	NF$NETWORK
                          FFFD'  30  0000   162 	BSBW	NT$OPEN2			; OPEN FILE ON REMOTE NODE
            03            50     E9  0003   163 	BLBC	R0,10$				; BRANCH OF ERROR
                          0037'  31  0006   164 	BRW	SET				; JOIN COMMON CODE
                          FFF4'  31  0009   165 10$:	BRW	RM$COPRTN			; JOIN COMMON CODE
                                     000C   166 	$PSECT_RESTORE
