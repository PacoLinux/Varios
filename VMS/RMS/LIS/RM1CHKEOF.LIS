                                     0000    58 	.SBTTL RM$CHKEOF1
                                     0000    59 ;++
                                     0000    60 ; FUNCTIONAL DESCRIPTION:
                                     0000    61 ;
                                     0000    62 ; THIS MODULE CHECKS FOR THE CURRENT RECORD POSITION AT OR
                                     0000    63 ; BEYOND THE LOGICAL END OF FILE, SETTING
                                     0000    64 ; THE IRB$V_EOF BIT IF SO.
                                     0000    65 ;
                                     0000    66 ; CALLING SEQUENCE:
                                     0000    67 ;
                                     0000    68 ;	BSBW	RM$CHKEOF1
                                     0000    69 ;
                                     0000    70 ; INPUT PARAMETERS:
                                     0000    71 ;
                                     0000    72 ;	R10	IFAB ADDR
                                     0000    73 ;	R9	IRAB ADDR
                                     0000    74 ;
                                     0000    75 ; IMPLICIT INPUTS:
                                     0000    76 ;
                                     0000    77 ;	IRB$V_EOF
                                     0000    78 ;
                                     0000    79 ; OUTPUT PARAMETERS:
                                     0000    80 ;
                                     0000    81 ;	IRB$V_EOF SET TO 1 IF AT OR BEYOND EOF, ELSE LEFT CLEAR.
                                     0000    82 ;
                                     0000    83 ; IMPLICIT OUTPUTS:
                                     0000    84 ;
                                     0000    85 ;	NONE
                                     0000    86 ;
                                     0000    87 ; COMPLETION CODES:
                                     0000    88 ;
                                     0000    89 ;	NONE
                                     0000    90 ;
                                     0000    91 ; SIDE EFFECTS:
                                     0000    92 ;
                                     0000    93 ;	NONE
                                     0000    94 ;
                                     0000    95 ;--
                                     0000    96 ;
                                     0000    97 RM$CHKEOF1::
            69            21     E0  0000    98 	BBS	#IRB$V_EOF,(R9),EXIT		; BRANCH IF ALREADY EOF
                          14         0003       
         44 AA         2C A9     D1  0004    99 	CMPL	IRB$L_NRP_VBN(R9),IFB$L_EBK(R10)	; AT EOF?
                          0D     1F  0009   100 	BLSSU	EXIT				; BRANCH IF NOT YET
                          07     1A  000B   101 	BGTRU	10$				; BRANCH IF DEFINITELY
                                     000D   102 ;
                                     000D   103 ; IN CURRENT EOF BLOCK
                                     000D   104 ; COMPARE THE BYTE OFFSETS
                                     000D   105 ;
         48 AA         30 A9     B1  000D   106 	CMPW	IRB$W_NRP_OFF(R9),IFB$W_FFB(R10)	; HOW ABOUT BYTE POSITION?
                          04     1F  0012   107 	BLSSU	EXIT				; NOPE - NOT THERE YET
                                     0014   108 10$:	SSB	#IRB$V_EOF,(R9)			; EOF - SET THE FLAG
                                 05  0018   109 EXIT:	RSB
                                     0019   110 	.END
