                                     0000    66 	.SBTTL	$TRUNCATE ROUTINE
                                     0000    67 ;++
                                     0000    68 ; FUNCTIONAL DESCRIPTION:
                                     0000    69 ;
                                     0000    70 ;	THIS ROUTINE TRUNCATES A MAGTAPE OR DISK FILE OF SEQUENTIAL ORGANIZATION,
                                     0000    71 ;	DECLARING EOF AT THE CURRENT RECORD POINTER.  TRUNCATE MUST IMMEDIATELY
                                     0000    72 ;	FOLLOW A SUCCESSFUL $GET, $FIND, OR $UPDATE.
                                     0000    73 ;	THE FILE MUST NOT BE ACCESSED FOR BLOCK I/O.
                                     0000    74 ;
                                     0000    75 ; CALLING SEQUENCE:
                                     0000    76 ;
                                     0000    77 ;	ENTERED AS A RESULT OF USER'S CALLING SYS$TRUNCATE
                                     0000    78 ;
                                     0000    79 ; INPUT PARAMETERS:
                                     0000    80 ;
                                     0000    81 ;	AP	USER'S ARGUMENT LIST
                                     0000    82 ;
                                     0000    83 ; IMPLICIT INPUTS:
                                     0000    84 ;
                                     0000    85 ;	THE CONTENTS OF THE RAB
                                     0000    86 ;
                                     0000    87 ; OUTPUT PARAMETERS:
                                     0000    88 ;
                                     0000    89 ;	R0	STATUS CODE
                                     0000    90 ;	R1	DESTROYED
                                     0000    91 ;
                                     0000    92 ; IMPLICIT OUTPUTS:
                                     0000    93 ;
                                     0000    94 ;	THE STS FIELD OF THE RAB
                                     0000    95 ;
                                     0000    96 ; COMPLETION CODES:
                                     0000    97 ;
                                     0000    98 ;	STANDARD RMS
                                     0000    99 ;
                                     0000   100 ; SIDE EFFECTS:
                                     0000   101 ;
                                     0000   102 ;	THERE IS NO CURRENT RECORD FOLLOWING A TRUNCATE
                                     0000   103 ;
                                     0000   104 ;--
RMS0TRUNC       TRUNCATE SEQUENTIAL FILE                         21-AUG-1978 22:13:05   VAX-11 MACRO X0.3-11               Page   4
002             $TRUNCATE ROUTINE                                                                                                (1)

                                     0000   106 ;++
                                     0000   107 ;
                                     0000   108 ; ENTRY POINT FOR $TRUNCATE
                                     0000   109 ;
                                     0000   110 ; VERIFY OPERATION ALLOWED (ACCESSED FOR RECORD I/O TO A MAGTAPE OR DISK,
                                     0000   111 ;	HAVING A CURRENT RECORD)
                                     0000   112 ; AND SET EOF TO THE RECORD POINTER, BACKSPACING 1 BLOCK IF MAGTAPE
                                     0000   113 ;
                                     0000   114 ;--
                                     0000   115 	$ENTRY	RMS$TRUNCATE
                                 00000000       	.PSECT	L$RMS1TRUNC,GBL,NOWRT
                                     0000   116 	$TSTPT	TRUNCATE
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_TRUNCATE
                                     0006   117 	$RABSET	FAC=IFB$V_TRN,BIO=0,CFLG=1	; SET UP STREAM
                          FFF7'  30  0006       		BSBW	RM$RSET
                                 14  0009       		.BYTE	<0 @ 5>!<1 @ 4>!<0 @ 3>!IFB$V_TRN
                                     000A   118 						; (MUST BE TRUNCATE ACCESSED,
                                     000A   119 						; NOT DOING BLOCK I/O,
                                     000A   120 						; CLEAR FIND LAST)
                                     000A   121 	ASSUME	IFB$C_SEQ EQ 0
                       23 AA     95  000A   122 	TSTB	IFB$B_ORGCASE(R10)		; SEQ. FILE ORG.?
                          54     12  000D   123 	BNEQ	ERRIOP				; BRANCH IF NOT
                                     000F   124 	ASSUME	DEV$V_REC EQ 0
            51            6A     E8  000F   125 	BLBS	IFB$L_PRIM_DEV(R10),ERRIOP	; BRANCH IF UNIT RECORD DEV.
                       42 A9     B5  0012   126 	TSTW	IRB$W_CSIZ(R9)			; DO WE HAVE A CURRENT RECORD?
                          4F     13  0015   127 	BEQL	ERRCUR				; BRANCH IF NOT
                                     0017   128 ;
                                     0017   129 ;  VALIDITY CHECKS ALL O.K.
                                     0017   130 ;
                                     0017   131 ;  IF THIS IS A MAGTAPE, BACKSPACE TO REWRITE THE BLOCK JUST READ.
                                     0017   132 ;
            6A            05     E1  0017   133 	BBC	#DEV$V_SQD,IFB$L_PRIM_DEV(R10),20$	; BRANCH IF NOT MAGTAPE
                          09         001A       
            56            01     CE  001B   134 	MNEGL	#1,R6					; BACKSPACE 1 BLOCK
                          FFDF'  30  001E   135 	BSBW	RM$SPACE_MT				; GO SPACE IT
            39            50     E9  0021   136 	BLBC	R0,ERRXIT				; GET OUT ON ERROR
                                     0024   137 ;
                                     0024   138 ;  SET NRP AND EOF FROM RP
                                     0024   139 ;
         2C A9         34 A9     7D  0024   140 20$:	MOVQ	IRB$L_RP(R9),IRB$L_NRP(R9)	; SET NRP
                                     0029   141 	ASSUME	IRB$W_RP_OFF EQ IRB$L_RP+4
                                     0029   142 	ASSUME	IFB$W_FFB EQ IFB$L_EBK+4
         34 A9            06     28  0029   143 	MOVC3	#6,IRB$L_RP(R9),IFB$L_EBK(R10)	; AND EOF
                       44 AA         002D       
RMS0TRUNC       TRUNCATE SEQUENTIAL FILE                         21-AUG-1978 22:13:05   VAX-11 MACRO X0.3-11               Page   5
002             $TRUNCATE ROUTINE                                                                                                (1)

                                     002F   145 ;
                                     002F   146 ;  CLEAN UP CURRENT BDB.
                                     002F   147 ;
                                     002F   148 ;  IF THE NEW EOF VBN IS IN THE BUFFER, POINT TO IT AND SAY IT'S DIRTY,
                                     002F   149 ;  THUS CAUSING IT TO GET REWRITTEN WITH APPROPRIATE PADDING,
                                     002F   150 ;  ELSE JUST RELEASE THE BUFFER.
                                     002F   151 ;
            54         20 A9     D0  002F   152 	MOVL	IRB$L_CURBDB(R9),R4		; IS THERE A CURRENT BDB?
                          21     13  0033   153 	BEQL	SUCXIT				; BRANCH IF NOT
         34 A9         18 A4     C3  0035   154 	SUBL3	BDB$L_VBN(R4),IRB$L_RP_VBN(R9),R1 ; GET RELATIVE VBN
                          51         003A       
            51            51     F6  003B   155 	CVTLB	R1,R1				; MAKE BYTE VALUE
                          10     1D  003E   156 	BVS	40$				; BRANCH IF NOT IN RANGE
         20 A4            51     91  0040   157 	CMPB	R1,BDB$B_REL_VBN(R4)		; IS IT IN BUFFER?
                          0A     1A  0044   158 	BGTRU	40$				; BRANCH IF NOT
         20 A4            51     90  0046   159 	MOVB	R1,BDB$B_REL_VBN(R4)		; POINT TO THIS BLOCK
         0A A4            02     88  004A   160 	BISB2	#BDB$M_DRT,BDB$B_FLGS(R4)	; SAY DIRTY
                          06     11  004E   161 	BRB	SUCXIT
                                     0050   162 ;
                                     0050   163 ;  RELEASE UNNEEDED BDB
                                     0050   164 ;
                          FFAD'  30  0050   165 40$:	BSBW	RM$RLNERR
                       20 A9     D4  0053   166 	CLRL	IRB$L_CURBDB(R9)		; SAY NO CURRENT BDB
                                     0056   167 ;
                                     0056   168 ;  SUCCESSFUL EXIT (FALL THRU TO ERROR EXIT)
                                     0056   169 ;
                                     0056   170 SUCXIT:	SSB	#IRB$V_EOF,(R9)			; SAY AT EOF
            69            21     E2  0056       		BBSS	#IRB$V_EOF,(R9),30000$
                          00         0059       
                                     005A       30000$:
                                     005A   171 	RMSSUC
            50            01     D0  005A       		MOVL	#1,R0
                                     005D   172 ;
                                     005D   173 ; ERROR EXIT  -  SAY NO CURRENT RECORD
                                     005D   174 ;
                       42 A9     B4  005D   175 ERRXIT:	CLRW	IRB$W_CSIZ(R9)
                          FF9D'  31  0060   176 	BRW	RM$EXRMS
