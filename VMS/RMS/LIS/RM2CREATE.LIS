                                     0000    70 	.SBTTL RM$CREATE2  -  RELATIVE CREATE ROUTINE
                                     0000    71 ;++
                                     0000    72 ; FUNCTIONAL DESCRIPTION:
                                     0000    73 ;
                                     0000    74 ;	THIS ROUTINE PERFORMS ALL OF THE FILE CREATE
                                     0000    75 ;	FUNCTIONS THAT ARE SPECIFIC TO THE RELATIVE 
                                     0000    76 ;	FILE ORGANIZATION, INCLUDING:
                                     0000    77 ;
                                     0000    78 ;	1.  CHECKING THAT SHARING HAS NOT BEEN SPECIFIED IN SUCH A WAY
                                     0000    79 ;	    THAT INTER-PROCESS RECORD LOCKING IS REQUIRED.
                                     0000    80 ;	2.  CHECKING THAT DEVICE IS A DISK
                                     0000    81 ;	3.  CHECKING THAT RECORD FORMAT IS NOT UNDEFINED
                                     0000    82 ;	4.  CHECKING THAT BUCKET SIZE AND MAXIMUM RECORD SIZE ARE COMPATIBLE
                                     0000    83 ;	5.  VERIFYING MAXIMUM RECORD NUMBER
                                     0000    84 ;	6.  CHECKING XAB CHAIN VALIDITY
                                     0000    85 ;	7.  CALLING THE COMMON CREATE ROUTINE
                                     0000    86 ;	8.  LOCKING THE PROLOG, INITIAL FORMATTING OF THE DATA BUCKETS TO ZEROES
                                     0000    87 ;	9.  INITIALIZING AND UNLOCKING THE PROLOG
                                     0000    88 ;
                                     0000    89 ; CALLING SEQUENCE:
                                     0000    90 ;
                                     0000    91 ;	ENTERED VIA CASE BRANCH FROM RM$OPEN
                                     0000    92 ;	RETURNS BY JUMPING TO RM$CREATEXIT
                                     0000    93 ;
                                     0000    94 ; INPUT PARAMETERS:
                                     0000    95 ;
                                     0000    96 ;	R11	IMPURE AREA ADDRESS
                                     0000    97 ;	R10	FWA ADDRESS
                                     0000    98 ;	R9	IFAB ADDRESS
                                     0000    99 ;	R8	FAB ADDRESS
                                     0000   100 ;
                                     0000   101 ; IMPLICIT INPUTS:
                                     0000   102 ;
                                     0000   103 ;	THE CONTENTS OF THE FAB, IFAB, & FWA.
                                     0000   104 ;
                                     0000   105 ; OUTPUT PARAMETERS:
                                     0000   106 ;
                                     0000   107 ;	R0	STATUS CODE
                                     0000   108 ;	R1-R7	DESTROYED
                                     0000   109 ;
                                     0000   110 ; IMPLICIT OUTPUTS:
                                     0000   111 ;
                                     0000   112 ;	VARIOUS FIELDS IN THE IFAB & FAB ARE INITIALIZED.
                                     0000   113 ;
                                     0000   114 ; COMPLETION CODES:
                                     0000   115 ;
                                     0000   116 ;	STANDARD RMS
                                     0000   117 ;
                                     0000   118 ; SIDE EFFECTS:
                                     0000   119 ;
                                     0000   120 ;	NONE
                                     0000   121 ;
                                     0000   122 ;--
