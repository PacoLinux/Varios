                                     0058   170 ;  SAVE RAT IN IFI FOR LATER INPUT TO CONNECT
                                     0058   171 ;
            16         1E A8     F0  0058   172 	INSV	FAB$B_RAT(R8),#FAB$V_PPF_RAT+<FAB$W_IFI*8>,#FAB$S_PPF_RAT,(R8)
            68            08         005C       
            68            33     E1  005E   173 	BBC	#FAB$V_INP+FOP,(R8),20$	;BRANCH IF NOT 'INPUT'
                          14         0061       
                                     0062   174 	SSB	#IFB$V_PPF_INPUT,(R9)	;SET 'INPUT' FLAG
            69            2E     E2  0062       		BBSS	#IFB$V_PPF_INPUT,(R9),30000$
                          00         0065       
                                     0066       30000$:
                                     0066   175 ;
                                     0066   176 ;  INITIALIZE THE END OF DATA SCAN STRING TO A SINGLE '$'
                                     0066   177 ;
   00000000'9F       2401 8F     B0  0066   178 	MOVW	#1+<^A/$/@8>,@#PIO$GT_ENDSTR
   00000000'9F            02     8A  006F   179 	BICB2	#1@PIO$V_EOD,@#PIO$GW_STATUS	;CLEAR 'LOOKING FOR EOD' FLAG
                          FF87'  31  0076   180 20$:	BRW	RM$FSET_ALT		;FINISH SETUP PROCESSING
