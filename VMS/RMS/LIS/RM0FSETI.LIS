                                     0000    66 	.SBTTL RM$FSETI  -  $OPEN AND $CREATE COMMON SETUP ROUTINE
                                     0000    67 ;++
                                     0000    68 ; FUNCTIONAL DESCRIPTION:
                                     0000    69 ;
                                     0000    70 ;  THIS ROUTINE CALLS RM$FABCHK TO CHECK THE BASIC PARAMETERS
                                     0000    71 ;  FOR A FAB FUNCTION,  ALLOCATES AND INITS AN IFAB, AND
                                     0000    72 ;  THEN DISPATCHES TO RM$FSET (AT ITS ALTERNATE ENTRY POINT)
                                     0000    73 ;  TO FINISH THE SETUP PROCESSING.
                                     0000    74 ;
                                     0000    75 ;
                                     0000    76 ; CALLING SEQUENCE:
                                     0000    77 ;
                                     0000    78 ;	BSBW	RM$FSETI
                                     0000    79 ;
                                     0000    80 ; INPUT PARAMETERS:
                                     0000    81 ;
                                     0000    82 ;	AP	PARAMETER LIST ADDR
                                     0000    83 ;
                                     0000    84 ; IMPLICIT INPUTS:
                                     0000    85 ;
                                     0000    86 ;	THE VARIOUS FIELDS OF THE FAB.
                                     0000    87 ;
                                     0000    88 ; OUTPUT PARAMETERS:
                                     0000    89 ;
                                     0000    90 ;	R11	IMPURE AREA ADDRESS
                                     0000    91 ;	R10	IFAB ADDRESS
                                     0000    92 ;	R9	IFAB ADDRESS
                                     0000    93 ;	R8	FAB ADDRESS
                                     0000    94 ;	R7	CALLER'S MODE
                                     0000    95 ;	R0 THRU R5 DESTROYED
                                     0000    96 ;
                                     0000    97 ; IMPLICIT OUTPUTS:
                                     0000    98 ;
                                     0000    99 ;	VARIOUS FIELDS OF THE IFAB ARE INITIALIZED AS
                                     0000   100 ;	WELL AS THE IFI, STS, AND STV FIELDS OF THE FAB.
                                     0000   101 ;
                                     0000   102 ; COMPLETION CODES:
                                     0000   103 ;
                                     0000   104 ;	NONE.  IF AN ERROR IS DETECTED, RETURNS TO USER
                                     0000   105 ;	(NOT CALLER) AFTER CLEANING UP AND RETURNING A STANDARD
                                     0000   106 ;	RMS ERROR CODE, IN PARTICULAR, DME, IFI, OR ANY OF
                                     0000   107 ;	THE CODES RETURNED BY RM$FABCHK.
                                     0000   108 ;
                                     0000   109 ; SIDE EFFECTS:
                                     0000   110 ;
                                     0000   111 ;	NONE
                                     0000   112 ;
                                     0000   113 ;--
RM0FSETI        OPEN AND CREATE COMMON SETUP                     21-AUG-1978 21:23:56   VAX-11 MACRO X0.3-11               Page   4
010             RM$FSETI  -  $OPEN AND $CREATE COMMON SETUP ROU                                                                  (1)

                                     0000   115 RM$FSETI::
                          FFFD'  30  0000   116 	BSBW	RM$FABCHK		;VALID FAB?
                                     0003   117 					;RETURNS ONLY IF O.K.
                                     0003   118 					;NOTE: SETS R11 TO IMPURE ADDR
                                     0003   119 					;	R9 TO IFI VALUE
                                     0003   120 					;	R8 TO FAB ADDR
                          74     12  0003   121 	BNEQ	ERRIFI			;ERROR IF IFI NON-ZERO
                                     0005   122 ;
                                     0005   123 ;  SO FAR, SO GOOD  -  TIME TO ALLOCATE AN IFAB
                                     0005   124 ;
                                     0005   125 ;!!!!!
                                     0005   126 ;  \MAY NEED TO ADD CODE TO SHARE A PAGE.\
                                     0005   127 ;!!!!!!
                                     0005   128 ;
                                     0005   129 ;  CHECK FOR PROCESS PERMANENT FILE SETUP, AND
                                     0005   130 ;  IF SO, SWITCH THE IMPURE AREA POINTER
                                     0005   131 ;
            68            32     E1  0005   132 	BBC	#FAB$V_PPF+FOP,(R8),10$	;BRANCH IF IMAGE I/O FILE
                          0C         0008       
            03            57     91  0009   133 	CMPB	R7,#PSL$C_USER		;ATTEMPT FROM USER MODE?
                          6E     13  000C   134 	BEQL	ERRPRV			;CAUGHT YOU!
            5B   00000000'9F     DE  000E   135 	MOVAL	@#PIO$GW_PIOIMPA,R11	;NO, SO IT'S OK - SWITCH IMPURE POINTER
                          FFE8'  30  0015   136 10$:	BSBW	RM$GET1PAG		;GO GET A FREE PAGE
            67            50     E9  0018   137 	BLBC	R0,ERROR1		;BRANCH ON FAILURE
                                     001B   138 ;
                                     001B   139 ;  SET UP THE PAGE/FILE FREE SPACE HEADER AT THE START OF THE PAGE
                                     001B   140 ;
            51            53     D0  001B   141 	MOVL	R3,R1			;SAVE ADDRESS OF PAGE
            83         08 A3     9E  001E   142 	MOVAB	8(R3),(R3)+		;SET FREE SPACE ADDRESS (FLINK)
            83         04 A3     9E  0022   143 	MOVAB	4(R3),(R3)+		; AND BLINK
            83            51     D0  0026   144 	MOVL	R1,(R3)+		;SET FREE SPACE'S F LINK
            83            51     D0  0029   145 	MOVL	R1,(R3)+		;AND ITS B LINK
            52            08     C3  002C   146 	SUBL3	#8,R2,(R3)		;AND ITS SIZE (512 BYTES)
                          63         002F       
            52            18     9A  0030   147 	MOVZBL	#IFB$C_BLN/4,R2		;BLOCK SIZE FOR IFAB
                          FFCA'  30  0033   148 	BSBW	RM$GETBLK		;GET IFAB SPACE (CAN'T FAIL)
            59            51     D0  0036   149 	MOVL	R1,R9			;SAVE IFAB ADDRESS
                                     0039   150 ;
                                     0039   151 ;  SET IFAB BID  (R9 POINTS TO IFAB)
                                     0039   152 ;
         08 A9            0B     90  0039   153 	MOVB	#IFB$C_BID,IFB$B_BID(R9)
                                     003D   154 ;
                                     003D   155 ;  INITIALIZE BDB CHAIN LIST HEADER
                                     003D   156 ;
         30 A9         30 A9     DE  003D   157 	MOVAL	IFB$L_BDB_FLNK(R9),IFB$L_BDB_FLNK(R9)
         34 A9         30 A9     DE  0042   158 	MOVAL	IFB$L_BDB_FLNK(R9),IFB$L_BDB_BLNK(R9)
                                     0047   159 ;
                                     0047   160 ;  ALLOCATE AND INITIALIZE A SLOT IN THE IFAB TABLE
                                     0047   161 ;
            55         18 AB     D0  0047   162 	MOVL	IMP$L_IFABTBL(R11),R5	;GET TABLE ADDR
                          FFB2'  30  004B   163 	BSBW	RM$GTSLT		;AND GET A SLOT
            2E            50     E9  004E   164 	BLBC	R0,ERRNOSLT		;BRANCH IF NONE
         02 A8            56     B0  0051   165 	MOVW	R6,FAB$W_IFI(R8)	;STORE IFI VALUE
                                     0055   166 	ASSUME	IMP$W_RMSSTATUS EQ 0
                                     0055   167 	ASSUME	IMP$V_IIOS EQ 0
            1E            6B     E8  0055   168 	BLBS	(R11),20$		;BRANCH IF NOT PIO SEGMENT
                                     0058   169 ;
RM0FSETI        OPEN AND CREATE COMMON SETUP                     21-AUG-1978 21:23:56   VAX-11 MACRO X0.3-11               Page   5
