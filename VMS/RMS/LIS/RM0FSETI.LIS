                          FF88'  31  007C       		BRW	L$RMS01
                                     007F       		.SAVE
                                 00000007       		.PSECT	L$RMS0,GBL,NOWRT
                                     0007       L$RMS01:
                                     0007   196 	RMSERR	PRV
            50       829A 8F     3C  0007       		MOVZWL	#<RMS$_PRV&^XFFFF>,R0
                          0B     11  000C   197 	BRB	ERROR
                                     000E   198 	$PSECT_RESTORE
                                 0000007F       		.RESTORE
                                     007F   199 ;
                                     007F   200 ERRNOSLT:
                                     007F   201 	$PSECT_BR	L$RMS0
                          FF8C'  31  007F       		BRW	L$RMS02
                                     0082       		.SAVE
                                 0000000E       		.PSECT	L$RMS0,GBL,NOWRT
                                     000E       L$RMS02:
            54            59     D0  000E   202 	MOVL	R9,R4			;IFAB ADDR
                          FFEC'  30  0011   203 	BSBW	RM$RETPAG
                                     0014   204 	RMSERR	DME			;RESTORE THE ERROR CODE
            50       84D4 8F     3C  0014       		MOVZWL	#<RMS$_DME&^XFFFF>,R0
                                     0019   205 ;
                          FFE4'  31  0019   206 ERROR:	BRW	RM$EX_NOSTR
                                     001C   207 	$PSECT_RESTORE
                                 00000082       		.RESTORE
                                     0082   208 ;
                          FF7B'  31  0082   209 ERROR1:	BRW	RM$EX_NOSTR		;GET OUT ON DME ERROR
                                     0085   210 
                                     0085   211 
                                     0085   212 	.END
