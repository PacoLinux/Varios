                                     0000    67 	.SBTTL RM$CREATE1  -  SEQUENTIAL CREATE ROUTINE
                                     0000    68 ;++
                                     0000    69 ; FUNCTIONAL DESCRIPTION:
                                     0000    70 ;
                                     0000    71 ;	THIS ROUTINE PERFORMS ALL OF THE FILE CREATE
                                     0000    72 ;	FUNCTIONS THAT ARE SPECIFIC TO THE SEQUENTIAL 
                                     0000    73 ;	FILE ORGANIZATION, INCLUDING:
                                     0000    74 ;
                                     0000    75 ;	1.  CHECKING THAT WRITE-SHARING HAS NOT BEEN SPECIFIED
                                     0000    76 ;	2.  CHECKING THAT RFM IS NOT STREAM FOR NON-DISK DEVICES
                                     0000    77 ;	3.  ALLOWING USER TO OVERRIDE DEFAULT BLOCK SIZE FOR MAGTAPE
                                     0000    78 ;	4.  PERFORMING EOF POSITIONING TO START OF FILE
                                     0000    79 ;	5.  CHECKING XAB CHAIN VALIDITY
                                     0000    80 ;	6.  CALLING THE COMMON CREATE ROUTINE
                                     0000    81 ;
                                     0000    82 ; CALLING SEQUENCE:
                                     0000    83 ;
                                     0000    84 ;	ENTERED VIA CASE BRANCH FROM RM$OPEN
                                     0000    85 ;	RETURNS BY JUMPING TO RM$CREATEXIT
                                     0000    86 ;
                                     0000    87 ; INPUT PARAMETERS:
                                     0000    88 ;
                                     0000    89 ;	R11	IMPURE AREA ADDRESS
                                     0000    90 ;	R10	FWA ADDRESS
                                     0000    91 ;	R9	IFAB ADDRESS
                                     0000    92 ;	R8	FAB ADDRESS
                                     0000    93 ;
                                     0000    94 ; IMPLICIT INPUTS:
                                     0000    95 ;
                                     0000    96 ;	THE CONTENTS OF THE FAB, IFAB, & FWA.
                                     0000    97 ;
                                     0000    98 ; OUTPUT PARAMETERS:
                                     0000    99 ;
                                     0000   100 ;	R0	STATUS CODE
                                     0000   101 ;	R1-R7	DESTROYED
                                     0000   102 ;
                                     0000   103 ; IMPLICIT OUTPUTS:
                                     0000   104 ;
                                     0000   105 ;	VARIOUS FIELDS IN THE IFAB & FAB ARE INITIALIZED.
                                     0000   106 ;
                                     0000   107 ; COMPLETION CODES:
                                     0000   108 ;
                                     0000   109 ;	STANDARD RMS
                                     0000   110 ;
                                     0000   111 ; SIDE EFFECTS:
                                     0000   112 ;
                                     0000   113 ;	NONE
                                     0000   114 ;
                                     0000   115 ;--
RM1CREATE       SEQUENTIAL-SPECIFIC CREATE                       21-AUG-1978 21:38:11   VAX-11 MACRO X0.3-11               Page   4
012             RM$CREATE1  -  SEQUENTIAL CREATE ROUTINE                                                                         (1)

                                     0000   117 RM$CREATE1::
                                     0000   118 	$TSTPT	CREATE1
                 00000200'9F     D6  0000       		INCL	@#PIO$GL_FMLH+512+TPT$L_CREATE1
                                     0006   119 ;
                                     0006   120 ;  SINCE CREATE IMPLIES PUT ACCESS, CHECK THAT SHARING HAS NOT BEEN SPECIFIED
                                     0006   121 ;  UNLESS USER IS PREPARED TO PROVIDE HIS OWN INTERLOCKING
                                     0006   122 ;
         17 A8            0B     93  0006   123 	BITB	#FAB$M_GET!FAB$M_PUT!FAB$M_UPD,FAB$B_SHR(R8)	; ANY SHARING?
                          05     13  000A   124 	BEQL	5$					; BRANCH IF NONE
         17 A8            06     E1  000C   125 	BBC	#FAB$V_UPI,FAB$B_SHR(R8),ERRSHR		; BRANCH IF USER NOT
                          70         0010       
                                     0011   126 							; PROVIDING INTERLOCKING
            69            1C     E0  0011   127 5$:	BBS	#DEV$V_RND,IFB$L_PRIM_DEV(R9),SETBL1	; BRANCH IF DISK
                          38         0014       
                                     0015   128 ;
                                     0015   129 ; CHECK THAT RFM IS NOT STREAM
                                     0015   130 ;
                                     0015   131 	ASSUME	FAB$C_UDF EQ 0
                                     0015   132 	ASSUME 	FAB$C_FIX EQ 1
                                     0015   133 	ASSUME	FAB$C_VAR EQ 2
                                     0015   134 	ASSUME	FAB$C_VFC EQ 3
            03         1F A8     91  0015   135 	CMPB	FAB$B_RFM(R8),#FAB$C_VFC		; RFM IN RANGE?
                          69     1A  0019   136 	BGTRU	ERRRFM					; BRANCH IF NOT
         3D A9            08     8A  001B   137 	BICB2	#FAB$M_BLK,IFB$B_RAT(R9)		; CLEAR BLK FOR UNIT REC DEVICES
            69            05     E1  001F   138 	BBC	#DEV$V_SQD,IFB$L_PRIM_DEV(R9),SETBLS	; BRANCH IF NOT MT
                          22         0022       
                                     0023   139 ;
                                     0023   140 ; FORCE 'RECORDS CAN'T CROSS BLOCK BOUNDARIES' AND
                                     0023   141 ; CHECK FOR DEFAULT BLOCK SIZE OVERRIDE FOR MAGTAPE
                                     0023   142 ;
            51         3C A8     B0  0023   143 	MOVW	FAB$W_BLS(R8),R1			; GET BLOCK SIZE
                          0C     13  0027   144 	BEQL	20$					; BRANCH IF NOT  SPECED
            12            51     B1  0029   145 	CMPW	R1,#18					; IS BLOCK SIZE GE MIN.
                          03     1E  002C   146 	BGEQU	10$					; BRANCH IF YES
            51            12     B0  002E   147 	MOVW	#18,R1					; MINIMUM RECORD SIZE
         38 A9            51     B0  0031   148 10$:	MOVW	R1,IFB$W_DEVBUFSIZ(R9)			; SET DEVICE BUFFER SIZE
                                     0035   149 ;
                                     0035   150 ; IF MAGTAPE HAS FIXED LENGTH RFM, DISALLOW
                                     0035   151 ; RECORDS LESS THAN 18 BYTES LONG AS
                                     0035   152 ; THIS COULD CAUSE AN ILLEGAL EOF BLOCK
                                     0035   153 ;
            01         3C A9     91  0035   154 20$:	CMPB	IFB$B_RFMORG(R9),#FAB$C_FIX		; FIXED RFM?
                          06     12  0039   155 	BNEQ	30$					; BRANCH IF NOT
            12         3E A9     B1  003B   156 	CMPW	IFB$W_LRL(R9),#18			; RECORD > MIN SIZE?
                          46     1F  003F   157 	BLSSU	ERRMRS					; BRANCH IF SO
                                     0041   158 30$:	SSB	#IFB$V_ANSI_D,(R9)			; FLAG AS ANSI
            69            26     E2  0041       		BBSS	#IFB$V_ANSI_D,(R9),30000$
                          00         0044       
                                     0045       30000$:
                                     0045   159 ;
                                     0045   160 ; ROUND UP BLS & RETURN IT
                                     0045   161 ;
                                     0045   162 SETBLS:	
         38 A9            03     A0  0045   163 	ADDW2	#3,IFB$W_DEVBUFSIZ(R9)
         38 A9            03     AA  0049   164 	BICW2	#3,IFB$W_DEVBUFSIZ(R9)
         3C A8         38 A9     B0  004D   165 SETBL1:	MOVW	IFB$W_DEVBUFSIZ(R9),FAB$W_BLS(R8)
         54 A9            06     E1  0052   166 	BBC	#DEV$V_SPL,IFB$L_AS_DEV(R9),SETEOF	; BRANCH IF DEVICE NOT SPOOLED
RM1CREATE       SEQUENTIAL-SPECIFIC CREATE                       21-AUG-1978 21:38:11   VAX-11 MACRO X0.3-11               Page   5
012             RM$CREATE1  -  SEQUENTIAL CREATE ROUTINE                                                                         (1)

                          05         0056       
         3C A8         5A A9     B0  0057   167 	MOVW	IFB$W_ASDEVBSIZ(R9),FAB$W_BLS(R8)	; RETURN ASSIGNED DEVICE BUF SIZE
                                     005C   168 ;
                                     005C   169 ; SET EOF BLOCK TO VBN 1 AND FIRST FREE BYTE TO 0.
                                     005C   170 ;
                       44 A9     D6  005C   171 SETEOF:	INCL	IFB$L_EBK(R9)				; EOF AT VBN 1
                                     005F   172 ;
                                     005F   173 ; IF FIXED RECORD FORMAT AND 'BLK' SET IN RAT,
                                     005F   174 ; MAKE SURE RECORD SIZE IS LESS THAN 1 BLOCK
                                     005F   175 ;
            01         3C A9     91  005F   176 	CMPB	IFB$B_RFMORG(R9),#FAB$C_FIX		; FIXED RFM?
                          0C     12  0063   177 	BNEQ	CHKXAB					; BRANCH IF NOT
         3D A9            03     E1  0065   178 	BBC	#FAB$V_BLK,IFB$B_RAT(R9),CHKXAB		; BRANCH IF 'BLK' NOT SPECED
                          07         0069       
         38 A9         4C A9     B1  006A   179 	CMPW	IFB$W_MRS(R9),IFB$W_DEVBUFSIZ(R9)	; DOES RECORD FIT?
                          16     1A  006F   180 	BGTRU	ERRMRS					; BRANCH IF NOT
                                     0071   181 ;++
                                     0071   182 ;
                                     0071   183 ;  ENTRY POINT FOR BLOCK I/O CREATE FOR UNKNOWN FILE ORG
                                     0071   184 ;
                                     0071   185 ;--
                                     0071   186 RM$BIO_CREATE::
                                     0071   187 ;
                                     0071   188 ;  PROCESS ALLOCATION XAB IF PRESENT, AND SET DEQ AND RTDEQ
                                     0071   189 ;
                                     0071   190 CHKXAB:
                          FF8C'  30  0071   191 	BSBW	RM$SETALLOC
            07            50     E9  0074   192 	BLBC	R0,EXIT
                                     0077   193 ;
                                     0077   194 ; CALL THE COMMON CREATE FILE ROUTINE
                                     0077   195 ;
                          FF86'  30  0077   196 	BSBW	RM$CREATECOM
                                     007A   197 ;
                                     007A   198 ; NOTE THAT FILE IS POSITIONED AT EOF IN THE IFAB
                                     007A   199 ; SO THAT CONNECT WILL BE SET UP FOR $PUT.
                                     007A   200 ;
                                     007A   201 	SSB	#IFB$V_EOF,(R9)				; FLAG EOF
            69            21     E2  007A       		BBSS	#IFB$V_EOF,(R9),30001$
                          00         007D       
                                     007E       30001$:
                          FF7F'  31  007E   202 EXIT:	BRW	RM$CREATEXIT
