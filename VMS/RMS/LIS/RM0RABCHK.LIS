                                     0000    64 	.SBTTL RM$RABCHK  -  COMMON ARGLIST AND RAB VALIDATION ROUTINE
                                     0000    65 ;++
                                     0000    66 ; FUNCTIONAL DESCRIPTION:
                                     0000    67 ;
                                     0000    68 ;  THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:
                                     0000    69 ;
                                     0000    70 ;	1. SETUP R11 TO POINT TO THE IMAGE I/O IMPURE AREA
                                     0000    71 ;	   (MAY BE CHANGED BY RMS0CONN OR RM0IFISI)
                                     0000    72 ;	2. CHECK ARGUMENT LIST FOR ACCESSIBILITY AND VALIDITY
                                     0000    73 ;	3. CHECK RAB FOR ACCESSIBILITY AND VALIDITY
                                     0000    74 ;	4. SET R8 TO ADDRESS OF RAB
                                     0000    75 ;	5. INHIBIT RMS INTERNAL ASTS
                                     0000    76 ;	6. SET R9 TO THE VALUE OF ISI
                                     0000    77 ;
                                     0000    78 ;
                                     0000    79 ;
                                     0000    80 ; CALLING SEQUENCE:
                                     0000    81 ;
                                     0000    82 ;	BSBW	RM$RABCHK
                                     0000    83 ;
                                     0000    84 ; INPUT PARAMETERS:
                                     0000    85 ;
                                     0000    86 ;	AP	ARGLIST ADDR
                                     0000    87 ;
                                     0000    88 ; IMPLICIT INPUTS:
                                     0000    89 ;
                                     0000    90 ;	THE CONTENTS OF THE ARGLIST AND THE BID, BLN, AND ISI FIELDS
                                     0000    91 ;	OF THE RAB.
                                     0000    92 ;
                                     0000    93 ; OUTPUT PARAMETERS:
                                     0000    94 ;
                                     0000    95 ;	R11	IMPURE AREA ADDRESS
                                     0000    96 ;	R9	ISI VALUE
                                     0000    97 ;	R8	RAB ADDRESS
                                     0000    98 ;	R7	MODE OF CALLER
                                     0000    99 ;
                                     0000   100 ; IMPLICIT OUTPUTS:
                                     0000   101 ;
                                     0000   102 ;	NONE
                                     0000   103 ;
                                     0000   104 ; COMPLETION CODES:
                                     0000   105 ;
                                     0000   106 ;	Z-BIT SET IF ISI = ZERO, ELSE CLEAR.
                                     0000   107 ;	IF ANY ERRORS, THE RMS ERROR CODE IS SET INTO R0
                                     0000   108 ;	AND RETURN IS MADE TO THE USER (NOT CALLER).
                                     0000   109 ;
                                     0000   110 ; SIDE EFFECTS:
                                     0000   111 ;
                                     0000   112 ;	RMS INTERNAL ASTS ARE INHIBITED.
                                     0000   113 ;
                                     0000   114 ;--
RM0RABCHK       COMMON RAB CHECKING                              21-AUG-1978 21:27:35   VAX-11 MACRO X0.3-11               Page   4
008             RM$RABCHK  -  COMMON ARGLIST AND RAB VALIDATION                                                                  (1)

                                     0000   116 ;++
                                     0000   117 ;
                                     0000   118 ;  SET UP POINTER TO IMAGE I/O IMPURE AREA AND SET R7 TO THE MODE OF THE CALLER
                                     0000   119 ;
                                     0000   120 ;--
                                     0000   121 RM$RABCHK::
                          5B     DC  0000   122 	MOVPSL	R11				;GET PSL
            02            16     EF  0002   123 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R11,R7
            57            5B         0005       
                                     0007   124 						;EXTRACT THE PREVIOUS MODE
            5B   00000000'9F     DE  0007   125 	MOVAL	@#PIO$GW_IIOIMPA,R11		;IMAGE IO IMPURE AREA ADDR
                                     000E   126 ;++
                                     000E   127 ;
                                     000E   128 ;  PERFORM ACCESSIBILITY CHECKS
                                     000E   129 ;
                                     000E   130 ;--
            58         04 AC     D0  000E   131 	MOVL	4(AP),R8			;GET RAB ADDRESS
                                     0012   132 	IFNOWRT	#RAB$C_BLN,(R8),ERRRAB		;RAB WRITEABLE?
       0044 8F            00     0D  0012       		PROBEW	#0,#RAB$C_BLN,(R8)
                          68         0017       
                          22     13  0018       		BEQL	ERRRAB
                                     001A   133 	ASSUME	RAB$B_BID EQ 0
                                     001A   134 	ASSUME	RAB$B_BLN EQ 1
       4401 8F            68     B1  001A   135 	CMPW	(R8),#RAB$C_BID+<RAB$C_BLN@8>	;RAB BLOCK ID & LENGTH?
                          0A     12  001F   136 	BNEQ	CHKRAB				;BRANCH IF PERHAPS NOT
       0000'CB            01     A8  0021   137 INHAST:	BISW2	S^#1@PIO$V_INHAST,W^<PIO$GW_STATUS-PIO$GW_IIOIMPA>(R11)
                                     0026   138 						;DISABLE ASTS
            59         02 A8     3C  0026   139 	MOVZWL	RAB$W_ISI(R8),R9		;SET R9 = ISI VALUE
                                     002A   140 ;++
                                     002A   141 ;
                                     002A   142 ;  DUMMY ROUTINE TO SIMPLY RETURN VIA AN RSB
                                     002A   143 ;
                                     002A   144 ;--
                                     002A   145 RM$NULL::
                                 05  002A   146 	RSB
                                     002B   147 
                                     002B   148 ;++
                                     002B   149 ;
                                     002B   150 ;  MAKE LONGER VERSION OF RAB VALIDITY CHECK
                                     002B   151 ;
                                     002B   152 ;--
            01            68     91  002B   153 CHKRAB:	CMPB	(R8),#RAB$C_BID			;IS IT A RAB?
                          0C     12  002E   154 	BNEQ	ERRRAB
         44 8F         01 A8     91  0030   155 	CMPB	RAB$B_BLN(R8),#RAB$C_BLN	;IS IT LONG ENOUGH?
                          EA     1E  0035   156 	BGEQU	INHAST				;CONTINUE IF YES
                                     0037   157 						;ELSE FALL THRU TO ERROR
                          00     1F  0037   158 	BLSSU	ERRBLN
RM0RABCHK       COMMON RAB CHECKING                              21-AUG-1978 21:27:35   VAX-11 MACRO X0.3-11               Page   5
