                                     0039   160 ;++
                                     0039   161 ;
                                     0039   162 ;  AN ERROR HAS OCCURRED IN VALIDATING THE ARGUMENT LIST OR RAB
                                     0039   163 ;
                                     0039   164 ;  SINCE AN ERROR CODE CANNOT BE SAFELY STORED IN THE RAB,
                                     0039   165 ;  NO ATTEMPT TO GENERATE AN ERR= AST WILL BE MADE.
                                     0039   166 ;  R0 WILL BE SET TO THE APPROPRIATE ERROR CODE AND AN
                                     0039   167 ;  EXCEPTION, IF ENABLED, WILL BE GENERATED UPON RET.
                                     0039   168 ;
                                     0039   169 ;--
                                     0039   170 ERRBLN:	$PSECT_BR	L$RMS0
                          FFC4'  31  0039       		BRW	L$RMS00
                                     003C       		.SAVE
                                 00000000       		.PSECT	L$RMS0,GBL,NOWRT
                                     0000       L$RMS00:
                          06     10  0000   171 	BSBB	RM$BASIC_ERR
                                     0002   172 	RMSERR_WORD	BLN			;INVALID BLOCK LENGTH
                               842C  0002       		.WORD	RMS$_BLN&^XFFFF
                                     0004   173 	$PSECT_RESTORE
                                 0000003C       		.RESTORE
                                     003C   174 ;
                                     003C   175 ERRRAB:	$PSECT_BR	L$RMS0
                          FFC5'  31  003C       		BRW	L$RMS01
                                     003F       		.SAVE
                                 00000004       		.PSECT	L$RMS0,GBL,NOWRT
                                     0004       L$RMS01:
                          02     10  0004   176 	BSBB	RM$BASIC_ERR
                                     0006   177 	RMSERR_WORD	RAB			;INVALID RAB
                               863C  0006       		.WORD	RMS$_RAB&^XFFFF
                                     0008   178 ;
                                     0008   179 RM$BASIC_ERR::
            50            9E     3C  0008   180 	MOVZWL	@(SP)+,R0			;PICK UP ERROR CODE
                                     000B   181 	SSB	#16,R0				;AND PREFIX THE FACILITY CODE
            50            10     E2  000B       		BBSS	#16,R0,30000$
                          00         000E       
                                     000F       30000$:
                                     000F   182 						;... TO THE ERROR CODE
                                 04  000F   183 	RET					;AND RETURN TO CALLER
                                     0010   184 						;EVERYTHING CLEANED UP MAGICALLY!
                                     0010   185 	$PSECT_RESTORE
                                 0000003F       		.RESTORE
                                     003F   186 
                                     003F   187 	.END
