                                     002D   178 ; OUTPUT PARAMETERS:
                                     002D   179 ;
                                     002D   180 ;	R7 THRU R1	DESTROYED
                                     002D   181 ;	R0		STATUS
                                     002D   182 ;
                                     002D   183 ; IMPLICIT OUTPUTS:
