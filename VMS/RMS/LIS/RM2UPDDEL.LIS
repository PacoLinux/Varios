                                     0000    70 	.SBTTL RM$UPDATE2 - HIGH LEVEL RELATIVE $UPDATE
                                     0000    71 ;++
                                     0000    72 ; FUNCTIONAL DESCRIPTION:
                                     0000    73 ;
                                     0000    74 ; THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:
                                     0000    75 ;
                                     0000    76 ;	1. CALLS UPDDLT2 SUBROUTINE TO ACCESS THE BUCKET AND DO RECORD LOCKING
                                     0000    77 ;	   AS REQUIRED
                                     0000    78 ;	2. CALLS RM$PUTUPD2 TO MOVE THE UPDATED RECORD TO THE BUFFER
                                     0000    79 ;	3. RELEASES ACCESS TO THE BUCKET (CAUSING IT TO BE WRITTEN UNLESS
                                     0000    80 ;	   DEFERRED WRITE WAS SPECIFIED AT OPEN TIME) AND EXITS RMS
                                     0000    81 ;
                                     0000    82 ; CALLING SEQUENCE:
                                     0000    83 ;
                                     0000    84 ; ENTERED VIA CASE BRANCH FROM RM$UPDATE AT RM$UPDATE1.
                                     0000    85 ;
                                     0000    86 ; INPUT PARAMETERS:
                                     0000    87 ;
                                     0000    88 ;	R11	IMPURE AREA ADDRESS
                                     0000    89 ;	R10	IFAB ADDR
                                     0000    90 ;	R9	IRAB ADDR
                                     0000    91 ;	R8	RAB ADDR
                                     0000    92 ;
                                     0000    93 ; IMPLICIT INPUTS:
                                     0000    94 ;
                                     0000    95 ;	THE CONTENTS OF THE RAB AND RELATED IRAB AND IFAB.
                                     0000    96 ;
                                     0000    97 ; OUTPUT PARAMETERS:
                                     0000    98 ;
                                     0000    99 ;	R7 THRU R1	DESTROYED
                                     0000   100 ;	R0		STATUS
                                     0000   101 ;
                                     0000   102 ; IMPLICIT OUTPUTS:
                                     0000   103 ;
                                     0000   104 ;	VARIOUS FIELDS OF THE RAB ARE FILLED IN TO REFLECT
                                     0000   105 ;	THE STATUS OF THE OPERATION (SEE FUNCTIONAL SPEC
                                     0000   106 ;	FOR DETAILS).
                                     0000   107 ;
                                     0000   108 ;	THE IRAB IS SIMILARLY UPDATED.
                                     0000   109 ;
                                     0000   110 ; COMPLETION CODES:
                                     0000   111 ;
                                     0000   112 ;	STANDARD RMS (SEE FUNCTIONAL SPEC).
                                     0000   113 ;
                                     0000   114 ; SIDE EFFECTS:
                                     0000   115 ;
                                     0000   116 ;	NONE
                                     0000   117 ;
                                     0000   118 ;--
