RM2UPDDEL       RELATIVE SPECIFIC $UPDATE AND $DELETE            21-AUG-1978 21:55:53   VAX-11 MACRO X0.3-11               Page   7
007             UPDDLT2  -  COMMON $UPDATE/$DELETE RELATIVE ROU                                                                  (1)

                                     004E   227 	.SBTTL	UPDDLT2  -  COMMON $UPDATE/$DELETE RELATIVE ROUTINE
                                     004E   228 ;++
                                     004E   229 ;  FUNCTIONAL DESCRIPTION:
                                     004E   230 ;
                                     004E   231 ;  THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:
                                     004E   232 ;
                                     004E   233 ;	1. CHECKS THAT THERE IS A CURRENT RECORD.
                                     004E   234 ;	2. ACCESSES THE BUCKET WITH LOCK.
                                     004E   235 ;	3. VERIFIES THAT THE RECORD IS LOCKED.
                                     004E   236 ;	4. UNLOCKS THE RECORD UNLESS MANUAL LOCKING IS SPECIFIED (RAB$V_ULK = 1)
                                     004E   237 ;
                                     004E   238 ;  CALLING SEQUENCE:
                                     004E   239 ;
                                     004E   240 ;	BSBB	UPDDLT2
                                     004E   241 ;
                                     004E   242 ;  INPUT PARAMETERS:
                                     004E   243 ;
                                     004E   244 ;	AS FOR RM$UPDATE2
                                     004E   245 ;
                                     004E   246 ;  IMPLICIT INPUTS:
                                     004E   247 ;
                                     004E   248 ;  THE FIELDS OF THE VARIOUS CONTROL BLOCKS, INCLUDING:
                                     004E   249 ;
                                     004E   250 ;	IRB$L_RP	RECORD #
                                     004E   251 ;	IRB$L_CURVBN	STARTING VBN FOR BUCKET
                                     004E   252 ;	IRB$L_RP_OFF	OFFSET TO RECORD IN BUCKET
                                     004E   253 ;	RAB$V_ULK	MANUAL LOCKING FLAG
                                     004E   254 ;
                                     004E   255 ;  OUTPUT PARAMETERS:
                                     004E   256 ;
                                     004E   257 ;	R5		RECORD ADDRESS IN BUFFER
                                     004E   258 ;	R4		BDB ADDRESS
                                     004E   259 ;	R1		RECORD #
                                     004E   260 ;	R0		STATUS CODE
                                     004E   261 ;	R2_R3,AP	DESTROYED
                                     004E   262 ;
                                     004E   263 ;  IMPLICIT OUTPUTS:
                                     004E   264 ;
                                     004E   265 ;	NONE.
                                     004E   266 ;
                                     004E   267 ;  COMPLETION CODES:
                                     004E   268 ;
                                     004E   269 ;	STANDARD RMS.
                                     004E   270 ;
                                     004E   271 ;  SIDE EFFECTS:
                                     004E   272 ;
                                     004E   273 ;	MAY HAVE SWITCHED TO RUNNING AT AST LEVEL.
                                     004E   274 ;--
RM2UPDDEL       RELATIVE SPECIFIC $UPDATE AND $DELETE            21-AUG-1978 21:55:53   VAX-11 MACRO X0.3-11               Page   8
007             UPDDLT2  -  COMMON $UPDATE/$DELETE RELATIVE ROU                                                                  (1)

                                     004E   276 UPDDLT2:
            51         30 A9     D0  004E   277 	MOVL	IRB$L_CURVBN(R9),R1		; GET CURRENT VBN
                          31     13  0052   278 	BEQL	ERRCUR				; ERROR IF NONE
                                     0054   279 	$CSHFLAGS	LOCK			; GET LOCK ON BUCKET
            53            01     D0  0054       		MOVL #$$.TMP,R3
                          FFA6'  30  0057   280 	BSBW	RM$READBKT2			; ACCESS THE BUCKET
            24            50     E9  005A   281 	BLBC	R0,50$				; GET OUT ON ERROR
            51         34 A9     D0  005D   282 	MOVL	IRB$L_RP(R9),R1			; GET RECORD #
                          22     13  0061   283 	BEQL	ERRCUR				; ERROR IF NONE
            6A            33     E0  0063   284 	BBS	#IFB$V_NORECLK,(R10),50$	; BRANCH IF NO LOCKING
                          1A         0066       
            69            2D     E5  0067   285 	BBCC	#IRB$V_UNLOCK_RP,(R9),30$	; CLEAR AUTO UNLOCK FLAG 
                          09         006A       
                                     006B   286 						; IF MANUAL LOCK, DON'T UNLOCK
                                     006B   287 ;
                                     006B   288 ;  RECORD LOCKING REQUIRED.  IF RECORD LOCKED VIA AUTOMATIC LOCKING
                                     006B   289 ;  (IRB$V_UNLOCK_RP = 1), UNLOCK THE RECORD, GIVING AN ERROR IF IT WAS NOT
                                     006B   290 ;  LOCKED.  SINCED THE BUCKET IS STILL LOCKED, NO OTHER USER CAN LOCK THE
                                     006B   291 ;  RECORD BEFORE THE BUCKET IS RELEASED.  
                                     006B   292 ;
                                     006B   293 ;  IF MANUAL LOCKING (IRB$V_UNLOCK_RP = 0) NEED MERELY CHECK THAT THE
                                     006B   294 ;  RECORD IS LOCKED.
                                     006B   295 ;
                          52     D4  006B   296 	CLRL	R2				; CLEAR HIGH ORDER RP
                          FF90'  30  006D   297 	BSBW	RM$UNLOCK			; UNLOCK RECORD
            0F            50     E9  0070   298 	BLBC	R0,ERRRNL			; BRANCH IF NOT LOCKED
                                 05  0073   299 	RSB					; ALL SET
                          FF89'  30  0074   300 30$:	BSBW	RM$QUERY_LCK			; IS RECORD LOCKED?
       8039 8F            50     B1  0077   301 	CMPW	R0,#RMS$_OK_ALK&^XFFFF		; WELL IS IT?
                          04     12  007C   302 	BNEQ	ERRRNL				; BRANCH IF NOT
                                     007E   303 	RMSSUC					; VANILLA SUCCESS
            50            01     D0  007E       		MOVL	#1,R0
                                 05  0081   304 50$:	RSB					; ALL SET
                                     0082   305 ;
                                     0082   306 ;  HANDLE ERRORS
                                     0082   307 ;
                                     0082   308 ERRRNL:	$PSECT_BR	L$RMS2
                          FF7B'  31  0082       		BRW	L$RMS22
                                     0085       		.SAVE
                                 00000000       		.PSECT	L$RMS2,GBL,NOWRT
                                     0000       L$RMS22:
         0C A8   000181A0 8F     D0  0000   309 	MOVL	#RMS$_RNL,RAB$L_STV(R8)		; SUB ERROR CODE OF RECORD NOT
                                     0008   310 						; LOCKED
                                     0008   311 						; AND FALL THRU TO ERRCUR1
                                     0008   312 ERRCUR1:
                                     0008   313 	RMSERR	CUR				; NO CURRENT RECORD
            50       84B4 8F     3C  0008       		MOVZWL	#<RMS$_CUR&^XFFFF>,R0
                                 05  000D   314 	RSB
                                     000E   315 	$PSECT_RESTORE
                                 00000085       		.RESTORE
                                     0085   316 ;
                          FF80'  31  0085   317 ERRCUR:	BRW	ERRCUR1				; GO SET ERROR CODE AND RETURN
                                     0088   318 
                                     0088   319 
                                     0088   320 	.END
