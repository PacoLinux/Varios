MACRO		XAB$W_POS4	= 38,0,16,0%; 	! SEGMENT 4 
MACRO		XAB$W_POS5	= 40,0,16,0%; 	! SEGMENT 5 
MACRO		XAB$W_POS6	= 42,0,16,0%; 	! SEGMENT 6 
MACRO		XAB$W_POS7	= 44,0,16,0%; 	! SEGMENT 7 
MACRO		XAB$B_SIZ	= 46,0,0,0%; 	! KEY FIELD SEGMENT SIZES 
LITERAL	XAB$S_SIZ	= 8; 
