MACRO		XAB$W_IFL	= 26,0,16,0%; 		! INDEX BUCKET FILL SIZE (BYTES) 
MACRO		XAB$W_DFL	= 28,0,16,0%; 		! DATA BUCKET FIL SIZE (BYTES) 
MACRO		XAB$W_POS	= 30,0,0,0%; 	! KEY FIELD RECORD OFFSET POSITIONS 
