                                     0008   100 	.SBTTL	LNK$CALCELAPS
                                     0008   200 ;++
                                     0008   300 ; FUNCTIONAL DESCRIPTION:
                                     0008   400 ;
                                     0008   500 ;	THIS ROUTINE IS CALLED TO CALCULATE THE ELAPSED TIME BETWEEN TWO
                                     0008   600 ;	ABSOLUTE TIME VALUES. THE ADDRESS OF THE RESULT IS RETURNED AS THE VALUE OF THE RO
                                     0008   700 ;	THE ELAPSED TIME IS IN THE "DELTA TIME" FORMAT OF THE SYSTEM AND
                                     0008   800 ;	IS THEREFORE SUITABLE FOR DISPLAYING THROUGH SYSTEM SERVICES
                                     0008   900 ;	SUCH AS FAO %T.
                                     0008  1000 ;
                                     0008  1100 ; CALLING SEQUENCE:
                                     0008  1200 ;
                                     0008  1300 ;	LNK$CALCELAPS(TIMEADDR1,TIMEADDR2)
                                     0008  1400 ;
                                     0008  1500 ;	WHERE:	TIMEADDR1 = ADDRESS OF A QUADWORD CONTAINING THE
                                     0008  1600 ;				START TIME
                                     0008  1700 ;		TIMEADDR2 = ADDRESS OF A QUADWORD CONTAINING THE
                                     0008  1800 ;				END TIME.
                                     0008  1900 ;
                                     0008  2000 ;
                                     0008  2100 ; INPUT PARAMETERS:
                                     0008  2200 ;
                                     0008  2300 ;	AS ABOVE
                                     0008  2400 ;
                                     0008  2500 ; IMPLICIT INPUTS:
                                     0008  2600 ;
                                     0008  2700 ;	NONE
                                     0008  2800 ;
                                     0008  2900 ; OUTPUT PARAMETERS:
                                     0008  3000 ;
                                     0008  3100 ;	THE ROUTINE HAS AS ITS VALUE (I.E. CONTENT RETURNED IN R0)
                                     0008  3200 ;	THE ADDRESS OF AN OWN QUADWORD CONTAINING THE VALUE
                                     0008  3300 ;		TIME1 - TIME2
                                     0008  3400 ;	WHICH IS (PRESUMABLY) NEGATIVE.
                                     0008  3500 ;
                                     0008  3600 ;
                                     0008  3700 ; IMPLICIT OUTPUTS:
                                     0008  3800 ;
                                     0008  3900 ;	NONE
                                     0008  4000 ;
                                     0008  4100 ; COMPLETION CODES:
                                     0008  4200 ;
                                     0008  4300 ;	NONE
                                     0008  4400 ;
                                     0008  4500 ; SIDE EFFECTS:
                                     0008  4600 ;
                                     0008  4700 ;	NONE
                                     0008  4800 ;
                                     0008  4900 ;--
                                     0008  5000 
                                 00000000  5100 	.PSECT	$CODE$,NOWRT,LONG
                                     0000  5200 LNK$CALCELAPS::
                               0000  0000  5300 	.WORD	0			;
            50       0000'CF     9E  0002  5500 	MOVAB	W^ELAPSED,R0		; GET ADDRESS OF QUADWORD TO RECEIVE RESULT
            51         08 AC     D0  0007  5600 	MOVL	TIMEADDR2(AP),R1	; GET ADDRESS OF TIME 2
            60         04 BC     7D  000B  5700 	MOVQ	@TIMEADDR1(AP),(R0)	; INITIALIZE RESULT WITH TIME 1
            60            81     C2  000F  5800 	SUBL	(R1)+,(R0)		; SUBTRACT LOW ORDER HALF OF
