X0.1-4          FAL$CVT_BIN2EXT - CONVERT BINARY TO EXTENSIBLE                                                                   (3)

                                     0000    62 	.SBTTL	FAL$CVT_BIN2EXT - CONVERT BINARY TO EXTENSIBLE
                                 00000000    63 	.PSECT	FAL$CODE	NOSHR,EXE,RD,NOWRT,BYTE
                                     0000    64 
                                     0000    65 ;++
                                     0000    66 ; FUNCTIONAL DESCRIPTION:
                                     0000    67 ;
                                     0000    68 ;	FAL$CVT_BIN2EXT CONVERTS AN UNSIGNED LONGWORD VALUE TO AN EXTENSIBLE
                                     0000    69 ;	FIELD FORMAT AND STORES THE RESULT IN A MINIMAL NUMBER OF BYTES.
                                     0000    70 ;
                                     0000    71 ; CALLING SEQUENCE:
                                     0000    72 ;
                                     0000    73 ;	BSB/JSB	FAL$CVT_BIN2EXT
                                     0000    74 ;
                                     0000    75 ; INPUT PARAMETERS:
                                     0000    76 ;
                                     0000    77 ;	R1	THE BINARY VALUE TO CONVERT AND STORE
                                     0000    78 ;	R3	THE ADDRESS OF NEXT BYTE IN BUFFER TO STORE THE RESULT
                                     0000    79 ;
                                     0000    80 ; IMPLICIT INPUTS:
                                     0000    81 ;
                                     0000    82 ;	NONE
                                     0000    83 ;
                                     0000    84 ; OUTPUT PARAMETERS:
                                     0000    85 ;
                                     0000    86 ;	R1	ZEROED
                                     0000    87 ;	R3	ADDRESS OF LAST BYTE OF RESULT + 1
                                     0000    88 ;
                                     0000    89 ; IMPLICIT OUTPUTS:
                                     0000    90 ;
                                     0000    91 ;	NONE
                                     0000    92 ;
                                     0000    93 ; COMPLETION CODES:
                                     0000    94 ;
                                     0000    95 ;	NONE
                                     0000    96 ;
                                     0000    97 ; SIDE EFFECTS:
                                     0000    98 ;
                                     0000    99 ;	NONE
                                     0000   100 ;
                                     0000   101 ;--
                                     0000   102 
                                     0000   103 FAL$CVT_BIN2EXT::			; CONTROL POINT
            83            51     90  0000   104 	MOVB	R1,(R3)+		; COPY 7 BITS TO DST BYTE--THE HIGH
                                     0003   105 					;  BIT WILL BE CORRECTED LATER
            51         7F 8F     8A  0003   106 	BICB2	#^X7F,R1		; DISCARD SRC BITS JUST COPIED
            51         F9 8F     9C  0007   107 	ROTL	#-7,R1,R1		; MOVE NEXT 7 BITS INTO PLACE
                          51         000B       
                          07     13  000C   108 	BEQL	10$			; ALL DONE IF REMAINING SRC BITS
                                     000E   109 					;  ARE ZERO
         FF A3         80 8F     88  000E   110 	BISB2	#^X80,-1(R3)		; SET EXTENSIBLE BIT IN DST BYTE
                          EB     11  0013   111 	BRB	FAL$CVT_BIN2EXT		;  AND PROCESS NEXT BYTE
                                 05  0015   112 10$:	RSB				; EXIT
FALENCODE       - ENCODE DAP MESSAGE                             22-AUG-1978 01:12:24   VAX-11 MACRO X0.3-11               Page   4
X0.1-4          FAL$CVT_BIN2IMG - CONVERT BINARY TO IMAGE                                                                        (4)

                                     0016   114 	.SBTTL	FAL$CVT_BIN2IMG - CONVERT BINARY TO IMAGE
                                 00000016   115 	.PSECT	FAL$CODE	NOSHR,EXE,RD,NOWRT,BYTE
                                     0016   116 
                                     0016   117 ;++
                                     0016   118 ; FUNCTIONAL DESCRIPTION:
                                     0016   119 ;
                                     0016   120 ;	FAL$CVT_BIN2IMG CONVERTS AN UNSIGNED LONGWORD VALUE TO AN IMAGE
                                     0016   121 ;	FIELD FORMAT (COUNTED BINARY STRING) AND STORES THE RESULT IN A
                                     0016   122 ;	MINIMAL NUMBER OF BYTES.
                                     0016   123 ;
                                     0016   124 ; CALLING SEQUENCE:
                                     0016   125 ;
                                     0016   126 ;	BSB/JSB	FAL$CVT_BIN2IMG
                                     0016   127 ;
                                     0016   128 ; INPUT PARAMETERS:
                                     0016   129 ;
                                     0016   130 ;	R1	THE BINARY VALUE TO CONVERT AND STORE
                                     0016   131 ;	R3	THE ADDRESS OF NEXT BYTE IN BUFFER TO STORE THE RESULT
                                     0016   132 ;
                                     0016   133 ; IMPLICIT INPUTS:
                                     0016   134 ;
                                     0016   135 ;	NONE
                                     0016   136 ;
                                     0016   137 ; OUTPUT PARAMETERS:
                                     0016   138 ;
                                     0016   139 ;	R1	ZEROED
                                     0016   140 ;	R2	DESTROYED
                                     0016   141 ;	R3	ADDRESS OF LAST BYTE OF RESULT + 1
                                     0016   142 ;
                                     0016   143 ; IMPLICIT OUTPUTS:
                                     0016   144 ;
                                     0016   145 ;	NONE
                                     0016   146 ;
                                     0016   147 ; COMPLETION CODES:
                                     0016   148 ;
                                     0016   149 ;	NONE
                                     0016   150 ;
                                     0016   151 ; SIDE EFFECTS:
                                     0016   152 ;
                                     0016   153 ;	NONE
                                     0016   154 ;
                                     0016   155 ;--
                                     0016   156 
                                     0016   157 FAL$CVT_BIN2IMG::			; CONTROL POINT
            52            53     D0  0016   158 	MOVL	R3,R2			; SAVE ADDRESS OF DST COUNT BYTE
                          83     94  0019   159 	CLRB	(R3)+			; ZERO DST COUNT BYTE
                          51     D5  001B   160 	TSTL	R1			; TEST VALUE TO CONVERT
                          0E     13  001D   161 	BEQL	20$			; ALL DONE IF VALUE IS ZERO
            83            51     90  001F   162 10$:	MOVB	R1,(R3)+		; COPY NEXT BYTE TO DST
                          62     96  0022   163 	INCB	(R2)			; INCREMENT BYTE COUNTER
                          51     94  0024   164 	CLRB	R1			; DISCARD BYTE JUST COPIED
            51         F8 8F     9C  0026   165 	ROTL	#-8,R1,R1		; MOVE NEXT BYTE INTO PLACE
                          51         002A       
                          F2     12  002B   166 	BNEQ	10$			; MORE TO DO IF ANY REMAINING BITS
                                     002D   167 					;  ARE NON-ZERO
                                 05  002D   168 20$:	RSB				; EXIT
                                     002E   169 	.END
