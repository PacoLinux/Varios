     25					;+
     26					; PARSE DIRECTORY NAME - FIRST HALF.
     27					;
     28					; CALLING SEQUENCE:
     29					;	CALL	.PRSDI
     30					; INPUTS:
     31					;	R0=FDB ADDRESS
     32					;	R1=ADDRESS OF FILE NAME BLOCK
     33					;	R2=POINTER TO DEV, DIR, AND NAME DESCRIPTORS
     34					;	R3=DEFAULT FILE NAME BLOCK OR ZERO IF NONE SPECIFIED
     35					;
     36					; OUTPUTS:
     37					;	C=0 IF SUCCESSFUL, C=1 IF ERROR, F.ERR SET TO REASON
     38					;	ALL REGISTERS PRESERVED IF RETURN FROM THIS MODULE,
     39					;	ELSE CONTROL IS TRANSFERRED TO PARDI, THE SECOND HALF
     40					;	OF PARSE FILE NAME ROUTINE, AND REGISTERS RESTORED AND
     41					;	CONDITION CODE SET ON RETURN TO CALLER THERE.
     42					;-
     43
     44	000000				.PRSDI::.SAVR1
     45	000004	012760 	000001 	000052 		MOV	#1,F.ERR(R0)
     46	000012	132760 	000010 	000017 	..PSDI::BITB	#FD.DIR,F.RCTL(R0)
     47	000020	001002 				BNE	5$
     48	000022	000167 	000100 		3$:	JMP	70$		;DEFAULT TO MFD FOR NON-DIRECTORY DEVICE
     49	000026	132760 	000020 	000017 	5$:	BITB	#FD.SDI,F.RCTL(R0) ;IF SINGLE DIRECTORY DEVICE
     50	000034	001372 				BNE	3$		;ALSO USE MFD
     51	000036	132760 	000020 	000043 		BITB	#FA.TMP,F.FACC(R0) ;IF A TEMPORARY FILE
     52	000044	001366 				BNE	3$		;SKIP UFD SEARCH
     53	000046	005702 				TST	R2
     54	000050	001415 				BEQ	30$		;BRANCH IF NULL DIRECTORY DESCRIPTOR
     55	000052	005762 	000004 			TST	N.DRSZ(R2)
     56	000056	001412 				BEQ	30$		;BRANCH IF NULL DIRECTORY DESCRIPTOR
PARSDI 0106MS, PARSE DIRECTORY 	MACRO M1110  22-AUG-78 01:22  PAGE 7-1


