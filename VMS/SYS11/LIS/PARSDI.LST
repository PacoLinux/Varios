PARSDI 0106MS, PARSE DIRECTORY 	MACRO M1110  22-AUG-78 01:22  PAGE 9


     80					;
     81					; SUBROUTINE TO GET DIRECTORY ID FROM DEFAULT FILE NAME BLOCK IF IT EXISTS
     82					; AND IS APPLICABLE (I.E., IS MFD OR DEVICE AND UNIT MATCH).
     83					;
     84					; INPUTS:
     85					;	R1 = FNB
     86					;	R3 = DFNB OR 0
     87					;
     88					; OUTPUTS:
     89					;	C = 0 IF DID USED
     90					;	  = 1 IF NOT
     91					;
     92
     93	000150	005703 			..DFDI::TST	R3
     94	000152	001431 				BEQ	20$		;BRANCH IF NO DEFAULT NAME BLOCK
     95	000154	016304 	000024 			MOV	N.DID(R3),R4
     96	000160	001426 				BEQ	20$		;BRANCH IF NO DEFAULT ID SPECIFIED
     97	000162	022704 	000004 			CMP	#4,R4		;IF 4, THEN MFD
     98	000166	001410 				BEQ	10$		;UNIVERSAL FOR ALL DEVICES
     99					;
    100					; DIRECTORY ID IS SPECIFIED, USE IT ONLY IF DEVICE AND UNIT AGREE
    101	000170	026361 	000032 	000032 		CMP	N.DVNM(R3),N.DVNM(R1)
    102	000176	001017 				BNE	20$
    103	000200	026361 	000034 	000034 		CMP	N.UNIT(R3),N.UNIT(R1)
    104	000206	001013 				BNE	20$
    105	000210	016361 	000024 	000024 	10$:	MOV	N.DID(R3),N.DID(R1) ;MOVE THE DEFAULT DIR ID
    106	000216	016361 	000026 	000026 		MOV	N.DID+2(R3),N.DID+2(R1) ;TO THE FILE NAME BLOCK
    107	000224	016361 	000030 	000030 		MOV	N.DID+4(R3),N.DID+4(R1)
    108	000232	000241 				CLC			;INDICATE SUCCESS
    109	000234					RETURN
    110
    111	000236	000261 			20$:	SEC			;INDICATE FAILURE
    112	000240					RETURN
    113					;
    114					;
    115	000242					$END	PARSDI
    116					;
    117		000001 				.END
