     24					; GET THE DIRECTORY ID - USER CALLABLE ROUTINE
     25					;
     26					; THE ENTRY POINT TO FIND DIRECTORY ID ROUTINE(DIRFND) IS PUSHED
     27					; ON THE STACK, CONTROL IS THEN TRANSFERRED TO THE
     28					; SECOND HALF OF THIS ROUTINE - GETDI, AND RETURNS TO CALLER THERE.
     29					;
     30					;-
     31	000000				.GTDIR::.SAVR1
     32	000004					CALL	..ELP3		;ATTEMPT SYSTEM PARSE DIRECTIVE
     33	000010	103406 				BCS	10$		;BRANCH IF FAILURE
     34	000012	001005 				BNE	10$		;BRANCH IF SUCCESS
     35									;FALL THROUGH IS NOT SUPPORTED
     36	000014	010746 				MOV	PC,-(SP)	;PUSH
     37	000016	062716 	000000C			ADD	#..DIRF-.,(SP)	;ENTRY TO DIRFND ON STACK
     38	000022					CALLR	..GTDI		;JUMP TO GETDI
     39	000026				10$:	RETURN
     40					;
     41	000030					$END	GETDIR
     42					;
     43					;
     44		000001 				.END
