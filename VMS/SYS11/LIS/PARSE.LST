     25					;+
     26					; SET UP FILE NAME BLOCK BY PARSING IF NECESSARY OR BY
     27					; ASSIGNING THE LUN IF THE FILE ID IS SET UP
     28					; CALLING SEQUENCE:
     29					;	CALL	..STFN
     30					; INPUTS:
     31					;	R0=FDB
     32					;OUTPUTS:
     33					;	R1=FILE NAME BLOCK ADDRESS
     34					;	R0 PRESERVED, R2-R5 DESTROYED
     35					;-
     36	000000	010001 			..STFN::MOV	R0,R1
     37	000002	062701 	000102 			ADD	#F.FNB,R1	;FORM R1=ADDRESS OF FILE NAME BLOCK
     38	000006	005711 				TST	@R1		;IS FILE ID ALREADY SET UP
     39	000010	001406 				BEQ	10$		;BRANCH IF NO
     40	000012	005760 	000070 			TST	F.BDB(R0)	;IF FILE IS ALREADY OPEN
     41	000016	001002 				BNE	5$		;SKIP THE ASSIGN LUN
     42	000020					CALL	..ALUN		;ASSIGN THE LUN ANYWAY
     43	000024				5$:	RETURN
     44					; FILE ID NOT SET UP, DO THE PARSE
     45	000026	016002 	000044 		10$:	MOV	F.DSPT(R0),R2
     46	000032	016003 	000046 			MOV	F.DFNB(R0),R3
     47	000036	000405 				BR	..PARS
PARSE 0104MS, PARSE THE FILE NA	MACRO M1110  22-AUG-78 01:23  PAGE 8


     49					;+
     50					; PARSE ROUTINE - ACCEPTS ADDRESS OF DEVICE, DIRECTORY, AND
     51					; FILE NAME DESCRIPTORS
     52					;
     53					; CALLING SEQUENCE
     54					;	CALL	..PARS
     55					; INPUTS:
     56					;	R0=FDB ADDRESS
     57					;	R1=ADDRESS OF FILE NAME BLOCK
     58					;	R2=POINTER TO DEV, DIR, AND NAME DESCRIPTORS
     59					;	R3=DEFAULT FILE NAME BLOCK OR ZERO IF NONE SPECIFIED
     60					; OUTPUTS:
     61					;	R0,R1 PRESERVED
     62					;	R2-R5 ALTERED
     63					;	C=0 FOR SUCCESSFUL PARSE
     64					;	C=1 FOR FAILURE IN PARSE, F.ERR SET TO REASON.
     65					; OPERATION:
     66					;	FILLS IN THE FILE NAME BLOCK FROM THE STRING AND FILLS IN
     67					; UNSPECIFIED FIELDS FROM THE DEFAULT FILE NAME BLOCK, IF PRESENT.
     68					; ALSO ASSIGNS THE LUN TO THE SPECIFIED DEVICE
     69					;
     70					; ALTERNATE ENTRY POINT:
     71					;	CALL	.PARSE
     72					; OPERATION:
     73					;	SAME AS ..PARS, EXCEPT IT PRESERVES ALL REGISTERS.  THIS ENTRY IS
     74					; INTENDED FOR THE USER LEVEL CALL.
     75					;
     76					;-
     77	000040				.PARSE::.SAVR1			;SAVE REGISTERS R1-R5
     78	000044	012760 	000001 	000052 		MOV	#1,F.ERR(R0)
     79	000052				..PARS::CALL	..ELP0		;ATTEMPT SYSTEM PARSE DIRECTIVE
     80	000056	103415 				BCS	20$		;BRANCH IF ERROR
     81	000060	001404 				BEQ	10$		;BRANCH IF DIRECTIVE NOT SUPPORTED
     82	000062					CALL	..DFDI		;SUCCESS - APPLY DEFAULT DID FROM DFNB
     83	000066	000241 				CLC
     84	000070	000410 				BR	20$
     85
     86	000072				10$:	CALL	..PSDV		;PARSE THE DEVICE NAME
     87	000076	103405 				BCS	20$
     88						.IF	EQ,R$$SPL-1	;CONDITIONAL FOR SPOOLING
     89						BITB	#FD.SPL,F.BKP1(R0);IF SPOOLING IS ENABLED
     90						BNE	20$		;SIMPLY EXIT BECAUSE FULL FILE SPEC
     91						.ENDC			;HAS BEEN SUPPLIED BY ASSLUN ROUTINE
     92	000100					CALL	..PSDI		;PARSE THE DIRECTORY NAME
     93	000104	103402 				BCS	20$
     94	000106					CALL	..PSFN		;PARSE THE FILE NAME DESCRIPTOR
     95	000112				20$:	RETURN
     96					;
     97					;
     98	000114					$END	PARSE
     99					;
    100		000001 				.END
