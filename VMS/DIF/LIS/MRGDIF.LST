0001    	SUBROUTINE MRGDIF(ILUN,RECORD,LENINP,ISTAT,INPNAM,INMLNG,ITYPE,
        	1CLUN,CSTAT,COMREC,LENCOM,COMNAM,CNMLNG,CTYPE,OUTMOD)
        C
        C
0002    	COMMON MAP(100,2),NUMLIN,MAPTOP(2),LSTMAP(2),MLUN,
        	1IHOLD(30,2),NINCOR,IFREE(2),IFIRST(2),LAST(2),MAXBUF,
        	2BUFR,ISWTCH,COMCHR,NSWVAL(10),LSTMAT(2),IRECSZ,
        	3MRKPNT(4,2),INOUT,MAPEND(2),LINSIZ,MAPNAM,MNMLNG,MTYPE,MSTAT
        	4,IERR1,IERR2,IKIND,OLUN,MAPMRK(3,2),MPRINT,BARCHR
        C
        C
0003    	DIMENSION NOW(2)
0004    	INTEGER OLUN,CLUN,CSTAT,CNMLNG,CTYPE,CPRNT
0005    	BYTE INPNAM(1),COMNAM(1),COMCHR(16),BUFR(7680,2),MAPNAM(12)
0006    	BYTE COMREC(1),RECORD(1),OUTMOD,BARCHR
         
0007    101	FORMAT(' ',<MIN0(LINSIZ,72)>('*'),/' ',<MIN0(LINSIZ,72)/2-14>
        	1('*'),' MERGED LIST OF DIFFERENCES ',<MIN0(LINSIZ,72)/2-14>
        	2('*'),/' ',<MIN0(LINSIZ,72)>('*')//)
0008    111	FORMAT(' FILE ',<INMLNG>A1)
0009    112	FORMAT(' FILE ',<CNMLNG>A1)
0010    113	FORMAT(' ',<29>('*'))
0011    114	FORMAT(' ',<54>('*')/' ',<54>('*'))
0012    121	FORMAT(' ',15X,'NONE')
0013    131	FORMAT('1')
        C
        C	OPEN MAP FILE.
        C
0014    	CALL OPNFIL(MAPNAM,MNMLNG,MLUN,RECORD,IRECSZ,MTYPE,IERR2)
0015    	IF (IERR2 .LE. 0) GOTO 924
        C
        C	OPEN COMPARE AND INPUT FILES.
        C
0016    400	CALL OPNFIL(INPNAM,INMLNG,ILUN,RECORD,IRECSZ,ITYPE,IERR2)
0017    	IF (IERR2 .LE. 0) GOTO 912
0018    	CALL OPNFIL(COMNAM,CNMLNG,CLUN,COMREC,IRECSZ,CTYPE,IERR2)
0019    	IF (IERR2 .LE. 0) GOTO 920
        C
        C	INITIALIZE RECORD NUMBER IN EACH FILE (NOW) AND THE NUMBER
        C	OF RECORDS PRINTED (CPRNT,IPRNT).
        C
0020    	NOW(1)=1
0021    	NOW(2)=1
0022    	CPRNT=0
0023    	IPRNT=0
0024    	IOUT=0
        C
        C	CONDITIONALLY OUTPUT HEADER.
        C
0025    	IF (INOUT .GT. 1) WRITE(OLUN,131,ERR=918)
0026    	WRITE(OLUN,101,ERR=918)
        C
        C	FILL BUFFERS WITH RECORDS.
        C
0027    	CALL FILBUF(ILUN,RECORD,LENINP,ISTAT,INPNAM,INMLNG,ITYPE)
FORTRAN IV-PLUS V02-51D		02:43:06    22-AUG-78		PAGE 4   
MRGDIF.FTN   	/TR:BLOCKS/WR

0028    	IF (IERR2 .EQ. 0) GOTO 350
0029    	IF (IERR1 .LT. 0) GOTO 1000
0030    350	IERR1=1
0031    	CALL FILBUF(CLUN,COMREC,LENCOM,CSTAT,COMNAM,CNMLNG,CTYPE)
0032    	IF (IERR2 .EQ. 0) GOTO 360
0033    	IF (IERR1 .LT. 0) GOTO 1000
        C
        C	MARK MAP FILE FOR NEXT READ FOR MAPPING VALUES FOR INPUT-FILE
        C	AND COMPARE-FILE.
        C
0034    360	IERR1=1
0035    	CALL MRKREC(MLUN,MAPMRK(1,1),IERR2)
0036    	IF (IERR2 .LE. 0) GOTO 927
0037    	CALL MRKREC(MLUN,MAPMRK(1,2),IERR2)
0038    	IF (IERR2 .LE. 0) GOTO 927
0039    	ISUB=1
        C
        C	READ A MAPPING TABLE'S WORTH OF INPUT-FILE MAP VALUES AND 
        C	THEN COMPARE-FILE MAP VALUES.
        C
0040    410	CALL READMP(ISUB)
0041    	IF (IERR2 .EQ. 0) GOTO 470
0042    	IF (IERR1 .LT. 0) GOTO 1000
0043    	ISUB=2
0044    	CALL READMP(ISUB)
0045    	IF (IERR2 .EQ. 0) GOTO 460
0046    	IF (IERR1 .LT. 0) GOTO 1000
        C
        C	TEST IF A DIFFERENCE OCCURRED.
        C
0047    415	IF ((MAP(NOW(1)-MAPTOP(1)+1,1) .EQ. 0) .OR.
        	1(MAP(NOW(2)-MAPTOP(2)+1,2) .EQ. 0)) GOTO 430
0048    	IF (MAP(NOW(1)-MAPTOP(1)+1,1) .GE. 0) GOTO 416
0049    	NOW(1)=NOW(1)+1
0050    	IF (MAP(NOW(2)-MAPTOP(2)+1,2) .GE. 0) GOTO 419
0051    	GOTO 418
0052    416	IF (MAP(NOW(2)-MAPTOP(2)+1,2) .GE. 0) GOTO 417
0053    	GOTO 418
        C
        C	NO DIFFERENCE.  INCREMENT RECORD NUMBERS.
        C
0054    417	NOW(1)=NOW(1)+1
0055    418	NOW(2)=NOW(2)+1
        C
        C	CHECK IF BOTH RECORDS ARE MAPPED.  IF NOT CONTAINED IN THIS TABLE,
        C	READ ANOTHER TABLE'S WORTH OF VALUES.
        C
0056    419	IF (NOW(1) .LE. MAPEND(1)) GOTO 420
0057    	ISUB=1
0058    	CALL READMP(ISUB)
0059    	IF (IERR2 .EQ. 0) GOTO 470
0060    	IF (IERR1 .LT. 0) GOTO 1000
0061    420	IF (NOW(2) .LE. MAPEND(2)) GOTO 415
0062    	ISUB=2
0063    	CALL READMP(ISUB)
0064    	IF (IERR2 .EQ. 0) GOTO 460
FORTRAN IV-PLUS V02-51D		02:43:06    22-AUG-78		PAGE 5   
MRGDIF.FTN   	/TR:BLOCKS/WR

0065    	IF (IERR1 .LT. 0) GOTO 1000
0066    	GOTO 415
        C
        C	A DIFFERENCE OCCURRED.  WRITE OUT HEADER.
        C
0067    430	WRITE(OLUN,111,ERR=918) (INPNAM(I),I=1,INMLNG)
0068    	ISUB=1
        C
        C	WRITE OUT RECORDS UNTIL MPRINT MATCHED/IGNORED RECORDS ARE
        C	OUTPUT.  IF MPRINT MATCHED/IGNORED RECORDS ARE NOT
        C	BETWEEN TWO MISMATCHES, THEN WRITE OUT HOWEVER MANY THERE ARE.
        C
0069    435	LSTMAT(1)=NOW(1)-1
0070    	CALL DIFOUT(ILUN,RECORD,LENINP,ISTAT,INPNAM,INMLNG,ITYPE,NOW(1)
        	1,OUTMOD)
0071    	IOUT=IOUT+1
0072    	IF (NOW(1) .EQ. 0) GOTO 470
0073    	IF (IERR1 .LT. 0) GOTO 1000
0074    437	IF (MAP(NOW(1)-MAPTOP(1)+1,1) .GT. 0) IPRNT=IPRNT+1
0075    	IF (IPRNT .GE. MPRINT) GOTO 440
0076    	IF (NOW(1) .LE. MAPEND(1)) GOTO 438
0077    	CALL READMP(ISUB)
0078    	IF (IERR2 .EQ. 0) GOTO 470
0079    	IF (IERR1 .LT. 0) GOTO 1000
0080    438	NOW(1)=NOW(1)+1
0081    	GOTO 435
        C
        C	NOW WRITE OUT CORRESPONDING COMPARE-FILE HEADER AND RECORDS.
        C
0082    440	NOW(1)=NOW(1)+1
0083    	WRITE(OLUN,113,ERR=918)
0084    	WRITE(OLUN,112,ERR=918)(COMNAM(I),I=1,CNMLNG)
0085    	ISUB=2
0086    445	LSTMAT(2)=NOW(2)-1
0087    	CALL DIFOUT(CLUN,RECORD,LENINP,CSTAT,COMNAM,CNMLNG,CTYPE,NOW(2)
        	1,OUTMOD)
0088    	IOUT=IOUT+1
0089    	IF (NOW(2) .EQ. 0) GOTO 460
0090    	IF (IERR1 .LT. 0) GOTO 1000
0091    447	IF (MAP(NOW(2)-MAPTOP(2)+1,2) .GT. 0) CPRNT=CPRNT+1
0092    	IF (CPRNT .GE. MPRINT) GOTO 450
0093    	NOW(2)=NOW(2)+1
0094    	IF (NOW(2) .LE. MAPEND(2)) GOTO 445
0095    	CALL READMP(ISUB)
0096    	IF (IERR2 .EQ. 0) GOTO 460
0097    	IF (IERR1 .LT. 0) GOTO 1000
0098    	GOTO 445
        C
        C	DONE WRITING COMPARE FILE MISMATCH RECORDS.
        C
0099    450	NOW(2)=NOW(2)+1
0100    	WRITE(OLUN,114,ERR=918)
0101    	IPRNT=0
0102    	CPRNT=0
0103    	GOTO 415
        C
FORTRAN IV-PLUS V02-51D		02:43:06    22-AUG-78		PAGE 6   
MRGDIF.FTN   	/TR:BLOCKS/WR

        C	NO MORE COMPARE FILE.  WRITE OUT REST OF MAPPED INPUT FILE.
        C
0104    460	IERR1=1
0105    	ISUB=1
0106    	IF (NOW(1) .LE. MAPEND(1)) GOTO 462
0107    	CALL READMP(ISUB)
0108    	IF (IERR2 .EQ. 0) GOTO 480
0109    	IF (IERR1 .LT. 0) GOTO 1000
0110    462	WRITE(OLUN,111,ERR=918)(INPNAM(I),I=1,INMLNG)
0111    463	LSTMAT(1)=NOW(1)-1
0112    	CALL DIFOUT(ILUN,RECORD,LENINP,ISTAT,INPNAM,INMLNG,ITYPE,NOW(1)
        	1,OUTMOD)
0113    	IOUT=IOUT+1
0114    	IF (NOW(1) .EQ. 0) GOTO 480
0115    	IF (IERR1 .LT. 0) GOTO 1000
0116    465	NOW(1)=NOW(1)+1
0117    	IF (NOW(1) .LE. MAPEND(1)) GOTO 463
0118    	CALL READMP(ISUB)
0119    	IF (IERR2 .EQ. 0) GOTO 480
0120    	IF (IERR1 .LT. 0) GOTO 1000
0121    	GOTO 463
        C
        C	NO MORE INPUT FILE.  WRITE OUT REST OF MAPPED COMPARE FILE.
        C
0122    470	IERR1=1
0123    	ISUB=2
0124    	IF (NOW(2) .LE. MAPEND(2)) GOTO 472
0125    	CALL READMP(ISUB)
0126    	IF (IERR2 .EQ. 0) GOTO 480
0127    	IF (IERR1 .LT. 0) GOTO 1000
0128    472	WRITE(OLUN,113,ERR=918)
0129    	WRITE(OLUN,112,ERR=918)(COMNAM(I),I=1,CNMLNG)
0130    473	LSTMAT(2)=NOW(2)-1
0131    	CALL DIFOUT(CLUN,COMREC,LENCOM,CSTAT,COMNAM,CNMLNG,CTYPE,NOW(2)
        	1,OUTMOD)
0132    	IOUT=IOUT+1
0133    	IF (NOW(2) .EQ. 0) GOTO 480
0134    	IF (IERR1 .LT. 0) GOTO 1000
0135    475	NOW(2)=NOW(2)+1
0136    	IF (NOW(2) .LE. MAPEND(2))  GOTO 473
0137    	CALL READMP(ISUB)
0138    	IF (IERR2 .EQ. 0) GOTO 480
0139    	IF (IERR1 .LT. 0) GOTO 1000
0140    	GOTO 473
0141    480	IF (IOUT .EQ. 0) WRITE(OLUN,121,ERR=918)
0142    	IERR1=1
0143    	CALL CLSFIL(ILUN,ISTAT,IERR2)
0144    	IF (IERR2 .LE. 0) GOTO 913
0145    490	CALL CLSFIL(CLUN,CSTAT,IERR2)
0146    	IF (IERR2 .LE. 0) GOTO 921
0147    	GOTO 1000
        C
        C	ERROR CODES
        C
        C	INPUT FILE:
        C		-12 = OPEN ERROR
FORTRAN IV-PLUS V02-51D		02:43:06    22-AUG-78		PAGE 7   
MRGDIF.FTN   	/TR:BLOCKS/WR

        C		-13 = CLOSE ERROR
        C
        C	OUTPUT FILE:
        C
        C		-18 = READ/WRITE ERROR
        C
        C	COMPARE FILE:
        C
        C		-20 = OPEN ERROR
        C		-21 = CLOSE ERROR
        C
        C	MAP FILE:
        C
        C		-24 = OPEN ERROR
        C		-25 = CLOSE ERROR
        C		-27 = MARK/POINT ERROR
        C
0148    912	IERR1=-12
0149    	GOTO 1000
0150    913	IERR1=-13
0151    	GOTO 1000
0152    918	IERR1=-18
0153    	GOTO 1000
0154    920	IERR1=-20
0155    	GOTO 1000
0156    921	IERR1=-21
0157    	GOTO 1000
0158    924	IERR1=-24
0159    	GOTO 1010
0160    925	IERR1=-25
0161    	GOTO 1010
0162    927	IERR1=-27
0163    1000	CALL CLSFIL(MLUN,MSTAT,IERR2)
0164    	IF (IERR2 .LE. 0) GOTO 925
0165    1010	RETURN
0166    	END
