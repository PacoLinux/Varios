    123	000160	001107 				BNE	90$		; YES - FORCE NEXT DIRECTORY
    124	000162	005767 	000000C			TST	TDRFDB+F.FNB+N.DID ; SEE IF SCRATCH DIRECTORY IS IN USE
    125	000166	001007 				BNE	FNDNX1		; BRANCH IF YES
    126	000170					CALL	.FIND
