                                     0000   100 	.SBTTL	FOR$TIME_T_DS - return system time to FORTRAN string
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Use system service ASCTIM to move system time directly to the
                                     0000   600 ;	caller.  Pad the rest of the string with blanks if necessary.
                                     0000   700 ;
                                     0000   800 ; CALLING SEQUENCE:
                                     0000   900 ;	CALL	FOR$TIME_T_DS (time_string.wt.ds)
                                     0000  1000 ;
                                     0000  1100 ;
                                     0000  1200 ; INPUT PARAMETERS:
                                     0000  1300 ;	time_string is the address of a fixed-len string.
                                     0000  1400 ;
                                     0000  1500 ; IMPLICIT INPUTS:
                                     0000  1600 ;	NONE
                                     0000  1700 ;
                                     0000  1800 ; OUTPUT PARAMETERS:
                                     0000  1900 ;	NONE
                                     0000  2000 ;
                                     0000  2100 ; IMPLICIT OUTPUTS:
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; COMPLETION CODES:
                                     0000  2500 ;	NONE
                                     0000  2600 ;
                                     0000  2700 ; SIDE EFFECTS:
                                     0000  2800 ;	NONE
                                     0000  2900 ;
                                     0000  3000 ;--
                                     0000  3100 
                                     0000  3200 
                                     0000  3300 
                               003C' 0000  3400 	.ENTRY	FOR$TIME_T_DS,	^M<R2,R3,R4,R5>
            53         04 AC     D0  0002  3600 	MOVL	4(AP), R3		; R3 -> string desc. of users string
                                     0006  3700 	$ASCTIM_S	,(R3),,#1	; put ASCII info in users string
            08            63     B1  0015  3800 	CMPW	(R3), #8		; if users string size is 8 bytes
                                     0018  3900 					; or less, then no padding
                          0F     15  0018  4000 	BLEQ	1$			; is necessary
         04 A3            08     C1  001A  4100 	ADDL3	#8, 4(R3), R0		; R0 -> where to pad with blanks
                          50         001E       
            63            08     A3  001F  4200 	SUBW3	#8, (R3), R1		; R1 = # bytes to blank pad
                          51         0022       
            60            00     2C  0023  4300 	MOVC5	#0, (R0), #^A/ /, R1, (R0)
            51            20         0026       
                          60         0028       
                                     0029  4400 					; pad rest of users string
                                 04  0029  4500 1$:	RET				; back to caller
                                     002A  4600 
                                     002A  4700 
                                     002A  4800 	.END
