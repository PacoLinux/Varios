                                     0000   100 	.SBTTL	FOR$DATE_T_DS - return system date to FORTRAN string
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Use system service ASCTIM to move system date to a local (stack)
                                     0000   600 ;	buffer.  Move the last 2 digits of date on top of 1st 2 -
                                     0000   700 ;	"1977" -> "7777".  Return this date to user string.
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;	CALL	FOR$DATE_T_DS (date_string.wt.ds)
                                     0000  1100 ;
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;	date_string is the address of a fixed-len string.
                                     0000  1500 ;
                                     0000  1600 ; IMPLICIT INPUTS:
                                     0000  1700 ;	NONE
                                     0000  1800 ;
                                     0000  1900 ; OUTPUT PARAMETERS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; IMPLICIT OUTPUTS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; COMPLETION CODES:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; SIDE EFFECTS:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ;--
                                     0000  3200 
                                     0000  3300 
                                     0000  3400 
                               003C' 0000  3500 	.ENTRY	FOR$DATE_T_DS,	^M<R2,R3,R4,R5>
            5E            0C     C2  0002  3700 	SUBL	#12, SP			; make room for date string
            52            5E     D0  0005  3800 	MOVL	SP, R2			; R2 -> room
            7E            52     D0  0008  3900 	MOVL	R2, -(SP)		; build a string descriptor for room
            7E   010E000B 8F     D0  000B  4000 	MOVL	#^X10E000B, -(SP)	; len = 11
            50            5E     D0  0012  4100 	MOVL	SP, R0			; R0 -> src descriptor
                                     0015  4200 	$ASCTIM_S	, (R0)		; R2 -> date string
         07 A2         09 A2     B0  0024  4300 	MOVW	9(R2), 7(R2)		; mung up year
            50         04 BC     7D  0029  4400 	MOVQ	@4(AP), R0		; R0/R1 = string descriptor
            62            09     2C  002D  4500 	MOVC5	#9, (R2), #^A/ /, R0, (R1)
            50            20         0030       
                          61         0032       
                                     0033  4600 					; move into dest buffer
                                 04  0033  4700 	RET
                                     0034  4800 
                                     0034  4900 
                                     0034  5000 	.END
