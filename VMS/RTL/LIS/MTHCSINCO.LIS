0-3             MTH$CSIN - COMPLEX SINE                                                                                          (4)

                                     0000    79 	.SBTTL	MTH$CSIN - COMPLEX SINE
                                     0000    80 
                                     0000    81 ;++
                                     0000    82 ; FUNCTIONAL DESCRIPTION:
                                     0000    83 ;
                                     0000    84 ;	MTH$CSIN computes the SINE of a COMPLEX number (r, i) as
                                     0000    85 ;
                                     0000    86 ;	result = (SIN(r) * COSH(i), COS(r) * SINH(i))
                                     0000    87 ;
                                     0000    88 ; CALLING SEQUENCE:
                                     0000    89 ;	Sine.wfc.v	= MTH$CSIN(arg.rfc.r)
                                     0000    90 ;
                                     0000    91 ;
                                     0000    92 ; INPUT PARAMETERS:
                                     0000    93 ;	The one input parameter is the address of a COMPLEX number (r, i),
                                     0000    94 ;	where r and i are both single-precision floating point values.
                                     0000    95 ;
                                     0000    96 ; IMPLICIT INPUTS:
                                     0000    97 ;	NONE
                                     0000    98 ;
                                     0000    99 ; OUTPUT PARAMETERS:
                                     0000   100 ;	NONE
                                     0000   101 ;
                                     0000   102 ; IMPLICIT OUTPUTS:
                                     0000   103 ;	NONE
                                     0000   104 ;
                                     0000   105 ; COMPLETION CODES:
                                     0000   106 ;	NONE
                                     0000   107 ;
                                     0000   108 ; SIDE EFFECTS:
                                     0000   109 ;	Signals:	Reserved Operand if r or i are invalid (-0.0)
                                     0000   110 ;			MTH$_SINSIGLOS if |r| > 2*PI*2**31.
                                     0000   111 ;			Floating Overflow if i > 88.028.
                                     0000   112 ;
                                     0000   113 ;--
                                     0000   114 
                                     0000   115 
                               00FC' 0000   116 	.ENTRY	MTH$CSIN,	^M<R2,R3,R4,R5,R6,R7>
                 00000025'EF     16  0002   117 	JSB	WORKER			; R0 = SIN(r)
                                     0008   118 					; R1 = COS(r)
                                     0008   119 					; R2 = SINH(i)
                                     0008   120 					; R3 = COSH(i)
            50            53     44  0008   121 	MULF	R3, R0			; R0 = SIN(r) * COSH(i)
            51            52     44  000B   122 	MULF	R2, R1			; R1 = COS(r) * SINH(i)
                                 04  000E   123 	RET
MTH$CSINCOS     COMPLEX SINE AND COSINE                          21-AUG-1978 19:03:01   VAX-11 MACRO X0.3-11               Page   5
0-3             MTH$CCOS - COMPLEX COSINE                                                                                        (5)

                                     000F   125 	.SBTTL	MTH$CCOS - COMPLEX COSINE
                                     000F   126 
                                     000F   127 ;++
                                     000F   128 ; FUNCTIONAL DESCRIPTION:
                                     000F   129 ;
                                     000F   130 ;	MTH$CCOS computes the COSINE of COMPLEX number (r, i) as follows:
                                     000F   131 ;
                                     000F   132 ;	result = (COS(r) * COSH(i), -SIN(r) * SINH(-i))
                                     000F   133 ;
                                     000F   134 ; CALLING SEQUENCE:
                                     000F   135 ;	Cosine.wfc.v	= MTH$CCOS (arg.rfc.r)
                                     000F   136 ;
                                     000F   137 ;
                                     000F   138 ; INPUT PARAMETERS:
                                     000F   139 ;	The one input parameter is the address of a COMPLEX number (r, i),
                                     000F   140 ;	where r and i are both single-precision floating point values.
                                     000F   141 ;
                                     000F   142 ; IMPLICIT INPUTS:
                                     000F   143 ;	NONE
                                     000F   144 ;
                                     000F   145 ; OUTPUT PARAMETERS:
                                     000F   146 ;	NONE
                                     000F   147 ;
                                     000F   148 ; IMPLICIT OUTPUTS:
                                     000F   149 ;	NONE
                                     000F   150 ;
                                     000F   151 ; COMPLETION CODES:
                                     000F   152 ;	NONE
                                     000F   153 ;
                                     000F   154 ; SIDE EFFECTS:
                                     000F   155 ;	Signals:	Reserved Operand if r or i are invalid (-0.0)
                                     000F   156 ;			MTH$_SINSIGLOS if |r| > 2*PI*2**31.
                                     000F   157 ;			Floating Overflow if i > 88.028.
                                     000F   158 ;
                                     000F   159 ;--
                                     000F   160 
                                     000F   161 
                               00FC' 000F   162 	.ENTRY	MTH$CCOS,	^M<R2,R3,R4,R5,R6,R7>
                 00000025'EF     16  0011   163 	JSB	WORKER			; R0 = SIN(r)
                                     0017   164 					; R1 = COS(r)
                                     0017   165 					; R2 = SINH(i)
                                     0017   166 					; R3 = COSH(i)
            53            51     44  0017   167 	MULF	R1, R3			; R3 = COS(r) * COSH(i)
            50            50     52  001A   168 	MNEGF	R0, R0			; R0 = -SIN(r)
            52            50     45  001D   169 	MULF3	R0, R2, R1		; R1 = -SIN(r) * SINH(i)
                          51         0020       
            50            53     D0  0021   170 	MOVL	R3, R0			; R0 = COS(r) * COSH(i)
                                 04  0024   171 	RET
