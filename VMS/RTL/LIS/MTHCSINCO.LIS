0-3             WORKER - do all the work                                                                                         (6)

                                     0025   173 	.SBTTL	WORKER - do all the work
                                     0025   174 
                                     0025   175 ;+
                                     0025   176 ; Setup error handler
                                     0025   177 ; Compute:
                                     0025   178 ;	R0 = SIN(r)
                                     0025   179 ;	R1 = COS(r)
                                     0025   180 ;	R2 = SINH(i)
                                     0025   181 ;	R3 = COSH(i)
                                     0025   182 ;-
                                     0025   183 
                                     0025   184 WORKER:
                                     0025   185 	MTH$FLAG_JACKET			; set up error handler
                                     0025      
            6D       0000'CF     3E  0025       	MOVAW	W^MTH$$JACKET_HND, (FP)
                                     002A       					; set handler address to jacket
                                     002A       					; handler
                                     002A       
            50         04 AC     D0  002A   186 	MOVL	argadr(AP), R0		; R0 -> (r, i)
            50         04 A0     50  002E   187 	MOVF	4(R0), R0		; R0 = i
                 00000000'EF     16  0032   188 	JSB	MTH$EXP_R4		; R0 = EXP(i)
            08            50     47  0038   189 	DIVF3	R0, #1.0, R1		; R1 = EXP(-i)
                          51         003B       
                                     003C   190 
            50            51     43  003C   191 	SUBF3	R1, R0, R5		; R5 = EXP(i) - EXP(-i)
                          55         003F       
            55            00     44  0040   192 	MULF	#0.5, R5		; R5 = (EXP(i) - EXP(-i))/2
                                     0043   193 
            50            51     41  0043   194 	ADDF3	R1, R0, R6		; R6 = EXP(i) + EXP(-i)
                          56         0046       
            56            00     44  0047   195 	MULF	#0.5, R6		; R6 = (EXP(i) + EXP(-i))/2
                                     004A   196 
            50         04 BC     50  004A   197 	MOVF	@argadr(AP), R0		; R0 = r
                 00000000'EF     16  004E   198 	JSB	MTH$COS_R4		; R0 = COS(r)
            57            50     D0  0054   199 	MOVL	R0, R7			; R7 = COS(r)
                                     0057   200 
            50         04 BC     50  0057   201 	MOVF	@argadr(AP), R0		; R0 = r
                 00000000'EF     16  005B   202 	JSB	MTH$SIN_R4		; R0 = SIN(r)
                                     0061   203 
            51            57     D0  0061   204 	MOVL	R7, R1			; R1 = COS(r)
            52            55     D0  0064   205 	MOVL	R5, R2			; R2 = SINH(i)
            53            56     D0  0067   206 	MOVL	R6, R3			; R3 = COSH(i)
                                     006A   207 
                                 05  006A   208 	RSB
                                     006B   209 
                                     006B   210 
                                     006B   211 
                                     006B   212 	.END
