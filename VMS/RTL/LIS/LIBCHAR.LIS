                                     0000    77 	.SBTTL	LIB$CHAR - Return INTEGER*1 as a one byte string
                                     0000    78 
                                     0000    79 ;++
                                     0000    80 ; FUNCTIONAL DESCRIPTION:
                                     0000    81 ;
                                     0000    82 ;	Return unsigned INTEGER*1 as a one byte 8-bit ASCII string with
                                     0000    83 ;	trailing spaces.  Range of input byte is 0 through 255.
                                     0000    84 ;	Since output string is first argument, this can be called as either
                                     0000    85 ;	a subroutine of two arguments, or a string function of one argument.
                                     0000    86 ;
                                     0000    87 ; CALLING SEQUENCE:
                                     0000    88 ;
                                     0000    89 ;	CALL LIB$CHAR (one_char_str.wt.dx, byte_integer.rbu.r)
                                     0000    90 
                           00000004  0000    91 	one_char_str = 4		; Adr. of string descriptor
                                     0000    92 					; to receive one character result.
                                     0000    93 					; If descriptor is longer than 1 char., add traililng blanks.
                           00000008  0000    94 	byte_integer = 8		; Adr. of unsigned byte integer
                                     0000    95 					; to be converted to ASCII string.
                                     0000    96 
                                     0000    97 ;
                                     0000    98 ; INPUT PARAMETERS:
                                     0000    99 ;	NONE
                                     0000   100 ;
                                     0000   101 ; IMPLICIT INPUTS:
                                     0000   102 ;	NONE
                                     0000   103 ;
                                     0000   104 ; OUTPUT PARAMETERS:
                                     0000   105 ;	NONE
                                     0000   106 ;
                                     0000   107 ; IMPLICIT OUTPUTS:
                                     0000   108 ;	NONE
                                     0000   109 ;
                                     0000   110 ; COMPLETION CODES:
                                     0000   111 ;	NONE
                                     0000   112 ;
                                     0000   113 ; SIDE EFFECTS:
                                     0000   114 ;	NONE
                                     0000   115 ;
                                     0000   116 ;--
                                     0000   117 
                                     0000   118 
                                     0000   119 
                               003C' 0000   120 	.ENTRY	LIB$CHAR, ^M<R2,R3,R4,R5> ; entry point and entry mask
                                     0002   121 
            50         04 BC     7D  0002   122 	MOVQ	@one_char_str(AP), R0	; R0/R1 = len/adr. of result string
                          01     2C  0006   123 	MOVC5	#1, @byte_integer(AP),-	; move one-char string
            20         08 BC         0008   124 		#^A/ /, R0, (R1)	; pad with trailing blanks
            61            50         000B       
                                 04  000D   125 	RET				; return from LIB$CHAR routine
                                     000E   126 
                                     000E   127 	.END				; End of module LIB$ICHAR
