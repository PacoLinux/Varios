                                     0000   300 	.SBTTL	MTH$AINT	Real to Real truncation
                                     0000   400 
                                     0000   500 ;++
                                     0000   600 ; FUNCTIONAL DESCRIPTION:
                                     0000   700 ;	Return the arguments with zeroes to the right of the decimal
                                     0000   800 ;	point.
                                     0000   900 ;
                                     0000  1000 ;
                                     0000  1100 ; CALLING SEQUENCE:
                                     0000  1200 ;	Truncation.wf.v = MTH$AINT (arg.rf.r)
                                     0000  1300 ;	
                                     0000  1400 ;
                                     0000  1500 ;
                                     0000  1600 ; INPUT PARAMETERS:
                                     0000  1700 ;	The one argument is a single-precision floating-point value
                                     0000  1800 ;	and is call-by-reference.
                                     0000  1900 ;	
                                     0000  2000 ;
                                     0000  2100 ; IMPLICIT INPUTS:
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; OUTPUT PARAMETERS:
                                     0000  2500 ;	NONE
                                     0000  2600 ;
                                     0000  2700 ; IMPLICIT OUTPUTS:
                                     0000  2800 ;	NONE
                                     0000  2900 ;
                                     0000  3000 ; COMPLETION CODES:
                                     0000  3100 ;	NONE
                                     0000  3200 ;
                                     0000  3300 ; SIDE EFFECTS:
                                     0000  3400 ;	Reserved Operand and Floating Underflow exceptions can occur.
                                     0000  3500 ;	
                                     0000  3600 ;
                                     0000  3700 ;--
                                     0000  3800 
                                     0000  3900 
                               0000' 0000  4000 	.ENTRY	MTH$AINT,	^M<>
            50         04 BC     50  0002  4200 	MOVF	@4(AP), R0		; R0 = arg
            00            50     54  0006  4300 	EMODF	R0, #0, #1, R1, R1	; R1 = fraction_part(R0)
            51            08         0009       
                          51         000B       
            50            51     42  000C  4400 	SUBF	R1, R0			; R0 = integer_part(R0)
                                 04  000F  4500 	RET
                                     0010  4600 
                                     0010  4700 
                                     0010  4800 	.END
