00-01           LIB$INSV  - insert a field                                                                                       (3)

                                     0000    64 	.SBTTL	LIB$INSV  - insert a field 
                                     0000    65 ;++
                                     0000    66 ; FUNCTIONAL DESCRIPTION:
                                     0000    67 ;
                                     0000    68 ;	The field specified by the base, position, and size is replaced by bits
                                     0000    69 ;	size-1 : 0 of the source.  If the size is zero, no insertion is performed.
                                     0000    70 ;	A reserved operand fault occurs if a size greater than 32 is specified.
                                     0000    71 ;
                                     0000    72 ; CALLING SEQUENCE:
                                     0000    73 ;
                                     0000    74 ;	CALL LIB$INSV (src.rl.r, pos.rl.r, size.rbu.r, base.wv.r)
                                     0000    75 ;
                           00000004  0000    76 	SOURCE = 4				; Adr. of source
                           00000008  0000    77 	POSITION = 8				; Adr. of beginning position
                           0000000C  0000    78 	SIZE = 12				; Adr. of size to insert
                           00000010  0000    79 	BASE = 16				; Adr. of base to insert into
                                     0000    80 ; INPUT PARAMETERS:
                                     0000    81 ;
                                     0000    82 ;	NONE
                                     0000    83 ;
                                     0000    84 ; IMPLICIT INPUTS:
                                     0000    85 ;
                                     0000    86 ;	NONE
                                     0000    87 ;
                                     0000    88 ; OUTPUT PARAMETERS:
                                     0000    89 ;
                                     0000    90 ;	NONE
                                     0000    91 ;
                                     0000    92 ; IMPLICIT OUTPUTS:
                                     0000    93 ;
                                     0000    94 ;	NONE
                                     0000    95 ;
                                     0000    96 ; COMPLETION CODES:
                                     0000    97 ;
                                     0000    98 ;	NONE
                                     0000    99 ;
                                     0000   100 ; SIDE EFFECTS:
                                     0000   101 ;
                                     0000   102 ;	SS$_ROPRAND - a reserved operand fault occurs if:
                                     0000   103 ;		    1.) a size greater than 32 is specified
                                     0000   104 ;		    2.) a position greater is greater than 31 and the field is in
                                     0000   105 ;			registers
                                     0000   106 ;
                                     0000   107 ;--
                                     0000   108 
                               0000' 0000   109 	.ENTRY LIB$INSV , ^M< >		; Entry point
                                     0002   110 
         08 BC         04 BC     F0  0002   111 	INSV	@SOURCE(AP), @POSITION(AP), -
         10 BC         0C BC         0007       
                                     000B   112 		@SIZE(AP), @BASE(AP)		; insert source field
                                 04  000B   113 	RET
                                     000C   114 	.END
