                                     0000   100 	.SBTTL	MTH$DNINT - return nearest integer as REAL*8
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Returns the rounded (away from zero) argument.
                                     0000   600 ;
                                     0000   700 ;
                                     0000   800 ; CALLING SEQUENCE:
                                     0000   900 ;	NONE
                                     0000  1000 ;	Nearest_integer.wd.v = MTH$DNINT (arg.rd.r)
                                     0000  1100 ;
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;	The input parameter is a double-precision floating-point value
                                     0000  1500 ;	and is call-by-reference.
                                     0000  1600 ;	
                                     0000  1700 ;
                                     0000  1800 ; IMPLICIT INPUTS:
                                     0000  1900 ;	NONE
                                     0000  2000 ;
                                     0000  2100 ; OUTPUT PARAMETERS:
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; IMPLICIT OUTPUTS:
                                     0000  2500 ;	NONE
                                     0000  2600 ;
                                     0000  2700 ; COMPLETION CODES:
                                     0000  2800 ;	NONE
                                     0000  2900 ;
                                     0000  3000 ; SIDE EFFECTS:
                                     0000  3100 ;	Reserved Operand and Floating Overflow exceptions can occur.
                                     0000  3200 ;	
                                     0000  3300 ;
                                     0000  3400 ;--
                                     0000  3500 
                                     0000  3600 
                                     0000  3700 
                               0000' 0000  3800 	.ENTRY	MTH$DNINT,	^M<>
         04 BC            00     61  0002  4000 	ADDD3	#0.5, @4(AP), R0	; R0/R1 = arg + 0.5
                          50         0006       
                          03     14  0007  4100 	BGTR	1$			; branch if positive
            50            08     62  0009  4200 	SUBD	#1.0, R0		; R0/R1 = arg - 0.5
            00            50     74  000C  4300 1$:	EMODD	R0, #0, #1, R0, R0	; R0/R1 = fraction_part(R0/R1)
            50            08         000F       
                          50         0011       
         04 BC            50     63  0012  4400 	SUBD3	R0, @4(AP), R0		; R0/R1 = integer_part(R0)
                          50         0016       
                                 04  0017  4500 	RET
                                     0018  4600 
                                     0018  4700 
                                     0018  4800 	.END
