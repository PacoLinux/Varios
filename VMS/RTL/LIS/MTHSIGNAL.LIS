0-7             MTH$$SIGNAL  ; Signal any math error                                                                             (6)

                               000C' 0008  5400 	.ENTRY	MTH$$SIGNAL, ^M<R2,R3>	; save R2:R3
                                     000A  5500 					; but do not use R0:R1, (passed as
                                     000A  5600 					; implicit input to LIB$SIGNAL) and
                                     000A  5700 					; DO NOT RESTORE R0/R1, since they
                                     000A  5800 					; may have been modified by error
                                     000A  5900 					; handling routine.
                                     000A  6000 
                                     000A  6400 ;+
                                     000A  6500 ; Check number of arguments to see if math routine is called by CALL or JSB
                                     000A  6600 ;-
                                     000A  6700 
            01            6C     91  000A  6800 	CMPB	(AP), #1		; if called with only one arg
                          0E     13  000D  6900 	BEQLU	5$			; assume mth routine called with CALL
                                     000F  7000 					; (rather than JSB)
                                     000F  7100 
                                     000F  7200 ;+
                                     000F  7300 ; If math routine is called by JSB then check if its caller is a math
                                     000F  7400 ; routine or the user.
                                     000F  7500 ;-
                                     000F  7600 
            52         08 AC     D0  000F  7700 	MOVL	JSB_PC(AP), R2		; R2 = JSB PC
            53         EA AF     DE  0013  7800 	MOVAL	B^MTH$$JACKET_HND, R3	; R3 = absolute adr. of handler
                                     0017  7900 					; setup by each CALL MTH$
                                     0017  8000 					; (as opposed to JSB MTH$)
            53         0C BD     D1  0017  8100 	CMPL	@SF$L_SAVE_FP(FP), R3	; compare prev. frame handler
                          0B     12  001B  8200 	BNEQ	10$			; branch if not jacket handler
                                     001D  8300 
                                     001D  8400 ;+
                                     001D  8500 ; Previous frame had established MTH$$JACKET_HND as a handler
                                     001D  8600 ; so it must have been math routine called with CALL (not JSB)
                                     001D  8700 ; Get user PC stored by CALL instruction
                                     001D  8800 ;-
                                     001D  8900 
            52         0C AD     D0  001D  9000 5$:	MOVL	SF$L_SAVE_FP(FP), R2	; R2 = previous frame adr.
                                     0021  9100 					; i.e., math routine frame
            52         10 A2     D0  0021  9200 	MOVL	SF$L_SAVE_PC(R2), R2	; R2 = saved user's PC
                          24     10  0025  9210 	BSBB	DO_SIGNAL		; signal error code 4(AP) and
                                     0027  9220 					; user PC (R2)
                                 04  0027  9230 	RET				; return to caller (math routine)
                                     0028  9300 
                                     0028  9400 ;+
                                     0028  9500 ; User called math routine with JSB - create extra frame
                                     0028  9600 ; so that stack looks just like user had called math routine with CALL.
                                     0028  9700 ; Then traceback and stack depth (handler argument) will be same
                                     0028  9800 ; for JSB and CALL.
                                     0028  9900 ;-
                                     0028 10000 
            53         10 AD     D0  0028 10100 10$:	MOVL	SF$L_SAVE_PC(FP), R3	; R3 = return PC to math routine
                          53     DD  002C 10200 	PUSHL	R3			; save return PC to math routine
         10 AD            52     D0  002E 10300 	MOVL	R2, SF$L_SAVE_PC(FP)	; set return PC in frame to be user JSB PC
         3B'AF            6C     FA  0032 10400 	CALLG	(AP), B^SIGNAL1		; create another frame and signal
         10 AD            8E     D0  0036 10500 	MOVL	(SP)+, SF$L_SAVE_PC(FP)	; restore return PC to math routine
                                 04  003A 10600 	RET				; return to caller (math routine)
