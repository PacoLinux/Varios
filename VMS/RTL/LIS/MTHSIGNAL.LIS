                                     003B   100 	.SBTTL	SIGNAL1			; Local signal routine (when user JSB)
                                     003B   200 
                                     003B   300 ;++
                                     003B   400 ; FUNCTIONAL DESCRIPTION:
                                     003B   500 ;
                                     003B   600 ;	Move return PCs up a frame and signal error
                                     003B   700 ;
                                     003B   800 ; CALLING SEQUENCE:
                                     003B   900 ;
                                     003B  1000 ;	MOVL	user JSB PC, R2
                                     003B  1010 ;	MOVL	return PC to math routine, R3
                                     003B  1100 ;	CALL	SIGNAL1 (error_message.rl.v [,ignored])
                                     003B  1200 ;
                                     003B  1300 ; INPUT PARAMETERS:
                                     003B  1400 ;
                                     003B  1500 ;	error_message.rl.v		; math error message
                                     003B  1600 ;
                                     003B  1700 ; IMPLICIT INPUTS:
                                     003B  1800 ;
                                     003B  1900 ;	R0/R1 - passed to LIB$SIGNAL to be put in signal mechanism vector
                                     003B  2000 ;		so user's or any error handler can fixup.
                                     003B  2100 ;	R2    - user JSB PC
                                     003B  2200 ;	R3    - return PC to math routine
                                     003B  2300 ;
                                     003B  2400 ; OUTPUT PARAMETERS:
                                     003B  2500 ;	NONE
                                     003B  2600 ;
                                     003B  2700 ; IMPLICIT OUTPUTS:
                                     003B  2800 ;	NONE
                                     003B  2900 ;
                                     003B  3000 ; COMPLETION CODES:
                                     003B  3100 ;	NONE
                                     003B  3200 ;
                                     003B  3300 ; SIDE EFFECTS:
                                     003B  3400 ;	Signal error message
                                     003B  3500 ;--
                                     003B  3600 
                                     003B  3700 SIGNAL1:
                               0000  003B  3800 	.WORD ^M<>			; save nothing - access R0:R3
                       10 AD     DD  003D  3900 	PUSHL	SF$L_SAVE_PC(FP)	; save return PC to MTH$$SIGNAL
         10 AD            53     D0  0040  4000 	MOVL	R3, SF$L_SAVE_PC(FP)	; set return PC to be math routine
                          05     10  0044  4100 	BSBB	DO_SIGNAL		; signal using error code in R2
         10 AD            8E     D0  0046  4200 	MOVL	(SP)+, SF$L_SAVE_PC(FP)	; restore return PC to MTH$$SIGNAL
                                 04  004A  4300 	RET				; return to caller (MTH$$SIGNAL)
MTH$$SIGNAL     ; MATH ERROR SIGNAL ROUTINE                      21-AUG-1978 19:08:05   VAX-11 MACRO X0.3-11               Page   8
0-7             DO_SIGNAL  ; Local JSB routine to do signal                                                                      (8)

                                     004B   100 	.SBTTL	DO_SIGNAL		; Local JSB routine to do signal
                                     004B   200 
                                     004B   300 ;++
                                     004B   400 ; FUNCTIONAL DESCRIPTION:
                                     004B   500 ;
                                     004B   600 ;	Convert MTH$ error code to 32-bit VAX error code.
                                     004B   700 ;	Set bits 31:16 to MTH$ facility code
                                     004B   800 ;	Shift small error code left 3 places to make room for severity code
                                     004B   900 ;	and then call LIB$SIGNAL with implicit input in R0/R1 (= math routine
                                     004B  1000 ;	result.  Then perform the following call to do signal:
                                     004B  1100 ;
                                     004B  1200 ;	CALL LIB$SIGNAL (cond_val, 1, user_PC)
                                     004B  1300 ;
                                     004B  1400 ; CALLING SEQUENCE:
                                     004B  1500 ;
                                     004B  1600 ;	JSB	DO_SIGNAL
                                     004B  1610 ;
                                     004B  1620 ; INPUT PARAMETERS:
                                     004B  1630 ;
                                     004B  1640 ;	error_code.rlu.v		small math error number
                                     004B  1700 ;
                                     004B  1800 ; IMPLICIT INPUTS:
                                     004B  1900 ;
                                     004B  2000 ;	R0/R1	Math routine function value to be copied to CHF$L_MCH_R0/R1
                                     004B  2100 ;	R2	User PC to be used in message
                                     004B  2200 ;
                                     004B  2300 ; OUTPUT PARAMETERS:
                                     004B  2400 ;	NONE
                                     004B  2500 ;
                                     004B  2600 ; IMPLICIT OUTPUTS:
                                     004B  2700 ;	NONE
                                     004B  2800 ;
                                     004B  2900 ; COMPLETION CODES:
                                     004B  3000 ;	NONE
                                     004B  3100 ;
                                     004B  3200 ; SIDE EFFECTS:
                                     004B  3300 ;
                                     004B  3400 ;	Signal error
                                     004B  3500 ;--
                                     004B  3600 
                                     004B  3700 DO_SIGNAL:				; Local JSB entry point
                          52     DD  004B  3800 	PUSHL	R2			; setup last arg as user PC
                          01     DD  004D  3900 	PUSHL	#1			; indicate that one FAO arg is following
                                     004F  4000 					; so SYS$PUT_MESSAGE will know.
            16            10     78  004F  4100 	ASHL	#16, #MTH$K_FAC_NO, -(SP)
                          7E         0052       
                                     0053  4200 					; MTH$ error prefix code to LH
            6E            04     90  0053  4300 	MOVB	#STS$K_SEVERE, (SP)	; set error severity to SEVERE
                                     0056  4400 					; so image will EXIT unless user handles.
            03         04 AC     F0  0056  4500 	INSV	ERROR_CODE(AP), #STS$V_CODE, -
            6E            0C         005A       
                                     005C  4600 		#STS$S_CODE, (SP)	; insert math code shifted left
            6E       8000 8F     A8  005C  4700 	BISW	#STS$M_FAC_SP, (SP)	; Set subsystem specific message bit
                                     0061  4800 					; 3 bits to make room for severity
   00000000'EF            03     FB  0061  4900 	CALLS	#3, LIB$SIGNAL		; save R0/R1 in signal mechanism
                                     0068  5000 					; vector (CHF$L_MCH_R0/R1)
                                     0068  5100 					; return with R0/R1 = signal mechanism
