                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Call routine MAXF to compute the maximum of n arguments, n is
                                     0000   600 ;	greater than or equal to 1.
                                     0000   700 ;	Return result converted to the proper type.
                                     0000   800 
                                     0000   900 ;
                                     0000  1000 ;
                                     0000  1100 ; CALLING SEQUENCE:
                                     0000  1200 ;	Maximum.ww.v =MTH$IMAX1 ({arg.rf.r})
                                     0000  1300 ;	Maximum.wl.v =MTH$JMAX1 ({arg.rf.r})
                                     0000  1400 ;	Maximum.wf.v =MTH$AMAX1 ({arg.rf.r})
                                     0000  1500 ;	
                                     0000  1600 ;
                                     0000  1700 ;
                                     0000  1800 ; INPUT PARAMETERS:
                                     0000  1900 ;	The n parameters are single-precision floating-point values
                                     0000  2000 ;	and are call-by-reference.
                                     0000  2100 ;	
                                     0000  2200 ;
                                     0000  2300 ; IMPLICIT INPUTS:
                                     0000  2400 ;	NONE
                                     0000  2500 ;
                                     0000  2600 ; OUTPUT PARAMETERS:
                                     0000  2700 ;	NONE
                                     0000  2800 ;
                                     0000  2900 ; IMPLICIT OUTPUTS:
                                     0000  3000 ;	NONE
                                     0000  3100 ;
                                     0000  3200 ; COMPLETION CODES:
                                     0000  3300 ;	NONE
                                     0000  3400 ;
                                     0000  3500 ; SIDE EFFECTS:
                                     0000  3600 ;	Reserved Operand and Integer Overflow exceptions can occur.
                                     0000  3700 ;	
                                     0000  3800 ;
                                     0000  3900 ;--
                                     0000  4000 
                                     0000  4100 
                                     0000  4200 
                                     0000  4300 
                               4000' 0000  4400 	.ENTRY	MTH$IMAX1,	^M<IV>
                          11     10  0002  4500 	BSB	MAXF		; R0 = max arg
            50            50     49  0004  4600 	CVTFW	R0, R0		; INTEGER*2 it
                                 04  0007  4700 	RET
                                     0008  4800 
                               4000' 0008  4900 	.ENTRY	MTH$JMAX1,	^M<IV>
                          09     10  000A  5000 	BSB	MAXF		; R0 = max arg
            50            50     4A  000C  5100 	CVTFL	R0, R0		; INTEGER*4 it
                                 04  000F  5200 	RET
                                     0010  5300 
                               0000' 0010  5400 	.ENTRY	MTH$AMAX1,	^M<>
                          01     10  0012  5500 	BSB	MAXF		; R0 = max arg
                                 04  0014  5600 	RET
