                                     0000    66 	.SBTTL	LIB$MOVTC  - translate and move characters 
                                     0000    67 ;++
                                     0000    68 ; FUNCTIONAL DESCRIPTION:
                                     0000    69 ;
                                     0000    70 ;	Each character in the source is used as an index into the table.  The byte entry
                                     0000    71 ;	found is then placed into the destination as a translation.  The fill character
                                     0000    72 ;	is used if the destination string is longer than the source string.  If the
                                     0000    73 ;	source is longer than the destination, then truncation results.  Overlap of
                                     0000    74 ;	the source and destination strings does not affect execution.
                                     0000    75 ;
                                     0000    76 ; CALLING SEQUENCE:
                                     0000    77 ;
                                     0000    78 ;	CALL	LIB$MOVTC (src.rt.dx, fill.rt.dx, table.rt.dx, dst.wt.dx)
                                     0000    79 ;
                                     0000    80 
                           00000004  0000    81 	SOURCE = 4				; Adr. of desc. of source string
                           00000008  0000    82 	FILL = 8				; Adr. of desc. of fill character
                           0000000C  0000    83 	TABLE = 12				; Adr. of desc. of translation table
                           00000010  0000    84 	DEST = 16				; Adr. of desc. of destination string
                                     0000    85 
                                     0000    86 ; INPUT PARAMETERS:
                                     0000    87 ;
                                     0000    88 ;	NONE
                                     0000    89 ;
                                     0000    90 ; IMPLICIT INPUTS:
                                     0000    91 ;
                                     0000    92 ;	NONE
                                     0000    93 ;
                                     0000    94 ; OUTPUT PARAMETERS:
                                     0000    95 ;
                                     0000    96 ;	NONE
                                     0000    97 ;
                                     0000    98 ; IMPLICIT OUTPUTS:
                                     0000    99 ;
                                     0000   100 ;	NONE
                                     0000   101 ;
                                     0000   102 ; COMPLETION CODES:
                                     0000   103 ;
                                     0000   104 ;	NONE
                                     0000   105 ;
                                     0000   106 ; SIDE EFFECTS:
                                     0000   107 ;
                                     0000   108 ;	NONE
                                     0000   109 ;
                                     0000   110 ;--
                                     0000   111 
                               003C' 0000   112 	.ENTRY LIB$MOVTC , ^M<R2, R3, R4, R5>	; Entry point
                                     0002   113 
            54         0C AC     D0  0002   114 	MOVL	TABLE(AP), R4			; R4 points to table desc.
            55         08 AC     D0  0006   115 	MOVL	FILL(AP), R5			; R5 points to fill desc.
            50         04 BC     7D  000A   116 	MOVQ	@SOURCE(AP), R0			; R0/R1 source string len and adr
            52         10 BC     7D  000E   117 	MOVQ	@DEST(AP), R2			; R2/R3 dest string len and adr
            61            50     2E  0012   118 	MOVTC	R0, (R1), @4(R5), @4(R4), -	; move translated characters
         04 B4         04 B5         0015       
                                     0019       
            63            52         0019   119 		R2, (R3)
                                 04  001B   120 	RET
