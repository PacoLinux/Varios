                                     007E  8400 
                                     007E  8500 ;
                                     007E  8600 ; 0.25 =< |X|
                                     007E  8700 ;
                                     007E  8800 
                                     007E  8900 GEQ_TO_0.25:
         86 AF            50     71  007E  9000 	CMPD	R0, D_87.0		; compare |X| with 87.0
                          1D     14  0082  9100 	BGTR	GTR_THAN_87.0		; branch if |X| > 87.0
                                     0084  9200 
                                     0084  9300 ;
                                     0084  9400 ; 0.25 =< |X| =< 87.0
                                     0084  9500 ;
                                     0084  9600 
            50            56     70  0084  9700 	MOVD	R6, R0		; R0/R1 = X
                 00000000'EF     16  0087  9800 	JSB	MTH$DEXP_R7		; R0/R1 = DEXP(X)
            7E            50     7D  008D  9900 	MOVQ	R0, -(SP)		; push DEXP(X) on stack
            50         04 BC     72  0090 10000 	MNEGD	@value(AP), R0		; R0/R1 = -X
                 00000000'EF     16  0094 10100 	JSB	MTH$DEXP_R7		; R0/R1 = DEXP(-X)
            50            8E     60  009A 10200 	ADDD	(SP)+, R0		; R0/R1 = DEXP(X) + DEXP(-X)
            50            00     64  009D 10300 	MULD	S^#SD_0.5, R0		; R0/R1 = (DEXP(X) + DEXP(-X))/2
                                 04  00A0 10400 	RET				; return with result in R0/R1
                                     00A1 10500 
                                     00A1 10600 ;
                                     00A1 10700 ; 87.0 < |X|
