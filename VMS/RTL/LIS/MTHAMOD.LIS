0-5             MTH$AMOD - REAL remainder                                                                                        (4)

                                     0000   100 	.SBTTL	MTH$AMOD - REAL remainder
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Return the remainder of arg1/arg2 in floating format
                                     0000   600 ;	Remainder = arg1 - (int(arg1/arg2))*arg2
                                     0000   700 ;
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;	Remainder.wf.v =MTH$AMOD (dividend.rf.r, divisor.rf.r)
                                     0000  1100 ;	
                                     0000  1200 ;
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;	The two arguments are single-precision floating-point values
                                     0000  1600 ;	and are call-by-reference.
                                     0000  1700 ;	
                                     0000  1800 ;
                                     0000  1900 ; IMPLICIT INPUTS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; OUTPUT PARAMETERS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; IMPLICIT OUTPUTS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; COMPLETION CODES:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; SIDE EFFECTS:
                                     0000  3200 ;	Reserved Operand and Floating Overflow exceptions can occur.
                                     0000  3300 ;	
                                     0000  3400 ;
                                     0000  3500 ;--
                                     0000  3600 
                                     0000  3700 
                                     0000  3800 
                               0000' 0000  3900 	.ENTRY	MTH$AMOD,	^M<>
         04 BC         08 BC     47  0002  4100 	DIVF3	@8(AP), @4(AP), R0	; R0 = arg1/arg2
                          50         0007       
            00            08     54  0008  4200 	EMODF	#1, #0, R0, R1, R1	; R1 = fract(arg1/arg2)
            51            50         000B       
                          51         000D       
            50            51     42  000E  4300 	SUBF2	R1, R0			; R0 = int(arg1/arg2)			
            50         08 BC     44  0011  4400 	MULF	@8(AP), R0		; R0 = int(arg1/arg2)*arg2
         04 BC            50     43  0015  4500 	SUBF3	R0, @4(AP), R0		; R0 = arg1 - int(arg1/arg2)*arg2
                          50         0019       
                                 04  001A  4600 	RET
                                     001B  4700 
                                     001B  4800 
                                     001B  4900 	.END
