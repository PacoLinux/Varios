                                     0000    64 	.SBTTL	LIB$MATCHC  - match characters 
                                     0000    65 ;++
                                     0000    66 ; FUNCTIONAL DESCRIPTION:
                                     0000    67 ;
                                     0000    68 ;	The character string supplied is searched for the first incidence of
                                     0000    69 ;	the substring specified.  An index is returned which is the relative
                                     0000    70 ;	position of the first character of the substring within the string or
                                     0000    71 ;	zero if no occurrence of the substring was encountered.  If both strings have
                                     0000    72 ;	zero length or if the substring has a zero length, then the routine returns
                                     0000    73 ;	as if the substring has been found at the first character position.  If the
                                     0000    74 ;	source string has a zero length and the substring a non-zero length, then
                                     0000    75 ;	a zero is returned.
                                     0000    76 ;
                                     0000    77 ; CALLING SEQUENCE:
                                     0000    78 ;
                                     0000    79 ;	index.wlu.v = LIB$MATCHC(sub_str.rt.dx,string.rt.dx)
                                     0000    80 ;
                           00000004  0000    81 	SUB = 4					; Adr of substring desc
                           00000008  0000    82 	STRING = 8				; Adr of string desc
                                     0000    83 
                                     0000    84 ; INPUT PARAMETERS:
                                     0000    85 ;
                                     0000    86 ;	NONE
                                     0000    87 ;
                                     0000    88 ; IMPLICIT INPUTS:
                                     0000    89 ;
                                     0000    90 ;	NONE
                                     0000    91 ;
                                     0000    92 ; OUTPUT PARAMETERS:
                                     0000    93 ;
                                     0000    94 ;	NONE
                                     0000    95 ;
                                     0000    96 ; IMPLICIT OUTPUTS:
                                     0000    97 ;
                                     0000    98 ;	NONE
                                     0000    99 ;
                                     0000   100 ; FUNCTION VALUE:
                                     0000   101 ;
                                     0000   102 ;	index.wlu.v
                                     0000   103 ;
                                     0000   104 ; SIDE EFFECTS:
                                     0000   105 ;
                                     0000   106 ;	NONE
                                     0000   107 ;
                                     0000   108 ;--
                                     0000   109 
                               0030' 0000   110 	.ENTRY LIB$MATCHC , ^M<R4, R5>		; Entry point
                                     0002   111 
            54         04 AC     D0  0002   112 	MOVL	SUB(AP), R4			; R4 points to substring desc
            55         08 AC     D0  0006   113 	MOVL	STRING(AP), R5			; R5 points to string desc
         04 B4            64     39  000A   114 	MATCHC	(R4), @4(R4), (R5), @4(R5)	; match characters
         04 B5            65         000E       
                          0A     12  0011   115 	BNEQ	10$				; branch if no match
            65            64     C2  0013   116 	SUBL2	(R4), (R5)			; R5 = difference of source length
                                     0016   117 						; and substring length
            65            52     C3  0016   118 	SUBL3	R2, (R5), R0			; R0 = first char. position
                          50         0019       
