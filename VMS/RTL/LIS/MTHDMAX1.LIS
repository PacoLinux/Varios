                                     0000   100 	.SBTTL	MTH$DMAX1
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Returns the maximum of n arguments, n is greater or equal to 1.
                                     0000   600 ;
                                     0000   700 ;
                                     0000   800 ; CALLING SEQUENCE:
                                     0000   900 ;	Maximum.wd.v = MTH$DMAX1 ({arg.rd.r})
                                     0000  1000 ;	
                                     0000  1100 ;
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;	The n input parameters are double-precision floating-point
                                     0000  1500 ;	values and are call-by-reference.
                                     0000  1600 ;	
                                     0000  1700 ;
                                     0000  1800 ; IMPLICIT INPUTS:
                                     0000  1900 ;	NONE
                                     0000  2000 ;
                                     0000  2100 ; OUTPUT PARAMETERS:
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; IMPLICIT OUTPUTS:
                                     0000  2500 ;	NONE
                                     0000  2600 ;
                                     0000  2700 ; COMPLETION CODES:
                                     0000  2800 ;	NONE
                                     0000  2900 ;
                                     0000  3000 ; SIDE EFFECTS:
                                     0000  3100 ;	Reserved Operand exception can occur.
                                     0000  3200 ;	
                                     0000  3300 ;
                                     0000  3400 ;--
                                     0000  3500 
                               0004' 0000  3600 	.ENTRY	MTH$DMAX1,	^M<R2>
            52            6C     9A  0002  3800 	MOVZBL	(AP), R2	; R2 = arg count
                          8C     D5  0005  3900 	TSTL	(AP)+		; AP -> first arg
            50            9C     70  0007  4000 1$:	MOVD	@(AP)+, R0	; R0/R1 = trial max
                          08     11  000A  4100 	BRB	3$		; check arg count
                                     000C  4200 
            50         00 BC     71  000C  4300 2$:	CMPD	@0(AP), R0	; if this arg is greater than trial max
                          F5     14  0010  4400 	BGTR	1$		; then it becomes trial max
                          8C     D5  0012  4500 	TSTL	(AP)+		; else ignore it
            F5            52     F5  0014  4600 3$:	SOBGTR	R2, 2$		; return if arg count exausted
                                 04  0017  4700 	RET
                                     0018  4800 
                                     0018  4900 	.END
