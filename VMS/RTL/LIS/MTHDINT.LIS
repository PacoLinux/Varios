                                     0000   100 	.SBTTL	MTH$DINT	Double to Double truncation
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Returns the argument with all zeroes to the right of the decimal
                                     0000   600 ;	point.
                                     0000   700 ;
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;	Truncation.wd.v = MTH$DINT (arg.rd.r)
                                     0000  1100 ;
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;	The one argument is a double-precision floating-point value
                                     0000  1500 ;	and is call-by-reference.
                                     0000  1600 ;
                                     0000  1700 ; IMPLICIT INPUTS:
                                     0000  1800 ;	NONE
                                     0000  1900 ;
                                     0000  2000 ; OUTPUT PARAMETERS:
                                     0000  2100 ;	NONE
                                     0000  2200 ;
                                     0000  2300 ; IMPLICIT OUTPUTS:
                                     0000  2400 ;	NONE
                                     0000  2500 ;
                                     0000  2600 ; COMPLETION CODES:
                                     0000  2700 ;	NONE
                                     0000  2800 ;
                                     0000  2900 ; SIDE EFFECTS:
                                     0000  3000 ;	Reserved Operand exception can occur.
                                     0000  3100 ;	
                                     0000  3200 ;
                                     0000  3300 ;--
                                     0000  3400 
                                     0000  3500 
                               0000' 0000  3600 	.ENTRY	MTH$DINT,	^M<>
            00         04 BC     74  0002  3800 	EMODD	@4(AP), #0, #1, R0, R0	; R0/R1 = fraction_part(arg)
            50            08         0006       
                          50         0008       
         04 BC            50     63  0009  3900 	SUBD3	R0, @4(AP), R0		; R0/R1 = integer_part(arg)
