                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Return remainder after division.
                                     0000   600 ;
                                     0000   700 ;
                                     0000   800 ; CALLING SEQUENCE:
                                     0000   900 ;	Remainder.ww.v = MTH$IMOD (dividend.rw.r, divisor.rw.r)
                                     0000  1000 ;	
                                     0000  1100 ;
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;	The two parameters are one-word values and are call-by-reference.
                                     0000  1500 ;	
                                     0000  1600 ;
                                     0000  1700 ; IMPLICIT INPUTS:
                                     0000  1800 ;	NONE
                                     0000  1900 ;
                                     0000  2000 ; OUTPUT PARAMETERS:
                                     0000  2100 ;	NONE
                                     0000  2200 ;
                                     0000  2300 ; IMPLICIT OUTPUTS:
                                     0000  2400 ;	NONE
                                     0000  2500 ;
                                     0000  2600 ; COMPLETION CODES:
                                     0000  2700 ;	NONE
                                     0000  2800 ;
                                     0000  2900 ; SIDE EFFECTS:
                                     0000  3000 ;	Integer Overflow and Divide by Zero exceptions can occur.
                                     0000  3100 ;	
                                     0000  3200 ;
                                     0000  3300 ;--
                                     0000  3400 
                                     0000  3500 
                                     0000  3600 
                                     0000  3700 
                               4004' 0000  3800 	.ENTRY	MTH$IMOD,	^M<R2, IV> ; enable integer overflow
            50         04 BC     32  0002  4000 	CVTWL	@4(AP), R0	; R0 = divd
            01            50     7A  0006  4100 	EMUL	R0, #1, #0, R0	; R0/R1 = quadword divd
            50            00         0009       
            52         08 BC     32  000B  4200 	CVTWL	@8(AP), R2	; R2 = divr
            50            52     7B  000F  4300 	EDIV	R2, R0, R0, R0	; R0 = longword remainder
            50            50         0012       
            50            50     F7  0014  4400 	CVTLW	R0, R0		; R0 = word remainder
                                 04  0017  4500 	RET
