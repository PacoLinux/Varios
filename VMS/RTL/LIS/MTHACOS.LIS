                                     000E   150 	.SBTTL	MTH$ACOS_R5 - Special ACOS routine
                                     000E   151 
                                     000E   152 ; Special ACOS - used by the stnadard routine, direct JSB call.
                                     000E   153 ;
                                     000E   154 ; CALLING SEQUENCE:
                                     000E   155 ;	save anything needed in R0:R5
                                     000E   156 ;	MOVF	..., R0			; input in R0
                                     000E   157 ;	JSB	MTH$ACOS_R5
                                     000E   158 ;	RSB				; return with result in R0
                                     000E   159 ;
                                     000E   160 
                                     000E   161 MTH$ACOS_R5::				; special ACOS routine
            55            50     50  000E   162 	MOVF	R0, R5			; save X in R5
            50       8000 8F     AA  0011   163 	BICW	#^X8000, R0		; R0 = |X|
                          50     53  0016   164 	TSTF	R0			; test if |X| > 0
                          08     14  0018   165 	BGTR	TEST_FOR_1.0		; branch if |X| > 0
                                     001A   166 
                                     001A   167 ;
                                     001A   168 ; X = 0
                                     001A   169 ;
                                     001A   170 
            50   0FDB40C9 8F     50  001A   171 	MOVF	#LF_PI_OV_2, R0		; R0 = PI/2
                                 05  0021   172 	RSB				; return PI/2 if |X| = 0
                                     0022   173 
                                     0022   174 ;
                                     0022   175 ; 0 < |X|
                                     0022   176 ;
                                     0022   177 
                                     0022   178 TEST_FOR_1.0:
            08            50     51  0022   179 	CMPF	R0, S^#SF_1.0		; compare |X| with 1.0
                          19     18  0025   180 	BGEQ	GEQ_TO_1.0		; branch if |X| >= 1.0
                                     0027   181 
                                     0027   182 ;
                                     0027   183 ; 0 < |X| < 1.0
                                     0027   184 ;
                                     0027   185 
            55            55     45  0027   186 	MULF3	R5, R5, R0		; R0 = X**2
                          50         002A       
            08            50     43  002B   187 	SUBF3	R0, S^#SF_1.0, R0	; R0 = 1.0 - X**2
                          50         002E       
                 00000000'EF     16  002F   188 	JSB	MTH$SQRT_R2		; R0 = SQRT(1-X**2)
            50            55     46  0035   189 	DIVF	R5, R0			; R0 = SQRT(1-X**2)/X
                 00000000'EF     16  0038   190 	JSB	MTH$ATAN_R4		; R0 = ATAN(SQRT(1-X**2)/X)
                          04     11  003E   191 	BRB	TEST_SIGN		; branch to TEST_SIGN
                                     0040   192 
                                     0040   193 ;
                                     0040   194 ; 1 =< |X| 
                                     0040   195 ;
                                     0040   196 
                                     0040   197 GEQ_TO_1.0:
                          0E     14  0040   198 	BGTR	ERROR			; branch to ERROR if |X| > 1.0
                                     0042   199 
                                     0042   200 
                                     0042   201 ;
                                     0042   202 ; |X| = 1.0
                                     0042   203 ;
                                     0042   204 
MTH$ACOS        ; Floating Point Arc-cosine routine              21-AUG-1978 19:01:20   VAX-11 MACRO X0.3-11               Page   7
01-5            MTH$ACOS_R5 - Special ACOS routine                                                                               (5)

                          50     D4  0042   205 	CLRF	R0			; R0 = 0
                                     0044   206 
                                     0044   207 
                                     0044   208 ;
                                     0044   209 ; Test the sign of X in order to decide if add PI to the result
                                     0044   210 ;
                                     0044   211 
                                     0044   212 TEST_SIGN:
                          55     53  0044   213 	TSTF	R5			; test the sign of X
                          07     18  0046   214 	BGEQ	10$			; branch if X > 0
            50   0FDB4149 8F     40  0048   215 	ADDF	#LF_PI, R0		; add PI to R0 if X < 0
                                 05  004F   216 10$:	RSB				; return with result in R0
                                     0050   217 
                                     0050   218 ;
                                     0050   219 ; 1 < |X|, error
                                     0050   220 ;
                                     0050   221 
                          6E     DD  0050   222 ERROR:	PUSHL	(SP)			; return PC from JSB routine
            7E         51 8F     9A  0052   223 	MOVZBL	#MTH__INVARG, -(SP)	; condition value
            01            0F     78  0056   224 	ASHL	#15, #1, R0		; R0 = result = reserved operand -0.0
                          50         0059       
                                     005A   225 					; goes to signal mechanism vector
                                     005A   226 					; (CHF$L_MCH_R0/R1) so error handler
                                     005A   227 					; can modify the result.
       0000'CF            02     FB  005A   228 	CALLS	#2, W^MTH$$SIGNAL	; signal error and use real user's PC
                                     005F   229 					; independent of CALL vs JSB
                                 05  005F   230 	RSB				; return - R0 restored from CHF$L_MCH_R0/R1
                                     0060   231 
                                     0060   232 
                                     0060   233 
                                     0060   234 
                                     0060   235 	.END
