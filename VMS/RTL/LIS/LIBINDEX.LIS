0-5             LIB$INDEX - return relative pos. of substring                                                                    (5)

                               000C' 0000   122 	.ENTRY	LIB$INDEX, ^M<R2,R3> ; entry point and entry mask
                                     0002   123 					; integer enable is off for SUBW3 (unsigned)
                                     0002   124 
            50         08 BC     7D  0002   125 	MOVQ	@sub_string(AP), R0	; R0/R1 = sub string len/adr.
            52         04 BC     7D  0006   126 	MOVQ	@src_string(AP), R2	; R2/R3 = source string len/adr.
            61            50     39  000A   127 	MATCHC	R0, (R1), R2, (R3)	; look for sub string in source
            63            52         000D       
                          0F     12  000F   128 	BNEQ	10$			; branch if no match found
                                     0011   129 					; Note: could use following code except
                                     0011   130 					; for src len = 0 and sub len > 0.
         04 BC         08 BC     A3  0011   131 	SUBW3	@sub_string(AP), -	; R0 = unsigned difference in lengths of
                          50         0016       
                                     0017   132 		@src_string(AP), R0	; src minus sub.
            50            50     3C  0017   133 	MOVZWL	R0, R0			; R0 = len src - len sub
            50            52     C2  001A   134 	SUBL	R2, R0			; R0 = len src - len sub - # char left
                                     001D   135 					; to scan = rel. pos. of 1st char of sub.
                          50     D6  001D   136 	INCL	R0			; wants 1-origin, not 0-origin position.
                                 04  001F   137 	RET				; R0 = relative position
                                     0020   138 
                                     0020   139 
                                     0020   140 ;+
                                     0020   141 ; Here when sub_string not found
                                     0020   142 ;-
                                     0020   143 
                          50     D4  0020   144 10$:	CLRL	R0			; R0 = 0 to indicate not found
                                 04  0022   145 	RET				; return
                                     0023   146 
                                     0023   147 	.END				; End of module LIB$INDEX
