0-3             FOR$DATE - return system date as an 9 byte stri                                                                  (4)

                                     0000   100 	.SBTTL	FOR$DATE - return system date as an 9 byte string
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Returns system date as an 9 byte string in the form
                                     0000   600 ;	DD-MMM-YY. Build a work space on the stack to con-
                                     0000   700 ;	tain the date returned by the system (DD-MMM-YYYY).
                                     0000   800 ;	Do a $ASCTIM to get date into this area.  Move the string
                                     0000   900 ;	into the user area, artfully avoiding the first two YY's.
                                     0000  1000 ;
                                     0000  1100 ; CALLING SEQUENCE:
                                     0000  1200 ;	CALL	FOR$DATE (9_byte_array.wb.ra)
                                     0000  1300 ;
                                     0000  1400 ;
                                     0000  1500 ; INPUT PARAMETERS:
                                     0000  1600 ;	9_byte_array		the addr of 9 bytes into which the
                                     0000  1700 ;				string is to be placed
                                     0000  1800 ;
                                     0000  1900 ; IMPLICIT INPUTS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; OUTPUT PARAMETERS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; IMPLICIT OUTPUTS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; COMPLETION CODES:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; SIDE EFFECTS:
                                     0000  3200 ;	NONE
                                     0000  3300 ;
                                     0000  3400 ;--
                                     0000  3500 
                                     0000  3600 
                               003C' 0000  3700 	.ENTRY	FOR$DATE,	^M<R2,R3,R4,R5>
            5E            0C     C2  0002  3900 	SUBL	#12, SP			; make space for date string
            52            5E     D0  0005  4000 	MOVL	SP, R2			; R2 -> space
            7E            52     D0  0008  4100 	MOVL	R2, -(SP)		; build a string descriptor
            7E   010E000B 8F     D0  000B  4200 	MOVL	#^X10E000B, -(SP)	; len=11
            50            5E     D0  0012  4300 	MOVL	SP, R0			; R0 -> string descriptor
                                     0015  4400 	$ASCTIM_S	,(R0)		; R2 -> date string
            62            07     28  0024  4500 	MOVC3	#7, (R2), @4(AP)	; move in "dd-mmm-"
                       04 BC         0027       
            81            81     91  0029  4600 	CMPB	(R1)+, (R1)+		; skip "19"
            61            02     28  002C  4700 	MOVC3	#2, (R1), (R3)		; move in rest of year
                          63         002F       
                                 04  0030  4800 	RET				; return to user
                                     0031  4900 
                                     0031  5000 
                                     0031  5100 	.END
