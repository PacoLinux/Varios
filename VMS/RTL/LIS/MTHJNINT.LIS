                                     0000   100 	.SBTTL	MTH$JNINT - return nearest integer as INTEGER*4
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Round the argument to the nearest integer and return as a 
                                     0000   600 ;	longword integer.
                                     0000   700 ;
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;	Nearest_integer.wl.v = MTH$JNINT (arg.rf.r)
                                     0000  1100 ;	
                                     0000  1200 ;
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;	The input parameter is single-precision floating-point value
                                     0000  1600 ;	and is call-by-reference.
                                     0000  1700 ;	
                                     0000  1800 ;
                                     0000  1900 ; IMPLICIT INPUTS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; OUTPUT PARAMETERS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; IMPLICIT OUTPUTS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; COMPLETION CODES:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; SIDE EFFECTS:
                                     0000  3200 ;	Reserved Operand, Floating Overflow, and Floating Underflow
                                     0000  3300 ;	exceptions can occur.
                                     0000  3400 ;	
                                     0000  3500 ;
                                     0000  3600 ;--
                                     0000  3700 
                                     0000  3800 
                               0000' 0000  3900 	.ENTRY	MTH$JNINT,	^M<>
         04 BC            00     41  0002  4100 	ADDF3	#0.5, @4(AP), R0	; R0 = arg + 0.5
                          50         0006       
                          03     14  0007  4200 	BGTR	1$			; branch if positive
            50            08     42  0009  4300 	SUBF	#1.0, R0		; else R0 = arg - 0.5
            50            50     4A  000C  4400 1$:	CVTFL	R0, R0			; R0 = truncated R0
                                 04  000F  4500 	RET
                                     0010  4600 
                                     0010  4700 	.END
