0-3             MTH$DPROD - return DOUBLE product of two FLOATI                                                                  (4)

                                     0000   100 	.SBTTL	MTH$DPROD - return DOUBLE product of two FLOATING args
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Convert the two single-precision floating-point arguments to
                                     0000   600 ;	double-precision.  Return the result of their multiplication
                                     0000   700 ;	in double-precision.
                                     0000   800 ;
                                     0000   900 ;
                                     0000  1000 ; CALLING SEQUENCE:
                                     0000  1100 ;	Double_product.wd.v = MTH$DPROD (arg1.rf.r, arg2.rf.r)
                                     0000  1200 ;	
                                     0000  1300 ;
                                     0000  1400 ;
                                     0000  1500 ; INPUT PARAMETERS:
                                     0000  1600 ;	The two input parameters are single-precision floating-point
                                     0000  1700 ;	values and are call-by-reference.
                                     0000  1800 ;	
                                     0000  1900 ;
                                     0000  2000 ; IMPLICIT INPUTS:
                                     0000  2100 ;	NONE
                                     0000  2200 ;
                                     0000  2300 ; OUTPUT PARAMETERS:
                                     0000  2400 ;	NONE
                                     0000  2500 ;
                                     0000  2600 ; IMPLICIT OUTPUTS:
                                     0000  2700 ;	NONE
                                     0000  2800 ;
                                     0000  2900 ; COMPLETION CODES:
                                     0000  3000 ;	NONE
                                     0000  3100 ;
                                     0000  3200 ; SIDE EFFECTS:
                                     0000  3300 ;	Reserved Operand and Floating Overflow exceptions can occur.
                                     0000  3400 ;	
                                     0000  3500 ;
                                     0000  3600 ;--
                                     0000  3700 
                                     0000  3800 
                                     0000  3900 
                               000C' 0000  4000 	.ENTRY	MTH$DPROD,	^M<R2, R3>	; save R2 and R3
            50         04 BC     56  0002  4200 	CVTFD	@4(AP), R0		; R0/R1 = arg1
            52         08 BC     56  0006  4300 	CVTFD	@8(AP), R2		; R2/R3 = arg2
            50            52     64  000A  4400 	MULD	R2, R0			; R0/R1 = R0/R1 * R2/R3
                                 04  000D  4500 	RET
                                     000E  4600 
                                     000E  4700 
                                     000E  4800 	.END
