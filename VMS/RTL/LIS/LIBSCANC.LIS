                                     0000    65 	.SBTTL	LIB$SCANC  - scan characters 
                                     0000    66 ;++
                                     0000    67 ; FUNCTIONAL DESCRIPTION:
                                     0000    68 ;
                                     0000    69 ;	The bytes of the string specified by the source descriptor are successively
                                     0000    70 ;	used to index into a the table.
                                     0000    71 ;	The byte selected from the table is ANDed with the mask byte specified
                                     0000    72 ;	by the mask descriptor.  The operation continues until the result of the
                                     0000    73 ;	AND is non-zero.  The relative position of the character in the source string
                                     0000    74 ;	which terminated the operation is returned if one is found.  Otherwise, zero
                                     0000    75 ;	is returned.  If the source striing has a length of zero then a zero is
                                     0000    76 ;	returned.
                                     0000    77 ;
                                     0000    78 ; CALLING SEQUENCE:
                                     0000    79 ;
                                     0000    80 ;	index.wlu.v = LIB$SCANC (src.rt.dx, table.rbu.ra, mask.rbu.r)
                                     0000    81 ;
                           00000004  0000    82 	SOURCE = 4				; Adr of source string desc
                           00000008  0000    83 	TABLE = 8				; Adr of base of table
                           0000000C  0000    84 	MASK = 12				; adr of mask byte
                                     0000    85 
                                     0000    86 ; INPUT PARAMETERS:
                                     0000    87 ;
                                     0000    88 ;	NONE
                                     0000    89 ;
                                     0000    90 ; IMPLICIT INPUTS:
                                     0000    91 ;
                                     0000    92 ;	NONE
                                     0000    93 ;
                                     0000    94 ; OUTPUT PARAMETERS:
                                     0000    95 ;
                                     0000    96 ;	NONE
                                     0000    97 ;
                                     0000    98 ; IMPLICIT OUTPUTS:
                                     0000    99 ;
                                     0000   100 ;	NONE
                                     0000   101 ;
                                     0000   102 ; FUNCTION VALUE:
                                     0000   103 ;
                                     0000   104 ;	index.wlu.v
                                     0000   105 ;
                                     0000   106 ; SIDE EFFECTS:
                                     0000   107 ;
                                     0000   108 ;	NONE
                                     0000   109 ;
                                     0000   110 ;--
                                     0000   111 
                               0000' 0000   112 	.ENTRY LIB$SCANC , ^M< >		; Entry point
                                     0002   113 
            50         04 BC     7D  0002   114 	MOVQ	@SOURCE(AP), R0			; R0/R1 = source len and adr
            61            50     2A  0006   115 	SCANC	R0, (R1), @TABLE(AP), @MASK(AP)	; Scan character
         0C BC         08 BC         0009       
                          07     13  000D   116 	BEQL	10$				; branch if no character found
         04 BC            50     A3  000F   117 	SUBW3	R0, @SOURCE(AP), R0		; get position of character
                          50         0013       
                          50     D6  0014   118 	INCL	R0				; one origin
                                 04  0016   119 10$:	RET
