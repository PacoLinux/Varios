                                     0015  5800 ; MINF returns smallest of the REAL*4 args.
                                     0015  5900 ;-
                                     0015  6000 
                                     0015  6100 MINF:
            51            6C     9A  0015  6300 	MOVZBL	(AP), R1		; R1 = number of args
                          8C     D5  0018  6400 	TSTL	(AP)+			; AP -> first arg adr
            50            9C     50  001A  6500 1$:	MOVF	@(AP)+, R0		; get trial min
                          08     11  001D  6600 	BRB	3$			; check arg count
            50         00 BC     51  001F  6700 2$:	CMPF	@0(AP),R0		; if this arg is less then trial min
                          F5     19  0023  6800 	BLSS	1$			; then it becomes trial min
                          8C     D5  0025  6900 	TSTL	(AP)+			; else ignore it
            F5            51     F5  0027  7000 3$:	SOBGTR	R1, 2$			; check arg count
                                 05  002A  7100 	RSB
                                     002B  7200 
                                     002B  7300 
                                     002B  7400 	.END
