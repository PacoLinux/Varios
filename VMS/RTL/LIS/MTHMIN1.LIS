                                     0000   100 	.SBTTL	MTH$IMIN1, MTH$JMIN1, MTH$AMIN1
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Use routine MINF to compute the minimum of n arguments,
                                     0000   600 ;	n is greater than or equal to 1.  Return the result converted
                                     0000   700 ;	to the proper type.
                                     0000   800 ;
                                     0000   900 ;
                                     0000  1000 ; CALLING SEQUENCE:
                                     0000  1100 ;	Minimum.ww.v = MTH$IMIN1 ({arg.rf.r})
                                     0000  1200 ;	Minimum.wl.v = MTH$JMIN1 ({arg.rf.r})
                                     0000  1300 ;	Minimum.wf.v = MTH$AMIN1 ({arg.rf.r})
                                     0000  1400 ;	
                                     0000  1500 ;
                                     0000  1600 ;
                                     0000  1700 ; INPUT PARAMETERS:
                                     0000  1800 ;	The n parameters are single-precision floating-point values
                                     0000  1900 ;	and are call-by-reference.
                                     0000  2000 ;	
                                     0000  2100 ;
                                     0000  2200 ; IMPLICIT INPUTS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; OUTPUT PARAMETERS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; IMPLICIT OUTPUTS:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; COMPLETION CODES:
                                     0000  3200 ;	NONE
                                     0000  3300 ;
                                     0000  3400 ; SIDE EFFECTS:
                                     0000  3500 ;	Reserved Operand and Integer Overflow exceptions can occur.
                                     0000  3600 ;	
                                     0000  3700 ;
                                     0000  3800 ;--
                                     0000  3900 
                                     0000  4000 
                                     0000  4100 
                                     0000  4200 
                               4000' 0000  4300 	.ENTRY	MTH$IMIN1,	^M<IV>
                          11     10  0002  4400 	BSB	MINF		; R0 = min arg
            50            50     49  0004  4500 	CVTFW	R0, R0		; INTEGER*2 it
                                 04  0007  4600 	RET
                                     0008  4700 
                               4000' 0008  4800 	.ENTRY	MTH$JMIN1, 	^M<IV>
                          09     10  000A  4900 	BSB	MINF		; R0 = min arg
            50            50     4A  000C  5000 	CVTFL	R0, R0		; INTEGER*4 it
                                 04  000F  5100 	RET
                                     0010  5200 
                               0000' 0010  5300 	.ENTRY	MTH$AMIN1,	^M<>
                          01     10  0012  5400 	BSB	MINF		; R0 = min arg
                                 04  0014  5500 	RET
                                     0015  5600 
                                     0015  5700 ;+
