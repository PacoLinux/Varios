                                     0000    79 	.SBTTL	MTH$CLOG
                                     0000    80 
                                     0000    81 ;++
                                     0000    82 ; FUNCTIONAL DESCRIPTION:
                                     0000    83 ;
                                     0000    84 ;	The Natural Logaritm of a complex number (r, i) is
                                     0000    85 ;	computed as follows:
                                     0000    86 ;
                                     0000    87 ;		CLOG(r, i) = (ALOG(CABS(r, i)), ATAN2(i, r))
                                     0000    88 ;
                                     0000    89 ; CALLING SEQUENCE:
                                     0000    90 ;	Natural_log.wfc.v = MTH$CLOG(arg.rfc.r)
                                     0000    91 ;
                                     0000    92 ;
                                     0000    93 ; INPUT PARAMETERS:
                                     0000    94 ;	The one parameter is the address of a complex number (r, i),
                                     0000    95 ;	where r and i are both single-precision floating point values.
                                     0000    96 ;
                                     0000    97 ; IMPLICIT INPUTS:
                                     0000    98 ;	NONE
                                     0000    99 ;
                                     0000   100 ; OUTPUT PARAMETERS:
                                     0000   101 ;	NONE
                                     0000   102 ;
                                     0000   103 ; IMPLICIT OUTPUTS:
                                     0000   104 ;	NONE
                                     0000   105 ;
                                     0000   106 ; COMPLETION CODES:
                                     0000   107 ;	NONE
                                     0000   108 ;
                                     0000   109 ; SIDE EFFECTS:
                                     0000   110 ;	Signals	Reserved Operand if the input parameter is bad (-0.0)
                                     0000   111 ;
                                     0000   112 ;--
                                     0000   113 
                                     0000   114 
                               007C' 0000   115 	.ENTRY	MTH$CLOG,	^M<R2,R3,R4,R5,R6>
                                     0002   116 	MTH$FLAG_JACKET			; set up error handler
                                     0002      
            6D       0000'CF     3E  0002       	MOVAW	W^MTH$$JACKET_HND, (FP)
                                     0007       					; set handler address to jacket
                                     0007       					; handler
                                     0007       
            50         04 AC     D0  0007   117 	MOVL	argadr(AP), R0		; R0 -> (r, i)
                          80     DF  000B   118 	PUSHAF	(R0)+			; push addr(r)
                          50     DD  000D   119 	PUSHL	R0			; push addr(i)
   00000000'EF            02     FB  000F   120 	CALLS	#2, MTH$ATAN2		; R0 = ATAN2(i, r)
            56            50     D0  0016   121 	MOVL	R0, R6			; R6 = ATAN2(i, r)
   00000000'EF            6C     FA  0019   122 	CALLG	(AP), MTH$CABS		; R0 = CABS((r, i))
                 00000000'EF     16  0020   123 	JSB	MTH$ALOG_R5		; R0 = ALOG(CABS((r, i)))
            51            56     D0  0026   124 	MOVL	R6, R1			; R1 = ATAN2(i, r)
                                 04  0029   125 	RET
                                     002A   126 
                                     002A   127 
                                     002A   128 	.END
