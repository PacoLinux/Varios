00-01           LIB$EXTZV - Extract a zero extended field                                                                        (3)

                                     0000    62 	.SBTTL LIB$EXTZV - Extract a zero extended field
                                     0000    63 ;++
                                     0000    64 ; FUNCTIONAL DESCRIPTION:
                                     0000    65 ;
                                     0000    66 ;	Return a longword zero extended field which has been extracted from the specified
                                     0000    67 ;	variable bit field.  If the size specified is zero, a zero is returned.
                                     0000    68 ;	Specifying a size greater than 31 results in a reserved operand fault.
                                     0000    69 ;
                                     0000    70 ; CALLING SEQUENCE:
                                     0000    71 ;
                                     0000    72 ;	field.wlu.v = LIB$EXTZV (pos.rl.r, size.rbu.r, base.rv.r)
                                     0000    73 ;
                           00000004  0000    74 	POSITION = 4			; Adr. of longword position
                           00000008  0000    75 	SIZE = 8			; Adr. of byte containing size
                           0000000C  0000    76 	BASE = 12			; Adr. of base of field
                                     0000    77 
                                     0000    78 
                                     0000    79 ; INPUT PARAMETERS:
                                     0000    80 ;
                                     0000    81 ;	NONE
                                     0000    82 ;
                                     0000    83 ; IMPLICIT INPUTS:
                                     0000    84 ;
                                     0000    85 ;	NONE
                                     0000    86 ;
                                     0000    87 ; OUTPUT PARAMETERS:
                                     0000    88 ;
                                     0000    89 ;	NONE
                                     0000    90 ;
                                     0000    91 ; IMPLICIT OUTPUTS:
                                     0000    92 ;
                                     0000    93 ;	NONE
                                     0000    94 ;
                                     0000    95 ; FUNCTION VALUE:
                                     0000    96 ;
                                     0000    97 ;	field
                                     0000    98 ;
                                     0000    99 ; SIDE EFFECTS:
                                     0000   100 ;
                                     0000   101 ;	SS$ROPRAND - reserved operand fault for size greater than 31
                                     0000   102 ;
                                     0000   103 ;--
                                     0000   104 
                                     0000   105 
                                     0000   106 
                               0000' 0000   107 	.ENTRY LIB$EXTZV, ^M<>	; ENTRY POINT (OR MASK)
                                     0002   108 
         08 BC         04 BC     EF  0002   109 	EXTZV	@POSITION(AP), @SIZE(AP), -
            50         0C BC         0007       
                                     000A   110 		@BASE(AP), R0			; extract and zero extend
                                 04  000A   111 	RET
                                     000B   112 
                                     000B   113 
                                     000B   114 	.END
