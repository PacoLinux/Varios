0-4             MTH$CEXP - perform COMPLEX exponentiation                                                                        (4)

                                     0000    78 	.SBTTL	MTH$CEXP - perform COMPLEX exponentiation
                                     0000    79 
                                     0000    80 ;++
                                     0000    81 ; FUNCTIONAL DESCRIPTION:
                                     0000    82 ;
                                     0000    83 ;	The result of the operation e ** (r, i) is computed
                                     0000    84 ;	by:
                                     0000    85 ;
                                     0000    86 ;	result = (EXP(r) * COS(i), EXP(r) * SIN(i))
                                     0000    87 ;
                                     0000    88 ; CALLING SEQUENCE:
                                     0000    89 ;	Exponential.wfc.v	= MTH$CEXP(arg.rfc.r)
                                     0000    90 ;
                                     0000    91 ;
                                     0000    92 ; INPUT PARAMETERS:
                                     0000    93 ;	The one input parameter is the address of COMPLEX number (r, i),
                                     0000    94 ;	where r and i are both single-precision floating point values.
                                     0000    95 ;
                                     0000    96 ; IMPLICIT INPUTS:
                                     0000    97 ;	NONE
                                     0000    98 ;
                                     0000    99 ; OUTPUT PARAMETERS:
                                     0000   100 ;	NONE
                                     0000   101 ;
                                     0000   102 ; IMPLICIT OUTPUTS:
                                     0000   103 ;	NONE
                                     0000   104 ;
                                     0000   105 ; COMPLETION CODES:
                                     0000   106 ;	NONE
                                     0000   107 ;
                                     0000   108 ; SIDE EFFECTS:
                                     0000   109 ;	Signals:	MTH$_SINSIGLOS	if |i| > 2*PI*2**31.
                                     0000   110 ;			Floating Overflow if r > 88.028
                                     0000   111 ;
                                     0000   112 ;--
                                     0000   113 
                                     0000   114 
                               00FC' 0000   115 	.ENTRY	MTH$CEXP,	^M<R2,R3,R4,R5,R6,R7>
                                     0002   116 	MTH$FLAG_JACKET			; resignal
                                     0002      
            6D       0000'CF     3E  0002       	MOVAW	W^MTH$$JACKET_HND, (FP)
                                     0007       					; set handler address to jacket
                                     0007       					; handler
                                     0007       
                                     0007   117 
            50         04 BC     D0  0007   118 	MOVL	@argadr(AP), R0		; R0 = r
                 00000000'EF     16  000B   119 	JSB	MTH$EXP_R4		; R0 = EXP(r)
            55            50     D0  0011   120 	MOVL	R0, R5			; R5 = EXP(r)
                                     0014   121 
            56         04 AC     D0  0014   122 	MOVL	argadr(AP), R6		; R6 -> (r, i)
                                     0018   123 
            50         04 A6     D0  0018   124 	MOVL	4(R6), R0		; R0 = i
                 00000000'EF     16  001C   125 	JSB	MTH$SIN_R4		; R0 = SIN(i)
            57            50     D0  0022   126 	MOVL	R0, R7			; R7 = SIN(i)
                                     0025   127 
            50         04 A6     D0  0025   128 	MOVL	4(R6), R0		; R0 = i
                 00000000'EF     16  0029   129 	JSB	MTH$COS_R4		; R0 = COS(i)
MTH$CEXP        COMPLEX EXPONENTIATION                           21-AUG-1978 19:02:24   VAX-11 MACRO X0.3-11               Page   5
0-4             MTH$CEXP - perform COMPLEX exponentiation                                                                        (4)

                                     002F   130 
            50            55     44  002F   131 	MULF	R5, R0			; R0 = COS(i) * EXP(r)
            55            57     45  0032   132 	MULF3	R7, R5, R1		; R1 = SIN(i) * EXP(r)
                          51         0035       
                                 04  0036   133 	RET
                                     0037   134 
                                     0037   135 
                                     0037   136 	.END
