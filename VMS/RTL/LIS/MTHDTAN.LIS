MTH$DTAN        ; Double Precision Floating Point Tangent routi  21-AUG-1978 19:05:32   VAX-11 MACRO X0.3-11               Page   4
0-3             MTH$DTAN  - Standard Double Precision Floating                                                                   (4)

                                     0000    90 	.SBTTL	MTH$DTAN  - Standard Double Precision Floating DTAN
                                     0000    91 
                                     0000    92 
                                     0000    93 ;++
                                     0000    94 ; FUNCTIONAL DESCRIPTION:
                                     0000    95 ;
                                     0000    96 ; DTAN  - double precision floating point function
                                     0000    97 ;
                                     0000    98 ; Algorithmic steps:
                                     0000    99 ;	1) Calculate DSIN
                                     0000   100 ;	   If error from DSIN then return with reserved operand.
                                     0000   101 ;	   If DSIN(X) = 0 then return DTAN(X) = 0.
                                     0000   102 ;	2) Calculate DCOS
                                     0000   103 ;	   (No need to check for reserved operand, should have been
                                     0000   104 ;	   caught in step 1.)
                                     0000   105 ;	   (No need to check for zero, will be caught in step3 as a
                                     0000   106 ;	   hardware trap.)
                                     0000   107 ;	3) Calculate DSIN/DCOS
                                     0000   108 ;	   Hardware trap happens if divided by zero or overflow.
                                     0000   109 ;
                                     0000   110 ; CALLING SEQUENCE:
                                     0000   111 ;
                                     0000   112 ;	DTAN.wd.v = MTH$DTAN(x.rd.r)
                                     0000   113 ;
                                     0000   114 ; INPUT PARAMETERS:
                                     0000   115 
                           00000004  0000   116 	LONG = 4			; define longword multiplier
                           00000004  0000   117 	x = 1 * LONG			; Contents of x is the argument
                                     0000   118 
                                     0000   119 ; IMPLICIT INPUTS:	none
                                     0000   120 ;
                                     0000   121 ; OUTPUT PARAMETERS:
                                     0000   122 ;
                                     0000   123 ;	VALUE:	double precision floating tangent of the argument
                                     0000   124 ;
                                     0000   125 ; IMPLICIT OUTPUTS:	none
                                     0000   126 ;
                                     0000   127 ; COMPLETION CODES:	none
                                     0000   128 ;
                                     0000   129 ; SIDE EFFECTS:
                                     0000   130 ;
                                     0000   131 ; Signal: MTH$_FLOOVEMAT if DCOS(X) = 0 with reserved operand in R0/R1 (copied
                                     0000   132 ; to the signal mechanism vector CHF$L_MCH_R0/R1 by LIB$SIGNAL). Associated
                                     0000   133 ; message is: "FLOATING OVERFLOW IN MATH LIBRARY". Result is reserved operand -0.0
                                     0000   134 ; unless a user supplied (or any) error handler changes CHF$L_MCH_R0/R1.
                                     0000   135 ;
                                     0000   136 ; NOTE: This procedure disables floating point underflow, enables integer
                                     0000   137 ; overflow.
                                     0000   138 ;
                                     0000   139 ;---
                                     0000   140 
                                     0000   141 
                               40FC' 0000   142 	.ENTRY	MTH$DTAN, ^M<IV, R2, R3, R4, R5, R6, R7>
                                     0002   143 					; standard call-by-reference entry
                                     0002   144 					; disable DV (and FU), enable IV
            6D       0000'CF     DE  0002   145 	MOVAL	W^MTH$$SIGNAL_CON, (FP)	; jacket routine in case of error
                                     0007   146 					; call MTH$$SIGNAL_CON in order
MTH$DTAN        ; Double Precision Floating Point Tangent routi  21-AUG-1978 19:05:32   VAX-11 MACRO X0.3-11               Page   5
0-3             MTH$DTAN  - Standard Double Precision Floating                                                                   (4)

                                     0007   147 					; to hide away error message from
                                     0007   148 					; MTH$SIN_R4 or MTH$COS_R4
                                     0007   149 					; If an error, convert signal to user PC
                                     0007   150 					; and resignal
            50         04 BC     70  0007   151 	MOVD	@x(AP), R0		; R0/R1 = argument
                 00000000'EF     16  000B   152 	JSB	MTH$DSIN_R7		; call DSIN(X), R0/R1 = DSIN(X)
                                     0011   153 					; Note: lost of significant can occur 
                                     0011   154 					; if |X| >= 2*PI*2**31. The handler will
                                     0011   155 					; convert the PC to the user's CALL PC
                                     0011   156 					; and resignal
       8000 8F            50     B1  0011   157 	CMPW	R0, #^X8000		; test for reserved operand
                          14     13  0016   158 	BEQL	ERROR_OVF		; branch if reserved operand
            7E            50     70  0018   159 	MOVD	R0, -(SP)		; save DSIN(X) on stack
                          0E     13  001B   160 	BEQL	DTANX			; return 0 if DSIN(X) = 0
            50         04 BC     70  001D   161 	MOVD	@x(AP), R0		; R0/R1 = argument
                 00000000'EF     16  0021   162 	JSB	MTH$DCOS_R7		; call DCOS(X), R0/R1 = DCOS(X)
            8E            50     67  0027   163 	DIVD3	R0, (SP)+, R0		; R0/R1 = DTAN(X) = DSIN(X)/DCOS(X)
                          50         002A       
                                     002B   164 					; Note: divide by 0 can occur!
                                     002B   165 					; The handler will convert the PC to
                                     002B   166 					; the user's call PC and resignal.
                                 04  002B   167 DTANX:	RET				; return, R0/R1 = result
                                     002C   168 
                                     002C   169 
                                     002C   170 ERROR_OVF:
                                     002C   171 					; Note: user PC arg not needed when
                                     002C   172 					; always have a jacket routine
            7E         58 8F     9A  002C   173 	MOVZBL	#MTH__FLOOVEMAT, -(SP)	; condition value
            01            0F     79  0030   174 	ASHQ	#15, #1, R0		; R0/R1 = result = reserved operand -0.0
                          50         0033       
                                     0034   175 					; goes to signal mechanism vector
                                     0034   176 					; (CHF$L_MCH_R0/R1) so error handler
                                     0034   177 					; can modify the result.
       0000'CF            01     FB  0034   178 	CALLS	#1, W^MTH$$SIGNAL	; signal error and use real user's PC
                                     0039   179 					; independent of CALL vs JSB
                                 04  0039   180 10$:	RET				; return - R0/R1 restored from CHF$L_MCH_R0/R1
                                     003A   181 
                                     003A   182 
                                     003A   183 	.END
