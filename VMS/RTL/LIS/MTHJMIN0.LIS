                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Call routine MINL to compute the minimum of n arguments, n is
                                     0000   600 ;	greater than or equal to 1.
                                     0000   700 ;	Return the result converted to the proper type.
                                     0000   800 ;
                                     0000   900 ;
                                     0000  1000 ; CALLING SEQUENCE:
                                     0000  1100 ;	Minimum.wl.v =MTH$JMIN0 ({arg.rl.r})
                                     0000  1200 ;	Minimum.wf.v =MTH$AJMIN0 ({arg.rl.r})
                                     0000  1300 ;	
                                     0000  1400 ;
                                     0000  1500 ;
                                     0000  1600 ; INPUT PARAMETERS:
                                     0000  1700 ;	The n parameters are longword values and are call-by-reference.
                                     0000  1800 ;	
                                     0000  1900 ;
                                     0000  2000 ; IMPLICIT INPUTS:
                                     0000  2100 ;	NONE
                                     0000  2200 ;
                                     0000  2300 ; OUTPUT PARAMETERS:
                                     0000  2400 ;	NONE
                                     0000  2500 ;
                                     0000  2600 ; IMPLICIT OUTPUTS:
                                     0000  2700 ;	NONE
                                     0000  2800 ;
                                     0000  2900 ; COMPLETION CODES:
                                     0000  3000 ;	NONE
                                     0000  3100 ;
                                     0000  3200 ; SIDE EFFECTS:
                                     0000  3300 ;	NONE
                                     0000  3400 ;
                                     0000  3500 ;--
                                     0000  3600 
                                     0000  3700 
                                     0000  3800 
                                     0000  3900 
                               0000' 0000  4000 	.ENTRY	MTH$JMIN0,	^M<>
                          09     10  0002  4100 	BSB	MINL		; R0 = min arg
                                 04  0004  4200 	RET
                                     0005  4300 
                               0000' 0005  4400 	.ENTRY	MTH$AJMIN0,	^M<>
                          04     10  0007  4500 	BSB	MINL		; R0 = min arg
            50            50     4E  0009  4600 	CVTLF	R0, R0		; REAL*4 it
                                 04  000C  4700 	RET
                                     000D  4800 
                                     000D  4900 ;+
                                     000D  5000 ; MINL returns the smallest of the INTEGER*4 args.
                                     000D  5100 ;-
                                     000D  5200 
                                     000D  5300 MINL:
            51            6C     9A  000D  5400 	MOVZBL	(AP), R1		; R1 = number of args
