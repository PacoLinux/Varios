0-2             FOR$IDATE - return month, day, year as INTEGER*                                                                  (4)

                                     0000   100 	.SBTTL	FOR$IDATE - return month, day, year as INTEGER*2
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;
                                     0000   600 ;	FOR$IDATE uses system service $NUMTIM to get date information.
                                     0000   700 ;	This information is converted to the appropriate type and stored
                                     0000   800 ;	through the addresses passed as parameters.
                                     0000   900 ;
                                     0000  1000 ; CALLING SEQUENCE:
                                     0000  1100 ;
                                     0000  1200 ;	CALL FOR$IDATE (month.ww.r, day.ww.r, year.ww.r)
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;
                                     0000  1600 ;	month.ww.r	adr of word to place month (1-12) integer
                                     0000  1700 ;	day.ww.r	adr of word to place day (1-31) integer
                                     0000  1800 ;	year.ww.r	adr of word to place year (0-99)integer
                                     0000  1900 ;
                                     0000  2000 ; IMPLICIT INPUTS:
                                     0000  2100 ;	NONE
                                     0000  2200 ;
                                     0000  2300 ; OUTPUT PARAMETERS:
                                     0000  2400 ;	NONE
                                     0000  2500 ;
                                     0000  2600 ; IMPLICIT OUTPUTS:
                                     0000  2700 ;	NONE
                                     0000  2800 ;
                                     0000  2900 ; COMPLETION CODES:
                                     0000  3000 ;	NONE
                                     0000  3100 ;
                                     0000  3200 ; SIDE EFFECTS:
                                     0000  3300 ;	SIGNALs Integer Overflow (this should never happen)
                                     0000  3400 ;
                                     0000  3500 ;--
                                     0000  3600 
                                     0000  3700 
                                     0000  3800 
                               4000' 0000  3900 	.ENTRY	FOR$IDATE,	^M<IV>
            5E            10     C2  0002  4100 	SUBL	#16, SP			; save 8 words for $NUMTIM
            50            5E     D0  0005  4200 	MOVL	SP, R0			; R0 -> data area
                                     0008  4300 	$NUMTIM_S	(R0)		; get system time values
            50            8E     3C  0013  4400 	MOVZWL	(SP)+, R0		; R0 = "year since 0"
                          51     D4  0016  4500 	CLRL	R1			; R0 is now a quadword value
            50   00000064 8F     7B  0018  4600 	EDIV	#100, R0, R0, R0	; R0 = two digit year value
            50            50         001F       
         0C BC            50     B0  0021  4700 	MOVW	R0, @12(AP)		; return year
         04 BC            8E     B0  0025  4800 	MOVW	(SP)+, @4(AP)		; return month
         08 BC            6E     B0  0029  4900 	MOVW	(SP), @8(AP)		; return day
                                 04  002D  5000 	RET				; return
                                     002E  5100 
                                     002E  5200 
                                     002E  5300 	.END
