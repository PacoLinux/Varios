                                     001E   152 	.SBTTL	MTH$DACOS_R9 - Special DACOS routine
                                     001E   153 
                                     001E   154 ; Special DACOS - used by the stnadard routine and direct JSB call.
                                     001E   155 ;
                                     001E   156 ; CALLING SEQUENCE:
                                     001E   157 ;	save anything needed in R0:R9
                                     001E   158 ;	MOVD	..., R0			; input in R0/R1
                                     001E   159 ;	JSB	MTH$DACOS_R9
                                     001E   160 ;	RSB				; return with result in R0/R1
                                     001E   161 ;
                                     001E   162 
                                     001E   163 MTH$DACOS_R9::				; special DACOS routine
            58            50     70  001E   164 	MOVD	R0, R8			; save X in R8/R9
            50       8000 8F     AA  0021   165 	BICW	#^X8000, R0		; R0/R1 = |X|
                          50     73  0026   166 	TSTD	R0			; test if |X| > 0
                          05     14  0028   167 	BGTR	TEST_FOR_1.0		; branch if |X| > 0
                                     002A   168 
                                     002A   169 ;
                                     002A   170 ; X = 0
                                     002A   171 ;
                                     002A   172 
            50         D3 AF     70  002A   173 	MOVD	D_PI_OVER_2, R0		; R0/R1 = PI/2
                                 05  002E   174 	RSB				; return PI/2 if |X| = 0
                                     002F   175 
                                     002F   176 ;
                                     002F   177 ; 0 < |X|
                                     002F   178 ;
                                     002F   179 
                                     002F   180 TEST_FOR_1.0:
            08            50     71  002F   181 	CMPD	R0, S^#SD_1.0		; compare |X| with 1.0
                          19     18  0032   182 	BGEQ	GEQ_TO_1.0		; branch if |X| >= 1.0
                                     0034   183 
                                     0034   184 ;
                                     0034   185 ; 0 < |X| < 1.0
                                     0034   186 ;
                                     0034   187 
            58            58     65  0034   188 	MULD3	R8, R8, R0		; R0/R1 = X**2
                          50         0037       
            08            50     63  0038   189 	SUBD3	R0, S^#SD_1.0, R0	; R0/R1 = 1.0 - X**2
                          50         003B       
                 00000000'EF     16  003C   190 	JSB	MTH$DSQRT_R5		; R0/R1 = DSQRT(1-X**2)
            50            58     66  0042   191 	DIVD	R8, R0			; R0/R1 = DSQRT(1-X**2)/X
                 00000000'EF     16  0045   192 	JSB	MTH$DATAN_R7		; R0/R1 = DATAN(DSQRT(1-X**2)/X)
                          04     11  004B   193 	BRB	TEST_SIGN		; branch to TEST_SIGN
                                     004D   194 
                                     004D   195 ;
                                     004D   196 ; 1 =< |X| 
                                     004D   197 ;
                                     004D   198 
                                     004D   199 GEQ_TO_1.0:
                          0B     14  004D   200 	BGTR	ERROR			; branch to ERROR if |X| > 1.0
                                     004F   201 
                                     004F   202 
                                     004F   203 ;
                                     004F   204 ; |X| = 1.0
                                     004F   205 ;
                                     004F   206 
