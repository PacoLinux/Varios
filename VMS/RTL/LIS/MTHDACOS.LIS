                                     0010    91 	.SBTTL	MTH$DACOS  - Standard Double Precision Floating DACOS
                                     0010    92 
                                     0010    93 
                                     0010    94 ;++
                                     0010    95 ; FUNCTIONAL DESCRIPTION:
                                     0010    96 ;
                                     0010    97 ; DACOS  - double precision floating point function
                                     0010    98 ;
                                     0010    99 ; DACOS(X) is computed as:
                                     0010   100 ;
                                     0010   101 ;	If X = 0, then DACOS(X) = PI/2.
                                     0010   102 ;	If X = 1, then DACOS(X) = 0.
                                     0010   103 ;	If X = -1, then DACOS(X) = PI.
                                     0010   104 ;	If 0 < X < 1, then DACOS(X) = ATAN(SQRT(1-X**2)/X).
                                     0010   105 ;	IF -1 < X < 0, then DACOS(X) = ATAN(SQRT(1-X**2)/X) + PI.
                                     0010   106 ;	If 1 < |X|, error.
                                     0010   107 ;
                                     0010   108 ; CALLING SEQUENCE:
                                     0010   109 ;
                                     0010   110 ;	DACOS.wd.v = MTH$DACOS(x.rd.r)
                                     0010   111 ;
                                     0010   112 ; INPUT PARAMETERS:
                                     0010   113 
                           00000004  0010   114 	LONG = 4			; define longword multiplier
                           00000004  0010   115 	x = 1 * LONG			; Contents of x is the argument
                                     0010   116 
                                     0010   117 ; IMPLICIT INPUTS:	none
                                     0010   118 ;
                                     0010   119 ; OUTPUT PARAMETERS:
                                     0010   120 ;
                                     0010   121 ;	VALUE:	double precision floating arc-cosine of the argument
                                     0010   122 ;
                                     0010   123 ; IMPLICIT OUTPUTS:	none
                                     0010   124 ;
                                     0010   125 ; COMPLETION CODES:	none
                                     0010   126 ;
                                     0010   127 ; SIDE EFFECTS:
                                     0010   128 ;
                                     0010   129 ; Signals: MTH$_INVARG if |X| > 1 with reserved operand in R0/R1 (copied
                                     0010   130 ; to the signal mechanism vector CHF$L_MCH_R0/R1 by LIB$SIGNAL).
                                     0010   131 ; Associated message is: "INVALID ARGUMENT". Result is reserved operand -0.0
                                     0010   132 ; unless a user supplied (or any) error handler changes CHF$L_MCH_R0/R1.
                                     0010   133 ;
                                     0010   134 ; NOTE: This procedure disables floating point underflow, enables integer
                                     0010   135 ; overflow.
                                     0010   136 ;
                                     0010   137 ;---
                                     0010   138 
                                     0010   139 
                               43FC' 0010   140 	.ENTRY	MTH$DACOS, ^M<IV, R2, R3, R4, R5, R6, R7, R8, R9>
