                          50     7C  004F   207 	CLRD	R0			; R0/R1 = 0
                                     0051   208 
                                     0051   209 
                                     0051   210 ;
                                     0051   211 ; Test the sign of X in order to decide if add PI to the result
                                     0051   212 ;
                                     0051   213 
                                     0051   214 TEST_SIGN:
                          58     73  0051   215 	TSTD	R8			; test the sign of X
                          04     18  0053   216 	BGEQ	10$			; branch if X > 0
            50         B0 AF     60  0055   217 	ADDD	D_PI, R0		; add PI to R0/R1 if X < 0
                                 05  0059   218 10$:	RSB				; return with result in R0/R1
                                     005A   219 
                                     005A   220 ;
                                     005A   221 ; 1 < |X|, error
                                     005A   222 ;
                                     005A   223 
                          6E     DD  005A   224 ERROR:	PUSHL	(SP)			; return PC from JSB routine
            7E         51 8F     9A  005C   225 	MOVZBL	#MTH__INVARG, -(SP)	; condition value
            01            0F     79  0060   226 	ASHQ	#15, #1, R0		; R0 = result = reserved operand -0.0
                          50         0063       
                                     0064   227 					; goes to signal mechanism vector
                                     0064   228 					; (CHF$L_MCH_R0/R1) so error handler
                                     0064   229 					; can modify the result.
       0000'CF            02     FB  0064   230 	CALLS	#2, W^MTH$$SIGNAL	; signal error and use real user's PC
                                     0069   231 					; independent of CALL vs JSB
                                 05  0069   232 	RSB				; return - R0 restored from CHF$L_MCH_R0/R1
                                     006A   233 
