                                     0002      
            6D       0000'CF     3E  0002       	MOVAW	W^MTH$$JACKET_HND, (FP)
                                     0007       					; set handler address to jacket
                                     0007       					; handler
                                     0007       
            50         04 BC     7D  0007  6500 	MOVQ	@argadr(AP), R0		; R0/R1 = (r, i)
            50       8000 8F     AB  000B  6600 	BICW3	#^X8000, R0, R2		; R2 = ABS(r)
                          52         0010       
            51       8000 8F     AB  0011  6700 	BICW3	#^X8000, R1, R3		; R3 = ABS(i)
                          53         0016       
            53            52     B1  0017  6800 	CMPW	R2, R3			; compare the magnitudes of r and i
                          0D     19  001A  6900 	BLSS	REALLO			; and branch if r is smaller
                          52     53  001C  7000 	TSTF	R2			; quit if r = 0
                          27     13  001E  7100 	BEQL	ZERO
            52            51     D0  0020  7200 	MOVL	R1, R2			; else swap so R0 is smaller
            51            50     D0  0023  7300 	MOVL	R0, R1			; R1 = larger part (r)
            50            52     D0  0026  7400 	MOVL	R2, R0			; R0 = smaller part (i)
                                     0029  7500 
                                     0029  7600 REALLO:
                                     0029  7700 
                                     0029  7800 ; at this point R0 contains MIN (the smaller of |r| and |i|), and
                                     0029  7900 ; R1 contains MAX (the larger of |r| and |i|).
                                     0029  8000 ;
            50            51     46  0029  8100 	DIVF	R1, R0			; R0 = MIN/MAX
            50            50     44  002C  8200 	MULF	R0, R0			; R0 = (MIN/MAX)**2
            50            08     40  002F  8300 	ADDF	#1.0, R0		; R0 = (MIN/MAX)**2 + 1
            53            51     D0  0032  8400 	MOVL	R1, R3			; R3 = MAX
                 00000000'EF     16  0035  8500 	JSB	MTH$SQRT_R2		; R0 = SQRT((MIN/MAX)**2 + 1)
