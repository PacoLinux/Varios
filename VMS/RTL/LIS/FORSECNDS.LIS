                                     0000   100 	.SBTTL	FOR$SECNDS - get delta system time
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;
                                     0000   600 ;	Routine FOR$SECNDS returns the system time in seconds as
                                     0000   700 ;	a single precision floating point value minus the value
                                     0000   800 ;	of its single real argument.
                                     0000   900 
                                     0000  1000 ; CALLING SEQUENCE:
                                     0000  1100 ;
                                     0000  1200 ;	Time_difference.wf.v = FOR$SECNDS( time_origin.rf.r)
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;
                                     0000  1600 ;	The time_origin parameter is single precision floating point
                                     0000  1700 ;	and called by reference.
                                     0000  1800 ;
                                     0000  1900 ; IMPLICIT INPUTS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; OUTPUT PARAMETERS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; IMPLICIT OUTPUTS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; COMPLETION CODES:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; SIDE EFFECTS:
                                     0000  3200 ;
                                     0000  3300 ;	Integer Overflow can occur (but it shouldn't)
                                     0000  3400 ;	Floating Underflow and Overflow can occur.
                                     0000  3500 ;
                                     0000  3600 ;--
FOR$SECNDS      - GET DELTA TIME                                 21-AUG-1978 18:56:31   VAX-11 MACRO X0.3-11               Page   5
0-6             FOR$SECNDS - get delta system time                                                                               (5)

                               4000' 0000   100 	.ENTRY	FOR$SECNDS,	^M<IV>	
            5E            10     C2  0002   400 	SUBL	#2*8, SP		; make an 8 word work space
                                     0005   500 					; for $NUMTIM
            50            5E     D0  0005   600 	MOVL	SP, R0			; R0 -> work space
                                     0008   700 	$NUMTIM_S	(R0)		; get system time values
            5E            06     C0  0013   800 	ADDL	#2*3, SP			; SP -> "hour of day"
            3C            8E     A5  0016   900 	MULW3	(SP)+, #60, R0		; R0 = hours * 60
                          50         0019       
            50            8E     A0  001A  1000 	ADDW	(SP)+, R0		; add in "minutes of hour"
            50            50     3C  001D  1100 	MOVZWL	R0, R0			; get more precision
            50            3C     C4  0020  1200 	MULL	#60, R0			; so this doesn't overflow
            51            8E     3C  0023  1300 	MOVZWL	(SP)+, R1		; convert "seconds of minute"
            50            51     C0  0026  1400 	ADDL	R1, R0			; and add to total time
            50            50     4E  0029  1500 	CVTLF	R0, R0			; convert to floating
            51            6E     4D  002C  1600 	CVTWF	(SP), R1		; R1 = "hundredths of seconds"
            51   000043C8 8F     46  002F  1700 	DIVF	#100, R1		; make a fraction
            50            51     40  0036  1800 	ADDF	R1, R0			; add in hundredths
            50         04 BC     42  0039  1900 	SUBF	@arg(AP), R0		; and get delta time from
                                     003D  2000 					; users' argument
                          07     18  003D  2010 	BGEQ	10$			; branch if not negative
            50   C00048A8 8F     40  003F  2020 	ADDF	#86400., R0		; must have gone by midnight
                                     0046  2030 					; so add # of seconds in 1 day.
                                 04  0046  2100 10$:	RET				; R0 = delta time
                                     0047  2200 
                                     0047  2300 	.END
