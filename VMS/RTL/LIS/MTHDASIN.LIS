                                     001E   151 	.SBTTL	MTH$DASIN_R9 - Special DASIN routine
                                     001E   152 
                                     001E   153 ; Special DASIN - used by the stnadard routine and direct JSB call.
                                     001E   154 ;
                                     001E   155 ; CALLING SEQUENCE:
                                     001E   156 ;	save anything needed in R0:R9
                                     001E   157 ;	MOVD	..., R0			; input in R0/R1
                                     001E   158 ;	JSB	MTH$DASIN_R9
                                     001E   159 ;	RSB				; return with result in R0/R1
                                     001E   160 ;
                                     001E   161 
                                     001E   162 MTH$DASIN_R9::				; special DASIN routine
            58            50     70  001E   163 	MOVD	R0, R8			; save X in R8/R9
            50       8000 8F     AA  0021   164 	BICW	#^X8000, R0		; R0/R1 = |X|
                          50     73  0026   165 	TSTD	R0			; test if |X| > 0
                          01     14  0028   166 	BGTR	TEST_FOR_1.0		; branch if |X| > 0
                                     002A   167 
                                     002A   168 ;
                                     002A   169 ; X = 0
                                     002A   170 ;
                                     002A   171 
                                 05  002A   172 	RSB				; return 0 if |X| = 0
                                     002B   173 
                                     002B   174 ;
                                     002B   175 ; 0 < |X|
                                     002B   176 ;
                                     002B   177 
                                     002B   178 TEST_FOR_1.0:
            08            50     71  002B   179 	CMPD	R0, S^#SD_1.0		; compare |X| with 1.0
                          19     18  002E   180 	BGEQ	GEQ_TO_1.0		; branch if |X| >= 1.0
                                     0030   181 
                                     0030   182 ;
                                     0030   183 ; 0 < |X| < 1.0
                                     0030   184 ;
                                     0030   185 
            58            58     65  0030   186 	MULD3	R8, R8, R0		; R0/R1 = X**2
                          50         0033       
            08            50     63  0034   187 	SUBD3	R0, S^#SD_1.0, R0	; R0/R1 = 1.0 - X**2
                          50         0037       
                 00000000'EF     16  0038   188 	JSB	MTH$DSQRT_R5		; R0/R1 = DSQRT(1-X**2)
            58            50     67  003E   189 	DIVD3	R0, R8, R0		; R0/R1 = X/DSQRT(1-X**2)
                          50         0041       
                 00000000'EF     16  0042   190 	JSB	MTH$DATAN_R7		; R0/R1 = DATAN(X/DSQRT(1-X**2))
                                 05  0048   191 	RSB				; return with result in R0/R1
                                     0049   192 
                                     0049   193 
                                     0049   194 ;
                                     0049   195 ; 1 =< |X| 
                                     0049   196 ;
                                     0049   197 
                                     0049   198 GEQ_TO_1.0:
                          0C     14  0049   199 	BGTR	ERROR			; branch to ERROR if |X| > 1.0
                                     004B   200 
                                     004B   201 
                                     004B   202 ;
                                     004B   203 ; |X| = 1.0
                                     004B   204 ;
