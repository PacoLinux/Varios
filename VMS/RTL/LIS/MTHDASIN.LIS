                          6E     DD  0057   216 ERROR:	PUSHL	(SP)			; return PC from JSB routine
            7E         51 8F     9A  0059   217 	MOVZBL	#MTH__INVARG, -(SP)	; condition value
            01            0F     79  005D   218 	ASHQ	#15, #1, R0		; R0 = result = reserved operand -0.0
