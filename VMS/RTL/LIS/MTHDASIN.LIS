                                     0010    91 	.SBTTL	MTH$DASIN  - Standard Double Precision Floating DASIN
                                     0010    92 
                                     0010    93 
                                     0010    94 ;++
                                     0010    95 ; FUNCTIONAL DESCRIPTION:
                                     0010    96 ;
                                     0010    97 ; DASIN  - double precision floating point function
                                     0010    98 ;
                                     0010    99 ; DASIN(X) is computed as:
                                     0010   100 ;
                                     0010   101 ;	If X = 0, then DASIN(X) = 0.
                                     0010   102 ;	If X = 1, then DASIN(X) = PI/2.
                                     0010   103 ;	If X = -1, then DASIN(X) = -PI/2.
                                     0010   104 ;	If 0 < |X| < 1, then DASIN(X) = ATAN(X/SQRT(1-X**2)).
                                     0010   105 ;	If 1 < |X|, error.
                                     0010   106 ;
                                     0010   107 ; CALLING SEQUENCE:
                                     0010   108 ;
                                     0010   109 ;	DASIN.wd.v = MTH$DASIN(x.rd.r)
                                     0010   110 ;
                                     0010   111 ; INPUT PARAMETERS:
                                     0010   112 
                           00000004  0010   113 	LONG = 4			; define longword multiplier
                           00000004  0010   114 	x = 1 * LONG			; Contents of x is the argument
                                     0010   115 
                                     0010   116 ; IMPLICIT INPUTS:	none
                                     0010   117 ;
                                     0010   118 ; OUTPUT PARAMETERS:
                                     0010   119 ;
                                     0010   120 ;	VALUE:	double precision floating arcsine of the argument
                                     0010   121 ;
                                     0010   122 ; IMPLICIT OUTPUTS:	none
                                     0010   123 ;
                                     0010   124 ; COMPLETION CODES:	none
                                     0010   125 ;
                                     0010   126 ; SIDE EFFECTS:
                                     0010   127 ;
                                     0010   128 ; Signals: MTH$_INVARG if |X| > 1 with reserved operand in R0/R1
                                     0010   129 ; (copied to the signal mechanism vector CHF$L_MCH_R0/R1 by LIB$SIGNAL).
                                     0010   130 ; Associated message is: "INVALID ARGUMENT". Result is reserved
                                     0010   131 ; operand -0.0 unless a user supplied (or any) error handler changes CHF$L_MCH_R0/R1
                                     0010   132 ;
                                     0010   133 ; NOTE: This procedure disables floating point underflow, enables integer
                                     0010   134 ; overflow.
                                     0010   135 ;
                                     0010   136 ;---
                                     0010   137 
                                     0010   138 
                               43FC' 0010   139 	.ENTRY	MTH$DASIN, ^M<IV, R2, R3, R4, R5, R6, R7, R8, R9>
                                     0012   140 					; standard call-by-reference entry
