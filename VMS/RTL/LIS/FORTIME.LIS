                                     0000   100 	.SBTTL	FOR$TIME - return system as an 8 byte string
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Returns system time as an 8 byte string in the form
                                     0000   600 ;	HH:MM:SS.  Build a string descriptor on the stack that points
                                     0000   700 ;	to the users area.  Call system service $ASCTIM to do all
                                     0000   800 ;	the work.
                                     0000   900 ;
                                     0000  1000 ; CALLING SEQUENCE:
                                     0000  1100 ;	CALL	FOR$TIME (8_byte_array.wb.ra)
                                     0000  1200 ;
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;	8_byte_array		the addr of 8 bytes into which the
                                     0000  1600 ;				string is to be placed
                                     0000  1700 ;
                                     0000  1800 ; IMPLICIT INPUTS:
                                     0000  1900 ;	NONE
                                     0000  2000 ;
                                     0000  2100 ; OUTPUT PARAMETERS:
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; IMPLICIT OUTPUTS:
                                     0000  2500 ;	NONE
                                     0000  2600 ;
                                     0000  2700 ; COMPLETION CODES:
                                     0000  2800 ;	NONE
                                     0000  2900 ;
                                     0000  3000 ; SIDE EFFECTS:
                                     0000  3100 ;	NONE
                                     0000  3200 ;
                                     0000  3300 ;--
                                     0000  3400 
                                     0000  3500 
                               0000' 0000  3600 	.ENTRY	FOR$TIME,	^M<>
            7E         04 AC     D0  0002  3800 	MOVL	4(AP), -(SP)		; build a string descriptor
            7E   010E0008 8F     D0  0006  3900 	MOVL	#^X10E0008, -(SP)	; len=8
            50            5E     D0  000D  4000 	MOVL	SP, R0			; R0 -> string descriptor
                                     0010  4100 	$ASCTIM_S	,(R0),,#1	; get time info only
                                 04  001F  4200 	RET				; return to user
                                     0020  4300 
                                     0020  4400 
                                     0020  4500 	.END
