                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Return the minimum of n arguments, n is greater than or equal
                                     0000   600 ;	to 1.
                                     0000   700 ;
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;	Minimum.wd.v = MTH$DMIN1 ({arg.rd.r})
                                     0000  1100 ;	
                                     0000  1200 ;
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;	The n parameters are double-precision floating point values
                                     0000  1600 ;	and are call-by-reference.
                                     0000  1700 ;	
                                     0000  1800 ;
                                     0000  1900 ; IMPLICIT INPUTS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; OUTPUT PARAMETERS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; IMPLICIT OUTPUTS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; COMPLETION CODES:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; SIDE EFFECTS:
                                     0000  3200 ;	Reserved Operand exception can occur.
                                     0000  3300 ;	
                                     0000  3400 ;
                                     0000  3500 ;--
                                     0000  3600 
                               0004' 0000  3700 	.ENTRY	MTH$DMIN1,	^M<R2>
            52            6C     9A  0002  3900 	MOVZBL	(AP), R2	; R2 = arg count
                          8C     D5  0005  4000 	TSTL	(AP)+		; AP -> first arg
            50            9C     70  0007  4100 1$:	MOVD	@(AP)+, R0	; R0/R1 = trial min
                          08     11  000A  4200 	BRB	3$		; check arg count
            50         00 BC     71  000C  4300 2$:	CMPD	@0(AP), R0	; if this arg is less than trial min
                          F5     19  0010  4400 	BLSS	1$		; then it becomes new trial min
                          8C     D5  0012  4500 	TSTL	(AP)+		; else ignore it
            F5            52     F5  0014  4600 3$:	SOBGTR	R2, 2$		; return if arg count exausted
                                 04  0017  4700 	RET			; with min in R0/R1
                                     0018  4800 
                                     0018  4900 
                                     0018  5000 	.END
