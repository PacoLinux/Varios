                                     0000    86 	.SBTTL	IRAD50 - CONVERT HOLLERITH STRINGS TO RADIX-50 REPRESATATION
                                     0000    87 
                                     0000    88 ;++
                                     0000    89 ; FUNCTIONAL DESCRIPTION:
                                     0000    90 ;
                                     0000    91 ; 	Algorithmic steps:
                                     0000    92 ;	1) Initialization
                                     0000    93 ;	   CHARS_REM = max_char_cnt.rbu.ra
                                     0000    94 ;	   NEXT_INPUT_POSITION = char_array.rbu.ra
                                     0000    95 ;	   NEXT_OUTPUT_POSITION = radix50_array.rbu.ra
                                     0000    96 ;	   ACTUAL_CHAR_COUNT = 0
                                     0000    97 ;	2) Call COM$$R50WD_R6 to convert one word at a time.
                                     0000    98 ;	   If CHARS_REM =< 0 then return with function_value = ACTUAL_CHAR_COUNT,
                                     0000    99 ;	NOTE: Three characters of ASCII input are packed into each word
                                     0000   100 ;	of output in radix-50 format. The number of output word modified
                                     0000   101 ;	is computed by the expression (in integer mode) (ICNT+2)/3.
                                     0000   102 ;  
; CALLING SEQUENCE:
                                     0000   103 ;
                                     0000   104 ;	[no_char_conv.ww.v] = IRAD50 (max_char_cnt.rw.r, char_array.rbu.ra,
                                     0000   105 ;				radix50_array.wbu.ra)
                                     0000   106 
                           00000004  0000   107 	max_char_cnt	= 4		; max_char_cnt.rw.r
                           00000008  0000   108 	char_array	= 8		; char_array.rbu.ra
                           0000000C  0000   109 	radix50_array	= 12		; radix50_array.rbu.ra
                                     0000   110 ;
                                     0000   111 
                                     0000   112 ;
                                     0000   113 ; INPUT PARAMETERS:
                                     0000   114 
                                     0000   115 ;	max_char_cnt.rw.r		; maximum number of chars to convert
                                     0000   116 ;	char_array.rbu.ra		; ascii string to be converted
                                     0000   117 ;
                                     0000   118 ; IMPLICIT INPUTS:
                                     0000   119 ;	NONE
                                     0000   120 ;
                                     0000   121 ; OUTPUT PARAMETERS:
                                     0000   122 ;
                                     0000   123 ;	radix50_array.wbu.ra		; output location for the result
                                     0000   124 ;					; of the conversion
                                     0000   125 ;
                                     0000   126 ; IMPLICIT OUTPUTS:
                                     0000   127 ;	NONE
                                     0000   128 ;
                                     0000   129 ; COMPLETION CODES:
                                     0000   130 ;	NONE
                                     0000   131 ;
                                     0000   132 ; SIDE EFFECTS:
                                     0000   133 ;	NONE
                                     0000   134 ;
                                     0000   135 ;--
                                     0000   136 
                                     0000   137 
                                     0000   138 
                               007C' 0000   139 	.ENTRY	IRAD50, ^M<R2, R3, R4, R5, R6>
                                     0002   140 					; standard call-by-reference entry
                                     0002   141 
                                     0002   142 ;
COM$IRAD50      ; FORTRAN COMPATIBILITY - ASCII to RAD50 conver  21-AUG-1978 18:53:20   VAX-11 MACRO X0.3-11               Page   5
1-1             IRAD50 - CONVERT HOLLERITH STRINGS TO RADIX-50                                                                   (4)

                                     0002   143 ; Initialization
                                     0002   144 ;
                                     0002   145 
            55         04 BC     3C  0002   146 	MOVZWL	@max_char_cnt(AP), R5	; R5 = maximum number of chars
                                     0006   147 					; to be converted
            52         08 AC     D0  0006   148 	MOVL	char_array(AP), R2	; R2 = address of input string
            54         0C AC     D0  000A   149 	MOVL	radix50_array(AP), R4	; R4 = address of the output location
                          50     D4  000E   150 	CLRL	R0			; R0 = ACTUAL_CHAR_COUNT = 0
                                     0010   151 
                                     0010   152 ;
                                     0010   153 ; If CHARS_REM =< 0 then return with function_value equal to ACTUAL_CHAR_COUNT
                                     0010   154 ; else call R50WD_R5 to convert one word at a time.
                                     0010   155 ;
                                     0010   156 
                                     0010   157 10$:
                 00000000'EF     16  0010   158 	JSB	COM$$R50WD_R6		; convert one word at a time
            84            51     B0  0016   159 	MOVW	R1, (R4)+		; output one word at a time
                          55     D5  0019   160 	TSTL	R5			; any more?
                          F3     14  001B   161 	BGTR	10$			; branch if so
                                 04  001D   162 	RET				; return with R0 = ACTUAL_CHAR_COUNT
                                     001E   163 
                                     001E   164 
                                     001E   165 	.END
