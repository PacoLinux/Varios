COM$RAD50       ; FORTRAN COMPATIBILITY - ASCII to RAD50 conver  21-AUG-1978 18:53:26   VAX-11 MACRO X0.3-11               Page   4
1-1             RAD50 - CONVERT 6-CHAR ASCII STRING INTO RADIX-                                                                  (4)

                                     0000    87 	.SBTTL	RAD50 - CONVERT 6-CHAR ASCII STRING INTO RADIX-50 STRING
                                     0000    88 
                                     0000    89 
                                     0000    90 ;++
                                     0000    91 ; FUNCTIONAL DESCRIPTION:
                                     0000    92 ;
                                     0000    93 ; 	Algorithmic steps:
                                     0000    94 ;	1) Initialization
                                     0000    95 ;	   CHARS_REM = 6
                                     0000    96 ;	   NEXT_INPUT_POSITION = char_array.rbu.ra
                                     0000    97 ;	2) Call COM$$R50WD_R6 to convert one word at a time.
                                     0000    98 ;
                                     0000    99 ; CALLING SEQUENCE:
                                     0000   100 ;
                                     0000   101 ;	radix50_array.wbu.v = RAD50 (ascii_array.rbu.ra)
                           00000004  0000   102 	ASCII_ARRAY = 4		; arg list offset
                                     0000   103 ;
                                     0000   104 
                                     0000   105 ;
                                     0000   106 ; INPUT PARAMETERS:
                                     0000   107 
                                     0000   108 ;	ascii_array.rbu.ra		; ascii string to be converted
                                     0000   109 ;
                                     0000   110 ; IMPLICIT INPUTS:
                                     0000   111 ;	NONE
                                     0000   112 ;
                                     0000   113 ; OUTPUT PARAMETERS:
                                     0000   114 ;
                                     0000   115 ;	radix50_array.wlu.v		; output location for the result
                                     0000   116 ;					; of the conversion
                                     0000   117 ;
                                     0000   118 ; IMPLICIT OUTPUTS:
                                     0000   119 ;	NONE
                                     0000   120 ;
                                     0000   121 ; COMPLETION CODES:
                                     0000   122 ;	NONE
                                     0000   123 ;
                                     0000   124 ; SIDE EFFECTS:
                                     0000   125 ;	NONE
                                     0000   126 ;
                                     0000   127 ;--
                                     0000   128 
                               007C' 0000   129 	.ENTRY	RAD50, ^M<R2, R3, R4, R5, R6>
                                     0002   130 					; standard call-by-reference entry
                                     0002   131 
                                     0002   132 ;
                                     0002   133 ; Initialization
                                     0002   134 ;
                                     0002   135 
            55            06     D0  0002   136 	MOVL	#6, R5			; CHARS_REM = 6
            52         04 AC     D0  0005   137 	MOVL	ascii_array(AP), R2	; R2 = NEXT_INPUT_POSITION
                                     0009   138 
                                     0009   139 ;
                                     0009   140 ; Call COM$$R50WD_R6
                                     0009   141 ;
                                     0009   142 
                 00000000'EF     16  0009   143 	JSB	COM$$R50WD_R6		; convert first word
COM$RAD50       ; FORTRAN COMPATIBILITY - ASCII to RAD50 conver  21-AUG-1978 18:53:26   VAX-11 MACRO X0.3-11               Page   5
1-1             RAD50 - CONVERT 6-CHAR ASCII STRING INTO RADIX-                                                                  (4)

                                     000F   144 
                                     000F   145 ;
                                     000F   146 ; Save RADIX_VALUE on stack and call COM$$R50WD_R6 again
                                     000F   147 ;
                                     000F   148 
                          51     DD  000F   149 	PUSHL	R1			; save RADIX_VALUE on stack
                 00000000'EF     16  0011   150 	JSB	COM$$R50WD_R6		; call COM$$R50WD_R6 to convert 2nd word
                                     0017   151 
                                     0017   152 ;
                                     0017   153 ; LP(R0) = saved RADIX_VALUE on stack (1st word)
                                     0017   154 ; HP(R0) = RADIX_VALUE (2nd word)
                                     0017   155 ;
                                     0017   156 
            10            51     F0  0017   157 	INSV	R1, #16, #16, (SP)	; HP(SP) = first word converted
            6E            10         001A       
            50            6E     D0  001C   158 	MOVL	(SP), R0		; giving result in R0
                                 04  001F   159 	RET				; return with result in R0
                                     0020   160 
                                     0020   161 
                                     0020   162 	.END
