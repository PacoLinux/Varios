                               0000' 0000   156 	.ENTRY	MTH$RANDOM,0		;no registers save, clear IV
                                     0002   157 
                                     0002   158 ;-
                                     0002   159 ; If this were to be placed as an inline expansion, then
                                     0002   160 ; EMUL SEED,#69069,#1,R0 should replace the next two
                                     0002   161 ; instructions because this would prevent the possibility
                                     0002   162 ; of integer overflow trapping.
                                     0002   163 ;-
                                     0002   164 
         04 BC   00010DCD 8F     C4  0002   165 	MULL2	#69069,@SEED(AP)	;update seed with multiplier
                       04 BC     D6  000A   166 	INCL	@SEED(AP)		;increment seed to protect
                                     000D   167 					; against strange seeds
                                     000D   168 
                                     000D   169 ;
                                     000D   170 ; The next instructions convert the seed from unsigned integer
                                     000D   171 ; to floating point in the range 0.0 to 1.0 exclusive.
                                     000D   172 ;
                                     000D   173 
            18            08     EF  000D   174 	EXTZV	#8,#24,@SEED(AP),R0	;Get the most significant bits
            50         04 BC         0010       
                                     0013   175 					; of the seed in the range
                                     0013   176 					; 0 .. (2**24)-1
            50            50     4E  0013   177 	CVTLF	R0,R0			;Convert to floating without
                                     0016   178 					; rounding. The result is
                                     0016   179 					; positive and in the range
                                     0016   180 					; 0.0 .. (2.0**24)-1.0
                                     0016   181 
                                     0016   182 ;
                                     0016   183 ; If this were to be placed as an inline expansion, then
                                     0016   184 ; MULF #^X00003480,R0 could replace the next two instructions.
                                     0016   185 ;
                                     0016   186 
                          05     13  0016   187 	BEQL	10$			;If zero, already correct
            50       0C00 8F     A2  0018   188 	SUBW	#24@7,R0		;DIVF #^F2.0**24
                                     001D   189 					; the result is now in the
                                     001D   190 					; range 0.0 .. 1.0 exclusive
                                     001D   191 
                                 04  001D   192 10$:	RET
