0-3             MTH$CONJG - return COMPLEX conjugate                                                                             (4)

                                     0000   100 	.SBTTL	MTH$CONJG - return COMPLEX conjugate
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Returns the complex conjugate of COMPLEX*8 number (r,i).
                                     0000   600 ;	Result is (r,-i).
                                     0000   700 ;
                                     0000   800 ;
                                     0000   900 ; CALLING SEQUENCE:
                                     0000  1000 ;	Complex_conjugate.wfc.v =MTH$CONJG (complex_arg.rfc.r)
                                     0000  1100 ;	
                                     0000  1200 ;
                                     0000  1300 ;
                                     0000  1400 ; INPUT PARAMETERS:
                                     0000  1500 ;	The input parameter is a FORTRAN COMPLEX*8 value and is a
                                     0000  1600 ;	call-by-reference.
                                     0000  1700 ;	
                                     0000  1800 ;
                                     0000  1900 ; IMPLICIT INPUTS:
                                     0000  2000 ;	NONE
                                     0000  2100 ;
                                     0000  2200 ; OUTPUT PARAMETERS:
                                     0000  2300 ;	NONE
                                     0000  2400 ;
                                     0000  2500 ; IMPLICIT OUTPUTS:
                                     0000  2600 ;	NONE
                                     0000  2700 ;
                                     0000  2800 ; COMPLETION CODES:
                                     0000  2900 ;	NONE
                                     0000  3000 ;
                                     0000  3100 ; SIDE EFFECTS:
                                     0000  3200 ;	Reserved Operand exception can occur.
                                     0000  3300 ;	
                                     0000  3400 ;
                                     0000  3500 ;--
                                     0000  3600 
                                     0000  3700 
                                     0000  3800 
                               0000' 0000  3900 	.ENTRY	MTH$CONJG,	^M<>
            51         04 AC     D0  0002  4100 	MOVL	4(AP), R1		; R1 -> COMPLEX parameter
            50            81     50  0006  4200 	MOVF	(R1)+, R0		; R0 = real part
            51            61     52  0009  4300 	MNEGF	(R1), R1		; R1 = - imag part
                                 04  000C  4400 	RET
                                     000D  4500 
                                     000D  4600 
                                     000D  4700 	.END
