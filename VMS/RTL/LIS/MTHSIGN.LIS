                                     0000   100 	.SBTTL	MTH$SIGN
                                     0000   200 
                                     0000   300 ;++
                                     0000   400 ; FUNCTIONAL DESCRIPTION:
                                     0000   500 ;	Return arg1 with sign of arg2.
                                     0000   600 ;
                                     0000   700 ;
                                     0000   800 ; CALLING SEQUENCE:
                                     0000   900 ;	Transfer_of_sign.wf.v = MTH$SIGN (arg1.rf.r, arg2.rf.r)
                                     0000  1000 ;	
                                     0000  1100 ;
                                     0000  1200 ;
                                     0000  1300 ; INPUT PARAMETERS:
                                     0000  1400 ;	The two parameters are single-precision floating-point values
                                     0000  1500 ;	and are call-by-reference.
                                     0000  1600 ;	
                                     0000  1700 ;
                                     0000  1800 ; IMPLICIT INPUTS:
                                     0000  1900 ;	NONE
                                     0000  2000 ;
                                     0000  2100 ; OUTPUT PARAMETERS:
                                     0000  2200 ;	NONE
                                     0000  2300 ;
                                     0000  2400 ; IMPLICIT OUTPUTS:
                                     0000  2500 ;	NONE
                                     0000  2600 ;
                                     0000  2700 ; COMPLETION CODES:
                                     0000  2800 ;	NONE
                                     0000  2900 ;
                                     0000  3000 ; SIDE EFFECTS:
                                     0000  3100 ;	Reserved Operand exception can occur.
                                     0000  3200 ;	
                                     0000  3300 ;
                                     0000  3400 ;--
                                     0000  3500 
                               0000' 0000  3600 	.ENTRY	MTH$SIGN,	^M<>
                                     0002  3800 
            50         04 BC     50  0002  3900 	MOVF	@4(AP), R0	; R0 = arg1
                          09     19  0006  4000 	BLSS	NEGARG		; branch if negative arg1
                                     0008  4100 
                                     0008  4200 ; arg1 is non-negative, check sign of arg2
                                     0008  4300 ;
                       08 BC     53  0008  4400 	TSTF	@8(AP)
                          03     18  000B  4500 	BGEQ	EXIT		; both args positive, nothing to do
                                     000D  4600 NEGATE:
            50            50     52  000D  4700 	MNEGF	R0, R0		; negate arg1
                                     0010  4800 EXIT:
                                 04  0010  4900 	RET
                                     0011  5000 
                                     0011  5100 ; arg1 is negative, check sign of arg2
                                     0011  5200 ;
                                     0011  5300 NEGARG:
                       08 BC     53  0011  5400 	TSTF	@8(AP)
                          F7     18  0014  5500 	BGEQ	NEGATE		; arg2 positive negate arg1
                                 04  0016  5600 	RET
                                     0017  5700 
                                     0017  5800 
