                                     0000    63 	.SBTTL	LIB$CRC  - calculate cyclic redundancy check 
                                     0000    64 ;++
                                     0000    65 ; FUNCTIONAL DESCRIPTION:
                                     0000    66 ;
                                     0000    67 ;	The CRC of the data stream specified is calculated and returned.
                                     0000    68 ;
                                     0000    69 ; CALLING SEQUENCE:
                                     0000    70 ;
                                     0000    71 ;	crc.wl.v = LIB$CRC (table.rl.ra, inicrc.rl.r, stream.rt.dx)
                                     0000    72 ;
                           00000004  0000    73 	TABLE = 4				; Adr. of table desc.
                           00000008  0000    74 	INICRC = 8				; Adr. of adr. of initial CRC longword
                           0000000C  0000    75 	STREAM = 12				; Adr. of data stream desc.
                                     0000    76 
                                     0000    77 ; INPUT PARAMETERS:
                                     0000    78 ;
                                     0000    79 ;	NONE
                                     0000    80 ;
                                     0000    81 ; IMPLICIT INPUTS:
                                     0000    82 ;
                                     0000    83 ;	NONE
                                     0000    84 ;
                                     0000    85 ; OUTPUT PARAMETERS:
                                     0000    86 ;
                                     0000    87 ;	NONE
                                     0000    88 ;
                                     0000    89 ; IMPLICIT OUTPUTS:
                                     0000    90 ;
                                     0000    91 ;	NONE
                                     0000    92 ;
                                     0000    93 ; FUNCTION VALUE:
                                     0000    94 ;
                                     0000    95 ;	crc
                                     0000    96 ;
                                     0000    97 ; SIDE EFFECTS:
                                     0000    98 ;
                                     0000    99 ;	NONE
                                     0000   100 ;
                                     0000   101 ;--
                                     0000   102 
                               0000' 0000   103 	.ENTRY LIB$CRC, ^M< >		; Entry point
                                     0002   104 
            50         0C BC     7D  0002   105 	MOVQ	@STREAM(AP), R0			; R0/R1 = stream len and adr.
         08 BC         04 BC     0B  0006   106 	CRC	@TABLE(AP), @INICRC(AP), R0, -	; Calculate CRC
                          50         000B       
                          61         000C   107 		(R1)
                                 04  000D   108 	RET					; return result in R0
                                     000E   109 	.END
