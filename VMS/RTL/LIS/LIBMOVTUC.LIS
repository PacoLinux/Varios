00-03           LIB$MOVTUC  - translate and move until escape f                                                                  (3)

                                     0000    66 	.SBTTL	LIB$MOVTUC  - translate and move until escape found 
                                     0000    67 ;++
                                     0000    68 ; FUNCTIONAL DESCRIPTION:
                                     0000    69 ;
                                     0000    70 ;	Each character of the source string is used as an index into the translation
                                     0000    71 ;	table.  If the translation character is the escape character then the move halts.
                                     0000    72 ;	Otherwise the translated character is placed into the destination string.
                                     0000    73 ;	Translation continues until either the source or destination strings are
                                     0000    74 ;	exhausted, or an escape character is found.
                                     0000    75 ;	If an escape character is found, the relative index of the character in the
                                     0000    76 ;	source string which translated into the escape character is returned.  Otherwise
                                     0000    77 ;	a zero is returned.  If either string is of zero length, then a zero is returned.
                                     0000    78 ;
                                     0000    79 ; CALLING SEQUENCE:
                                     0000    80 ;
                                     0000    81 ;	esc_index.wlu.v = LIB$MOVTUC (src.rt.dx, esc.rt.dx, table.rt.dx, dst.wt.dx
                                     0000    82 ;					(,fill.rt.dx))
                                     0000    83 ;
                           00000004  0000    84 	SOURCE = 4				; Adr of source string desc.
                           00000008  0000    85 	ESC = 8					; Adr of escape string desc
                           0000000C  0000    86 	TABLE = 12				; Adr of translation table desc
                           00000010  0000    87 	DEST = 16				; Adr of destination string desc
                           00000014  0000    88 	FILL = 20				; Adr of fill character desc
                                     0000    89 
                                     0000    90 ; INPUT PARAMETERS:
                                     0000    91 ;
                                     0000    92 ;	NONE
                                     0000    93 ;
                                     0000    94 ; IMPLICIT INPUTS:
                                     0000    95 ;
                                     0000    96 ;	NONE
                                     0000    97 ;
                                     0000    98 ; OUTPUT PARAMETERS:
                                     0000    99 ;
                                     0000   100 ;	NONE
                                     0000   101 ;
                                     0000   102 ; IMPLICIT OUTPUTS:
                                     0000   103 ;
                                     0000   104 ;	NONE
                                     0000   105 ;
                                     0000   106 ; FUNCTION VALUE:
                                     0000   107 ;
                                     0000   108 ;	esc_index.wlu.v
                                     0000   109 ;
                                     0000   110 ; SIDE EFFECTS:
                                     0000   111 ;
                                     0000   112 ;	NONE
                                     0000   113 ;
                                     0000   114 ;--
                                     0000   115 
                               003C' 0000   116 	.ENTRY LIB$MOVTUC , ^M<R2, R3, R4, R5>		; Entry point
                                     0002   117 
            50         04 BC     7D  0002   118 	MOVQ	@SOURCE(AP), R0			; R0/R1 = source len and adr
            52         08 AC     D0  0006   119 	MOVL	ESC(AP), R2			; R2 points to escape desc
            53         0C AC     D0  000A   120 	MOVL	TABLE(AP), R3			; R3 points to table desc
            54         10 BC     7D  000E   121 	MOVQ	@DEST(AP), R4			; R4/R5 = dest len and adr
            61            50     2F  0012   122 	MOVTUC	R0, (R1), @4(R2), @4(R3), -	; translate and move until escape
