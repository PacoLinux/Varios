         04 B3         04 B2         0015       
                                     0019       
            65            54         0019   123 		R4, (R5)
                          07     1C  001B   124 	BVC	10$				; branch if no escape
         04 BC            50     A3  001D   125 	SUBW3	R0, @SOURCE(AP), R0		; R0 = length of source - no. of bytes
                          50         0021       
                                     0022   126 						; remaining in source including
                                     0022   127 						;  escape character
                          50     D6  0022   128 	INCL	R0				; 1 - origin
            05            6C     91  0024   129 10$:	CMPB	(AP), #5			; Check for fill character
                          12     12  0027   130 	BNEQ	20$				; branch if no fill
            7E            50     3C  0029   131 	MOVZWL	R0, -(SP)			; save index on stack
            51         14 AC     D0  002C   132 	MOVL	FILL(AP), R1			; R1 points to FILL desc
         04 B1            01     2C  0030   133 	MOVC5	#1, @4(R1), @4(R1), R4, (R5)	; fill destination string
            54         04 B1         0034       
                          65         0037       
            50            8E     D0  0038   134 	MOVL	(SP)+, R0			; R0 = return value
                                 04  003B   135 20$:	RET
                                     003C   136 	.END
