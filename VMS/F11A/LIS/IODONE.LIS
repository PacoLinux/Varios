       0000'CF            2C     2C  003A   114 	MOVC5	#FIB$C_LENGTH,W^LOCAL_FIB,#0,-
                                     003F       
         0A A4            00         003F   115 		<ABD$C_FIB*ABD$C_LENGTH>+ABD$W_COUNT(R4),1(R2)
                       01 A2         0042       
                                     0044   116 					; COPY LOCAL FIB BACK INTO PACKET
         2A A6            01     E2  0044   117 	BBSS	#IRP$V_FUNC,IRP$W_STS(R6),30$ ; IF READ BIT IS SET, KEEP
                          15         0048       
         32 A6            05     B0  0049   118 	MOVW	#ABD$C_ATTRIB,IRP$W_BCNT(R6) ; ELSE DUMP ATTRIBUTE TEXT
                          0F     11  004D   119 	BRB	30$
                                     004F   120 ;
                                     004F   121 ; FOR READ/WRITE PHYSICAL, KNOCK DOWN THE VIRTUAL BIT IN THE PACKET. ONLY
                                     004F   122 ; ERRORS COME THROUGH HERE, AND WE DON'T WANT TO SEE THEM AGAIN (I/O POST
                                     004F   123 ; RECYCLES VIRTUAL I/O ERRORS FOR ACP ERROR PROCESSING). WE ALSO RESTORE
                                     004F   124 ; THE ORIGINAL SVAPTE AND BYTE COUNT FROM WHERE THEY WERE SAVED IN THE PACKET
                                     004F   125 ; SO THAT THE PAGES CAN BE UNLOCKED.
                                     004F   126 ;
         2A A6            04     E5  004F   127 10$:	BBCC	#IRP$V_VIRTUAL,IRP$W_STS(R6),20$ ; CLEAR THE VIRTUAL BIT
                          00         0053       
         2C A6         44 A6     D0  0054   128 20$:	MOVL	IRP$L_DIAGBUF(R6),IRP$L_SVAPTE(R6) ; RESTORE ORIGINAL PAGE ADDRESS
         32 A6         3E A6     B0  0059   129 	MOVW	IRP$W_OBCNT(R6),IRP$W_BCNT(R6)	   ; AND BYTE COUNT FOR PAGE UNLOCK
                                     005E   130 
                                     005E   131 
   00000000'FF            66     0E  005E   132 30$:	INSQUE	(R6),@IOC$GL_PSBL	; INSERT PACKET INTO QUEUE
                          03     12  0065   133 	BNEQ	40$			; BRANCH IF QUEUE NOT EMPTY
                                     0067   134 	SOFTINT	#IPL$_IOPOST		; SIGNAL I/O POST INTERRUPT
       0000'CF            00     FB  006A   135 40$:	CALLS	#0,W^CHECK_DISMOUNT	; CHECK THE VOLUME FOR DISMOUNT
                                 04  006F   136 	RET
                                     0070   137 
                                     0070   138 
                                     0070   139 
                                     0070   140 	.END
