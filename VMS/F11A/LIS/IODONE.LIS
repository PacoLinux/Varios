                                     0000    59 ;++
                                     0000    60 ;
                                     0000    61 ; FUNCTIONAL DESCRIPTION:
                                     0000    62 ;
                                     0000    63 ;	THIS ROUTINE POSTS I/O COMPLETION FOR THE INDICATED FCP REQUEST.
                                     0000    64 ;
                                     0000    65 ; CALLING SEQUENCE:
                                     0000    66 ;	CALL	IODONE (ARG1)
                                     0000    67 ;
                                     0000    68 ; INPUT PARAMETERS:
                                     0000    69 ;	ARG1: ADDRESS OF I/O PACKET
                                     0000    70 ;
                                     0000    71 ; IMPLICIT INPUTS:
                                     0000    72 ;	USER_STATUS: STATUS OF I/O REQUEST
                                     0000    73 ;
                                     0000    74 ; OUTPUT PARAMETERS:
                                     0000    75 ;	NONE
                                     0000    76 ;
                                     0000    77 ; IMPLICIT OUTPUTS:
                                     0000    78 ;	IOC$GL_PSBL: TAIL OF I/O POST QUEUE
                                     0000    79 ;
                                     0000    80 ; ROUTINE VALUE:
                                     0000    81 ;	NONE
                                     0000    82 ;
                                     0000    83 ; SIDE EFFECTS:
                                     0000    84 ;	I/O PACKET PLACED ON I/O POST QUEUE
                                     0000    85 ;	VOLUME CHECKED FOR DISMOUNT
                                     0000    86 ;
                                     0000    87 ;--
                                     0000    88 
                                 00000000    89 	.PSECT	$CODE$,NOWRT,LONG
                                     0000    90 
                                     0000    91 IO_DONE::
                               00FC  0000    92 	.WORD	^M<R2,R3,R4,R5,R6,R7>	; SAVE REGISTERS
            56         04 AC     D0  0002    93 	MOVL	PACKET(AP),R6		; GET PACKET ADDRESS
         34 A6       0000'CF     7D  0006    94 	MOVQ	W^USER_STATUS,IRP$L_MEDIA(R6) ; SET STATUS IN PACKET
                          00     EF  000C    95 	EXTZV	#IRP$V_FCODE,#IRP$S_FCODE,-
         20 A6            06         000E    96 		IRP$W_FUNC(R6),R7	; GET FUNCTION CODE WITHOUT QUALIFIERS
                          57         0011       
            0C            57     91  0012    97 	CMPB	R7,#IO$_READPBLK	; IF READ PHYSICAL
                          38     13  0015    98 	BEQL	10$
            0B            57     91  0017    99 	CMPB	R7,#IO$_WRITEPBLK	; OR WRITE DO SPECIAL PROCESSING
                          33     13  001A   100 	BEQL	10$
                                     001C   101 ;
                                     001C   102 ; POST PROCESSING FOR ALL ACP FUNCTIONS: BUMP DOWN THE VOLUME TRANSACTION
                                     001C   103 ; COUNT AND DO THE FIXUPS FOR THE BUFFER PACKET.
                                     001C   104 ;
            54       0000'CF     D0  001C   105 	MOVL	W^CURRENT_VCB,R4	; GET VCB ADDRESS
                       0C A4     B7  0021   106 	DECW	VCB$W_TRANS(R4)		; DEDUCT THIS REQ FROM TRANS COUNT
         2A A6            03     E1  0024   107 	BBC	#IRP$V_COMPLX,IRP$W_STS(R6),30$ ; BRANCH IF NO BUFFER PACKET
                          35         0028       
            54         2C B6     D0  0029   108 	MOVL	@IRP$L_SVAPTE(R6),R4	; GET BUFFER DESCRIPTOR ADDRESS
                       12 A4     B4  002D   109 	CLRW	<ABD$C_NAME*ABD$C_LENGTH>+ABD$W_COUNT(R4)
                                     0030   110 					; INHIBIT WRITE-BACK OF NAME STRING
            52         08 A4     9E  0030   111 	MOVAB	<ABD$C_FIB*ABD$C_LENGTH>+ABD$W_TEXT(R4),R2
            53            62     3C  0034   112 	MOVZWL	(R2),R3			; GET OFFSET ADDRESS OF FIB IN PACKET
            52            53     C0  0037   113 	ADDL	R3,R2			; COMPUTE ABSOLUTE ADDRESS
IODONE          - POST REQUEST DONE TO USER                      21-AUG-1978 22:46:57   VAX-11 MACRO X0.3-11               Page   3
