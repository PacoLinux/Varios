SVAPTE          - SYSTEM VIRTUAL ADDRESS OF PAGE TABLE ENTRY     22-AUG-1978 00:18:05   VAX-11 MACRO X0.3-11               Page   3
00.1            PTEADR, PTEINDX - PAGE TABLE ENTRY ADDRESS OR I                                                                  (2)

                                     0000    78 	.SBTTL	PTEADR, PTEINDX - PAGE TABLE ENTRY ADDRESS OR INDEX
                                     0000    79 ;++
                                     0000    80 ; FUNCTIONAL DESCRIPTION:
                                     0000    81 ;
                                     0000    82 ;	GIVEN A VIRTUAL ADDRESS (SYSTEM OR PROCESS), CALCULATE
                                     0000    83 ;	THE SYSTEM VIRTUAL ADDRESS OF THE PAGE TABLE ENTRY.
                                     0000    84 ;
                                     0000    85 ; CALLING SEQUENCE:
                                     0000    86 ;
                                     0000    87 ;	BSBW	MMG$PTEREF		;FAULT PTE, RETURN SYS VIRT ADR OF PTE
                                     0000    88 ;					;RETURN IPL=SYNCH, R0 = SYSTEM STATUS
                                     0000    89 ;
                                     0000    90 ; INPUT PARAMETERS:
                                     0000    91 ;
                                     0000    92 ;	R2 = PROCESS VIRTUAL ADDRESS (P0 OR P1 SPACE)
                                     0000    93 ;	R4 = PROCESS CONTROL BLOCK ADDRESS
                                     0000    94 ;	R5 = PROCESS HEADER ADDRESS
                                     0000    95 ;		THIS MAY BE EITHER THE SYSTEM SPACE OR P1 SPACE ADDRESS
                                     0000    96 ;		OF THE PROCESS HEADER, BUT IF IT IS THE SYSTEM SPACE ADDRESS
                                     0000    97 ;		THE CALLING IPL MUST BE AT SYNCH TO PREVENT SWAPPING
                                     0000    98 ;	IPL LEQ SYNCH - IF REQUESTING THE ADDRESS OF THE PAGE TABLE ENTRY
                                     0000    99 ;		OR IF THE SYSTEM SPACE ADDRESS OF THE PROCESS HEADER IS
                                     0000   100 ;		SPECIFIED, IPL MUST BE AT SYNCH TO PREVENT SWAPPING
                                     0000   101 ;
                                     0000   102 ;  OR
                                     0000   103 ;
                                     0000   104 ;	R2 = SYSTEM SPACE VIRTUAL ADDRESS
                                     0000   105 ;	R4 = SYSTEM PROCESS CONTROL BLOCK ADDRESS
                                     0000   106 ;	R5 = SYSTEM ADDRESS (NO P1 SPACE EQUIVALENT) OF SYSTEM PROCESS HEADER
                                     0000   107 ;	IPL NOT REQUIRED TO BE AT SYNCH SINCE THE SYSTEM PAGE TABLE IS
                                     0000   108 ;		NOT EVER MOVED.
                                     0000   109 ;
                                     0000   110 ; IMPLICIT INPUTS:
                                     0000   111 ;
                                     0000   112 ;	NONE
                                     0000   113 ;
                                     0000   114 ; OUTPUT PARAMETERS:
                                     0000   115 ;
                                     0000   116 ;	R0 = SYSTEM STATUS CODE
                                     0000   117 ;	R2 PRESERVED
                                     0000   118 ;	R3 = SYSTEM VIRTUAL ADDRESS OF PAGE TABLE ENTRY OR LONG WORD
                                     0000   119 ;		INDEX TO PAGE TABLE ENTRY ACCORDING TO THE ENTRY POINT
                                     0000   120 ;
                                     0000   121 ; IMPLICIT OUTPUTS:
                                     0000   122 ;
                                     0000   123 ;	NONE
                                     0000   124 ;
                                     0000   125 ; COMPLETION CODES:
                                     0000   126 ;
                                     0000   127 ;	SS$_LENVIO			;PAGE TABLE LENGTH VIOLATION
                                     0000   128 ;
                                     0000   129 ; SIDE EFFECTS:
                                     0000   130 ;
                                     0000   131 ;	NONE
                                     0000   132 ;
                                     0000   133 ;--
SVAPTE          - SYSTEM VIRTUAL ADDRESS OF PAGE TABLE ENTRY     22-AUG-1978 00:18:05   VAX-11 MACRO X0.3-11               Page   4
00.1            PTEADR, PTEINDX - PAGE TABLE ENTRY ADDRESS OR I                                                                  (4)

                                     0000   135 
                                     0000   136 MMG$PTEREF::				;
            50       018C 8F     3C  0000   137 	MOVZWL	#SS$_LENVIO,R0		; ASSUME LENGTH VIOLATION
         28 A5            52     D1  0005   138 	CMPL	R2,PHD$L_FREP0VA(R5)	; MAKE SURE ADDRESS IS IN P0 SPACE
                          13     1E  0009   139 	BGEQU	10$			; IF GEQU,NO
            50            01     3C  000B   140 	MOVZWL	#SS$_NORMAL,R0		; SET SUCCESS STATUS
            16            09     EE  000E   141 	EXTV	#VA$V_VPN,#VA$S_VPN+1,R2,R3 ; GET VIRTUAL PAGE NUMBER
            53            52         0011       
            53   00000000'EF43   DE  0013   142 	MOVAL	DSC$GL_P0PTE[R3],R3	; GET SYSTEM VIRTUAL ADDRESS OF PTE
                                     001B   143 	SETIPL	#IPL$_SYNCH		; RAISE TO SYNCH
                                     001E   144 10$:					;
                                 05  001E   145 	RSB				;
                                     001F   146 
                                     001F   147 	.END
