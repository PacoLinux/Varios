                                     0000   101 	.SBTTL	SCH$RSE - REPORT SYSTEM EVENT
                                     0000   102 
                                     0000   103 ;++
                                     0000   104 ; FUNCTIONAL DESCRIPTION:
                                     0000   105 ;	SCH$RSE RECEIVES SYSTEM EVENT REPORTS FROM VARIOUS SOURCES
                                     0000   106 ;	AND PERFORMS THE APPROPRIATE ACTION FOR THE SPECIFIED PROCESS.
                                     0000   107 ;	EVENT REPORTING MUST BE PERFORMED WITH IPL=IPL$_SYNCH.
                                     0000   108 ;	AS A SIDE EFFECT OF AN EVENT REPORT, THE RESCHEDULING INTERRUPT
                                     0000   109 ;	MAY BE TRIGGERED IF APPROPRIATE.
                                     0000   110 ;
                                     0000   111 ; CALLING SEQUENCE:
                                     0000   112 ;	BSB/JSB	SCH$RSE
                                     0000   113 ;	.BYTE	EVT$_EVENTNAME
                                     0000   114 ;
                                     0000   115 ;	THIS CALLING SEQUENCE IS GENERATED BY THE RPTEVT SYSTEM MACRO
                                     0000   116 ;	
                                     0000   117 ;	REPEVT	EVENTNAME
                                     0000   118 ;
                                     0000   119 ; INPUT PARAMETERS:
                                     0000   120 ;	R2 - SITUATIONAL PRIORITY INCREMENT CLASS NUMBER
                                     0000   121 ;	R4 - PCB ADDRESS OF PROCESS FOR WHICH EVENT IS REPORTED
                                     0000   122 ;
                                     0000   123 ;	EVENT NUMBER CONTAINED IN BYTE LOCATED BY ADDRESS AT TOP 
                                     0000   124 ;	OF STACK.  @(SP)
                                     0000   125 ;
                                     0000   126 ; IMPLICIT INPUTS:
                                     0000   127 ;	SCHEDULER DATA BASE
                                     0000   128 ;
                                     0000   129 ; OUTPUT PARAMETERS:
                                     0000   130 ;	NONE
                                     0000   131 ;
                                     0000   132 ; IMPLICIT OUTPUTS:
                                     0000   133 ;	NONE
                                     0000   134 ;
                                     0000   135 ; COMPLETION CODES:
                                     0000   136 ;	NONE
                                     0000   137 ;
                                     0000   138 ; SIDE EFFECTS:
                                     0000   139 ;	A RESECHEDULING INTERRUPT MAY BE REQUESTED IF THE SPECIFIED
                                     0000   140 ;	PROCESS IS HIGHER IN PRIORITY THAN THE CURRENT PROCESS.
                                     0000   141 ;
                                     0000   142 ;--
                                     0000   143 
                                     0000   144 SCH$RSE::				; REPORT SYSTEM EVENT
            53         00 BE     9A  0000   145 	MOVZBL	@(SP),R3		; GET EVENT NUMBER 
                          6E     D6  0004   146 	INCL	(SP)			; UPDATE RETURN ADDRESS
            51         30 A4     3C  0006   147 	MOVZWL	PCB$W_STATE(R4),R1	; GET CURRENT STATE NUMBER
            50       0000'CF43   D0  000A   148 10$:	MOVL	W^STET[R3],R0		; GET STATE MASK FOR EVENT
            50            51     E0  0010   149 	BBS	R1,R0,ACTION		; DO ACTION IF STATE BIT SET
                          06         0013       
                          53     D6  0014   150 	INCL	R3			; CHECK NEXT ACTION
            F1            50     E8  0016   151 	BLBS	R0,10$			; IF CONTINUATION
                                 05  0019   152 	RSB				; OTHERWISE IGNORE EVENT
                                     001A   153 	
            00            53     CF  001A   154 ACTION:	CASEL	R3,#0,S^#MAXEVT		; SWITCH ON EVENT NUMBER(UPDATED)
                          0A'        001D       
                                     001E   155 STACT:					; BASE OF ACTION TABLE
