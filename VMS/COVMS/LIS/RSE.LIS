                                     0079   245 	.SBTTL	SCH$UNWAIT - DECREMENT COUNT IN WAIT QUEUE
                                     0079   246 ;++
                                     0079   247 ;
                                     0079   248 ; FUNCTIONAL DESCRIPTION:
                                     0079   249 ;	SCH$UNWAIT DECREMENTS THE NUMBER OF PROCESSES IN THE WAIT
                                     0079   250 ;	QUEUE SELECTED BY THE SPECIFIED PCB AND STATE VALUE.
                                     0079   251 ;
                                     0079   252 ; CALLING SEQUENCE:
                                     0079   253 ;	BSB/JSB	SCH$UNWAIT
                                     0079   254 ;
                                     0079   255 ; INPUT PARAMETERS:
                                     0079   256 ;	R1 - STATE NUMBER (PRESERVED)
                                     0079   257 ;	R2 - UNUSED	  (PRESERVED)
                                     0079   258 ;	R4 - PCB ADDRESS  (PRESERVED)
                                     0079   259 ;
                                     0079   260 ; IMPLICIT INPUTS:
                                     0079   261 ;	PCB LOCATED BY ADDRESS IN R4
                                     0079   262 ;
                                     0079   263 ; IMPLICIT OUTPUTS:
                                     0079   264 ;	COUNT IN WAIT QUEUE HEADER IS DECREMENTED IF STATE IS A WAIT
                                     0079   265 ;	STATE.
                                     0079   266 ;
                                     0079   267 ;--
                                     0079   268 
                                     0079   269 SCH$UNWAIT::				; DECREMENT PROPER WAIT COUNT
         9E'AF            51     E1  0079   270 	BBC	R1,B^WAITMSK,20$	; SKIP OUT IF NOT WAIT STATE
                          11         007D       
            51            03     B1  007E   271 	CMPW	#SCH$C_CEF,R1		; CHECK FOR COMMON EVENT FLAG WAIT
                          0D     13  0081   272 	BEQL	30$			; CEF WAIT
            51            0C     C4  0083   273 	MULL	#WQH$C_LENGTH,R1	; COMPUTE BYTE INDEX TO WQ HDR
