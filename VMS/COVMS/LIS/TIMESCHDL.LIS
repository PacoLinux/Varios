                                     0000    52 	.SBTTL	HARDWARE CLOCK INTERRUPTS
                                     0000    53 ;+
                                     0000    54 ; EXE$HWCLKINT - HARDWARE CLOCK INTERRUPT
                                     0000    55 ;
                                     0000    56 ; THIS ROUTINE IS AUTOMATICALLY VECTORED TO WHEN THE CLOCK COUNT REGISTER
                                     0000    57 ; OVERFLOWS. THE CURRENT ABSOLUTE TIME IS UPDATED, THE ACCOUNTING INTEGRAL
                                     0000    58 ; OF THE CURRENT PROCESS IS INCREMENTED, AND THE QUANTUM OF THE CURRENT
                                     0000    59 ; PROCESS IS INCREMENTED. IF THE PROCESS QUANTUM TRANSISTS TO ZERO OR THE
                                     0000    60 ; FIRST ENTRY IN THE TIMER QUEUE CAN BE REMOVED, THEN A SOFTWARE INTERRUPT
                                     0000    61 ; IS REQUESTED ON THE TIMER LEVEL. THE CLOCK INTERRUPT IS THEN DISMISSED.
                                     0000    62 ;-
                                     0000    63  
                                 00000000    64 	.PSECT	A$EXENONPAGED,LONG
                                     0000    65 EXE$HWCLKINT::				;HARDWARE CLOCK INTERRUPT
                          50     DD  0000    66 	PUSHL	R0			;SAVE REGISTER R0
            18   000000C1 8F     DA  0002    67 	MTPR	#^XC1,#PR$_ICCS		;CLEAR INTERRUPT REQUEST
            50            18     DB  0009    68 	MFPR	#PR$_ICCS,R0		;READ ERROR STATUS
                          07     18  000C    69 	BGEQ	10$			;IF GEQ NO ERROR
                                     000E    70 ;********
                                     000E    71 ; LOG CLOCK INTERRUPT OVERFLOW
                                     000E    72 ;********
            18   800000C1 8F     DA  000E    73 	MTPR	#^X800000C1,#PR$_ICCS	;CLEAR ERROR STATUS
       0000'CF   000186A0 8F     C0  0015    74 10$:	ADDL	#100000,W^EXE$GQ_SYSTIME ;UPDATE SYSTEM ABSOLUTE TIME
       0004'CF            00     D8  001E    75 	ADWC	#0,W^EXE$GQ_SYSTIME+4	;
                                     0023    76 
                                     0023    77 ;
                                     0023    78 ; IF MEASUREMENT IS ENABLED, UPDATE TIMER STATISTICS FIELDS.
                                     0023    79 ;
                                     0023    80 
                           00000002  0023    81 	.IF NE	CA$_MEASURE
                                     0023    82 
            50         0B AE     98  0023    83 	CVTBL	11(SP),R0		;GET UPPER BYTE OF SAVED PSL
                          03     18  0027    84 	BGEQ	20$			;BRANCH IF CM BIT NOT SET
            50            05     9A  0029    85 	MOVZBL	#5,R0			;ELSE INSERT CM INDEX
            50         F8 8F     8A  002C    86 20$:	BICB	#^XF8,R0		;CONVERT EXTENDED BYTE TO INDEX
                     0000'CF40   D6  0030    87 	INCL	W^PMS$GL_KERNEL[R0]	;INCREMENT STATISTICS VECTOR
                                     0035    88 
                                     0035    89 	.ENDC
                                     0035    90 
            50       0000'CF     D0  0035    91 	MOVL	W^SCH$GL_CURPCB,R0	;GET ADDRESS OF CURRENT PROCESS PCB
