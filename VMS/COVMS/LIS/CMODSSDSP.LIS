                                     0057   306 	.SBTTL	CHANGE MODE TO KERNEL DISPATCHER
                                     0057   307 ;+
                                     0057   308 ; EXE$CMODKRNL - CHANGE MODE TO KERNEL DISPATCHER
                                     0057   309 ;
                                     0057   310 ; THIS ROUTINE IS AUTOMATICALLY VECTORED TO WHEN A CHANGE MODE TO KERNEL
                                     0057   311 ; INSTRUCTION IS EXECUTED. THE STATE OF THE STACK ON ENTRY IS:
                                     0057   312 ;
                                     0057   313 ; INPUTS:
                                     0057   314 ;
                                     0057   315 ;	00(SP) = CHANGE MODE PARAMETER CODE.
                                     0057   316 ;	04(SP) = SAVED PC OF EXCEPTION.
                                     0057   317 ;	08(SP) = SAVED PSL OF EXCEPTION.
                                     0057   318 ;
                                     0057   319 ;	00(AP) = NUMBER OF SYSTEM SERVICE CALL ARGUMENTS.
                                     0057   320 ;	04(AP) = FIRST ARGUMENT.
                                     0057   321 ;	      .
                                     0057   322 ;	      .
                                     0057   323 ;	      .
                                     0057   324 ;	4*N(AP) = N'TH ARGUMENT.
                                     0057   325 ;
                                     0057   326 ; OUTPUTS:
                                     0057   327 ;
                                     0057   328 ;	***TBS***
                                     0057   329 ;-
                                     0057   330  
                                 00000057   331 	.PSECT	Y$CMODK,QUAD
                                     0057   332 	.ALIGN	QUAD
                                     0058   333 EXE$CMODKRNL::				;CHANGE MODE TO KERNEL DISPATCH
                          50   8ED0  0058   334 	POPL	R0			;REMOVE CHANGE MODE PARAMETER FROM STACK
                          A3     13  005B   335 	BEQL	ASTEXIT			;IF ZERO, AST EXIT SYSTEM SERVICE
                       C7 AF     9F  005D   336 	PUSHAB	B^SRVEXIT		;RETURN ADDRESS
            51       FFFF'CF40   9A  0060   337 	MOVZBL	W^B_KRNLNARG[R0],R1	;GET NUMBER OF REQUIRED ARGUMENTS
            7E            5C     7D  0066   338 	MOVQ	AP,-(SP)		;AP,FP
            5D   00000004 9F41   DE  0069   339 	MOVAL	@#4[R1],FP		;CALCULATE LENGTH OF ARGUMENT LIST
                          7E     7C  0071   340 	CLRQ	-(SP)			;PSW AND REGISTER SAVE MASK
                                     0073   341 	IFNORD	FP,(AP),ACCVIO		;DECLARE ACCESS VIOLATION
            5D            5E     D0  0079   342 	MOVL	SP,FP			;SET FRAME POINTER FOR CALL FRAME
            51            6C     91  007C   343 	CMPB	(AP),R1			;CHECK FOR REQUIRED NUMBER OF ARGS
                          A0     1F  007F   344 	BLSSU	INSARG			;IF LSSU, INSUFFICIENT ARGUMENTS
            54   00000000'EF     D0  0081   345 	MOVL	SCH$GL_CURPCB,R4	;GET CURRENT PROCESS PCB ADDRESS
            01            50     AF  0088   346 KERDSP:	CASEW	R0,#1,#KCASMAX		;DISPATCH TO PROPER SERVICE ROUTINE
                     004B'8F         008B       
                                     008E   347 KCASE:					;BASE OF CHMK CASE TABLE
                           00000001  008E   348 KCASCTR=1				;CHMK CODES START AT 1
                                 00000000   349 	.PSECT	Y$CMODKN,BYTE		;REQUIRED NUMBER OF ARG TABLE
                           FFFFFFFF  0000   350 B_KRNLNARG=.-1				;CHMK CODES ARE 1 ORIGIN
