                                     0000    58 ;
                                     0000    59 ;	GSYSSRV	- GENERATE SYSTEM SERVICE ENTRY VECTOR
                                     0000    60 ;
                                     0000    61 ;	GSYSSRV	SRVNAME,MODE,NARG,REGISTERS,DUMMY
                                     0000    62 ;
                                     0000    63 ;	WHERE:
                                     0000    64 ;		SRVNAME - SERVICE NAME LESS ANY PREFIX (SYS$,EXE$,RMS$$)
                                     0000    65 ;		MODE - MODE DESIGNATOR FOR SERVICE (K,E,ALL,R)
                                     0000    66 ;		NARG - REQUIRED NUMBER OF ARGUMENTS
                                     0000    67 ;		REGISTERS - REGISTER SAVE LIST
                                     0000    68 ;		DUMMY - REDIRECT CALL TO ILLEGAL SERVICE HANDLER
                                     0000    69 ;
                                     0000    70 
                                     0000    71 	.MACRO	GSYSSRV,SRVNAME,MODE,NARG,REGS,DUMMY
                                     0000    72 	.IF	NDF,RMSSWITCH
                                     0000    73 	.IF	DF,LIBSWITCH
                                     0000    74 	.PSECT	$$$0000,QUAD
                                     0000    75 	.IFF
                                     0000    76 	.PSECT	$$$000,QUAD
                                     0000    77 	.ENDC
                                     0000    78 	.ALIGN	QUAD
                                     0000    79 	.IF DF	LIBSWITCH
                                     0000    80 SYS$'SRVNAME::
                                     0000    81 	.IFF
                                     0000    82 	.WORD	^M<REGS>
                                     0000    83 	SRV'MODE	SRVNAME,NARG,DUMMY
                                     0000    84 	.IFT
                                     0000    85 	.BLKL	2	
                                     0000    86 	.ENDC
                                     0000    87 	.IFF
                                     0000    88 	SRV'MODE	SRVNAME,NARG,DUMMY
                                     0000    89 	.ENDC
                                     0000    90 	.ENDM	GSYSSRV
                                     0000    91 
                                     0000    92 ;
                                     0000    93 ;	SRVK - GENERATE ENTRY FOR KERNEL MODE SERVICE
                                     0000    94 ;
                                     0000    95 ;	SRVK	SRVNAME,NARG
                                     0000    96 ;
                                     0000    97  
                                     0000    98 	.MACRO	SRVK,SRVNAME,NARG,DUMMY
                                     0000    99 	.IF	NDF,RMSSWITCH
                                     0000   100 	CHMK	#SRVNAME
                                     0000   101 	RET
                                     0000   102 	.PSECT	Y$CMODKN,BYTE
                                     0000   103 	.BYTE	NARG
                                     0000   104 	.PSECT	Y$CMODK,BYTE
                                     0000   105 	.IF B	DUMMY
                                     0000   106 	.WORD	EXE$'SRVNAME-KCASE+2
                                     0000   107 	.IFF
                                     0000   108 	.WORD	ILLSER-KCASE+2
                                     0000   109 	.ENDC
                                     0000   110 SRVNAME=KCASCTR
                                     0000   111 KCASCTR=KCASCTR+1
                                     0000   112 	.ENDC
                                     0000   113 	.ENDM	SRVK
                                     0000   114 
