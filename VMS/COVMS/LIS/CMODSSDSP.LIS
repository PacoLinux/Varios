                                     0126   670 ;
                                     0126   671 ;
                                     0126   672 ;  RMS SYNCHRONIZATION ROUTINE
                                     0126   673 ;
                                     0126   674 ;  THE FOLLOWING ROUTINE IS USED BY THE VARIOUS RMS SERVICES IN ORDER
                                     0126   675 ;  TO AWAIT I/O COMPLETION.  THE ROUTINE IS IN THE VECTOR AREA IN ORDER
                                     0126   676 ;  TO WAIT AT THE CALLER'S MODE,  THUS ALLOWING AST ACTIVITY FOR EITHER
                                     0126   677 ;  USER OR SUPERVISOR MODE, OR BOTH.
                                     0126   678 ;
                                     0126   679 ;  NOTE THAT EACH RMS SERVICE VECTOR TERMINATES WITH A BRANCH TO THIS
                                     0126   680 ;  ROUTINE.
                                     0126   681 ;
                                     0126   682 	.IF NDF,LIBSWITCH
                                 000002DF   683 	.PSECT	$$$000,QUAD
                                     02DF   684 	.IFF
                                     02DF   685 	.PSECT	$$$0000,QUAD
                                     02DF   686 	.IFTF
                           00000320  02DF   687 	.BLKB	^X320-<.-VECBASE>
                                     0320   688 	.IFT
                                     0320   689 	.IF DF	RMS
                                     0320   690  
                                     0320   691 RMSWAIT_IO_DONE:
                                     0320   692 10$:	$WAITFR_S	R3		;WAIT FOR RMS EVENT FLAG
                                     0320   693 	MOVL	4(AP),R2		;GET FAB OR RAB ADDRESS
                                     0320   694 20$:	MOVL	8(R2),R0		;GET RMS STATUS CODE
                                     0320   695 	BEQL	30$			;AND WAIT AGAIN IF NOT SET
                                     0320   696 	BLBC	R0,35$			;BRANCH IF FAILURE CODE
                                     0320   697 	RET				;RETURN TO CALLER
                                     0320   698 ;
                                     0320   699 ;  CLEAR THE RMS EVENT FLAG, CHECK STATUS AGAIN AND WAIT 1 MORE TIME IF
                                     0320   700 ;  OPERATION STILL NOT DONE
                                     0320   701 ;
                                     0320   702 30$:	$CLREF_S	R3		;CLEAR RMS EVENT FLAG
                                     0320   703 	MOVL	8(R2),R0		;AND RE-CHECK STATUS
                                     0320   704 	BEQL	10$			;BRANCH TO WAIT FOR FLAG AGAIN..
                                     0320   705 					;... IF STATUS STILL ZERO
                                     0320   706 	$SETEF_S	R3		;I/O COMPLETE - LEAVE EFN SET
                                     0320   707 	BRB	20$			;AND RESTORE R0 STATUS CODE
                                     0320   708 ;
                                     0320   709 ;  CHECK STATUS CODE FOR ERROR OR SEVERE ERROR
                                     0320   710 ;
                                     0320   711 35$:	BITB	#6,R0			;ERROR OR SEVERE ERROR?
