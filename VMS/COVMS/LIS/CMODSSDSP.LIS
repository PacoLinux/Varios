CMODSSDSP       - CHANGE MODE SYSTEM SERVICE DISPATCHER          22-AUG-1978 00:06:02   VAX-11 MACRO X0.3-11               Page   5
00.1            CHANGE MODE TO EXECUTIVE DISPATCHER                                                                              (1)

                                     0000   192 	.SBTTL	CHANGE MODE TO EXECUTIVE DISPATCHER
                                     0000   193 ;+
                                     0000   194 ; EXE$CMODEXEC - CHANGE MODE TO EXECUTIVE DISPATCHER
                                     0000   195 ;
                                     0000   196 ; THIS ROUTINE IS AUTOMATICALLY VECTORED TO WHEN A CHANGE MODE TO EXECUTIVE
                                     0000   197 ; INSTRUCTION IS EXECUTED. THE STATE OF THE STACK ON ENTRY IS:
                                     0000   198 ;
                                     0000   199 ; INPUTS:
                                     0000   200 ;
                                     0000   201 ;	00(SP) = CHANGE MODE PARAMETER CODE.
                                     0000   202 ;	04(SP) = SAVED PC OF EXCEPTION.
                                     0000   203 ;	08(SP) = SAVED PSL OF EXCEPTION.
                                     0000   204 ;
                                     0000   205 ;	00(AP) = NUMBER OF SYSTEM SERVICE CALL ARGUMENTS.
                                     0000   206 ;	04(AP) = FIRST ARGUMENT.
                                     0000   207 ;	      .
                                     0000   208 ;	      .
                                     0000   209 ;	      .
                                     0000   210 ;	4*N(AP) = N'TH ARGUMENT.
                                     0000   211 ;
                                     0000   212 ; OUTPUTS:
                                     0000   213 ;
                                     0000   214 ;	***TBS***
                                     0000   215 ;-
                                     0000   216  
                                 00000000   217 	.PSECT	Y$CMODE,QUAD
                                     0000   218 EXACCVIO:				;CHANGE MODE TO EXEC ACCESS VIOLATION
                          0017'  31  0000   219 	BRW	ACCVIO			;
                                     0003   220 EXINSARG:				;CHANGE MODE TO EXEC INSUFFICIENT ARGS
                          001B'  31  0003   221 	BRW	INSARG			;
                                     0006   222 	.ALIGN	QUAD
                                     0008   223 EXE$CMODEXEC::				;CHANGE MODE TO EXECUTIVE DISPATCH
                          01     BA  0008   224 	POPR	#^M<R0>			;REMOVE CHANGE MODE PARAMETER FROM STACK
                     0027'CF     9F  000A   225 	PUSHAB	W^SRVEXIT		;RETURN ADDRESS FOR CALL FRAME
            51       0000'CF40   9A  000E   226 	MOVZBL	W^B_EXECNARG[R0],R1	;GET REQUIRED NUMBER OF ARGUMENTS
            7E            5C     7D  0014   227 	MOVQ	AP,-(SP)		;SAVE AP,FP
            5D   00000004 9F41   DE  0017   228 	MOVAL	@#4[R1],FP		;CALCULATE LENGTH OF ARGUMENT LIST
                          7E     7C  001F   229 	CLRQ	-(SP)			;PSW, REGISTER SAVE MASK FOR CALL FRAME
                                     0021   230 	IFNORD	FP,(AP),EXACCVIO	;BR IF ARGLIST INACCESSIBLE
            5D            5E     D0  0027   231 	MOVL	SP,FP			;SET FP TO POINT TO CALL FRAME
            51            6C     91  002A   232 	CMPB	(AP),R1			;CHECK FOR REQUIRED NUMBER OF ARGUMENTS
                          D4     1F  002D   233 	BLSSU	EXINSARG		;INSUFFICIENT NUMBER OF ARGUMENTS
            00            50     AF  002F   234 EXEDSP:	CASEW	R0,#0,S^#ECASMAX	;DISPATCH TO PROPER SERVICE ROUTINE
                          06'        0032       
                           00000000  0033   235 ECASCTR=0				;START WITH 0 FOR CHME CODE
                                     0033   236 ECASE:					;BASE OF CHME CASE TABLE
                                 00000000   237 	.PSECT	Y$CMODEN,BYTE		;REQUIRED NUMBER OF ARG TABLE
                                     0000   238 B_EXECNARG:				;DEFINE TABLE BASE
                                     0000   239 
                                     0000   240 ;
                                     0000   241 ;	NOTE THAT THE OUT OF RANGE FALL THROUGH FROM THE CASEW FOLLOWS
                                     0000   242 ;	THE SYSTEM SERVICE VECTOR GENERATION.
CMODSSDSP       - CHANGE MODE SYSTEM SERVICE DISPATCHER          22-AUG-1978 00:06:02   VAX-11 MACRO X0.3-11               Page   6
00.1            ASTEXIT SYSTEM SERVICE                                                                                           (1)

                                     0000   244 	.SBTTL	ASTEXIT SYSTEM SERVICE
                                     0000   245 ;+
                                     0000   246 ; ASTEXIT - SERVICE TO EXIT AN ACTIVE AST AND ALLOW PENDING ASTS TO 
                                     0000   247 ;	    BE DELIVERED.
                                     0000   248 ;
                                     0000   249 ; THIS SYSTEM SERVICE IS INVOKED WITH A CHMK #ASTEXIT NOT CONTAINED IN
                                     0000   250 ; A STANDARD SYSTEM SERVICE VECTOR TO AVOID CLUTTERING THE STACK WITH AN
                                     0000   251 ; ADDITIONAL CALL FRAME DURING AST EXIT PROCESSING.
                                     0000   252 ;
                                     0000   253 ; INPUTS:
                                     0000   254 ; 	NONE
                                     0000   255 ;
                                     0000   256 ; OUTPUTS:
                                     0000   257 ;	PCB$B_ASTACT IS CLEARED FOR THE ISSUING MODE
                                     0000   258 ;	PHD$B_ASTLVL IS SET TO THE ACCESS MODE OF THE NEXT PENDING AST, IF ANY.
                                     0000   259 ;
                                     0000   260 ;-
                                     0000   261 
                                 00000000   262 	.PSECT	Y$CMODK,QUAD
                                     0000   263 ASTEXIT:				;EXIT ACTIVE AST
            02            16     EF  0000   264 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,4(SP),R0 ;GET PREVIOUS MODE
            50         04 AE         0003       
                          14     BB  0006   265 	PUSHR	#^M<R2,R4>		;SAVE R2,R4
            54   00000000'EF     D0  0008   266 	MOVL	SCH$GL_CURPCB,R4	;GET PCB CURRENT PCB ADDRESS
         0C A4            50     E7  000F   267 	BBCCI	R0,PCB$B_ASTACT(R4),10$	;CLEAR AST ACTIVE BIT FOR MODE
                          00         0013       
                          FFE9'  30  0014   268 10$:	BSBW	SCH$NEWLVL		;COMPUTE NEW AST LEVEL SETTING
                          14     BA  0017   269 	POPR	#^M<R2,R4>		;RESTORE REGISTERS<R2,R4>
                                 02  0019   270 	REI				;AND EXIT
CMODSSDSP       - CHANGE MODE SYSTEM SERVICE DISPATCHER          22-AUG-1978 00:06:02   VAX-11 MACRO X0.3-11               Page   7
00.1            CHANGE MODE DETECTED ERROR HANDLING                                                                              (1)

                                     001A   272 	.SBTTL	CHANGE MODE DETECTED ERROR HANDLING
                                     001A   273 ;+
                                     001A   274 ; ACCVIO - ACCESS VIOLATION DETECTED IN ARGUMENT LIST
                                     001A   275 ; INSARG - INSUFFICIENT ARGUMENTS SUPPLIED FOR SERVICE
                                     001A   276 ; SSFAIL - ABNORMAL STATUS RETURNED BY SERVICE ROUTINE
                                     001A   277 ;
                                     001A   278 ; THESE ROUTINES TAKE THE APPROPRIATE ACTION TO RETURN THE ERROR INDICATION
                                     001A   279 ; TO THE ORIGINAL CALLER.
                                     001A   280 ;
                                     001A   281 ;-
                                     001A   282 
            5D            5E     D0  001A   283 ACCVIO:	MOVL	SP,FP			;SET FRAME POINTER BEFORE RET
            50            0C     3C  001D   284 	MOVZWL	#SS$_ACCVIO,R0		;SET ACCESS VIOLATION
                                 04  0020   285 	RET				;
            50       0114 8F     3C  0021   286 INSARG:	MOVZWL	#SS$_INSFARG,R0		;SET INSUFFICIENT NUMBER OF ARGUMENTS
                                 04  0026   287 	RET				;
                                     0027   288 SRVEXIT:				;SERVICE EXIT
            07            50     E9  0027   289 	BLBC	R0,SSFAIL		;BR IF ABNORMAL COMPLETION
                                 02  002A   290 SRVREI:	REI				;
                                     002B   291 EXE$EXCPTN::				;SYSTEM SERVICE EXCEPTION
                               0000  002B   292 	.WORD	0			;ENTRY MASK
                                     002D   293 	BUG_CHECK SSRVEXCEPT,FATAL	;UNEXPECTED SYSTEM SERVICE EXCEPTION
            50            07     D3  0031   294 SSFAIL:	BITL	#7,R0			;TEST SEVERITY FIELD
                          F4     13  0034   295 	BEQL	SRVREI			;IF EQL WARNING
            51   00000000'EF     D0  0036   296 	MOVL	SCH$GL_CURPCB,R1	;GET ADDRESS OF CURRENT PROCESS PCB
                       0E A1     B5  003D   297 	TSTW	PCB$W_MTXCNT(R1)	;MUTEX COUNT ZERO?
                          11     12  0040   298 	BNEQ	10$			;IF NEQ NO
                          18     EF  0042   299 	EXTZV	#PSL$V_CURMOD,#PSL$S_CURMOD,- ;EXTRACT PREVIOUS MODE FROM
         04 AE            02         0044   300 		4(SP),-(SP)		;SAVED PSL
                          7E         0047       
            6E            06     C0  0048   301 	ADDL	#PCB$V_SSFEXC,(SP)	;ADD IN BASE BIT NUMBER
         28 A1            8E     E1  004B   302 	BBC	(SP)+,PCB$L_STS(R1),SRVREI ;IF CLEAR, FAILURE EXCEPTION DISABLED
                          DA         004F       
                          FFAD'  31  0050   303 	BRW	EXE$SSFAIL		;GENERATE SYSTEM SERVICE FAILURE EXCEPTION
                                     0053   304 10$:	BUG_CHECK MTXCNTNONZ,FATAL	;MUTEX COUNT NONZERO AT SERVICE EXIT
