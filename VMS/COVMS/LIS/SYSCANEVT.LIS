01              CANCEL TIMER REQUEST                                                                                             (1)

                                     0000    54 	.SBTTL	CANCEL TIMER REQUEST
                                     0000    55 ;+
                                     0000    56 ; EXE$CANTIM - CANCEL TIMER REQUEST
                                     0000    57 ;
                                     0000    58 ; THIS SERVICE PROVIDES THE CAPABILITY TO CANCEL ALL OR A SPECIFIED SUBSET
                                     0000    59 ; OF TIMERS THAT WERE PREVIOUSLY ESTABLISHED.
                                     0000    60 ;
                                     0000    61 ; INPUTS:
                                     0000    62 ;
                                     0000    63 ;	REQIDT(AP) = REQUEST IDENTIFICATION (ZERO IMPLIES ALL).
                                     0000    64 ;	ACMODE(AP) = ACCESS MODE (ALL EQUAL AND HIGHER ACCESS MODES).
                                     0000    65 ;
                                     0000    66 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    67 ;
                                     0000    68 ; OUTPUTS:
                                     0000    69 ;
                                     0000    70 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    71 ;
                                     0000    72 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0000    73 ;-
                                     0000    74  
                                     0000    75 	.ENABL	LSB
                               003C' 0000    76 	.ENTRY	EXE$CANTIM,^M<R2,R3,R4,R5>
            02            00     EF  0002    77 	EXTZV	#0,#2,ACMODE(AP),R0	;GET SPECIFIED ACCESS MODE
            50         08 AC         0005       
                          FFF5'  30  0008    78 	BSBW	EXE$MAXACMODE		;MAXIMIZE ACCESS MODE WITH PREVIOUS MODE
            55         60 A4     D0  000B    79 	MOVL	PCB$L_PID(R4),R5	;SET PROCESS ID
            54            00     9A  000F    80 	MOVZBL	#TQE$C_TMSNGL,R4	;SET TYPE OF ENTRY
            53         04 AC     D0  0012    81 	MOVL	REQIDT(AP),R3		;SET REQUEST IDENTIFICATION
            52            50     D0  0016    82 	MOVL	R0,R2			;SET CANCELLATION ACCESS MODE
                          11     11  0019    83 	BRB	10$			;FINISH IN COMMON CODE
SYSCANEVT       - SYSTEM SERVICES TO CANCEL EVENTS               21-AUG-1978 23:35:58   VAX-11 MACRO X0.3-11               Page   3
01              CANCEL WAKEUP                                                                                                    (1)

                                     001B    85 	.SBTTL	CANCEL WAKEUP
                                     001B    86 ;+
                                     001B    87 ; EXE$CANWAK - CANCEL WAKEUP
                                     001B    88 ;
                                     001B    89 ; THIS SERVICE PROVIDES THE CAPABILITY TO CANCEL ALL WAKE UP REQUESTS FOR
                                     001B    90 ; A SPECIFIED PROCESS.
                                     001B    91 ;
                                     001B    92 ; INPUTS:
                                     001B    93 ;
                                     001B    94 ;	PIDADR(AP) = ADDRESS OF PROCESS IDENTIFICATION.
                                     001B    95 ;	LOGNAM(AP) = ADDRESS OF PROCESS NAME STRING DESCRIPTOR.
                                     001B    96 ;
                                     001B    97 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     001B    98 ;
                                     001B    99 ; OUTPUTS:
                                     001B   100 ;
                                     001B   101 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO CANCEL WAKE UP REQUESTS.
                                     001B   102 ;
                                     001B   103 ;		R0 = SS$_ACCVIO - PROCESS NAME STRING OR PROCESS NAME
                                     001B   104 ;			STRING DESCRIPTOR CANNOT BE READ BY CALLING ACCESS
                                     001B   105 ;			MODE, OR PROCESS IDENTIFICATION CANNOT BE WRITTEN
                                     001B   106 ;			BY CALLING ACCESS MODE.
                                     001B   107 ;
                                     001B   108 ;		R0 = SS$_IVLOGNAM - ZERO OR GREATER THAN MAXIMUM LENGTH
                                     001B   109 ;			PROCESS NAME STRING SPECIFIED.
                                     001B   110 ;
                                     001B   111 ;		R0 = SS$_NONEXPR - NONEXISTENT PROCESS OF INVALID PROCESS
                                     001B   112 ;			IDENTIFICATION SPECIFIED.
                                     001B   113 ;
                                     001B   114 ;		R0 = SS$_NOPRIV - PROCESS DOES NOT HAVE PRIVILEGE TO WAKE
                                     001B   115 ;			SPECIFIED PROCESS.
                                     001B   116 ;
                                     001B   117 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     001B   118 ;
                                     001B   119 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     001B   120 ;-
                                     001B   121  
                               003C' 001B   122 	.ENTRY	EXE$CANWAK,^M<R2,R3,R4,R5>
                          FFE0'  30  001D   123 	BSBW	EXE$NAMPID		;GET TARGET PROCESS ID
                                     0020   124 	SETIPL	#0			;ALLOW INTERRUPTS
            0C            50     E9  0023   125 	BLBC	R0,20$			;IF LBC INVALID ARGUMENT
            54            02     9A  0026   126 	MOVZBL	#TQE$C_WKSNGL,R4	;SET TYPE OF ENTRY
            55            51     D0  0029   127 	MOVL	R1,R5			;SET TARGET PROCESS ID
                          FFD1'  30  002C   128 10$:	BSBW	EXE$RMVTIMQ		;REMOVE ENTRIES FROM TIMER QUEUE
            50            01     3C  002F   129 	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION STATUS
                                 04  0032   130 20$:	RET				;
                                     0033   131 	.DSABL	LSB
                                     0033   132  
                                     0033   133 	.END
