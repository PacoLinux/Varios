X01             BUILD MESSAGE SUBROUTINE                                                                                         (5)

                                     002A   166 	.SBTTL	BUILD MESSAGE SUBROUTINE
                                     002A   167 ;++
                                     002A   168 ; BUILDMSG - BUILD MESSAGE ROUTINE FOR EXE$SNDOPR/EXE$SNDSMB
                                     002A   169 ;
                                     002A   170 ; FUNCTIONAL DESCRIPTION:
                                     002A   171 ;
                                     002A   172 ; THIS ROUTINE BUILDS THE REQUESTED MESSAGE ON THE EXEC STACK
                                     002A   173 ; AND ENTERS A KERNEL MODE ROUTINE TO PERFORM THE MAILBOX VALIDATION
                                     002A   174 ; AND SEND THE MESSAGE.
                                     002A   175 ;
                                     002A   176 ; INPUTS:
                                     002A   177 ;
                                     002A   178 ;	MSG(AP) = ADDRESS OF THE MESSAGE DESCRIPTER
                                     002A   179 ;	MBX(AP) = CHANNEL NUMBER OF THE REPLY MAILBOX IF ANY
                                     002A   180 ;	R5 = MAILBOX UCB ADDRESS
                                     002A   181 ;	R11 = MESSAGE TYPE
                                     002A   182 ;
                                     002A   183 ; OUTPUTS:
                                     002A   184 ;
                                     002A   185 ;	R0 = STATUS OF THE OPERATION
                                     002A   186 ;
                                     002A   187 ; STATUS CODES RETURNED:
                                     002A   188 ;
                                     002A   189 ;	SS$_NORMAL - SUCCESSFUL OPERATION
                                     002A   190 ;	SS$_IVCHAN - SPECIFIED CHANNEL INVALID
                                     002A   191 ;	SS$_MBTOOSML - MESSAGE EXCEEDS MAILBOX SIZE
                                     002A   192 ;	SS$_DEVOFFLIN - DEVICE OFFLINE
                                     002A   193 ;	SS$_INSFMEM - INSUFFICIENT MEMORY FOR THE REQUEST
                                     002A   194 ;	SS$_ACCVIO - ACCESS VIOLATION ON BUFFER
                                     002A   195 ;	SS$_NOPRIV - PROCESS DOES NOT HAVE READ ACCESS TO SPECIFIED MAILBOX
                                     002A   196 ;	SS$_DEVNOTMBX - SPECIFIED CHANNEL IS NOT TO MAILBOX
                                     002A   197 ;	SS$_BADPARAM - MESSAGE SIZE ERROR
                                     002A   198 ;--
                                     002A   199 BUILDMSG:				; BUILD MESSAGE
            50            0C     3C  002A   200 	MOVZWL	#SS$_ACCVIO,R0		; ASSUME ERROR
            59         04 AC     D0  002D   201 	MOVL	MSG(AP),R9		; GET MESSAGE DESCRIPTER
                                     0031   202 	IFNORD	#8,(R9),100$		; BR IF NO READ ACCESS
            58            69     3C  0037   203 	MOVZWL	(R9),R8			; GET SIZE OF MESSAGE
                          6D     13  003A   204 	BEQL	110$			; IF EQL THEN ILLEGAL
       0080 8F            58     B1  003C   205 	CMPW	R8,#128			; LEGAL SIZE?
                          66     1A  0041   206 	BGTRU	110$			; IF GTRU THEN NO
                                     0043   207 	IFNORD	R8,@4(R9),100$		; BR IF NO READ ACCESS
            58            29     C1  004A   208 	ADDL3	#38+3,R8,R1		; CALC SIZE OF TOTAL MESSAGE
                          51         004D       
            51            03     8A  004E   209 	BICB	#3,R1			; ROUND DOWN SIZE
            50       0124 8F     3C  0051   210 	MOVZWL	#SS$_INSFMEM,R0		; ASSUME NO STACK
            5E            51     C3  0056   211 	SUBL3	R1,SP,R3		; ADDRESS MESSAGE STORAGE
                          53         0059       
   00000000'9F            53     D1  005A   212 	CMPL	R3,@#CTL$AL_STACK	; IN KERNEL STACK?
                          45     1B  0061   213 	BLEQU	100$			; IF LEQU THEN YES
            5E            53     D0  0063   214 	MOVL	R3,SP			; ALLOCATE THE SPACE
                          28     BB  0066   215 	PUSHR	#^M<R3,R5>		; SAVE SIZE AND UCB ADDRESS
            83            5B     B0  0068   216 	MOVW	R11,(R3)+		; INSERT MESAGE TYPE
            83         08 AC     B0  006B   217 	MOVW	MBX(AP),(R3)+		; INSERT REPLY MAILBOX CHANNEL NUMBER
            56       0000'CF     D0  006F   218 	MOVL	W^SCH$GL_CURPCB,R6	; ADDRESS PCB
            83         64 B6     7D  0074   219 	MOVQ	@PCB$L_PHD(R6),(R3)+	; INSERT PRIVILEGE MASK
            83         20 A6     D0  0078   220 	MOVL	PCB$L_UIC(R6),(R3)+	; INSERT UIC
