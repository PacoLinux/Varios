X01             SEND MESSAGE ROUTINE                                                                                             (6)

                                     00AD   232 	.SBTTL	SEND MESSAGE ROUTINE
                                     00AD   233 ;++
                                     00AD   234 ; SENDMSG - KERNEL MODE MESSAGE SEND ROUTINE
                                     00AD   235 ;
                                     00AD   236 ; FUNCTIONAL DESCRIPTION:
                                     00AD   237 ;
                                     00AD   238 ; THIS ROUTINE RUNS IN KERNEL MODE AND SENDS THE MESSAGE TO THE
                                     00AD   239 ; TARGET MAILBOX.
                                     00AD   240 ;
                                     00AD   241 ; INPUTS:
                                     00AD   242 ;
                                     00AD   243 ;	R3 = SIZE OF MESSAGE
                                     00AD   244 ;	R4 = ADDRESS OF THE MESSAGE
                                     00AD   245 ;	R5 = MAILBOX UCB ADDRESS
                                     00AD   246 ;
                                     00AD   247 ; OUTPUTS:
                                     00AD   248 ;
                                     00AD   249 ;	R0 = STATUS OF THE OPERTATION
                                     00AD   250 ;
                                     00AD   251 ; STATUS CODES RETURNED:
                                     00AD   252 ;
                                     00AD   253 ;	SS$_NORMAL - SUCCESSFUL OPERATION
                                     00AD   254 ;	SS$_IVCHAN - SPECIFIED CHANNEL INVALID
                                     00AD   255 ;	SS$_MBTOOSML - MESSAGE EXCEEDS MAILBOX SIZE
                                     00AD   256 ;	SS$_DEVOFFLIN - DEVICE OFFLINE
                                     00AD   257 ;	SS$_INSFMEM - INSUFFICIENT MEMORY FOR THE REQUEST
                                     00AD   258 ;	SS$_NOPRIV - PROCESS DOES NOT HAVE READ ACCESS TO SPECIFIED MAILBOX
                                     00AD   259 ;	SS$_DEVNOTMBX - SPECIFIED CHANNEL IS NOT TO MAILBOX
                                     00AD   260 ;--
                                     00AD   261 SENDMSG:				; 
                               0000  00AD   262 	.WORD	0			; SAVE NO REGISTERS
            55         04 AC     D0  00AF   263 	MOVL	4(AP),R5		; GET MESSAGE ADDRESS
            50         02 A5     3C  00B3   264 	MOVZWL	2(R5),R0		; GET CHANNEL NUMBER
                          31     13  00B7   265 	BEQL	10$			; IF EQL THEN NO REPLY
            54       0000'CF     D0  00B9   266 	MOVL	W^SCH$GL_CURPCB,R4	; GET CURRENT PCB
                          FF3F'  30  00BE   267 	BSBW	IOC$VERIFYCHAN		; CHECK OUT CHANNEL NUMBER
            30            50     E9  00C1   268 	BLBC	R0,100$			; BR IF ERROR
            51            61     D0  00C4   269 	MOVL	CCB$L_UCB(R1),R1	; GET UCB OF REPLY MAILBOX
         02 A5         48 A1     B0  00C7   270 	MOVW	UCB$W_UNIT(R1),2(R5)	; INSERT UNIT NUMBER OF MAILBOX
            50       0074 8F     3C  00CC   271 	MOVZWL	#SS$_DEVNOTMBX,R0	; ASSUME DEVICE NOT MAILBOX
         34 A1            14     E1  00D1   272 	BBC	#DEV$V_MBX,UCB$L_DEVCHAR(R1),100$; BR IF NOT MAILBOX
                          1E         00D5       
            52         1C A1     D0  00D6   273 	MOVL	UCB$L_OWNUIC(R1),R2	; GET UIC OF BOX OWNER
            51         1A A1     3C  00DA   274 	MOVZWL	UCB$W_VPROT(R1),R1	; GET PROTECTION MASK
                          FF1F'  30  00DE   275 	BSBW	EXE$CHKRDACCES		; CHECK ACCESS
            10            50     E9  00E1   276 	BLBC	R0,100$			; BR IF ERROR IN ACCESS
                          FF19'  30  00E4   277 	BSBW	EXE$CHKWRTACCES		; CHECK OUT FOR WRITE
            0A            50     E9  00E7   278 	BLBC	R0,100$			; AND RETURN IF NO ACCESS
            55         08 AC     D0  00EA   279 10$:	MOVL	8(AP),R5		; ADDRESS UCB OF MAILBOX
            53            6C     7D  00EE   280 	MOVQ	(AP),R3			; GET SIZE AND ADDRESS OF MESSAGE
                          FF0C'  30  00F1   281 	BSBW	EXE$WRTMAILBOX		; WRITE MESSAGE
                                 04  00F4   282 100$:	RET				; RETURN
                                     00F5   283 ;
                                     00F5   284 	.END
