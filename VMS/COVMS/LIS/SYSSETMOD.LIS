SYSSETMOD       - SYSTEM SERVICES TO SET PROCESS MODES           21-AUG-1978 23:47:31   VAX-11 MACRO X0.3-11               Page   2
01              SET RESOURCE WAIT MODE                                                                                           (1)

                                     0000    49 	.SBTTL	SET RESOURCE WAIT MODE
                                     0000    50 ;+
                                     0000    51 ; EXE$SETRWM - SET RESOURCE WAIT MODE
                                     0000    52 ;
                                     0000    53 ; THIS SERVICE PROVIDES THE CAPABILITY TO CONTROL WHETHER IMPLICIT WAITS
                                     0000    54 ; ARE TO BE PERFORMED BY THE SYSTEM WHEN A REQUEST FOR A RESOURCE FAILS.
                                     0000    55 ;
                                     0000    56 ; INPUTS:
                                     0000    57 ;
                                     0000    58 ;	WATFLG(AP) = WAIT MODE INDICATOR.
                                     0000    59 ;		LOW BIT CLEAR TO ENABLE IMPLIED RESOURCE WAIT.
                                     0000    60 ;		LOW BIT SET TO DISABLE IMPLIED RESOURCE WAIT.
                                     0000    61 ;
                                     0000    62 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    63 ;
                                     0000    64 ; OUTPUTS:
                                     0000    65 ;
                                     0000    66 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    67 ;
                                     0000    68 ;		R0 = SS$_WASCLR - NORMAL COMPLETION, RESOURCE WAIT WAS
                                     0000    69 ;			PREVIOUSLY ENABLED.
                                     0000    70 ;
                                     0000    71 ;		R0 = SS$_WASSET - NORMAL COMPLETION, RESOURCE WAIT WAS
                                     0000    72 ;			PREVIOUSLY DISABLED.
                                     0000    73 ;-
                                     0000    74  
                                 00000000    75 	.PSECT	Y$EXEPAGED
                               0010' 0000    76 	.ENTRY	EXE$SETRWM,^M<R4>
            51            0A     9A  0002    77 	MOVZBL	#PCB$V_SSRWAIT,R1	;SET RESOURCE WAIT BIT NUMBER
                          1B     11  0005    78 	BRB	SETMOD			;FINISH IN COMMON CODE
SYSSETMOD       - SYSTEM SERVICES TO SET PROCESS MODES           21-AUG-1978 23:47:31   VAX-11 MACRO X0.3-11               Page   3
01              SET SYSTEM SERVICE FAILURE EXCEPTION MODE                                                                        (1)

                                     0007    80 	.SBTTL	SET SYSTEM SERVICE FAILURE EXCEPTION MODE
                                     0007    81 ;+
                                     0007    82 ; EXE$SETSFM - SET SYSTEM SERVICE FAILURE EXCEPTION MODE
                                     0007    83 ;
                                     0007    84 ; THIS SERVICE PROVIDES THE CAPABILITY TO CONTROL THE GENERATION OF SYSTEM
                                     0007    85 ; SERVICE FAILURE EXCEPTIONS WHEN A SERVICE RETURNS A STATUS VALUE WITH A
                                     0007    86 ; SEVERITY OF ERROR OR SEVERE ERROR.
                                     0007    87 ;
                                     0007    88 ; INPUTS:
                                     0007    89 ;
                                     0007    90 ;	ENBFLG(AP) = FAILURE MODE INDICATOR.
                                     0007    91 ;		LOW BIT CLEAR TO DISABLE FAILURE EXCEPTIONS.
                                     0007    92 ;		LOW BIT SET TO ENABLE FAILURE EXCEPTIONS.
                                     0007    93 ;
                                     0007    94 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0007    95 ;
                                     0007    96 ; OUTPUTS:
                                     0007    97 ;
                                     0007    98 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0007    99 ;
                                     0007   100 ;		R0 = SS$_WASCLR - NORMAL COMPLETION, FAILURE EXCEPTIONS
                                     0007   101 ;			WERE PREVIOUSLY DISABLED.
                                     0007   102 ;
                                     0007   103 ;		R0 = SS$_WASSET - NORMAL COMPLETION, FAILURE EXCEPTIONS
                                     0007   104 ;			WERE PREVIOUSLY ENABLED.
                                     0007   105 ;-
                                     0007   106  
                               0010' 0007   107 	.ENTRY	EXE$SETSFM,^M<R4>
                          51     DC  0009   108 	MOVPSL	R1			;READ CURRENT PSL
            02            16     EF  000B   109 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R1,R1 ;EXTRACT PREVIOUS MODE
            51            51         000E       
            51            06     C0  0010   110 	ADDL	#PCB$V_SSFEXC,R1	;ADD BASE FAILURE ENABLE BIT NUMBER
                          0D     11  0013   111 	BRB	SETMOD			;FINISH IN COMMON CODE
SYSSETMOD       - SYSTEM SERVICES TO SET PROCESS MODES           21-AUG-1978 23:47:31   VAX-11 MACRO X0.3-11               Page   4
01              SET PROCESS SWAP MODE                                                                                            (1)

                                     0015   113 	.SBTTL	SET PROCESS SWAP MODE
                                     0015   114 ;+
                                     0015   115 ; EXE$SETSWM - SET PROCESS SWAP MODE
                                     0015   116 ;
                                     0015   117 ; THIS SERVICE PROVIDES THE CAPABILITY TO LOCK OR UNLOCK A PROCESS FROM
                                     0015   118 ; THE BALANCE SET. LOCKED IN THE BALANCE SET PREVENTS A PROCESS FROM BEING
                                     0015   119 ; SWAPPED OUT OF MEMORY.
                                     0015   120 ;
                                     0015   121 ; INPUTS:
                                     0015   122 ;
                                     0015   123 ;	SWPFLG(AP) = PROCESS SWAP MODE INDICATOR.
                                     0015   124 ;		LOW BIT CLEAR TO DISABLE PROCESS SWAPPING.
                                     0015   125 ;		LOW BIT SET TO ENABLE PROCESS SWAPPING.
                                     0015   126 ;
                                     0015   127 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0015   128 ;
                                     0015   129 ; OUTPUTS:
                                     0015   130 ;
                                     0015   131 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO SET PROCESS SWAP MODE.
                                     0015   132 ;
                                     0015   133 ;		R0 = SS$_NOPRIV - PROCESS DOES NOT HAVE PRIVILEGE TO
                                     0015   134 ;			CHANGE ITS SWAP MODE.
                                     0015   135 ;
                                     0015   136 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0015   137 ;
                                     0015   138 ;		R0 = SS$_WASCLR - NORMAL COMPLETION, PROCESS WAS PREVIOUSLY
                                     0015   139 ;			NOT LOCKED IN BALANCE SET.
                                     0015   140 ;
                                     0015   141 ;		R0 = SS$_WASSET - NORMAL COMPLETION, PROCESS WAS PREVIOUSLY
                                     0015   142 ;			LOCKED IN BALANCE SET.
                                     0015   143 ;-
                                     0015   144  
                                     0015   145 	.ENABL	LSB
                               0010' 0015   146 	.ENTRY	EXE$SETSWM,^M<R4>
            50            24     3C  0017   147 	MOVZWL	#SS$_NOPRIV,R0		;ASSUME PROCESS CANNOT CHANGE SWAP MODE
                                     001A   148 	IFNPRIV	PSWAPM,20$		;PROCESS HAVE PRIVILEGE TO CHANGE SWAP MODE?
            51            04     9A  001F   149 	MOVZBL	#PCB$V_PSWAPM,R1	;SET PROCESS SWAP BIT NUMBER
            50            09     3C  0022   150 SETMOD:	MOVZWL	#SS$_WASSET,R0		;ASSUME PREVIOUSLY SET
         28 A4            51     E0  0025   151 	BBS	R1,PCB$L_STS(R4),10$	;IF SET, MODE CURRENTLY SET
                          03         0029       
            50            01     3C  002A   152 	MOVZWL	#SS$_WASCLR,R0		;PREVIOUSLY CLEAR
            51         04 AC     F0  002D   153 10$:	INSV	ENBFLG(AP),R1,#1,PCB$L_STS(R4) ;SET NEW MODE
         28 A4            01         0031       
                                 04  0034   154 20$:	RET				;
                                     0035   155 	.DSABL	LSB
                                     0035   156  
                                     0035   157 	.END
