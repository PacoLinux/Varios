NLDRIVER        - NULL DEVICE DRIVER                             21-AUG-1978 23:56:34   VAX-11 MACRO X0.3-11               Page   3
01              NULL DEVICE FUNCTION DECISION TABLE ROUTINES                                                                     (1)

                                     0040    68 	.SBTTL	NULL DEVICE FUNCTION DECISION TABLE ROUTINES
                                     0040    69 ;+
                                     0040    70 ; READ - READ FUNCTIONS
                                     0040    71 ; WRITE - WRITE FUNCTIONS
                                     0040    72 ;
                                     0040    73 ; THE NULL DEVICE IS A SPECIAL DEVICE IN THAT ALL WRITES ARE DISCARDED AND ALL
                                     0040    74 ; READS RETURN AN END OF FILE STATUS. THERE IS NO ACTUAL DRIVER, BUT RATHER ALL
                                     0040    75 ; PROCESSING IS DONE BY FUNCTION DECISION TABLE ROUTINES.
                                     0040    76 ;
                                     0040    77 ; INPUTS:
                                     0040    78 ;
                                     0040    79 ;	R0 = SCRATCH.
                                     0040    80 ;	R1 = SCRATCH.
                                     0040    81 ;	R2 = SCRATCH.
                                     0040    82 ;	R3 = ADDRESS OF I/O REQUEST PACKET.
                                     0040    83 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0040    84 ;	R5 = ASSIGNED DEVICE UCB ADDRESS.
                                     0040    85 ;	R6 = ADDRESS OF CCB.
                                     0040    86 ;	R7 = I/O FUNCTION CODE BIT NUMBER.
                                     0040    87 ;	R8 = FUNCTION DECISION TABLE DISPATCH ADDRESS.
                                     0040    88 ;	R9 = SCRATCH.
                                     0040    89 ;	R10 = SCRATCH.
                                     0040    90 ;	R11 = SCRATCH.
                                     0040    91 ;	AP = ADDRESS OF FIRST FUNCTION DEPENDENT PARAMETER.
                                     0040    92 ;
                                     0040    93 ; OUTPUTS:
                                     0040    94 ;
                                     0040    95 ;	IF THE FUNCTION IS A READ, THEN SS$_ENDOFFILE IS RETURNED.
                                     0040    96 ;
                                     0040    97 ;	IF THE FUNCTION IS A WRITE, THEN SS$_NORMAL IS RETURNED.
                                     0040    98 ;-
                                     0040    99  
                                     0040   100 	.ENABL	LSB
                                 00000000   101 	.PSECT	Y$EXEPAGED
            50       0870 8F     3C  0000   102 READ:	MOVZWL	#SS$_ENDOFFILE,R0	;SET END OF FILE STATUS
                          03     11  0005   103 	BRB	10$			;
            50            01     3C  0007   104 WRITE:	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION
                 00000000'EF     17  000A   105 10$:	JMP	EXE$FINISHIOC		;FINISH I/O OPERATION
                                     0010   106 	.DSABL	LSB
                                     0010   107  
                                     0010   108 	.END
