                                     016C 25600 	.SBTTL	DELETE MAILBOX
                                     016C 25700 ;+
                                     016C 25800 ; EXE$DELMBX - DELETE MAILBOX
                                     016C 25900 ;
                                     016C 26000 ; THIS SERVICE PROVIDES THE CAPABILITY TO MARK A PERMANENT MAILBOX FOR
                                     016C 26100 ; DELETION. ACTUAL DELETION OF THE MAILBOX OCCURS WHEN ITS REFERENCE COUNT
                                     016C 26200 ; REACHES ZERO.
                                     016C 26300 ;
                                     016C 26400 ; INPUTS:
                                     016C 26500 ;
                                     016C 26600 ;	DLCHAN(AP) = CHANNEL NUMBER THAT THE MAILBOX TO BE DELETED IS ASSIGNED TO.
                                     016C 26700 ;
                                     016C 26800 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     016C 26900 ;
                                     016C 27000 ; OUTPUTS:
                                     016C 27100 ;
                                     016C 27200 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO MARK MAILBOX FOR DELETE.
                                     016C 27300 ;
                                     016C 27400 ;		R0 = SS$_DEVNOTMBX - SPECIFIED CHANNEL IS NOT ASSIGNED TO A
                                     016C 27500 ;			MAILBOX.
                                     016C 27600 ;
                                     016C 27700 ;		R0 = SS$_IVCHAN - INVALID CHANNEL NUMBER SPECIFIED.
                                     016C 27800 ;
                                     016C 27900 ;		R0 = SS$_NOPRIV - PROCESS DOES NOT HAVE PRIVILEGE TO DELETE A
                                     016C 28000 ;			PERMANENT MAILBOX, SPECIFIED CHANNEL IS NOT ASSIGNED
                                     016C 28100 ;			TO A DEVICE, OR THE CALLER DOES NOT HAVE SUFFICIENT
                                     016C 28200 ;			PRIVILEGE TO ACCESS THE CHANNEL.
                                     016C 28300 ;
