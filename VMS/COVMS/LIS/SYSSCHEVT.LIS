                                     00B1   215 	.SBTTL	BUILD PROTOTYPE TIME QUEUE ENTRY
                                     00B1   216 ;
                                     00B1   217 ; SUBROUTINE TO CHECK ARGUMENTS, ALLOCATE TIME QUEUE ENTRY, AND FILL IN
                                     00B1   218 ; PROTOTYPE TIME QUEUE ENTRY.
                                     00B1   219 ;
                                     00B1   220  
                                     00B1   221 BUILDTQE:				;BUILD TIME QUEUE ENTRY
            50            0C     3C  00B1   222 	MOVZWL	#SS$_ACCVIO,R0		;ASSUME ACCESS VIOLATION
                                     00B4   223 	IFNORD	#8,(R7),40$		;CAN EXPIRATION TIME BE READ?
                                     00BA   224 	SETIPL	#IPL$_ASTDEL		;RAISE TO AST DELIVERY LEVEL
                          9E     16  00BD   225 	JSB	@(SP)+			;CHECK QUOTAS
                          FF3E'  30  00BF   226 	BSBW	EXE$ALLOCTQE		;ALLOCATE TIME QUEUE ENTRY
            5A            50     E9  00C2   227 	BLBC	R0,40$			;IF LBC ALLOCATION FAILURE
         20 A2            55     7D  00C5   228 	MOVQ	R5,TQE$Q_DELTA(R2)	;INSERT REPEAT TIME
         0B A2            58     90  00C9   229 	MOVB	R8,TQE$B_RQTYPE(R2)	;SET REQUEST TYPE
         0C A2            59     D0  00CD   230 	MOVL	R9,TQE$L_PID(R2)	;INSERT TARGET PROCESS ID
         2C A2         60 A4     D0  00D1   231 	MOVL	PCB$L_PID(R4),TQE$L_RQPID(R2) ;INSERT REQUESTER PROCESS ID
            50            67     7D  00D6   232 	MOVQ	(R7),R0			;MAKE SURE EXPIRATION TIME IS IN MEMORY
                                     00D9   233 	SETIPL	#IPL$_SYNCH		;SYNCHRONIZE ACCESS TO SYSTEM DATA BASE
            58       0000'CF     7D  00DC   234 	MOVQ	W^EXE$GQ_SYSTIME,R8	;GET CURRENT ABSOLUTE TIME
            50            50     7D  00E1   235 	MOVQ	R0,R0			;GET EXPIRATION TIME
                          0F     18  00E4   236 	BGEQ	10$			;IF GEQ ALREADY ABSOLUTE TIME
            51            51     CE  00E6   237 	MNEGL	R1,R1			;NEGATE EXPIRATION TIME
