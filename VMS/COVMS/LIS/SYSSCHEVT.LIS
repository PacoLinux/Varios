                                     004F   142 ;+
                                     004F   143 ; EXE$SETIMR - SET TIMER
                                     004F   144 ;
                                     004F   145 ; THIS SERVICE PROVIDES THE CAPABILITY TO SCHEDULE THE SETTING OF AN EVENT
                                     004F   146 ; FLAG AND DECLARATION OF AN AST TO OCCUR AT A SPECIFIED TIME.
                                     004F   147 ;
                                     004F   148 ; INPUTS:
                                     004F   149 ;
                                     004F   150 ;	EFN(AP) = EVENT FLAG NUMBER.
                                     004F   151 ;	TMDAYTIM(AP) = ADDRESS OF DELTA TIME TO EXPIRATION.
                                     004F   152 ;	ASTADR(AP) = ADDRESS OF AST ROUTINE.
                                     004F   153 ;	REQIDT(AP) = REQUEST IDENTIFICATION.
                                     004F   154 ;
                                     004F   155 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     004F   156 ;
                                     004F   157 ; OUTPUTS:
                                     004F   158 ;
                                     004F   159 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO SET TIMER.
                                     004F   160 ;
                                     004F   161 ;		R0 = SS$_ACCVIO - EXPIRATION TIME CANNOT BE READ BY CALLING
                                     004F   162 ;			ACCESS MODE.
                                     004F   163 ;
                                     004F   164 ;		R0 = SS$_EXQUOTA - INSUFFICIENT AST OR TIMER QUOTA REMAINING
                                     004F   165 ;			AND IMPLICIT RESOURCE WAIT IS NOT ENABLED.
                                     004F   166 ;
                                     004F   167 ;		R0 = SS$_ILLEFC - ILLEGAL EVENT FLAG CLUSTER SPECIFIED.
                                     004F   168 ;
                                     004F   169 ;		R0 = SS$_INSFMEM - SUFFICIENT SYSTEM DYNAMIC MEMORY DOES NOT
                                     004F   170 ;			EXIST TO ALLOCATE TIME QUEUE ENTRY AND IMPLICIT
                                     004F   171 ;			RESOURCE WAIT IS NOT ENABLED.
                                     004F   172 ;
                                     004F   173 ;		R0 = SS$_UNASEFC - SPECIFIED EVENT FLAG CLUSTER IS NOT
                                     004F   174 ;			ASSIGNED.
                                     004F   175 ;
                                     004F   176 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     004F   177 ;
                                     004F   178 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     004F   179 ;-
                                     004F   180  
