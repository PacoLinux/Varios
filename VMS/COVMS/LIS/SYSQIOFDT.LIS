SYSQIOFDT       - SYSTEM SERVICE QUEUE I/O FDT SUBROUTINES       21-AUG-1978 23:45:25   VAX-11 MACRO X0.3-11               Page   9
01              CHECK BUFFER ACCESSIBILITY FOR READ FUNCTION                                                                     (1)

                                     00C3   282 	.SBTTL	CHECK BUFFER ACCESSIBILITY FOR READ FUNCTION
                                     00C3   283 ;+
                                     00C3   284 ; EXE$READCHK - CHECK BUFFER ACCESSIBILITY FOR READ FUNCTION
                                     00C3   285 ;
                                     00C3   286 ; THIS ROUTINE IS CALLED TO CHECK BUFFER ACCESSIBILITY FOR A READ I/O
                                     00C3   287 ; FUNCTION.
                                     00C3   288 ;
                                     00C3   289 ; INPUTS:
                                     00C3   290 ;
                                     00C3   291 ;	R0 = ADDRESS OF BUFFER.
                                     00C3   292 ;	R1 = SIZE OF TRANSFER IN BYTES.
                                     00C3   293 ;	R3 = ADDRESS OF I/O REQUEST PACKET.
                                     00C3   294 ;
                                     00C3   295 ; OUTPUTS:
                                     00C3   296 ;
                                     00C3   297 ;	IF BUFFER IS NOT WRITE ACCESSIBLE, THEN THE I/O REQUEST IS TERM-
                                     00C3   298 ;	INATED VIA EXE$IOFINISH WITH A STATUS OF SS$_ACCVIO.
                                     00C3   299 ;
                                     00C3   300 ;	IF BUFFER IS WRITE ACCESSIBLE, THEN THE FOLLOWING VALUES ARE RE-
                                     00C3   301 ;	TURNED:
                                     00C3   302 ;
                                     00C3   303 ;		R0 = ADDRESS OF BUFFER.
                                     00C3   304 ;		R1 = SIZE OF TRANSFER IN BYTES.
                                     00C3   305 ;		R2 = READ FUNCTION INDICATOR (1).
                                     00C3   306 ;		R3 = ADDRESS OF I/O REQUEST PACKET.
                                     00C3   307 ;
                                     00C3   308 ;		IRP$W_BCNT(R3) = SIZE OF TRANSFER IN BYTES.
                                     00C3   309 ;		IRP$W_FUNC(R3) = READ.
                                     00C3   310 ;-
