                                     0000    53 	.SBTTL	PLACE PROCESS IN I/O RESOURCE WAIT
                                     0000    54 ;+
                                     0000    55 ; EXE$IORSNWAIT - PLACE PROCESS IN I/O RESOURCE WAIT
                                     0000    56 ;
                                     0000    57 ; FUNCTIONAL DESCRIPTION:
                                     0000    58 ;
                                     0000    59 ; THIS ROUTINE IS USED BY FDT PROCEEDURES TO RE-START A QIO REQUEST
                                     0000    60 ; AFTER A RESOURCE WAIT. THE CURRENT I/O IS CLEANED UP AND THE PRE-QIO
                                     0000    61 ; STACK IS SET UP. THEN THE PROCESS IS PLACED IN THE WAIT STATE.
                                     0000    62 ; IF THE PROCESS DOES NOT HAVE RESOURCE WIAT ENABLED, THE I/O IS ABORTED
                                     0000    63 ; WITH A STATUS SPECIFIED BY THE CALLER.
                                     0000    64 ;
                                     0000    65 ;
                                     0000    66 ; IMPLICIT INPUTS:
                                     0000    67 ;
                                     0000    68 ;	CALLER MUST BE AT IPL=IPL$_SYNCH OR GREATER.
                                     0000    69 ;
                                     0000    70 ; INPUTS:
                                     0000    71 ;
                                     0000    72 ;	R0 = STATUS TO RETURN IF NO WAIT REQUESTED
                                     0000    73 ;	R1 = RESOURCE NUMBER TO WAIT FOR
                                     0000    74 ;	R3 = ADDRESS OF CURRENT PACKET
                                     0000    75 ;	R4 = ADDRESS OF THE CURRENT PCB
                                     0000    76 ;	R6 = ADDRESS OF CHANNEL CONTROL BLOCK
                                     0000    77 ;
                                     0000    78 ; OUTPUTS:
                                     0000    79 ;
                                     0000    80 ;	R0,R1,R2,R3 ARE USED.
                                     0000    81 ;
                                     0000    82 ;	CONTROL IS TRANSFERED TO EXE$ABORTIO IF NO RESOURCE WAIT
                                     0000    83 ;	HAS BEEN REQUESTED,
                                     0000    84 ;
                                     0000    85 ;	OR TO SCH$WAIT IF RESOURCE WAIT IS REQUESTED.
                                     0000    86 ;
                                     0000    87 ;-
                                     0000    88  
                                     0000    89 EXE$IORSNWAIT::				;PLACE PROCESS IN RESOURCE WIAT
         28 A4            0A     E0  0000    90 	BBS	#PCB$V_SSRWAIT,PCB$L_STS(R4),20$ ;BR IF NO WAIT REQUEST
                          31         0004       
         4C A4            51     9A  0005    91 	MOVZBL	R1,PCB$L_EFWM(R4)	;SET UP WAIT MARKER
                       0A A6     B7  0009    92 	DECW	CCB$W_IOC(R6)		;DECREMENT CHANNEL I/O COUNT
         2A A3            00     E1  000C    93 	BBC	#IRP$V_BUFIO,IRP$W_STS(R3),5$ ;BR IF NOT BUFFERED I/O
                          05         0010       
                       3E A4     B6  0011    94 	INCW	PCB$W_BIOCNT(R4)	;ADJUST COUNT OF BUFFERED I/O
                          03     11  0014    95 	BRB	10$			;CONTINUE
                       44 A4     B6  0016    96 5$:	INCW	PCB$W_DIOCNT(R4)	;ADJUST DIRECT I/O COUNT
         0B A3            06     E1  0019    97 10$:	BBC	#ACB$V_QUOTA,IRP$B_RMOD(R3),15$ ;BR IF AST NOT REQUESTED
                          03         001D       
                       3C A4     B6  001E    98 	INCW	PCB$W_ASTCNT(R4)	;ADJUST AST COUNT
            50            53     D0  0021    99 15$:	MOVL	R3,R0			;DEALLOCATE PACKET
                          FFD9'  30  0024   100 	BSBW	EXE$DEANONPAGED		;
            5C         08 AD     D0  0027   101 	MOVL	8(FP),AP		;RESTORE THE PRE-QIO STACK
            5E            5D     D0  002B   102 	MOVL	FP,SP			;
            52       0000'CF     7E  002E   103 	MOVAQ	W^SCH$GQ_MWAIT,R2	;ADDRESS WAIT LIST
                          FFCA'  31  0033   104 	BRW	SCH$WAIT		;PLACE PROCESS IN WAIT STATE
                                     0036   105 ;
                                     0036   106 ; NO RESOURCE WIAT REQUESTED - ABORT THE I/O
