                                     0071   224 	.SBTTL	READ AND WRITE FUNCTION BUFFER CHECK AND LOCK ROUTINES
                                     0071   225 ;+
                                     0071   226 ; EXE$READLOCK - CHECK BUFFER FOR READ ACCESSIBILITY AND LOCK
                                     0071   227 ; EXE$WRITELOCK - CHECK BUFFER FOR WRITE ACCESSIBILITY AND LOCK
                                     0071   228 ;
                                     0071   229 ; THESE ROUTINES ARE CALLED TO CHECK THE ACCESSIBILITY OF AN I/O BUFFER AND
                                     0071   230 ; TO LOCK THE BUFFER IN MEMORY FOR A DIRECT MEMORY TRANSFER.
                                     0071   231 ;
                                     0071   232 ; INPUTS:
                                     0071   233 ;
                                     0071   234 ;	R0 = STARTING ADDRESS OF I/O BUFFER.
                                     0071   235 ;	R1 = LENGTH OF TRANSFER IN BYTES.
                                     0071   236 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0071   237 ;	R6 = ADDRESS OF CCB.
                                     0071   238 ;
                                     0071   239 ; OUTPUTS:
                                     0071   240 ;
                                     0071   241 ;	THE I/O BUFFER IS CHECKED FOR THE PROPER ACCESSIBILITY. IF THE
                                     0071   242 ;	CHECK SUCCEEDS, THEN THE BUFFER IS LOCKED IN MEMORY AND THE STARTING
                                     0071   243 ;	ADDRESS OF THE PAGE TABLE ENTRIES THAT MAP THE TRANSFER IS STORED
                                     0071   244 ;	IN THE I/O PACKET. ELSE THE I/O IS COMPLETED WITH A STATUS OF
                                     0071   245 ;	ACCESS VIOLATION.
                                     0071   246 ;-
                                     0071   247  
                                     0071   248 	.ENABL	LSB
                                     0071   249 EXE$READLOCK::				;CHECK BUFFER FOR READ FUNCTION AND LOCK
                          50     10  0071   250 	BSBB	EXE$READCHK		;CHECK BUFFER FOR READ FUNCTION
                          02     11  0073   251 	BRB	10$			;
                                     0075   252 EXE$WRITELOCK::				;CHECK BUFFER FOR WRITE FUNCTION AND LOCK
                          77     10  0075   253 	BSBB	EXE$WRITECHK		;CHECK BUFFER FOR WRITE FUNCTION
            50       FE00 8F     AB  0077   254 10$:	BICW3	#^C<VA$M_BYTE>,R0,IRP$W_BOFF(R3) ;SET BYTE OFFSET IN PAGE
                       30 A3         007C       
                          53     DD  007E   255 	PUSHL	R3			;SAVE ADDRESS OF I/O PACKET
                          FF7D'  30  0080   256 	BSBW	MMG$IOLOCK		;LOCK PAGES FOR I/O
            53            8E     D0  0083   257 	MOVL	(SP)+,R3		;RETRIEVE ADDRESS OF I/O PACKET
            05            50     E9  0086   258 	BLBC	R0,20$			;IF LBC LOCK FAILURE
         2C A3            51     D0  0089   259 	MOVL	R1,IRP$L_SVAPTE(R3)	;INSERT ADDRESS OF FIRST PTE IN PACKET
                                 05  008D   260 	RSB				;
                          50     D5  008E   261 20$:	TSTL	R0			;ERRORS ENCOUNTERED?
                          2E     12  0090   262 	BNEQ	50$			;IF NEQ YES
                          51     DD  0092   263 	PUSHL	R1			;SAVE VIRTUAL ADDRESS OF PAGE TO FAULT
                       44 A4     B6  0094   264 	INCW	PCB$W_DIOCNT(R4)	;ADJUST DIRECT I/O COUNT
                       0A A6     B7  0097   265 	DECW	CCB$W_IOC(R6)		;DECREMENT CHANNEL I/O COUNT
         0B A3            06     E1  009A   266 	BBC	#ACB$V_QUOTA,IRP$B_RMOD(R3),30$ ;IF CLR, NO AST SPECIFIED
                          03         009E       
                       3C A4     B6  009F   267 	INCW	PCB$W_ASTCNT(R4)	;ADJUST AST COUNT
            50            53     D0  00A2   268 30$:	MOVL	R3,R0			;SET ADDRESS OF BLOCK TO DEALLOCATE
                          FF58'  30  00A5   269 	BSBW	EXE$DEANONPAGED		;DEALLOCATE I/O PACKET
                          02     BA  00A8   270 	POPR	#^M<R1>			;RETRIEVE VIRTUAL ADDRESS OF PAGE TO FAULT
            5E            5D     D0  00AA   271 	MOVL	FP,SP			;TRIM STACK BACK TO CHANGE MODE FRAME
            5C         08 AD     7D  00AD   272 	MOVQ	8(FP),AP		;RESTORE USER ARGUMENT AND FRAME POINTERS
            5E            00'    C0  00B1   273 	ADDL	S^#EXE$C_CMSTKSZ,SP	;REMOVE CHANGE MODE CALL FRAME FROM STACK
            8E            04     C3  00B4   274 	SUBL3	#4,(SP)+,R0		;CALCULATE RESTART ADDRESS
                          50         00B7       
                       BC'AF     9F  00B8   275 	PUSHAB	B^40$			;SET NEW RETURN ADDRESS
                                 02  00BB   276 	REI				;
                          61     95  00BC   277 40$:	TSTB	(R1)			;FAULT USER BUFFER AGAIN
