                                     004D    93 	.SBTTL	GET JOB AND PROCESS PARAMETERS
                                     004D    94 ;+
                                     004D    95 ; EXE$GETJPP - GET JOB AND PROCESS PARAMETERS
                                     004D    96 ;
                                     004D    97 ; THIS SERVICE PROVIDES THE CAPABILITY TO RETRIEVE INFORMATION ABOUT A
                                     004D    98 ; PROCESS AND THE JOB IT IS ATTACHED TO. THE INFORMATION IS RETURNED IN
                                     004D    99 ; THE SPECIFED BUFFER.
                                     004D   100 ;
                                     004D   101 ; INPUTS:
                                     004D   102 ;
                                     004D   103 ;	PIDADR(AP) = ADDRESS OF PROCESS IDENTIFICATION.
                                     004D   104 ;	LOGNAM(AP) = ADDRESS OF PROCESS NAME STRING DESCRIPTOR.
                                     004D   105 ;	LENGTH(AP) = ADDRESS TO STORE LENGTH OF INFORMATION STRING.
                                     004D   106 ;	BUFFER(AP) = ADDRESS OF INFORMATION BUFFER DESCRIPTOR.
                                     004D   107 ;
                                     004D   108 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     004D   109 ;
                                     004D   110 ; OUTPUTS:
                                     004D   111 ;
                                     004D   112 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO RETURN PROCESS AND JOB INFORMATION.
                                     004D   113 ;
                                     004D   114 ;		R0 = SS$_ACCVIO - INFORMATION BUFFER DESCRIPTOR CANNOT BE
                                     004D   115 ;			READ BY CALLING ACCESS MODE, OR INFORMATION BUFFER
                                     004D   116 ;			OR INFORMATION STRING LENGTH CANNOT BE WRITTEN BY
                                     004D   117 ;			CALLING ACCESS MODE.
                                     004D   118 ;
                                     004D   119 ;		R0 = SS$_IVLOGNAM - ZERO OR GREATER THAN MAXIMUM LENGTH
                                     004D   120 ;			PROCESS NAME STRING SPECIFIED.
                                     004D   121 ;
                                     004D   122 ;		R0 = SS$_NONEXPR - NONEXISTENT PROCESS OR INVALID PROCESS
                                     004D   123 ;			IDENTIFICATION SPECIFIED.
                                     004D   124 ;
                                     004D   125 ;		R0 = SS$_NOPRIV - PROCESS DOES NOT HAVE PRIVILEGE TO GET
                                     004D   126 ;			INFORMATION ABOUT SPECIFIED PROCESS.
                                     004D   127 ;
                                     004D   128 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     004D   129 ;
                                     004D   130 ;		R0 = SS$_BUFFEROVF - NORMAL COMPLETION, ALL PROCESS/JOB
                                     004D   131 ;			INFORMATION DID NOT FIT IN SPECIFIED BUFFER.
                                     004D   132 ;
                                     004D   133 ;		R0 = SS$_NORMAL - NORMAL COMPLETION, ALL PROCESS/JOB
                                     004D   134 ;			INFORMATION TRANSFERED.
                                     004D   135 ;-
                                     004D   136  
                               01FC' 004D   137 	.ENTRY	EXE$GETJPP,^M<R2,R3,R4,R5,R6,R7,R8>
                          FFAE'  30  004F   138 	BSBW	EXE$NAMPID		;GET TARGET PROCESS ID
                                     0052   139 	SETIPL	#0			;ALLOW INTERRUPTS
            4B            50     E9  0055   140 	BLBC	R0,30$			;IF LBC INVALID PROCESS
            57         10 AC     D0  0058   141 	MOVL	BUFFER(AP),R7		;GET ADDRESS OF INFORMATION BUFFER DESCRIPTOR
                                     005C   142 	IFNORD	#8,(R7),40$		;CAN INFORMATION BUFFER DESCRIPTOR BE READ?
            56            67     3C  0062   143 	MOVZWL	(R7),R6			;GET LENGTH OF INFORMATION BUFFER
            58            56     D0  0065   144 	MOVL	R6,R8			;COPY LENGTH OF INFORMATION BUFFER
                          0A     13  0068   145 	BEQL	10$			;IF EQL ZERO LENGTH BUFFER
            57         04 A7     D0  006A   146 	MOVL	4(R7),R7		;GET ADDRESS OF INFORMATION BUFFER
                                     006E   147 	IFNOWRT	R6,(R7),40$		;CAN INFORMATION BUFFER BE WRITTEN?
            50            01     3C  0074   148 10$:	MOVZWL	#SS$_NORMAL,R0		;ASSUME NORMAL COMPLETION
            53       FF85 CF     DE  0077   149 	MOVAL	W^OFFSETABLE,R3		;GET ADDRESS OF OFFSET TABLE
