01              GET JOB AND PROCESS PARAMETERS                                                                                   (1)

            55            54     D0  007C   150 	MOVL	R4,R5			;SET BASE ADDRESS OF INFORMATION STRUCTURE
                          27     10  007F   151 	BSBB	FILBUF			;MOVE INFORMATION TO BUFFER
            55       0000'CF     D0  0081   152 	MOVL	W^SCH$GL_CURPCB,R5	;GET CURRENT PROCESS PCB ADDRESS
         60 A5         60 A4     D1  0086   153 	CMPL	PCB$L_PID(R4),PCB$L_PID(R5) ;TARGET AND CURRENT PROCESS SAME?
                          06     12  008B   154 	BNEQ	20$			;IF NEQ NO
                                     008D   155 ;***
                                     008D   156 ; LOCK PHD IN R5
                                     008D   157 ;***
                                     008D   158 	
            55         64 A4     D0  008D   159 	MOVL	PCB$L_PHD(R4),R5	;SET BASE ADDRESS OF INFORMATION STRUCTURE
                          15     10  0091   160 	BSBB	FILBUF			;MOVE INFORMATION TO BUFFER
                                     0093   161 ;***
                                     0093   162 ; UNLOCK PHD FROM R5
                                     0093   163 ;***
            51         0C AC     D0  0093   164 20$:	MOVL	LENGTH(AP),R1		;GET ADDRESS TO STORE INFORMATION STRING LENGTH
                          0A     13  0097   165 	BEQL	30$			;IF EQL NONE SPECIFIED
                                     0099   166 	IFNOWRT	#2,(R1),40$		;CAN INFORMATION STRING LENGTH BE WRITTEN?
            58            56     A3  009F   167 	SUBW3	R6,R8,(R1)		;STORE LENGTH OF INFORMATION STRING
                          61         00A2       
                                 04  00A3   168 30$:	RET				;
            50            0C     3C  00A4   169 40$:	MOVZWL	#SS$_ACCVIO,R0		;SET ACCESS VIOLATION
                                 04  00A7   170 	RET				;
                                     00A8   171  
                                     00A8   172 ;
                                     00A8   173 ; SUBROUTINE TO FILL INFORMATION BUFFER
                                     00A8   174 ;
                                     00A8   175  
            51            83     9A  00A8   176 FILBUF:	MOVZBL	(R3)+,R1		;GET NUMBER OF BYTES TO MOVE
                          17     13  00AB   177 	BEQL	30$			;IF EQL END OF LIST
            52            83     3C  00AD   178 	MOVZWL	(R3)+,R2		;GET OFFSET TO INFORMATION
                          56     D7  00B0   179 10$:	DECL	R6			;ANY SPACE LEFT IN BUFFER?
                          09     19  00B2   180 	BLSS	20$			;IF LSS NO
            87            8245   90  00B4   181 	MOVB	(R2)+[R5],(R7)+		;MOVE BYTE TO BUFFER
            F5            51     F5  00B8   182 	SOBGTR	R1,10$			;ANY MORE BYTES TO MOVE?
                          EB     11  00BB   183 	BRB	FILBUF			;
                          56     D6  00BD   184 20$:	INCL	R6			;ADJUST COUNT OF REMAINING BYTES
            50       0601 8F     3C  00BF   185 	MOVZWL	#SS$_BUFFEROVF,R0	;SET INFORMATION BUFFER OVERFLOW
                                 05  00C4   186 30$:	RSB				;
                                     00C5   187  
                                     00C5   188 	.END
