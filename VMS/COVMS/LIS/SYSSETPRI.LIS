01              EXE$SETPRI - SET PRIORITY SYSTEM SERVICE ROUTIN                                                                  (1)

                                     0000  7200 	.SBTTL	EXE$SETPRI - SET PRIORITY SYSTEM SERVICE ROUTINE
                                     0000  7300 
                                     0000  7400 ;++
                                     0000  7500 ; FUNCTIONAL DESCRIPTION:
                                     0000  7600 ;
                                     0000  7700 ; CALLING SEQUENCE:
                                     0000  7800 ;	CALLG	ARGLIST,EXE$SETPRI
                                     0000  7900 ;
                                     0000  8000 ; INPUT PARAMETERS:
                                     0000  8100 ;	PIDADR(AP) - ADDRESS OF PID FOR TARGET PROCESS (OPTIONAL)
                                     0000  8200 ;	LOGNAM(AP) - ADDRESS OF PROCESS LOGICAL NAME   (OPTIONAL)
                                     0000  8300 ;	PRI(AP) - BITS 0:4 CONTAIN DESIRED PRIORITY
                                     0000  8400 ;	PRVPRI(AP) - ADDRESS OF LONGWORD TO RECEIVE PREVIOUS PRIORITY
                                     0000  8450 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0000  8500 ;
                                     0000  8600 ; IMPLICIT INPUTS:
                                     0000  8700 ;	CURRENT PCB
                                     0000  8800 ;
                                     0000  8900 ; OUTPUT PARAMETERS:
                                     0000  9000 ;	R0 - COMPLETION STATUS
                                     0000  9100 ;	@PIDADR(AP) - PID OF PROCESS FOR WHICH PRIORITY WAS CHANGED
                                     0000  9200 ;
                                     0000  9300 ; IMPLICIT OUTPUTS:
                                     0000  9400 ;	NONE
                                     0000  9500 ;
                                     0000  9600 ; COMPLETION CODES:
                                     0000  9700 ;	SS$_NORMAL   -   SUCCESSFUL COMPLETION
                                     0000  9800 ;	SS$_NOPRIV   -   INSUFFICIENT PRIVILEGE FOR REQUESTED OPERATION
                                     0000  9900 ;	SS$_NONEXPR  -   NON-EXISTENT PROCESS SPECIFIED
                                     0000 10000 ;
                                     0000 10100 ; SIDE EFFECTS:
                                     0000 10200 ;	A RESCHEDULING INTERRUPT MAY BE REQUESTED IF THE TARGET PROCESS
                                     0000 10300 ;	IS RAISED TO A PRIORITY GREATER THAN THAT OF THE CURRENT PROCESS.
                                     0000 10400 ;
                                     0000 10500 ;--
                                     0000 10600 
                               003C' 0000 10700 	.ENTRY	EXE$SETPRI,^M<R2,R3,R4,R5>	; SET PRIORITY SYSTEM SERVICE
            55         10 AC     D0  0002 10800 	MOVL	PRVPRI(AP),R5		; SAVE PERVIOUS PRIORITY
                          06     13  0006 10900 	BEQL	10$			; NONE SUPPLIED
                                     0008 11000 	IFNOWRT	#4,(R5),ACCVIO		; ACCESS VIOLATION IF NO WRT
                          FFEF'  30  000E 11100 10$:	BSBW	EXE$NAMPID		; LOOK UP PID/PNAME
            3F            50     E9  0011 11200 	BLBC	R0,EXITN		; EXIT IF ERROR
            7E         33 A4     9A  0014 11250 	MOVZBL	PCB$B_PRIB(R4),-(SP)	; SAVE CURRENT PRIORITY
            05            00     EF  0018 11300 	EXTZV	#0,#5,PRI(AP),R3	; ACQUIRE PRIORITY
            53         0C AC         001B       
            1F            53     C3  001E 11400 	SUBL3	R3,#31,R3		; AND CONVERT TO INTERNAL FORM
                          53         0021       
            52       0000'CF     D0  0022 11500 	MOVL	W^SCH$GL_CURPCB,R2	; GET POINTER TO CURRENT PCB
                                     0027 11600 	IFPRIV	SETPRI,20$,R2		; GENERAL SETPRI PRIVILEGE?
            53         33 A2     91  002C 11700 	CMPB	PCB$B_PRIB(R2),R3	; COMPARE DESIRED VALUE WITH CALLER
                          04     15  0030 11800 	BLEQ	20$			; WITHIN RANGE, ALLOW AS IS
            53         33 A2     9A  0032 11900 	MOVZBL	PCB$B_PRIB(R2),R3	; MINIMIZE WITH BASE OF CALLER
         33 A4            53     90  0036 12300 20$:	MOVB	R3,PCB$B_PRIB(R4)	; SET BASE PRIORITY
            52            01     9A  003A 12400 	MOVZBL	#PRI$_IOCOM,R2		; GIVE BENEFIT OF DOUBT
                                     003D 12500 	RPTEVT	SETPRI			; REPORT EVENT
            50            01     3C  0041 12600 	MOVZWL	#SS$_NORMAL,R0		; SET SUCCESS CODE
                                     0044 12700 	SETIPL	#0			; DROP IPL
SYSSETPRI       SET PRIORITY SYSTEM SERVICE                      21-AUG-1978 23:47:55   VAX-11 MACRO X0.3-11               Page   4
01              EXE$SETPRI - SET PRIORITY SYSTEM SERVICE ROUTIN                                                                  (1)

                          55     D5  0047 12705 	TSTL	R5			; RETURN PREVIOUS PRIORITY?
                          04     13  0049 12710 	BEQL	30$			; NO,
            1F            8E     C3  004B 12715 	SUBL3	(SP)+,#31,(R5)		; STORE PREVIOUS PRIORITY IN EXTERNAL FORM
                          65         004E       
                                 04  004F 12720 30$:	RET				; AND RETURN
                                     0050 12800 
            50            0C     3C  0050 12900 ACCVIO:	MOVZWL	#SS$_ACCVIO,R0		; SET ACCESS VIOLATION
                                     0053 13000 EXITN:	SETIPL	#0			; ENABLE INTERRUPTS
                                 04  0056 13100 RETN:	RET				; AND RETURN TO CALLER
                                     0057 13200 
                                     0057 13300 
                                     0057 13400 	.END
