            50         30 A5     D0  009E   124 	MOVL	UCB$L_VCB(R5),R0	;GET ADDRESS OF VCB
                          09     13  00A2   125 	BEQL	90$			;IF EQL NONE
                       30 A5     D4  00A4   126 	CLRL	UCB$L_VCB(R5)		;CLEAR ADDRESS OF VCB
                          FF56'  30  00A7   127 	BSBW	EXE$DEANONPAGED		;DEALLOCATE VCB
                       1A A5     B4  00AA   128 	CLRW	UCB$W_VPROT(R5)		;CLEAR VOLUME PROTECTION MASK
            50         24 A5     D0  00AD   129 90$:	MOVL	UCB$L_DDB(R5),R0	;GET ADDRESS OF DDB
            50         0C A0     D0  00B1   130 	MOVL	DDB$L_DDT(R0),R0	;GET ADDRESS OF DDT
            50         0C A0     D0  00B5   131 	MOVL	DDT$L_CANCEL(R0),R0	;GET ADDRESS OF CANCEL I/O ENTRY POINT
                          10     13  00B9   132 	BEQL	100$			;IF EQL NONE
            53         4C A5     D0  00BB   133 	MOVL	UCB$L_IRP(R5),R3	;GET CURRENT I/O PACKET ADDRESS
            52            57     D0  00BF   134 	MOVL	R7,R2			;SET CHANNEL INDEX
                                     00C2   135 	SETIPL	UCB$B_FIPL(R5)		;RAISE TO DEVICE FORK IPL
                          60     16  00C6   136 	JSB	(R0)			;CALL DRIVER AT CANCEL I/O ENTRY POINT
                                     00C8   137 	SETIPL	#IPL$_ASTDEL		;LOWER TO AST DELIVERY LEVEL
            55         54 A5     D0  00CB   138 100$:	MOVL	UCB$L_AMB(R5),R5	;GET ASSOCIATED MAILBOX UCB ADDRESS
                          06     13  00CF   139 	BEQL	110$			;IF EQL NONE
                       50 A5     B7  00D1   140 	DECW	UCB$W_REFC(R5)		;DECREMENT MAILBOX UCB REFERENCE COUNT
                          FF29'  30  00D4   141 	BSBW	IOC$DELMBX		;CHECK FOR MAILBOX DELETION
            55            66     D0  00D7   142 110$:	MOVL	CCB$L_UCB(R6),R5	;RETRIEVE ASSIGNED DEVICE UCB ADDRESS
                       54 A5     D4  00DA   143 	CLRL	UCB$L_AMB(R5)		;CLEAR ASSOCIATED MAILBOX UCB ADDRESS
                          FF20'  30  00DD   144 	BSBW	IOC$DELMBX		;CHECK FOR MAILBOX DELETION
            50            01     3C  00E0   145 120$:	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION
                          FF1A'  31  00E3   146 	BRW	IOC$UNLOCK		;UNLOCK I/O DATA BASE AND RETURN
                                     00E6   147  
