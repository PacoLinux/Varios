                                     014C 44100 	.SBTTL	EXE$SETPRN - SET PROCESS NAME
                                     014C 44200 
                                     014C 44300 ;++
                                     014C 44400 ; FUNCTIONAL DESCRIPTION:
                                     014C 44500 ;	EXE$SETPRN IMPLEMENTS THE SET PROCESS NAME SYSTEM
                                     014C 44600 ;	SERVICE WHICH ALLOWS A PROCESS TO ESTABLISH A LOGICAL NAME
                                     014C 44700 ;	FOR ITSELF.  ALL SUCH LOGICAL NAMES ARE IMPLICITLY QUALIFIED
                                     014C 44800 ;	BY THE GROUP NUMBER OF THE PROCESS THEREBY ALLOWING THE SAME
                                     014C 44900 ;	LOGICAL NAME TO BE USED BY PROCESSES IN DIFFERENT GROUPS.
                                     014C 45000 ;
                                     014C 45100 ; CALLING SEQUENCE:
                                     014C 45200 ;	CALLG	ARGLIST,EXE$SETPRN
                                     014C 45300 ;
                                     014C 45400 ; INPUT PARAMETERS:
                                     014C 45500 ;	04(AP) - ADDRESS OF PROCESS NAME STRING DESCRIPTOR
                           00000004  014C 45600 PRCNAM=4
                                     014C 45650 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     014C 45700 ;
                                     014C 45800 ; IMPLICIT INPUTS:
                                     014C 45900 ;	SCH$GL_CURPCB - POINTER TO PCB OF CURRENT PROCESS
                                     014C 46000 ;	SCH$AL_PCB - VECTOR OF ALL PCB ADDRESSES
                                     014C 46100 ;
                                     014C 46200 ; OUTPUT PARAMETERS:
                                     014C 46300 ; 	NONE
                                     014C 46400 ;
                                     014C 46500 ; IMPLICIT OUTPUTS:
                                     014C 46600 ;	PCB$T_NAME IN CURRENT PCB IS FILLED WITH THE SPECIFIED NAME
                                     014C 46700 ;	PROVIDED NO ERROR HAS OCURRED.
                                     014C 46800 ;
                                     014C 46900 ; SIDE EFFECTS:
                                     014C 47000 ;	NONE
                                     014C 47100 ; 
                                     014C 47200 ; COMPLETION CODES:
                                     014C 47300 ;	SS$_NORMAL    -  NORMAL SUCCESSFUL COMPLETION STATUS
                                     014C 47400 ;	SS$_ACCVIO    -  ALL OR PART OF NAME STRING IS INACCESSIBLE FOR READ
                                     014C 47500 ;	SS$_IVLOGNAM  -  ILLEGAL LOGICAL NAME STRING LENGTH (>15)
                                     014C 47600 ;	SS$_DUPLNAM   -  DUPLICATE PROCESS NAME WITHIN GROUP
                                     014C 47700 ;
                                     014C 47800 ;--
                                     014C 47900 
                                     014C 48000 EXE$SETPRN::				; SET PROCESS NAME
                               00FC  014C 48100 	.WORD	^M<R2,R3,R4,R5,R6,R7>	; SAVE REGISTERS R2-R7
            55         04 AC     D0  014E 48200 	MOVL	PRCNAM(AP),R5		; GET ADDRESS OF PROCESS NAME
                          05     12  0152 48250 	BNEQ	5$			; WAS SPECIFIED
                       6C A4     D4  0154 48275 	CLRL	PCB$T_LNAME(R4)		; CLEAR NAME FIELD OF PCB
                          53     11  0157 48287 	BRB	65$			; AND EXIT WITH NORMAL STATUS
                                     0159 48300 5$:	IFNORD	#8,(R5),80$		; CHECK ACCESS FOR DESCRIPTOR
            7E            65     7D  015F 48400 	MOVQ	(R5),-(SP)		; PUSH DESCRIPTOR ON STACK
                          6E     B5  0162 48500 	TSTW	(SP)			; CHECK FOR ZERO LENGTH STRING
                          0C     13  0164 48600 	BEQL	10$			; INVALID NAME
                                     0166 48700 	IFNORD	(SP),@4(SP),80$		; PROBE ENDS OF STRING
            0F            6E     B1  016D 48800 	CMPW	(SP),#15		; CHECK FOR MAXIMUM LENGTH
                          06     1B  0170 48900 	BLEQU	20$			; IF LEQU, WITHIN LIMIT
            50       0154 8F     3C  0172 49000 10$:	MOVZWL	#SS$_IVLOGNAM,R0	; INVALID PROCESS NAME STATUS
                                 04  0177 49100 	RET				; AND RETURN
            56       0000'8F     3C  0178 49300 20$:	MOVZWL	#SCH$C_MAXPIX,R6	; SET MAXIMUM PROCESS INDEX
