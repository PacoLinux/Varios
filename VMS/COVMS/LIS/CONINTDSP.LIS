X05             START I/O ON CONSOLE INTERFACE                                                                                   (4)

                                     0040   151 	.SBTTL	START I/O ON CONSOLE INTERFACE
                                     0040   152 ;++
                                     0040   153 ; CON$STARTIO -- START I/O ON CONSOLE INTERFACE
                                     0040   154 ;
                                     0040   155 ; FUNCTIONAL DESCRIPTION:
                                     0040   156 ;
                                     0040   157 ; THIS ROUTINE IS ENTERED TO OUTPUT A CHARACTER TO THE CONSOLE INTERFACE.
                                     0040   158 ; IF THE INTERFACE IS READY THE DATA IS OUTPUT DIRECTLY. IF THE INTERFACE
                                     0040   159 ; IS NOT READY THEN THE DATA IS QUEUED AND SUBSEQUENTLY OUTPUT ON THE
                                     0040   160 ; NEXT READY INTERRUPT.
                                     0040   161 ;
                                     0040   162 ; IN EITHER CASE, A RETURN TO THE CALLER IS DONE TO ENTER A "WAIT FOR
                                     0040   163 ; INTERRUPT" STATE.
                                     0040   164 ;
                                     0040   165 ; INPUTS:
                                     0040   166 ;
                                     0040   167 ;	R3 = DATA TO OUTPUT
                                     0040   168 ;	R5 = UCB ADDRESS
                                     0040   169 ;
                                     0040   170 ; OUTPUTS:
                                     0040   171 ;
                                     0040   172 ;	R3,R4,R5 ARE PRESERVED.
                                     0040   173 ;--
                                     0040   174  
                                     0040   175 CON$STARTIO::				;
                                     0040   176  
                                     0040   177 ;
                                     0040   178 ; INSERT UNIT NUMBER IN DATA WORD
                                     0040   179 ;
                                     0040   180  
            52         48 A5     9A  0040   181 	MOVZBL	UCB$W_UNIT(R5),R2	; PICK UP UNIT NUMBER
            08            52     F0  0044   182 	INSV	R2,#8,#2,R3		; INSERT UNIT NUMBER AND CLEAR OTHER BITS
            53            02         0047       
                                     0049   183  
                                     0049   184 ;
                                     0049   185 ; INITIATE I/O IF NO PREVIOUS UNIT WAITING
                                     0049   186 ;
                 00000000'EF     95  0049   187 	TSTB	CURR			; OTHER UNIT WAITING?
                          10     19  004F   188 	BLSS	10$			; IF LSS NO, OUTPUT DATA AND RETURN
   00000001'EF            52     90  0051   189 	MOVB	R2,NEXT			; SAVE UNIT NUMBER
   00000002'EF            53     B0  0058   190 	MOVW	R3,DATA			; SAVE DATA
                          0A     11  005F   191 	BRB	20$			; EXIT
                                     0061   192 10$:					;
   00000000'EF            52     90  0061   193 	MOVB	R2,CURR			; SAVE UNIT NUMBER OF CURRENT SENDER
            23            53     DA  0068   194 	MTPR	R3,#PR$_TXDB		; SEND DATA
                                     006B   195 20$:					;
                                 05  006B   196 	RSB				;
                                     006C   197  
CONINTDSP       -  CONSOLE INTERRUPT DISPATCHER                  21-AUG-1978 23:49:40   VAX-11 MACRO X0.3-11               Page   6
X05             CONSOLE TRANSMITTER INTERRUPT SERVICE                                                                            (5)

                                     006C   199 	.SBTTL	CONSOLE TRANSMITTER INTERRUPT SERVICE
                                     006C   200  
                                     006C   201 ;++
                                     006C   202 ; CON$INTOUT - CONSOLE TRANSMITTER INTERRUPT SERVICE
                                     006C   203 ;
                                     006C   204 ; FUNCTIONAL DESCRIPTION:
                                     006C   205 ;
                                     006C   206 ; THIS ROUTINE IS ENTERED WHEN A CONSOLE UNIT READY INTERRUPT OCCURS.
                                     006C   207 ;
                                     006C   208 ; A SEARCH IS MADE OF THE COLD STORAGE BUFFER FOR DATA TO OUTPUT.
                                     006C   209 ; IF ANY IS FOUND THEN IT IS OUTPUT. IF NO VALID DATA IS FOUND THEN
                                     006C   210 ; THE INTERRUPT STATE OF THE UNIT IS CHECKED FOR EXPECTED INTERRUPT.
                                     006C   211 ; IF NO INTERRUPT IS EXPECTED, THE INTERRUPT IS DISMISSED. IF AN INTERRUPT
                                     006C   212 ; IS EXPECTED THEN THE DRIVER IS ENTERED. IN THE CASE OF THE CONSOLE TERMINAL,
                                     006C   213 ; A SPECIFIC ROUTINE IS ENTERED TO GET THE NEXT CHARACTER AVAILABLE TO OUTPUT
                                     006C   214 ; ON THE UNIT. IN THE CASE OF THE CONSOLE FLOPPY, THE WAIT FOR INTERRUPT
                                     006C   215 ; CONTEXT IS TAKEN FROM THE UCB AND THE SPECIFIED ROUTINE IS ENTERED.
                                     006C   216 ;
                                     006C   217 ;
                                     006C   218 ; INPUTS:
                                     006C   219 ;
                                     006C   220 ;	R2,R3,R4,R5 ARE SAVED ON THE INTERRUPT STACK.
                                     006C   221 ;
                                     006C   222 ;	00(SP) = ADDRESS OF THE IDB
                                     006C   223 ;
                                     006C   224 ; OUTPUTS:
                                     006C   225 ;
                                     006C   226 ;	THE SAVED REGISTERS ARE RESTORES BEFORE REI.
                                     006C   227 ;--
                                     006C   228 CON$INTOUT::				;
            54            9E     D0  006C   229 	MOVL	@(SP)+,R4		; GET ADDRESS OF IDB
            52   00000001'EF     98  006F   230 	CVTBL	NEXT,R2			; GET UNIT NUMBER OF NEXT SENDER
                          11     19  0076   231 	BLSS	10$			; IF LSS NO SENDER WAITING
   00000001'EF            01     8E  0078   232 	MNEGB	#1,NEXT			; RESET SENDER-WAITNG
            53   00000002'EF     3C  007F   233 	MOVZWL	DATA,R3			; GET DATA TO SEND
            23            53     DA  0086   234 	MTPR	R3,#PR$_TXDB		; TRANSMIT DATA
                                     0089   235 10$:					;
            53   00000000'EF     9A  0089   236 	MOVZBL	CURR,R3			; GET UNIT NUMBER OF CURRENT SENDER
   00000000'EF            52     90  0090   237 	MOVB	R2,CURR			; SET UNIT NUMBER OF NEXT CURRENT
                          53     95  0097   238 	TSTB	R3			; SPURIOUS INTERRUPT?
                          28     19  0099   239 	BLSS	DISMIS			; IF LSS YES
            55         10 A443   D0  009B   240 	MOVL	IDB$L_UCBLST(R4)[R3],R5	; GET ADDRESS OF UCB
         58 A5            07     E5  00A0   241 	BBCC	#UCB$V_INTTYPE,UCB$W_STS(R5),FLOPINT ; SET OUTPUT INTERRUPT
                          00         00A4       
                                     00A5   242 FLOPINT:				;
         58 A5            00     E5  00A5   243 	BBCC	#UCB$V_TIM,UCB$W_STS(R5),10$ ; CLEAR TIMEOUT PENDING
                          00         00A9       
                                     00AA   244 10$:					;
         58 A5            01     E5  00AA   245 	BBCC	#UCB$V_INT,UCB$W_STS(R5),DISMIS ; EXIT IF NOT WAITING FOR INTERRUPT
                          14         00AE       
         34 A5            02     E1  00AF   246 	BBC	#DEV$V_TRM,UCB$L_DEVCHAR(R5),20$ ; IF BIT CLEAR, UNIT = FLOPPY
                          08         00B3       
                       C3'AF     9F  00B4   247 	PUSHAB	B^DISMIS		; PUSH ADDRESS TO DISMISS INTERRUPT
                          FF46'  30  00B7   248 	BSBW	TTY$GETNEXTCHAR		; GET ANOTHER CHARACTER
                          84     11  00BA   249 	BRB	CON$STARTIO		; INITIATE I/O
                                     00BC   250 20$:					;
            54         14 A5     D0  00BC   251 	MOVL	UCB$L_FR4(R5),R4	; RESTORE R4 CONTEXT
CONINTDSP       -  CONSOLE INTERRUPT DISPATCHER                  21-AUG-1978 23:49:40   VAX-11 MACRO X0.3-11               Page   7
X05             CONSOLE TRANSMITTER INTERRUPT SERVICE                                                                            (5)

                       0C B5     16  00C0   252 	JSB	@UCB$L_FPC(R5)		; ENTER FLOPPY INTERRUPT HANDLER
                                     00C3   253 DISMIS:					;
                          3C     BA  00C3   254 	POPR	#^M<R2,R3,R4,R5>	; RESTORE REGISTERS
                                 02  00C5   255 	REI				;
                                     00C6   256  
                                     00C6   257 	.END
