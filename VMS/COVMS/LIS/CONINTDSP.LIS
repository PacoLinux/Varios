X05             RECIEVER INTERRUPT DISPATCHER                                                                                    (3)

                                     0016    86 	.SBTTL RECIEVER INTERRUPT DISPATCHER
                                     0016    87 ;++
                                     0016    88 ; CON$INTINP -- CONSOLE INTERRUPT ON INPUT READY
                                     0016    89 ;
                                     0016    90 ; FUNCTIONAL DESCRIPTION:
                                     0016    91 ;
                                     0016    92 ; THIS ROUTINE IS ENTERED AS A RESULT OF A RECEIVER INTERRUPT ON THE
                                     0016    93 ; CONSOLE INTERFACE. THE INTERRUPT CAN BE GENERATED BY THE CONSOLE
                                     0016    94 ; TERMINAL OR FLOPPY DRIVE.
                                     0016    95 ;
                                     0016    96 ; CONSOLE TERMINAL:	ALL RECEIVED DATA CHARACTERS ARE CONSIDERED
                                     0016    97 ;			UNSOLICITED AND RESULT IN AN ENTRY INTO THE
                                     0016    98 ;			TERMINAL DRIVER COMMON CHARACTER BUFFERING
                                     0016    99 ;			ROUTINE "TTY$PUTNEXTCHAR".
                                     0016   100 ;
                                     0016   101 ; CONSOLE FLOPPY:	RECEIVER INTERRUPTS FROM THE CONSOLE FLOPPY
                                     0016   102 ;			MUST BE EXPECTED (UCB$V_INT = 1). IF THE 
                                     0016   103 ;			INTERRUPT IS EXPECTED THEN UCB$V_INTTYPE IS SET
                                     0016   104 ;			IN UCB$W_STS TO SIGNAL A RECEIVER INTERRUPT.
                                     0016   105 ;			THE DRIVER IS THEN ENTERED AT ITS WAIT FOR INTERRUPT
                                     0016   106 ;			CONTEXT PC.
                                     0016   107 ;
                                     0016   108 ; INPUTS:
                                     0016   109 ;
                                     0016   110 ;	R2,R3,R4,R5 ARE SAVED ON THE INTERRUPT STACK.
                                     0016   111 ;
                                     0016   112 ;	00(SP) = ADDRESS OF THE IDB
                                     0016   113 ;
                                     0016   114 ; OUTPUTS:
                                     0016   115 ;
                                     0016   116 ;	THE SAVED REGISTERS ARE RESTORED BEFORE REI.
                                     0016   117 ;
                                     0016   118 ; IMPLICIT OUTPUTS:
                                     0016   119 ;
                                     0016   120 ; ON ENTRY INTO THE DRIVER,
                                     0016   121 ;
                                     0016   122 ;	R3 = DATA FROM PR$_RXDB
                                     0016   123 ;	R4 = UCB$L_FR4
                                     0016   124 ;	R5 = UCB ADDRESS
                                     0016   125 ;--
                                     0016   126 CON$INTINP::
            53            21     DB  0016   127 	MFPR	#PR$_RXDB,R3		; MOVE DATA FROM INTERFACE
                                     0019   128 ;
                                     0019   129 ; GET THE ASSOCIATED UCB
                                     0019   130 ;
            54            9E     D0  0019   131 	MOVL	@(SP)+,R4		; GET IDB ADDRESS
            02            08     EF  001C   132 	EXTZV	#8,#2,R3,R2		; EXTRACT THE UNIT NUMBER
            52            53         001F       
         0B A4            52     91  0021   133 	CMPB	R2,IDB$B_UNITS(R4)	; LEGAL UNIT NUMBER?
                          03     1B  0025   134 	BLEQU	5$			; IF LEQU YES
                          0099   31  0027   135 	BRW	DISMIS			; DISMISS SPURIOUS INTERRUPT
                                     002A   136 5$:					;
            55         10 A442   D0  002A   137 	MOVL	IDB$L_UCBLST(R4)[R2],R5	; GET THE UCB ADDRESS
         58 A5            07     E2  002F   138 	BBSS	#UCB$V_INTTYPE,UCB$W_STS(R5),10$ ; SET RECEIVER INTERRUPT
                          00         0033       
                                     0034   139 10$:					;
                                 E1  0034   140 	BBC	#DEV$V_TRM,-		; ENTER FLOPPY INTERRUPT CODE
CONINTDSP       -  CONSOLE INTERRUPT DISPATCHER                  21-AUG-1978 23:49:40   VAX-11 MACRO X0.3-11               Page   4
X05             RECIEVER INTERRUPT DISPATCHER                                                                                    (3)

         34 A5            02         0035   141 		UCB$L_DEVCHAR(R5),FLOPINT; IF NOT TERMINAL UCB
                          6C         0038       
                                     0039   142 ;
                                     0039   143 ; CONSOLE TERMINAL INTERRUPT
                                     0039   144 ;
                     00C3'CF     9F  0039   145 	PUSHAB	W^DISMIS		; PUSH "NO OUTPUT TO DO" RETURN ADDRESS
                                     003D   146 ;
                                     003D   147 ; GOTO CHARACTER BUFFER ROUTINE AS CO-ROUTINE
                                     003D   148 ;
                          FFC0'  30  003D   149 	BSBW	TTY$PUTNEXTCHAR		; BUFFER THE CHARACTER
