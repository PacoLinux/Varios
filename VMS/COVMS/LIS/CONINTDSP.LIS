                                     0000    51 	.SBTTL	CONSOLE INTERFACE INITIALIZATION
                                     0000    52 ;++
                                     0000    53 ; CON$INITIAL -- INITIALIZE CONSOLE INTERFACE
                                     0000    54 ;
                                     0000    55 ; FUNCTIONAL DESCRIPTION:
                                     0000    56 ;
                                     0000    57 ; THIS ROUTINE IS CALLED DURING SYSTEM STARTUP TO INITIALIZE THE
                                     0000    58 ; CONTROLLER ASSOCIATED WITH A PARTICULAR CRB.
                                     0000    59 ;
                                     0000    60 ; IN THE CASE OF THE CONSOLE, RECEIVER AND TRANSMITTER INTERRUPTS
                                     0000    61 ; ARE ENABLED.
                                     0000    62 ;
                                     0000    63 ; INPUTS:
                                     0000    64 ;
                                     0000    65 ;	R4 = CSR FROM ASSOCIATED IDB
                                     0000    66 ;	R5 = IDB ADDRESS
                                     0000    67 ;	R9 = CRB ADDRESS
                                     0000    68 ;
                                     0000    69 ;	IPL = IPL$V_POWER
                                     0000    70 ;
                                     0000    71 ; THE CSR HAS NOT BEEN ADDRESSED AND MAY CAUSE A FAULT IF THE DEVICE
                                     0000    72 ; IS NOT PRESENT. 
                                     0000    73 ;
                                     0000    74 ; OUTPUTS:
                                     0000    75 ;
                                     0000    76 ;	ALL REGISTERS ARE PRESERVED.
                                     0000    77 ;--
                                     0000    78 CON$INITIAL::				; INITIALIZE CONSOLE INTERFACE
            20   00000040 8F     DA  0000    79 	MTPR	#^X040,#PR$_RXCS	; ENABLE INPUT INTERRUPTS
            22   00000040 8F     DA  0007    80 	MTPR	#^X040,#PR$_TXCS	; ENABLE OUTPUT INTERRUPTS
   00000000'EF            01     AE  000E    81 	MNEGW	#1,CURR			; SET NO UNITS WAITING FOR OUTPUT
                                     0015    82 CON$INITLINE::
                                     0015    83 CON$DISCON::
                                 05  0015    84 	RSB				;
