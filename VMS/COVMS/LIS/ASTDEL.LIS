                                     01C8 39700 80$:	ENBINT				; ENABLE INTERRUPTS
            50            01     3C  01CB 39800 	MOVZWL	#SS$_NORMAL,R0		; SET SUCCESS STATUS CODE
                                 05  01CE 39900 	RSB				; AND RETURN
                                     01CF 40000  
