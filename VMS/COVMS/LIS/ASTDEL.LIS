                                     00BE 23000 ;
                                     00BE 23100 ;	CALL AST ROUTINE WITH AST ARGUMENT LIST
                                     00BE 23200 ;
                                     00BE 23300 ;	THE CALL IS EXECUTED AT THE MODE WHICH RECEIVED THE AST WITH
                                     00BE 23400 ;	THE AST ARGUMENT LIST ON THE TOP OF THE STACK.  WHEN THE
                                     00BE 23500 ;	AST ROUTINE RETURNS FROM THE CALL, AN ASTEXIT CHANGE MODE
                                     00BE 23600 ;	TO KERNEL INSTRUCTION WILL BE ISSUED.  ASTEXIT WILL RESET 
                                     00BE 23700 ;	THE AST ACTIVE BIT FOR THE CURRENT MODE AND MAY CAUSE DELIVERY 
                                     00BE 23800 ;	OF ADDITIONAL ASTS.
                                     00BE 23900 ;
                                     00BE 24000 ;	AST ARGUMENT LIST:
                                     00BE 24100 ;	------------------
                                     00BE 24200 ;
                                     00BE 24300 ;	00(SP) = NUMBER OF ARGUMENTS, =5
                                     00BE 24400 ;	04(SP) = AST PARAMETER
                                     00BE 24500 ;	08(SP) = SAVED R0
                                     00BE 24600 ;	12(SP) = SAVED R1
                                     00BE 24700 ;	16(SP) = SAVED PC
                                     00BE 24800 ;	20(SP) = SAVED PSL
                                     00BE 24900 ;
                                     00BE 25000 EXE$ASTDEL:				; DELIVER AST CALL
            61            6E     FA  00BE 25100 	CALLG	(SP),(R1)		; CALL AST ROUTINE
            5E            08     C0  00C1 25200 	ADDL	#8,SP			; REMOVE ARG COUNT AND ASTPRM
                          00     BC  00C4 25300 	CHMK	S^#ASTEXIT		; AND EXIT FROM AST ROUTINE
                          03     BA  00C6 25400 	POPR	#^M<R0,R1>		; RESTORE R0,R1
                                 02  00C8 25500 	REI				; EXECUTE REI IN MODE OF AST
                                     00C9 25600 
                                     00C9 25700 ;
                                     00C9 25800 ;	A WINDOW EXISTS BETWEEN THE ASTEXIT CHMK AND THE REI, DURING
                                     00C9 25900 ;	WHICH 4 LONGWORDS REMAIN ON THE STACK OF THE ACCESS MODE EXITING
                                     00C9 26000 ;	THE AST.  IF ASTS OCCUR MORE RAPIDLY FOR AN ACCESS LEVEL THAN
                                     00C9 26100 ;	THEY CAN BE PROCESSED, THERE WILL BE AN ACCUMULATION OF THESE
                                     00C9 26200 ;	4 LONGWORD UNITS ON THE STACK OF THAT MODE UNTIL THE STACK
                                     00C9 26300 ;	OVERFLOWS.
                                     00C9 26400 ;	
                                     00C9 26500 ;	IF THIS BECOMES A PROBLEM THE ASTEXIT CHMK CAN BE MADE TO 
                                     00C9 26600 ;	CONCLUDE THE STACK CLEANING AT A SUBSTANTIAL COST IN TIME
                                     00C9 26700 ;	AND CODE.  
                                     00C9 26800 
