                                     0138 31600 	.SBTTL	SCH$QAST - ENQUEUE AST CONTROL BLOCK FOR PROCESS
                                     0138 31700 
                                     0138 31800 ;++
                                     0138 31900 ;
                                     0138 32000 ; FUNCTIONAL DESCRIPTION:
                                     0138 32100 ;	SCH$QAST INSERTS THE AST CONTROL BLOCK SUPPLIED IN THE PROPER
                                     0138 32200 ;	POSITION BY ACCESS MODE IN THE AST QUEUE OF THE PROCESS SPECIFIED
                                     0138 32300 ;	BYT THE PID FIELD OF THE AST CONTROL BLOCK.  AN AST ARRIVAL EVENT
                                     0138 32400 ;	IS THEN REPORTED FOR THE PROCESS TO REACTIVATE FROM A WAIT STATE
                                     0138 32500 ;	IF APPROPRIATE.  THE AST CONTROL BLOCK WILL BE RELEASED IMMEDIATELY
                                     0138 32600 ;	IF THE PID SPECIFIES A NON-EXISTENT PROCESS.
                                     0138 32700 ;
                                     0138 32800 ; CALLING SEQUENCE:
                                     0138 32900 ;	BSB/JSB	SCH$QAST
                                     0138 33000 ;
                                     0138 33100 ; INPUT PARAMETERS:
                                     0138 33200 ;	R2 - PRIORITY INCREMENT CLASS
                                     0138 33300 ;	R5 - POINTER TO AST CONTROL BLOCK
                                     0138 33400 ;
                                     0138 33500 ; IMPLICIT INPUTS:
                                     0138 33600 ;	PCB OF PROCESS IDENTIFIED BY PID FIELD
                                     0138 33700 ;
                                     0138 33800 ; OUTPUT PARAMETERS:
                                     0138 33900 ;	R0 - COMPLETION STATUS CODE
                                     0138 34000 ;	R4 - PCB ADDRESS OF PROCESS FOR WHICH AST WAS QUEUED
                                     0138 34100 ;
                                     0138 34200 ; IMPLICIT OUTPUTS:
                                     0138 34300 ;	*** TBS ***
                                     0138 34400 ;
                                     0138 34500 ; SIDE EFFECTS:
                                     0138 34600 ;	THE PROCESS IDENTIFIED BY THE PID IN THE AST CONTROL BLOCK
                                     0138 34700 ;	WILL BE MADE EXECUTABLE IF NOT SUSPENDED.
                                     0138 34800 ;
                                     0138 34900 ; COMPLETION CODES:
                                     0138 35000 ;	SS$_NORMAL  -  NORMAL SUCCESSFUL COMPLETION STATUS
                                     0138 35100 ;	SS$_NONEXPR -  NON-EXISTENT PROCESS
                                     0138 35200 ;
                                     0138 35300 ;--
                                     0138 35400 
                                     0138 35500 SCH$QAST::				; ENQUEUE AST FOR PROCESS
            50         0C A5     3C  0138 35600 	MOVZWL	ACB$L_PID(R5),R0	; GET PROCESS INDEX FOR AST TARGET
                                     013C 35700 	DSBINT	#IPL$_SYNCH		; DISABLE SYSTEM EVENTS
            54       0000'CF40   D0  0142 35800 	MOVL	W^SCH$AL_PCB[R0],R4	; LOOK UP PCB ADDRESS
         60 A4         0C A5     D1  0148 35900 	CMPL	ACB$L_PID(R5),PCB$L_PID(R4) ; CHECK FOR MATCH IN PID
                          0F     13  014D 36000 	BEQL	20$			; OK, PID MATCHES
                                     014F 36100 10$:					; PROCESS NOT FOUND
            50            55     D0  014F 36200 	MOVL	R5,R0			; RELEASE AST CONTROL BLOCK
                          FEAB'  30  0152 36300 	BSBW	EXE$DEANONPAGED		; IF NO SUCH PROCESS
                                     0155 36400 
                                     0155 36500 	ENBINT				; ENABLE INTERRUPTS
            50       08E8 8F     3C  0158 36600 	MOVZWL	#SS$_NONEXPR,R0		; SET ERROR STATUS CODE
                                 05  015D 36700 	RSB				; AND RETURN
                                     015E 36800 
                                     015E 36900 20$:					; PROCESS EXISTS
            02            00     EF  015E 37000 	EXTZV	#ACB$V_MODE,#ACB$S_MODE,ACB$B_RMOD(R5),R0  ; EXTRACT REQUEST ACCESS MODE
            50         0B A5         0161       
            51         10 A4     DE  0164 37100 	MOVAL	PCB$L_ASTQFL(R4),R1	; POINT TO QUEUE HEADER
