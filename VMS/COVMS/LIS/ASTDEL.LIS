            51         64 A4     D0  01AA 39000 50$:	MOVL	PCB$L_PHD(R4),R1	; POINT TO PROCESS HEADER
            18            50     F0  01AE 39100 	INSV	R0,#PHD$V_ASTLVL,#PHD$S_ASTLVL,PHD$L_P0LRASTL(R1)
       00CC C1            08         01B1       
         28 A4            11     E2  01B5 39200 	BBSS	#PCB$V_ASTPEN,PCB$L_STS(R4),60$	; SET AST PENDING
                          00         01B9       
            54       0000'CF     D1  01BA 39300 60$:	CMPL	W^SCH$GL_CURPCB,R4	; IS PROCESS CURRENT PROCESS
                          03     12  01BF 39400 	BNEQ	70$			; NO,
