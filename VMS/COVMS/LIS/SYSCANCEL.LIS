         34 A5            13     E1  0097   133 	BBC	#DEV$V_MNT,UCB$L_DEVCHAR(R5),60$ ;IF CLR, DEVICE DISMOUNTED
                          EF         009B       
            51       0000'8F     3C  009C   134 	MOVZWL	#SS$_EXQUOTA,R1		;SET EXCEEDED QUOTA STATUS
            50            01     3C  00A1   135 	MOVZWL	#RSN$_ASTWAIT,R0	;SET AST WAIT RESOURCE WAIT NUMBER
