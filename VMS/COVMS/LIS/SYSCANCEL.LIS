            51         4C 8F     9A  00A9   138 	MOVZBL	#IRP$C_LENGTH,R1	;SET LENGTH OF I/O PACKET
                          FF50'  30  00AD   139 	BSBW	EXE$ALONONPAGED		;ALLOCATE I/O PACKET
            13            50     E8  00B0   140 	BLBS	R0,100$			;IF LBS SUCCESSFUL ALLOCATION
            51       0000'8F     3C  00B3   141 	MOVZWL	#SS$_INSFMEM,R1		;SET INSUFFICIENT MEMORY STATUS
            50            03     3C  00B8   142 	MOVZWL	#RSN$_NPDYNMEM,R0	;SET NONPAGED DYNAMIC MEMORY RESOURCE NUMBER
         28 A4            0A     E0  00BB   143 90$:	BBS	#PCB$V_SSRWAIT,PCB$L_STS(R4),60$ ;IF SET, NO WAIT
                          CB         00BF       
                          FF3D'  30  00C0   144 	BSBW	SCH$RWAIT		;WAIT FOR RESOURCE
                          FF4F   31  00C3   145 	BRW	10$			;TRY AGAIN
                       44 A4     B7  00C6   146 100$:	DECW	PCB$W_DIOCNT(R4)	;UPDATE DIRECT I/O COUNT
                       0A A6     B6  00C9   147 	INCW	CCB$W_IOC(R6)		;INCREMENT I/O COUNT
            53            52     D0  00CC   148 	MOVL	R2,R3			;COPY ADDRESS OF I/O PACKET
            52            08     C0  00CF   149 	ADDL	#IRP$W_SIZE,R2		;CALCULATE ADDRESS OF PACKET SIZE
            82         4C 8F     9B  00D2   150 	MOVZBW	#IRP$C_LENGTH,(R2)+	;INSERT SIZE OF I/O REQUEST PACKET
            82            0A     9B  00D6   151 	MOVZBW	#DYN$C_IRP,(R2)+	;INSERT DATA STRUCTURE TYPE AND ZERO MODE
