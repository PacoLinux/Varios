                                     0000    54 	.SBTTL	CANCEL I/O ON CHANNEL
                                     0000    55 ;+
                                     0000    56 ; EXE$CANCEL - CANCEL I/O ON CHANNEL
                                     0000    57 ;
                                     0000    58 ; THIS SERVICE CANCELS ALL I/O ISSUED TO A DEVICE FORM THE SPECIFIED CHANNEL.
                                     0000    59 ;
                                     0000    60 ; INPUTS:
                                     0000    61 ;
                                     0000    62 ;	CHAN(AP) = NUMBER OF THE I/O CHANNEL TO CANCEL I/O FOR.
                                     0000    63 ;
                                     0000    64 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    65 ;
                                     0000    66 ; OUTPUTS:
                                     0000    67 ;
                                     0000    68 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO CANCEL I/O.
                                     0000    69 ;
                                     0000    70 ;		SS$_EXQUOTA - DIRECT I/O QUOTA EXCEEDED WHILE TRYING TO
                                     0000    71 ;			CANCEL FILE I/O.
                                     0000    72 ;
                                     0000    73 ;		SS$_INSFMEM - INSUFFICIENT MEMORY AVAILABLE TO ALLOCATE I/O
                                     0000    74 ;			PACKET.
                                     0000    75 ;
                                     0000    76 ;		SS$_IVCHAN - INVALID CHANNEL NUMBER SPECIFIED.
                                     0000    77 ;
                                     0000    78 ;		SS$_NOPRIV - SPECIFIED CHANNEL IS NOT ASSIGNED TO A DEVICE
                                     0000    79 ;			OR THE CALLER DOES NOT HAVE SUFFICIENT PRIVILEGE TO
                                     0000    80 ;			ACCESS THE CHANNEL.
                                     0000    81 ;
                                     0000    82 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    83 ;
                                     0000    84 ;		SS$_NORMAL - NORMAL COMPLETION.
                                     0000    85 ;-
                                     0000    86  
                               00FC' 0000    87 	.ENTRY	EXE$CANCEL,^M<R2,R3,R4,R5,R6,R7>
            50         04 AC     3C  0002    88 	MOVZWL	CHAN(AP),R0		;GET I/O CHANNEL NUMBER
                          FFF7'  30  0006    89 	BSBW	IOC$VERIFYCHAN		;VERIFY I/O CHANNEL NUMBER
            57            52     D0  0009    90 	MOVL	R2,R7			;SAVE CHANNEL INDEX
            56            51     D0  000C    91 	MOVL	R1,R6			;SAVE ADDRESS OF CCB
            7F            50     E9  000F    92 	BLBC	R0,70$			;IF LBC INVALID CHANNEL
            55            66     D0  0012    93 	MOVL	CCB$L_UCB(R6),R5	;GET ASSIGNED DEVICE UCB ADDRESS
                          7E     DC  0015    94 10$:	MOVPSL	-(SP)			;SAVE CURRENT PROCESSOR STATUS
                                     0017    95 	SETIPL	#IPL$_ASTDEL		;RAISE TO AST DELIVERY LEVEL
                       0A A6     B5  001A    96 	TSTW	CCB$W_IOC(R6)		;ANY I/O OUTSTANDING?
                          48     13  001D    97 	BEQL	40$			;IF EQL NO
            53         40 A5     9E  001F    98 	MOVAB	UCB$L_IOQFL(R5),R3	;GET ADDRESS OF I/O QUEUE LISTHEAD
            52            53     D0  0023    99 	MOVL	R3,R2			;COPY ADDRESS OF I/O QUEUE LISTHEAD
                                     0026   100 	SETIPL	UCB$B_FIPL(R5)		;RAISE TO DRIVER FORK IPL
            52            62     D0  002A   101 20$:	MOVL	IRP$L_IOQFL(R2),R2	;GET ADDRESS OF NEXT I/O PACKET IN QUEUE
            53            52     D1  002D   102 	CMPL	R2,R3			;END OF QUEUE?
                          35     13  0030   103 	BEQL	40$			;IF EQL YES
         2A A2            04     E0  0032   104 	BBS	#IRP$V_VIRTUAL,IRP$W_STS(R2),20$ ;IF SET, VIRTUAL I/O REQUEST
                          F3         0036       
         60 A4         0C A2     D1  0037   105 	CMPL	IRP$L_PID(R2),PCB$L_PID(R4) ;PROCESS ID MATCH?
                          EC     12  003C   106 	BNEQ	20$			;IF NEQ NO
         28 A2            57     B1  003E   107 	CMPW	R7,IRP$W_CHAN(R2)	;I/O CHANNEL NUMBER MATCH?
                          E6     12  0042   108 	BNEQ	20$			;IF NEQ NO
            52         04 A2     D0  0044   109 	MOVL	IRP$L_IOQBL(R2),R2	;GET BACKWARD LINK OF CURRENT ENTRY
