SYSASTCON       ASTCONTROL SYSTEM SERVICES                       21-AUG-1978 23:34:47   VAX-11 MACRO X0.3-11               Page   3
01              EXE$SETAST - SET AST ENABLES                                                                                     (1)

                                     0000  7200 	.SBTTL	EXE$SETAST - SET AST ENABLES
                                     0000  7300 
                                     0000  7400 ;++
                                     0000  7500 ; FUNCTIONAL DESCRIPTION:
                                     0000  7600 ;	EXE$SETAST ALLOWS A PROCESS TO ENABLE OR DISABLE THE DELIVERY
                                     0000  7700 ;	OF ASTS FOR THE PROCESS AT THE ACCESS MODE ISSUING THE CALL TO
                                     0000  7800 ;	EXE$SETAST.   THE PREVIOUS STATE OF THE AST ENABLE FOR THAT
                                     0000  7900 ;	ACCESS MODE IS RETURNED IN BIT 1 OF REGISTER 0 AS PART OF
                                     0000  8000 ;	THE SUCCESSFUL COMPLETION STATUS CODE.
                                     0000  8100 ;
                                     0000  8200 ; CALLING SEQUENCE:
                                     0000  8300 ;	CALLG	ARGLIST,EXE$SETAST
                                     0000  8400 ;
                                     0000  8500 ; INPUT PARAMETERS:
                                     0000  8600 ;	04(AP) - NEW SETTING FOR AST ENABLE
                                     0000  8650 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0000  8700 ;
                                     0000  8800 ; IMPLICIT INPUTS:
                                     0000  8900 ;	SCH$GL_CURPCB - POINTER TO CURRENT PROCESS PCB
                                     0000  9000 ;
                                     0000  9100 ; OUTPUT PARAMETERS:
                                     0000  9200 ;	R0 - COMPLETION STATUS CODE
                                     0000  9300 ;
                                     0000  9400 ; IMPLICIT OUTPUTS:
                                     0000  9500 ;	AST ENABLE FOR PREVIOUS MODE IS SET ACCORDING TO ARGUMENT
                                     0000  9600 ;
                                     0000  9700 ;	THE NEW SETTING FOR THE ASTLVL REGISTER IS PLACED BOTH IN
                                     0000  9800 ;	THE PROCESSOR REGISTER AND THE CURRENT HARDWARE PCB.
                                     0000  9900 ;
                                     0000 10000 ; COMPLETION CODES:
                                     0000 10100 ;	SS$_WASCLR  -  AST ENABLE FOR ACCESS MODE WAS CLEAR
                                     0000 10200 ;	SS$_WASSET  -  AST ENABLE FOR ACCESS MODE WAS SET
                                     0000 10300 ;
                                     0000 10400 ; SIDE EFFECTS:
                                     0000 10500 ;	ANY PENDING ASTS FOR THIS PROCESS WHICH BECOME DELIVERABLE
                                     0000 10600 ;	AS A RESULT OF SETTING AN AST ENABLE MAY BE DELIVERED UPON
                                     0000 10700 ;	EXIT FROM THIS SERVICE.
                                     0000 10800 ;
                                     0000 10900 ;--
                                     0000 11000 
                                     0000 11100 EXE$SETAST::				; SET AST ENABLE SYSTEM SERVICE
                               003C  0000 11200 	.WORD	^M<R2,R3,R4,R5>		; SAVE REGISTERS R2-R5
                          51     DC  0002 11300 	MOVPSL	R1			; GET CURRENT PSL
            02            16     EF  0004 11400 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R1,R1 ; EXTRACT PREVIOUS MODE
            51            51         0007       
            55            09     3C  0009 11500 	MOVZWL	#SS$_WASSET,R5		; ASSUME SET
         0D A4            51     E0  000C 11700 	BBS	R1,PCB$B_ASTEN(R4),10$	; TEST OLD ENABLE SETTING
                          03         0010       
            55            01     3C  0011 11800 	MOVZWL	#SS$_WASCLR,R5		; IT WAS CLEAR
            51         04 AC     F0  0014 11900 10$:	INSV	ENABLE(AP),R1,#1,PCB$B_ASTEN(R4)  ; SET NEW ENABLE VALUE
         0D A4            01         0018       
                          FFE2'  30  001B 12100 	BSBW	SCH$NEWLVL		;;; COMPUTE NEW ASTLVL 
            50            55     D0  001E 12300 	MOVL	R5,R0			; SET COMPLETION STATUS
                                 04  0021 12400 	RET				; AND RETURN TO CALLER
SYSASTCON       ASTCONTROL SYSTEM SERVICES                       21-AUG-1978 23:34:47   VAX-11 MACRO X0.3-11               Page   4
01              EXE$DCLAST - DECLARE AST SYSTEM SERVICE                                                                          (1)

                                     0022 12600 	.SBTTL	EXE$DCLAST - DECLARE AST SYSTEM SERVICE
                                     0022 12700 
                                     0022 12800 ;++
                                     0022 12900 ; FUNCTIONAL DESCRIPTION:
                                     0022 13000 ;	EXE$DCLAST ALLOWS A PROCESS TO ENQUEUE AN AST FOR THE ACCESS
                                     0022 13100 ;	MODE ISSUING THE CALL OR A LESS PRIVILEGED ACCESS MODE.
                                     0022 13200 ;
                                     0022 13300 ; CALLING SEQUENCE:
                                     0022 13400 ;	CALLG	ARGLIST,EXE$DCLAST
                                     0022 13500 ;
                                     0022 13600 ; INPUT PARAMETERS:
                                     0022 13700 ;	04(AP) - ADDRESS OF AST ROUTINE
                                     0022 13800 ;	08(AP) - AST PARAMETER
                                     0022 13900 ;	12(AP) - ACCESS MODE AT WHICH TO ENQUEUE AST
                                     0022 13950 ;	R4 - PCB ADDRESS OF CURRENT PROCESS
                                     0022 14000 ;
                                     0022 14100 ; IMPLICIT INPUTS:
                                     0022 14200 ;	CURRENT PCB
                                     0022 14300 ;
                                     0022 14400 ; OUTPUT PARAMETERS:
                                     0022 14500 ;	R0 - COMPLETION STATUS
                                     0022 14600 ;
                                     0022 14700 ; IMPLICIT OUTPUTS:
                                     0022 14800 ;	NONE
                                     0022 14900 ;
                                     0022 15000 ; SIDE EFFECTS:
                                     0022 15100 ;	THE ENQUEUING OF AN AST WILL EVENTUALLY CAUSE A DIVERSION
                                     0022 15200 ;	OF CONTROL TO OCCUR WHICH WILL CAUSE THE AST ROUTINE TO BE
                                     0022 15300 ;	CALLED.
                                     0022 15400 ;
                                     0022 15500 ; COMPLETION CODES:
                                     0022 15600 ;	SS$_NORMAL  -  NORMAL SUCCESSFUL COMPLETION STATUS
                                     0022 15700 ;	SS$_EXQUOTA -  QUOTA EXCEEDED FOR AST CONTROL BLOCKS
                                     0022 15800 ;	SS$_INSFMEM -  INSUFFICIENT DYNAMIC MEMORY
                                     0022 15900 ;
                                     0022 16000 ;--
                                     0022 16100 
                                     0022 16200 EXE$DCLAST::				; DECLARE AST SYSTEM SERVICE
                               003C  0022 16300 	.WORD	^M<R2,R3,R4,R5>		; SAVE REGISTERS R2-R5
                       3C A4     B5  0024 16500 	TSTW	PCB$W_ASTCNT(R4)		; TEST FOR QUOTA
                          33     15  0027 16600 	BLEQ	20$			; YES, ERROR RETURN
                          FFD4'  30  0029 16700 	BSBW	EXE$ALLOCIRP		; ALLOCATE PACKET
            29            50     E9  002C 16800 	BLBC	R0,10$			; DID NOT GET IT
                       3C A4     B7  002F 16900 	DECW	PCB$W_ASTCNT(R4)		;;; ACCOUNT FOR IT
            55            52     D0  0032 17000 	MOVL	R2,R5			;;; SAVE BASE ADDRESS OF ACB
            02            00     EF  0035 17100 	EXTZV	#0,#2,ACMODE(AP),R0	; GET ACCESS MODE ARGUMENT
            50         0C AC         0038       
                          FFC2'  30  003B 17200 	BSBW	EXE$MAXACMODE		; MAXIMIZE ACCESS MODE
            50         40 8F     88  003E 17300 	BISB	#<1@ACB$V_QUOTA>,R0	; SET QUOTA FLAG FOR PROCESS
         0B A5            50     90  0042 17400 	MOVB	R0,ACB$B_RMOD(R5)	; SET ACCESS MODE FOR AST
         0C A5         60 A4     D0  0046 17500 	MOVL	PCB$L_PID(R4),ACB$L_PID(R5)     ; SET PID IN AST CB
         10 A5         04 AC     7D  004B 17600 	MOVQ	AST(AP),ACB$L_AST(R5)	; STORE AST,ASTPRM
                          52     D4  0050 17650 	CLRL	R2			; NULL PRIORITY INCR
                          FFAB'  30  0052 17700 	BSBW	SCH$QAST		; AND ENQUEUE IT
            50            01     3C  0055 17800 	MOVZWL	#SS$_NORMAL,R0		; SET NORMAL STATUS
                                     0058 17900 10$:	SETIPL	#0			; ENABLE
                                 04  005B 18000 	RET				; AND RETURN TO CALLER
SYSASTCON       ASTCONTROL SYSTEM SERVICES                       21-AUG-1978 23:34:47   VAX-11 MACRO X0.3-11               Page   5
01              EXE$DCLAST - DECLARE AST SYSTEM SERVICE                                                                          (1)

            50            1C     3C  005C 18100 20$:	MOVZWL	#SS$_EXQUOTA,R0		; SET ERROR STATUS
                                 04  005F 18200 	RET				; RETURN
                                     0060 18300 	.END
