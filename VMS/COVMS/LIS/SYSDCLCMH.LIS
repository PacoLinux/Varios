SYSDCLCMH       - DECLARE CHANGE MODE OR COMPATIBILITY MODE HAN  21-AUG-1978 23:37:57   VAX-11 MACRO X0.3-11               Page   2
01              DECLARE CHANGE MODE OR COMPATIBILITY MODE HANDL                                                                  (1)

                                     0000    43 	.SBTTL	DECLARE CHANGE MODE OR COMPATIBILITY MODE HANDLER
                                     0000    44 ;+
                                     0000    45 ; EXE$DCLCMH - DECLARE CHANGE MODE OR COMPATIBILITY MODE HANDLER
                                     0000    46 ;
                                     0000    47 ; THIS SERVICE PROVIDES THE CAPABILITY TO SPECIFY A COMPATIBILITY MODE
                                     0000    48 ; HANDLER FOR COMPATIBILITY MODE TRAPS, OR A CHANGE MODE HANDLER FOR
                                     0000    49 ; THE CALLING ACCESS MODE.  A CHANGE MODE HANDLER MAY ONLY BE
                                     0000    50 ; FROM SUPERVISOR OR USER MODES.
                                     0000    51 ;
                                     0000    52 ; INPUTS:
                                     0000    53 ;
                                     0000    54 ;	ADDRES(AP) = ADDRESS OF HANDLER TO BE DECLARED
                                     0000    55 ;	PRVHND(AP) = ADDRESS TO RETURN OLD HANDLER ADDRESS
                                     0000    56 ;	TYPE(AP)  = TYPE OF HANDLER TO BE DECLARED
                                     0000    57 ;		0 -> DECLARE CHANGE MODE HANDLER FOR CURRENT MODE
                                     0000    58 ;		1 -> DECLARE COMPATIBILITY MODE HANDLER
                                     0000    59 ;
                                     0000    60 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    61 ;
                                     0000    62 ; IMPLICIT INPUTS:
                                     0000    63 ;
                                     0000    64 ;	CTL$GL_CMSUPR = ADDRESS OF THE FIRST OF THREE VECTORS, THE FIRST
                                     0000    65 ;			TWO BEING THE CHANGE MODE VECTORS FOR SUPERVISOR
                                     0000    66 ;			AND USER MODES, THE THIRD BEING THE COMPATIBILITY
                                     0000    67 ;			MODE VECTOR.
                                     0000    68 ;
                                     0000    69 ; OUTPUTS:
                                     0000    70 ;
                                     0000    71 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO DECLARE CHANGE MODE HANDLER
                                     0000    72 ;
                                     0000    73 ;		R0 = SS$_ACCVIO - PREVIOUS HANDLER ADDRESS CANNOT BE
                                     0000    74 ;			WRITTEN TO @PRVHND(AP) BY CALLING ACCESS MODE.
                                     0000    75 ;
                                     0000    76 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    77 ;
                                     0000    78 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0000    79 ;
                                     0000    80 ; SIDE EFFECTS:
                                     0000    81 ;
                                     0000    82 ;	SERVICE WILL CAUSE A BUGCHECK IF CALLED TO DECLARE CHANGE
                                     0000    83 ;	MODE HANDLER FROM KERNEL OR EXECUTIVE MODES.
                                     0000    84 ;-
                                     0000    85  
                                 00000000    86 	.PSECT	Y$EXEPAGED
                               0010' 0000    87 	.ENTRY	EXE$DCLCMH,^M<R4>
            54   00000000'9F     DE  0002    88 	MOVAL	@#CTL$GL_CMSUPR,R4	; ADDRESS OF VECTORS
            50            02     D0  0009    89 	MOVL	#2,R0			; INDEX TO COMPAT. MODE HANDLER VECTOR
            0C         0C AC     E8  000C    90 	BLBS	TYPE(AP),10$		; BRANCH IF DECLARE COMPAT. HANDLER SERVICE
                          50     DC  0010    91 	MOVPSL	R0			; READ CURRENT PSL
            02            16     EF  0012    92 	EXTZV	#PSL$V_PRVMOD,#PSL$S_PRVMOD,R0,R0 ; EXTRACT PREVIOUS MODE
            50            50         0015       
            50            02     C2  0017    93 	SUBL	#2,R0			; SUBTRACT SUPERVISOR MODE BIAS
                          19     19  001A    94 	BLSS	30$			; IF LSS, CALLED FROM KERNEL OR EXEC
            51         08 AC     D0  001C    95 10$:	MOVL	PRVHND(AP),R1		; GET ADDRESS TO RETURN PREVIOUS HANDLER
                          0A     13  0020    96 	BEQL	20$			; BRANCH IF NONE SPECIFIED
                                     0022    97 	IFNOWRT	#4,(R1),40$		; CAN IT BE WRITTEN?
            61            6440   D0  0028    98 	MOVL	(R4)[R0],(R1)		; YES, RETURN PREVIOUS HANDLER ADDRESS
SYSDCLCMH       - DECLARE CHANGE MODE OR COMPATIBILITY MODE HAN  21-AUG-1978 23:37:57   VAX-11 MACRO X0.3-11               Page   3
01              DECLARE CHANGE MODE OR COMPATIBILITY MODE HANDL                                                                  (1)

            6440       04 AC     D0  002C    99 20$:	MOVL	ADDRES(AP),(R4)[R0]	; SET NEW HANDLER ADDRESS
            50            01     3C  0031   100 	MOVZWL	#SS$_NORMAL,R0		; NORMAL STATUS
                                 04  0034   101 	RET				;
                                     0035   102 30$:	BUG_CHECK IVSSRVRQST		; INVALID SYSTEM SERVICE REQUEST
            50       0174 8F     3C  0039   103 	MOVZWL	#SS$_IVSSRQ,R0		; INVALID SERVICE REQUEST RETURN CODE
                                 04  003E   104 	RET				;
            50            0C     3C  003F   105 40$:	MOVZWL	#SS$_ACCVIO,R0		; ACCESS VIOLATION CODE
                                 04  0042   106 	RET
                                     0043   107 
                                     0043   108 	.END
