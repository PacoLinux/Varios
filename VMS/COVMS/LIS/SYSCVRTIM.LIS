                                     02DE   518 ; EXE$NUMTIM - CONVERT BINARY TIME TO NUMERIC TIME
                                     02DE   519 ;
                                     02DE   520 ; THIS SERVICE PROVIDES THE CAPABILITY TO CONVERT AN ABSOLUTE OR DELTA TIME
                                     02DE   521 ; FROM 64-BIT FORMAT TO INTEGER DATE AND TIME VALUES.
                                     02DE   522 ;
                                     02DE   523 ; INPUTS:
                                     02DE   524 ;
                                     02DE   525 ;	NTIMBUF(AP) = ADDRESS OF 7-WORD BUFFER TO RECEIVE CONVERTED DATE AND
                                     02DE   526 ;		TIME VALUES.
                                     02DE   527 ;	NTIMADR(AP) = ADDRESS OF 64-BIT TIME VALUE. IF ZERO, THEN THE CURRENT
                                     02DE   528 ;		SYSTEM TIME IS USED. POSITIVE VALUES ARE INTERPRETED AS
                                     02DE   529 ;		ABSOLUTE TIMES AND NEGATIVE VALUES AS DELTA TIMES.
                                     02DE   530 ;
                                     02DE   531 ; OUTPUTS:
                                     02DE   532 ;
                                     02DE   533 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO CONVERT TO NUMERIC TIME.
                                     02DE   534 ;
                                     02DE   535 ;		R0 = SS$_ACCVIO - 64-BIT TIME VALUE CANNOT BE READ BY CALLING
                                     02DE   536 ;			ACCESS MODE OR TIME BUFFER CANNOT BE WRITTEN BY
                                     02DE   537 ;			CALLING ACCESS MODE.
                                     02DE   538 ;
                                     02DE   539 ;		R0 = SS$_IVTIME - SPECIFIED DELTA TIME IS GREATER THAN 9999
                                     02DE   540 ;			DAYS.
                                     02DE   541 ;
                                     02DE   542 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     02DE   543 ;
                                     02DE   544 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     02DE   545 ;-
                                     02DE   546  
                                     02DE   547 EXE$NUMTIM::				;CONVERT TO NUMERIC TIME
                               00FC  02DE   548 	.WORD	^M<R2,R3,R4,R5,R6,R7>	;ENTRY MASK
            57         04 AC     D0  02E0   549 	MOVL	NTIMBUF(AP),R7		;GET ADDRESS OF 7-WORD TIME BUFFER
                                     02E4   550 	IFNOWRT	#7*2,(R7),10$		;CAN TIME BUFFER BE WRITTEN?
            50            01     3C  02EA   551 	MOVZWL	#SS$_NORMAL,R0		;ASSUME NORMAL COMPLETION
            51   00000000'EF     7D  02ED   552 	MOVQ	EXE$GQ_SYSTIME,R1	;ASSUME TIME NOT SPECIFIED
            53         08 AC     D0  02F4   553 	MOVL	NTIMADR(AP),R3		;GET ADDRESS OF 64-TIME VALUE
