SYSGETTIM       - SYSTEM SERVICE GET TIME                        21-AUG-1978 23:42:52   VAX-11 MACRO X0.3-11               Page   2
01              GET TIME                                                                                                         (1)

                                     0000    40 	.SBTTL	GET TIME
                                     0000    41 ;+
                                     0000    42 ; EXE$GETTIM - GET TIME
                                     0000    43 ;
                                     0000    44 ; THIS SERVICE PROVIDES THE CAPABILITY TO RETRIEVE THE CURRENT SYSTEM TIME
                                     0000    45 ; IN 64 BIT FORMAT.
                                     0000    46 ;
                                     0000    47 ; INPUTS:
                                     0000    48 ;
                                     0000    49 ;	TIMADR(AP) = ADDRESS OF QUADWORD THAT IS TO RECEIVE TIME.
                                     0000    50 ;
                                     0000    51 ; OUTPUTS:
                                     0000    52 ;
                                     0000    53 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO RETRIEVE SYSTEM TIME.
                                     0000    54 ;
                                     0000    55 ;		R0 = SS$_ACCVIO - QUADWORD TO RECEIVE TIME CANNOT BE
                                     0000    56 ;			WRITTEN BY CALLING ACCESS MODE.
                                     0000    57 ;
                                     0000    58 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    59 ;
                                     0000    60 ;		R0 = SS$_NORMAL - NORMAL COMPLETION.
                                     0000    61 ;-
                                     0000    62  
                                 00000000    63 	.PSECT	YEXEPAGED
                                     0000    64 EXE$GETTIM::				;GET TIME
                               0000  0000    65 	.WORD	0			;ENTRY MASK
            51         04 AC     D0  0002    66 	MOVL	TIMADR(AP),R1		;GET ADDRESS OF QUADWORD TO RECEIVE TIME
            50            0C     3C  0006    67 	MOVZWL	#SS$_ACCVIO,R0		;ASSUME QUADWORD NOT WRITABLE
                                     0009    68 	IFNOWRT	#8,(R1),10$		;CAN QUADWORD BE WRITTEN?
            61   00000000'EF     7D  000F    69 	MOVQ	EXE$GQ_SYSTIME,(R1)	;STORE SYSTEM TIME IN QUADWORD
            50            01     3C  0016    70 	MOVZWL	#SS$_NORMAL,R0		;SET NORMAL COMPLETION
                                 04  0019    71 10$:	RET				;
                                     001A    72  
                                     001A    73 	.END
