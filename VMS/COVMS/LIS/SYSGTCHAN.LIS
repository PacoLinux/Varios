            54            55     D0  002F   104 20$:	MOVL	R5,R4			;SET SECONDARY DEVICE UCB ADDRESS
            57         10 AC     7D  0032   105 30$:	MOVQ	SCDLEN(AP),R7		;GET SECONDARY BUFFER PARAMETERS
                          04     10  0036   106 	BSBB	FILBUF			;FILL SECONDARY BUFFER
            50            56     D0  0038   107 	MOVL	R6,R0			;SET FINAL REQUEST STATUS
                                 04  003B   108 40$:	RET				;
                                     003C   109  
                                     003C   110 ;
                                     003C   111 ; SUBROUTINE TO FILL CHARACTERISTIC BUFFER
                                     003C   112 ;
                                     003C   113  
                          58     D5  003C   114 FILBUF:	TSTL	R8			;ANY BUFFER SPECIFIED?
                          55     13  003E   115 	BEQL	50$			;IF EQL NO
                                     0040   116 	IFNORD	#8,(R8),60$		;CAN OUTPUT BUFFER DESCRIPTOR BE READ?
            52            68     3C  0046   117 	MOVZWL	(R8),R2			;GET SIZE OF OUTPUT BUFFER
                          37     13  0049   118 	BEQL	30$			;IF EQL NULL
            53         04 A8     D0  004B   119 	MOVL	4(R8),R3		;GET ADDRESS OF OUTPUT BUFFER
                                     004F   120 	IFNOWRT	R2,(R3),60$		;CAN ENTIRE BUFFER BE WRITTEN?
            51         34 A4     DE  0055   121 	MOVAL	UCB$L_DEVCHAR(R4),R1	;GET ADDRESS OF CHARACTERISTICS
            50            0C     D0  0059   122 	MOVL	#12,R0			;SET BYTE COUNT
                          16     10  005C   123 	BSBB	10$			;MOVE CHARACTERISTICS TO BUFFER
            51         48 A4     DE  005E   124 	MOVAL	UCB$W_UNIT(R4),R1	;GET ADDRESS OF UNIT NUMBER
            50            02     D0  0062   125 	MOVL	#2,R0			;SET BYTE COUNT
                          0D     10  0065   126 	BSBB	10$			;MOVE UNIT NUMBER OT BUFFER
            51         24 A4     D0  0067   127 	MOVL	UCB$L_DDB(R4),R1	;GET ADDRESS OF DDB
            51         14 A1     DE  006B   128 	MOVAL	DDB$T_NAME(R1),R1	;GET ADDRESS OF GENERIC DEVICE NAME
            50            61     9A  006F   129 	MOVZBL	(R1),R0			;GET LENGTH OF NAME IN BYTES
                          50     D6  0072   130 	INCL	R0			;ACCOUNT FOR COUNT BYTE
                          52     D7  0074   131 10$:	DECL	R2			;ANY SPACE LEFT IN BUFFER?
                          08     19  0076   132 	BLSS	20$			;IF LSS NO
            83            81     90  0078   133 	MOVB	(R1)+,(R3)+		;MOVE BYTE TO BUFFER
            F6            50     F5  007B   134 	SOBGTR	R0,10$			;ANY MORE BYTES TO MOVE?
                          07     11  007E   135 	BRB	40$			;
                          52     D6  0080   136 20$:	INCL	R2			;ADJUST COUNT OF REMAINING BYTES
            50       0601 8F     3C  0082   137 30$:	MOVZWL	#SS$_BUFFEROVF,R0	;SET BUFFER OVERFLOW
                          57     D5  0087   138 40$:	TSTL	R7			;ADDRESS SPECIFIED TO STORE RESULT LENGTH?
                          0A     13  0089   139 	BEQL	50$			;IF EQL NO
                                     008B   140 	IFNOWRT	#2,(R7),60$		;CAN LENGTH BE WRITTEN?
            68            52     A3  0091   141 	SUBW3	R2,(R8),(R7)		;CALCULATE LENGTH OF RESULT STRING
                          67         0094       
                                 05  0095   142 50$:	RSB				;
            50            0C     3C  0096   143 60$:	MOVZWL	#SS$_ACCVIO,R0		;SET ACCESS VIOLATION
                                 04  0099   144 	RET				;
                                     009A   145  
                                     009A   146 	.END
