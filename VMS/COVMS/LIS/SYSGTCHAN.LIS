                                     0000    48 	.SBTTL	GET I/O CHANNEL DEVICE INFORMATION
                                     0000    49 ;+
                                     0000    50 ; EXE$GTCHAN - GET I/O CHANNEL DEVICE INFORMATION
                                     0000    51 ;
                                     0000    52 ; THIS SERVICE PROVIDES THE CAPABILITY TO RETRIEVE INFORMATION ABOUT A
                                     0000    53 ; DEVICE THAT IS ASSIGNED TO A CHANNEL AND ITS ASSOCIATED DEVICE IF ANY.
                                     0000    54 ;
                                     0000    55 ; INPUTS:
                                     0000    56 ;
                                     0000    57 ;	CHAN(AP) = I/O CHANNEL NUMBER.
                                     0000    58 ;	PRILEN(AP) = ADDRESS TO STORE LENGTH OF PRIMARY STRING.
                                     0000    59 ;	PRIBUF(AP) = ADDRESS OF PRIMARY BUFFER DESCRIPTOR.
                                     0000    60 ;	SCDLEN(AP) = ADDRESS TO STORE LENGTH OF SECONDARY STRING.
                                     0000    61 ;	SCDBUF(AP) = ADDRESS OF SECONDARY BUFFER DESCRIPTOR.
                                     0000    62 ;
                                     0000    63 ;	R4 = CURRENT PROCESS PCB ADDRESS.
                                     0000    64 ;
                                     0000    65 ; OUTPUTS:
                                     0000    66 ;
                                     0000    67 ;	R0 LOW BIT CLEAR INDICATES FAILURE TO RETRIEVE DEVICE INFORMATION.
                                     0000    68 ;
                                     0000    69 ;		R0 = SS$_ACCVIO - PRIMARY OR SECONDARY BUFFER DESCRIPTOR
                                     0000    70 ;			CANNOT BE READ BY CALLING ACCESS MODE, OR PRIMARY
                                     0000    71 ;			OR SECONDARY BUFFER CANNOT BE WRITTEN BY CALLING
                                     0000    72 ;			ACCESS MODE.
                                     0000    73 ;
                                     0000    74 ;		R0 = SS$_IVCHAN - INVALID CHANNEL NUMBER SPECIFIED.
                                     0000    75 ;
                                     0000    76 ;		R0 = SS$_NOPRIV - SPECIFIED CHANNEL IS NOT ASSIGNED TO A
                                     0000    77 ;			DEVICE OR THE CALLING ACCESS MODE DOES NOT HAVE
                                     0000    78 ;			PRIVILEGE TO ACCESS THE CHANNEL.
                                     0000    79 ;
                                     0000    80 ;	R0 LOW BIT SET INDICATES SUCCESSFUL COMPLETION.
                                     0000    81 ;
                                     0000    82 ;		R0 = SS$_BUFFEROVF - NORMAL COMPLETION, ALL CHARACTERISTIC
                                     0000    83 ;			INFORMATION DID NOT FIT IN SPECIFIED BUFFER(S).
                                     0000    84 ;
                                     0000    85 ;		R0 = SS$_NORMAL - NORMAL COMPLETION, ALL CHARACTERISTIC
                                     0000    86 ;			INFORMATION TRANSFERED.
                                     0000    87 ;-
                                     0000    88  
                                 00000000    89 	.PSECT	Y$EXEPAGED
                               01FC' 0000    90 	.ENTRY	EXE$GTCHAN,^M<R2,R3,R4,R5,R6,R7,R8>
            50         04 AC     3C  0002    91 	MOVZWL	CHAN(AP),R0		;GET CHANNEL NUMBER
                          FFF7'  30  0006    92 	BSBW	IOC$VERIFYCHAN		;VERIFY CHANNEL NUMBER
            2F            50     E9  0009    93 	BLBC	R0,40$			;IF LBC INVALID CHANNEL
            55            61     D0  000C    94 	MOVL	CCB$L_UCB(R1),R5	;GET DEVICE UNIT UCB ADDRESS
            54            55     D0  000F    95 	MOVL	R5,R4			;ASSUME ASSOCIATED UCB NOT SPOOL DEVICE
         34 A5            06     E1  0012    96 	BBC	#DEV$V_SPL,UCB$L_DEVCHAR(R5),10$ ;IF CLR, THEN DEVICE NOT SPOOLED
                          04         0016       
            54         54 A5     D0  0017    97 	MOVL	UCB$L_AMB(R5),R4	;GET INTERMEDIATE DEVICE UCB ADDRESS
            56            01     3C  001B    98 10$:	MOVZWL	#SS$_NORMAL,R6		;ASSUME ALL INFO CAN BE RETURNED
            57         08 AC     7D  001E    99 	MOVQ	PRILEN(AP),R7		;GET PRIMARY BUFFER PARAMETERS
                          18     10  0022   100 	BSBB	FILBUF			;FILL PRIMARY BUFFER
            54         54 A5     D0  0024   101 	MOVL	UCB$L_AMB(R5),R4	;ANY ASSOCIATED UCB?
                          05     13  0028   102 	BEQL	20$			;IF EQL NO
         34 A5            06     E1  002A   103 	BBC	#DEV$V_SPL,UCB$L_DEVCHAR(R5),30$ ;IF CLR, THEN DEVICE NOT SPOOLED
