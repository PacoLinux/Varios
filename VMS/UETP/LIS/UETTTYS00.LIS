                                     01C8   133 	.MACRO	TERMTEST	N
                                     01C8   134 SUCA'N:
                                     01C8   135 		.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9,R10,R11>		; AST ENTRY MASK
                                     01C8   136 		BITB	#BIT1,IOSTAT	; ANY CONTROL C'S?
                                     01C8   137 		BNEQ	5$		; BR IF YES
                                     01C8   138 		INCL	LINES'N		; INCREMENT ITERATION COUNT
                                     01C8   139 		CMPB	LINES'N,TTY_CHAR'N ; IS THIS PAGE DONE?
                                     01C8   140 		BNEQ	20$		; BR IF NOT
                                     01C8   141 		CLRL	LINES'N		; RESET THE LINE COUNT
                                     01C8   142 		INCL	ITER'N		; BUMP PAGE COUNT
                                     01C8   143 		CMPB	ITER'N,TTY_CHAR'N+1 ; IS THIS TTY  DONE?
                                     01C8   144 		BNEQ	30$		; BR IF NOT
                                     01C8   145 5$:
                                     01C8   146 		BISB2	#1@N,DONE_FLAG	; SET DONE FLAG
                                     01C8   147 		CMPB	DONE_FLAG,UNIT_MASK ; ARE WE ALL DONE?
                                     01C8   148 		BEQL	10$		; BR IF YES
                                     01C8   149 		BRW	70$		; RET
                                     01C8   150 10$:
                                     01C8   151 		BRW	SUC_EXIT	; ALL DONE!!!!
                                     01C8   152 20$:
                                     01C8   153 		$QIO_S	,CHAN'N,#IO$_WRITEVBLK,,SUCA'N,,@DATA'N,DATA'N+4,,#^X20
                                     01C8   154 		SERVCHEK		; CHECK FOR ERRORS
                                     01C8   155 		RET			; HIBERNATE
                                     01C8   156 30$:
                                     01C8   157 		MOVB	#^X30+N,CNTRL_NAME+3 ; SET UNIT NUMBER
                                     01C8   158 		MOVAL	CNTRL_NAME_STR,ARGS  ; SET STRING DESCRIPTOR
                                     01C8   159 		CLRL	ARGS+4		; SET FOR CURRENT TIME
                                     01C8   160 		MOVL	ITER'N,ARGS+8	; GET PAGE NUMBER
                                     01C8   161 		CMPB	TTY_CHAR'N,#LA36_LIN_PER ; IS THIS ONE <= TO LA36?
                                     01C8   162 		BLSS	40$		; BR IF VIDEO
                                     01C8   163 		MOVAL	PAGE_HEAD0,R6	; SET UP FOR HARD COPY
                                     01C8   164 		BRB	60$		; GO TO COMMON CODE
                                     01C8   165 40$:
                                     01C8   166 		CMPB	TTY_CHAR'N,#VT05_LIN_PER ; IS THIS A VT05?
                                     01C8   167 		BNEQ	50$		; BR IF NOT
                                     01C8   168 		MOVAL	PAGE_HEAD2,R6	; SET UP FOR VT05
                                     01C8   169 		BRB	60$		; GO TO COMMON CODE
                                     01C8   170 50$:
                                     01C8   171 		MOVAL	PAGE_HEAD1,R6	; SET FOR VT50,2,5
                                     01C8   172 60$:
                                     01C8   173 		DISPLAY	(R6),ARGS	; SET UP PAGE HEADER
                                     01C8   174 		$QIO_S	,CHAN'N,#IO$_WRITEVBLK,,SUCA'N,,FAO_BUFFER,STRLEN,,#^X20
                                     01C8   175 		SERVCHEK		; CHECK FOR ERRORS
                                     01C8   176 70$:
                                     01C8   177 		RET			; HIBERNATE
                                     01C8   178 	.ENDM	TERMTEST
                                     01C8   179 ;
