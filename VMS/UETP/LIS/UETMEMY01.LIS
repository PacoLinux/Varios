UETMEMY01       VAX/VMS UETP MEMORY MANAGEMENT TEST              22-AUG-1978 03:23:30   VAX-11 MACRO X0.3-11               Page   3
1.0             MAIN ROUTINE                                                                                                     (2)

                                     0025    61 	.SBTTL MAIN ROUTINE
                                 00000000    62 	.PSECT UETMEMY01,EXE,RD,NOWRT
                               0000' 0000    63 	.ENTRY	UETMEMY01,0		; ENTRY MASK
                                     0002    64 ;********************************************************************************
                                     0002    65 ;	THIS MODULE USES MEMORY MANAGEMENT SERVICES INEFFICIENTLY
                                     0002    66 ;	IN ORDER TO CAUSE A LOAD ON SYSTEM PAGING RESOURCES
                                     0002    67 ;********************************************************************************
                                     0002    68 	PUTMSG	<#UETP$_BEGIND,#2,#PROC_NAME,#0> ; GREETINGS
                          00     DD  0002       	  	PUSHL	#0
                 00000000'8F     DD  0004       	  	PUSHL	#PROC_NAME
                          02     DD  000A       	  	PUSHL	#2
                 0074103B 8F     DD  000C       	  	PUSHL	#UETP$_BEGIND
   00000000'9F            04     FB  0012       		CALLS	#$$T1,@#LIB$SIGNAL
                                     0019    69 	$SETPRN_S PROC_NAME		; SET OUR PROCESS NAME
                 00000000'EF     7F  0019       		PUSHAQ	PROC_NAME
   00000000'GF            01     FB  001F       		CALLS	#1,G^SYS$SETPRN
                                     0026    70 	$EXPREG_S PAGES,EXPADDR,,#0	;EXPAND PROG P0 REGION
                          00     DD  0026       		PUSHL	#0
                          00     DD  0028       		PUSHL	#0
                 00000019'EF     7F  002A       		PUSHAQ	EXPADDR
                 00000021'EF     DD  0030       		PUSHL	PAGES
   00000000'GF            04     FB  0036       		CALLS	#4,G^SYS$EXPREG
                                     003D    71 ;
                                     003D    72 ;	LOOP TO MAKE A 9 BYTE ACCESS TO EACH PAGE IN THE VIRTUAL
                                     003D    73 ;	ADDRESS SPACE. DO THIS TO ALL PAGES IN THE RANGE 'ITER' TIMES
                                     003D    74 ;
            57   00000011'EF     D0  003D    75 	MOVL	ITER,R7			; NUMBER OF ITERATIONS THRU VAS
                                     0044    76 INIT_LOOP:
            56   00000019'EF     D0  0044    77 	MOVL	EXPADDR,R6		; INITIAL PAGE TO ACCESS
   00000001'EF46          09     28  004B    78 LOOP:	MOVC3	#9,1[R6],0[R6]		; MOVE 9 BYTES ON CURRENT PAGE
                 00000000'EF46       0053       
            56   00000200 8F     C0  0059    79 	ADDL2	#512,R6			; SELECT NEXT PAGE
   0000001D'EF            56     D1  0060    80 	CMPL	R6,EXPADDR+4		; ARE WE AT HI END OF VAS?
                          E2     15  0067    81 	BLEQ	LOOP			; NO,  DO MORE
            D8            57     F4  0069    82 	SOBGEQ	R7,INIT_LOOP		; YES,  ANY MORE ITERATIONS?
                                     006C    83 ;
                                     006C    84 	PUTMSG	<#UETP$_ENDEDD,#2,#PROC_NAME,#0> ; SAY GOOD BYE
                          00     DD  006C       	  	PUSHL	#0
                 00000000'8F     DD  006E       	  	PUSHL	#PROC_NAME
                          02     DD  0074       	  	PUSHL	#2
                 00741083 8F     DD  0076       	  	PUSHL	#UETP$_ENDEDD
   00000000'9F            04     FB  007C       		CALLS	#$$T1,@#LIB$SIGNAL
                                     0083    85 	$EXIT_S				; E X I T
                          01     DD  0083       		PUSHL	#1
   00000000'GF            01     FB  0085       		CALLS	#1,G^SYS$EXIT
                                     008C    86 	.END	UETMEMY01
