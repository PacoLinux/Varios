                                     0400   186 REL_TEST2:
                                     0400   187 	;ENTRY POINT TO BYPASS CREATE
                                     0400   188 	$CONNECT RAB=RELATIVE_RAB,ERR=REPORT_ERROR
                          FBEC'  30  0411   189 	BSBW	ERR
                                     0414   190 ;DO 26 SEQUENTIAL PUTS WITH 'BAD' DATA INTO FILE
                                     0414   191 ;THEN DELETE 5 OF THOSE RECORDS AND DO PUTS TO PUT RIGHT DATA INTO THEM
                                     0414   192 ;UPDATE THE OTHER RECORDS AND THEN DO GETS TO MAKE SURE IT'S ALL OK
            59            01     D0  0414   193 	MOVL	#1,R9			;R9 IS RECORD NUMBER
            5B       FC35 CF     DE  0417   194 	MOVAL	RELATIVE_RAB,R11	;R11 IS ADDRESS OF RAB
                                     041C   195 	TYPE	<FILL FILE>
                                     044B   196 PUT_RECORD_SEQ::
            56            0F     D0  044B   197 	MOVL	#15,R6			;ALL OF LENGTH 15
       FBCC CF            01     91  044E   198 	CMPB	#FAB$C_FIX,FAB$B_RFM+RELATIVE_FAB
                          03     12  0453   199 	BNEQ	30$
            56            34     D0  0455   200 	MOVL	#52,R6			;LEN OF FIXED REC. IS 52
            6E            00     2C  0458   201 30$:	MOVC5	#0,(SP),#^A/1/,R6,RELBUF	;FILL BUFFER FOR OUTPUT
            56            31         045B       
                     FC90 CF         045D       
                                     0460   202 					;PUT ASCII '1'S INTO RECORDS
         22 AB            56     B0  0460   203 	MOVW	R6,RAB$W_RSZ(R11)	;GIVE SIZE OF RECORD
       FC4D CF            59     D0  0464   204 	MOVL	R9,HEAD			;FILL HEADER IN CASE IT'S VFC
                                     0469   205 				;SEQ MODE IS DEFAULT
                                     0469   206 	$PUT	RAB=R11,ERR=REPORT_ERROR
                          FB85'  30  0478   207 	BSBW	ERR
            59         38 AB     D1  047B   208 	CMPL	RAB$L_BKT(R11),R9	;BKT SHOULD BE REC. # ON OUTPUT
                          15     13  047F   209 	BEQL	BKT_OK
                                     0481   210 	FIELD	<BKT IN RAB (RECORD NUMBER)>
            59            1A     F3  0496   211 BKT_OK:	AOBLEQ	#26,R9,PUT_RECORD_SEQ	;KEEP GOING?
                          B1         0499       
                                     049A   212 ;CHECK ONE RECORD JUST TO BE SURE
            59            0F     D0  049A   213 	MOVL	#15,R9
         30 AB       FC19 CF     DE  049D   214 	MOVAL	KEY,RAB$L_KBF(R11)
         1E AB            01     90  04A3   215 	MOVB	#RAB$C_KEY,RAB$B_RAC(R11)
       FC0E CF            59     D0  04A7   216 	MOVL	R9,KEY
                                     04AC   217 	$GET	RAB=R11,ERR=REPORT_ERROR
                          FB42'  30  04BB   218 	BSBW	ERR
                          01B7   30  04BE   219 	BSBW	CHK_BAD_DATA
         04 AB            10     CA  04C1   220 	BICL2	#RAB$M_UIF,RAB$L_ROP(R11)		;IN CASE ITS SET
            59            05     D0  04C5   221 	MOVL	#5,R9
                                     04C8   222 FIND_DEL:
                                     04C8   223 	;DO SOME FINDS AND DELETES BY KEYED ACCESS
       FBED CF            59     D0  04C8   224 	MOVL	R9,KEY
                                     04CD   225 	$FIND	RAB=R11,ERR=REPORT_ERROR
                          FB21'  30  04DC   226 	BSBW	ERR
                                     04DF   227 	$DELETE	RAB=R11,ERR=REPORT_ERROR
