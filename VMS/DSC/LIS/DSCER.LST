    151					;
    152					; MACRO TO GENERATE ERROR MESSAGE TABLES. THE MESSAGE FORMAT STRINGS
    153					; ARE ACCUMULATED, AND A POINTER TABLE IS BUILT, INDEXABLE BY MESSAGE
    154					; CODE. ALSO, A POINTER TABLE FOR THE ACTION ROUTINES IS CONSTRUCTED.
    155					;
    156						.MACRO	MSG	CODE,STRING
    157						ER.'CODE == $$CODE
    158						$$CODE = $$CODE + 1
    159						.PSECT	$$MSG
    160						...MSG = .
    161						.ASCIZ	\%N%VA'STRING\
    162						.PSECT	$$MST
    163						.WORD	...MSG
    164						.PSECT	$$ACT
    165						.WORD	AC.'CODE
    166						.PSECT  $$MSV
    167						.WORD  MV.'CODE
    168						.PSECT	$$MSG
    169						.ENDM	MSG
    170
    171						.MACRO	MSGC	CODE,STRING
    172						ER.'CODE == $$CODE
    173						$$CODE = $$CODE + 1
    174						.PSECT	$$MSG
    175						...MSG = .
    176						.ASCII	\%N%VA'STRING\
    177						.PSECT	$$MST
    178						.WORD	...MSG
    179						.PSECT	$$ACT
    180						.WORD	AC.'CODE
    181						.PSECT   $$MSV
    182						.WORD   MV.'CODE
    183						.PSECT	$$MSG
    184						.ENDM	MSGC
    185
