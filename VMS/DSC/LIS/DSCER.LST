     62						.SBTTL	ERROR MESSAGE CODE
     63
     64					;+
     65					;
     66					; *** - $DSCER	ERROR MESSAGE MODULE FOR DSC
     67					;
     68					; THIS ROUTINE IS ENTERED FROM THE TRAP VECTOR TO REPORT AN ERROR
     69					; CONDITION. THE TRAP CODE IS THE ERROR CODE; IF POSITIVE, THE
     70					; CURRENT COMMAND IS ABORTED; IF NEGATIVE, OPERATION IS RESUMED
     71					; AFTER PRINTING THE MESSAGE.
     72					;
     73					; INPUTS:
     74					;
     75					;	STACK:	ERROR CODE * 2
     76					;		PC
     77					;		PS
     78					;	ALL REGISTERS AT TIME OF TRAP
     79					;
     80					; OUTPUTS:
     81					;
     82					;	MESSAGE OUTPUT TO TERMINAL
     83					;	ALL REGISTERS PRESERVED
     84					;	CC - C	SET
     85					;	RETURNS TO DSC INIT POINT IF TRAP CODE IS NEGATIVE
     86					;
     87					;-
     88
     89	000576				$DSCER::
     90	000576					LET $CODE := (SP)+		; SAVE THE ERROR CODE
     91	000602					LET (SP) := (SP)+		; CLEAN PS OFF STACK
     92	000604					IF #400 OFF.IN $CODE		; IF ABORT REQUESTED
     93	000614					  LET SP := $STAK		; CLEAN THE ENTIRE STACK
     94	000620					  PUSH #$DSC			; SET RETURN TO INIT POINT
     95	000624					  LET $PARAM := #FATLL		; SET FATAL MESSAGE HEADER
     96	000632					  LET $PARAM+2 := #FATAL
     97	000640					  LET $ESTAT  :=  #2	;INDICATE FATAL ERROR OCCURRED
     98	000646					ELSE
     99	000650					  LET $CODE := - $CODE OFF.BY #^C377 ; MAKE CODE POSITIVE
    100	000662					  LET $PARAM := #WARNL		; SET WARNING MESSAGE HEADER
    101	000670					  LET $PARAM+2 := #WARN
    102	000676					   IF $ESTAT  LT  #0	;IF NO WARN. OR FATAL YET
    103	000704						LET $ESTAT  :=  #-2	; INDICATE WARNING MESSAGE SENT
    104	000712					   END
    105	000712					END
    106	000712					$CALL $SAVAL			; SAVE ALL REGISTERS
    107					;
    108					;NOW MOVE BASIC PART OF MSG TO COMMON MESSAGE BUFFER
    109					;
    110	000716					LET R4 := #0		;OFFSET IN CURRENT MESSAGE
    111	000720					LET R2 := $CODE		;ORIGINAL ERROR CODE
    112	000724					LET R3 :=  #COMMSG	;1ST BYTE COMMON BUFFER
    113	000730				TAG1:	LET R1 := R2		;GET ERROR CODE
    114	000732					IF R1 HI #MAXCOD
    115	000740						FATAL <GARBAGE MESSAGE CODE>
    116	000742					END
    117	000742					LET R1 := $MSG(R1) + R4	;CURRENT POSITION IN MESSAGE
    118	000750					LET R5 := R3		;CURRENT POS. IN COMMON BUFFER
DSCER	X0032 - DSC ERROR REPOR	MACRO M1110  21-AUG-78 20:31  PAGE 5-1
ERROR MESSAGE CODE

    119	000752					WHILEB (R1) NE #0	;UNTIL NULL CHAR
    120	000756						LET (R5)+ :B= (R1)+	;MOVE CHAR. TO COMMON BUFFER
    121	000760					END
    122	000762					LET R1 := R2		;RESET R1TO ERR.CODE--
    123								;R5 IS ADDRESS OF NEXT BYTE IN COMM.BUFFR
    124	000764					$CALL  @$MSV(R1)	;TO APPEND FORMAT CHARS.
    125					;
    126					; NOW CALL THE ACTION ROUTINE ASSOCIATED WITH THE ERROR
    127					;
    128	000770					LET R5 := #$PARAM+4		; POINT TO PARAMETER BLOCK
    129	000774					LET R1 := SP + #6		;RESTORE R2,R3,R4
    130	001002					LET R2 := (R1)+
    131	001004					LET R3 := (R1)+
    132	001006					LET R4 := (R1)+
    133	001010					LET R1 := $CODE			; GET CODE
    134	001014					$CALL @$ACT(R1)			; CALL THE ACTION ROUTINE
    135					;
    136					; NOW GENERATE THE MESSAGE FROM THE FORMAT STRING THE THE PARAMETER
    137					; BLOCK PREPARED BY THE ACTION ROUTINE.
    138					;
    139	001020					LET R1 := $CODE			; GET CODE AGAIN
    140	001024					LET R1 := #COMMSG		; GET ADDRESS OF COMMON BUFFER
    141	001030					$CALL $EDMSG <#$MBUF,R1,#$PARAM>
    142										; AND OUTPUT TO TERMINAL
    143	001044					QIOW$S #IO.WVB,#TTYLUN,#TTEFN,,,,<#$MBUF,R1,#0>
    144	001112					IF $ESTAT EQ #-2
    145	001122					   LET $ESTAT :=	#0  ;SET WARNING INDICATOR
    146	001126					END
    147	001126					RETURN ERROR
