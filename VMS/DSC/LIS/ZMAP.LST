     31					; *** - $ZMAP	ZERO MAP AREA OF FILE HEADER
     32					;
     33					; THIS ROUTINE INITIALIZES THE MAP AREA OF THE INDICATED FILE HEADER
     34					; TO AN EMPTY MAP AREA.
     35					;
     36					; INPUTS:
     37					;
     38					;	R5 = ADDRESS OF FILE HEADER
     39					;
     40					; OUTPUTS:
     41					;
     42					;	MAP AREA INITIALIZED
     43					;	R0,R1 CLOBBERED, OTHER REGISTERS PRESERVED
     44					;
     45					;-
     46
     47	000000				$ZMAP::
     48	000000	005001 				CLR	R1
     49	000002	156501 	000002 			BISB	H.ACOF(R5),R1		; ACCESS CONTROL LIST OFFSET TO R1.
     50	000006	005000 				CLR	R0
     51	000010	156500 	000001 			BISB	H.MPOF(R5),R0		; MAP AREA OFFSET TO R0.
     52	000014	160001 				SUB	R0,R1			; COMPUTE MAP AREA SIZE IN WORDS.
     53	000016	006300 				ASL	R0			; CONVERT TO A BYTE OFFSET.
     54	000020	060500 				ADD	R5,R0			; COMPUTE MAP AREA ADDRESS.
     55	000022					THRU  	R1			; ZERO OUT THE
     56	000022	005020 				  CLR  	  (R0)+			; MAP AREA.
     57	000024					END	<LOOP>
     58	000030	105065 	000072 			CLRB	H.USE(R5)		; ZERO OUT MAP AREA WORDS IN USE COUNT.
     59	000034					RETURN
