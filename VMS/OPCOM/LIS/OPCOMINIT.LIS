                                     0000    40 ;++
                                     0000    41 ; OPC$START - ENTRY ADDRESS FOR OPERATOR COMMUNICATION
                                     0000    42 ;
                                     0000    43 ; FUNCTIONAL DESCRIPTION:
                                     0000    44 ;
                                     0000    45 ; THIS ROUTINE INITIALIZES THE OPRERATOR DATA BASE, OPENS THE LOG FILE
                                     0000    46 ; AND BEGINS THE MESSAGE FLOW.
                                     0000    47 ;
                                     0000    48 ; THE LOG FILE NAME IS SYS$SYSTEM:[1,2]OPERATOR.LOG.
                                     0000    49 ;
                                     0000    50 ; INPUTS:
                                     0000    51 ;
                                     0000    52 ;	NONE
                                     0000    53 ;
                                     0000    54 ; OUTPUTS:
                                     0000    55 ;
                                     0000    56 ;	NONE
                                     0000    57 ;--
                                     0000    58 OPC$START:
                               0000  0000    59 	.WORD	<0>
                 00000000'8F     DD  0002    60 	PUSHL	#SYS$C_OPRMBX		;
                          5E     DD  0008    61 	PUSHL	SP
                          04     DD  000A    62 	PUSHL	#4
            52            5E     D0  000C    63 	MOVL	SP,R2
                                     000F    64 	$ALLOC_S	(R2)
                                     001E    65 	$ASSIGN_S	(R2),W^OPC$GL_BOX
            03            50     E8  002D    66 	BLBS	R0,10$
                          005B   31  0030    67 	BRW	ERROR
                                     0033    68 ;
                                     0033    69 ; INITIALIZE THE LISTS AND POOL
                                     0033    70 ;
                     0000'CF     7C  0033    71 10$:	CLRQ	W^OPC$GQ_POOL		; SET NO SPACE AVAILABLE
       0000'CF       0000'CF     9E  0037    72 	MOVAB	W^OPC$GQ_TERMLIST,W^OPC$GQ_TERMLIST; INIT TERMINAL LIST
       0004'CF       0000'CF     9E  003E    73 	MOVAB	W^OPC$GQ_TERMLIST,W^OPC$GQ_TERMLIST+4;
       0000'CF       0000'CF     9E  0045    74 	MOVAB	W^OPC$GQ_MSGLIST,W^OPC$GQ_MSGLIST; INIT MESSAGE LIST
       0004'CF       0000'CF     9E  004C    75 	MOVAB	W^OPC$GQ_MSGLIST,W^OPC$GQ_MSGLIST+4;
            51            28     9A  0053    76 	MOVZBL	#OT_SIZE,R1		; SET UP DEFAULT OPERATOR
                          FFA7'  30  0056    77 	BSBW	OPC$ALLOCATE		;
       0000'DF            62     0E  0059    78 	INSQUE	(R2),@W^OPC$GQ_TERMLIST	; INSERT ON LIST
            00   FFFFFFFF 8F     F0  005E    79 	INSV	#-1,#0,#24,OT_SERVICE(R2); SET ALL SERVICES
         08 A2            18         0065       
                       0C A2     D4  0068    80 	CLRL	OT_STATUS(R2)		; SET NOT ACTIVE
            50       0000'CF     9E  006B    81 	MOVAB	W^OPC$T_DEFOPR,R0	; ADDRESS DEFAULT OPERATOR STRING
         14 A2         18 A2     9E  0070    82 	MOVAB	OT_NAME(R2),OT_DESC+4(R2);
            51            80     9A  0075    83 	MOVZBL	(R0)+,R1		; GET NAME SIZE
         10 A2            51     D0  0078    84 	MOVL	R1,OT_DESC(R2)		;
            60            51     2C  007C    85 	MOVC5	R1,(R0),#0,#16,OT_NAME(R2); 
            10            00         007F       
                       18 A2         0081       
            52       0000'CF     D0  0083    86 	MOVL	W^OPC$GQ_TERMLIST,R2	; ADDRESS INITIAL TERMINAL
                          FF75'  30  0088    87 	BSBW	OPC$INITLOG		; INITIALIZE THE LOG
                                     008B    88 ;
                                     008B    89 ; BEGIN THE MESSAGE FLOW
                                     008B    90 ;
                          FF72'  31  008B    91 	BRW	OPC$RESTART		; START MESSAGE FLOW
                                     008E    92 ;
