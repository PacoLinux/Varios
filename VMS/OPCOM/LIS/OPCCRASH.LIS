                                     0037    62 ;
   00000000'GF   00000001'8F     CA  0037    63 	BICL	#1@EXE$V_BUGREBOOT,G^EXE$GL_FLAGS; 
                                     0042    64 ;
                                     0042    65 ; WRITE MODIFIED PAGE LIST
                                     0042    66 ;
                 00000000'GF     D4  0042    67 	CLRL	G^SCH$GL_MFYLIM		; FORCE SWAPPER ACTIVITY
                 00000000'GF     D4  0048    68 	CLRL	G^SCH$GL_MFYLOLIM	;
                                     004E    69 ;
                                     004E    70 ; WAIT FOR IT TO COMPLETE
                                     004E    71 ;
                 00000000'GF     D5  004E    72 15$:	TSTL	G^SCH$GL_MFYCNT		; DONE
                          F8     12  0054    73 	BNEQ	15$			; IF NEQ THEN NO
                                     0056    74 ;
