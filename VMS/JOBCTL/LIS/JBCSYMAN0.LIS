                                     003B   188 		SYM$NXTJOBSTEP,-	; NEXT JOB STEP FOR BATCH JOB
                                     003B   189 		SYM$SYNCJOB,-		; SYNCRONIZE WITH JOB
                                     003B   190 		>
                                     0063   191 	SETSTAT	SMINVREQ		; SET ERROR-INVALID REQUEST CODE
                                 04  0066   192 120$:	RET				; GO BACK TO DISPATCHER
