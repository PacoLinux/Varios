                                     0008   100 	.SBTTL	SYMBIONT MANAGER MAIN DISPATCHER
                                     0008   101 ;++
                                     0008   102 ; FUNCTIONAL DESCRIPTION:
                                     0008   103 ;
                                     0008   104 ;	THIS ROUTINE IS ENTERED BY THE JOB CONTROLLER MAINLINE
                                     0008   105 ;	WHEN IT DETECTS A MESSAGE IN THE APPROPRIATE LIST.
                                     0008   106 ;	THIS ROUTINE IN TURN DISPATCHES TO ONE OF SEVERAL
                                     0008   107 ;	ROUTINES DEPENDING UPON THE MESSAGE TYPE CODE INCLUDED.
                                     0008   108 ;	THE DISPATCH IS DONE WITH A CASE INSTRUCTION BASED ON
                                     0008   109 ;	THE REQUEST CODE FOLLOWED BY A CALL.  THE CALL IS DONE
                                     0008   110 ;	TO GENERATE A CALL FRAME SO THAT IF A FATAL ERROR IS
                                     0008   111 ;	DETECTED, ALL THAT NEED BE DONE IS ISSUE A RET WITH
                                     0008   112 ;	THE FAILURE INDICATION IN R0. PRIOR TO CASING ON THE
                                     0008   113 ;	REQUEST TYPE, A SEARCH OF THE SYMBIONT MANAGER QUEUES
                                     0008   114 ;	IS DONE TO FIND THE QUEUE ASSOCIATED WITH THE REQUEST.
                                     0008   115 ;	THE LOCATION "QUESRCFLG" HAS A BIT SET CORRESPONDING
                                     0008   116 ;	TO THE FUNCTIONS WHICH REQUIRE A QUEUE SEARCH.
                                     0008   117 ;	THERE IS ONE SPECIAL CASE, THAT IS INITIALIZE A
                                     0008   118 ;	QUEUE MAY OR MAY NOT FAIL TO FIND THE QUEUE AND EITHER
                                     0008   119 ;	CASE IS VALID. IN OTHER CASES, FAILURE TO FIND THE
                                     0008   120 ;	QUEUE RESULTS IN TERMINATION OF THE REQUEST.
                                     0008   121 ;
                                     0008   122 ; CALLING SEQUENCE:
                                     0008   123 ;
                                     0008   124 ;	BSB/JSB	JBC$SYMBMAN
                                     0008   125 ;
                                     0008   126 ; INPUT PARAMETERS:
                                     0008   127 ;
                                     0008   128 ;	R2 HAS THE ADDRESS OF THE MESSAGE TO BE PROCESSED
                                     0008   129 ;	R10 HAS BASE ADDRESS OF THE SYSTEM QUEUE FILE
                                     0008   130 ;	R11 HAS ADDRESS OF THE JOB CONTROLLER DATA BLOCK
                                     0008   131 ;
                                     0008   132 ; OUTPUT PARAMETERS:
                                     0008   133 ;
                                     0008   134 ; COMPLETION CODES:
                                     0008   135 ;
                                     0008   136 ;	R0 CONTAINS THE FINAL STATUS OF THE REQUEST
                                     0008   137 ;	R2 CONTAINS THE MESSAGE CODE FOR RESPONCE
                                     0008   138 ;
                                     0008   139 ; SIDE EFFECTS:
                                     0008   140 ;
                                     0008   141 ;	THE QUEUE IS MODIFIED AS PER THE REQUEST.
                                     0008   142 ;--
                                     0008   143 
                                     0008   144 	PURE_SECTION
                                     0008   145 	.ENABL	LSB
                                     0008   146 
                                     0008   147 SYM$SYMBMAN::
                                     0008   148 	SETSTAT	SYMBDSAB		; SYMBIONT MANAGER DISABLED ERROR
                          5A     D5  000B   149 	TSTL	R10			; IS THERE A SYSTEM QUEUE SECTION?
                          04     13  000D   150 	BEQL	20$			; BR IF NO-RETURN ERROR
         17'AF            6C     FA  000F   151 	CALLG	(AP),B^100$		; CREATE A CALL FRAME
            52            20     3C  0013   152 20$:	MOVZWL	#MSG$_SMBRSP,R2		; SET MESSAGE TYPE
                                 05  0016   153 	RSB				; BACK TO JOB CONTROLLER
                                     0017   154 ;
                                     0017   155 ; DISPATCH TO THE APPROPRIATE ROUTINE.  IF QUEUE SEARCH WAS DONE
                                     0017   156 ; R5 POINTS AT NAME IF SEARCHED FAILED, R6 CONTAINS ADDRESS OF
