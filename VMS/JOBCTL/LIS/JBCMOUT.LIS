                                     0000    57 ; MACRO TO GENERATE AN ENTRY IN THE ERROR MESSAGE TABLE
                                     0000    58 ;
                                     0000    59 ;	JBC_ERR	ERR,MESG,GETERR=NO,SEVERITY=WARN
                                     0000    60 ;
                                     0000    61 ; WHERE:
                                     0000    62 ;
                                     0000    63 ;	ERR = THE NAME OF THE ERROR
                                     0000    64 ;	MESG =	THE SYSTEM ERROR CODE ID FIELD
                                     0000    65 ;	GETERR = A FLAG TO INDICATE IF THIS HAS AN ERROR CAUSE CODE IN R0
                                     0000    66 ;	SEVERITY = SEVERITY
                                     0000    67 ;
                                     0000    68  
                                     0000    69 	.MACRO	JBC_ERR	ERR,MESG,GETERR=NO,SEVERITY=WARN
                                     0000    70 ERR==JBC_ERR_NUM
                                     0000    71 	.$$. = 0			; DEFAULT SEVERITY IS WARNING
                                     0000    72 .IIF IDN <SEVERITY><ERROR>, .$$. = 2	; SET "ERROR" SEVERITY
                                     0000    73 .IIF IDN <SEVERITY><FATAL>, .$$. = 4	; SET "SEVER" SEVERITY
                                     0000    74  
                                     0000    75 	.WORD	<<JBC$_'MESG ! .$$.> & ^XFFFF>
                                     0000    76  
                                     0000    77 .IIF IDN <GETERR><YES>,	JBC_GETERR = JBC_GETERR!<1 @ JBC_ERR_NUM>
                                     0000    78  
                                     0000    79 JBC_ERR_NUM = JBC_ERR_NUM + 1
                                     0000    80  
                                     0000    81 	.ENDM
