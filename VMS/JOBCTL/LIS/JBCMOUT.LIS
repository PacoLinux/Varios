V01             FATAT EXCEPTION HANDLER                                                                                          (3)

                                     0100   203 	.SBTTL	FATAT EXCEPTION HANDLER
                                     0100   204 ;++
                                     0100   205 ; FUNCTIONAL DESCRIPTION:
                                     0100   206 ;
                                     0100   207 ;	THIS ROUTINE IS ENTERED WHEN THE JOB CONTROLLER GETS AN EXCEPTION.
                                     0100   208 ;	IT PRINTS THE CONTEXT OF THE ERROR, STARTS A NEW INCARNATION
                                     0100   209 ;	OF THE JOB CONTROLLER, THEN DELETES ITSELF.
                                     0100   210 ;
                                     0100   211 ; CALLING SEQUENCE:
                                     0100   212 ;
                                     0100   213 ;	ENTER VIA A CALL INSTRUCTION FROM THE EXCEPTION DISPATCHER
                                     0100   214 ;
                                     0100   215 ; INPUT PARAMETERS:
                                     0100   216 ;
                                     0100   217 ;	THE EXCEPTION ARGUMENT LIST
                                     0100   218 ;
                                     0100   219 ; OUTPUT PARAMETERS:	NONE
                                     0100   220 ;
                                     0100   221 ; COMPLETION CODES:	NONE
                                     0100   222 ;
                                     0100   223 ; SIDE EFFECTS:
                                     0100   224 ;
                                     0100   225 ;	AN IMAGE EXIT IS EFFECTED.
                                     0100   226 ;
                                     0100   227 ;--
                                     0100   228 	.ENABL	LSB
                                     0100   229  
                               0FFC' 0100   230 	.ENTRY	JBC$EXCEPTION,^M<R2,R3,R4,R5,R6,R7,R8,R9,R10,R11>
                                     0102   231  
                                     0102   232 	$SETAST_S  #0			; DISABLE AST'S
            5B       0000'CF     9E  010B   233 	MOVAB	W^JBC$T_DATABLK,R11	; SET JOB_CONTROLLER DATA BLOCK
            59         04 AC     D0  0110   234 	MOVL	CHF$L_SIGARGLST(AP),R9	; ADDRESS OF SIGNAL ARRAY
            58         08 AC     D0  0114   235 	MOVL	CHF$L_MCHARGLST(AP),R8	; AND THE MECHANISM ARRAY
            50            69     D0  0118   236 	MOVL	(R9),R0			; GET SIGNAL ARRAY COUNT
            57         04 A940   DE  011B   237 	MOVAL	4(R9)[R0],R7		; GET TOP OF EXCEPTION STACK
                          6940   DD  0120   238 10$:	PUSHL	(R9)[R0]		; PUSH SIGNAL ARGUMENTS
            FA            50     F5  0123   239 	SOBGTR	R0,10$			; LOOP IF MORE
            69            02     C3  0126   240 	SUBL3	#2,(R9),-(SP)		; SET ERROR ARGUMENT COUNT
                          7E         0129       
            69            04     C1  012A   241 	ADDL3	#<1+3>,(R9),R1		; SET TOTAL FAO ARGUMENT COUNT
                          51         012D       
                          00EA   30  012E   242 	BSBW	FAOUT0			; FORMAT AND OUTPUT MESSAGE
                               FF01  0131   243 	.WORD	<HDRMSG-.-2>		; OFFSET TO MESSAGE FORMAT STRING
            51         3C AD     DE  0133   244 	MOVAL	20+<10*4>(FP),R1	; POINT AT REGISTERS SAVED BY CALL
            50            0A     D0  0137   245 	MOVL	#10,R0			; SET NUMBER OF REGISTERS SAVED
                          71     DD  013A   246 20$:	PUSHL	-(R1)			; PUT REGISTERS IN STACK IN PROPER ORDER
            FB            50     F5  013C   247 	SOBGTR	R0,20$			; LOOP
            7E         0C A8     7D  013F   248 	MOVQ	CHF$L_MCH_SAVR0(R8),-(SP) ; NOW PUSH R0 AND R1
                          05     DD  0143   249 	PUSHL	#5			; HI REQISTER
                          00     DD  0145   250 	PUSHL	#0			; LO REGISTER
                                     0147   251 	FAOUT	REGMSG,8		; PRINT RGISTER 0-5
                          0B     DD  014D   252 	PUSHL	#11			; HI REGISTER
                          06     DD  014F   253 	PUSHL	#6			; LO REGISTER
                                     0151   254 	FAOUT	REGMSG,8		; NOW REGISTERS 6-11
            51       FF63 CF     9E  0157   255 	MOVAB	STKHDR,R1		; SET STACK DUMP PREFIX
                          00F3   30  015C   256 	BSBW	TYPE			; PRINT THAT
            51            04     D0  015F   257 30$:	MOVL	#4,R1			; SET STACK FRAME DUMP LOOP COUNT
