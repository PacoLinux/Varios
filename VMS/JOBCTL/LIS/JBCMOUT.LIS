                                     0266   359 	.SBTTL	JBC ERROR/MESSAGE OUTPUT
                                     0266   360 ;++
                                     0266   361 ; FUNCTIONAL DESCRIPTION:
                                     0266   362 ;
                                     0266   363 ;	THIS MODULE IS CALL TO DISPLAY AN ERROR FOR THE
                                     0266   364 ;	JOB CONTROLLER.
                                     0266   365 ;
                                     0266   366 ; CALLING SEQUENCE:
                                     0266   367 ;
                                     0266   368 ;	BSB	JBC$SYSMSG		; TO DISPLAY A SYSTEM ERROR AND RETURN
                                     0266   369 ;
                                     0266   370 ; INPUT PARAMETERS:
                                     0266   371 ;
                                     0266   372 ;	R0 IS ERROR CODE
                                     0266   373 ;
                                     0266   374 ; OUTPUT PARAMETERS:	NONE
                                     0266   375 ;
                                     0266   376 ; COMPLETION CODES:
                                     0266   377 ;
                                     0266   378 ;	R0,R1 RETURNED AS SET BY QIO SYSTEM SERVICE
                                     0266   379 ;
                                     0266   380 ; SIDE EFFECTS:		NONE
                                     0266   381 ;--
                                     0266   382 JBC$SYSMSG::				; PRINT SYSTEM MESSAGE
                     0008'CF     9F  0266   383 	PUSHAB	W^OPRDATA		; FORM QUAD WORD DESCRIPTOR
            7E         50 8F     9A  026A   384 	MOVZBL	#OPRDSIZ,-(SP)		; FOR OPERATOR MESSAGE BUFFER
            51            5E     D0  026E   385 	MOVL	SP,R1			; ADDRESS OF DESCRIPTOR
                                     0271   386 	$GETMSG_S  R0,(R1),(R1),#15	; CONVERT ERROR CODE
            6E            08     C0  0282   387 	ADDL	#OPRHDR,(SP)		; ADD IN THE HEADER
         04 AE            08     C2  0285   388 	SUBL	#OPRHDR,4(SP)		; BACKUP ADDRESS FOR HEADER
                          7E     D4  0289   389 	CLRL	-(SP)			; RESPONCE MAILBOX CHANNEL
                       04 AE     7F  028B   390 	PUSHAQ	4(SP)			; ADDRESS OF MESSAGE DESCRIPTOR
   00000000'9F            04     FB  028E   391 	CALLS	#4,@#SYS$SNDOPR		; SENT TO OPER AND CLEAR QUAD WORD
                                 05  0295   392 	RSB				; RETURN TO CALLER
                                     0296   393  
                                     0296   394 	.END
