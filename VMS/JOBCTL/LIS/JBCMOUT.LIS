                                     021B   306 ; FUNCTIONAL DESCRIPTION:
                                     021B   307 ;
                                     021B   308 ;	THIS ROUTINE IS CALLED TO FORMAT THEN PRINT A MESSAGE
                                     021B   309 ;
                                     021B   310 ; CALLING SEQUENCE:
                                     021B   311 ;
                                     021B   312 ;	BSB/JSB	FAOUT0			; CALL WITH ARG COUNT IN R1
                                     021B   313 ;	BSB/JSB	FAOUT1			; CALL WIHT ARG COUNT IN LINE
                                     021B   314 ;
                                     021B   315 ; INPUT PARAMETERS:
                                     021B   316 ;
                                     021B   317 ; AT FAOUT0:	R1 IS TOTAL ARGUMENT COUNT FOR FAO
                                     021B   318 ;
                                     021B   319 ; AT FAOUT1:	(SP) IS TOTAL ARGUMENT COUNT FOR FAO
                                     021B   320 ;
                                     021B   321 ; EITHER:	NEXT SEQENTIAL LOCATION IS OFFSER TO MESSAGE FORMAT STRING
                                     021B   322 ;		R11 HAS ADDRESS JOB CONTROLLER DATA BLOCK
                                     021B   323 ;
                                     021B   324 ; OUTPUT PARAMETERS:
                                     021B   325 ;
                                     021B   326 ;	MESSAGE IS FORMATTED AND DISPLAYED
                                     021B   327 ;
                                     021B   328 ; COMPLETION CODES:	NONE
                                     021B   329 ;
                                     021B   330 ; SIDE EFFECTS:		NONE
                                     021B   331 ;--
                                     021B   332 	.ENABL	LSB
                                     021B   333  
                          08     BA  021B   334 FAOUT0:	POPR	#^M<R3>			; GET PC OF SUBROUTINE CALL
                          05     11  021D   335 	BRB	10$			;
                          08     BA  021F   336 FAOUT1:	POPR	#^M<R3>			; GET PC OF SUBROUTINE CALL
            51            83     9A  0221   337 	MOVZBL	(R3)+,R1		; PICK UP FAO ARGUMENT COUNT
            50            83     32  0224   338 10$:	CVTWL	(R3)+,R0		; GET OFFSET TO MESSAGE
            50            53     C0  0227   339 	ADDL	R3,R0			; FIND ADDRESS OF STRING
         10 AB            80     9A  022A   340 	MOVZBL	(R0)+,JCD_Q_TMPDESC(R11); SET SIZE OF STRING
         14 AB            50     D0  022E   341 	MOVL	R0,JCD_Q_TMPDESC+4(R11)	; SET ADDRESS OF STRING
            52         0A AB     3E  0232   342 	MOVAW	JCD_W_LBUFSIZ(R11),R2	; SET POINTER TO LINE BUFFER SIZE WORD
            72            62     B0  0236   343 	MOVW	(R2),-(R2)		; SET LENGTH
                          62     7F  0239   344 	PUSHAQ	(R2)			; BUFFER DESCRIPTOR
                          62     3F  023B   345 	PUSHAW	(R2)			; PLACE TO RETURN RESULTANT LENGTH
                       10 AB     7F  023D   346 	PUSHAQ	JCD_Q_TMPDESC(R11)	; FORMAT STRING DESCRIPTOR
   00000000'9F            51     FB  0240   347 	CALLS	R1,@#SYS$FAO		; FORMAT MESSAGE
                          53     DD  0247   348 	PUSHL	R3			; SET REAL RETURN ADDRESS
            50            62     9A  0249   349 TYPR2:	MOVZBL	(R2),R0			; GET THE LENGTH
            51         04 A2     D0  024C   350 	MOVL	4(R2),R1		; STARTING ADDRESS OF MESSAGE
                          03     11  0250   351 	BRB	TYP0			; TYPE THAT OUT
            50            81     9A  0252   352 TYPE:	MOVZBL	(R1)+,R0		; COUNT TO R0, STRING ADDRESS TO R1
                          03     BB  0255   353 TYP0:	PUSHR	#^M<R0,R1>		; PUSH STRING DESCRIPTOR IN STACK
                     FDCD CF     DF  0257   354 	PUSHAL	W^ERRMSGDEV		; ADDRESS OF DEVICE NAME
                       04 AE     DF  025B   355 	PUSHAL	4(SP)			; ADDRESS OF STRING DESCIPTOR OF MESSAGE
   00000000'9F            04     FB  025E   356 	CALLS	#4,@#SYS$BRDCST		; PRINT MESSAGE ON 1 DEVICE
                                 05  0265   357 	RSB				; GET OUT
