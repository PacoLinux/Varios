V01             ERROR MESSAGE HANDLER                                                                                            (3)

                                     00CC   153 	.SBTTL	ERROR MESSAGE HANDLER
                                     00CC   154 ;++
                                     00CC   155 ; FUNCTIONAL DESCRIPTION:
                                     00CC   156 ;
                                     00CC   157 ;	THIS ROUTINE IS ENTERED BY THE CHANGE MODE DISPATCHER
                                     00CC   158 ;	WHEN A CHANGE MODE TO USER INSTRUCTION HAS BEEN EXECUTED.
                                     00CC   159 ;	THIS IS THE MEANS FOR THE JOB CONTROLLER TO REPORT ALL
                                     00CC   160 ;	ERRORS.
                                     00CC   161 ;
                                     00CC   162 ; CALLING SEQUENCE:
                                     00CC   163 ;
                                     00CC   164 ;	CALLED SUCH THAT AN REI WILL RETURN CONTROL TO
                                     00CC   165 ;	POINT OF THE CHANGE MODE INSTRUCTION AFTER CLEARING
                                     00CC   166 ;	THE ARGUMENT LONG WORD FROM THE STACK.
                                     00CC   167 ;
                                     00CC   168 ; INPUT PARAMETERS:
                                     00CC   169 ;
                                     00CC   170 ;	(SP) IS CHANGE MODE ARGUEMENT
                                     00CC   171 ;	4(SP) IS PC OF CHANGE MODE INSTRUCTION
                                     00CC   172 ;	8(SP) IS PSL OF CHANGE MODE INSTRUCTION
                                     00CC   173 ;	R11 HAS ADDRESS JOB CONTROLLER DATA BLOCK
                                     00CC   174 ;
                                     00CC   175 ; OUTPUT PARAMETERS:
                                     00CC   176 ;
                                     00CC   177 ;	ERROR MESSAGE IS DISPLAYED
                                     00CC   178 ;
                                     00CC   179 ; COMPLETION CODES:	NONE
                                     00CC   180 ;
                                     00CC   181 ; SIDE EFFECTS:
                                     00CC   182 ;
                                     00CC   183 ;	AN IMAGE EXIT IS EFFECTED IF THE ERROR WAS FATAL
                                     00CC   184 ;
                                     00CC   185 ;--
                                     00CC   186  
                                     00CC   187 JBC$ERRORMSG::				; ENTER HERE AS VIA TRAP
                          02     BA  00CC   188 	POPR	#^M<R1>			; GET INTERNAL ERROR NUMBER
                          03     BB  00CE   189 	PUSHR	#^M<R0,R1>		; SAVE R0 AND INTERNAL ERROR NUMBER
            50       FF2B CF41   3C  00D0   190 	MOVZWL	JBC$AW_ERR_CODE[R1],R0	; GET INTERNAL ERROR CODE
                                 F0  00D6   191 	INSV	#<JBC$_NORMAL@-STS$V_FAC_NO>,-; INSERT JBC FACILITY NUMBER
            10            04         00D7   192 		#STS$V_FAC_NO,#STS$S_FAC_NO,R0 ; INTO ERROR MESSAGE NUMBER
            50            0C         00D9       
                          0188   30  00DB   193 	BSBW	JBC$SYSMSG		; PRINT MESSAGE FOR SYSTEM MESSAGE FILE
            50            6E     7D  00DE   194 	MOVQ	(SP),R0			; RESTORE ERROR CODE AND SAVED R0
       FF3E CF            51     E1  00E1   195 	BBC	R1,JBC$L_GETERR,10$	; BR IF R0 IS NOT A SYSTEM ERROR CODE
                          03         00E6       
                          017C   30  00E7   196 	BSBW	JBC$SYSMSG		; PRINT THE SYSTEM MESSAGE
                          03     BA  00EA   197 10$:	POPR	#^M<R0,R1>		; GET INTERNAL ERROR NUMBER AND R0
            51       FF0F CF41   3C  00EC   198 	MOVZWL	JBC$AW_ERR_CODE[R1],R1	; GET INTERNAL ERROR CODE BACK
            51            02     E0  00F2   199 	BBS	#2,R1,20$		; BR IF FATAL BIT IS SET
                          01         00F5       
                                 02  00F6   200 	REI				;
                                     00F7   201 20$:	$EXIT_S	R0			; EXIT WITH ERROR
