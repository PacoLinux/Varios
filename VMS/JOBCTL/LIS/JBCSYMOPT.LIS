                                     0000    59 	.SBTTL	PROCESS OPTIONS
                                     0000    60 ;++
                                     0000    61 ; FUNCTIONAL DESCRIPTION:
                                     0000    62 ;
                                     0000    63 ;	THIS ROUTINE IS CALLED TO PROCESS ALL THE OPTIONS
                                     0000    64 ;	ASSOCIATED WITH A GIVEN SYMBIONT MANAGER REQUEST.
                                     0000    65 ;	THERE ARE SEVERAL MAIN SUBROTINES THAT ARE USED
                                     0000    66 ;	DEPENDING UPON THE CURRENT REQUEST BEING PROCESSED
                                     0000    67 ;	BY THE SYMBIONT MANAGER.
                                     0000    68 ;
                                     0000    69 ; CALLING SEQUENCE:
                                     0000    70 ;
                                     0000    71 ;	BSB/JSB	SYM$CNVOPTINI		; CONVERT OPTIONS FOR INIT & START
                                     0000    72 ;	BSB/JSB	SYM$CNVOPTENT		; CONVERT OPTIONS FOR AN ENTER
                                     0000    73 ;	BSB/JSB	SYM$CNVOPTJOB		; CONVERT OPTIONS FOR JOB CREATE
                                     0000    74 ;
                                     0000    75 ; INPUT PARAMETERS:
                                     0000    76 ;
                                     0000    77 ; IN ALL CASES:		R5 HAS ADDRESS OF FIRST OPTION
                                     0000    78 ; 			R10 HAS BASE OF PRINT QUEUE FILE
                                     0000    79 ;			R9 HAS CURRENT SYMBIONT MANAGER RECORD
                                     0000    80 ;
                                     0000    81 ; AT SYM$CNVOPTINI:	R6 IS THE ADDRESS OF THE DEVICE QUEUE
                                     0000    82 ;
                                     0000    83 ; AT SYM$CNVOPTENT:	R7 HAS ADDRESS OF PRINT JOB HEADER RECORD
                                     0000    84 ;			R8 HAS ADDRESS OF THE PRINT FILE DESCRIPTOR
                                     0000    85 ;
                                     0000    86 ; OUTPUT PARAMETERS:
                                     0000    87 ;
                                     0000    88 ;	ALL OPTIONS ARE PROCESS ADJUSTING DATA IN THE RECORDS
                                     0000    89 ;	AS NECESSARY. R5 IS ADVANCED TO THE END OF THE OPTION LIST
                                     0000    90 ;	AS SPECIFIED BY THE LENGTH OF THE MESSAGE, OR BY ENCOUNTERING
                                     0000    91 ;	A ZERO BYTE IN THE OPTION FIELD
                                     0000    92 ;
                                     0000    93 ; COMPLETION CODES:
                                     0000    94 ;
                                     0000    95 ;	R0 WILL CONTAIN SMINVOPT IF AN INVALID OPTION IS DETECTED
                                     0000    96 ;
                                     0000    97 ; SIDE EFFECTS:		NONE
                                     0000    98 ;--
                                     0000    99 	PURE_SECTION
                                     0000   100 
                                     0000   101 SYM$CNVOPTINI::				; CONVERT OPTIONS FOR INIT AND START
                          0197   30  0000   102 	BSBW	SYM$GETOPT		; PICK UP NEXT OPTION CODE
                       FA AF     DF  0003   103 	PUSHAL	B^SYM$CNVOPTINI		; SET RETURN ADDRESS
                                     0006   104 	CASE	R0,-			; DISPATCH ON CODE
                                     0006   105 		LIMIT = #SMO$K_CURFORM,<- ; LOW VALUE FOR CASE IS THE FORMS-TYPE
                                     0006   106 		DEVQUEFORM,-		; SET DEVICE FORMS TYPE
                                     0006   107 		GENERICPRT,-		; GENERIC PRINTING ALLOWED
                                     0006   108 		NOGENERIC,-		; AND THE ONE TO INHIBIT GENERIC OUTPUT
                                     0006   109 		DETJOB,-		; THE QUEUE IS FOR DETACHED JOB QUEUING
                                     0006   110 		GENDEVQUE,-		; THE QUEUE IS FOR GENERIC DEVICE
                                     0006   111 		NOGENDEV,-		; NOT A GENERIC DEVICE
                                     0006   112 		DEFBRST,-		; DEFAULT A BURST PAGE
                                     0006   113 		NODEFBRST,-		; NO DEFAULT BURST PAGE
                                     0006   114 		DEFFLAG,-		; DEFAULT A FLAG PAGE
