                                     00F3   261 ;+
                                     00F3   262 ; ACM$SENDMSG - SNED MESSAGE TO THE ACCOUNTING FILE
                                     00F3   263 ;
                                     00F3   264 ; THIS ROUTINE IS ENTERED WHEN A USER REQUESTS TO HAVE A MESSAGE INSERTED
                                     00F3   265 ; INTO THE ACCOUNTING FILE. NOTE THAT THE HEADER FOR THE ACCOUNTING FILE
                                     00F3   266 ; IS 4 BYTES LONGER THAN THE HEADER FOR A JOB CONTROLLER MESSAGE.  THEREFORE
                                     00F3   267 ; THE HEADER FOR THE ACCOUNTING FILE IS BUILT USING THE LONGWORD PRECEEDING
                                     00F3   268 ; THE JOB CONTROLLER HEADER WHICH IS THE PROCESS ID FROM THE MAILBOX READ
                                     00F3   269 ; IO STATUS BLOCK.  AS THE ACCOUNTING HEADER IS BUILT, THE AREAS THAT MUST
                                     00F3   270 ; BE SAVED FROM THE MESSAGE HEADER ARE PUT INTO TEMP LOCATIONS.
                                     00F3   271 ;-
                                     00F3   272  
                                     00F3   273 ACM$SENDMSG:				;
            53         FC A9     DE  00F3   274 	MOVAL	-4(R9),R3		; SET START OF ACCOUNTING MESSAGE
            52            53     D0  00F7   275 	MOVL	R3,R2			; COPY START OF MESSAGE
            50            62     D0  00FA   276 	MOVL	(R2),R0			; SAVE PID OF SENDING PROCESS
                                     00FD   277 	ASSUME	ACC$W_MSGTYP EQ 0	;
            82            11     9A  00FD   278 	MOVZBL	#ACC$K_INSMSG,(R2)+	; SET MESSAGE TYPE FIELD
                                     0100   279 	ASSUME	ACC$L_FINALSTS EQ 4	;
            82            00'    D0  0100   280 	MOVL	S^#SS$_NORMAL,(R2)+	; FINAL STATUS IS NORMAL
                                     0103   281 	ASSUME	ACC$L_PID EQ 8		;
            82            50     D0  0103   282 	MOVL	R0,(R2)+		; INSERT PID OF SENDING PROCESS
                                     0106   283 	ASSUME	ACC$L_JOBID EQ 12	;
                          82     D4  0106   284 	CLRL	(R2)+			; SET JOB ID TO 0
                       10 A9     DD  0108   285 	PUSHL	SMR$T_USERNAM(R9)	; SAVE FIRST 4 BYTE OF USERNAME
                                     010B   286 	ASSUME	ACC$Q_TERMTIME EQ 16	;
            82         C4 AB     7D  010B   287 	MOVQ	JCD_Q_TIME(R11),(R2)+	; SET TIME OF DAY
            50            62     7D  010F   288 	MOVQ	(R2),R0			; SAVE REMAINDER OF USER NAME
                                     0112   289 	ASSUME	ACC$T_ACCOUNT EQ 24	;
            82         1C A9     7D  0112   290 	MOVQ	SMR$Q_ACNTNAM(R9),(R2)+ ; FILL IN USERS ACCOUNT NAME
                                     0116   291 	ASSUME	ACC$T_USERNAME EQ 32	;
                          82   8ED0  0116   292 	POPL	(R2)+			; INSERT FIRST 4 BYTES OF USERNAME
            82            50     7D  0119   293 	MOVQ	R0,(R2)+		; AND REST OF USERNAME
            51            82     9A  011C   294 	MOVZBL	(R2)+,R1		; GET COUNT OF USER DATA
            52            51     C0  011F   295 	ADDL	R1,R2			; FIND END OF TOTAL MESSAGE
            52            53     C2  0122   296 	SUBL	R3,R2			; FIND BYTE COUNT FOR MESSAGE
                          FEFD   31  0125   297 	BRW	JBC$WRTACNT		; WRITE RECORD TO ACCOUNTING FILE
