                                     0022    94 	.SBTTL	ACCOUNT FILE MAINTAINANCE ROUTINES
                                     0022    95 ;++
                                     0022    96 ; FUNCTIONAL DESCRIPTION:
                                     0022    97 ;
                                     0022    98 ;	THIS ROUTINE IS CALLED TO WRITE THEN OPTIONAL FLUSH
                                     0022    99 ;	THE ACCOUNTING FILE.
                                     0022   100 ;
                                     0022   101 ; CALLING SEQUENCE:
                                     0022   102 ;
                                     0022   103 ;	BSB/JSB	JBC$WRTFLSACNT		; WRITE AND FLUSH THE ACCOUNTING FILE
                                     0022   104 ;	BSB/JSB	JBC$WRTACNT		; WRITE THE ACCOUNTING FILE
                                     0022   105 ;
                                     0022   106 ; INPUT PARAMETERS:
                                     0022   107 ;
                                     0022   108 ;	R2 = BYTE COUNT OF RECORD TO WRITE
                                     0022   109 ;	R3 = BUFFER ADDRESS
                                     0022   110 ;
                                     0022   111 ; IMPLICIT INPUTS:
                                     0022   112 ;
                                     0022   113 ;	THE ACCOUNT FILE IS OPEN IF ACCOUNTING IS ENABLED.
                                     0022   114 ;
                                     0022   115 ; OUTPUT PARAMETERS:
                                     0022   116 ;
                                     0022   117 ;	R2 = ADDRESS OF ACCOUNT FILE RAB
                                     0022   118 ;
                                     0022   119 ; COMPLETION CODES:
                                     0022   120 ;
                                     0022   121 ;	R0 LOW BIT CLEAR INDICATES THE FILE WAS NOT WRITTEN.
                                     0022   122 ;
                                     0022   123 ; SIDE EFFECTS:
                                     0022   124 ;
                                     0022   125 ;	A NEW COPY OF THE ACCOUNTING FILE MAY BE OPENNED
                                     0022   126 ;	IF THERE IS AN ERROR DETECTED WRITING THE FILE.
                                     0022   127 ;
                                     0022   128 ;--
                                     0022   129  
                                     0022   130 	PURE_SECTION
                                     0022   131 	.ENABL	LSB
                                     0022   132  
                                     0022   133 JBC$WRTFLSACNT::			; WRITE AND FLUSH THE FILE
                          04     10  0022   134 	BSBB	10$			; COMMON ROUTINE
                                 01  0024   135 	.BYTE	1			; THE FLUSH FLAG IS TRUE
                                     0025   136  
                                     0025   137 JBC$WRTACNT::				; WRITE THE ACCOUNTING FILE
                          01     10  0025   138 	BSBB	10$			; COMMON ROUTINE
                                 00  0027   139 	.BYTE	0			; THE FLUSH FLAG IS FALSE
         02 A3            52     B0  0028   140 10$:	MOVW	R2,ACC$W_MSGSIZ(R3)	; SET SIZE IN RECORD
       0072'CF            52     B0  002C   141 	MOVW	R2,W^JBC$G_ACNTRAB+RAB$W_RSZ ; SET RECORD SIZE IN RAB
            52       0050'CF     9E  0031   142 	MOVAB	W^JBC$G_ACNTRAB,R2	; GET RAB ADDRESS
         18 A2            9E     90  0036   143 	MOVB	@(SP)+,RAB$L_CTX(R2)	; COPY FLUSH FLAG INTO USER CONTEXT AREA
         28 A2            53     D0  003A   144 	MOVL	R3,RAB$L_RBF(R2)	; SET RECORD BUFFER ADDRESS
                          50     D4  003E   145 20$:	CLRL	R0			; ASSUME NO ACCOUNTING
                       02 A2     B5  0040   146 	TSTW	RAB$W_ISI(R2)		; FILE OPENNED?
                          39     13  0043   147 	BEQL	40$			; BR IF NO
            51            63     3C  0045   148 	MOVZWL	ACC$W_MSGTYP(R3),R1	; GET THE TYPE OF ACCOUNTING
       0094'CF            51     E1  0048   149 	BBC	R1,W^JBC$L_ACNTFLAG,40$	; BR IF THIS TYPE IS OFF
                          30         004D       
