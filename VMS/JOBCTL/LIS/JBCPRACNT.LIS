                                     0085   171 	.SBTTL	ACCOUNT FILE OPEN ROUTINE
                                     0085   172 ;++
                                     0085   173 ; FUNCTIONAL DESCRIPTION:
                                     0085   174 ;
                                     0085   175 ;	THIS ROUTINE IS CALLED TO OPEN AN ACCOUNTING FILE.
                                     0085   176 ;	IF THERE IS ALREADY ONE OPEN IT IS CLOSED AND A NEW
                                     0085   177 ;	COPY IS CREATED.
                                     0085   178 ;
                                     0085   179 ; CALLING SEQUENCE:
                                     0085   180 ;
                                     0085   181 ;	BSB/JSB	JBC$OPENACNT		;
                                     0085   182 ;
                                     0085   183 ; INPUT PARAMETERS:	NONE
                                     0085   184 ;
                                     0085   185 ; OUTPUT PARAMETERS:
                                     0085   186 ;
                                     0085   187 ;	R2 = ADDRESS OF ACCOUNT FILE RAB
                                     0085   188 ;
                                     0085   189 ; COMPLETION CODES:
                                     0085   190 ;
                                     0085   191 ;	R0 LOW BIT CLEAR INDICATES THE FILE WAS NOT OPENED.
                                     0085   192 ;
                                     0085   193 ; SIDE EFFECTS:		NONE
                                     0085   194 ;--
                                     0085   195 	PURE_SECTION
                                     0085   196  
                                     0085   197 JBC$OPENACNT::				; OPEN THE ACCOUNTING FILE
            52       0000'CF     9E  0085   198 	MOVAB	W^JBC$G_ACNTFAB,R2	; SET FAB ADDRESS
                                     008A   199 	SETBIT	FAB$V_CIF,FAB$L_FOP(R2)	; ASSUME OPEN PREVIOUS IS DESIRED
                       02 A2     B5  008F   200 	TSTW	FAB$W_IFI(R2)		; IS FILE OPEN?
                          0E     13  0092   201 	BEQL	10$			; BR IF FILE NOT OPEN
                                     0094   202 	CLRBIT	FAB$V_CIF,FAB$L_FOP(R2)	; IF ALREADY OPEN - CREATE A NEW
                                     0099   203 	$CLOSE	FAB=R2			; CLOSE THE CURRENT FILE
                       10 A2     D4  00A2   204 10$:	CLRL	FAB$L_ALQ(R2)		; SET FOR NO INITIAL ALLOCATION
                                     00A5   205 	$CREATE	FAB=R2			; CREATE OR OPEN THE FILE
            52       0050'CF     9E  00AE   206 	MOVAB	W^JBC$G_ACNTRAB,R2	; GET THE RAB
                       02 A2     B4  00B3   207 	CLRW	RAB$W_ISI(R2)		; MAKE SURE DISCONNECT IS DONE
            0D            50     E9  00B6   208 	BLBC	R0,30$			; BR IF ERROR OPENING FILE
                                     00B9   209 	$CONNECT RAB=R2			; CONNECT TO OUTPUT STREAM
            01            50     E9  00C2   210 	BLBC	R0,30$			; SHOW ERROR
                                 05  00C5   211 	RSB
                                     00C6   212 30$:	SHOW_ERROR  ACNT_FIL_OPN	; ERROR ON ACCOUNTING FILE
                                 05  00C8   213 	RSB				; 
