                                     011F   369 	.SBTTL	REMOVE JOB FORM QUEUE
                                     011F   370 ;++
                                     011F   371 ; FUNCTIONAL DESCRIPTION:
                                     011F   372 ;
                                     011F   373 ;	THIS ROUTINE IS CALLED TO REMOVE A JOB FROM THE QUEUE
                                     011F   374 ;	AND RELEASE THE QUEUE SPACE TO THE FREE LIST
                                     011F   375 ;
                                     011F   376 ; CALLING SEQUENCE:
                                     011F   377 ;
                                     011F   378 ;	ENTERED FROM SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     011F   379 ;
                                     011F   380 ; INPUT PARAMETERS:
                                     011F   381 ;
                                     011F   382 ;	R5 POINTS AT THE RECORD NUMBER FOR THE JOB HEADER TO RELEASE
                                     011F   383 ;	R6 CONTAINS THE ADDRESS OF THE ASSOCIATED DEVICE QUEUE HEADER
                                     011F   384 ;
                                     011F   385 ; IMPLICIT INPUTS:
                                     011F   386 ;
                                     011F   387 ;	R9 HAS THE ADDRESS OF THE SYMBIONT MANAGER RECORD
                                     011F   388 ;	R10 HAS THE BASE ADDRESS OF THE QUEUE FILE
                                     011F   389 ;	R11 HAS THE ADDRESS OF THE JOB CONTROLLER DATA BLOCK
                                     011F   390 ;
                                     011F   391 ; OUTPUT PARAMETERS:
                                     011F   392 ;
                                     011F   393 ;	THE JOB IS REMOVED FROM THE QUEUE AND THE RECORDS ARE FREED.
                                     011F   394 ;
                                     011F   395 ;
                                     011F   396 ; COMPLETION CODES:
                                     011F   397 ;
                                     011F   398 ;	R0 = NORMAL IF JOB IS SUCESSFULLY DEQUEUED AND RELEASED
                                     011F   399 ;
                                     011F   400 ; SIDE EFFECTS:		NONE
                                     011F   401 ;--
                                     011F   402  
                                     011F   403 SYM$RMVJOB::				; REMOVE JOB FROM QUEUE
            57            66     D0  011F   404 	MOVL	SMQ$L_CURRENT(R6),R7	; GET THE INDEX TO CURRENT JOB
                          0F     13  0122   405 10$:	BEQL	30$			; BR IF NOT A CURRENT JOB
            57            5A     C0  0124   406 	ADDL	R10,R7			; FIND ADDRESS OF JOB
         04 A7            65     B1  0127   407 	CMPW	(R5),SJH$W_JOBSEQ(R7)	; IS THIS THE JOB TO REMOVE?
                          52     13  012B   408 	BEQL	90$			; BR IF YES-GET RID OF IT
            57         08 A7     D0  012D   409 	MOVL	SJH$L_NEXTJOB(R7),R7	; FOLLOW THE LINK TO NEXT JOB
                          EF     11  0131   410 	BRB	10$			; AND CHECK IF THAT IS THE ONE
                          56     DD  0133   411 30$:	PUSHL	R6			; SAVE ADDRESS OF QUEUE HEADER
            56       0120 CA     DE  0135   412 	MOVAL	SQH$K_SIZE(R10),R6	; POINT AT FIRST QUEUE
            54         06 AA     9A  013A   413 	MOVZBL	SQH$B_MAXQUE(R10),R4	; SET COUNT OF QUEUES TO SEARCH
         08 A6            00     E1  013E   414 40$:	BBC	#SMQ$V_INUSE,SMQ$B_FLAGS(R6),50$ ; BR IF QUEUE NOT IN USE
                          13         0142       
         08 A6            01     E0  0143   415 	BBS	#SMQ$V_DETJOB,SMQ$B_FLAGS(R6),50$ ; DO NOT CHECK BATCH QUEUES
                          0E         0147       
            57            66     D0  0148   416 	MOVL	SMQ$L_CURRENT(R6),R7	; GET ADDRESS OF CURRENT JOB
                          09     13  014B   417 	BEQL	50$			; BR IF NO SUCH JOB
            57            5A     C0  014D   418 	ADDL	R10,R7			; FIND REAL ADDRESS OF JOB HEADER
         04 A7            65     B1  0150   419 	CMPW	(R5),SJH$W_JOBSEQ(R7)	; IS THIS THE JOB WERE LOOKING FOR
                          29     13  0154   420 	BEQL	90$			; BR IF YES
            56            20     C0  0156   421 50$:	ADDL	#SMQ$K_SIZE,R6		; POINT AT NEXT QUEUE
            E2            54     F5  0159   422 	SOBGTR	R4,40$			; CHECK NEXT IF THERE IS MORE
                          56   8ED0  015C   423 	POPL	R6			; GET ORIGINAL QUEUE BACK
