V01             MERGE QUEUE CONTENTS                                                                                             (2)

                                     0000    55 	.SBTTL	MERGE QUEUE CONTENTS
                                     0000    56 ;++
                                     0000    57 ; FUNCTIONAL DESCRIPTION:
                                     0000    58 ;
                                     0000    59 ;	THIS ROUTINE IS CALLED TO HAVE THE CONTENTS OF A QUEUE
                                     0000    60 ;	REMOVED AND MERGED INTO ANOTHER QUEUE.
                                     0000    61 ;
                                     0000    62 ; CALLING SEQUENCE:
                                     0000    63 ;
                                     0000    64 ;	ENTERED FROM SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     0000    65 ;
                                     0000    66 ; INPUT PARAMETERS:
                                     0000    67 ;
                                     0000    68 ;	R5 POINTS AT THE NAME OF THE QUEUE TO MERGE FROM
                                     0000    69 ;	R6 CONTAINS THE ADDRESS OF THE QUEUE TO MERGE TO.
                                     0000    70 ;
                                     0000    71 ; IMPLICIT INPUTS:
                                     0000    72 ;
                                     0000    73 ;	R9 HAS THE ADDRESS OF THE SYMBIONT MANAGER RECORD
                                     0000    74 ;	R10 HAS THE BASE ADDRESS OF THE QUEUE FILE
                                     0000    75 ;	R11 HAS THE ADDRESS OF THE JOB CONTROLLER DATA BLOCK
                                     0000    76 ;
                                     0000    77 ; OUTPUT PARAMETERS:
                                     0000    78 ;
                                     0000    79 ;	QUEUES ARE MERGED INTO 1, THE SOURCE QUEUE IS EMPTY.
                                     0000    80 ;--
                                     0000    81 	PURE_SECTION
                                     0000    82  
                                     0000    83 SYM$MERGE::				; MERGE A QUEUE WITH ANOTHER
                          56     DD  0000    84 	PUSHL	R6			; SAVE QUEUE TO MERGE INTO
                          FFFB'  30  0002    85 	BSBW	SYM$FINDQUEUE		; LOOK FOR THE QUEUE TO MERGE FROM
            58            56     D0  0005    86 	MOVL	R6,R8			; SET ADDRESS OF QUEUE TO MERGE FROM
                          56   8ED0  0008    87 	POPL	R6			; GET THE TARGET QUEUE BACK
            48            50     E9  000B    88 	BLBC	R0,90$			; EXIT IF TARGET QUEUE IS NOT HERE
            57         04 A8     D0  000E    89 10$:	MOVL	SMQ$L_HOLDLST(R8),R7	; GET FIRST RECORD IN HOLD LIST
                          18     13  0012    90 	BEQL	20$			; BR IF NONE
            57            5A     C0  0014    91 	ADDL	R10,R7			; FIND REAL ADDRESS OF JOB
                                 D0  0017    92 	MOVL	SJH$L_NEXTJOB(R7),-	; CLOSE UP THE LIST OF HOLD LIST
         04 A8         08 A7         0018    93 		SMQ$L_HOLDLST(R8)	; ENTRIES FOR MERGE FROM QUEUE
                                     001C    94 	SYM_QUECHANGE			; FLAG CHANGE IN THE QUEUE
            56            5A     C3  001F    95 	SUBL3	R10,R6,R0		; FIND INDEX TO THIS QUEUE
                          50         0022       
         06 A7            50     B0  0023    96 	MOVW	R0,SJH$W_QINDEX(R7)	; SET NEW QUEUE INDEX
                          FFD6'  30  0027    97 	BSBW	SYM$JOBINSQUE		; INSERT THIS JOB IN THE QUEUE
                          E2     11  002A    98 	BRB	10$			; CONTINUE TILL HOLD LIST IS EMPTY
                     0000'CF     DF  002C    99 20$:	PUSHAL	W^SYM$FINDQUEJOB	; SET ADDRESS FOR FIRST CO-ROUTINE CALL
            57            56     D0  0030   100 30$:	MOVL	R6,R7			; SAVE ADDRESS OF QUEUE TO MERGE TO
            56            58     D0  0033   101 	MOVL	R8,R6			; SET ADDRESS OF QUEUE TO REMOVE FROM
                          50     D4  0036   102 40$:	CLRL	R0			; INHIBIT DEQUEUING OF JOBS
                          9E     16  0038   103 	JSB	@(SP)+			; GET NEXT JOB FROM THE LIST
            16            50     E9  003A   104 	BLBC	R0,80$			; BR IF THERE ARE NO MORE
            57            5A     C3  003D   105 	SUBL3	R10,R7,R1		; FIND OFFSET TO QUEUE HEADER
                          51         0040       
         06 A2            51     B0  0041   106 	MOVW	R1,SJH$W_QINDEX(R2)	; SET INDEX OF QUEUE TO JOB IS GOING TO
                          54     D5  0045   107 	TSTL	R4			; CHECK IF THIS JOB WAS IN A PRINT QUEUE
                          ED     15  0047   108 	BLEQ	40$			; BR IF NOT IN PRINT QUEUE
                          9E     16  0049   109 	JSB	@(SP)+			; RETURN AND REQUEST REMOVAL OF JOB
JBCSYMAN2       - SYMBIONT MANAGER MAIN ROUTINE                  22-AUG-1978 00:50:07   VAX-11 MACRO X0.3-11               Page   3
V01             MERGE QUEUE CONTENTS                                                                                             (2)

            57            52     D0  004B   110 	MOVL	R2,R7			; SET JOB ADDRESS IN STANDARD REGISTER
                          FFAF'  30  004E   111 	BSBW	SYM$JOBINSQUE		; QUEUE THE JOB TO THE NEW DEVICE
                                     0051   112 					; NOTE:R6 IS SET TO ADDRESS OF QUEUE
                                     0051   113 					; HEADER INTO WHICH THE JOB WAS MERGED.
                          DD     11  0051   114 	BRB	30$			; REQUEST SEARCH FOR NEXT
                                     0053   115 80$:	SETSTAT	NORMAL			; SET GOOD FINAL STATUS
                                 04  0056   116 90$:	RET				; BACK TO TOP LEVEL
