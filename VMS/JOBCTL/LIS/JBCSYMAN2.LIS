V01             ALLOCATE/DEALOCATE A DEVICE                                                                                      (6)

                                     00D6   269 	.SBTTL	ALLOCATE/DEALOCATE A DEVICE
                                     00D6   270 ;++
                                     00D6   271 ; FUNCTIONAL DESCRIPTION:
                                     00D6   272 ;
                                     00D6   273 ;	THIS ROUTINE IS CALLED TO ALLOCATE OR DEALLOCATE A DEVICE
                                     00D6   274 ;	FOR WHICH A QUEUE HAS BEEN STARTED OR STOPPED.
                                     00D6   275 ;
                                     00D6   276 ; CALLING SEQUENCE:
                                     00D6   277 ;
                                     00D6   278 ;	BSB/JSB	SYM$ALLODEAL			;
                                     00D6   279 ;
                                     00D6   280 ; INPUT PARAMETERS:
                                     00D6   281 ;
                                     00D6   282 ;	R6 CONTAINS THE ADDRESS OF THE PROPER DEVICE QUEUE
                                     00D6   283 ;	R0 IS ALLOCATE/DEALLOCATE FLAG WHERE TRUE=ALLOCATE
                                     00D6   284 ;
                                     00D6   285 ; IMPLICIT INPUTS:
                                     00D6   286 ;
                                     00D6   287 ;	R10 HAS ADDRESS OF THE QUEUE FILE
                                     00D6   288 ;
                                     00D6   289 ; OUTPUT PARAMETERS:		NONE
                                     00D6   290 ;
                                     00D6   291 ; COMPLETION CODES:
                                     00D6   292 ;
                                     00D6   293 ;	R0 CONTAINS THE FINAL STATUS OF THE SYSTEM SERVICE
                                     00D6   294 ;
                                     00D6   295 ; SIDE EFFECTS:
                                     00D6   296 ;
                                     00D6   297 ;	R2,R3 ARE USED BY THIS ROUTINE
                                     00D6   298 ;
                                     00D6   299 ;--
                                     00D6   300 
                                     00D6   301 SYM$ALLODEAL::					;
            53         10 A6     9E  00D6   302 	MOVAB	SMQ$T_NAME(R6),R3	; ADDRESS OF THE DEVICE NAME
            52            63     9A  00DA   303 	MOVZBL	(R3),R2			; GET LENGTH OF DEVICE NAME
            63         5F 8F     90  00DD   304 	MOVB	#^A/_/,(R3)		; DISABLE LOGICAL NAME SUBSTITUTION
                          0C     BB  00E1   305 	PUSHR	#^M<R2,R3>		; FORM DESCRIPTOR FOR DEVICE NAME
                          6E     D6  00E3   306 	INCL	(SP)			; ADJUST LENGTH FOR THE UNDER SCORE
            51            5E     D0  00E5   307 	MOVL	SP,R1			; ADDRESS OF NAME DESCRIPTOR
            11            50     E9  00E8   308 	BLBC	R0,10$			; BR IF REQUEST IS DEALLOCATE
                                     00EB   309 	$ALLOC_S  DEVNAM=(R1)		; ALLOCATE THE DEVICE
                          0B     11  00FA   310 	BRB	20$			;
                                     00FC   311 10$:	$DALLOC_S DEVNAM=(R1)		; DEALLOCATE THE DEVICE
                          8E     7C  0107   312 20$:	CLRQ	(SP)+			; REMOVE DEVICE NAME DESCRIPTOR
            63            52     90  0109   313 	MOVB	R2,(R3)			; RESET DEVICE NAME STRING LENGTH
                                 05  010C   314 	RSB
JBCSYMAN2       - SYMBIONT MANAGER MAIN ROUTINE                  22-AUG-1978 00:50:07   VAX-11 MACRO X0.3-11               Page  10
