V01             REDIRECT QUEUE CONTENTS                                                                                          (3)

                                     0057   118 	.SBTTL	REDIRECT QUEUE CONTENTS
                                     0057   119 ;++
                                     0057   120 ; FUNCTIONAL DESCRIPTION:
                                     0057   121 ;
                                     0057   122 ;	THIS ROUTINE IS CALLED TO HAVE THE CONTENTS OF A QUEUE
                                     0057   123 ;	REDIRECTED TO ANOTHER QUEUE FOR PROCESSING.
                                     0057   124 ;
                                     0057   125 ; CALLING SEQUENCE:
                                     0057   126 ;
                                     0057   127 ;	ENTERED FROM SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     0057   128 ;
                                     0057   129 ; INPUT PARAMETERS:
                                     0057   130 ;
                                     0057   131 ;	R5 POINTS AT THE NAME OF THE QUEUE TO REDIRECT TO
                                     0057   132 ;	R6 CONTAINS THE ADDRESS OF THE QUEUE TO BE REDIRECTED.
                                     0057   133 ;
                                     0057   134 ;	IF THE TARGET QUEUE NAME IS NULL, THIS IS A REDIRECT OF
                                     0057   135 ;	A QUEUE BACK TO ITSELF.
                                     0057   136 ;
                                     0057   137 ; IMPLICIT INPUTS:
                                     0057   138 ;
                                     0057   139 ;	R9 HAS THE ADDRESS OF THE SYMBIONT MANAGER RECORD
                                     0057   140 ;	R10 HAS THE BASE ADDRESS OF THE QUEUE FILE
                                     0057   141 ;	R11 HAS THE ADDRESS OF THE JOB CONTROLLER DATA BLOCK
                                     0057   142 ;
                                     0057   143 ; OUTPUT PARAMETERS:
                                     0057   144 ;
                                     0057   145 ;	THE REDIRECTION IS EFFECTED IF THE OPERATION IS PROPERLY REQUESTED.
                                     0057   146 ;--
                                     0057   147 	PURE_SECTION
                                     0057   148  
                                     0057   149 SYM$REDIRECT::				; REDIRECT A QUEUE
         08 A6            01     E0  0057   150 	BBS	#SMQ$V_DETJOB,SMQ$B_FLAGS(R6),50$ ; BR IF BATCH QUEUE-CAN'T DO!
                          30         005B       
                          65     95  005C   151 	TSTB	(R5)			; IS SECOND NAME NULL?
                          24     13  005E   152 	BEQL	20$			; BR IF YES-GO UNDO A REDIRECT
         0A A6            04     E0  0060   153 	BBS	#SMQ$V_REDIRECT,SMQ$B_CHAR(R6),50$ ; ONE LEVEL OF REDIRECTION
                          27         0064       
            58            56     D0  0065   154 	MOVL	R6,R8			; SAVE ADDRESS OF TARGET QUEUE
                          FF95'  30  0068   155 	BSBW	SYM$FINDQUEUE		; LOCATE THE OTHER QUEUE
            1D            50     E9  006B   156 	BLBC	R0,40$			; BR IF SEARCH FOR QUEUE FAILED
            58            56     D1  006E   157 	CMPL	R6,R8			; TRYING TO REDIRECT QUEUE TO ITSELF?
                          19     13  0071   158 	BEQL	50$			; BR IF YES-THAT IS NOT ALLOWED
         08 A6            01     E0  0073   159 	BBS	#SMQ$V_DETJOB,SMQ$B_FLAGS(R6),50$ ; BR IF BATCH QUEUE-CAN'T DO!
                          14         0077       
                          16     10  0078   160 	BSBB	60$			; CHECK THAT QUEUE IS STOPPED
            58            5A     C3  007A   161 	SUBL3	R10,R8,SMQ$L_ASNSCT(R6) ; SET INDEX TO ACTUAL QUEUE TO USE
                       0C A6         007D       
                                     007F   162 	SETBIT	SMQ$V_REDIRECT,SMQ$B_CHAR(R6) ; MARK QUEUE REDIRECTED
                                 04  0083   163 	RET				;
                                     0084   164 ;
                                     0084   165 ; SECOND QUEUE NAME IS NULL - UNDO REDIRECT
                                     0084   166 ;
                          0A     10  0084   167 20$:	BSBB	60$			; VERIFY QUEUE IS STOPPED
         0A A6            04     E5  0086   168 	BBCC	#SMQ$V_REDIRECT,SMQ$B_CHAR(R6),50$ ; BR IF NOT REDIRECTED
                          01         008A       
                                 04  008B   169 40$:	RET				;
JBCSYMAN2       - SYMBIONT MANAGER MAIN ROUTINE                  22-AUG-1978 00:50:07   VAX-11 MACRO X0.3-11               Page   5
V01             REDIRECT QUEUE CONTENTS                                                                                          (3)

                                     008C   170  
                                     008C   171 50$:	SETSTAT	SMINVOPR		; SET STATUS INVALID OPERATION
                                 04  008F   172 	RET				;
                                     0090   173 ;
                                     0090   174 ; LOCAL SUBROUTINE TO VERIFY THAT THE TARGET QUEUE IS STOPPED
                                     0090   175 ;
                                     0090   176  
                          66     D5  0090   177 60$:	TSTL	SMQ$L_CURRENT(R6)	; JOB RUNNING?
                          06     12  0092   178 	BNEQ	70$			; BR IF JOB IS PRINTING
         08 A6            03     E1  0094   179 	BBC	#SMQ$V_STOPPED,SMQ$B_FLAGS(R6),70$ ; QUEUE STOPPED EXPLICITLY
                          01         0098       
                                 05  0099   180 	RSB				;
                                     009A   181 70$:	SETSTAT	QUENOSTOP		; ASSUME QUEUE IS NOT STOPPED
                                 04  009D   182 	RET				;
