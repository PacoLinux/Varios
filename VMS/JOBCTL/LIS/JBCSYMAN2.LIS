V01             STOP PRINTING OF DEVICE                                                                                          (5)

                                     009F   213 	.SBTTL	STOP PRINTING OF DEVICE
                                     009F   214 ;++
                                     009F   215 ; FUNCTIONAL DESCRIPTION:
                                     009F   216 ;
                                     009F   217 ;	THIS ROUTINE IS ENTERED WHEN A REQUEST HAS BEEN MADE TO STOP
                                     009F   218 ;	PRINTING ON A DEVICE.  THE QUEUE IS MARKED STOPPED AND ANY
                                     009F   219 ;	FILES QUEUED TO THAT SPECIFIC DEVICE ARE INSERTED IN THE
                                     009F   220 ;	DEVICE HOLD QUEUE.
                                     009F   221 ;
                                     009F   222 ; CALLING SEQUENCE:
                                     009F   223 ;
                                     009F   224 ;	ENTERED FROM SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     009F   225 ;
                                     009F   226 ; INPUT PARAMETERS:
                                     009F   227 ;
                                     009F   228 ;	R6 CONTAINS THE ADDRESS OF THE PROPER DEVICE QUEUE
                                     009F   229 ;	R9 HAS ADDRESS OF SYMBIONT MANAGER MESSAGE
                                     009F   230 ;
                                     009F   231 ; IMPLICIT INPUTS:
                                     009F   232 ;
                                     009F   233 ;	R10 HAS ADDRESS OF THE QUEUE FILE
                                     009F   234 ;
                                     009F   235 ; OUTPUT PARAMETERS:
                                     009F   236 ;
                                     009F   237 ;	IF THE QUEUE EXISTS IT IS STOPPED
                                     009F   238 ;
                                     009F   239 ; COMPLETION CODES:
                                     009F   240 ;
                                     009F   241 ;	R0 IS RETURN WITH THE FINAL STATUS OF THE REQUEST.
                                     009F   242 ;
                                     009F   243 ; SIDE EFFECTS:		NONE
                                     009F   244 ;--
                                     009F   245 
                                     009F   246 SYM$STOP::
                                 E2  009F   247 	BBSS	#SMQ$V_STOPPED,-	; DON'T STOP WHATS STOPPED
         08 A6            03         00A0   248 		SMQ$B_FLAGS(R6),40$	;
                          2E         00A3       
                     0000'CF     9F  00A4   249 	PUSHAB	W^SYM$FINDACTJOB	; INITIALIZE COROUTINE ADRESSS
                          9E     16  00A8   250 10$:	JSB	@(SP)+			; COROUTINE LINKAGE TO SEARCH QUEUE
            0E            50     E9  00AA   251 	BLBC	R0,30$			; BR IF NO MORE FOUND
                          9E     16  00AD   252 	JSB	@(SP)+			; ASK TO REMOVE THIS JOB
                                 D0  00AF   253 	MOVL	SMQ$L_HOLDLST(R6),-	; COPY PREVIOUS FIRST FILE IN HOLD LIST
         08 A2         04 A6         00B0   254 		SJH$L_NEXTJOB(R2)	; AS NEXT JOB AFTER ONE JUST DEQUEUED
            52            5A     C3  00B4   255 	SUBL3	R10,R2,SMQ$L_HOLDLST(R6) ; SET THIS FILE AS FIRST IN HOLD LIST
                       04 A6         00B7       
                          ED     11  00B9   256 	BRB	10$			; SEE IF GOT ONE
                                 93  00BB   257 30$:	BITB	#<SMQ$M_DETJOB!SMQ$M_GENDEV>,- ; CK IF QUEUE IS JOB OR GENERIC
         08 A6            12         00BC   258 		SMQ$B_FLAGS(R6)		; DEVICE QUEUE.
                          11     12  00BF   259 	BNEQ	40$			; BR IF EITHER-NO SYMBIONT INVOLVED
         0A A6            04     E0  00C1   260 	BBS	#SMQ$V_REDIRECT,SMQ$B_CHAR(R6),40$ ; LIKEWISE IF REDIRECTED
                          0C         00C5       
                          50     D4  00C6   261 	CLRL	R0			; SET REQUEST TO DEALLOCATE
                          0C     10  00C8   262 	BSBB	SYM$ALLODEAL		; ALLOCATE/DEALLOCATE DEVICE
                                     00CA   263 	SETBIT	#JBC_V_SYMINIREQ,-	; SET FLAG TO INITIATE A SYMBIONT, THAT
                                     00CA   264 		JCD_W_FLAGS(R11)	; CAUSE A DELETE IF A SYMBIONT IS IDLE
                       1D AB     97  00CF   265 	DECB	JCD_B_CURPDEV(R11)	; COUNT DOWN NUMBER OF ACTIVE DEVICES
                                     00D2   266 40$:	SETSTAT	NORMAL			; SET SUCESSFUL STATUS
