                                 11  0193   347 	.BYTE	MSG$_ABOOPR		; MESSAGE FOR SYMBIONT
                          1D     10  0194   348 195$:	BSBB	SYM$SNDASNSYM		; SENT TO ASSIGNED SYMBIONT
