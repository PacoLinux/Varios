                                     0154   297 	.SBTTL	ABORT CURRENT JOB
                                     0154   298 ;++
                                     0154   299 ; FUNCTIONAL DESCRIPTION:
                                     0154   300 ;
                                     0154   301 ;	THIS ROUTINE WILL SEND A MESSAGE TO A SYMBIONT INSTRUCTING
                                     0154   302 ;	THE SYMBIONT TO TERMINATE IS PRINTING ITS CURRENT FILE
                                     0154   303 ;
                                     0154   304 ; CALLING SEQUENCE:
                                     0154   305 ;
                                     0154   306 ;	ENTERED FROM SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     0154   307 ;
                                     0154   308 ; INPUT PARAMETERS:
                                     0154   309 ;
                                     0154   310 ;	R6 CONTAINS THE ADDRESS OF THE ASSOCIATED DEVICE QUEUE HEADER
                                     0154   311 ;	R9 HAS THE ADDRESS OF THE SYMBIONT MANAGER RECORD
                                     0154   312 ;	R10 HAS THE BASE ADDRESS OF THE QUEUE FILE
                                     0154   313 ;	R11 HAS THE ADDRESS OF THE JOB CONTROLLER DATA BLOCK
                                     0154   314 ;
                                     0154   315 ; OUTPUT PARAMETERS:
                                     0154   316 ;
                                     0154   317 ;	SYMBIONT IS INSTRUCTED TO STOP PRINTING
                                     0154   318 ;
                                     0154   319 ; COMPLETION CODES:
                                     0154   320 ;
                                     0154   321 ;	R0 IS RETURN WITH THE FINAL STATUS OF THE REQUEST.
                                     0154   322 ;
                                     0154   323 ; SIDE EFFECTS:		NONE
                                     0154   324 ;--
                                     0154   325 
                                     0154   326 SYM$ABORT::				; ABORT THE CURRENT JOB
            57            66     D0  0154   327 	MOVL	SMQ$L_CURRENT(R6),R7	; GET OFFSET TO CURRENT JOB
                          3F     13  0157   328 160$:	BEQL	200$			; BR IF NO CURRENT JOB
            57            5A     C0  0159   329 	ADDL	R10,R7			; FIND REAL ADDRESS OF JOB HEADER
         08 A6            01     E1  015C   330 	BBC	#SMQ$V_DETJOB,SMQ$B_FLAGS(R6),190$ ; BR IF PRINT QUEUE
                          23         0160       
         04 A7            65     B1  0161   331 	CMPW	(R5),SJH$W_JOBSEQ(R7)	; SEQUENCE NUMBER MATCH?
                          06     13  0165   332 	BEQL	170$			; BR IF YES-FOUND THE JOB
            57         08 A7     D0  0167   333 	MOVL	SJH$L_NEXTJOB(R7),R7	; GET INDEX OF NEXT JOB
                          EA     11  016B   334 	BRB	160$			; CHECK THIS ONE OUT
                          FE90'  30  016D   335 170$:	BSBW	SYM$CKREQPRV		; CHECK IF THIS GUY CAN ABORT THIS JOB
                                     0170   336 	$DELPRC_S PIDADR=SJH$L_PROCID(R7) ; DELETE THE JOB
            04            50     E9  017C   337 	BLBC	R0,180$			; BR IF SERVICE WAS UNSUCCESSFUL
                                     017F   338 	SETBIT	SJH$V_JOBABORT,SJH$B_FLAGS(R7) ; REMEMBER ABORT WAS DONE
                                 04  0183   339 180$:	RET
                                     0184   340  
                          FE79'  30  0184   341 190$:	BSBW	SYM$CKREQPRV		; VERIFY THAT THE REQUESTOR CAN DO THIS
                          FE76'  30  0187   342 	BSBW	SYM$CNVOPTABO		; CHECK FOR ABORT OPTIONS
            0E            50     E9  018A   343 	BLBC	R0,205$			; BR IF INVALID OPTION
