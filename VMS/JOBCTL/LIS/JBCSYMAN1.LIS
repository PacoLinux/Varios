V01             PAUSE OPERATION ON QUEUE                                                                                         (4)

                                     019C   354 	.SBTTL	PAUSE OPERATION ON QUEUE
                                     019C   355 ;++
                                     019C   356 ; FUNCTIONAL DESCRIPTION:
                                     019C   357 ;
                                     019C   358 ;	THIS ROUTINE IS ENTERED WHEN IS IS NECESSARY TO SUSPEND
                                     019C   359 ;	THE OPERATION OF A SPECIFIC QUEUE FOR A WHILE
                                     019C   360 ;
                                     019C   361 ; CALLING SEQUENCE:
                                     019C   362 ;
                                     019C   363 ;	ENTERED FROM THE SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     019C   364 ;
                                     019C   365 ; INPUT PARAMETERS:
                                     019C   366 ;
                                     019C   367 ;	R5 POINTS AT NEXT BYTE TO PROCESS IN THE MESSAGE
                                     019C   368 ;	R6 CONTAINS THE ADDRESS OF THE ASSOCIATED QUEUE HEADER
                                     019C   369 ;	R9 CONTAINS THE ADDRESS OF THE SYMBIONT MANAGER MESSAGE
                                     019C   370 ;
                                     019C   371 ; IMPLICIT INPUTS:
                                     019C   372 ;
                                     019C   373 ;	R10 HAS THE BASE ADDRESS OF THE PRINT QUEUE FILE
                                     019C   374 ;	R11 HAS THE JOB CONTROLLER DATA BLOCK
                                     019C   375 ;
                                     019C   376 ; OUTPUT PARAMETERS:	NONE
                                     019C   377 ;
                                     019C   378 ; COMPLETION CODES:
                                     019C   379 ;
                                     019C   380 ;	R0 IS RETURN WITH THE FINAL STATUS OF THE REQUEST.
                                     019C   381 ;
                                     019C   382 ; SIDE EFFECTS:		NONE
                                     019C   383 ;--
                                     019C   384 
                                     019C   385 SYM$PAUSE::				; SUSPEND OPERATION ON A QUEUE
         08 A6            12     93  019C   386 	BITB	#SMQ$M_GENDEV!SMQ$M_DETJOB,SMQ$B_FLAGS(R6) ; CK IF QUEUE CAN BE PAUSED
                          0E     12  01A0   387 	BNEQ	210$			; BR IF NO - TRY A STOP
         0A A6            04     E0  01A2   388 	BBS	#SMQ$V_REDIRECT,SMQ$B_CHAR(R6),210$ ; ALSO STOP REDIRECTED QUEUES
                          09         01A6       
         08 A6            02     E2  01A7   389 	BBSS	#SMQ$V_PAUSED,	-	; SET PAUSED FLAG - AND BR IF ALREADY-
                          1B         01AB       
                                     01AC   390 		SMQ$B_FLAGS(R6),230$	; PAUSED
                          05     10  01AC   391 	BSBB	SYM$SNDASNSYM		; SEND MESSAGE TO ASSIGNED SYMBIONT
                                 07  01AE   392 	.BYTE	SCT_K_SUSPND		; NEXT STATE IS SUSPENDED
                                 12  01AF   393 	.BYTE	MSG$_SUSOPR		; MESSAGE CODE
                          FE4D'  31  01B0   394 210$:	BRW	SYM$STOP		; 
JBCSYMAN1       - SYMBIONT MANAGER MAIN ROUTINE                  22-AUG-1978 00:49:18   VAX-11 MACRO X0.3-11               Page  11
V01             SEND TO ASSIGNED SYMBIONT                                                                                        (4)

                                     01B3   396 	.SBTTL	SEND TO ASSIGNED SYMBIONT
                                     01B3   397 ;++
                                     01B3   398 ; FUNCTIONAL DESCRIPTION:
                                     01B3   399 ;
                                     01B3   400 ;	THIS ROUTINE IS CALLED TO SEND A MESSAGE TO THE SYMBIONT CURRENTLY
                                     01B3   401 ;	ASSIGNED TO THE DEVICE WHOSE QUEUE HEADER IS IN R6, AND TO
                                     01B3   402 ;	SET THE STATE OF THE SYMBIONT IN ACCORDANCE WITH THE MESSAGE.
                                     01B3   403 ;
                                     01B3   404 ; CALLING SEQUENCE:
                                     01B3   405 ;
                                     01B3   406 ;	JSB/BSB	SYM$SNDASNSYM		;
                                     01B3   407 ;
                                     01B3   408 ; INPUT PARAMETERS:
                                     01B3   409 ;
                                     01B3   410 ;	NEW STATE FOLLOWED BY MESSAGE CODE IN LINE AFTER CALL
                                     01B3   411 ;	R6 CONTAINS THE PROPER DEVICE QUEUE ADDRESS
                                     01B3   412 ;	R10 HAS BASE ADDRESS OF PRINT QUEUE FILE
                                     01B3   413 ;
                                     01B3   414 ; OUTPUT PARAMETERS:
                                     01B3   415 ;
                                     01B3   416 ;	MESSAGE IS SENT, THE STATE IS SET, AND A RETURN IS TAKEN
                                     01B3   417 ;	TO SYMBIONT MANAGER DISPATCHER.
                                     01B3   418 ;
                                     01B3   419 ; COMPLETION CODES:
                                     01B3   420 ;
                                     01B3   421 ;	R0 IS SET WITH RESULTANT STATUS
                                     01B3   422 ;
                                     01B3   423 ; SIDE EFFECTS:		NONE
                                     01B3   424 ;--
                                     01B3   425 
                                     01B3   426 SYM$SNDASNSYM::				; SEND TO ASSIGNED SYMBIONT
                          66     D5  01B3   427 	TSTL	SMQ$L_CURRENT(R6)	; ANYTHING GOING?
                          10     13  01B5   428 	BEQL	230$			; BR IF NO
            55         0C A6     D0  01B7   429 	MOVL	SMQ$L_ASNSCT(R6),R5	; SET ADR OF ASSIGNED SYMBIONT CONTROL
                          01     BA  01BB   430 	POPR	#^M<R0>			; GET ADDRESS OF MESSAGE
         08 A5            80     90  01BD   431 	MOVB	(R0)+,SCT_B_STATE(R5)	; SET STATE OF SYMBIONT
            50            60     9A  01C1   432 	MOVZBL	(R0),R0			; GET MESSAGE CODE
                          FE39'  30  01C4   433 220$:	BSBW	SYM$SND2SMB		; SEND THAT TO THE SYMBIONT
                                     01C7   434 230$:	SETSTAT	NORMAL			; STATUS OF OPERATION
                                 04  01CA   435 240$:	RET				; BACK TO THE DISPATCHER
                                     01CB   436 	.DSABL	LSB
                                     01CB   437 	.END				;
