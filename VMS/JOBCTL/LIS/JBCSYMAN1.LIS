                                     0000    59 	.SBTTL	INITIALIZE DEVICE QUEUE
                                     0000    60 ;++
                                     0000    61 ; FUNCTIONAL DESCRIPTION:
                                     0000    62 ;
                                     0000    63 ; THIS ROUTINE CREATES A QUEUE FOR THE DEVICE SPECIFIED
                                     0000    64 ; IF THE QUEUE ALREADY EXISTS, IT IS INITIALIZED EMPTY AND STOPPED
                                     0000    65 ;
                                     0000    66 ;
                                     0000    67 ; CALLING SEQUENCE:
                                     0000    68 ;
                                     0000    69 ;	ENTERED FROM SYMBIONT MANAGER DISPATCHER VIA CASE FOLLOWING A CALL
                                     0000    70 ;
                                     0000    71 ; INPUT PARAMETERS:
                                     0000    72 ;
                                     0000    73 ;	R0 IS SET FROM THE RESULT OF ATTEMPT TO FIND THE DEVICE QUEUE
                                     0000    74 ;	R5 POINTS AT DEVICE NAME IF NOT FOUND,JUST PAST IT IF FOUND
                                     0000    75 ;	R6 CONTAINS DEVICE QUEUE ADDRESS IN QUEUE FILE
                                     0000    76 ;	R9 CONTAINS ADDRESS OF SYMBIONT MANAGER MESSAGE PACKET
                                     0000    77 ;
                                     0000    78 ;
                                     0000    79 ; OUTPUT PARAMETERS:
                                     0000    80 ;
                                     0000    81 ;	THE QUEUE IS INITIALIZED, IF IT EXISTED, IT IS EMPTIED.
                                     0000    82 ;
                                     0000    83 ; COMPLETION CODES:
                                     0000    84 ;
                                     0000    85 ;	R0 IS RETURN WITH THE STATUS OF THE REQUEST
                                     0000    86 ;
                                     0000    87 ; SIDE EFFECTS:		NONE
                                     0000    88 ;--
                                     0000    89 	PURE_SECTION
                                     0000    90  
                                     0000    91 	.ENABL	LSB
