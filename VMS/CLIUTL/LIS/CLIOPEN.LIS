CLIOPEN         - COMMAND LANGUAGE INTERFACE OPEN ROUTINE        21-AUG-1978 18:55:33   VAX-11 MACRO X0.3-11               Page   2
V01             OPEN FIRST INPUT FILE                                                                                            (2)

                                     0000    59 	.SBTTL	OPEN FIRST INPUT FILE
                                     0000    60 ;++
                                     0000    61 ; FUNCTIONAL DESCRIPTION:
                                     0000    62 ;
                                     0000    63 ;	THIS ROUTINE IS CALLED OPEN THE FIRST FILE AND SET UP
                                     0000    64 ;	AS NECESSARY FOR WILD CARD PROCESSING.
                                     0000    65 ;
                                     0000    66 ; CALLING SEQUENCE:
                                     0000    67 ;
                                     0000    68 ;	SAME IS IF THE CALLER WERE CALLING OPEN DIRECTLY
                                     0000    69 ;
                                     0000    70 ; INPUT PARAMETERS:
                                     0000    71 ;
                                     0000    72 ;	4(AP) IS THE FAB ADDRESS
                                     0000    73 ;
                                     0000    74 ; IMPLICIT INPUTS:
                                     0000    75 ;
                                     0000    76 ;	FAB HAS A NAME BLOCK, WITH EXPANDED NAME STRING AND RESULTANT 
                                     0000    77 ;	NAME STRING BUFFERS AS UNIQUE MEMORY LOCATION. THE RELATED FILE
                                     0000    78 ;	NAME BLOCK HAS ADDRESS OF EXPANDED STRING AS RESULTANT NAME 
                                     0000    79 ;	ADDRESS IN ORDER TO GET WILD CARD DEFAULTING CORRECT.
                                     0000    80 ;
                                     0000    81 ; OUTPUT PARAMETERS:
                                     0000    82 ;
                                     0000    83 ;	THE FILE IS OPENED AND WILD CARD PROCESSING IS INITED.
                                     0000    84 ;
                                     0000    85 ; IMPLICIT OUTPUTS:
                                     0000    86 ;
                                     0000    87 ;	NONE
                                     0000    88 ;
                                     0000    89 ; COMPLETION CODES:
                                     0000    90 ;
                                     0000    91 ;	AS SET BY RMS
                                     0000    92 ;
                                     0000    93 ; SIDE EFFECTS:
                                     0000    94 ;
                                     0000    95 ;	DEFAULT NAME STRING ADDRESS IS ZEROED INORDER TO USE THE
                                     0000    96 ;	TYPE FROM THE PREVIOUS OPEN AS DEFUALT
                                     0000    97 ;
                                     0000    98 ;--
                                     0000    99  
                                     0000   100 	.ENABL	LSB			;
                                     0000   101 	PURE_SECTION
                                     0000   102  
                               003C' 0000   103 	.ENTRY	CLI$OPENFILE,^M<R2,R3,R4,R5>
                                     0002   104  
                          0062   30  0002   105 	BSBW	SETUP			; SET COMMON REGISTER VALUES
                       03 A3     94  0005   106 	CLRB	NAM$B_RSL(R3)		; ZERO LENGTH OF RESULTANT STRING
                       0B A3     94  0008   107 	CLRB	NAM$B_ESL(R3)		; SET TO ZERO TO TELL IF ONE COMES BACK
                                     000B   108 	CLRBIT	FAB$V_NAM,FAB$L_FOP(R2)	; TELL RMS TO IGNORE NAME BLOCK FOR NOW
   00000000'9F            6C     FA  0010   109 	CALLG	(AP),@#SYS$PARSE	; PARSE THE FILE SPEC
            2D            50     E9  0017   110 	BLBC	R0,80$			; BR IF NO PARSE POSSIBLE
                                     001A   111 	SETBIT	FAB$V_NAM,FAB$L_FOP(R2)	; NOW USE NAME BLOCKED JUST FILLED IN
                                 90  001F   112 	MOVB	NAM$B_ESL(R3),-		; GET LENGTH OF PREVIOUS EXPANDED STRING
         03 A4         0B A3         0020   113 		NAM$B_RSL(R4)		; AND SET AS LENGTH OF RELATED FILE
                       35 A2     94  0024   114 	CLRB	FAB$B_DNS(R2)		; DON'T USE DEFAULTS ANY MORE
         34 A3            08     E1  0027   115 	BBC	#NAM$V_WILDCARD,NAM$L_FNB(R3),60$ ; BR IF NO WILD CARDS SEEN
CLIOPEN         - COMMAND LANGUAGE INTERFACE OPEN ROUTINE        21-AUG-1978 18:55:33   VAX-11 MACRO X0.3-11               Page   3
V01             OPEN FIRST INPUT FILE                                                                                            (2)

                          0A         002B       
   00000000'9F            6C     FA  002C   116 40$:	CALLG	(AP),@#SYS$SEARCH	; SEARCH FOR THE NEXT FILE
            0A            50     E9  0033   117 	BLBC	R0,70$			; ANOTHER BAD ERROR
   00000000'9F            6C     FA  0036   118 60$:	CALLG	(AP),@#SYS$OPEN		; OPEN THE FILE
            0C            50     E8  003D   119 	BLBS	R0,90$			; BR IF OPEN OK!
            50       0000'8F     B1  0040   120 70$:	CMPW	#<RMS$_FNF&^XFFFF>,R0	; WAS ERROR FILE NOT FOUND
                          05     12  0045   121 	BNEQ	90$			; MAY CONTIUE WILD CARDS
                                     0047   122 80$:	CLRBIT	NAM$V_WILDCARD,NAM$L_FNB(R3) ; MAKE SURE WILD CARDS STOP
                                 04  004C   123 90$:	RET				; RETURN TO CALLER
CLIOPEN         - COMMAND LANGUAGE INTERFACE OPEN ROUTINE        21-AUG-1978 18:55:33   VAX-11 MACRO X0.3-11               Page   4
V01             OPEN NEXT INPUT FILE                                                                                             (3)

                                     004D   125 	.SBTTL	OPEN NEXT INPUT FILE
                                     004D   126 ;++
                                     004D   127 ; FUNCTIONAL DESCRIPTION:
                                     004D   128 ;
                                     004D   129 ;	THIS ROUTINE IS CALLED OPEN THE NEXT FILE IN A
                                     004D   130 ;	WILD CARD PROCESSING SEQUENCE.
                                     004D   131 ;
                                     004D   132 ; CALLING SEQUENCE:
                                     004D   133 ;
                                     004D   134 ;	SAME IS IF THE CALLER WERE CALLING OPEN DIRECTLY
                                     004D   135 ;
                                     004D   136 ; INPUT PARAMETERS:
                                     004D   137 ;
                                     004D   138 ;	4(AP) IS THE FAB ADDRESS
                                     004D   139 ;
                                     004D   140 ; IMPLICIT INPUTS:
                                     004D   141 ;
                                     004D   142 ;	FAB HAS A NAME BLOCK, WITH:
                                     004D   143 ;
                                     004D   144 ;		1) EXPANDED NAME STRING BUFFER
                                     004D   145 ;		2) RESULTANT NAME STRING BUFFER
                                     004D   146 ;		3) A DEFAULT NAME STRING BUFFER
                                     004D   147 ;
                                     004D   148 ; OUTPUT PARAMETERS:
                                     004D   149 ;
                                     004D   150 ;	THE CURRENTLY OPEN FILE IS CLOSED AND THE NEXT
                                     004D   151 ;	FILE IS OPENED AND WILD CARD PROCESSING IS UPDATED.
                                     004D   152 ;
                                     004D   153 ; IMPLICIT OUTPUTS:
                                     004D   154 ;
                                     004D   155 ;	NONE
                                     004D   156 ;
                                     004D   157 ; COMPLETION CODES:
                                     004D   158 ;
                                     004D   159 ;	AS SET BY RMS
                                     004D   160 ;
                                     004D   161 ; SIDE EFFECTS:
                                     004D   162 ;
                                     004D   163 ;	NONE
                                     004D   164 ;
                                     004D   165 ;--
                               003C' 004D   166 	.ENTRY	CLI$OPENNEXT,^M<R2,R3,R4,R5>
                                     004F   167  
                          16     10  004F   168 	BSBB	SETUP			; SET UP COMMON REGISTER VALUES
                                     0051   169 	$CLOSE	(R2)			; CLOSE THE INPUT
            50   00000000'8F     D0  005A   170 	MOVL	#RMS$_NMF,R0		; ASSUME WARNING STATUS OF NO MORE FILES
                                 E0  0061   171 	BBS	#NAM$V_WILDCARD,-	; BR IF WILD CARDS SEEN DURING
         34 A3            08         0062   172 		NAM$L_FNB(R3),40$	; AND SEARCH FOR NEXT FILE
                          C6         0065       
                                 04  0066   173 	RET				;
                                     0067   174  
                                     0067   175 ;
                                     0067   176 ; SETUP - SET UP COMMON REGISTER VALUES
                                     0067   177 ;
                                     0067   178  
            52         04 BC     9E  0067   179 SETUP:	MOVAB	@4(AP),R2		; GET FAB ADDRESS
            53         28 B2     9E  006B   180 	MOVAB	@FAB$L_NAM(R2),R3	; AND THE NAME BLOCK
CLIOPEN         - COMMAND LANGUAGE INTERFACE OPEN ROUTINE        21-AUG-1978 18:55:33   VAX-11 MACRO X0.3-11               Page   5
V01             OPEN NEXT INPUT FILE                                                                                             (3)

            54         10 B3     9E  006F   181 	MOVAB	@NAM$L_RLF(R3),R4	; SET ADDRESS OF RELATED FILE NAME BLOCK
                                 05  0073   182 	RSB
                                     0074   183  
                                     0074   184 	.END
