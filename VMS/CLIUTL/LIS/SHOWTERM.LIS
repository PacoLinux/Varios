                                     0000   176 ; SHOW$TERM - SHOW TERMINAL CGHARACTERISTICS
                                     0000   177 ;
                                     0000   178 ; FUNCTIONAL DESCRIPTION:
                                     0000   179 ;
                                     0000   180 ; THIS ROUTINE SHOWS THE CHARACTERISTICS OF A SPECIFIED TERMINAL.
                                     0000   181 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SHOW COMMAND PROCESSOR.
                                     0000   182 ;
                                     0000   183 ; INPUTS:
                                     0000   184 ;
                                     0000   185 ;	THE CLI DATA BASE IS SET UP. 
                                     0000   186 ;
                                     0000   187 ; OUTPUTS:
                                     0000   188 ;
                                     0000   189 ;	R0 = STATUS OF THE OPERATION
                                     0000   190 ;--
                                     0000   191 SHOW$TERM::				; SHOW TERMINAL COMMAND
                               03FC  0000   192 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>
            56       00FD'CF     9E  0002   193 	MOVAB	W^TERMNAME,R6		; ADDRESS QUAL DESC
                                     0007   194 	CALLBAK	(R6)			; CALL CLI FOR TERMINAL NAME
            56         08 A6     9E  0015   195 	MOVAB	CLI$Q_RQDESC(R6),R6	; ADDRESS NAME DESC
                          66     D5  0019   196 	TSTL	(R6)			; NAME PRESENT?
                          47     12  001B   197 	BNEQ	20$			; IF NEQ THEN ASSIGN THAT NAME
                                     001D   198 ;
                                     001D   199 ; DEFUALT NAME IS "SYS$COMMAND"
                                     001D   200 ;
            55            03     9A  001D   201 	MOVZBL	#3,R5			; SET UP TRANSLATE COUNTER
            56       04EF'CF     9E  0020   202 	MOVAB	W^SYS_COMMAND,R6	; ADDRESS DESC FOR ORIGINAL NAME
       0010'CF       0000'CF     7D  0025   203 10$:	MOVQ	W^SHOW$GQ_OUT_DSC,W^UTIL_DESC
                                     002C   204 	$TRNLOG_S	(R6),W^UTIL_DESC,W^UTIL_DESC; TRANSLATE NAME
            1E            50     E9  0043   205 	BLBC	R0,20$			; IF LOW CLEAR THEN DONE
       0000'8F            50     B1  0046   206 	CMPW	R0,#SS$_NOTRAN		; DONE?
                          17     13  004B   207 	BEQL	20$			; IF EQL THEN YES
            66       0010'CF     7D  004D   208 	MOVQ	W^UTIL_DESC,(R6)	; SET UP FOR NEXT TRANSLATION
            1B         04 B6     91  0052   209 	CMPB	@4(R6),#^X01B		; ESCAPE?
                          09     12  0056   210 	BNEQ	15$			; IF NEQ THEN CONTINUE
            66            04     C2  0058   211 	SUBL	#4,(R6)			; FOUND PROCESS PERM NAME
         04 A6            04     C0  005B   212 	ADDL	#4,4(R6)		;
                          03     11  005F   213 	BRB	20$			; 
            C1            55     F5  0061   214 15$:	SOBGTR	R5,10$			; CONTINUE UNTIL DONE
                                     0064   215 ;
                                     0064   216 ; GET DEVICE INFO IN CASE THE TERMINAL IS ASSIGNED TO SOMEONE ELSE
                                     0064   217 ;
                                     0064   218 ; THIS ALLOWS SOME INFORMATION TO BE PRINTED.
                                     0064   219 ;
                                     0064   220 20$:	$GETDEV_S	(R6),,W^SPOOL_DEV,,W^GET_CHAN_INFO; GET THE DEVICE INFO
       0000'CF            50     3C  0079   221 	MOVZWL	R0,W^SHOW$L_STATUS	; COPY CODE
            66            50     E9  007E   222 	BLBC	R0,40$			; BR IF ERROR - NO SUCH DEVICE
       0000'CF       0000'8F     3C  0081   223 	MOVZWL	#SS$_IVDEVNAM,W^SHOW$L_STATUS; ASSUME DEVICE NOT TERMINAL
       0020'CF            02     E1  0088   224 	BBC	#DEV$V_TRM,W^CHAN_INFO-4,40$; BR IF NOT TERMINAL
