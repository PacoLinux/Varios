                                     0000   300 ; SET$PRINT - SET PRINTER CHARACTERISTICS
                                     0000   400 ;
                                     0000   500 ; FUNCTIONAL DESCRIPTION:
                                     0000   600 ;
                                     0000   700 ; THIS ROUTINE SETS THE CHARACTERISTICS OF A SPECIFIED PRINTER.
                                     0000   800 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SET COMMAND PROCESSOR.
                                     0000   900 ;
                                     0000  1000 ; INPUTS:
                                     0000  1100 ;
                                     0000  1200 ;	THE CLI DATA BASE IS SET UP. 
                                     0000  1300 ;
                                     0000  1400 ; OUTPUTS:
                                     0000  1500 ;
                                     0000  1600 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     0000  1700 ;--
                                     0000  1800 SET$PRINT::				; SET PRINTER COMMAND
                               03FC  0000  1900 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>
            56       0060'CF     9E  0002  2000 	MOVAB	W^LP_NAME,R6		; ADDRESS QUAL DESC
                                     0007  2100 	CALLBAK	(R6)			; CALL CLI FOR PRINTER NAME
            56         08 A6     9E  0015  2200 	MOVAB	CLI$Q_RQDESC(R6),R6	; ADDRESS NAME DESC
                                     0019  2300 20$:	$ASSIGN_S	(R6),W^QIO+8	; ASSIGN CHANNEL TO DEVICE
            26            50     E9  0028  2400 	BLBC	R0,ERROR		; IF LOW CLEAR THEN ERROR AND EXIT
                                     002B  2500 	$GETCHN_S	W^QIO+8,,W^GET_CHAN_INFO
            0D            50     E9  0041  2600 	BLBC	R0,ERROR		; IF LOW CLEAR THEN ERROR
         43 8F       001C'CF     91  0044  2610 	CMPB	W^CLASS,#DC$_LP		; IS DEVICE A PRINTER?
                          0B     13  004A  2620 	BEQL	SET_CHAR		; IF EQL YES
            50       0000'8F     3C  004C  2630 	MOVZWL	#SS$_IVDEVNAM,R0	; ELSE INVALID DEVICE ERROR
                                     0051  2700 ;
                                     0051  2800 ; ERROR IN GETTING CHARACTERISTICS
                                     0051  2900 ;
       0000'CF            50     D0  0051  3000 ERROR:	MOVL	R0,W^SET$L_STATUS	; SAVE FOR EXIT STATUS
                                 04  0056  3100 	RET				; RETURN
                                     0057  3200 ;
                                     0057  3300 ; PROCESS DATA
                                     0057  3400 ;
                                     0057  3500 SET_CHAR:				; PROCESS DATA
                     0024'CF     7C  0057  3600 	CLRQ	W^MASK			; ZERO MASK AND ENABLES
       002B'CF            01     8E  005B  3700 	MNEGB	#1,W^ENABLE+3		; SET PAGE MAGIC
       0027'CF       0023'CF     90  0060  3800 	MOVB	W^CHAN_INFO+7,W^PAGESIZ	; SET UP OLD PAGE
       0038'CF       0000'8F     3C  0067  3900 	MOVZWL	#IO$_SETMODE,W^QIO+12	; ASSUME NO PRIVILEGE NEEDED
       0000'CF       0000'8F     3C  006E  4000 	MOVZWL	#SS$_NORMAL,W^SET$L_STATUS; ASSUME SUCCESS
                                     0075  4100 	CALLBAK	W^CHAR_QUAL
            26       0000'CF     E9  0085  4200 	BLBC	W^SET$L_STATUS,100$
       0020'CF       0028'CF     CA  008A  4300 	BICL	W^ENABLE,W^CHAN_INFO+4
       0020'CF       0024'CF     C8  0091  4400 	BISL	W^MASK,W^CHAN_INFO+4
   00000000'9F       002C'CF     FA  0098  4500 	CALLG	W^QIO,@#SYS$QIOW	; SET THE MODE OR CHARACTERISTICS
       0000'CF            50     B0  00A1  4600 	MOVW	R0,W^SET$L_STATUS	; SAVE STATUS
            07            50     E9  00A6  4700 	BLBC	R0,100$			; IF LOW CLEAR THEN ERROR
       0000'CF       0008'CF     3C  00A9  4800 	MOVZWL	W^IOSTATUS,W^SET$L_STATUS; SAVE STATUS
                                     00B0  4900 100$:	$DASSGN_S	W^QIO+8		; DEASSIGN THE PRINTER
                                 04  00BC  5000 	RET				; RETURN
                                     00BD  5100 ;
                                     00BD  5200 ; ACTION ROUTINES
                                     00BD  5300 ;
                                     00BD  5400 	.ENABL	LSB
