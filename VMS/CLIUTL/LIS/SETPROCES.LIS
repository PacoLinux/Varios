                                     0000  9200 ;++
                                     0000  9300 ; SET$PROCESS - SET PROCESS PARAMETERS
                                     0000  9400 ;
                                     0000  9500 ; FUNCTIONAL DESCRIPTION:
                                     0000  9600 ;
                                     0000  9700 ; THIS ROUTINE SETS A PROCESS'S:
                                     0000  9800 ;
                                     0000  9900 ;	PRIORITY
                                     0000 10000 ;	RESOURCE WAIT MODE
                                     0000 10100 ;	SWAP MODE
                                     0000 10200 ;
                                     0000 10300 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SET COMMAND PROCESSOR
                                     0000 10400 ;
                                     0000 10500 ; INPUTS:
                                     0000 10600 ;
                                     0000 10700 ;	THE CLI DATA BASE IS SET UP. 
                                     0000 10800 ;
                                     0000 10900 ; OUTPUTS:
                                     0000 11000 ;
                                     0000 11100 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     0000 11200 ;--
                                     0000 11300 SET$PROCESS::				; SET A PROCESS'S PARAMETERS
                               0000  0000 11400 	.WORD	0
       0000'CF       0000'8F     3C  0002 11500 	MOVZWL	#SS$_NORMAL,W^SET$L_STATUS; ASSUME SUCCESS
                                     0009 11600 	CALLBAK	W^PROC_NAME		; CALL CLI FOR PROCESS NAME
                                     0019 11700 	CALLBAK	W^QUAL_REQ		; CALL CLI FOR QUALIFIERS
            05            50     E8  0029 11800 	BLBS	R0,10$			; IF LOW SET SUCCESS
       0000'CF            50     D0  002C 11900 	MOVL	R0,W^SET$L_STATUS	; SET STATUS
                                     0031 12000 10$:
                                 04  0031 12100 	RET				; RETURN
                                     0032 12200 
                                     0032 12300 
SETPROCES       - SET PROCESS PARAMETERS                         21-AUG-1978 19:11:42   VAX-11 MACRO X0.3-11               Page   4
X01             SET PROCESS PRIORITY                                                                                             (1)

                                     0032 12500 	.SBTTL	SET PROCESS PRIORITY
                                     0032 12600 ;++
                                     0032 12700 ; PRIORITY - SET PROCESS PRIORITY
                                     0032 12800 ;
                                     0032 12900 ; FUNCTIONAL DESCRIPTION:
                                     0032 13000 ;
                                     0032 13100 ; THIS ACTION ROUTINE CALLS THE SYSTEM SERVICE SYS$SETPRI TO SET
                                     0032 13200 ; THE PRIORITY OF A PROCESS.
                                     0032 13300 ;
                                     0032 13400 ;
                                     0032 13500 ; INPUTS:
                                     0032 13600 ;
                                     0032 13700 ;	IF SPECIFIED, THE PROCESS NAME DESCRIPTOR IS IN PROC_NAME
                                     0032 13800 ;
                                     0032 13900 ; OUTPUTS:
                                     0032 14000 ;
                                     0032 14100 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     0032 14200 ;--
                                     0032 14300 PRIORITY:
                               0000  0032 14400 	.WORD	0
                                     0034 14500 	CALLBAK	W^NUM_VAL,W^PRIO_Q	; GET THE PRIORITY VALUE
                                     0044 14600 
            3E            50     E9  0044 14700 	BLBC	R0,60$			;
                                 D0  0047 14800 	MOVL	W^CLI$L_RQVALU+NUM_VAL,- ; SET PRIORITY VALUE
       000C'CF       0028'CF         0048 14900 		W^SETPRI$_PRI+SETPRI
                     0040'CF     B5  004E 15000 	TSTW	W^CLI$W_RQSIZE+PROC_NAME ; PROCESS NAME SPECIFIED?
                          09     13  0052 15100 	BEQL	40$			; IF EQL NO
                                 DE  0054 15500 	MOVAL	W^CLI$Q_RQDESC+PROC_NAME,- ; SET PROCESS NAME
       0008'CF       0040'CF         0055 15600 		W^SETPRI$_PRCNAM+SETPRI
                          1F     11  005B 15700 	BRB	50$
                                     005D 15800 
       0014'CF            00     E1  005D 15900 40$:	BBC	#Q_V_IDEN,W^BITS,50$	; IF CLEAR NO PROCESS ID
                          19         0062       
                     0018'CF     DF  0063 16000 	PUSHAL	W^ID			; SET ADDRESS OF RESULT
                     0074'CF     7F  0067 16010 	PUSHAQ	W^CLI$Q_QDVALDESC+IDEN_Q ; SET ADDRESS OF VALUE DESC
   00000000'EF            02     FB  006B 16020 	CALLS	#2,LIB$CVT_HEXBIN	; GET PROCESS ID VALUE
            10            50     E9  0072 16100 	BLBC	R0,60$			;
       0004'CF       0018'CF     DE  0075 16110 	MOVAL	W^ID,W^SETPRI$_PIDADR+SETPRI ; SET ADDRESS OF PID
                                     007C 16400 50$:	$SETPRI_G W^SETPRI		; SET THE PRIORITY
       0000'CF            50     D0  0085 16500 60$:	MOVL	R0,W^SET$L_STATUS	; SET STATUS
                                 04  008A 16600 	RET
                                     008B 16700 
