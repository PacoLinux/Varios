X01             ENABLE/DISABLE RESOURCE WAIT MODE                                                                                (1)

                                     00C8 20600 	.SBTTL	ENABLE/DISABLE RESOURCE WAIT MODE
                                     00C8 20700 ;++
                                     00C8 20800 ; RES_WAIT - ENABLE PROCESS RESOURCE WAIT MODE
                                     00C8 20900 ; NORES_WAIT - DISABLE PROCESS RESOURCE WAIT MODE
                                     00C8 21000 ;
                                     00C8 21100 ; FUNCTIONAL DESCRIPTION:
                                     00C8 21200 ;
                                     00C8 21300 ; THIS ACTION ROUTINE CALLS THE SYSTEM SERVICE SYS$SETRWM TO SET
                                     00C8 21400 ; THE PROCESS RESOURCE WAIT MODE.
                                     00C8 21500 ;
                                     00C8 21600 ;
                                     00C8 21700 ; INPUTS:
                                     00C8 21800 ;
                                     00C8 21900 ;	THE CLI DATA BASE IS SET UP. 
                                     00C8 22000 ;
                                     00C8 22100 ; OUTPUTS:
                                     00C8 22200 ;
                                     00C8 22300 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     00C8 22400 ;--
                                     00C8 22500 	.ENABL	LSB
                                     00C8 22600 
                                     00C8 22700 RES_WAIT:
                               0000  00C8 22800 	.WORD	0
                          50     D4  00CA 22900 	CLRL	R0			; SET FLAG TO ENABLE WAITING
                          05     11  00CC 23000 	BRB	10$
                                     00CE 23100 
                                     00CE 23200 NORES_WAIT:
                               0000  00CE 23300 	.WORD	0
            50            01     D0  00D0 23400 	MOVL	#1,R0			; SET FLAG TO DISABLE WAITING
                          D2     10  00D3 23500 10$:	BSBB	CHECK_PROC		; CHECK THAT PROCESS NOT SPECIFIED
                                     00D5 23900 	$SETRWM_S R0			; SET RESOURCE WAIT FLAG
                                 04  00DE 24000 	RET
                                     00DF 24100 	.DSABL	LSB
                                     00DF 24200 
                                     00DF 24300 	.END
