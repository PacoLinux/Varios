X01             ENABLE/DISABLE SWAP MODE                                                                                         (1)

                                     008B 16900 	.SBTTL	ENABLE/DISABLE SWAP MODE
                                     008B 17000 ;++
                                     008B 17100 ; SWAP - ENABLE PROCESS SWAP MODE
                                     008B 17200 ; NOSWAP - DISABLE PROCESS SWAP MODE
                                     008B 17300 ;
                                     008B 17400 ; FUNCTIONAL DESCRIPTION:
                                     008B 17500 ;
                                     008B 17600 ; THIS ACTION ROUTINE CALLS THE SYSTEM SERVICE SYS$SETSWM TO SET
                                     008B 17700 ; THE PROCESS SWAP MODE.
                                     008B 17800 ;
                                     008B 17900 ;
                                     008B 18000 ; INPUTS:
                                     008B 18100 ;
                                     008B 18200 ;	THE CLI DATA BASE IS SET UP.
                                     008B 18300 ;
                                     008B 18400 ; OUTPUTS:
                                     008B 18500 ;
                                     008B 18600 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     008B 18700 ;--
                                     008B 18800 	.ENABL	LSB
                                     008B 18900 
                               0000  008B 19000 SWAP:	.WORD	0
                          50     D4  008D 19100 	CLRL	R0			; SET FLAG TO ENABLE SWAPPING
                          05     11  008F 19200 	BRB	10$
                                     0091 19300 
                               0000  0091 19400 NOSWAP:	.WORD	0
            50            01     D0  0093 19500 	MOVL	#1,R0			; SET FLAG TO DISABLE SWAPPING
                          0F     10  0096 19600 10$:	BSBB	CHECK_PROC		; CHECK THAT PROCESS NOT SPECIFIED
                                     0098 20000 	$SETSWM_S R0			; SET SWAP FLAG
       0000'CF            50     D0  00A1 20100 	MOVL	R0,W^SET$L_STATUS	; SET STATUS
                                 04  00A6 20200 	RET
                                     00A7 20300 	.DSABL	LSB
                                     00A7 20301 
                                     00A7 20302 
                                     00A7 20310 ;++
                                     00A7 20311 ; CHECK_PROC -	LOCAL ROUTINE TO CHECK THAT A PROCESS NAME OR ID WERE
                                     00A7 20312 ;		NOT SPECIFIED
                                     00A7 20313 ;--
                                     00A7 20314 CHECK_PROC:
                     0040'CF     B5  00A7 20320 	TSTW	W^CLI$W_RQSIZE+PROC_NAME ; PROCESS NAME SPECIFIED?
                          0A     13  00AB 20340 	BEQL	10$			; IF EQL NO
       0000'CF   000380C0 8F     D0  00AD 20360 	MOVL	#CLI$_NOLIST,W^SET$L_STATUS ; SET NO PARAMETER ERROR
                                 04  00B6 20380 	RET				; EXIT
       0014'CF            00     E1  00B7 20381 10$:	BBC	#Q_V_IDEN,W^BITS,20$	; IF CLEAR NO ID SPECIFIED
                          0A         00BC       
       0000'CF   00038802 8F     D0  00BD 20382 	MOVL	#CLI$_CONFQUAL,W^SET$L_STATUS ;SET CONFLICTING QUALIFIERS
                                 04  00C6 20383 	RET
                                     00C7 20384 20$:
                                 05  00C7 20385 	RSB				; RETURN
                                     00C8 20386 
                                     00C8 20400 
