                          0B     1A  0081  5370 	BGTRU	20$			; BR IF NO
            54            03     C2  0083  5380 	SUBL	#MT$K_NRZI_800,R4	; CONVERT TO TABLE INDEX
                          06     19  0086  5382 	BLSS	20$			; BR IF NOT IN RANGE
            66       0043'CF44   D0  0088  5390 	MOVL	W^DENSITY[R4],(R6)	; SET DENSITY
                                     008E  5400 
                                     008E  5402 ;
                                     008E  5404 ; FORMAT,
                                     008E  5406 ;
                          86     D5  008E  5410 20$:	TSTL	(R6)+			;
            66       00DE'CF     9E  0090  5420 	MOVAB	W^DEFAULT,(R6)		; ASSUME DEFAULT FORMAT
            04            04     EF  0095  5430 	EXTZV	#MT$V_FORMAT,#MT$S_FORMAT,R5,R4 ; GET FORMAT VALUE
            54            55         0098       
                          10     13  009A  5440 	BEQL	30$			; BR IF DEFAULT
            0E            54     91  009C  5450 	CMPB	R4,#MT$K_NORMAL15	; IN RANGE?
                          0B     1A  009F  5460 	BGTRU	30$			; BR IF NO
            54            0C     C2  00A1  5470 	SUBL	#MT$K_NORMAL11,R4	; CONVERT TO TABLE INDEX
                          06     19  00A4  5472 	BLSS	30$			; BR IF NOT IN RANGE
            66       0054'CF44   D0  00A6  5480 	MOVL	W^FORMAT[R4],(R6)	; SET FORMAT
                                     00AC  5490 
                                     00AC  5492 ;
                                     00AC  5494 ; STATUS,
                                     00AC  5496 ;
            53            10     D0  00AC  5500 30$:	MOVL	#MT$V_BOT,R3		; GET LOWEST VALUED STATUS BIT
                          54     D4  00AF  5510 	CLRL	R4			; INIT TABLE INDEX
                          86     D5  00B1  5520 40$:	TSTL	(R6)+			; INCREMENT ARGLIST POINTER
            55            53     E1  00B3  5530 	BBC	R3,R5,50$		; BR IF STATUS NOT OCCURING
                          06         00B6       
            66       0080'CF44   D0  00B7  5540 	MOVL	W^STATUS[R4],(R6)	; SET STATUS NAME
                          54     D6  00BD  5550 50$:	INCL	R4			; INCREMENT TABLE INDEX
            53            14     F3  00BF  5560 	AOBLEQ	#MT$V_LOST,R3,40$	; CHECK NEXT BIT?
                          EE         00C2       
                                     00C3  5590 
                                     00C3  5592 ;
                                     00C3  5594 ; AND PARITY
                                     00C3  5596 ;
                          86     D5  00C3  5600 	TSTL	(R6)+			;
            66       00EB'CF     9E  00C5  5610 	MOVAB	W^ODD,(R6)		; ASSUME ODD PARITY
            55            03     E1  00CA  5620 	BBC	#MT$V_PARITY,R5,60$	; BR IF IT IS ODD
                          05         00CD       
            66       00E6'CF     9E  00CE  5630 	MOVAB	W^EVEN,(R6)		; ELSE SET TO EVEN
                                     00D3  5640 
       0000'CF            02     FB  00D3  6700 60$:	CALLS	#2,W^SHOW$PRINT_MSG
                                 04  00D8  6800 	RET
                                     00D9  6900 	.END
