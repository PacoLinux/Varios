                                 00000000   100 	.PSECT	SHOW$CODE,NOWRT
                                     0000   200 ;++
                                     0000   300 ; SHOW$MAGTAPE - SHOW MAGTAPE CHARACTERISTICS
                                     0000   400 ;
                                     0000   500 ; FUNCTIONAL DESCRIPTION:
                                     0000   600 ;
                                     0000   700 ; THIS ROUTINE SHOWS THE CHARACTERISTICS OF A SPECIFIED MAGTAPE.
                                     0000   800 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SHOW COMMAND PROCESSOR.
                                     0000   900 ;
                                     0000  1000 ; INPUTS:
                                     0000  1100 ;
                                     0000  1200 ;	THE CLI DATA BASE IS SET UP. 
                                     0000  1300 ;
                                     0000  1400 ; OUTPUTS:
                                     0000  1500 ;
                                     0000  1600 ;	R0 = STATUS OF THE OPERATION
                                     0000  1700 ;--
                                     0000  1800 SHOW$MAGTAPE::				; SHOW MAGTAPE COMMAND
                               03FC  0000  1900 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>
            56       0000'CF     9E  0002  2000 	MOVAB	W^DEV_NAME,R6		; ADDRESS QUAL DESC
                                     0007  2100 	CALLBAK	(R6)			; CALL CLI FOR DEVICE NAME
            56         08 A6     9E  0015  2200 	MOVAB	CLI$Q_RQDESC(R6),R6	; ADDRESS NAME DESC
                                     0019  2300 	$GETDEV_S	(R6),,W^GET_CHAN_INFO; GET THE DEVICE INFO
            06            50     E8  002C  2400 	BLBS	R0,SHOW
                                     002F  2500 ;
                                     002F  2600 ; ERROR IN GETTING CHARACTERISTICS
                                     002F  2700 ;
                                     002F  2800 ERROR:
       0000'CF            50     D0  002F  2900 	MOVL	R0,W^SHOW$L_STATUS	; SAVE FOR EXIT STATUS
                                 04  0034  3300 	RET				; RETURN
                                     0035  3400 ;
                                     0035  3500 ; OUTPUT CHARACTERISTICS
                                     0035  3600 ;
                                     0035  3700 SHOW:					; 
                          00'    DD  0035  3800 	PUSHL	S^#SHOW$_MAGT_L1	; SET UP TO OUTPUT MESSAGE
                     004C'CF     9F  0037  3900 	PUSHAB	W^CHAR_LIST		;
            56            6E     D0  003B  4000 	MOVL	(SP),R6			; ADDRESS LIST 
                                     003E  4100 ;
                                     003E  4200 ; OUTPUT NAME, TYPE,
                                     003E  4300 ;
            52       0024'CF     9E  003E  4400 	MOVAB	W^CHAN_INFO+8,R2	; ADDRESS CHARACTERISTICS
            50            82     3C  0043  4500 	MOVZWL	(R2)+,R0		; GET NAME AND UNIT
            52            62     9A  0046  4600 	MOVZBL	(R2),R2			; GET OFFSET TO NAME
            86       0018'CF42   9E  0049  4700 	MOVAB	W^CHAN_INFO-4[R2],(R6)+	; ADDRESS NAME
            86            50     D0  004F  4800 	MOVL	R0,(R6)+		; INSERT UNIT
                          54     D4  0052  4900 	CLRL	R4			; ASSUME UNKNOWN
            01       001D'CF     91  0054  5000 	CMPB	W^CHAN_INFO+1,#DT$_TE16	; IN RANGE?
                          0C     1F  0059  5100 	BLSSU	10$
            03       001D'CF     91  005B  5110 	CMPB	W^CHAN_INFO+1,#DT$_TU77	; IN RANGE?
                          05     1A  0060  5120 	BGTRU	10$
            54       001D'CF     9A  0062  5200  	MOVZBL	W^CHAN_INFO+1,R4	; GET TYPE VALUE
            86       001C'CF44   D0  0067  5300 10$:	MOVL	W^TYPE_NAME[R4],(R6)+	; ADDRESS TYPE
                                     006D  5310 
                                     006D  5315 ;
                                     006D  5317 ; DENSITY,
                                     006D  5319 ;
            55       0020'CF     D0  006D  5320 	MOVL	W^CHAN_INFO+4,R5	; GET DEVICE-DEPENDENT CHARACTERISTICS
