                                     0025   124 ;++
                                     0025   125 ; FUNCTIONAL DESCRIPTION:
                                     0025   126 ;
                                     0025   127 ;	THIS ROUTINE IS ENTERED BY THE SET COMMAND DISPATCHER WHEN
                                     0025   128 ;	A SET ACCOUNTING COMMAND IS EXECUTED. ALL WORK IS PERFORMED
                                     0025   129 ;	IN THE APPROPRAITE QUALIFER TRUE ACTION ROUTINES.
                                     0025   130 ;
                                     0025   131 ; CALLING SEQUENCE:	CALLS OR CALLG
                                     0025   132 ;
                                     0025   133 ; INPUT PARAMETERS:	NONE
                                     0025   134 ;
                                     0025   135 ; IMPLICIT INPUTS:	RESULT PARSE ADDRESS @ CLI$A_UTILSERV(AP)
                                     0025   136 ;
                                     0025   137 ; OUTPUT PARAMETERS:	NONE
                                     0025   138 ;
                                     0025   139 ; IMPLICIT OUTPUTS:	NONE
                                     0025   140 ;
                                     0025   141 ; SIDE EFFECTS:	NONE
                                     0025   142 ;
                                     0025   143 ; COMPLETION CODES:	SET$L_STATUS IS SET TO THE RESULT OF SEND MESSAGE
                                     0025   144 ;			SYSTEM SERVICE OR CLI$_INVKEY IF KEYWORD INCORRECT.
                                     0025   145 ;--
                                     0025   146 
                                 00000025   147 	.PSECT	SET$CODE	BYTE,RD,NOWRT,EXE
                                     0025   148 
