                                     0048   180 ;+
                                     0048   181 ; DISA_ENAB - DIABLE OR ENABLE ACCOUNTING
                                     0048   182 ;
                                     0048   183 ; THIS ROUTINE PERFORMS THE ENABLE OR DISABLE ACCOUNTING COMMANDS.
                                     0048   184 ; THIS ROUTINE IS SUPPLIED THE REQUEST CODE FOR ENABLE OR DISABLE ACCOUNTING.
                                     0048   185 ; IF THE COMMAND HAS ASSOCIATED VALUES, THE COMMAND IS A SELECTIVE OPERATION.
                                     0048   186 ; IN THIS CASE THE REQUEST CODE IS MODIFIED TO THE SELECTIVE OPERATION
                                     0048   187 ; AND THE KEYWORD VALUES ARE PARSED.
                                     0048   188 ;-
                                     0048   189  
                                     0048   190 ENAB_DISA:				;
            5E            14     C2  0048   191 	SUBL	#WRKSIZ,SP		; ALLOCATE SCRATCH BUFFER IN STACK
            52            5E     D0  004B   192 	MOVL	SP,R2			; SET ADDRESS OF BUFFER
            82            50     B0  004E   193 	MOVW	R0,(R2)+		; SET REQUEST CODE INTO MESSAGE BUFFER
            53         04 AC     D0  0051   194 	MOVL	4(AP),R3		; GET ADDRESS OF QUALIFIER DESCRIPTOR
                       04 A3     B5  0055   195 	TSTW	CLI$W_QDVALSIZ(R3)	; ANY VALUE PRESENT?
                          30     13  0058   196 	BEQL	40$			; WHEN NO VALUE, SEND THE MESSAGE
                                     005A   197 	ASSUME	<ACC$K_DISASEL-ACC$K_DISAACC> - ; CHECK FOR ASSUMED REALATIVE
                                     005A   198 	  EQ	<ACC$K_ENABSEL-ACC$K_ENABACC>   ; VALUE DIFFERENCES
                                 A0  005A   199 	ADDW	#<ACC$K_ENABSEL-ACC$K_ENABACC>,-; MODIFY THE REQUEST CODE
         FE A2            02         005B   200 		-2(R2)			; TO THE SELECTIVE OPERATION
                          53     DD  005E   201 10$:	PUSHL	R3			; PUSH ADDRESS OF QUALIFIER DESCRIPTOR
                     0000'CF     9F  0060   202 	PUSHAB	W^SET$A_CLIWORK		; THE COMMAND INTERPRETER WORK AREA
                     0000'CF     9F  0064   203 	PUSHAB	W^ACCO_G_ASCIIVAL	; THE REQUEST DESCRIPTOR ADDRESS
         08 BC            03     FB  0068   204 	CALLS	#3,@CLI$A_UTILSERV(AP)	; REQUEST AN ASCII VALUE
                     0008'CF     7F  006C   205 	PUSHAQ	W^ACCO_G_ASCIIVAL+CLI$Q_RQDESC ; ADDRESS OF STRING DESCRIPTOR
                       8D AF     9F  0070   206 	PUSHAB	ACCO_G_KEYWORDS		; ADDRESS OF KEYWORD LIST
       0000'CF            02     FB  0073   207 	CALLS	#2,W^LIB$MATCHKEY	; LOOK FOR KEYWORD MATCH
                          50     D5  0078   208 	TSTL	R0			; CHECK RESULT
                          19     19  007A   209 	BLSS	60$			; BR IF AMBIGUOUS MATCH
                          0E     12  007C   210 	BNEQ	50$			; BR IF NO MATCH
            82         9C AF40   90  007E   211 	MOVB	ACCO_G_VALS-1[R0],(R2)+	; INSERT OPTION CODE IN MESSAGE
                       04 A3     B5  0083   212 	TSTW	CLI$W_QDVALSIZ(R3)	; ANY VALUES REMAIN
                          D6     12  0086   213 	BNEQ	10$			; BR IF YES
                          82     94  0088   214 	CLRB	(R2)+			; MARK END OF LIST
                          1D     11  008A   215 40$:	BRB	SENDMSG			; SEND THE MESSAGE
            50   00038178 8F     D0  008C   216 50$:	MOVL	#CLI$_INVKEY,R0		; SET ERROR CODE - INVALID KEYWORD
                          07     11  0093   217 	BRB	70$			;
            50   000381A0 8F     D0  0095   218 60$:	MOVL	#CLI$_MULKEY,R0		; SET ERROR CODE - AMBIGUOUS KEYWORD
