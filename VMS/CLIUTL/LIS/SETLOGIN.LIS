                                     0000    72 ;++
                                     0000    73 ; SET$LOGIN - SET SYSTEM LOGIN PARAMETERS
                                     0000    74 ;
                                     0000    75 ; FUNCTIONAL DESCRIPTION:
                                     0000    76 ;
                                     0000    77 ; THIS ROUTINE SETS THE SYSTEM LOGIN COUNTS.
                                     0000    78 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SET COMMAND PROCESSOR.
                                     0000    79 ;
                                     0000    80 ; INPUTS:
                                     0000    81 ;
                                     0000    82 ;	THE CLI DATA BASE IS SET UP. 
                                     0000    83 ;
                                     0000    84 ; OUTPUTS:
                                     0000    85 ;
                                     0000    86 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     0000    87 ;--
                                     0000    88 SET$LOGIN::				; SET LOGIN PARAMETERS
                               03FC  0000    89 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>
       0000'CF       0000'8F     3C  0002    90 	MOVZWL	#SS$_NORMAL,W^SET$L_STATUS; ASSUME SUCCESS
                                     0009    91 	CALLBAK	W^QUAL_REQ		; CALL CLI FOR QUALIFIERS
                                 04  0019    92 	RET				; RETURN
                                     001A    93 ;
                                     001A    94 ; ACTION ROUTINES
                                     001A    95 ;
                                     001A    96 
                                     001A    97 ;
                                     001A    98 ; ACTION ROUTINE TO SET NUMBER OF INTERACTIVE USERS
                                     001A    99 ;
                                     001A   100 INTERACTIVE:
                               0000  001A   101 	.WORD	0
       0000'CF       0000'8F     3C  001C   102 	MOVZWL	#SS$_NOPRIV,W^SET$L_STATUS; ASSUME NO PRIVILEGE
   00000000'GF            12     E1  0023   103 	BBC	#PRV$V_OPER,G^CTL$GQ_PROCPRIV,60$; BR IF NOT PRIVILEGED
                          52         002A       
       0000'CF       0000'8F     3C  002B   104 	MOVZWL	#SS$_NORMAL,W^SET$L_STATUS ; SET SUCCESS
                     0040'CF     B5  0032   105 	TSTW	W^CLI$W_QDVALSIZ+INTE_QUAL; ANY VALUE
                          25     13  0036   106 	BEQL	10$			; IF EQL THEN OUTPUT MESSAGE ONLY
                                     0038   107 	CALLBAK	W^NUM_VAL,W^INTE_QUAL	; GET THE VALUE
                     0012'CF     B5  0048   108 	TSTW	W^NUM_VAL+CLI$L_RQVALU+2; VALUE IN RANGE?
                          28     12  004C   109 	BNEQ	50$			; IF NEQ THEN NO
                                     004E   110 	$CMKRNL_S 100$			; CONTINUE
                                     005D   111 ;
                                     005D   112 ; OUTPUT MESSAGE
                                     005D   113 ;
            7E   00000000'GF     3C  005D   114 10$:	MOVZWL	G^SYS$GW_IJOBCNT,-(SP)	; OUTPUT COUNT
            7E   00000000'GF     3C  0064   115 	MOVZWL	G^SYS$GW_IJOBLIM,-(SP)	; OUTPUT CURRENT LIMIT
                          00'    DD  006B   116 	PUSHL	S^#SET$_LOGIN		;
                       04 AE     9F  006D   117 	PUSHAB	4(SP)			;
       0000'CF            03     FB  0070   118 	CALLS	#3,W^SHOW$PRINT_MSG	; OUTPUT
                                 04  0075   119 	RET
                                     0076   120 ;
                                     0076   121 ; BAD PARAMETER VALUE
                                     0076   122 ;
       0000'CF       0000'8F     3C  0076   123 50$:	MOVZWL	#SS$_BADPARAM,W^SET$L_STATUS; SET UP EXIT STATUS
                                 04  007D   124 60$:	RET
                                     007E   125 ;
                                     007E   126 ; KERNEL MODE HANDLER TO CHANGE LOGIN LIMIT
