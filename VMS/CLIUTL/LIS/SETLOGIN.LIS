   00000000'GF       0010'CF     B0  0080   129 	MOVW	W^NUM_VAL+CLI$L_RQVALU,G^SYS$GW_IJOBLIM
                          09     13  0089   130 	BEQL	110$			; IF EQL THEN CONTINUE
   0000005A'GF       8000 8F     AA  008B   131 	BICW	#^X08000,G^SYS$GL_JOBCTLMB+UCB$W_DEVSTS; ENABLE TERMINALS
            50            01     D0  0094   132 110$:	MOVL	#1,R0
                                 04  0097   133 	RET
                                     0098   134 
                                     0098   135 	.END
