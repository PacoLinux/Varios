SHOW$MSG_UTIL   - MESSAGE PRINTING UTILITIES                     21-AUG-1978 19:21:32   VAX-11 MACRO X0.3-11               Page   4
X0101           SHOW$PRINT_MSG  - PRINT MESSAGE ROUTINE                                                                          (3)

                                     01B0   104 	.SBTTL	SHOW$PRINT_MSG		- PRINT MESSAGE ROUTINE
                                     01B0   105 
                                     01B0   106 ;++
                                     01B0   107 ; FUNCTIONAL DESCRIPTION:
                                     01B0   108 ;	THIS ROUTINE FORMATS ALL MESSAGES AND PRINTS THEM ON SYS$OUTPUT:
                                     01B0   109 ;	OR SYS$ERROR DEPENDING ON THE CODE ASSOCIATED WITH THE MESSAGE.
                                     01B0   110 ;	( SEE DESCRIPTION OF MACRO SHOW_DEFMSG FOR FORMAT OF MESSAGE TABLE)
                                     01B0   111 ;
                                     01B0   112 ; CALLING SEQUENCE:
                                     01B0   113 ;
                                     01B0   114 ;		PUSHL	#SHOW$_<MESSAGE MNEMONIC>
                                     01B0   115 ;		PUSHAL	ARGLIST
                                     01B0   116 ;		CALLS #2,SHOW$PRINT_MSG
                                     01B0   117 ;
                                     01B0   118 ; INPUT PARAMETERS:
                                     01B0   119 ;
                                     01B0   120 ;	4(AP)=	MESSAGE IDENTIFICATION NUMBER
                                     01B0   121 ;	8(AP)=	POINTER TO ARGUMENT LIST FOR $FAOL
                                     01B0   122 ;
                                     01B0   123 ; OUTPUT PARAMETERS:
                                     01B0   124 ;
                                     01B0   125 ;	NONE
                                     01B0   126 ;
                                     01B0   127 ; IMPLICIT INPUT PARAMETERS:
                                     01B0   128 ;
                                     01B0   129 ; 	NONE
                                     01B0   130 ;
                                     01B0   131 ; IMPLICIT OUTPUT PARAMETERS:
                                     01B0   132 ;
                                     01B0   133 ;	CALLS ROUTINE SHOW$PRINT_LINE
                                     01B0   134 ;
                                     01B0   135 ; COMPLETION CODES:
                                     01B0   136 ;
                                     01B0   137 ;	NONE
                                     01B0   138 ;
                                     01B0   139 ; SIDE EFFECTS:
                                     01B0   140 ;
                                     01B0   141 ;	NONE
                                     01B0   142 ;--
                                     01B0   143 
                                 00000000   144 	.PSECT	SHOW$CODE	BYTE,RD,NOWRT,EXE
                                     0000   145 
                               0C00' 0000   146 	.ENTRY	SHOW$PRINT_MSG,^M<R10,R11>		
                                     0002   147 
                                     0002   148 ;
                                     0002   149 ; initialize
                                     0002   150 ;
                                     0002   151 
                          01     BB  0002   152 	PUSHR	#^M<R0>			; save status code 
            0C         08 AC     C5  0004   153 	MULL3	8(AP),#MSG_K_LENGTH,R11
                          5B         0008       
                                     0009   154 					; CALCULATE OFFSET INTO MESSAGE TABLE
            5A         04 AC     D0  0009   155 	MOVL 	4(AP),R10		; POINTER TO FAO PARAMETER LIST
                                     000D   156 
                                     000D   157 ;
                                     000D   158 ; format a message line
                                     000D   159 ;
SHOW$MSG_UTIL   - MESSAGE PRINTING UTILITIES                     21-AUG-1978 19:21:32   VAX-11 MACRO X0.3-11               Page   5
X0101           SHOW$PRINT_MSG  - PRINT MESSAGE ROUTINE                                                                          (3)

                                     000D   160 
                                     000D   161 
                                     000D   162 10$:
                                     000D   163 	$FAOL_S		-		; FORMAT MESSAGE INTO OUPUT BUFFER
                                     000D   164 		CTRSTR=W^SHOW$A_MSGTXT(R11), -	; FAO CONTROL STRING
                                     000D   165 		OUTLEN=W^OUTPUT_LENGTH, -		; WORD TO RECEIVE ACTUAL LENGTH
                                     000D   166 		OUTBUF=W^SHOW$GQ_OUT_DSC, -	; OUTPUT BUFFER DESCRIPTOR
                                     000D   167 		PRMLST=(R10)			; PARAMETER LIST
            24            50     E9  0022   168 	BLBC	R0,99$			; branch on error
                                     0025   169 
                                     0025   170 ;
                                     0025   171 ; print formatted line 
                                     0025   172 ;
                                     0025   173 
                                     0025   174 
                     00B0'CF     DF  0025   175 	PUSHAL	W^OUTPUT_BUFFER		; ADDRESS OF OUTPUT MESSAGE STRING
                     00A4'CF     DD  0029   176 	PUSHL	W^OUTPUT_LENGTH		; RESULTANT LENGTH OF OUTPUT MESSAGE
       004A'CF            02     FB  002D   177 	CALLS	#2,W^SHOW$PRINT_LINE	; PRINT LINE ON SYS$OUTPUT:
            14            50     E9  0032   178 	BLBC	R0,99$			; branch on error
                                     0035   179 
                                     0035   180 ;
                                     0035   181 ; get next line of message
                                     0035   182 ;
                                     0035   183 
                                     0035   184 
            0D       0015'CB     E9  0035   185 	BLBC	W^SHOW$A_MSGTXT+MSG_K_LENGTH+MSG_B_CONTIN(R11),90$
                                     003A   186 					; NO CONTINUATION
            50       000A'CB     9A  003A   187 	MOVZBL	W^SHOW$A_MSGTXT+MSG_B_PARAM(R11),R0
                                     003F   188 					; SKIP FAO PARAMETERS ALREADY PROCESSED
            5A            50     C0  003F   189 	ADDL2	R0,R10			; UPDATE POINTER TO PARAMETER LIST
            5B            0C     C0  0042   190 	ADDL2	#MSG_K_LENGTH,R11	; SKIP TO NEXT MESSAGE ENTRY
                          C6     11  0045   191 	BRB	10$			; AND PROCESS NEXT LINE OF MESSAGE
                                     0047   192 
                                     0047   193 ;
                                     0047   194 ; exits
                                     0047   195 ;
                                     0047   196 
                                     0047   197 90$:
                          01     BA  0047   198 	POPR	#^M<R0>			; restore return code
                                 04  0049   199 99$:	RET				; ALL DONE
SHOW$MSG_UTIL   - MESSAGE PRINTING UTILITIES                     21-AUG-1978 19:21:32   VAX-11 MACRO X0.3-11               Page   6
X0101           SHOW$PRINT_LINE  - PRINT LINE ROUTINE                                                                            (4)

                                     004A   201 	.SBTTL	SHOW$PRINT_LINE 	- PRINT LINE ROUTINE
                                     004A   202 
                                     004A   203 ;++
                                     004A   204 ; FUNCTIONAL DESCRIPTION:
                                     004A   205 ;
                                     004A   206 ;	THIS ROUTINE PRINTS A LINE ON DEVICE SYS$OUTPUT
                                     004A   207 ;
                                     004A   208 ; CALLING SEQUENCE:
                                     004A   209 ;
                                     004A   210 ;		PUSHAL	OUT_BUFFER
                                     004A   211 ;		PUSHL	OUT_LENGTH
                                     004A   212 ;		CALLS 	#2,SHOW$PRINT_LINE
                                     004A   213 ;
                                     004A   214 ;	WHERE:	OUT_LENGTH =  LENGTH OF CHARACTER STRING TO PRINT
                                     004A   215 ;		OUT_BUFFER =  ADDRESS OF STRING TO PRINT
                                     004A   216 ;
                                     004A   217 ;	A CR/LF IS AUTOMATICALLY APPENDED FOR EACH CALL
                                     004A   218 ;
                                     004A   219 ;	NOTE:
                                     004A   220 ;	IT IS HIGHLY RECOMMENDED TO USE THE COMMON LINE BUFFER PROVIDED
                                     004A   221 ;	 THE DESCRIPTOR OF WHICH IS ADDRESSED BY GLOBAL SYMBOL:
                                     004A   222 ;			SHOW$GQ_OUT_DSC
                                     004A   223 ;
                                     004A   224 ;
                                     004A   225 ;
                                     004A   226 ; INPUT PARAMETERS:
                                     004A   227 ;
                                     004A   228 ;		OUT_BUFFER
                                     004A   229 ;		OUT_LENGTH
                                     004A   230 ;
                                     004A   231 ; IMPLICIT INPUTS:
                                     004A   232 ;
                                     004A   233 ;	NONE
                                     004A   234 ;
                                     004A   235 ; OUTPUT PARAMETERS:
                                     004A   236 ;
                                     004A   237 ;	NONE
                                     004A   238 ;
                                     004A   239 ; IMPLICIT OUTPUTS:
                                     004A   240 ;
                                     004A   241 ;	LINE IS PRINTED ON SYS$OUTPUT:
                                     004A   242 ;
                                     004A   243 ; COMPLETION CODES:
                                     004A   244 ;
                                     004A   245 ;	RMS COMPLETION CODES
                                     004A   246 ;
                                     004A   247 ; SIDE EFFECTS:
                                     004A   248 ;
                                     004A   249 ;
                                     004A   250 ;	OPENS AND CONNECTS SYS$OUTPUT: IF NECESSARY
                                     004A   251 ;
                                     004A   252 ;--
                                     004A   253 
                                 0000004A   254 	.PSECT	SHOW$CODE	BYTE,RD,NOWRT,EXE
                                     004A   255 
                               000C' 004A   256 	.ENTRY	SHOW$PRINT_LINE,^M<R2,R3>
                                     004C   257 
SHOW$MSG_UTIL   - MESSAGE PRINTING UTILITIES                     21-AUG-1978 19:21:32   VAX-11 MACRO X0.3-11               Page   7
X0101           SHOW$PRINT_LINE  - PRINT LINE ROUTINE                                                                            (4)

                                     004C   258 ;
                                     004C   259 ; open SYS$OUTPUT: if not opened
                                     004C   260 ;
                                     004C   261 
                     005E'CF     B5  004C   262 	TSTW	W^RAB$W_ISI+OUTPUT_RAB	; HAS OUTPUT ALREADY BEEN OPENED ?
                          1C     12  0050   263 	BNEQ	10$			; YES
                                     0052   264 	$CREATE	FAB=W^OUTPUT_FAB		; NO, OPEN IT
            25            50     E9  005D   265 	BLBC	R0,90$			; BRANCH ON ERROR
                                     0060   266 	$CONNECT RAB=W^OUTPUT_RAB		; AND CONNECT
            17            50     E9  006B   267 	BLBC	R0,90$			; branch on error
                                     006E   268 
                                     006E   269 ;
                                     006E   270 ; print line on SYS$OUTPUT:
                                     006E   271 ;
                                     006E   272 
                                     006E   273 10$:
       0084'CF         08 AC     D0  006E   274 	MOVL	8(AP),W^OUTPUT_RAB+RAB$L_RBF
                                     0074   275 					; INSERT OUTPUT BUFFER ADDRESS
       007E'CF         04 AC     B0  0074   276 	MOVW 	4(AP),W^OUTPUT_RAB+RAB$W_RSZ
                                     007A   277 					; INSERT OUTPUT BUFFER LENGTH
                                     007A   278 	$PUT	RAB=W^OUTPUT_RAB	; ISSUE RMS CALL TO PUT LINE
                                     0085   279 ;
                                     0085   280 ; exit
                                     0085   281 ;
                                     0085   282 
                                     0085   283 90$:
                                 04  0085   284 	RET				; RETURN
                                     0086   285 	.END
