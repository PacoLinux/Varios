                                     0000  7900 ; SET$CARD - SET CARD READER CHARACTERISTICS
                                     0000  8000 ;
                                     0000  8100 ; FUNCTIONAL DESCRIPTION:
                                     0000  8200 ;
                                     0000  8300 ; THIS ROUTINE SETS THE CHARACTERISTICS OF A SPECIFIED CARD READER.
                                     0000  8400 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SET COMMAND PROCESSOR.
                                     0000  8500 ;
                                     0000  8600 ; INPUTS:
                                     0000  8700 ;
                                     0000  8800 ;	THE CLI DATA BASE IS SET UP. 
                                     0000  8900 ;
                                     0000  9000 ; OUTPUTS:
                                     0000  9100 ;
                                     0000  9200 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     0000  9300 ;--
                                     0000  9400 SET$CARD::				; SET CARD READER COMMAND
                               03FC  0000  9500 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>
            56       0058'CF     9E  0002  9600 	MOVAB	W^CR_NAME,R6		; ADDRESS QUAL DESC
                                     0007  9700 	CALLBAK	(R6)			; CALL CLI FOR READER NAME
            56         08 A6     9E  0015  9800 	MOVAB	CLI$Q_RQDESC(R6),R6	; ADDRESS NAME DESC
                                     0019  9900 20$:	$ASSIGN_S	(R6),W^QIO+8	; ASSIGN CHANNEL TO DEVICE
            26            50     E9  0028 10000 	BLBC	R0,ERROR		; IF LOW CLEAR THEN ERROR AND EXIT
                                     002B 10100 	$GETCHN_S	W^QIO+8,,W^GET_CHAN_INFO
            0D            50     E9  0041 10200 	BLBC	R0,ERROR		; IF LOW CLEAR THEN ERROR
         41 8F       001C'CF     91  0044 10210 	CMPB	W^CLASS,#DC$_CARD	; IS DEVICE A CARD READER?
                          0B     13  004A 10220 	BEQL	SET_CHAR		; IF EQL YES
            50       0000'8F     3C  004C 10230 	MOVZWL	#SS$_IVDEVNAM,R0	; ELSE INVALID DEVICE ERROR
                                     0051 10300 ;
                                     0051 10400 ; ERROR IN GETTING CHARACTERISTICS
                                     0051 10500 ;
       0000'CF            50     D0  0051 10600 ERROR:	MOVL	R0,W^SET$L_STATUS	; SAVE FOR EXIT STATUS
                                 04  0056 10700 	RET				; RETURN
                                     0057 10800 ;
                                     0057 10900 ; PROCESS DATA
                                     0057 11000 ;
                                     0057 11100 SET_CHAR:				; PROCESS DATA
       0000'CF       0000'8F     3C  0057 11200 	MOVZWL	#SS$_NORMAL,W^SET$L_STATUS; ASSUME SUCCESS
                                     005E 11300 	CALLBAK	W^CHAR_QUAL
            18       0000'CF     E9  006E 11400 	BLBC	W^SET$L_STATUS,100$
   00000000'9F       0024'CF     FA  0073 11500 	CALLG	W^QIO,@#SYS$QIOW	; SET THE MODE
       0000'CF            50     B0  007C 11600 	MOVW	R0,W^SET$L_STATUS	; SAVE STATUS
            07            50     E9  0081 11700 	BLBC	R0,100$			; IF LOW CLEAR THEN ERROR
       0000'CF       0008'CF     3C  0084 11800 	MOVZWL	W^IOSTATUS,W^SET$L_STATUS; SAVE STATUS
                                     008B 11900 100$:	$DASSGN_S	W^QIO+8		; DEASSIGN THE CARD READER
                                 04  0097 12000 	RET				; RETURN
                                     0098 12100 ;
                                     0098 12200 ; ACTION ROUTINES
                                     0098 12300 ;
                                     0098 12400 
                                     0098 12500 ;
                                     0098 12600 ; ACTION ROUTINE FOR CR$K_T026
                                     0098 12700 ;
                               0000  0098 12800 T026:	.WORD	0
            00            00     F0  009A 12900 	INSV	#CR$K_T026,#CR$V_TMODE,#CR$S_TMODE,W^CR_CHAR
       0020'CF            04         009D       
