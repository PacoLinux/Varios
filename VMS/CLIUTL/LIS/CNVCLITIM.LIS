                                     003E   325 ;++
                                     003E   326 ; FUNCTIONAL DESCRIPTION:
                                     003E   327 ;
                                     003E   328 ; THESE ROUTINES ARE CALLED TO CONVERT AN ASCII STRING INTO A FORM
                                     003E   329 ; THAT IS ACCEPTABLE TO THE 'GET TIME' SYSTEM SERVICES.
                                     003E   330 ;
                                     003E   331 ; ENTRY POINT LIB$CNVCLITIME IS CALLED TO CONVERT A STRING TO ABSOLUTE
                                     003E   332 ; BINARY TIME.
                                     003E   333 ;
                                     003E   334 ; ENTRY POINT LIB$CNVCLIDTIME IS CALLED TO CONVERT A STRING TO DELTA
                                     003E   335 ; TIME
                                     003E   336 ;
                                     003E   337 ; ABSOLUTE TIME SYNTAX:
                                     003E   338 ;
                                     003E   339 ; THE FULLY EXPANDED TIME STRING, AS SEEN BY THE SYSTEM, HAS THE FORM:
                                     003E   340 ;
                                     003E   341 ;	DD-MMM-YYYY<BLANK>HH:MM:SS.SS
                                     003E   342 ;
                                     003E   343 ; WHERE ANY FIELD MAY BE EMPTY.
                                     003E   344 ;
                                     003E   345 ; THE TIME CONVERSION ROUTINES CONVERT ABBREVIATED INPUT SUPPLIED BY
                                     003E   346 ; THE CALLER INTO THE ABOVE FORMAT, THEN INVOKE THE SYSTEM SERVICE TO
                                     003E   347 ; CONVERT THE STRING TO BINARY.
                                     003E   348 ;
                                     003E   349 ; THE FOLLOWING FORMS ARE ACCEPTABLE:
                                     003E   350 ;
                                     003E   351 ;	DD-MMM-YYYY:HH:MM:SS.SS		- FULL
                                     003E   352 ;
                                     003E   353 ;	DD-MMM-YYYY<BLANK>HH:MM:SS.SS	- FULL
                                     003E   354 ;
                                     003E   355 ;	DD-MMM:HH:MM			- TRUNCATED FIELDS
                                     003E   356 ;
                                     003E   357 ;	DD--YYYY:HH::SS			- NULL FIELDS
                                     003E   358 ;
                                     003E   359 ;	HH:[MM:SS.SS]			- TIME ONLY
                                     003E   360 ;
                                     003E   361 ; DELTA TIME SYNTAX
                                     003E   362 ;
                                     003E   363 ; THE FULLY EXPANDED DELTA TIME STRING AS SEEN BY THE SYSTEM IS:
                                     003E   364 ;
                                     003E   365 ;	DD<BLANK>HH:MM:SS.S
                                     003E   366 ;
                                     003E   367 ; WHERE ANY FIELD MAY BE EMPTY.
                                     003E   368 ;
                                     003E   369 ; THE FOLLOWING FORMS ARE ACCEPTABLE:
                                     003E   370 ;
                                     003E   371 ;	DD-HH:MM:SS.SS			- FULL
                                     003E   372 ;
                                     003E   373 ;	DD-HH:				- TRUNCATED FIELDS
                                     003E   374 ;
                                     003E   375 ;	DD-:MM				- NULL FIELDS
                                     003E   376 ;
                                     003E   377 ;	HH[:MM:SS.SS]			- TIME ONLY
                                     003E   378 ;
                                     003E   379 ;
                                     003E   380 ; CALLING SEQUENCE:
CVTTIME         - CONVERT CLI TIME TO BINARY                     21-AUG-1978 18:57:26   VAX-11 MACRO X0.3-11               Page   9
00.2            LIB$CNVCLITIM                                                                                                    (4)

                                     003E   381 ;
                                     003E   382 ;	CALL	LIB$CNVCLITIME	(DESCR,TIME,[STRING])
                                     003E   383 ;	CALL	LIB$CNVCLIDTIME	(DESCR,TIME,[STRING])
                                     003E   384 ;
                                     003E   385 ; INPUT PARAMETERS:
                                     003E   386 ;
                                     003E   387 ;	DESCR = ADDRESS OF INPUT STRING DESCRIPTOR
                                     003E   388 ;
                                     003E   389 ;	TIME = ADDRESS OF QUADWORD TO RECEIVE CONVERTED TIME
                                     003E   390 ;
                                     003E   391 ;	STRING = ADDRESS OF OPTIONAL BYTE ARRAY TO RECEIVE CONVERTED STRING
                                     003E   392 ;
                                     003E   393 ; IMPLICIT INPUTS:
                                     003E   394 ;
                                     003E   395 ;	NONE
                                     003E   396 ;
                                     003E   397 ; OUTPUT PARAMETERS:
                                     003E   398 ;
                                     003E   399 ;	R0=CLI$_NORMAL
                                     003E   400 ;
                                     003E   401 ;
                                     003E   402 ;	TIME = ABSOLUTE BINARY TIME IN 64-BIT FORMAT
                                     003E   403 ;
                                     003E   404 ;
                                     003E   405 ;	R0=CLI$_IVVALU
                                     003E   406 ;
                                     003E   407 ;	INVALID TIME FORMAT DETECTED
                                     003E   408 ;
                                     003E   409 ; IMPLICIT OUTPUTS:
                                     003E   410 ;
                                     003E   411 ;	NONE
                                     003E   412 ;
                                     003E   413 ; COMPLETION CODES:
                                     003E   414 ;
                                     003E   415 ;	R0 LBS = SUCCESS
                                     003E   416 ;	R0 LBC = SYNTAX ERROR
                                     003E   417 ;
                                     003E   418 ; SIDE EFFECTS:
                                     003E   419 ;
                                     003E   420 ;	NONE
                                     003E   421 ;
                                     003E   422 ;--
                                     003E   423  
