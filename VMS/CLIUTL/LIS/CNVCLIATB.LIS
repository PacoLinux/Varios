00.1            CNVCLIATB - CONVERT ASCII TO SINGLE-PRECISION B                                                                  (1)

                                     0010    57 	.SBTTL	CNVCLIATB - CONVERT ASCII TO SINGLE-PRECISION BINARY
                                     0010    58  
                                     0010    59 ;++
                                     0010    60 ; FUNCTIONAL DESCRIPTION:
                                     0010    61 ;
                                     0010    62 ; THIS SUBROUTINE CONVERTS AN ASCII STRING TO A SINGLE PRECISION
                                     0010    63 ; BINARY VALUE. THE STRING IS CONSIDERED AS AN UNSIGNED VALUE.
                                     0010    64 ;
                                     0010    65 ; THE CONVERSION RADIX IS ONE OF THE FOLLOWING:
                                     0010    66 ;
                                     0010    67 ;	LIB$CVTCLIDTB - CONVERT DECIMAL TO BINARY
                                     0010    68 ;	LIB$CVTCLIOTB - CONVERT OCTAL TO BINARY
                                     0010    69 ;	LIB$CVTCLIHTB - CONVERT HEXADECIMAL TO BINARY
                                     0010    70 ;
                                     0010    71 ; CALLING SEQUENCE:
                                     0010    72 ;
                                     0010    73 ;	JSB	LIB$CVTCLIXXX
                                     0010    74 ;
                                     0010    75 ; INPUT PARAMETERS:
                                     0010    76 ;
                                     0010    77 ;	R2,R3 = DESCRIPTOR FOR STRING TO BE CONVERTED
                                     0010    78 ;
                                     0010    79 ; IMPLICIT INPUTS:
                                     0010    80 ;
                                     0010    81 ;	NONE
                                     0010    82 ;
                                     0010    83 ; OUTPUT PARAMETERS:
                                     0010    84 ;
                                     0010    85 ;	R0 = CLI$_NORMAL, SUCCESS:
                                     0010    86 ;
                                     0010    87 ;		R1 CONTAINS THE DOUBLE PRECISION RESULT
                                     0010    88 ;
                                     0010    89 ;	R0 = CLI$_IVCHAR, INVALID NUMERIC CHARACTER FOUND
                                     0010    90 ;	R0 = CLI$_IVVALU, VALUE EXCEEDS A SINGLE PRECISION INTEGER
                                     0010    91 ;
                                     0010    92 ; IMPLICIT OUTPUTS:
                                     0010    93 ;
                                     0010    94 ;	R3 = ADDRESS OF BYTE FOLLOWING LAST CHARACTER OR ONE BYTE
                                     0010    95 ;	     PAST AN INVALID CHARACTER
                                     0010    96 ;
                                     0010    97 ; COMPLETION CODES:
                                     0010    98 ;
                                     0010    99 ;	SEE ABOVE
                                     0010   100 ;
                                     0010   101 ; SIDE EFFECTS:
                                     0010   102 ;
                                     0010   103 ;	R2 IS DESTROYED
                                     0010   104 ;
                                     0010   105 ;--
                                     0010   106  
                                     0010   107 	.ENABL	LSB
                                     0010   108  
                                     0010   109 LIB$CVTCLIDTB::				;
            50            0A     D0  0010   110 	MOVL	#10,R0			; GET RADIX
                          08     11  0013   111 	BRB	10$			;
                                     0015   112 LIB$CVTCLIOTB::				;
            50            08     D0  0015   113 	MOVL	#8,R0			; GET RADIX
CNVCLIATB       - CONVERT ASCII TO BINARY                        21-AUG-1978 18:56:10   VAX-11 MACRO X0.3-11               Page   4
00.1            CNVCLIATB - CONVERT ASCII TO SINGLE-PRECISION B                                                                  (1)

                          03     11  0018   114 	BRB	10$			;
                                     001A   115 LIB$CVTCLIHTB::				;
            50            10     D0  001A   116 	MOVL	#16,R0			; GET RADIX
                                     001D   117 10$:					;
                     0071 8F     BB  001D   118 	PUSHR	#^M<R0,R4,R5,R6>	; SAVE REGISTERS AND RADIX
                          55     D4  0021   119 	CLRL	R5			; CLEAR RESULT
                                     0023   120 20$:					;
            54   00030001 8F     D0  0023   121 	MOVL	#CLI$_NORMAL,R4		; ASSUME SUCCESS
                          52     B7  002A   122 	DECW	R2			; DECREMENT BYTE COUNT
                          2D     1F  002C   123 	BLSSU	30$			; IF LSSU DONE
            54   00038050 8F     D0  002E   124 	MOVL	#CLI$_IVCHAR,R4		; ASSUME INVALID CHARACTER
            50            83     9A  0035   125 	MOVZBL	(R3)+,R0		; GET NEXT CHARACTER
            50            06     E1  0038   126 	BBC	#6,R0,25$		; BRANCH IF 0 - 9
                          04         003B       
            50            05     E5  003C   127 	BBCC	#5,R0,25$		; CONVERT TO UPPER CASE ALPHA
                          00         003F       
                                     0040   128 25$:					;
            6E            50     3A  0040   129 	LOCC	R0,(SP),CVTABL		; SEARCH FOR NUMERIC CHARACTER
                       BB AF         0043       
                          14     13  0045   130 	BEQL	30$			; IF EQL INVALID CHARACTER
            6E            50     C3  0047   131 	SUBL3	R0,(SP),R0		; CONVERT INDEX TO NUMBER
                          50         004A       
            54   00038088 8F     D0  004B   132 	MOVL	#CLI$_IVVALU,R4		; ASSUME OVERFLOW
            6E            55     7A  0052   133 	EMUL	R5,(SP),R0,R5		;  COMPUTE BINARY VALUE
            55            50         0055       
                          56     D5  0057   134 	TSTL	R6			; TEST FOR OVERFLOW
                          C8     13  0059   135 	BEQL	20$			; IF EQL CONTINUE
                                     005B   136 30$:					;
            50            54     7D  005B   137 	MOVQ	R4,R0			; GET STATUS AND RESULT
                     0074 8F     BA  005E   138 	POPR	#^M<R2,R4,R5,R6>	; RESTORE REGISTERS
                                 05  0062   139 	RSB				;
                                     0063   140  
                                     0063   141 	.END
