                                     0000  5700 ;++
                                     0000  5800 ; SET$DEV_SPOOL - KERNEL ACCESS MODE TO SET A DEVICE SPOOLED
                                     0000  5900 ;
                                     0000  6000 ; FUNCTIONAL DESCRIPTION:
                                     0000  6100 ;
                                     0000  6200 ; THIS ROUTINE WILL LINK TWO UCBS TOGETHER TO ALLOW IMPLICIT SPOOLING.
                                     0000  6300 ;
                                     0000  6400 ; 1) LOCK I/O DATABASE FOR WRITE;
                                     0000  6500 ; 2) CHECK THAT SPOOL DEVICE'S REFERENCE COUNT (UCB$W_REFC) IS 1;
                                     0000  6600 ; 3) CHECK THAT A VOLUME IS MOUNTED ON INTERMEDIATE DEVICE AND THAT
                                     0000  6700 ;    IT IS NOT MARKED FOR DISMOUNT;
                                     0000  6800 ; 4) GET DYNAMIC STORAGE, MOVE QUEUE NAME STRING TO IT, AND SET
                                     0000  6900 ;    SPOOLED DEVICE'S VCB ADDRESS TO THE ADDRESS OF THE QUEUE NAME;
                                     0000  7000 ; 5) INCREMENT REFERENCE COUNT OF INTERMEDIATE DEVICE AND TRANSACTION
                                     0000  7100 ;    COUNT OF OF ITS ASSOCIATED VCB;
                                     0000  7200 ; 6) STORE ADDRESS OF INTERMEDIATE DEVICE UCB IN SPOOLED DEVICE'S
                                     0000  7300 ;    ASSOCIATED UCB ADDRESS (UCB$L_AMB);
                                     0000  7400 ; 7) SET SPOOLED DEVICE'S SPOOL BIT (DEV$V_SPL IN UCB$L_DEVCHAR);
                                     0000  7500 ; 8) UNLOCK I/O DATABASE.
                                     0000  7600 ;
                                     0000  7700 ;
                                     0000  7800 ; INPUTS:
                                     0000  7900 ;
                                     0000  8000 ;	UCB1(AP) = UCB OF DEVICE TO BE SPOOLED
                                     0000  8100 ;	UCB2(AP) = UCB OF INTERMEDIATE DEVICE
                                     0000  8200 ;	Q_DESC(AP) = CHARACTER STRING DESCRIPTOR OF QUEUE NAME
                                     0000  8300 ;
                                     0000  8400 ; OUTPUTS:
                                     0000  8500 ;
                                     0000  8600 ;	R0 = 1 IF SUCCES SETTING UP UCBS
                                     0000  8700 ;	   = SYSTEM STATUS CODE IF ANY ERROR
                                     0000  8800 ;--
                                     0000  8900 SET$DEV_SPOOL::
                               00FC  0000  9000 	.WORD	^M<R2,R3,R4,R5,R6,R7>	; SAVE REGISTERS
                                     0002  9100 ;
                                     0002  9200 ; LOCK I/O DATABASE FOR WRITE AND CHECK IF DEVICES VALID FOR SPOOLING
                                     0002  9300 ;
                          0109   30  0002  9400 	BSBW	LOCK_IODB		; LOCK THE I/O DATABASE
            56         04 AC     D0  0005  9500 	MOVL	UCB1(AP),R6		; GET ADDR OF SPOOLED DEV'S UCB
         34 A6            06     E1  0009  9600 	BBC	#DEV$V_SPL,UCB$L_DEVCHAR(R6),5$ ; IF CLEAR DEVICE NO SPOOLED
                          09         000D       
                 0003885A 8F     DD  000E  9700 	PUSHL	#CLI$_DEVALSPL		; SET DEVICE ALREADY SPOOLED
                          007A   31  0014  9800 	BRW	100$			; EXIT
            01         50 A6     B1  0017  9900 5$:	CMPW	UCB$W_REFC(R6),#1	; IS SPOOLED DEV'S REF COUNT 1?
                          08     13  001B 10000 	BEQL	10$			; IF EQL YES
            7E       0004'8F     3C  001D 10100 	MOVZWL	#SS$_DEVASSIGN!4,-(SP)	; SET DEVICE ALREADY ASSIGNED
                          006C   31  0022 10200 	BRW	100$			; EXIT
            57         08 AC     D0  0025 10300 10$:	MOVL	UCB2(AP),R7		; GET ADDR OF INTER DEV'S UCB
         34 A7            0E     E1  0029 10400 	BBC	#DEV$V_FOD,UCB$L_DEVCHAR(R7),20$ ;IF CLEAR NON-FILE ORIENTED
                          05         002D       
         34 A7            1C     E0  002E 10500 	BBS	#DEV$V_RND,UCB$L_DEVCHAR(R7),30$ ;IF SET IT'S A DISK
                          07         0032       
            7E       0000'8F     3C  0033 10600 20$:	MOVZWL	#SS$_NOTFILEDEV,-(SP)	; SET NOT FILE DEVICE ERROR
                          57     11  0038 10700 	BRB	100$			; EXIT
         34 A7            13     E1  003A 10800 30$:	BBC	#DEV$V_MNT,UCB$L_DEVCHAR(R7),40$ ;IF CLEAR NOT MOUNTED
