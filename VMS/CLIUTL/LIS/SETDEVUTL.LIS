                                     0098 14700 	.SBTTL	SET NOSPOOLED
                                     0098 14800 ;++
                                     0098 14900 ; SET$DEV_NOSPOOL - KERNEL MODE ROUTINE TO SET A SPOOLED DEVICE NONSPOOLED
                                     0098 15000 ;
                                     0098 15100 ; FUNCTIONAL DESCRIPTION:
                                     0098 15200 ;
                                     0098 15300 ; THIS ROUTINE WILL DISABLE IMPLICIT SPOOLING FOR A DEVICE.
                                     0098 15400 ;
                                     0098 15500 ; 2) LOCK I/O DATABASE FOR WRITE;
                                     0098 15600 ; 3) CHECK THAT DEVICE IS ACTUALLY SET SPOOLED (DEV$V_SPL);
                                     0098 15700 ; 4) CHECK THAT REFERENCE COUNT (UCB$W_REFC) IS 2;
                                     0098 15800 ; 5) DECREMENT THE REFERENCE COUNT BY 2;
                                     0098 15900 ; 6) CLEAR SPOOL BIT (DEV$V_SPL);
                                     0098 16000 ; 7) DEALLOCATE THE QUEUE NAME'S DYNAMIC STORAGE, AND CLEAR VCB ADDRESS;
                                     0098 16100 ; 8) GET ADDRESS OF INTERMEDIATE DEVICE'S UCB FROM UCB$L_AMD AND
                                     0098 16200 ;    CLEAR UCB$L_AMD;
                                     0098 16300 ; 9) DECREMENT TRANSACTION COUNT OF INTERMEDIATE DEVICE'S VCB;
                                     0098 16400 ; 10) MOVE ADDRESS OF INTERMEDIATE DEVICE'S UCB TO CHANNEL CONTROL BLOCK
                                     0098 16500 ;     OF SPOOLED DEVICE;
                                     0098 16600 ; 11) UNLOCK I/O DATABASE;
                                     0098 16700 ;
                                     0098 16800 ;
                                     0098 16900 ; INPUTS:
                                     0098 17000 ;
                                     0098 17100 ;	UCB1(AP) = ADDRESS OF UCB OF SPOOLED DEVICE
                                     0098 17200 ;	CCB1(AP) = ADDRESS OF CCB OF SPOOLED DEVICE
                                     0098 17300 ;
                                     0098 17400 ; OUTPUTS:
                                     0098 17500 ;
                                     0098 17600 ;	R0 = STATUS OF FUNCTION
                                     0098 17700 ;--
                                     0098 17800 SET$DEV_NOSPOOL::
                               007C  0098 17900 	.WORD	^M<R2,R3,R4,R5,R6>
                                     009A 18000 ;
                                     009A 18100 ; LOCK I/O DATABASE, CHECK THAT NO ONE HAS ANY CHANNELS ASSIGNED TO
                                     009A 18200 ; THE DEVICE, CHECK THAT DEVICE IS ACTUALLY SPOOLED, AND CLEAR THE
                                     009A 18300 ; SPOOLED FLAG.
                                     009A 18400 ;
                          0071   30  009A 18500 	BSBW	LOCK_IODB		; LOCK THE I/O DATABASE
            56         04 AC     D0  009D 18600 	MOVL	UCB1(AP),R6		; GET ADDR OF SPOOLED DEV'S UCB
            02         50 A6     B1  00A1 18700 	CMPW	UCB$W_REFC(R6),#2	; ANY OTHER CHANNELS ASSIGNED?
                          07     15  00A5 18800 	BLEQ	10$			; IF LEQ NO
            7E       0004'8F     3C  00A7 18900 	MOVZWL	#SS$_DEVASSIGN!4,-(SP)	; SET DEVICE ASSIGNED ERROR
                          34     11  00AC 19000 	BRB	50$			; EXIT
                                     00AE 19100 
         34 A6            06     E4  00AE 19200 10$:	BBSC	#DEV$V_SPL,UCB$L_DEVCHAR(R6),20$ ; IF SET IT'S SPOOLED
                          08         00B2       
                 00038862 8F     DD  00B3 19300 	PUSHL	#CLI$_DEVNOTSPL		; SET NOT SPOOLED ERROR
                          27     11  00B9 19400 	BRB	50$			; EXIT
                                     00BB 19500 
                                     00BB 19600 ;
                                     00BB 19700 ; UNLOCK THE UCB'S AND VCB AND DEALLOCATE QUEUE NAME STORAGE
                                     00BB 19800 ;
                       50 A6     B7  00BB 19900 20$:	DECW	UCB$W_REFC(R6)		; "DE-ASSIGN" OUR CHANNEL
            50         30 A6     D0  00BE 20100 	MOVL	UCB$L_VCB(R6),R0	; SET ADDRESS OF QUEUE NAME
                 00000000'9F     16  00C2 20200 	JSB	@#EXE$DEANONPAGED	; DEALLOCATE ITS STORAGE
                       30 A6     D4  00C8 20300 	CLRL	UCB$L_VCB(R6)		; CLEAR QUEUE NAME ADDRESS
