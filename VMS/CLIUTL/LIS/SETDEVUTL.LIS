                          00         008B       
                       28 A6     D4  008C 13700 80$:	CLRL	UCB$L_PID(R6)		; CLEAR OUR OWNERSHIP
                                     008F 13800 ;
                                     008F 13900 ; UNLOCK I/O DATABASE AND RETURN STATUS
                                     008F 14000 ;
                          01     DD  008F 14100 	PUSHL	#1			; SET SUCCESS
                          008F   30  0091 14200 100$:	BSBW	UNLOCK_IODB		; UNLOCK I/O DATABASE
                          50   8ED0  0094 14300 	POPL	R0			; SET STATUS
                                 04  0097 14400 	RET
                                     0098 14500 
