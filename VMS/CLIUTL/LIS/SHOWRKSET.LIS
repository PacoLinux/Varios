                                     000C    70 	.SBTTL	SHOW$WORKING_S	- SHOW WORKING SET PARAMETERS
                                     000C    71 ;++
                                     000C    72 ; FUNCTIONAL DESCRIPTION:
                                     000C    73 ;	This routine retrieves the WORKING SET parameters from the current
                                     000C    74 ;	process header, formats and prints them on SYS$OUTPUT:
                                     000C    75 ;
                                     000C    76 ;
                                     000C    77 ; CALLING SEQUENCE:
                                     000C    78 ;
                                     000C    79 ;	CALLS 	SHOW$WORKING_S
                                     000C    80 ;	This routine is called by the CLI as a result of parsing option
                                     000C    81 ;	WORKING_SET of the SHOW command. (i.e , this routine is specified as
                                     000C    82 ;	a true action routine  address in the callback to the CLI in the
                                     000C    83 ;	SHOW option dispatcher.
                                     000C    84 ;
                                     000C    85 ; INPUT PARAMETERS:
                                     000C    86 ;
                                     000C    87 ;	4(AP)=	ADDRESS OF PARAMETER DESCRIPTOR THAT CAUSED THE CALL
                                     000C    88 ;		TO THIS ROUTINE
                                     000C    89 ;	8(AP)=	CLI$A_UTILSERV(AP) = ADDRESS OF CLI UTILITY SERVICE ROUTINE
                                     000C    90 ;
                                     000C    91 ; NOTE:		NO INPUT PARAMETERS ARE USED IN THIS OPTION
                                     000C    92 ;		 AS IT REQUIRES NO QUALIFIERS
                                     000C    93 ;
                                     000C    94 ; IMPLICIT INPUTS:
                                     000C    95 ;
                                     000C    96 ;	NONE
                                     000C    97 ;
                                     000C    98 ; OUTPUT PARAMETERS:
                                     000C    99 ;
                                     000C   100 ;	NONE
                                     000C   101 ;
                                     000C   102 ; IMPLICIT OUTPUTS:
                                     000C   103 ;
                                     000C   104 ;	DISPLAYS THE CURRENT PROCESS'S WORKING SET PARAMETERS ON SYS$OUTPUT:
                                     000C   105 ;
                                     000C   106 ; COMPLETION CODES:
                                     000C   107 ;
                                     000C   108 ;	NONE
                                     000C   109 ;
                                     000C   110 ; SIDE EFFECTS:
                                     000C   111 ;
                                     000C   112 ;	NONE
                                     000C   113 ;
                                     000C   114 ;--
                                     000C   115 
                                 00000000   116 	.PSECT	SHOW$CODE BYTE,RD,NOWRT,EXE
                               000C' 0000   117 	.ENTRY	SHOW$WORKING_S,^M<R2,R3>			; SHOW WORKING SET ROUITNE
            51   00000000'9F     D0  0002   118 	MOVL	@#CTL$GL_PHD,R1				; GET PROCESS HEADER ADDRESS
         08 A1            01     A3  0009   119 	SUBW3	#1,PHD$W_WSLIST(R1),R0			; BIAS TO FORM WORKING SET INDEX
                          50         000D       
         1A A1            50     A3  000E   120 	SUBW3	R0,PHD$W_DFWSCNT(R1),W^WORK_DEFAULT	; GET DEFAULT WORKING SET COUNT
                     0000'CF         0012       
         18 A1            50     A3  0015   121 	SUBW3	R0,PHD$W_WSQUOTA(R1),W^WORK_QUOTA		; GET WORKING SET QUOTA
                     0004'CF         0019       
         0A A1            50     A3  001C   122 	SUBW3	R0,PHD$W_WSAUTH(R1),W^WORK_AUTHORIZE	; GET MAXIMUM AUTHORIZED QUOTA
                     0008'CF         0020       
SHOW$WORKING_S  - SHOW WORKING SET PARAMETERS                    21-AUG-1978 19:26:13   VAX-11 MACRO X0.3-11               Page   4
X0101           SHOW$WORKING_S - SHOW WORKING SET PARAMETERS                                                                     (3)

                 00000000'8F     DD  0023   123 	PUSHL	#SHOW$_WORKSET				; MESSAGE IDENTIFIER
                     0000'CF     DF  0029   124 	PUSHAL	W^SHOW_WORK_LIST				; ARGUMENT LIST FOR $FAO
       0000'CF            02     FB  002D   125 	CALLS 	#2,W^SHOW$PRINT_MSG			; PRINT MESSAGE
       0000'CF   00000000'8F     D0  0032   126 	MOVL	#SS$_NORMAL,W^SHOW$L_STATUS		; store status
                                 04  003B   127 	RET						; RETURN
                                     003C   128 	.END
