                                     0108   138 ;++
                                     0108   139 ; FUNCTIONAL DESCRIPTION:
                                     0108   140 ;
                                     0108   141 ;	This routine performs the necessary calls to the CLI to parse the
                                     0108   142 ;	command line. True action routines are called for each of the
                                     0108   143 ;	qualifiers found present.
                                     0108   144 ;	The routine invoked if an input parameter is present parses the
                                     0108   145 ;	latter as a physical device name and, after validation,
                                     0108   146 ;	returns the parsed device name and unit in global locations.
                                     0108   147 ;
                                     0108   148 ; CALLING SEQUENCE:
                                     0108   149 ;
                                     0108   150 ;	CALLS	#2,SHOW$DEVICES
                                     0108   151 ;
                                     0108   152 ; INPUT PARAMETERS:
                                     0108   153 ;
                                     0108   154 ;	offset	CLI$A_UTILSERV	=	 address of the CLI utility service routine
                                     0108   155 ;
                                     0108   156 ; IMPLICIT INPUTS:
                                     0108   157 ;
                                     0108   158 ;	NONE
                                     0108   159 ;
                                     0108   160 ; OUTPUT PARAMETERS:
                                     0108   161 ;
                                     0108   162 ;	NONE
                                     0108   163 ;
                                     0108   164 ; IMPLICIT OUTPUTS:
                                     0108   165 ;
                                     0108   166 ;	NONE
                                     0108   167 ;
                                     0108   168 ; COMPLETION CODES:
                                     0108   169 ;
                                     0108   170 ;	R0	= status code
                                     0108   171 ;
                                     0108   172 ; SIDE EFFECTS:
                                     0108   173 ;
                                     0108   174 ;	NONE
                                     0108   175 ;
                                     0108   176 ;--
                                     0108   177 
