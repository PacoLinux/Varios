                                     006C   307 	.SBTTL	DEVI_DEVN_PRES	-ACTION ROUTINE FOR INPUT PARAMETER PRESENT
                                     006C   308 ;++
                                     006C   309 ; FUNCTIONAL DESCRIPTION:
                                     006C   310 ;
                                     006C   311 ;	Action routine for input parameter present.
                                     006C   312 ;
                                     006C   313 ;	Parses the input parameter as a physical device name, i.e:
                                     006C   314 ;		<alphabetic string> <digit string> < colon>
                                     006C   315 ;		   (mandatory)	      ( optional)   ( optional)
                                     006C   316 ;	If  valid, returns quadword descriptor of generic device name
                                     006C   317 ;	and binary value of unit (-1 if no unit specified) in the following
                                     006C   318 ;	global locations, respectively:
                                     006C   319 ;		DEVI$Q_DEVNAM
                                     006C   320 ;		DEVI$W_UNIT
                                     006C   321 ;	A error message is issued if an invalid string is detected.
                                     006C   322 ;
                                     006C   323 ;
                                     006C   324 ; CALLING SEQUENCE:
                                     006C   325 ;
                                     006C   326 ;	This routine is called by the CLI if an input parameter is found
                                     006C   327 ;	present.
                                     006C   328 ;
                                     006C   329 ; INPUT PARAMETERS:
                                     006C   330 ;
                                     006C   331 ;	NOT USED
                                     006C   332 ;
                                     006C   333 ; IMPLICIT INPUTS:
                                     006C   334 ;
                                     006C   335 ;	COMMAND LINE
                                     006C   336 ;
                                     006C   337 ; OUTPUT PARAMETERS:
                                     006C   338 ;
                                     006C   339 ;	DEVI$Q_DEVNAM	= QUADWORD DESCRIPTOR OF GENERIC DEVICE NAME STRING
                                     006C   340 ;	DEVI$W_UNIT	= BINARY UNIT NUMBER. ( =-1 MEANS NO UNIT SPECIFIED)
                                     006C   341 ;
                                     006C   342 ; IMPLICIT OUTPUTS:
                                     006C   343 ;
                                     006C   344 ;	NONE
                                     006C   345 ;
                                     006C   346 ; COMPLETION CODES:
                                     006C   347 ;
                                     006C   348 ;	SHOW$L_STATUS	= SS$_NORMAL	: SUCCESSFUL PARSING OF DEVICE NAME
                                     006C   349 ;			= SS$_IVDEVNAM	: INVALID DEVICE MAME
                                     006C   350 ;
                                     006C   351 ; SIDE EFFECTS:
                                     006C   352 ;
                                     006C   353 ;	NONE
                                     006C   354 ;
                                     006C   355 ;--
