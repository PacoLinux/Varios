                                     0000    52 ;
                                     0000    53 ; SHOW_DEFMS1 =	MACRO USED INTERNALLY BY SHOW_DEFMSG
                                     0000    54 ;
                                     0000    55 
                                     0000    56 	.MACRO	SHOW_DEFMS1	MSG_CODE,TXTSTRING,MSG_PAR,?L1,?L2
                                     0000    57 $TXT_COUNT$=$TXT_COUNT$+1		; COUNT OF MESSAGES CREATED THIS CALL
                                     0000    58 $temp1$=0				; FAO PARAMETER COUNT, IF PRESENT
                                     0000    59 	.IIF	NB,MSG_PAR,$temp1$=MSG_PAR@2
                                     0000    60 $temp2$=$MSG_CONTINUE$			; CONTINUATION FLAG
                                     0000    61 	.IIF	NE,$TXT_COUNT$-1,$temp2$=1
                                     0000    62 	.LONG	L2-L1			; BUILD DESCRIPTOR TO FAO CONTROL STRING
                                     0000    63 	.LONG	L1			;
                                     0000    64 	.BYTE	MSG_CODE			; CODE ASSOCIATED WITH MESSAGE
                                     0000    65 	.BYTE	$temp2$			; CONTINUATION FLAG
                                     0000    66 	.BYTE	$temp1$			; NUMBER OF FAO PARAMETERS REQUIRED
                                     0000    67 	.BYTE	0			; UNUSED
                                     0000    68 	.SAVE
                                     0000    69 	.PSECT	SHOW$MSG_TEXT	BYTE,RD,NOWRT,NOEXE
                                     0000    70 L1:	.ASCII	&TXTSTRING&			; FAO CONTROL STRING FOR MESSAGE
                                     0000    71 L2:
                                     0000    72 	.RESTORE
                                     0000    73 	.ENDM	SHOW_DEFMS1
                                     0000    74 
                                     0000    75 ;
                                     0000    76 ; COUNT_ARGS =		MACRO USED INTERNALLY BY SHOW_DEFMSG
                                     0000    77 ;
                                     0000    78 
                                     0000    79 	.MACRO	COUNT_ARGS	D1,D2,D3,D4,D5,D6,D7,D8,D9,D10,D11,D12,D13,D14,D15,D16,D17,D18,D1
                                     0000    80 	.NARG	$TXT_NB$
                                     0000    81 	.ENDM	COUNT_ARGS
                                     0000    82 
                                     0000    83 ;
                                     0000    84 ; SHOW_DEFMSG
                                     0000    85 ;
                                     0000    86 ;	THIS MACRO DEFINES A GLOBAL MESSAGE IDENTIFIER AND BUILDS ONE OR MORE
                                     0000    87 ;	MESSAGE TABLE ENTRIES, CONSISTING OF:
                                     0000    88 ;
                                     0000    89 ;	A 2 LONGWORDS DESCRIPTOR TO THE FAO CONTROL STRING OF THE
                                     0000    90 ;	MESSAGE, AND THE MESSAGE TEXT ITSELF.
                                     0000    91 ;
                                     0000    92 ;	ONE LONG WORD CONTAINING A CODE:
                                     0000    93 ;		BYTE 0:		0	= PRINT ON SYS$OUTPUT:
                                     0000    94 ;				1	= PRINT ONSYS$OUTPUT: AND SYS$ERROR:
                                     0000    95 ;		BYTE 1:		0	= LAST LINE OF MESSAGE
                                     0000    96 ;				1	= CONTINUATION LINE FOLLOWS
                                     0000    97 ;		BYTE 2:		LENGTH IN LONGWORDS OF FAO PARAMETER LIST FOR
                                     0000    98 ;				THIS LINE.
SHOW$MSG_TEXT   - MESSAGE TABLE TO SHOW UTILITY                  21-AUG-1978 19:20:57   VAX-11 MACRO X0.3-11               Page   3
X0101           DECLARATIONS                                                                                                     (2)

                                     0000    99 ;				( IGNORED IF CONTINUATION FLAG IS CLEAR)
                                     0000   100 ;
                                     0000   101 
                                     0000   102 		.MACRO	SHOW_DEFMSG	MSG_ID,MSG_TEXT,MSG_CODE
                                     0000   103 	.IIF	NDF,$MSG_COUNT$,$MSG_COUNT$=0
                                     0000   104 ;***	.IF	DF,'MSG_ID'			; SYMBOL ALREADY DEFINED ?
                                     0000   105 ;***	.IFT
                                     0000   106 	MSG_ID == $MSG_COUNT$		; DEFINE MESSAGE IDENTIFICATION SYMBOL
                                     0000   107 	$MSG_CONTINUE$=0		; MESSAGE CONTINUATION FLAG
                                     0000   108 ;***	.IFF
                                     0000   109 ;***	$MSG_CONTINUE$=1
                                     0000   110 ;***	.ENDC
                                     0000   111 $TXT_COUNT$=0				; COUNT FOR MESSAGES GENERATED
                                     0000   112 	COUNT_ARGS	MSG_TEXT		; COUNT NUMBER OF LINES TO GENERATE
                                     0000   113 	.IRP	TXTARG,<MSG_TEXT>		; REPEAT FOR EACH TEXT STRING
                                     0000   114 	SHOW_DEFMS1	MSG_CODE,TXTARG	; GENERATE THE STRING
                                     0000   115 	.ENDR
                                     0000   116 $MSG_COUNT$=$MSG_COUNT$+$TXT_COUNT$
                                     0000   117 	.ENDM	SHOW_DEFMSG
                                     0000   118 
                                     0000   119 ;
                                     0000   120 ; SHOW_DEFMSG_END =	GENERATES THE END OF THE MESSAGE TABLE
                                     0000   121 ;
                                     0000   122 
                                     0000   123 	.MACRO	SHOW_DEFMSG_END
                                     0000   124 	.QUAD	0		; NULL DESCRIPTOR
                                     0000   125 	.LONG	0		; CONTINUATION FLAG CLEAR
                                     0000   126 	.ENDM	SHOW_DEFMSG_END
                                     0000   127 
