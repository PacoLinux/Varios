                                     0009   115 
                                     0009   116 ;++
                                     0009   117 ; LIB$CVT_DECBIN - CONVERT NUMERIC STRING TO BINARY (ASSUMES DECIMAL DEFAULT)
                                     0009   118 ; LIB$CVT_OCTBIN - CONVERT NUMERIC STRING TO BINARY (ASSUMES OCTAL DEFAULT)
                                     0009   119 ; LIB$CVT_HEXBIN CLI CONVERT NUMERIC STRING TO BINARY (ASSUMES HEX. DEFAULT)
                                     0009   120 ;
                                     0009   121 ; THESE PROCEDURES ARE CALLED TO CONVERT AN ASCII NUMERIC STRING TO BINARY. THE
                                     0009   122 ; CONVERSION RADIX IS IMPLIED OR CAN BE EXPLICITELY SPECIFIED IN THE STRING
                                     0009   123 ; AS FOLLOWS:
                                     0009   124 ;
                                     0009   125 ;		NO RADIX SPECIFICATION - AS DETERMINED BY ENTRY POINT
                                     0009   126 ;
                                     0009   127 ;		STRING TERMINATED BY DECIMAL POINT = DECIMAL RADIX
                                     0009   128 ;
                                     0009   129 ;		%D PREFIX = DECIMAL RADIX
                                     0009   130 ;
                                     0009   131 ;		%O PREFIX = OCTAL RADIX
                                     0009   132 ;
                                     0009   133 ;		%X PREFIX = HEXADECIMAL PREFIX
                                     0009   134 ;
                                     0009   135 ; THE DECIMAL POINT IS ILLEGAL IF USED IN CONJUNCTION WITH OTHER RADIX SPECI-
                                     0009   136 ; FIERS.
                                     0009   137 ;
                                     0009   138 ; CALLING SEQUENCE:
                                     0009   139 ;
                                     0009   140 ;	CALL LIB$CVT_XXX (DESCR[,VALUE])
                                     0009   141 ;
                                     0009   142 ;
                                     0009   143 ; INPUT PARAMETERS:
                                     0009   144 ;
                                     0009   145 ;	DESCR = QUADWORD STRING DESCRIPTOR OF THE STRING TO BE CONVERTED
                                     0009   146 ;
                                     0009   147 ;	VALUE = OPTIONAL LONGWORD TO RECEIVE THE RESULT
                                     0009   148 ;
                                     0009   149 ; OUTPUTS:
                                     0009   150 ;
                                     0009   151 ;	R0 = CLI$_NORMAL, SUCCESS
                                     0009   152 ;
                                     0009   153 ;	R1 = CONVERTED VALUE
                                     0009   154 ;
