            50            01     D0  0019   192 	MOVL	#1,R0			; SET FOR DEFAULT HEX CONVERSION
                                     001C   193 5$:					;
            52         04 BC     7D  001C   194 	MOVQ	@STRNG(AP),R2		; GET COUNT AND ADDRESS OF STRING
            52            52     3C  0020   195 	MOVZWL	R2,R2			; EXTEND BYTE COUNT
                          3E     13  0023   196 	BEQL	20$			; IF EQL, NULL FIELD
                          55     D4  0025   197 	CLRL	R5			; ASSUME POSITIVE VALUE RETURNED
            2B            63     91  0027   198 	CMPB	(R3),#^A/+/		; PLUS SIGN SPECIFIED?
                          07     13  002A   199 	BEQL	7$			; IF EQL YES
            2D            63     91  002C   200 	CMPB	(R3),#^A/-/		; NEGATION SPECIFIED?
                          08     12  002F   201 	BNEQ	9$			; IF NEQ NO
                          55     D7  0031   202 	DECL	R5			; SET SIGN SPECIFIER NEGATIVE
                                     0033   203 7$:					;
                          53     D6  0033   204 	INCL	R3			; STRIP SIGN FROM STRING
                          52     D7  0035   205 	DECL	R2			; ...
                          2A     13  0037   206 	BEQL	20$			; IF EQL, NULL RESULT
                                     0039   207 9$:					;
            25            63     91  0039   208 	CMPB	(R3),#^A/%/		; RADIX SPECIFIED?
                          0E     13  003C   209 	BEQL	10$			; IF EQL YES
            2E         FF A342   91  003E   210 	CMPB	-1(R3)[R2],#^A/./	; STRING TERMINATED IN DECIMAL POINT?
                          1E     12  0043   211 	BNEQ	20$			; IF NEQ NO ASSUME DEFAULT
