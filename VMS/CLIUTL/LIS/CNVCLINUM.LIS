                                     0009   155 ;		THE CONVERTED VALUE IS RETURNED TO THE RESULT ARGUMENT (IF
                                     0009   156 ;		SPECIFIED).
                                     0009   157 ;
                                     0009   158 ;	R0 = CLI$_IVCHAR, INVALID CHARACTER IN NUMERIC STRING
                                     0009   159 ;	R0 = CLI$_NUMBER, INCORRECT NUMERIC FORMAT
                                     0009   160 ;
                                     0009   161 ;		ONE OF THE FOLLOWING ERROR CONDITIONS HAS BEEN DETECTED:
                                     0009   162 ;
                                     0009   163 ;			ILLEGAL RADIX SPECIFIER
                                     0009   164 ;			ILLEGAL CHARACTER
                                     0009   165 ;			DECIMAL POINT INCLUDED WITH RADIX SPECIFIER
