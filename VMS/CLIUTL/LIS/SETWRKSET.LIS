                                     00A1   220 	.SBTTL	SETLIM - SET LIMITS IN WORKING SET DATA BASE
                                     00A1   221 ;++
                                     00A1   222 ; FUNCTIONAL DESCRIPTION:
                                     00A1   223 ;
                                     00A1   224 ;	THIS ROUTINE STORES THE VALUES FROM THE ARGUMENT LIST
                                     00A1   225 ; INTO THE WORKING SET DATA BASE.  IT RUNS IN KERNEL MODE.
                                     00A1   226 ;
                                     00A1   227 ; CALLING SEQUENCE:
                                     00A1   228 ;
                                     00A1   229 ;	CALLG	ARGLIST,SETLIM
                                     00A1   230 ;
                                     00A1   231 ; INPUT PARAMETERS:
                                     00A1   232 ;
                                     00A1   233 ;	4(AP) = NEW DEFAULT WORKING SET COUNT
                                     00A1   234 ;	8(AP) = NEW WORKING SET QUOTA
                                     00A1   235 ;	IT IS ASSUMED THAT THE ABOVE VALUES HAVE BEEN VALIDATED
                                     00A1   236 ;
                                     00A1   237 ; IMPLICIT INPUTS:
                                     00A1   238 ;
                                     00A1   239 ;	NONE
                                     00A1   240 ;
                                     00A1   241 ; OUTPUT PARAMETERS:
                                     00A1   242 ;
                                     00A1   243 ;	R0 = SS$_NORMAL
                                     00A1   244 ;
                                     00A1   245 ; IMPLICIT OUTPUTS:
                                     00A1   246 ;
                                     00A1   247 ;	NONE
                                     00A1   248 ;
                                     00A1   249 ; COMPLETION CODES:
                                     00A1   250 ;
                                     00A1   251 ;	SS$_NORMAL
                                     00A1   252 ;
                                     00A1   253 ; SIDE EFFECTS:
                                     00A1   254 ;
                                     00A1   255 ;	NONE
                                     00A1   256 ;
                                     00A1   257 ;--
                               0000  00A1   258 SETLIM:	.WORD	0
