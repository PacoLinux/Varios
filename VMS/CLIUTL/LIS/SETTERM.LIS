                                     0000   203 ;++
                                     0000   204 ; SET$TERM - SET TERMINAL CHARACTERISTICS
                                     0000   205 ;
                                     0000   206 ; FUNCTIONAL DESCRIPTION:
                                     0000   207 ;
                                     0000   208 ; THIS ROUTINE SETS THE CHARACTERISTICS OF A SPECIFIED TERMINAL.
                                     0000   209 ; IT IS ENTERED AS A RESULT OF A DISPATCH FROM THE SET COMMAND PROCESSOR.
                                     0000   210 ;
                                     0000   211 ; INPUTS:
                                     0000   212 ;
                                     0000   213 ;	THE CLI DATA BASE IS SET UP. 
                                     0000   214 ;
                                     0000   215 ; OUTPUTS:
                                     0000   216 ;
                                     0000   217 ;	SET$L_STATUS = STATUS OF THE OPERATION
                                     0000   218 ;--
                                     0000   219 SET$TERM::				; SET TERMINAL COMMAND
                               03FC  0000   220 	.WORD	^M<R2,R3,R4,R5,R6,R7,R8,R9>
            56       0068'CF     9E  0002   221 	MOVAB	W^TERMNAME,R6		; ADDRESS QUAL DESC
                                     0007   222 	CALLBAK	(R6)			; CALL CLI FOR TERMINAL NAME
            56         08 A6     9E  0015   223 	MOVAB	CLI$Q_RQDESC(R6),R6	; ADDRESS NAME DESC
                          66     D5  0019   224 	TSTL	(R6)			; NAME PRESENT?
                          47     12  001B   225 	BNEQ	20$			; IF NEQ THEN ASSIGN THAT NAME
                                     001D   226 ;
                                     001D   227 ; DEFUALT NAME IS "SYS$COMMAND"
                                     001D   228 ;
            52            03     9A  001D   229 	MOVZBL	#3,R2			; SET UP TRANSLATE COUNTER
            56       0449'CF     9E  0020   230 	MOVAB	W^SYS_COMMAND,R6	; ADDRESS DESC FOR ORIGINAL NAME
       0010'CF       0000'CF     7D  0025   231 10$:	MOVQ	W^SHOW$GQ_OUT_DSC,W^UTIL_DESC
                                     002C   232 	$TRNLOG_S	(R6),W^UTIL_DESC,W^UTIL_DESC; TRANSLATE NAME
            1E            50     E9  0043   233 	BLBC	R0,20$			; IF LOW CLEAR THEN DONE
       0000'8F            50     B1  0046   234 	CMPW	R0,#SS$_NOTRAN		; DONE?
                          17     13  004B   235 	BEQL	20$			; IF EQL THEN YES
            66       0010'CF     7D  004D   236 	MOVQ	W^UTIL_DESC,(R6)	; SET UP FOR NEXT TRANSLATION
            1B         04 B6     91  0052   237 	CMPB	@4(R6),#^X01B		; PROCESS PERM?
                          09     12  0056   238 	BNEQ	15$			; IF NEQ THEN NO
         04 A6            04     C0  0058   239 	ADDL	#4,4(R6)		; SET NEW ADDRESS
            66            04     C2  005C   240 	SUBL	#4,(R6)			; AND SIZE
                          03     11  005F   241 	BRB	20$			; CONTINUE
            C1            52     F5  0061   242 15$:	SOBGTR	R2,10$			; CONTINUE UNTIL DONE
                                     0064   243 20$:	$ASSIGN_S	(R6),W^QIO+8	; ASSIGN CHANNEL TO DEVICE
            24            50     E9  0073   244 	BLBC	R0,ERROR		; IF LOW CLEAR THEN ERROR AND EXIT
                                     0076   245 ;
                                     0076   246 ; ASSUME SET OF TEMPORARY CHARACTERISTICS
                                     0076   247 ;
                                     0076   248 	$GETCHN_S	W^QIO+8,,W^GET_CHAN_INFO
            0B            50     E9  008C   249 	BLBC	R0,ERROR		; IF LOW CLEAR THEN ERROR
            50       0000'8F     3C  008F   250 	MOVZWL	#SS$_IVDEVNAM,R0	; ASSUME DEVICE NOT TERMINAL
       0020'CF            02     E0  0094   251 	BBS	#DEV$V_TRM,W^CHAN_INFO-4,SET_CHAR; BR IF NOT TERMINAL
                          06         0099       
                                     009A   252 ;
                                     009A   253 ; ERROR IN GETTING CHARACTERISTICS
                                     009A   254 ;
       0000'CF            50     D0  009A   255 ERROR:	MOVL	R0,W^SET$L_STATUS	; SAVE FOR EXIT STATUS
                                 04  009F   256 	RET				; RETURN
                                     00A0   257 ;
