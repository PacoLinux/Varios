SUBMIT %H!
JOB <%USER> * CREATE <LIB$.COLIB>
NOTIFY, ROUTE RECEPTION
COMP 2 SECS
LIMSTORE 100K
*
SW QUIET SW RESTORE
SET F 6 U 0 4096
SET CHAR END #
DELETE  <LIB$.COLIB>
LKED  (  MLIB  .COLIBNEW   BLIB  <BLIB$.COBLOBJ>
         LKIN   %H&    LIB  .BCPLOBJ   )                   #
   NAMEL  <LIB$.COLIB>/TRK5,3,2/F6      MAPL=MAP
  REPLACE  $MAIN$,BCPLMAIN,$BLOCK$,$CORTN$,$LOAD$,$TPUT$
  CHANGE  $BLOCK($BLOCK$)
  INCLUDE  MLIB
  NAME  $BLOCK$
  REPLACE  $MAIN$,BCPLMAIN,$BLOCK,$CORTN$,$BLOCK$,$TPUT$
  INCLUDE  MLIB
  NAME  $LOAD$
  REPLACE  $MAIN$,BCPLMAIN,$BLOCK,$CORTN$,$BLOCK$,$LOAD$
  INCLUDE  MLIB
  NAME  $TPUT$
  REPLACE  $MAIN$,BCPLMAIN,$BLOCK,$BLOCK$,$LOAD$,$TPUT$
  INCLUDE  MLIB
  INCLUDE  BLIB(CORTN)
  NAME  $CORTN$
  REPLACE  $BLOCK,$LOAD$,$TPUT$,$CORTN$
  INCLUDE  MLIB
  NAME  BCPLMAIN
  INCLUDE  LIB(IOS)
  NAME  $IOS$
  INCLUDE  LIB(PM)
  NAME  PM
  INCLUDE  BLIB(STANDARD)
  NAME  BLIB
&
!
