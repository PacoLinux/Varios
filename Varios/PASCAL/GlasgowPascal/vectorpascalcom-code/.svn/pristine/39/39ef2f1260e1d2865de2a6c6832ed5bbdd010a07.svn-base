Function Power( x,y: Float): Float;
Begin
  If y = 0 Then
    power := 1.0
  Else if x = 0 Then
    Power := 0.0
  Else If x > 0 Then
    Power := exp( y * ln(x))
  Else if Trunc(y) mod 2 = 0 Then
    Power := exp( y * ln(abs(x)))
  Else
    Power := -exp( y * ln(abs(x)));
End;

