    BigPtrMatrixInverse ( Dimen * 2, big, bigout, error);
    for i:=1 to Dimen do
      for j:=1 to Dimen do
      begin
        realinversematrix[i][j] :=  bigout^[i][j];
        imaginversematrix[i][j] := -bigout^[i][j+Dimen];
      end;
    dispose(bigout);
    dispose(big);
  end;

