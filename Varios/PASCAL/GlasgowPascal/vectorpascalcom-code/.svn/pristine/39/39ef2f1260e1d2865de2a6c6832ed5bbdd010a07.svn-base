Procedure MatrixMultiply         (    Dimen:             Integer;
                                  Var InputMatrix1:      Matrix;
                                  Var InputMatrix2:      Matrix;
                                  Var ProductMatrix:     Matrix);
Var I,J,K:Integer;
Begin
  For I:=1 to Dimen do
    For J:=1 to Dimen do
    Begin
      ProductMatrix[I][J] := 0;
      For K:=1 to Dimen do
        ProductMatrix[I][J] := ProductMatrix[I][J] +
                               InputMatrix1[I][K] * InputMatrix2[K][J];
    End;
End;

