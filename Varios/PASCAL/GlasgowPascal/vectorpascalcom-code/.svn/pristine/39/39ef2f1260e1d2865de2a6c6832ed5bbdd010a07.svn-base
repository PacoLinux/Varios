function I0(x: Float): Float;
var
  tnum,factor: integer;
  term,i0temp: Float;
begin
  term:=1;
  i0temp:=term;
  factor:=0;
  tnum:=1;
  repeat
    factor:=factor+2+2*(tnum-1);
    term:=term*sqr(x/2)/factor;
    i0temp:=i0temp+term;
    inc(tnum);
  until (abs(term)<1e-15) or (tnum=0);      {no overflow}
  i0:=i0temp;
end; {I0}

