    class Try extends Block {

        Try(Statement[] ok, Statement[] horrors) {
            super(ok);
            aversions = horrors;
        }

        public String toString() {
            if (outputFormat.equals("octaveC++")) {
                return toOctaveC();
            } else {
                return "try " + toString(actions) + " catch " + toString(aversions) + "end_try_catch";
            }
        }

        String toOctaveC() {

            String aversion = "aversion" + hashCode();
            String body = "\n";
            for (Statement s : actions) {
                String S = s.toString();
                body += "{" + prestatement;
                prestatement = "";
                body += tabs() + S + ";}";
                body += "\n" + tabs() + "if(error_state)goto " + aversion + ";\n";
            }
            body += tabs() + "   goto after" + aversion + ";" + aversion + ":";
            body += toString(aversions) + "\n" + tabs() + "after" + aversion + ":";
            return body;
        }
        // -------------------- fields
        Statement[] aversions;
    }

    class IfStatement extends Statement {
        //----------------------------- IfStatement constructor

        IfStatement(Expr cond, Statement[] iftrue, Statement[] iffalse) {
            condition = cond;
            trueActions = iftrue;
            falseActions = iffalse;
        }
        //----------------------------- IfStatement methods

        void visit(Visitor v) {
            condition.visit(v);
            for(Statement a:trueActions)a.visit(v);
            for(Statement b:falseActions)b.visit(v);
            v.examine(this);
        }
        public String toString() {
            if (outputFormat.equals("octaveC++")) {
                return toOctaveC();
            }
            return "if ( " + condition + ")\n" + toString(trueActions) + "\n\telse\n" + toString(falseActions) + "\nendif";
        }

        public String toOctaveC() {
            String prefix = "\n" + tabs() + "if ( " + booltest(condition) + "){";
            prefix = prestatement + prefix;
            prestatement = "";
            prefix += toString(trueActions) + "\n" + tabs() + "}";
            if (falseActions.length > 0) {
                prefix += "\n" + tabs() + "else{"
                        + toString(falseActions) + "\n" + tabs() + "}";
            }
            return prefix;
        }
        // ---------------------------- Variables of ifstatement
        Expr condition;
        Statement[] trueActions, falseActions;
    }

    class DoStatement extends WhileStatement {

        DoStatement(Statement[] list, Expr cond) {
            super(cond, list);
        }

        public String toString() {
            if (outputFormat.equals("octaveC++")) {
                return toOctaveC();
            }
            return "do\n" + toString(actions) + "until ( " + condition + ")\n";
        }

        public String toOctaveC() {
            String cond="localbool"+hashCode();
            String prefix = "\n" + tabs() + "do{";
            prefix= "int "+cond+";"+prefix;
            prefix = prestatement + prefix;
            prestatement = "";
            prefix += toString(actions);
            prestatement="";
            String last = "\n" + tabs() + cond+"="+booltest(condition)+";\n"+
                   tabs()+ "}while(!" + cond + ")";
           last = "\n"+tabs()+prestatement + last;
            prestatement = "";
            return prefix + last;
        }
    }

    class WhileStatement extends Statement {
        //------------------------------ While statement constructor

        WhileStatement(Expr cond, Statement[] list) {
