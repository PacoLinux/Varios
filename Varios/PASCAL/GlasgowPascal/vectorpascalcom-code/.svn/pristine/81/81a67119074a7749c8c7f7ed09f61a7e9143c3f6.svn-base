    class ForStatement extends Statement {

        // ------------------------------ ForStatement Constructor
        ForStatement(String var, Expr strt,/* Expr inc, Expr fin,*/ Statement[] l) {
            super();
            id = var;
            start = strt;
            identifiers.add(id.toString());
            /*  increment = inc;
            end = fin;*/
            list = l;
        }

        //-------------------- methods of ForStatement
        void visit(Visitor v) {

            start.visit(v);
            for(Statement s:list)s.visit(v);
            v.examine(this);
        }
        public String toString() {

            if (outputFormat.equals("octaveC++")) {
                return toOctaveC();
            }
            return "for " + id + "=" + start + "\n" + toString(list) + "\n\tend\n";
        }

        String toOctaveC() {
            if (simple()) {
                String prefix = "\n" + tabs() + "for(" + id + "=";
                prefix += init() + ";" + condition() + ";" + id + "+=" + increment() + "){\n";
                prefix = prestatement + "\n" + prefix;
                prestatement = "";
                prefix += toString(list) + "\n\t}";
                return prefix;
            }
            long unique = hashCode();
            String uniqueindex = "i" + unique + id;
            String uniqueend = "end" + unique + id;
            String prefix = "int " + uniqueindex + ";\n" + tabs();
            String uniquematrix = "M" + unique + start;
            prefix += "Matrix " + uniquematrix + "=" + start + ".matrix_value();\n" + tabs();
            prefix += "int " + uniqueend + "=" + start + ".columns();\n" + tabs();
            prefix += "for(" + uniqueindex + "=0;" + uniqueindex + "<" + uniqueend + ";" + uniqueindex + "++){\n";
            tabdepth++;
            prefix = prestatement + prefix;
            prestatement = "";

            prefix += "\n" + tabs() + id + "=" + uniquematrix + ".column(" + uniqueindex + ");\n" + toString(list) + "\n";
            tabdepth--;
            prefix += tabs() + "}";

            return prefix;
        }

        boolean simple() {
            // true if the for look is a simple iteration of the form
            // for i=a:b
            // or
            // for i=a:b:c
            if (!(start instanceof Dyadic)) {
                return false;
            }
            return ((Dyadic) start).op.equals(":");
        }

        Expr init() {
            // returns the first value of the loop
            return ((Dyadic) start).left;
        }

        String condition() {
            // builds a condition for a C for loop
            return "(" + id + "<=" + end() + ")";
        }

        Expr end() {
            // return the final value of the loop
            Dyadic d = (Dyadic) start;
            Expr r = d.right;
            if (!(r instanceof Dyadic)) {
                return r;
            }
            Dyadic rd = (Dyadic) r;
            if (rd.op.equals(":")) {
                return rd.left;
            }
            return rd;
        }

        Expr increment() {
            // return the step value of the loop
            Dyadic d = (Dyadic) start;
            Expr r = d.right;
            if (!(r instanceof Dyadic)) {
                return r;
            }
            Dyadic rd = (Dyadic) r;
            if (rd.op.equals(":")) {
                return rd.right;
            }
            return rd;
        }
        // ------------------------------ state of ForStatement
        String id;
        Expr start;
        Statement[] list;
    }

    class SyntaxError extends Exception {
        //----------------------------- constructor of the exception

        public SyntaxError(String s, int Line) {
            super(s);
            line = Line;
        }

        public SyntaxError(String s, int Line, int pos) {
            super(s);
            line = Line;
            cpos = pos;
        }
        //----------------------------- state of the exception
        public int line, cpos = 0;
        //------------------------------ method of the exception

        public String toString() {
            return "\nError on line " + line + ", " /*+ cpos*/ + " \t" + getMessage();
        }
    } // of SyntaxError

    class LexicalAnalyser {

        // this relies on SABLECC to have preprocessed the file into a linked list of lexemes
        // ------------------------ constructor of the lexer
        LexicalAnalyser(LinkedList<PLexeme> l) {
            lexemes = l.toArray(lexemes);
        }
        // ------------------------ state of the lexer
        PLexeme[] lexemes = new PLexeme[1];
        int current = 0;                   // currently scanned lexeme
        int previous = 0;                  // previous lexme
        int matrixdepth = 0;
        int line = 0;
        boolean list = true;
        String eof = "end of file";
        // ------------------------ methods of the lexer

        PLexeme currentLexeme() {
            if (current < lexemes.length) {
                return lexemes[current];
            }
            current = lexemes.length;
            AReservedLexeme r = new AReservedLexeme();
            r.setReservedsym(new TReservedsym(eof));// eof character
            return r;
        }

        void skip() {

            if (currentLexeme() instanceof ADelimiterLexeme) {
                PDelimiter d = ((ADelimiterLexeme) currentLexeme()).getDelimiter();
                if (d instanceof ALineDelimiter) {
                    TLinesym l = ((ALineDelimiter) d).getLinesym();
                    line = l.getLine() + 1;
                }
            }
            if (matrixdepth == 0) {
                if (currentLexeme() instanceof ASpaceLexeme) {
                    next();
                }
            }
        }

        void error(String s) throws SyntaxError {
            throw new SyntaxError(s, line);
        }

        String currentToken() // return current token as a string
        {
            skip();
            return currentLexeme().toString().trim();
        }

        void mustBeDelimiter() // generate an error if we do not have the correct token
                throws SyntaxError {
            if (haveDelimiter()) {
            } else {
                error(currentLexeme().toString().trim() + " found where , ; or newline was expected ");
            }
        }

        void mustBe(String token) // generate an error if we do not have the correct token
                throws SyntaxError {
            if (have(token)) {
            } else {
                error(currentLexeme().toString().trim() + " found where " + token + " was expected ");
            }
        }

        boolean have(String token) // tests for a token
        {
            String ct = currentToken();
            //    System.out.println("have("+token+") called with current token "+ct);
            if (token.equals(ct)) {
                next();
                return true;
            }
            return false;
        }

        boolean peek(String token) // tests for a token
        {
            String ct = currentToken();
            if (token.equals(ct)) {
                return true;
            }
            return false;
        }

        boolean haveInt() // test if we have an integer literal
        {
            skip();
            if (currentLexeme() instanceof AIntLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveNumber() // test if we have an integer literal
        {
            skip();
            if (currentLexeme() instanceof AIntLexeme || currentLexeme() instanceof AFloatLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveElementWiseSym() // test if we have a value
        {
            skip();
            if (currentLexeme() instanceof AElementwiseLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveExponentSym() //  a logical operator
        {
            skip();
            if (currentLexeme() instanceof AExponentiationLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveLogicalSym() //  a logical operator
        {
            skip();
            if (currentLexeme() instanceof ALogicalLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveMultiplydivideSym() // * / \
        {
            skip();
            if (currentLexeme() instanceof AMultiplyordivideLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveIncdecSym() // ++ --
        {
            skip();
            if (currentLexeme() instanceof AIncrementordecrementLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveNegateSym() // ++ --
        {
            skip();
            if (currentLexeme() instanceof ANegateLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveAdditiveSym() // + -
        {
            skip();
            if (currentLexeme() instanceof AAddorsubtractLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveIdentifier() // test if we have a value
        {
            skip();
            if (currentLexeme() instanceof AIdentifierLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveString() // test if we have a text value
        {
            skip();
            if (currentLexeme() instanceof AStringLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveDelimiter() // test if we have a delimiter
        {
            skip();
            if (currentLexeme() instanceof ADelimiterLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveLineDelimiter() // test if we have a ; or a line
        {
            skip();
            if (currentLexeme() instanceof ADelimiterLexeme) {
                PDelimiter pd = ((ADelimiterLexeme) currentLexeme()).getDelimiter();
                if (pd instanceof ALineDelimiter) {

                    next();
                    return true;
                }
                return have(";");
            }
            return false;
        }

        boolean haveRelationSym() // test if we have a relation
        {
            skip();
            if (currentLexeme() instanceof ARelationLexeme) {

                next();
                return true;
            }
            return false;
        }

        boolean haveSpace() // test if we have a space
        {
            if (currentLexeme() instanceof ASpaceLexeme) {

                next();
                return true;
            }
            return false;
        }

        void next() {
            if (list) {
                if (current < lexemes.length) {
                    System.out.print(lexemes[current]);
                }
            }
            previous = current;
            current++;
        }

        boolean haveValueDelimiter()// test if we have a space or a comma
        {
            if (have(",")) {
                return true;
            }
            return haveSpace();
        }
    }// of LexicalAnalyser
