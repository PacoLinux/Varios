            System.out.println("usage:");
            System.out.println("Mac   programfile    ");
            System.out.println("argcount =" + arguments.length + "\n" + arguments);
            System.exit(1);
        }
        Parser parser;
        boolean printSyntax = true;
        boolean printTypes = false;
        for (int f = 0; f < 1; f++) {
            try {
                String filename = arguments[f];
                if (!filename.endsWith(".m")) {
                    filename += ".m";
                }
                FileReader r = new FileReader(filename);

                PushbackReader pr = new PushbackReader(new BufferedReader(r), 1024);
                // we use sablecc to do the preliminary spliting into a stream of tokens
                // it does no substantial syntax analysis, all of the analysis is
                // explicitly coded in java by recursive descent
                Lexer lexer = new Lexer(pr);
                parser = new Parser(lexer);
                Node ast = parser.parse().getPFile();
                if (printSyntax) {
                    //    System.out.println(ast);
                }

                if (ast instanceof AFile) {
                    AFile fl = (AFile) ast;
                    LinkedList<PLexeme> lexemes = fl.getLexeme();
                    Mac compiler = new Mac(lexemes);
                    compiler.compile(filename);
                }

            } catch (Exception ex) {
                System.err.println(ex.toString());
                ex.printStackTrace(System.out);
            }
        }
    }
//-------------------------------- constructor of the compiler

    public Mac(LinkedList<PLexeme> lexemes) {
        lexer = new LexicalAnalyser(lexemes);
    }
//-------------------------------- state of the compiler------------------------
    LexicalAnalyser lexer;
    static String outputFormat = "octaveC++";
    static String preferedFormat = "unrolledC";
    static String prestatement = "";
    HashSet<String> identifiers = new HashSet<String>();
    HashSet paramidentifiers = new HashSet();
    HashMap<String,String> varmap = new HashMap<String,String>();// map matlab to C vars

    int tabdepth = 1;
//-------------------------------- methods of the compiler----------------------

    void compile() throws SyntaxError {
        Function f = parsefunctiondef();
        System.out.println("parsed");
        System.out.println(f.toString());
    }

    void compile(String fn) throws Exception {
        Function f = parsefunctiondef();
        if (fn.endsWith(".m")) {
            fn = fn.substring(0, fn.length() - 2);
        }
        PrintWriter p = new PrintWriter(new FileOutputStream(fn + ".cpp"));
        System.out.println("parsed");
        p.println(f.toString());
        p.close();
    }

    Function parsefunctiondef() throws SyntaxError {
        // this recotnises the grammar
        //functionmfdef =    fdefline Statement[]
        //                   |   fdefline;
        // fdefline = functionsym foutput eqsym identifiersym finput
        //                   |functionsym identifiersym finput;

        // it returns a function object
        skipLines();
        lexer.mustBe("function");
        String outputid = lexer.currentToken();
        String functionid = outputid;
        Statement[] body;
        String[] inputs;
        String[] o = {};
        if (lexer.haveIdentifier()) {
            // the identifier is either the output variable or the function name
            if (lexer.have("=")) {
                // it is the output id
                functionid = lexer.currentToken();
                if (!lexer.haveIdentifier()) {
                    lexer.error(functionid + " found where identifier expected");
                }
                inputs = parseInputs();
                body = parseBody();
                o = new String[1];
                o[0] = outputid; // convert string to an output array
            } else {
                inputs = parseInputs();
                body = parseBody();
            }

        } else {
            o = parseMultipleOutputs();
            lexer.mustBe("=");
            functionid = lexer.currentToken();
            if (!lexer.haveIdentifier()) {
                lexer.error(functionid + " found where identifier expected");
            }
            inputs = parseInputs();
            body = parseBody();
        }
        return new Function(functionid, o, inputs, body);
    }

    Statement[] parseBody() throws SyntaxError {
        //StatementList = delimiter statementlist;
        lexer.mustBeDelimiter();
        return parseStatementList();
    }

    Statement[] parseStatementList() throws SyntaxError {
        // statementlist =  statement
        //                |  statement delimiter
        //                |  statement delimiter statementlist
        //                ;
        Vector<Statement> list = new Vector<Statement>();
        Statement[] temp = new Statement[1];
        list:
        do {
            skipLines();
            if (lexer.have("until")) {
                break list;
            }
            if (lexer.have("end")) {
                break list;
            }
            if (lexer.have("end of file")) {
                break list;
            }
            if (lexer.have("endwhile")) {
                break list;
            }
            if (lexer.peek("case") || lexer.peek("otherwise")
                    || lexer.peek("endswitch") || lexer.peek("catch")
                    || lexer.peek("end_try_catch")) {
                break list;
            }
            temp[0] = parseStatement();
            list.add(temp[0]);
        } while (lexer.haveDelimiter());
        return list.toArray(temp);
    }

    String[] parseMultipleOutputs() throws SyntaxError {
        // multipleOutputs = '[' fargumentlist ']'
        lexer.mustBe("[");
        lexer.matrixdepth++;
        String[] list = parseFargumentList();
        lexer.mustBe("]");
        lexer.matrixdepth--;
        return list;
    }

    void skipLines() {
        while (lexer.haveDelimiter()) {
        }
    }

    String[] parseInputs() throws SyntaxError {
        // recognises the production rules
        // finput =
        //               | '(' ')'
        //               | '(' fargumentlist  ')'
        //               ;

        if (!lexer.have("(")) {
            String[] empty = {};
            return empty;
        } else {
            if (lexer.have(")")) {
                String[] empty = {};
                return empty;
            }
            String[] list = parseFargumentList();
            lexer.mustBe(")");
            return list;
        }
    }

    String[] parseFargumentList() throws SyntaxError {
        // fargumentlist = identifiersym
        //               |   identifiersym ',' fargumentlist
        Vector<String> list = new Vector<String>();
        String[] temp = new String[1];

        do {
            temp[0] = lexer.currentToken();
            if (lexer.haveIdentifier()) {
                list.add(temp[0]);
            } else {
                lexer.error("an identifier was expected not  " + temp[0]);
            }
        } while (lexer.have(",") || (lexer.matrixdepth > 0 && lexer.haveSpace()));
        return list.toArray(temp);
    }

    Statement parseStatement() throws SyntaxError {
        //  statement = commandformorassignment
        //                | forsym identifiersym eqsym expr delimitedinput endsym
        //                | ifsym ifcmdlist endsym
        //                | whilesym expr delimitedinput endwhilesym
        //                | returnsym
        //
        skipLines();
        if (lexer.have("for")) {
            return parseFor();
        }
        if (lexer.have("if")) {
            return parseIf();
        }
        if (lexer.have("while")) {
            return parseWhile();
        }
        if (lexer.have("do")) {
            return parseDo();
        }
        if (lexer.have("try")) {
            Statement[] actions = parseStatementList();
            lexer.mustBe("catch");
            Statement[] aversions = parseStatementList();
            lexer.mustBe("end_try_catch");
            return new Try(actions, aversions);
        }
        if (lexer.have("break")) {
            return new Break();
        }
        if (lexer.have("switch")) {
            return new Block(parseSwitch(parseExpr()));
        }
        return parseCommandOrAssign();
    }

    Statement[] parseSwitch(Expr e) throws SyntaxError {
        skipLines();
        if (lexer.have("case")) {
            Expr e2 = parseExpr();
            Statement st = new IfStatement(new Dyadic(e, "==", e2), parseStatementList(), parseSwitch(e));
            Statement[] stl = {st};
            return stl;
        }
        lexer.mustBe("otherwise");
        return parseStatementList();
    }

    DoStatement parseDo() throws SyntaxError {
//           lexer.mustBeDelimiter();
        Statement[] list = parseStatementList();
        //  lexer.mustBe("until");
        Expr condition = parseExpr();
        return new DoStatement(list, condition);
    }

    ForStatement parseFor() throws SyntaxError {
        // parses identifiersym eqsym expr:expr:expr delimitedinput endsym
        String id = lexer.currentToken();
        if (lexer.haveIdentifier()) {
            lexer.mustBe("=");
        }
        Expr start = parseExpr();
        //   lexer.mustBe(":");
        //   Expr increment = parseExpr();
        //   lexer.mustBe(":");
        //   Expr end = parseExpr();
        lexer.mustBeDelimiter();
        Statement[] list = parseStatementList();
        //  lexer.mustBe("end");
        return new ForStatement(id, start,/* increment, end,*/ list);
    }

    IfStatement parseIf() throws SyntaxError {
        Expr condition = parseExpr();
        lexer.mustBeDelimiter();
        Statement[] trueactions = parseStatementList();
        Statement[] falseactions = {};
        if (lexer.have("else")) {
            falseactions = parseStatementList();
        }
        if (lexer.have("end")) {
            return new IfStatement(condition, trueactions, falseactions);
        }
        if (lexer.have("elseif")) {
            return new IfStatement(condition, trueactions, parseElseif());
        } else {
            lexer.error("end expected");
        }
        return null;

    }

    Statement[] parseElseif() throws SyntaxError {
        Statement[] alternativ = {parseIf()};
        return alternativ;
    }

    WhileStatement parseWhile() throws SyntaxError {
        Expr cond = parseExpr();
        lexer.mustBeDelimiter();
        Statement[] list = parseStatementList();
        //    lexer.mustBe("endwhile");
        return new WhileStatement(cond, list);
    }

    Expr parseExpr() throws SyntaxError {
        // expr = expr8
        //                | expr8 elementwisesym  expr
        //                ;
        Expr left = parseExpr8();
        String op = lexer.currentToken();
        if (lexer.haveElementWiseSym()) {
            return new Dyadic(left, op, parseExpr());
        }
        return left;
    }

    Expr parseExpr8() throws SyntaxError {
        // expr8 = expr7
        //                | expr7 logicalsym  expr7
        //                ;
        Expr left = parseExpr7();
        String op = lexer.currentToken();
        if (lexer.haveLogicalSym()) {
            return new Dyadic(left, op, parseExpr8());
        }
        return left;
    }

    Expr parseExpr7() throws SyntaxError {
        // expr7 = expr6
        //                | expr6 relationsym  expr6
        //                ;
        Expr left = parseExpr6();
        String op = lexer.currentToken();
        if (lexer.haveRelationSym()) {
            return new Dyadic(left, op, parseExpr6());
        }
        return left;
    }

    Expr parseExpr6() throws SyntaxError {
        //  expr6 = expr5
        //                |  expr5 colonsym  expr5
        //                ;
        Expr left = parseExpr5();
        String op = lexer.currentToken();
        if (lexer.have(":")) {
            return new Dyadic(left, op, parseExpr5());
        }
        return left;
    }

    Expr parseExpr5() throws SyntaxError {
        //  expr5 = expr4
        //                |  expr4 additivesym  expr5
        //                ;
        Expr left = parseExpr4();
        String op = lexer.currentToken();
        if (lexer.haveAdditiveSym()) {
            return new Dyadic(left, op, parseExpr5());
        }
        return left;
    }

    Expr parseExpr4() throws SyntaxError {
        //  expr4 = expr3
        //                |expr3 multopsym expr4
        //                ;
        Expr left = parseExpr3();
        String op = lexer.currentToken();
        if (lexer.haveMultiplydivideSym()) {
            return new Dyadic(left, op, parseExpr4());
        }
        return left;
    }

    Expr parseExpr3() throws SyntaxError {
        // expr3 =   expr3 transposesym
        //                |postfixexpression
        //                ;
        Expr left = parsePostfixexpression();
        String op = lexer.currentToken();
        while (lexer.have("'")) {
            left = new Dyadic(left, op, null);
            op = lexer.currentToken();
        }
        return left;
    }

    Expr parsePostfixexpression() throws SyntaxError {
        //  postfixexpression =  expr2
        //		|postfixexpression incdecsym
        //                ;
        Expr left = parseExpr2();
        String op = lexer.currentToken();
        while (lexer.haveIncdecSym()) {
            left = new Dyadic(left, op, null);
            op = lexer.currentToken();
        }
        return left;
    }

    Expr parseExpr2() throws SyntaxError {
        //  expr2=  additivesym expr2
        //                |incdecsym expr2
        //                | negatesym expr2
        //		  |expr1
        //                ;
        String op = lexer.currentToken();
        if (lexer.haveIncdecSym()) {
            return new Dyadic(null, op, parseExpr2());

        }
        if (lexer.haveNegateSym()) {
            return new Dyadic(null, op, parseExpr2());

        }
        if (lexer.haveAdditiveSym()) {
            return new Dyadic(null, op, parseExpr2());

        }
        return parseExpr1();
    }

    Expr parseExpr1() throws SyntaxError {
        //  expr1 = expr0
        //                |expr0 exponentsym expr1
        //                ;
        Expr left = parseExpr0();
        String op = lexer.currentToken();
        if (lexer.haveExponentSym()) {
            return new Dyadic(left, op, parseExpr1());
        }
        return left;
    }

    Expr parseSubscript(String id) throws SyntaxError {
        boolean[] wildcard = new boolean[10];
        Expr[] list = {};
        Vector<Expr> v = new Vector<Expr>();
        int i = 0;
        if (lexer.have("(")) {
            do {
                if (lexer.have(":")) {
                    wildcard[i] = true;
                } else {
                    v.add(parseExpr());
                    wildcard[i] = false;
                }
                i++;
            } while (lexer.have(","));
            lexer.mustBe(")");
            boolean[] cards = new boolean[i];
            while (i > 0) {
                cards[i - 1] = wildcard[i - 1];
                i--;
            }
            return new Reference(id, cards, v.toArray(list));
        }
        return new Id(id);
    }

    Expr parseExpr0() throws SyntaxError {
        //expr0 = numbersym
        //                |  identifiersym
        //                |'(' brasym expr  ')'
        //                 ;
        // initially we ignore matrix literals

        String temp = lexer.currentToken();
        if (lexer.haveIdentifier()) {
            return parseSubscript(temp);
        }
        if (lexer.haveInt()) {
            return new IntLit(temp);
        }
        if (lexer.haveNumber()) {
            return new Lit(temp);
        }
        if (lexer.haveString()) {
            return new StringLit(temp);
        }
        if (lexer.have("[")) {
            lexer.matrixdepth++;  // change the line delimitor rules
            Matrix m = new Matrix(parseRows());
            lexer.mustBe("]");
            lexer.matrixdepth--;
            return m;
        }
        if (lexer.have("{")) {
            return parseCellArray();
        }
        lexer.mustBe("(");
        Expr e = parseExpr();
        lexer.mustBe(")");
        return e;

    }

    CellArray parseCellArray() throws SyntaxError {
        Expr[] row = parseRow();
        lexer.mustBe("}");
        Expr[][] rr = {row};
        return new CellArray(rr);
    }

    Expr[][] parseRows() throws SyntaxError {
        //rows = {null}
        //                |{single} row
        //                |{singlesemi} rows semicolonsym
        //                |{rr} rows semicolonsym row
        //                |{rl} rows linesym
        //                | {rlr}rows linesym row

        Vector<Expr[]> rows = new Vector<Expr[]>();
        Expr[] r = new Expr[0];
        do {
            r = parseRow();
            rows.add(r);
        } while (lexer.haveLineDelimiter());
        Expr[][] rr = {{}};
        return rows.toArray(rr);
    }

    Expr[] parseRow() throws SyntaxError {
        // parse a space or comma separated list of expressions
        Vector<Expr> row = new Vector<Expr>();
        Expr e;
        do {
            e = parseExpr();
            row.add(e);
        } while (lexer.haveValueDelimiter());
        Expr[] r = {};
        return row.toArray(r);
    }

    Statement parseCommandOrAssign() throws SyntaxError {
        //  String id=lexer.currentToken();
        //   if(lexer.haveIdentifier()){
        //       Expr  subscription = parseSubscript (id);
        Expr subscription = parseExpr();
        if (lexer.have("=")) {
            return new Assign(subscription, parseExpr());
        }
        if (!lexer.peek(";")) {
            return new Print(subscription);
        }
        return new Call(subscription);
        //    }
        // lexer.error("an identifier was expected but "+id+" was found");
        //    return null;
    }

    String tabs(int tb) {
        // return tabs for line layout
        String t = "";
        for (int i = tabdepth + tb; i > 0; i--) {
            t += "\t";
        }
        return t;
    }

    String tabs() {
        return tabs(0);
    }
//------------------------------------------------------------------------------
//-------------------------------- nested classes of the compiler

    abstract class Visitor {

        abstract void examine(Visitable thing);
    }

    abstract class Visitable {

        abstract void visit(Visitor v);
    }

    abstract class Mnode extends Visitable { // the parent class of the Matlab syntax tree

        //------------------------------ Mnode variables
        int lineNumber = lexer.line;
    }

    class Function extends Mnode {
        //---------------------- Constructor of a function

        Function(String n, String[] o, String[] i, Statement[] b) {
            name = n;
            output = o;
            input = i;
            body = b;
            for (String s : output) {
                identifiers.add(s);
                paramidentifiers.add(s);
            }
            for (String si : input) {
                identifiers.add(si);
                paramidentifiers.add(si);
            }
        }

        void visit(Visitor v) {
            for (Statement i : body) {
                i.visit(v);
            }
            v.examine(this);
        }

        public String toString() {
            if (outputFormat.equals("octaveC++")) {
                return toOctaveC();
            }
            return "function " + output + "=" + name + "(" + input + ")\n" + body;
        }

        String toOctaveC() {
            String temp = octaveCprelude;
            temp += name + ", args, ,\" \")\n{";
            temp += "\n\t/* -- actual function code   */\n\toctave_value_list retval;\n";
            for (int i = 0; i < input.length; i++) {
                temp += "\n\toctave_value " + input[i] + " =args(" + i + ");";
            }
            temp += "\n\t /* declare output variables */";
            for (int i = 0; i < output.length; i++) {
                temp += "\n\toctave_value " + output[i] + ";";
            }
            for (String s : identifiers) {
                if (!paramidentifiers.contains(s)) {
                    temp += "\n\toctave_value " + s + ";";
                }
            }
            temp += "\n\t /* do calculations */\n";
            for (int i = 0; i < body.length; i++) {
                if (body[i] != null) {

                    String line = "\n\t" + body[i] + ";";
                    temp += prestatement + tabs() + line;
                    prestatement = "";
                }

            }
            temp += "\n\t /* pack output variables */";
            for (int i = 0; i < output.length; i++) {
                temp += "\n\tretval( " + i + ")=" + output[i] + ";";
            }
            temp += "\n\treturn retval;\n }\n";
            return temp;
        }
        //---------------------- State of a Function
        String name;
        String[] output;
        String[] input;
        Statement[] body;
        static final String octaveCprelude = "#include<iostream>\n"
                + "#include <octave/oct.h>\n"
                + "#include <ov-colon.h>\n"
                + "#include <octave/parse.h>\nusing namespace std;\nDEFUN_DLD(";
    }

    abstract class Statement extends Mnode {

        String toString(Statement[] list) {
            String buf = prestatement;
            prestatement = "";
            tabdepth++;
            for (int i = 0; i < list.length; i++) {
                if (list[i] != null) {
                    String body= list[i].toString();
                    String thisline = ("\n" + tabs() + body + ";");
                    buf += "\n" + tabs() + prestatement  + thisline;
                    prestatement = "";
                }
            }
            prestatement = "";
            tabdepth--;
            return buf;
        }

        String booltest(Expr e) {
            int unique = hashCode();
            prestatement += "octave_value obool" + unique + "=" + e + ";";
            prestatement += "int bool" + unique + "=(obool" + unique + ".is_scalar_type ()?obool" + unique + ".bool_value():0);";
            return "bool" + unique;

        }
    }

    abstract class Expr extends Mnode {
    }

    class Matrix extends Expr {
        // ------------------------ Matrix abstract syntax contstructor

        Matrix(Expr[][] r) {
            rows = r;
        }
        String left = "[";
        String right = "]";

        public String toString() {
            String temp = left;
            long unique = hashCode();
            if (outputFormat.equals("octaveC++")) {
                int max = 0;
                for (Expr[] row : rows) {
                    if (row.length > max) {
                        max = row.length;
                    }
                }
                prestatement += "\n" + tabs(0) + "Matrix tMatrix" + unique + " (" + rows.length + "," + max + ");";
                for (int r = 0; r < rows.length; r++) {
                    for (int c = 0; c < rows[r].length; c++) {
                        prestatement += "\n" + tabs(0) + "tMatrix" + unique + "(" + (1 + r) + "," + (1 + c) + ")=" + rows[r][c] + ";";
                    }
                }
                return "tMatrix" + unique;
            }
            for (int i = 0; i < rows.length; i++) {
                Expr[] row = rows[i];
                for (Expr r : row) {
                    temp += "(" + r + ") ";
                }
                if (i < rows.length - 1) {
                    temp += ";";
                }
            }
            return temp + right;
        }
        // ------------------------ methods

        void visit(Visitor v) {
            for (Expr[] r : rows) {
                for (Expr e : r) {
                    e.visit(v);
                }
            }
            v.examine(this);
        }
        // ------------------------ fields
        Expr[][] rows;
    }

    class CellArray extends Matrix {

        CellArray(Expr[][] r) {

            super(r);


            left = "{";
            right = "}";
        }
    }

    class Lit extends Expr {

        public String toString() {

            return val;
        }

        Lit(String v) {
            val = v;
        }

        void visit(Visitor v) {
            v.examine(this);
        }
        String val;
    }

    class Id extends Lit {

        Id(String id) {
            super(id);
        }
        public String toString(){
            if(outputFormat.equals("unrolledC")){
                String mapped= varmap.get(val);
                return mapped;
            }
            return val;
        }
    }

    class Reference extends Id {
        // ------------------------- reference is an id that is subcripted

        Reference(String name, boolean[] Wildcards, Expr[] List) {
            super(name);
            list = List;
            wildcards = Wildcards;
        }

        public String toString() {
            long unique = hashCode();
            if (outputFormat.equals("octaveC++")) {
                if (!identifiers.contains(val)) {
                    //-------------------- this is a call
                    prestatement += "\n" + tabs() + "octave_value_list functionArguments" + unique + ";";
                    int j = 0;
                    for (int i = 0; i < wildcards.length; i++) {
                        if (wildcards[i]) {
                            // temp += ":";
                        } else {
                            String thisline = "\n" + tabs() + "functionArguments" + unique + "(" + i + ")=" + list[j] + ";";
                            prestatement += thisline;
                            j++;
                        }

                    }
                    String temp = "tempvar" + hashCode();
                    prestatement += "octave_value " + temp + ";";
                    prestatement += "\n" + tabs() + temp
                            + "=feval ( \"" + val + "\", functionArguments" + unique + "," + wildcards.length + ");";
                    return temp;
                }
            }
            prestatement += "\n" + tabs();

            String indexer = val + hashCode();
            String temp = val + ".do_index_op(" + indexer;
            prestatement += "octave_value_list " + indexer + ";\n" + tabs();

            int j = 0;
            for (int i = 0; i < wildcards.length; i++) {
                if (wildcards[i]) {
                    if (wildcards.length == 2) {
                        // it is a matrix
                        if (i == 0) {
                            // select a column
                            temp = val + ".matrix_value().column(" + indexer + "(1).int_value()-1";
                        } else {
                            // select a row
                            temp = val + ".matrix_value().row(" + indexer + "(0).int_value()-1";
                        }
                    } else {
                        String colonName = "colon" + (i + hashCode());
                        prestatement += "\n" + tabs() + "octave_magic_colon " + colonName + "();";
                        prestatement += indexer + "(" + i + ")=" + colonName + ";";
                    }
                } else {
                    String temp2 = list[j].toString();
                    j++;
                    prestatement += indexer + "(" + i + ")=" + temp2 + ";\n";
                }

            }
            return temp + ')';
        }
        //------------ fields  of a reference
        Expr[] list;
        boolean[] wildcards;
    }

    class IntLit extends Lit {

        public String toString() {

            return val;
        }

        IntLit(String v) {
            super(v);
        }
    }

    class StringLit extends Lit {

        StringLit(String v) {
            super(v);
        }

        public String toString() {
            if (outputFormat.equals("octaveC++")) {
                return "\"" + val.substring(1, val.length() - 1) + "\"";
            }
            return val;
        }
    }

    class Dyadic extends Expr {
        //--------------------------- Dyadic constructor

        Dyadic(Expr Left, String Op, Expr Right) {
            left = Left;
            right = Right;
            op = Op;
        }
        //------------------------------ methods

        void visit(Visitor v) {
            left.visit(v);
            right.visit(v);
            v.examine(this);
        }

        public String toString() {
            if (left == null) {
                if (op.equals("++")) {
                    return "( " + right + "+= 1)";
                }
                if (op.equals("++")) {
                    return "( " + right + "+=  - 1)";
                }
                return "(" + op + right + ")";
            }
            if (right == null) {
                 if(op.equals("++")){
                     String l=left.toString();
                    return "( "+l +"="+l+"+ 1)";
                }
                  if(op.equals("++")){
                    return "( "+left +"+=  - 1)";
                }
                return "(" + left + op + ")";
            }
            if (outputFormat.equals("octaveC++")) {
                if (op.equals("/")) {
                    if (!(left instanceof Id)) {
                        return "((  (double)" + left + ")" + op + right + ")";
                    }
                }
                if(op.startsWith(".")){
            		String oper=op.substring(1);
            		if (oper.equals("*"))return " op_el_mul( "+left+" ,"+right +")";
            		if (oper.equals("/"))return " op_el_div( "+left+" ,"+right +")";
            	}
               
            }
            String oper=op;
            if(outputFormat.equals("unrolledC")){
            	if(oper.startsWith(".")){
            		oper=oper.substring(1);
            		
            	}
            }
            return "(" + left + oper + right + ")";
        }
        // ---------------------------- variables of dyad
        Expr left, right;
        String op;
    }

    class Assign extends Statement {
        // ----------------------------- constructor for assign

        Assign(Expr Dest, Expr Src) {
            dest = Dest;
            src = Src;
            if (dest instanceof Id && !(dest instanceof Reference)) {
                identifiers.add(dest.toString());
            }
        }
        // -------------------- methods of Assign

        void visit(Visitor v) {
            dest.visit(v);
            src.visit(v);
            v.examine(this);
        }
        public String toString() {
            if (preferedFormat.equals("unrolledC")) {
                collectVariables();
                if(staticChecks())
                return "int "+switchvar()+"=0;\n"+tabs()+
                        "if(" + dynamicChecks() + "){" + decls()+  nakedCversion()
                        + "} else\n" + tabs() + dest + "=" + src;
            }
            return " " + dest + "=" + src;
        }
        String nakedCversion(){
            // this starts with a switch statement and then
            // contains a version for every combination of variables
            // being scalars or matrices that is allowed
            // they are switched to on the variable switchvar
            String prelude ="switch("+switchvar()+"){\n"+tabs();
            int range =(int)Math.pow(2, srcvars.size());
            for(int i=0;i<range;i++)
                prelude+= "case "+i+":"+nakedCversion(i)+"break;\n"+tabs();
            return prelude+tabs()+"}";
        }
        String nakedCversion(int i){
            // generate the ith version of the code with
            // the integer i encoding which variables are scalars
            int range = srcvars.size();
            int j=1;int unique=hashCode();
            String[] ids ={};
            ids = srcvars.toArray(ids);
            boolean matres=false;
            String varinit="";
            // set up the map from matlab to C variables
            for(int k=0;k<ids.length ;k++){
                if((i&j) !=0){
                    // it is a scalar
                    varmap.put(ids[k], ids[k]+unique+"T");
                    varinit+= "double "+ids[k]+unique+"T="+ids[k]+".scalar_value();\n"+tabs();

                }else{
                    // it is a matrix
                    varmap.put(ids[k], ids[k]+unique+"C["+" rowcount"+unique+"]");
                   
                    // generate the declarations for the temporary matrix forms and initialise the c pointers to them
                 //   varinit += "NDArray "+ids[k]+unique+"m="+ids[k]+".array_value();\n"+tabs();
                    varinit += ids[k]+unique+"C="+ids[k]+ ".array_value().fortran_vec();\n"+tabs();
                    // if this is the first matrix variable initialise myuniquerows and myuniquecols
                    if(!matres){
                    varinit += "my"+unique+"cols="+ids[k]+ ".columns();\n"+tabs();
                    varinit += "my"+unique+"rows="+ids[k]+ ".rows();\n"+tabs(); matres=true;
                    }
                }
                j++;
            }
            ids = destvars.toArray(ids);
            // set up the map from matlab to C variables

            if (!matres) {
                // it is a scalar
                varmap.put(ids[0], ids[0] /*+ unique + "C[0]"*/);
            } else {
                // it is a matrix
                varmap.put(ids[0], ids[0] + unique + "C[ "  + " rowcount" + unique + "]");
                varinit += "Matrix res"+unique+"(my"+unique+"rows, my"+unique+"cols);\n"+tabs();
                varinit += ids[0]+unique+"C=res"+unique+".fortran_vec();\n"+tabs();
                varinit += ids[0]+"=res"+unique+";\n"+tabs();

            }
            String oldformat=outputFormat;
            outputFormat = preferedFormat;
            String body="{\n"+tabs()+
                    "int my"+unique+"rows,rowcount"+unique+",colcount"+unique+
                    ",my"+unique+"cols"+
                    ";\n"+tabs();
            // now put in the initialisiation statements for
            // the c++ variables
            body+=varinit;
            if (matres) {
               
                body += "for(rowcount" + unique + "=0;rowcount" + unique + "<my" + unique + "rows*my"+unique+"cols;rowcount" + unique
                        + "++)\n" + tabs() + "\t";
            }
            body += dest+"="+src+";\n" +tabs()+"}";
            outputFormat =oldformat;
            return body;
        }

        String switchvar(){
            // this returns the name of a varialbe that is to be used to
            // test which code version to run
            return "switchvarz_"+hashCode();
        }
        boolean staticChecks(){
            // this checks to see if there are overriding reasons why
            // we should not do this in C code
            if(moreThanOneResult())return false;
            if(hascalls)return false;
            if(destNotSimpleVar())return false;
            if(hasmatrixops)return false;
            if(dataFlowHazard())return false;
            return true;
        }
        boolean destNotSimpleVar(){
            return !(dest instanceof Id);
        }
        boolean dataFlowHazard(){
            // return true if the dest variable appears on the right hand side
            return srcvars.contains(destvars.toArray()[0].toString());
        }
        boolean moreThanOneResult(){
            // returns true if there is more than one destination identifier
           return(destvars.size()>1) ;
        }
        String decls(){
            // this returns a string containing the C declarations
            // for the variables used in the nakedCVersion
            return srcDecls()+destDecls();
        }
        String srcDecls(){
            int unique=hashCode();
            String[] ids ={};
            ids = srcvars.toArray(ids);
            String res="";
            for(String i:ids)res+="double * "+i+unique+"C;\n"+tabs();
            return res;
        }
        String destDecls(){
            int unique=hashCode();
            String[] ids ={};
            ids = destvars.toArray(ids);
            String res="";
            for(String i:ids)res+="double * "+i+unique+"C;\n"+tabs();
            return res;
        }
        String dynamicChecks(){
            // this returns a string of C code containing run time
            // checks to see if code generation is worth while
            // initially we will check that src parameters are either
            // scalars or matrices
            String buf="true";
            int j=1;
            for(String i:srcvars){
                buf+= " \n&&("+i+".is_real_matrix()||("+i+".is_real_scalar()?"+
                        switchvar()+"+="+j
                        +",true:false))";
                j+=j;
            }
            
            return buf;
        }
        void collectVariables() // this finds all of the variables used in the assignment
        // and places them in srcvars and destvars
        // it also does other static checks
        {
            class Varfinder extends Visitor {

                void examine(Visitable v) {
                    if (v instanceof Id) {
                        String id = ((Id)v).val;
                        Ids.add(id);
                    }
                    if(v instanceof Call){
                        hascalls=true;
                    }
                    if(v instanceof Reference){
                        Reference rv=(Reference)v;
                        if(! identifiers.contains(rv.val))hascalls=true;
                    }
                    if(v instanceof Dyadic){
                        Dyadic dv=(Dyadic)v;
                        String o = dv.op;
                        if(o.equals("*")||
                                o.equals("**")||
                                o.equals("/")||
                                o.equals("^")||
                                o.equals("\\"))matrixops=true;
                    }
                }
                // variables of varfinder
                Set<String> Ids = new HashSet<String>();
                boolean hascalls=false;
                boolean matrixops=false;
            }
            Varfinder srcfinder = new Varfinder();
            Varfinder destfinder = new Varfinder();
            src.visit(srcfinder);
            srcvars = srcfinder.Ids;
            dest.visit(destfinder);
            destvars = destfinder.Ids;
            hascalls=srcfinder.hascalls||destfinder.hascalls;
            hasmatrixops=srcfinder.matrixops;
        }
        // ----------------------------- variables of assign
        Expr src, dest;
        Set<String> srcvars, destvars;
        boolean hascalls,hasmatrixops;
    }

