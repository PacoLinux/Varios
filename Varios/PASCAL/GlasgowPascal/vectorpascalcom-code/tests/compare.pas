program compare;
const data:array[1..4] of string[4]=
           ('abc','abcd','Abc','aba');
begin
	writeln('compare ''abc'' to:');
	write('  ',data,
	      '<>',  'abc'<>data,
	      '= ',  'abc'=data,	
	      '< ',  'abc'<data,
	      '> ',  'abc'>data,
	      '<=',  'abc'<=data,
	      '>=',  'abc'>=data);{}
end.

