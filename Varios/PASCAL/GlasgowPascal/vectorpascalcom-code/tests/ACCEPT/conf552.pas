{:tests vector product of  integer vectors                           }
program conf552;
const
	s:array[1..4] of string[16]=('dog','fish','bee','beans');
    i:array[1..4] of integer=(1,2,3,4);
    r:array[1..4] of real=(0.5,1.0,2.0,4.0);
	b:array[1..4] of boolean=(false,true,false,true);
var
    
	
    i1:array[1..4] of integer;
   
	
    r1:real;s1:string;
    j,k:integer;
begin
	 
  
	 
	i1:=i and b;
	j:=0;k:=\+i1;
	if (k)=6 then j:=j+1;
	r1:=\+r;
	if (r1)=7.5 then j:=j+1;
	s1:=\+s;
	if (s1)='dogfishbeebeans' then j:=j+1; 
	if j=3 then writeln('PASS CONF552  generalised vector product')
    else begin
    writeln ('i1,k,r1,s1');
    writeln( i1,k, r1, s1);
	writeln('FAIL CONF552 gneralised vector product  ' );
	
	end;
	 
end.
