        1        1        /*M*     B_DATA_D - DEFINITION OF VARIOUS SYSTEM WIDE DATA CELLS   */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /**/
        8        8        B_DATA_D: PROC;
        9        9        /**/
       10       10    1   DCL B$USRT$ PTR STATIC SYMDEF;          /* PTR TO USER TABLES - TIGR INITS    */
       11       11        /**/
       12       12    1   DCL B$P$ PTR STATIC SYMDEF;        /* PTR TO PROCESSOR TABLES - TIGR INITS    */
       13       13    1   DCL B$LOOKUP$ PTR STATIC SYMDEF;        /* PTR to lookup table - TIGR inits   */
       14       14    1   DCL P_NPROCS UBIN WORD STATIC SYMDEF  INIT(0);     /* NUMBER OF PROCESSORS   */
       15       15    1   DCL P_MAXPROCS UBIN WORD STATIC SYMDEF  INIT(0);  /* NUM. OF PROCESSOR SLOTS  */
       16       16        /* TIGR initializes all of this stuff */
       17       17    1   DCL P_AUTO_START UBIN WORD STATIC SYMDEF; /* Start of auto-share area in tbls */
       18       18    1   DCL P_LAST_AUTO UBIN WORD STATIC SYMDEF; /* Last slot used for auto-share     */
       19       19    1   DCL P_AUTOSPACE UBIN WORD STATIC SYMDEF INIT(1); /* Length of auto-share area */
       20       20    1   DCL P_USED_AUTO UBIN WORD STATIC SYMDEF; /* Last used auto-share slot         */
       21       21    1   DCL P_MAX_LOOKUP UBIN WORD STATIC SYMDEF INIT (0); /* # lookup tbl entries    */
       22       22        /**/
       23       23    1   DCL S_DL$ PTR STATIC SYMDEF;            /* PTR TO DOLIST TABLE - TIGR INITS   */
       24       24        /**/
       25       25    1   END;

