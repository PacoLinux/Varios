    8      %EQU          KZ#HIPEVENT_SPARE50      =50;
    9      %EQU          KZ#HIPEVENT_SPARE51      =51;
   10      %EQU          KZ#HIPEVENT_IDLE         =52;
   11      %EQU     KZ#HIPEVENT_SEND         =40; /* Send block on a device */
   12      %EQU     KZ#HIPEVENT_GPOLL        =41; /* Poll all active controllers */
   13      %EQU     KZ#HIPEVENT_SELECT       =42; /* Next device has been found */
   14      %EQU     KZ#HIPEVENT_GOTBLK       =43; /* Data block received from MLCP */
   15      %EQU     KZ#HIPEVENT_SEARCH       =44; /* Search for output block */
   16      %EQU     KZ#HIPEVENT_SWITCH       =45; /* Switch devices; all done */
   17      %EQU     KZ#HIPEVENT_WAKE         =46; /* Goose event out of catatonia */
   18      %EQU     KZ#HIPEVENT_INPUT        =47; /* Input is at hand, give to VDI */
   19      %EQU     KZ#HIPEVENT_POLALL       =48; /* Internal event- time to do poll of all controllers */
   20      %EQU     KZ#HIPEVENT_POLDUN       =49; /* Internal event- poll of all stations terminated */

