   10      /* FONT CLASS DEFINITIONS.  THEY ARE USED TO GET A QUICK IDEA OF */
   11      /* WHAT CHARACTER SET A PARTICULAR TRANSLATION TABLE DESCRIBES. */
   12
   13      %EQU KV_FNTCLS_ASC = 0;                 /* ASCII (NORMAL)                     */
   14      %EQU KV_FNTCLS_APLB = 1;                /* ASCII APL, BIT-PAIRED              */
   15      %EQU KV_FNTCLS_APLT = 2;                /* ASCII APL, TYPEWRITER-PAIRED       */
   16      %EQU KV_FNTCLS_EBC = 3;                 /* EBCDIC (NORMAL)                    */
   17      %EQU KV_FNTCLS_HI = 3;                  /* HI VALUE                           */

