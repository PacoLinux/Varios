        6        6        /*T***********************************************************/
        7        7        DAB$OCTDUMP: PROC(MEM,NWRDS,OFFSET,ASCFLG,LONGFLG);
        8        8    1   DCL MEM SBIN;
        9        9    1   DCL NWRDS SBIN;
       10       10    1   DCL OFFSET SBIN;
       11       11    1   DCL ASCFLG BIT(1) UNAL;
       12       12    1   DCL LONGFLG BIT(1) UNAL;
       13       13        /**/
       14       14    1   DCL DAF$WRITE EPTR SYMREF;
       15       15    1   DCL DAB$OUTPUT ENTRY(2) ALTRET;
       16       16        /**/
       17       17        %INCLUDE DA_ANLZ_R;
       18       30        %INCLUDE DA_MAP_R;
       19       60        %INCLUDE DA_FORMAT_R;
       20       72        %INCLUDE DA_FORMAT_C;
       21      314        %REPORT (NAME=REPORT,STCLASS="BASED(DA_REPORT$)");
       22      380        %INCLUDE XU_FORMAT_C;
       23      542        %F_FDS (NAME=DA_FDS,STCLASS=SYMREF);
       24      655        %INCLUDE B$TABLES_C;
       25      763        %B$XL_ASC_DUMP (NAME=DA_PRINTABLE_TBL);
       26      767        /**/
       27      768    1   DCL DAB_BUF2$ PTR STATIC SYMDEF;
       28      769    1   DCL DAB_STAR CHAR(1) STATIC SYMDEF;
       29      770    1   DCL DAB_LASTLINE BIT(1) ALIGNED STATIC SYMDEF;
       30      771    1   DCL DAB_LONG BIT(1) ALIGNED STATIC SYMDEF;
       31      772        /**/
       32      773    1   DCL BUF2 CHAR(256);
       33      774    1   DCL BUF1$ PTR;
       34      775    1   DCL OUTPUT EPTR;
       35      776    1   DCL AFLG SBIN;
       36      777    1   DCL X1 SBIN;
       37      778    1   DCL ENDW SBIN;
PL6.E3A0      #001=DAB$OCTDUMP File=DAB$OCTDUMP.:E05TSI                          TUE 07/29/97 12:39 Page:2    
       38      779    1   DCL WSZ SBIN;
       39      780    1   DCL WPL SBIN;
       40      781    1   DCL MEM$ PTR;
       41      782    1   DCL CSIZE SBIN;
       42      783    1   DCL MBASE CHAR(CSIZE) BASED(MEM$);
       43      784    1   DCL 1 DAT,
       44      785    1      2 WX SBIN,
       45      786    1      2 OTAB SBIN,
       46      787    1      2 WRDCNT SBIN,
       47      788    1      2 MEM CHAR(64),
       48      789    1      2 ATAB SBIN,
       49      790    1      2 ASCCNT SBIN,
       50      791    1      2 ASCII CHAR(64);
       51      792    1   DCL AVEC BIT(72);
       52      793    1   DCL TRTN REMEMBER ;
       53      794        /**/
       54      795    1   DCL FMT1 CHAR(0) CONSTANT INIT('%6ZO%N-%N(%12ZO %)%.19P%N-%N(%4A%)%1Q');
       55      796    1   DCL FMT2 CHAR(0) CONSTANT INIT('%6ZO%N-%N(%8ZY %)%.19P%N-%N(%4A%)%1Q');
       56      797        %EJECT;
PL6.E3A0      #001=DAB$OCTDUMP File=DAB$OCTDUMP.:E05TSI                          TUE 07/29/97 12:39 Page:3    
       57      798    1           WSZ=13;
       58      799    1           AVEC=VECTOR(FMT1);
       59      800    1           GOTO COMMON;
       60      801
       61      802    1   DAB$HEXDUMP: ENTRY(MEM,NWRDS,OFFSET,ASCFLG,LONGFLG);
       62      803
       63      804    1           WSZ=9;
       64      805    1           AVEC=VECTOR(FMT2);
       65      806    1   COMMON:;
       66      807    1           IF ADDR(LONGFLG) = ADDR(NIL) THEN
       67      808    1              DAB_LONG = '0'B;
       68      809    1           ELSE
       69      810    1              DAB_LONG = LONGFLG;
       70      811    1           DAB_BUF2$=ADDR(BUF2);
       71      812    1           BUF2=' ';
       72      813    1           BUF1$=DA_FDS.BUF_.BUF$;
       73      814    1           TRTN=DA_TRTN;
       74      815    1           REMEMBER FINI IN DA_TRTN;
       75      816    1           DAB_STAR=' ';
       76      817    1           OUTPUT=DA_FDS.OUTPUT#;
       77      818    1           DA_FDS.OUTPUT#=ENTADDR(DAB$OUTPUT);
       78      819    1           AFLG=BITBIN(ASCFLG);
       79      820    1           X1=(REPORT.PAGEWID.N-(8+AFLG))/(WSZ+4*AFLG);
       80      821    1           WPL=1;
       81      822    2           DO WHILE(X1>1);
       82      823    2              WPL=WPL*2;
       83      824    2              X1=X1/2;
       84      825    2              END;
       85      826    1           DAB_LASTLINE='0'B;
       86      827    1           MEM$=ADDR(MEM);
       87      828    1           DAT.WX=OFFSET;
       88      829    1           ENDW=DAT.WX+NWRDS;
       89      830    1           X1=MOD(OFFSET,WPL);
       90      831    2           DO WHILE DAT.WX<ENDW;
       91      832    2              DAT.WRDCNT=WPL-X1;
       92      833    3              IF DAT.WRDCNT+DAT.WX>=ENDW THEN DO;
       93      834    3                 DAT.WRDCNT=ENDW-DAT.WX;
PL6.E3A0      #001=DAB$OCTDUMP File=DAB$OCTDUMP.:E05TSI                          TUE 07/29/97 12:39 Page:4    
       94      835    3                 DAB_LASTLINE='1'B;
       95      836    3                 END;
       96      837    2              CSIZE=DAT.WRDCNT*4;
       97      838    2              DAT.OTAB=9+WSZ*X1;
       98      839    2              DAT.ATAB=10+WSZ*WPL+4*X1;
       99      840    2              DAT.MEM=MBASE;
      100      841    3              IF AFLG~=0 THEN DO;
      101      842    3                 CALL XLATE(DAT.ASCII,DA_PRINTABLE_TBL,DAT.MEM);
      102      843    3                 END;
      103      844    2              DAT.WX=DAT.WX-X1;
      104      845    2              DAT.ASCCNT=DAT.WRDCNT*AFLG;
      105      846    2              CALL DAF$WRITE (AVEC, VECTOR(DAT));
      106      847    2              DAT.WX=DAT.WX+WPL;
      107      848    2              MEM$=PINCRW(MEM$,DAT.WRDCNT);
      108      849    2              X1=0;
      109      850    2              END;
      110      851    1           DA_TRTN=TRTN;
      111      852    1           DA_FDS.BUF_.BUF$=BUF1$;
      112      853    1           DA_FDS.OUTPUT#=OUTPUT;
      113      854    1           RETURN;
      114      855    1   FINI:   DA_TRTN=TRTN;
      115      856    1           DA_FDS.BUF_.BUF$=BUF1$;
      116      857    1           DA_FDS.OUTPUT#=OUTPUT;
      117      858    1           UNWIND TO DA_TRTN;
      118      859    1   END DAB$OCTDUMP;
      119      860        %EOD;

