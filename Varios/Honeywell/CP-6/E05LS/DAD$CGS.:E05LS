     1407    22536        /*I* NAME:        DIDFR
     1408    22537             ENTRY:       DIDFR
     1409    22538             CALL:        CALL DIDFR;
     1410    22539             INPUT:       DFRFMT_ - VECTOR framing a string naming the head of
     1411    22540                                    the DFRBLK list to be displayed.
     1412    22541                          DFR$ - Unmapped pointer to head of DFRBLK list to display.
     1413    22542             SCRATCH:     Mapping context.
     1414    22543             DESCRIPTION: DIDFR displays the list of DFRBLKs headed by DFR$. */
     1415    22544
     1416    22545    1   DIDFR: PROC;
     1417    22546
     1418    22547    2   DCL CODETBL(0:10) CONSTANT CHAR(8) INIT(
     1419    22548    2         '??? 0','FETCH','RENEGE','DEL','??? 4','GET','WRITE','??? 7',
     1420    22549    2         '??? 8','GETFREE','GETNEW');
     1421    22550    2   DCL ADR UBIN;
     1422    22551    2   DCL P$ PTR;
     1423    22552    2   DCL UBIN$HALF UBIN HALF BASED HALIGNED;
     1424    22553    2   DCL 1 NAM BASED,
     1425    22554    2         2 L UBIN HALF HALIGNED,
     1426    22555    2         2 C CHAR(NAM.L);
     1427    22556
     1428    22557    2         CALL VALPTR (DFR$,AS0$) ALTRET(RET);
     1429    22558    2         CALL DAF$WRITE (DFRFMT_);
     1430    22559    3           DO WHILE(DFR$~=ADDR(NIL));
     1431    22560    3           DFRD = '0'B;
     1432    22561    3           CALL VALPTR(DFR$,AS0$,DFRD.SELF$.D,DFRD.SELF$.W) ALTRET(RET);
     1433    22562    3           CALL DAM$MAP(DFR$,M@DFR$) ALTRET(MAPERR);
     1434    22563    3           DFRD.CODE = CODETBL(KQ$DFRBLK.CODE);
     1435    22564    3           ADR = ADDR(KQ$DFRBLK.EPTR$)->UBIN$HALF;
     1436    22565    3           CALL DAS$IC2ED (ADR,P$) ALTRET(BLANK_IT);
     1437    22566    3           P$ = PINCRW(DA_DNAME$,P$->B$EDEF.NPOINTER);
     1438    22567    3           DFRD.ENTPTR = P$->NAM.C;
     1439    22568    4             DO WHILE('0'B);
     1440    22569    4   BLANK_IT: DFRD.ENTPTR = ' ';
     1441    22570    4             END;
     1442    22571    3           DFRD.GO = STARTBL(BITBIN(KQ$DFRBLK.GO));
     1443    22572    3           DFRD.EVNT = STARTBL(BITBIN(KQ$DFRBLK.EVNT));
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:53   
     1444    22573    3           DFRD.ABORT = STARTBL(BITBIN(KQ$DFRBLK.ABORT));
     1445    22574    3           DFRD.GFLG = STARTBL(BITBIN(KQ$DFRBLK.GFLG));
     1446    22575    3           IF KQ$DFRBLK.DA=0
     1447    22576    3           THEN DFRD.DA.N = 1;
     1448    22577    3           ELSE DFRD.DA.V = KQ$DFRBLK.DA;
     1449    22578    3           IF KQ$DFRBLK.DDA=0
     1450    22579    3           THEN DFRD.DDA.N = 1;
     1451    22580    3           ELSE DFRD.DDA.V = KQ$DFRBLK.DDA;
     1452    22581    3           IF KQ$DFRBLK.DBLK=0
     1453    22582    3           THEN DFRD.DBLK.N = 1;
     1454    22583    3           ELSE DFRD.DBLK.V = KQ$DFRBLK.DBLK;
     1455    22584    3           IF KQ$DFRBLK.DGRAN$=PS0$
     1456    22585    3           THEN DFRD.DGRAN$.N = 1;
     1457    22586    3           ELSE CALL VALPTR(KQ$DFRBLK.DGRAN$,AS0$,DFRD.DGRAN$.D,DFRD.DGRAN$.W,DFRD.
             22586                    DGRAN$.N1);
     1458    22587    3           IF KQ$DFRBLK.USR=0
     1459    22588    3           THEN DFRD.USR.N = 1;
     1460    22589    3           ELSE DFRD.USR.V = KQ$DFRBLK.USR;
     1461    22590    3           IF KQ$DFRBLK.ERRCODE=0
     1462    22591    3           THEN DFRD.ERR.N = 1;
     1463    22592    3           ELSE DFRD.ERR.V = KQ$DFRBLK.ERRCODE;
     1464    22593    3           IF KQ$DFRBLK.INFO=0
     1465    22594    3           THEN DFRD.INFO.N = 1;
     1466    22595    3           ELSE DFRD.INFO.V = KQ$DFRBLK.INFO;
     1467    22596    3   MAPERR: CALL DAF$WRITE (VECTOR(DFRBLKFMT),VECTOR(DFRD));
     1468    22597    3           DFR$ = KQ$DFRBLK.LNK$;
     1469    22598    3           END;
     1470    22599    2   RET:  RETURN;
     1471    22600    2   END DIDFR;
