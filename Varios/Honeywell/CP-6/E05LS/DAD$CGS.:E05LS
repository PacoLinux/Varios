     1373    22502
     1374    22503    1   DIMLH: PROC ALTRET;
     1375    22504
     1376    22505    2         MLHD = '0'B;
     1377    22506    2         MLHD.X.N = 'MLHmperr';
     1378    22507    2         CALL VALPTR (MLH$,AS0$,MLHD.X.D,MLHD.X.W);
     1379    22508    2         CALL DAM$MAP (MLH$,M@MLH$) ALTRET (MLHERR);
     1380    22509    2         MLHD.X.N = 'NODmperr';
     1381    22510    2         MLHD.Q = M@MLH$->KQ$MLH.QUEUE;
     1382    22511    2         CALL VALPTR (M@MLH$->KQ$MLH.HEAD$,AS0$,MLHD.H.D,MLHD.H.W,MLHD.H.N);
     1383    22512    2         CALL VALPTR (M@MLH$->KQ$MLH.TAIL$,AS0$,MLHD.T.D,MLHD.T.W,MLHD.T.N);
     1384    22513    2         TEMP$ = M@MLH$->KQ$MLH.DELAY$;
     1385    22514    2         ADDR(TEMP$)->KQ$DPTR.X = '0'B;
     1386    22515    2         CALL VALPTR (TEMP$,AS0$,MLHD.D.D,MLHD.D.W,MLHD.D.N);
     1387    22516    2         MLHD.S = M@MLH$->KQ$MLH.SUPERCNT;
     1388    22517    2         MLHD.C = M@MLH$->KQ$MLH.COUNT;
     1389    22518    2         IF M@MLH$->KQ$MLH.BUSY.RIP
     1390    22519    2         THEN MLHD.B = 'RIP';
     1391    22520    2         ELSE IF M@MLH$->KQ$MLH.BUSY.GIP
     1392    22521    2           THEN MLHD.B = 'GIP';
     1393    22522    2           ELSE MLHD.B = ' ';
     1394    22523    2         MLHD.RUSR = M@MLH$->KQ$MLH.RUSR#;
     1395    22524    2         IF M@MLH$->KQ$MLH.GACT THEN MLHD.G=1;
     1396    22525    2         MLHD.P = M@MLH$->KQ$MLH.PCSTAMP;
     1397    22526    2         MLHD.RQGC = M@MLH$->KQ$MLH.RQGC;
     1398    22527    2         MLHD.RQMEM = M@MLH$->KQ$MLH.RQMEM;
     1399    22528    2         CALL DAM$MAP (CUR$,M@CUR$) ALTRET(MLHERR);
     1400    22529    2         CALL XLATE(MLHD.X.N,DA_PRINTABLE_TBL,M@CUR$->KQ$STA.BTN.KEY);
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:51   
     1401    22530    2   MLHERR: CALL DAF$WRITE (VECTOR (MLHFMT0), VECTOR (MLHD));
