     1762    22891        %EJECT;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:64   
     1763    22892        /*I*  NAME:    FINDCG
     1764    22893              ENTRY:   FINDCG
     1765    22894              CALL:    CALL FINDCG ALTRET (not_found_or_not_valid_fid);
     1766    22895              INPUT:   TFID_NAME - name of CG to look for
     1767    22896                       TFID_ACCT - acct of CG to look for (default :SYS)
     1768    22897              OUTPUT:  CG$ - Pointer to selected comgroup
     1769    22898              DESCRIPTION: Search for a comgroup by name
     1770    22899        */
     1771    22900    1   FINDCG: PROC ALTRET;
     1772    22901
     1773    22902    2   DCL C$ PTR;
     1774    22903    2   DCL A$ PTR;
     1775    22904    2   DCL CTXT UBIN;
     1776    22905    2   DCL CFU UBIN;
     1777    22906    2   DCL CFU$ PTR;
     1778    22907
     1779    22908    2         CALL DAS$SDMONPTR (%M#KQ_MMSD,CG$) ALTRET (ARET);
     1780    22909    2         CALL DAM$MAP (CG$, M@CG$,,,DA_MONISDATA) ALTRET (ARET);
     1781    22910    2         CG$ = M@CG$->KQ$MMSD.CGHEAD$;
     1782    22911    2         CALL DAM$MAP (B$DS1$,M@F$CFU$) ALTRET (ARET);
     1783    22912    2         F$CFU$ = B$DS1$ ;
     1784    22913    3           DO WHILE (CG$~=ADDR(NIL));
     1785    22914    3           CALL VALPTR (CG$,B$CGCTXT$,,CTXT) ALTRET (NEXT);
     1786    22915    3           CALL DAM$MAP (CG$,M@CG$) ALTRET (NEXT);
     1787    22916    3           CFU$ = KQ$CG.CFU$;
     1788    22917    3           CALL VALPTR (CFU$,F$CFU$,,CFU) ALTRET (NEXT);
     1789    22918    3           CALL DAM$MAP (F$CFU$,M@F$CFU$) ALTRET (NEXT);
     1790    22919    3           C$ = PINCRW (M@F$CFU$,CFU);
     1791    22920    3           A$ = PINCRW (M@F$CFU$,C$->FM$CFU.ACCTX);
     1792    22921    4             DO WHILE (C$->FM$CFU.NAMEX < M@F$CFU$->FM$CFUX.CFUT);
     1793    22922    4             C$ = PINCRW (M@F$CFU$,C$->FM$CFU.NAMEX);
     1794    22923    4             END;
     1795    22924    3           C$ = PINCRC (M@F$CFU$,C$->FM$CFU.NAMEX);
     1796    22925    3           IF ( SUBSTR(TFID_NAME.NAME#,0,TFID_NAME.L#) =
     1797    22926    3           SUBSTR(C$->FM$CFUN.C,0,C$->FM$CFUN.L) ) AND
     1798    22927    3           ( A$->FM$CFUA.ACCT = TFID_ACCT.ACCT# ) THEN RETURN;
     1799    22928    3   NEXT:   CG$ = KQ$CG.LNK$;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:65   
     1800    22929    3           END;
     1801    22930
     1802    22931    2   ARET: ALTRETURN;
     1803    22932
     1804    22933    2   END;
     1805    22934        %EJECT;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:66   
     1806    22935        /*I* NAME:        VALPTR
     1807    22936             ENTRY:       VALPTR
     1808    22937             CALL:        CALL VALPTR (PTR$,VPTR$,D,W,N,C,B) ALTRET (invalid);
     1809    22938             INPUT:       PTR$ - Pointer to be validated.
     1810    22939                          VPTR$ - Prototype pointer for segid.
     1811    22940                          C - If addr (c) ~= addr (nil) then PTR$ may point to
     1812    22941                              an arbitrary character boundary.
     1813    22942                          B - If addr (c) ~= addr (nil) then PTR$ may point to
     1814    22943                              an arbitrary bit boundary.
     1815    22944             OUTPUT:      D - Segid D field of PTR$. (optional)
     1816    22945                          W - Word address field of PTR$. (optional)
     1817    22946                          N - Set to 1 if PTR$ = addr (nil), else 0. (optional)
     1818    22947                          C - Char address field of PTR$. (optional)
     1819    22948                          B - Bit address field of PTR$. (optional)
     1820    22949             DESCRIPTION: PTR$ must contain a pointer with the same S segid
     1821    22950                          field as VPTR$. In addition, it may be located on a
     1822    22951                          non word boundary only if C and/or B fields are
     1823    22952                          specified as input, i.e. not addr (nil). If VPTR$ has
     1824    22953                          an ASR segid then the D fields need not match. If PTR$
     1825    22954                          is found to be invalid then an on the fly error will
     1826    22955                          be produced, and the ALTRETurn will be taken, else if
     1827    22956                          valid then the normal RETURN will be taken with N set to 0.
     1828    22957                          Note that if PTR$ is addr (nil) then no error message
     1829    22958                          will be printed, D will be set to 88, W will be set
     1830    22959                          to .0000, N will be set to 1, and the ALTRETurn will
     1831    22960                          be taken. */
     1832    22961
     1833    22962    1   VALPTR: PROC (PTR$,VPTR$,D,W,N,C,B) ALTRET;
     1834    22963
     1835    22964    2   DCL PTR$ PTR;
     1836    22965    2   DCL 1 P REDEF PTR$,
     1837    22966    2         2 W UBIN(18) UNAL,
     1838    22967    2         2 C UBIN(2) UNAL,
     1839    22968    2         2 B UBIN(4) UNAL,
     1840    22969    2         2 S UBIN(2) UNAL,
     1841    22970    2         2 D UBIN(10) UNAL;
     1842    22971    2   DCL VPTR$ PTR;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:67   
     1843    22972    2   DCL 1 V REDEF VPTR$,
     1844    22973    2         2 W UBIN(18) UNAL,
     1845    22974    2         2 C UBIN(2) UNAL,
     1846    22975    2         2 B UBIN(4) UNAL,
     1847    22976    2         2 S UBIN(2) UNAL,
     1848    22977    2         2 D UBIN(10) UNAL;
     1849    22978    2   DCL D UBIN;
     1850    22979    2   DCL W UBIN;
     1851    22980    2   DCL N UBIN;
     1852    22981    2   DCL C UBIN;
     1853    22982    2   DCL B UBIN;
     1854    22983    2   DCL 1 E STATIC,
     1855    22984    2         2 W UBIN(18) UNAL INIT(1),
     1856    22985    2         2 C UBIN(2) UNAL INIT(3),
     1857    22986    2         2 B UBIN(4) UNAL INIT(8),
     1858    22987    2         2 S UBIN(2) UNAL INIT(2),
     1859    22988    2         2 D UBIN(10) UNAL INIT(99);
     1860    22989    2   DCL ERRFMT CHAR(0) CONSTANT INIT(
     1861    22990    2   '*** Invalid pointer %.2F will be displayed as %.2F');
     1862    22991
     1863    22992    2         IF PTR$ = ADDR (NIL) THEN
     1864    22993    3           DO;
     1865    22994    3           IF ADDR (N) ~= ADDR (NIL) THEN
     1866    22995    3             N = 1;
     1867    22996    3           IF ADDR (W) ~= ADDR (NIL) THEN
     1868    22997    3             W = BITBIN ('0000'O);
     1869    22998    3           IF ADDR (D) ~= ADDR (NIL) THEN
     1870    22999    3             D = 88;
     1871    23000    3           IF ADDR (C) ~= ADDR (NIL) THEN
     1872    23001    3             C = 0;
     1873    23002    3           IF ADDR (B) ~= ADDR (NIL) THEN
     1874    23003    3             B = 0;
     1875    23004    3           ALTRETURN;
     1876    23005    3           END;
     1877    23006
     1878    23007    2         IF ADDR (N) ~= ADDR (NIL) THEN
     1879    23008    2           N = 0;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:68   
     1880    23009    2         IF (P.S ~= V.S) OR
     1881    23010    2         (P.S ~= 2 AND P.D ~= V.D) OR
     1882    23011    2         (ADDR (C) = ADDR (NIL) AND P.C ~= 0) OR
     1883    23012    2         (ADDR (B) = ADDR (NIL) AND P.B ~= 0) THEN
     1884    23013    3           DO;
     1885    23014    3           CALL DAF$WRITE (VECTOR (ERRFMT),VECTOR (PTR$),VECTOR (E));
     1886    23015    3           IF ADDR (W) ~= ADDR (NIL) THEN
     1887    23016    3             W = E.W;
     1888    23017    3           E.W = E.W + 1;
     1889    23018    3           IF ADDR (D) ~= ADDR (NIL) THEN
     1890    23019    3             D = E.D;
     1891    23020    3           IF ADDR (C) ~= ADDR (NIL) THEN
     1892    23021    3             C = E.C;
     1893    23022    3           IF ADDR (B) ~= ADDR (NIL) THEN
     1894    23023    3             B = E.B;
     1895    23024    3           ALTRETURN;
     1896    23025    3           END;
     1897    23026
     1898    23027    2         IF ADDR (W) ~= ADDR (NIL) THEN
     1899    23028    2           W = P.W;
     1900    23029    2         IF ADDR (D) ~= ADDR (NIL) THEN
     1901    23030    2           D = P.D;
     1902    23031    2         IF ADDR (C) ~= ADDR (NIL) THEN
     1903    23032    2           C = P.C;
     1904    23033    2         IF ADDR (B) ~= ADDR (NIL) THEN
     1905    23034    2           B = P.B;
     1906    23035
     1907    23036    2         RETURN;
     1908    23037
     1909    23038    2   END VALPTR;
     1910    23039
     1911    23040    1   END DAD$CGS;

