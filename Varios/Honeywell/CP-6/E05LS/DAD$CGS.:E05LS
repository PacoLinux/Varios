     1670    22799        /*I* NAME:        SETUP
     1671    22800             ENTRY:       SETUP
     1672    22801             CALL:        CALL SETUP ALTRET (error);
     1673    22802             INPUT:       TOP - Top parse output block.
     1674    22803                          DA_CURRCG.CG$ - Pointer to current comgroup.
     1675    22804                          DA_ASR - ASR of current comgroup.
     1676    22805             OUTPUT:      CG$ - Pointer to current comgroup.
     1677    22806                          M@CG$ - Mapped pointer to current comgroup.
     1678    22807                          F$CFU$ - Pointer to CFU segment.
     1679    22808                          DA_CURRCG.CG$ - Pointer to current comgroup.
     1680    22809                          DA_ASR - ASR of current comgroup.
     1681    22810             DESCRIPTION: The pointers CG$ and F$CFU$ are setup. If TOP has
     1682    22811                          any sub-blocks then the new comgroup context address
     1683    22812                          is obtained, and the ASR is set with the ASR used to
     1684    22813                          address the new current comgroup. ALTRETurns if the
     1685    22814                          specified new comgroup context block and/or ASR could
     1686    22815                          not be setup, else RETURNs. */
     1687    22816
     1688    22817    1   SETUP: PROC ALTRET;
     1689    22818
     1690    22819    2   DCL CGID UBIN;
     1691    22820    2   DCL CCG$ PTR;
     1692    22821
     1693    22822    3         IF TOP.NSUBLKS ~= 0 THEN DO;
     1694    22823    4           IF TOP.SUBLK$(0)->OUT$BLK.NDTYPE = %FID# THEN DO;
     1695    22824    4             TEXTFID = TOP.SUBLK$(0)->OUT$SYM.TEXT;
     1696    22825    4             TEXTFIDL = TOP.SUBLK$(0)->OUT$SYM.COUNT;
     1697    22826    4             FPT_FID.TEXTFID_ = VECTOR(SUBSTR(TEXTFID,0,TEXTFIDL));
     1698    22827    5             CALL M$FID (FPT_FID) WHENALTRETURN DO;
     1699    22828    5               CALL DAA$ERRMSG (B$TCB$->B$TCB.ALT$->B$ALT.ERR);
     1700    22829    5               ALTRETURN;
     1701    22830    5               END;
     1702    22831    4             IF TFID_ACCT.ACCT# = ' ' THEN TFID_ACCT.ACCT# = ':SYS';
     1703    22832    4             CALL FINDCG ALTRET (NOCG);
     1704    22833    4             CCG$ = CG$;
     1705    22834    4             END;
     1706    22835    4           ELSE DO;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:62   
     1707    22836    4             CALL DAA$VALUE (TOP.SUBLK$ (0)->OUT$BLK,CGID) ALTRET (AERR);
     1708    22837    4             CCG$ = PINCRW (B$CGCTXT$,CGID);
     1709    22838    4             END;
     1710    22839    3           GOTO CMN;
     1711    22840    3           END;
     1712    22841    2   SETUPCUR: ENTRY ALTRET;
     1713    22842
     1714    22843        /*I* NAME:        SETUPCUR
     1715    22844             ENTRY:       SETUPCUR
     1716    22845             DESCRIPTION: Same as SETUP except that TOP is not used as a
     1717    22846                          possible new CG context address. The old context
     1718    22847                          is re-established if an old context exists, ALTRET
     1719    22848                          otherwise. */
     1720    22849
     1721    22850    2         CCG$ = DA_CURCG.CG$;
     1722    22851    2   CMN:  IF CCG$ = ADDR (NIL) THEN
     1723    22852    3           DO;
     1724    22853    3           CALL DAA$ERRMSG (E_DA_NO_CURCG);
     1725    22854        /*E* ERROR:    DAD-E$DA_NO_CURCG
     1726    22855             MESSAGE0: There is no currently selected comgroup to display. */
     1727    22856    3           ALTRETURN;
     1728    22857    3           END;
     1729    22858
     1730    22859    2         CALL DAS$SDMONPTR (%M#KQ_MMSD,CG$) ALTRET (AERR);
     1731    22860    2         CALL DAM$MAP (CG$,M@CG$,,,DA_MONISDATA) ALTRET (AERR);
     1732    22861    2         CG$ = M@CG$->KQ$MMSD.CGHEAD$; /* Pick up CGHEAD$ */
     1733    22862    2         CALL DAM$MAP (B$DS1$,M@F$CFU$) ALTRET (AERR);
     1734    22863    2         F$CFU$ = B$DS1$ ;
     1735    22864    3           DO WHILE (CG$ ~= ADDR (NIL));
     1736    22865    3           CALL DAM$MAP (CG$,M@CG$) ALTRET (AERR);
     1737    22866    3           IF CG$ = CCG$ THEN
     1738    22867    3             GOTO GOTCG;
     1739    22868    3           CG$ = KQ$CG.LNK$;
     1740    22869    3           END;
     1741    22870    2   NOCG:
     1742    22871    2         CALL DAA$ERRMSG (E_DA_NO_SETCG);
     1743    22872    2         ALTRETURN;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:63   
     1744    22873        /*E* ERROR:    DAD-E$DA_NO_SETCG
     1745    22874             MESSAGE0: Unable to access the currently selected comgroup. */
     1746    22875
     1747    22876    2   GOTCG:
     1748    22877    2         CALL VALPTR (KQ$CG.MMD.DSB$,B$CGCTXT$) ALTRET (NOCG);
     1749    22878    2         CALL DAM$MAP (KQ$CG.MMD.DSB$,M@DSB$) ALTRET (AERR);
     1750    22879    2         DA_ASR = M@DSB$->BIT$72DA;
     1751    22880    2         DA_CURCG.CG$ = CG$;
     1752    22881
     1753    22882    2         RETURN;
