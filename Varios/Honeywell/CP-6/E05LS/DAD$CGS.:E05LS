      694    21823        /*F* NAME:        DAD$MSGLIST
      695    21824             PURPOSE:     This routine does the MSGLIST command.
      696    21825             DESCRIPTION: This routine displays a list of MBLKs and/or
      697    21826                          super-MBLKs for the current comgroup. */
      698    21827        /*D* NAME:        DAD$MSGLIST
      699    21828             ENTRY:       DAD$MSGLIST
      700    21829             CALL:        CALL DAD$MSGLIST (TOP);
      701    21830             INPUT:       TOP - Parse output block for MSGLIST command.
      702    21831                          DA_CURCG - Current comgroup information.
      703    21832             SCRATCH:     Mapping context.
      704    21833             DESCRIPTION: Accepts the command:
      705    21834                             MSGLIST <location> [,<count>]
      706    21835                          and displays a list of MBLKs and/or super-MBLKs
      707    21836                          starting at <location> and continuing for <count>
      708    21837                          (super-)MBLKS or until the end of the referenced
      709    21838                          chain. If <count> is not specified, <count> is
      710    21839                          considered to be infinity. */
      711    21840
      712    21841    1   DAD$MSGLIST: ENTRY (TOP);
      713    21842
      714    21843    1         CALL SETUPCUR ALTRET(RETN);
      715    21844    1         CALL DAA$LOC (TOP.SUBLK$(0)->OUT$BLK,MBLK$) ALTRET (RETN);
      716    21845    1         IF TOP.NSUBLKS=2
      717    21846    1         THEN CALL DAA$VALUE (TOP.SUBLK$(1)->OUT$BLK,COUNT) ALTRET (RETN);
      718    21847    1         ELSE COUNT = BITBIN('377777777777'O);
      719    21848    1         CALL DAF$INIT (MSGLISTRPT);
      720    21849    2           DO I=1 TO COUNT;
      721    21850    2           MSGD = '0'B;
      722    21851    2           MSGD.NUM = I;
      723    21852    2           CALL VALPTR (MBLK$,AS0$,MSGD.X.D,MSGD.X.W) ALTRET (RETN);
      724    21853    2           CALL DAM$MAP (MBLK$,M@MBLK$) ALTRET(RETN);
      725    21854    2           CALL XLATE (MSGD.KEY1,DA_PRINTABLE_TBL,KQ$MBLK.KEY1);
      726    21855    2           CALL XLATE (MSGD.KEY2,DA_PRINTABLE_TBL,KQ$MBLK.KEY2);
      727    21856    2           MSGD.PRIO = KQ$MBLK.PRIO;
      728    21857    2           IF KQ$MBLK.LNK$=ADDR(NIL)
      729    21858    2           THEN MSGD.LNK = 'NIL';
      730    21859    2           ELSE MSGD.LNK = ' > ';
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:27   
      731    21860    2           MSGD.DDA = KQ$MBLK.DDA;
      732    21861    2           MSGD.UTS = KQ$MBLK.UTS;
      733    21862    2           IF KQ$MBLK.SUPER='0'B
      734    21863    3           THEN DO;
      735    21864    3             MSGD.S = ' ';
      736    21865    3             MSGD.STAR = STARTBL(BITBIN(KQ$MBLK.STAR));
      737    21866    3             MSGD.DVBYTE = KQ$MBLK.DVE.DVBYTE;
      738    21867    3             MSGD.MSGSIZE = KQ$MBLK.MSGSIZE;
      739    21868    3             MSGD.EV.EOM = KQ$MBLK.DVE.EOMCHAR;
      740    21869    3             CALL XLATE(MSGD.EV.VFC,DA_PRINTABLE_TBL,KQ$MBLK.DVE.VFC);
      741    21870    3             MSGD.MIDPRI = KQ$MBLK.MID.PRIMARY;
      742    21871    3             IF KQ$MBLK.MID.XT=0
      743    21872    3             THEN MSGD.MIDXTN = 0;
      744    21873    4             ELSE DO;
      745    21874    4               MSGD.MIDXTN = 1;
      746    21875    4               MSGD.MIDXT = KQ$MBLK.MID.XT;
      747    21876    4               END;
      748    21877    3             CALL DAF$WRITE (VECTOR(MBLKFMT),VECTOR(MSGD));
      749    21878    3             END;
      750    21879    3           ELSE DO;
      751    21880    3             MSGD.S = '*';
      752    21881    3             IF KQ$SMBLK.KEY1VALID='0'B
      753    21882    3             THEN MSGD.KEY1 = 'invalid ';
      754    21883    3             IF KQ$SMBLK.KEY2VALID='0'B
      755    21884    3             THEN MSGD.KEY2 = 'invalid ';
      756    21885    3             MSGD.KEY1ALL = STARTBL(BITBIN(KQ$SMBLK.KEY1ALL));
      757    21886    3             MSGD.KEY2ALL = STARTBL(BITBIN(KQ$SMBLK.KEY2ALL));
      758    21887    3             MSGD.COUNT = KQ$SMBLK.COUNT;
      759    21888    3             MSGD.PRIOLO = KQ$SMBLK.PRIOLO;
      760    21889    3             MSGD.UTSHI = KQ$SMBLK.UTSHI;
      761    21890    3             CALL VALPTR (KQ$SMBLK.INSHD$,AS0$,MSGD.INSHDD,MSGD.INSHDW,MSGD.INSHDN);
      762    21891    3             CALL DAF$WRITE (VECTOR(SMBLKFMT),VECTOR(MSGD));
      763    21892    3             END;
      764    21893    2           MBLK$ = KQ$MBLK.LNK$;
      765    21894    2           END;
      766    21895    1         RETURN;
      767    21896
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:28   
