      465    21594        /*F* NAME:        DAD$CGS
      466    21595             PURPOSE:     This routine does the CG SUMMARY command.
      467    21596             DESCRIPTION: This routine displays relevant data for each open
      468    21597                          comgroup. */
      469    21598        /*D* NAME:        DAD$CGS
      470    21599             ENTRY:       DAD$CGS
      471    21600             CALL:        CALL DAD$CGS (TOP);
      472    21601             INPUT:       TOP - Parse output block for CG SUMMARY command.
      473    21602             SCRATCH:     Mapping context.
      474    21603             DESCRIPTION: KQ_CGHEAD$ points to the first comgroup context
      475    21604                          block (KQ$CG). KQ$CG.LNK$ is the link to the next
      476    21605                          KQ$CG block. */
      477    21606
      478    21607    1         CALL DAS$SDMONPTR (%M#KQ_MMSD,CG$) ALTRET (AERR);
      479    21608    1         CALL DAM$MAP (CG$,M@CG$,,,DA_MONISDATA) ALTRET (AERR);
      480    21609    1         CG$ = M@CG$->KQ$MMSD.CGHEAD$; /* Pick up CGHEAD$ */
      481    21610    1         CALL DAM$MAP (B$DS1$,M@F$CFU$) ALTRET (AERR);
      482    21611    1         F$CFU$ = B$DS1$ ;
      483    21612    1         CALL DAF$INIT (CGSRPT);
      484    21613    2           DO WHILE (CG$ ~= ADDR (NIL));
      485    21614    2           CALL DI1CG ALTRET (RETN);
      486    21615    2           CG$ = KQ$CG.LNK$;
      487    21616    2           END;
      488    21617
      489    21618    2           DO WHILE ('0'B);
      490    21619    2   AERR:
      491    21620    2           CALL DAA$ERRMSG;
      492    21621    2           END;
      493    21622
      494    21623    1   RETN:
      495    21624    1         RETURN;
      496    21625
      497    21626        %EJECT;
PL6.E3A0      #001=DAD$CGS File=DAD$CGS.:E05TSI                                  TUE 07/29/97 12:40 Page:16   
