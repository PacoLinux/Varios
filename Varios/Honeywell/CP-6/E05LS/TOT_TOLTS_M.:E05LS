  963      /*F* NAME:         TOT$DMC
  964           DESCRIPTION:  This macro provides the Dynamic Memory Control
  965                         structure for TOLTS.
  966      */
  967
  968      %MACRO TOT$DMC (FPTN    = TOT$DMC,
  969                      STCLASS = STATIC) ;
  970         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  971         %IF (SUBSTR(STR,0,6) = 'STATIC')   OR
  972               (SUBSTR(STR,0,8) = 'CONSTANT') OR
  973               (SUBSTR(STR,0,3) = 'EXT');
  974            %LSET INIT = CHARTEXT('INIT');
  975         %ELSE;
  976            %LSET INIT = CHARTEXT('/*');
  977         %ENDIF;
  978
  979         %EQU TOO$IO_BUFFER_LGTH        = 14592 ;
  980         %EQU TOM$ITRTBL_LGTH           = 1725 ;
  981         %EQU TOM$SEGTBLS_LGTH          = 224 ;
  982
  983         DCL 1 FPTN STCLASS,
  984               2 TOO$IO_BUFFER      DALIGNED,
  985                 3 *                UBIN WORD UNAL
  986                                    %INIT (0) %CHARTEXT ('/**/'),
  987                 3 PTR$             PTR UNAL
  988                                    %INIT (ADDR(NIL)) %CHARTEXT ('/**/'),
  989               2 TOP$TCP            DALIGNED,
  990                 3 *                UBIN WORD UNAL
  991                                    %INIT (0) %CHARTEXT ('/**/'),
  992                 3 PTR$             PTR UNAL
  993                                    %INIT (ADDR(NIL)) %CHARTEXT ('/**/'),
  994               2 TOM$ITRTBL         DALIGNED,
  995                 3 *                UBIN WORD UNAL
  996                                    %INIT (0) %CHARTEXT ('/**/'),
  997                 3 PTR$             PTR UNAL
  998                                    %INIT (ADDR(NIL)) %CHARTEXT ('/**/'),
  999               2 TOM$SEGTBLS        DALIGNED,
15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        37   
 1000                 3 *                UBIN WORD UNAL
 1001                                    %INIT (0) %CHARTEXT ('/**/'),
 1002                 3 PTR$             PTR UNAL
 1003                                    %INIT (ADDR(NIL)) %CHARTEXT ('/**/') ;
 1004      %MEND ;
 1005      %EJECT ;

