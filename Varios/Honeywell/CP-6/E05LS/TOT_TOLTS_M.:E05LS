15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        12   
  304      /*F* NAME:         IS$
  305           DESCRIPTION:  This macro provides a declaration for a pointer
  306                         to the start of the Instruction Segment.
  307      */
  308
  309      %MACRO IS$ (FPTN    = IS$,
  310                  STCLASS = CONSTANT,
  311                  SEGID_V = %IS_SEGID) ;
  312
  313         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  314         %IF (SUBSTR(STR,0,6) = 'STATIC')     OR
  315               (SUBSTR(STR,0,8) = 'CONSTANT') OR
  316               (SUBSTR(STR,0,3) = 'EXT');
  317            %LSET INIT = CHARTEXT('INIT');
  318         %ELSE;
  319            %LSET INIT = CHARTEXT('/*');
  320         %ENDIF;
  321
  322         DCL 1 FPTN_ STCLASS ALIGNED,
  323               2 *  UBIN HALF UNAL %INIT (0) %CHARTEXT ('/**/'),
  324               2 *  UBIN HALF UNAL %INIT (SEGID_V) %CHARTEXT ('/**/') ;
  325         DCL FPTN REDEF FPTN_ PTR ALIGNED ;
  326      %MEND;
  327      %EJECT ;

15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        13   
  328      /*F* NAME:         MME
  329           DESCRIPTION:  This macro provides the MME structure declaration
  330                         for the Total On-Line Testing System.
  331      */
  332
  333      %MACRO MME (FPTN    = MME,
  334                  STCLASS = "") ;
  335
  336         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  337         %IF (SUBSTR(STR,0,6) = 'STATIC')     OR
  338               (SUBSTR(STR,0,8) = 'CONSTANT') OR
  339               (SUBSTR(STR,0,3) = 'EXT');
  340            %LSET INIT = CHARTEXT('INIT');
  341         %ELSE;
  342            %LSET INIT = CHARTEXT('/*');
  343         %ENDIF;
  344
  345         DCL 1 FPTN STCLASS ALIGNED,
  346               2 INSTR UBIN (36),
  347               2 INST REDEF INSTR,
  348                 3 NO SBIN (18) UNAL,
  349                 3 OP BIT  (18) UNAL,
  350               2 WD_1 UBIN (36),
  351               2 WD_2 UBIN (36),
  352               2 WD_3 UBIN (36),
  353               2 WD_4 UBIN (36),
  354               2 WD_5 UBIN (36),
  355               2 WD_6 UBIN (36) ;
  356      %MEND;
  357      %EJECT ;

15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        14   
  358      /*F* NAME:         MME_INSTR$
  359           DESCRIPTION:  This macro provides the MME INSTR ptr declaration
  360                         for the Total On-Line Testing System.
  361      */
  362
  363      %MACRO MME_INSTR$ (FPTN = MME_INSTR$) ;
  364
  365         DCL FPTN PTR ;
  366      %MEND;
  367      %EJECT ;

15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        15   
  368      /*F* NAME:         MREG
  369           DESCRIPTION:  This macro provides the MREG structure declaration
  370                         for the Total On-Line Testing System.
  371      */
  372
  373      %MACRO MREG (FPTN    = MREG,
  374                   STCLASS = "") ;
  375
  376         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  377         %IF (SUBSTR(STR,0,6) = 'STATIC')     OR
  378               (SUBSTR(STR,0,8) = 'CONSTANT') OR
  379               (SUBSTR(STR,0,3) = 'EXT');
  380            %LSET INIT = CHARTEXT('INIT');
  381         %ELSE;
  382            %LSET INIT = CHARTEXT('/*');
  383         %ENDIF;
  384
  385         DCL 1 FPTN STCLASS DALIGNED,
  386               2 X0    UBIN (18) UNAL,
  387               2 X1    UBIN (18) UNAL,
  388               2 X2    UBIN (18) UNAL,
  389               2 X3    UBIN (18) UNAL,
  390               2 X4    UBIN (18) UNAL,
  391               2 X5    UBIN (18) UNAL,
  392               2 X6    UBIN (18) UNAL,
  393               2 X7    UBIN (18) UNAL,
  394               2 A     UBIN (36) UNAL,
  395               2 Q     UBIN (36) UNAL,
  396               2 E     UBIN (36) UNAL ;
  397      %MEND;
  398      %EJECT ;

