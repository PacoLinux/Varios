  405         DCL FPTN SBIN HALF ;
  406      %MEND;
  407      %EJECT ;

15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        17   
  408      /*F* NAME: SCT
  409           DESCRIPTION:  This macro provides the SCT structure declaration
  410                         for the Total On-Line Testing System
  411      */
  412
  413      %MACRO SCT (FPTN = SCT,
  414                  STCLASS = "") ;
  415         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  416         %IF (SUBSTR(STR,0,6) = 'STATIC') OR
  417             (SUBSTR(STR,0,8) = 'CONSTANT') OR
  418             (SUBSTR(STR,0,3) = 'EXT');
  419           %LSET INIT = CHARTEXT('INIT');
  420         %ELSE;
  421           %LSET INIT = CHARTEXT('/*');
  422         %ENDIF;
  423         DCL 1 FPTN STCLASS ALIGNED,
  424               2 FICCDD ALIGNED,
  425                 3 FLAG# BIT (3) UNAL,
  426                 3 FORMAT# BIT (1) UNAL,
  427                 3 IOM# UBIN (2) UNAL,
  428                 3 CHANNEL# UBIN (18) UNAL,
  429                 3 DEVICE# UBIN (6) UNAL,
  430                 3 * UBIN (6) UNAL,
  431               2 TEST_TYPE UBIN (6) CALIGNED,
  432               2 DEVICE_CODE UBIN (6) CALIGNED,
  433               2 FIRMWARE,
  434                 3 TYPE# CHAR (4) CALIGNED,
  435                 3 LP# CHAR (4) CALIGNED,
  436                 3 RP# CHAR (4) CALIGNED,
  437               2 BIT17 BIT (1) CALIGNED,
  438               2 BIT13 BIT (1) CALIGNED,
  439               2 BIT12 BIT (1) CALIGNED,
  440               2 MODEL CHAR (8) CALIGNED,
  441               2 MPC_MODEL CHAR (8) CALIGNED;
  442      %MEND;
  443      %EJECT;

15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        18   
  444      /*F* NAME:         TOD$QUEUE
  445           DESCRIPTION:  This macro provides the DISPATCHER QUEUE
  446      */
  447
  448      %MACRO TOD$QUEUE (FPTN    = TOD$QUEUE,
  449                        STCLASS = STATIC) ;
  450         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  451         %IF (SUBSTR(STR,0,6) = 'STATIC')   OR
  452               (SUBSTR(STR,0,8) = 'CONSTANT') OR
  453               (SUBSTR(STR,0,3) = 'EXT');
  454            %LSET INIT = CHARTEXT('INIT');
  455         %ELSE;
  456            %LSET INIT = CHARTEXT('/*');
  457         %ENDIF;
  458
  459         DCL 1 FPTN STCLASS,
  460               2 E (0:15),
  461                 3 SERV_CODE        UBIN WORD ALIGNED
  462                                    %INIT (0*16) %CHARTEXT ('/**/'),
  463                 3 QENTRY,
  464                   4 *              UBIN WORD ALIGNED
  465                                    %INIT (0*16) %CHARTEXT ('/**/'),
  466                   4 *              UBIN WORD ALIGNED
  467                                    %INIT (0*16) %CHARTEXT ('/**/'),
  468                   4 *              UBIN WORD ALIGNED
  469                                    %INIT (0*16) %CHARTEXT ('/**/'),
  470               2 NEXT_SLOT          UBIN WORD ALIGNED
  471                                    %INIT (0) %CHARTEXT ('/**/'),
  472               2 CURR_SERVICE       UBIN WORD ALIGNED
  473                                    %INIT (0) %CHARTEXT ('/**/'),
  474               2 ENTRIES            UBIN WORD ALIGNED
  475                                    %INIT (0) %CHARTEXT ('/**/') ;
  476      %MEND ;
  477      %EJECT ;

15:17 JUL 28 '97 TOT_TOLTS_M.:E05SI                                        19   
  478      /*F* NAME:         TOD$SPA
  479           DESCRIPTION:  This macro provides the SPA declaration
  480                         for the Total On-Line Testing System.
  481           FAULT TYPES:  0 MME
  482                         1 MEMORY
  483                         2 FAULT TAG
  484                         3 COMMAND
  485                         4 DERAIL
