  708      /*F* NAME:         TOM$SEGTBL
  709           DESCRIPTION:  This macro provides a declaration for a segment
  710                         table for a test request.  This table has seven
  711                         entries  since an  NIO request may have up to 7
  712                         test files open at once.
  713      */
  714
  715      %MACRO TOM$SEGTBL (FPTN    = TOM$SEGTBL,
  716                         STCLASS = STATIC) ;
  717         %LSET STR = CONCAT (TEXTCHAR(STCLASS),'        ');
  718         %IF (SUBSTR(STR,0,6) = 'STATIC')   OR
  719               (SUBSTR(STR,0,8) = 'CONSTANT') OR
  720               (SUBSTR(STR,0,3) = 'EXT');
  721            %LSET INIT = CHARTEXT('INIT');
  722         %ELSE;
  723            %LSET INIT = CHARTEXT('/*');
  724         %ENDIF;
  725
  726         %EQU MAX_SEGS   = 7 ;
  727
  728         %EQU SEG_LGTH = 16593 ;
  729
  730         DCL 1 FPTN (0:%(MAX_SEGS - 1)) STCLASS,
  731               2 V DALIGNED,
  732                 3 *              BIT (36) UNAL
  733                                  %INIT ('0'B * %MAX_SEGS) %CHARTEXT ('/**/'),
  734                 3 PTR$           PTR UNAL
  735                                  %INIT (ADDR(NIL) * %MAX_SEGS) %CHARTEXT ('/**/'),
  736               2 ACTIVE           BIT (1) ALIGNED
  737                                  %INIT (%FALSE * %MAX_SEGS) %CHARTEXT ('/**/'),
  738               2 ENTRY#           UBIN HALF ALIGNED
  739                                  %INIT (0 * %MAX_SEGS) %CHARTEXT ('/**/') ;
  740      %MEND ;
  741      %EJECT ;

