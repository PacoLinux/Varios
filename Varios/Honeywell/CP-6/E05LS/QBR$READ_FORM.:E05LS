             17113            %SET LISTDIR = '0'B;
             17126    1     DCL 1 QCR@READ ALIGNED,
             17127    1           2 FRAME_START_INDEX UBIN,
             17128                          /* Starting index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             17129    1           2 FRAME_STOP_INDEX UBIN,
             17130                          /* Starting index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             17131    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             17132                          /* If IS_FIELD_INDEXES_VALID, use them;
             17133                             else use all the fields in the frame.  */
             17134    1           2 FIELD_START_INDEX UBIN,
             17135                          /* Starting index into QI$GZ_FIELD table.  */
             17136    1           2 FIELD_STOP_INDEX UBIN,
             17137                          /* Stopping index into QI$GZ_FIELD table.  */
             17138    1           2 IS_CURSOR_POSITION_VALID BIT(1) ALIGNED,
             17139                          /* If IS_CURSOR_POSITION_VALID, use it;
             17140                             else allow cursor to default to upper-left-most field.  */
             17141    1           2 CURSOR_POSITION BIT(16) ALIGNED,
             17142                          /* POSition of the field on which to place the cursor.  */
             17143    1           2 STATUS UBIN,
             17144                          /* Returned status code:  0 -> Immediate return to level 0;
             17145                                                   >0 -> Status is ok.  */
