      541     6188    1   GETMEM: PROC ALTRET;
      542     6189    2           IF DA_DFLT_RADIX=8 AND CNT(1)<=BITBIN('777777'O) AND CNT(0)<8
      543     6190    2           THEN MEMVEC.PTR$(0)->BHBIN=CNT(1);
      544     6191    2           ELSE MEMVEC.PTR$(0)->BUBIN=CNT(1);
      545     6192    2           IF CNT(4)>1 THEN DB_U#=CNT(2);
      546     6193    2           IF CNT(4)>2 THEN DB_D#=CNT(3);
      547     6194    2           IF DATSIZ=0 THEN RETURN;
      548     6195    2   FIXSIZ: ENTRY ALTRET;
      549     6196    2           I=FDS.MEMX;
      550     6197    2           IF I=0 THEN I=1;
      551     6198    2           MEMVEC.BOUND(I)=DATSIZ*32/DA_DFLT_RADIX-1;
      552     6199    2           IF DA_DFLT_RADIX=16
      553     6200    2           THEN CALL DBM$GETMEM (MEMVEC.PTR$(0)->BUBIN,MEMVEC.PTR$(I)->BUBIN,
      554     6201    2                DB_U#,DATSIZ*2,,DATSIZ*2,DB_D#) ALTRET(NOMEM);
      555     6202    3           ELSE IF DATSIZ=1 AND MEMVEC.PTR$(0)->BUBIN<262144 THEN DO;
      556     6203    3                 CALL DAM$MAP (MEMVEC.PTR$(0)->BUBIN,MEMVEC.PTR$(I),
      557     6204    3                   DB_U#,DB_D#) ALTRET(NOMEM);
      558     6205    3                 MEMVEC.BOUND(I)=BITBIN('3777777'O);
      559     6206    3                 END; ELSE DO;
      560     6207    3                 ADDR(MEMVEC.PTR$(I))->BUBIN=MEMVEC.PTR$(0)->BUBIN;
      561     6208    3                 CALL DAM$MAP (MEMVEC.PTR$(0)->BUBIN,MEMVEC.PTR$(I),
      562     6209    3                   DB_U#,DB_D#,MEMVEC(I)) ALTRET(NOMEM);
      563     6210    3                 END;
      564     6211    2           RETURN;
      565     6212    2   NOMEM:  ;
      566     6213    2           UNWIND TO FDS.BAILOUT;
      567     6214    2   END GETMEM;
      568     6215    1   END;

