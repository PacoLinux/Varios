      482     6129        /* Subroutines */
      483     6130    1   GETFMT: PROC ALTRET;
      484     6131
      485     6132    2   DCL CHAR4 CHAR(4) BASED CALIGNED;
      486     6133    2   DCL CHAR1 CHAR(1) BASED CALIGNED;
      487     6134    2   DCL CHARI CHAR(I) BASED CALIGNED;
      488     6135    2           CALL INDEX(I,'%)',SUBSTR(FMT,FDS.FMTX+2)) ALTRET(BADFMT);
      489     6136    3           IF CNT(0)=1 THEN DO;
      490     6137    3              IF DB_NXTX<I+9 THEN GOTO NOROOM;
      491     6138    3              PINCRC(DB_NXT$,DB_NXTX-4)->CHAR4=ADDR(CNT(1))->CHAR4;
      492     6139    3              DB_NXTX=DB_NXTX-5-I;
      493     6140    3              PINCRC(DB_NXT$,DB_NXTX+1)->CHARI=ADDR(SUBSTR(FMT,FDS.FMTX+2))->CHARI;
      494     6141    3              PINCRC(DB_NXT$,DB_NXTX)->CHAR1=BINASC(I);
      495     6142    3              RETURN;
      496     6143    3              END;
      497     6144    3           IF I~=FMTKEY.L OR SUBSTR(FMT,FDS.FMTX+2,I)~=FMTKEY.T THEN DO;
      498     6145    3              FMTKEY.L=I;
      499     6146    3              FMTKEY.T=SUBSTR(FMT,FDS.FMTX+2,I);
      500     6147    3              READFMT.BUF_.BUF$=FDS.FMT_.FMT$;
      501     6148    3              READFMT.BUF_.BOUND=2047;
      502     6149    4              CALL M$READ(READFMT) WHENALTRETURN DO;
      503     6150    4                 IF READFMT.V.DCB#=DCBNUM(M$FMTSYS) THEN GOTO NOFMT;
      504     6151    4                 READFMT.V.DCB#=DCBNUM(M$FMTSYS);
      505     6152    5                 CALL M$READ(READFMT) WHENALTRETURN DO;
      506     6153    5                    IF %M$FMT.FCD# THEN READFMT.V.DCB#=DCBNUM(M$FMT);
      507     6154    5                    FMTKEY.L=0;
      508     6155    5                    GOTO NOFMT;
      509     6156    5                    END;
      510     6157    4                 END;
      511     6158    3              FDS.FMTSIZE=DCBADDR(READFMT.V.DCB#)->BUBIN;
      512     6159    3              IF %M$FMT.FCD# THEN READFMT.V.DCB#=DCBNUM(M$FMT);
      513     6160    3              IF FDS.FMTSIZE<6 THEN GOTO NOFMT;
      514     6161    3              IF ADDR(FMT)->BUBIN~=0 OR CNT(1)~=0
      515     6162    3              THEN DATSIZ=ADDR(FMT)->BUBIN;
      516     6163    3              IF DATSIZ>1024 THEN GOTO BADSIZ;
      517     6164    3              SUBSTR(FMT,0,4)='%(%)';
      518     6165    3              END;
