      172     8053    2              END;
      173     8054    2           IF NOT TTLFLG THEN DO;
      174     8055    2   LLMORE:    CALL X$WRITE(FDS,VECTOR(TFMT),
      175     8056    2                VECTOR(MEMADR),
      176     8057    2                VECTOR(MEMWRDS),
