      274    18451    2             ENDING_INDEX = ORIGINAL_TARGET_LENGTH + STARTING_INDEX
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:9    
      275    18452    2               - QI@OP_ID_2_OR_5.LEN.S;
      276    18453    2           ELSE
      277    18454    2             ENDING_INDEX = ORIGINAL_TARGET_LENGTH + STARTING_INDEX
      278    18455    2               - QI@OP_ID_3_OR_6.LEN.S;
      279    18456
      280    18457    2         END INITIAL_STUFF;
      281    18458    1       ELSE
      282    18459    1   NO_INITIAL_STUFF:
      283    18460    2         DO;
      284    18461
