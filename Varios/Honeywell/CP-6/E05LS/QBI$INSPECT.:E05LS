PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:1    
        1        1        /*M* QBI$INSPECT:  INSPECT some dataname for things.  */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QBI$INSPECT
       10       10              PURPOSE:      INSPECT some dataname for things.
       11       11              DESCRIPTION:  Consult the FPL manual and the QI$OP_INSPECT %MACRO
       12       12                            for the various options on the INSPECT statement.
       13       13        */
       14       14
       15       15        QBI$INSPECT: PROC;
       16       16
       17       17        /*D*  NAME:         QBI$INSPECT
       18       18              CALL:         CALL QBI$INSPECT;
       19       19              PARAMETERS:   None.
       20       20              INTERFACE:    Called by QAT$TALLYHO when an INSPECT op-code is
       21       21                            encountered.
       22       22                            Calls QCS$SETUP_whatever,
       23       23                            all kinds of comparison, conversion, and move QCCs.
       24       24              ENVIRONMENT:  Nothing unusual.
       25       25              INPUT:        QI_1,
       26       26                            FPL rununit static data following the op-code.
       27       27              OUTPUT:       TALLY-ed data item; REPLACE-d data item.
       28       28              SCRATCH:      Various otter data.
       29       29              DESCRIPTION:  All sorts of convolutions of options are possible.
       30       30                            The noteable weird things include overpunched signs
       31       31                            within the dataname being INSPECT-ed, and a BINARY
       32       32                            item as the TALLY destination.
       33       33                            Although the compiler currently doesn't allow the
       34       34                            use of comparison strings longer than 1 character,
       35       35                            this code is (supposedly) able to handle longer
       36       36                            strings.  Someday, the compiler should be changed
       37       37                            to check only that the INSPECT-ed string and any
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:2    
       38       38                            REPLACE-ing strings are the same length.
       39       39                            There still exists some doubt as to what exactly
       40       40                            is to be done with signed DISPLAY items that are:
       41       41                            1)  being inspected;
       42       42                            2)  being inspected for.
       43       43                            COBOL seems inconsistent, but won't admit it
       44       44                            (a STAR against this was hastily, and I believe
       45       45                            wrongly, closed).
       46       46                            What's currently done is:
       47       47                            1)  the item being inspected has its sign stripped
       48       48                                and replaced again ONLY if it's overpunched;
       49       49                            2)  the item being inspected for is left alone
       50       50                                (currently, it can only be one digit long,
       51       51                                which excludes "SIGN ... SEPARATE" items).
       52       52        */
       53       53
       54       54          %INCLUDE LCP_6;
       55      245
       56      246          %INCLUDE QG_MACRO_M;
       57      383          %INCLUDE QI_EQUATES_E;
       58      742          %INCLUDE QI_MACROS_M;
       59     5800          %QI$ALL;
       60    16913
       61    16914    1     DCL QCS$SETUP_SOURCE ENTRY;
       62    16915    1     DCL QCS$SETUP_TARGET ENTRY;
       63    16916    1     DCL QCS$SETUP_AUX ENTRY;
       64    16917    1     DCL QCC$CBD ENTRY (1) ALTRET;
       65    16918    1     DCL QCC$CDB ENTRY (1) ALTRET;
       66    16919    1     DCL QCC$DAD ENTRY (1) ALTRET;
       67    16920    1     DCL QCC$DMC ENTRY (1) ALTRET;
       68    16921
       69    16922    1     DCL ENDING_INDEX SBIN;
       70    16923    1     DCL INSTR$ PTR;
       71    16924    1     DCL IT_WAS_OVERPUNCHED BIT(1) ALIGNED;
       72    16925    1     DCL IT_WAS_POSITIVE BIT(1) ALIGNED;
       73    16926    1     DCL ORIGINAL_TARGET_LENGTH UBIN;
       74    16927    1     DCL OVERPUNCHED_CHAR UBIN BYTE ALIGNED;
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:3    
       75    16928    1     DCL STARTING_INDEX SBIN;
       76    16929    1     DCL TALLIED_ITEM_IS_A_TEMP BIT(1) ALIGNED;
       77    16930    1     DCL TALLIED_ITEM_DAD$ PTR;
       78    16931    1     DCL TALLIED_ITEM_TYPE UBIN;
       79    16932    1     DCL TALLY UBIN;
       80    16933    1     DCL VARIABLE_SOURCE$ CPTR;
       81    16934    1     DCL VARIABLE_TARGET$ CPTR;
       82    16935
       83    16936          %QI@CIP_OPERATION ( NAME = QI@OP_ID_2_OR_5,
       84    16937                              STCLASS = "AUTO" );
       85    17573          %QI@CIP_OPERATION ( NAME = QI@OP_ID_3_OR_6,
       86    17574                              STCLASS = "AUTO" );
       87    18210          %QI@OTTER_SPACE;
       88    18238          %QI@OTTER_SPACE ( NAME = QI@OTTER_SPACE_2 );
       89    18266        %EJECT;
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:4    
       90    18267    1       INSTR$ = QI_1.INSTR$;
       91    18268    1       QI_1.INSTR$ = ADDR ( INSTR$ -> QI$OP_INSPECT.NEXT_CODE@P );
       92    18269
       93    18270    1       TALLY = 0;
       94    18271
       95    18272    1       CALL QCS$SETUP_TARGET;
       96    18273
       97    18274            /*  Assume it's normal...  */
       98    18275    1       IT_WAS_OVERPUNCHED = %NO;
       99    18276
      100    18277            /*  This stuff tries to 'un-sign' a signed numeric item.
      101    18278                QCU$UNSIGN isn't used because it doesn't worry about
      102    18279                trying to remember what the sign was to begin with,
      103    18280                and because it extends P-scaled items.  */
      104    18281    1       IF QI_1.TYPE.T = %QI#TYPE_DISPLAY
      105    18282    1       THEN
      106    18283    2         DO;
      107    18284                /*  Tweak the necessary bits to make this thing look
      108    18285                    ALPHANUMERIC and INDEXED.  */
      109    18286    2           QI_1.OP.CIP.T.CD.SIGN = %QI#CD_UNSIGNED;
      110    18287    2           QI_1.OP.CIP.T.LEN = 0;
      111    18288    2           QI_1.OP.CIP.T.AS = %QI#CIP_INDEXED_TARGET_AS;
      112    18289    2           QI_1.OP.INX.T = 0;
      113    18290
      114    18291    3           DO CASE ( QI_1.OP.CIP.T.CD.SIGN );
      115    18292
      116    18293    3             CASE ( %QI#CD_SIGN_LEADING_SEPARATE );
      117    18294        /*N*
      118    18295              Do we really not do this?
      119    18296                    QI_1.OP.INX.T = QI_1.OP.INX.T + 1;
      120    18297                    QI_1.OP.LEN.T = QI_1.OP.LEN.T - 1;
      121    18298        */
      122    18299
      123    18300    3             CASE ( %QI#CD_SIGN_TRAILING_SEPARATE );
      124    18301        /*N*
      125    18302              Do we really not do this?
      126    18303                    QI_1.OP.LEN.T = QI_1.OP.LEN.T - 1;
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:5    
      127    18304        */
      128    18305
      129    18306    3             CASE ( %QI#CD_SIGN_TRAILING_OVER );
      130    18307    3               IT_WAS_OVERPUNCHED = %YES;
      131    18308    3               OVERPUNCHED_CHAR = QI_1.OP.DATA.T$ ->
      132    18309    3                 QI$UBYTE_A ( QI_1.OFFSET.T + QI_1.OP.LEN.T - 1 );
      133    18310
      134    18311    4               DO CASE ( OVERPUNCHED_CHAR );
      135    18312
      136    18313    4                 CASE ( %ASCBIN('{') );
      137    18314    4                   OVERPUNCHED_CHAR = %QI#ZERO;
      138    18315    4                   IT_WAS_POSITIVE = %YES;
      139    18316
      140    18317    4                 CASE ( %ASCBIN('}') );
      141    18318    4                   OVERPUNCHED_CHAR = %QI#ZERO;
      142    18319    4                   IT_WAS_POSITIVE = %NO;
      143    18320
      144    18321    4                 CASE ( ELSE );
      145    18322    4                   IF  OVERPUNCHED_CHAR >= %ASCBIN('A')
      146    18323    4                     AND OVERPUNCHED_CHAR <= %ASCBIN('I')
      147    18324    4                   THEN
      148    18325    5                     DO;
      149    18326    5                       OVERPUNCHED_CHAR = OVERPUNCHED_CHAR
      150    18327    5                         + %(ASCBIN('1') - ASCBIN('A'));
      151    18328    5                       IT_WAS_POSITIVE = %YES;
      152    18329    5                     END;
      153    18330    4                   ELSE
      154    18331    5                     DO;
      155    18332    5                       OVERPUNCHED_CHAR = OVERPUNCHED_CHAR
      156    18333    5                         + %(ASCBIN('1') - ASCBIN('J'));
      157    18334    5                       IT_WAS_POSITIVE = %NO;
      158    18335    5                     END;
      159    18336
      160    18337    4               END;
      161    18338
      162    18339    3               QI_1.OP.DATA.T$ -> QI$UBYTE_A ( QI_1.OFFSET.T + QI_1.OP.LEN.T - 1 ) =
      163    18340    3                 OVERPUNCHED_CHAR;
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:6    
      164    18341
      165    18342    3           END;
      166    18343
      167    18344    2         END;
      168    18345
      169    18346            /*  We may need the original length of the
      170    18347                thing-to-be-inspected later on.  */
      171    18348    1       ORIGINAL_TARGET_LENGTH = QI_1.OP.LEN.T;
      172    18349
      173    18350            /*  If this item's ~= 0, then it's either TALLYING_ITEM@P:
      174    18351                "INSPECT ... TALLYING TALLYING_ITEM@P FOR ...";
      175    18352
      176    18353                or REPLACING_OLD_ITEM@P:
      177    18354                "INSPECT ... REPLACING REPLACING_OLD_ITEM@P WITH ...".  */
      178    18355    1       IF INSTR$ -> QI$OP_INSPECT.TALLYING_ITEM@P ~= 0
      179    18356    1       THEN
      180    18357    2         DO;
      181    18358    2           QI_1.SOURCE$ = ADDR ( INSTR$ -> QI$OP_INSPECT.TALLYING_ITEM@P );
      182    18359    2           CALL QCS$SETUP_SOURCE;
      183    18360    2           QI@OP_ID_2_OR_5 = QI_1.OP;
      184    18361    2           TALLIED_ITEM_IS_A_TEMP = QI_1.IS_TEMP.S;
      185    18362    2           TALLIED_ITEM_DAD$ = QI_1.DAD.S$;
      186    18363    2         END;
      187    18364    1       ELSE
      188    18365    2         DO;
      189    18366                /*  It's zero, which means:
      190    18367                    "INSPECT ... REPLACING CHARACTERS WITH ...".
      191    18368                    Setup a fake 1-character thing for "CHARACTERS".  */
      192    18369    2           QI@OP_ID_2_OR_5.DATA.S$ = ADDR (NIL);
      193    18370    2           QI@OP_ID_2_OR_5.LEN.S = 1;
      194    18371    2         END;
      195    18372
      196    18373            /*  If this item's ~= 0, then it's either TALLYING_FOR_ITEM@P:
      197    18374                "INSPECT ... TALLYING ... FOR TALLYING_FOR_ITEM@P ...";
      198    18375
      199    18376                or REPLACING_NEW_ITEM@P:
      200    18377                "INSPECT ... REPLACING ... WITH REPLACING_NEW_ITEM@P ...".  */
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:7    
      201    18378    1       IF INSTR$ -> QI$OP_INSPECT.TALLYING_FOR_ITEM@P ~= 0
      202    18379    1       THEN
      203    18380    2         DO;
      204    18381    2           QI_1.SOURCE$ = ADDR ( INSTR$ -> QI$OP_INSPECT.TALLYING_FOR_ITEM@P );
      205    18382    2           CALL QCS$SETUP_SOURCE;
      206    18383    2           QI@OP_ID_3_OR_6 = QI_1.OP;
      207    18384    2         END;
      208    18385    1       ELSE
      209    18386    2         DO;
      210    18387                /*  It's zero, which means:
      211    18388                    "INSPECT ... TALLYING ... FOR CHARACTERS ...".
      212    18389                    Setup a fake 1-character thing for "CHARACTERS".  */
      213    18390    2           QI@OP_ID_3_OR_6.DATA.S$ = ADDR (NIL);
      214    18391    2           QI@OP_ID_3_OR_6.LEN.S = 1;
      215    18392    2         END;
      216    18393
      217    18394            /*  If this item's ~= 0, then it's:
      218    18395                "INSPECT ... { TALLYING | REPLACING } ...
      219    18396                     ... { BEFORE | AFTER } INITIAL ...".  */
      220    18397    1       IF INSTR$ -> QI$OP_INSPECT.TALLYING_INITIAL_ITEM@P ~= 0
      221    18398    1       THEN
      222    18399    1   INITIAL_STUFF:
      223    18400    2         DO;
      224    18401    2           QI_1.SOURCE$ = ADDR ( INSTR$ -> QI$OP_INSPECT.TALLYING_INITIAL_ITEM@P );
      225    18402    2           CALL QCS$SETUP_SOURCE;
      226    18403
      227    18404    2           STARTING_INDEX = QI_1.OP.INX.T;
