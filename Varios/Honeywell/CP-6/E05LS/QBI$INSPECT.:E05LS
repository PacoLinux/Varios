      229    18406    2             - QI_1.OP.LEN.S;
      230    18407    2           QI_1.OP.LEN.T = QI_1.OP.LEN.S;
      231    18408
      232    18409    2   FIND_INITIAL_STRING:
      233    18410    3           DO QI_1.OP.INX.T = STARTING_INDEX TO ENDING_INDEX;
      234    18411
      235    18412    3             VARIABLE_SOURCE$ = PINCRC ( QI_1.OP.DATA.S$,
      236    18413    3               QI_1.OFFSET.S
      237    18414    3               + QI_1.OP.INX.S );
PL6.E3A0      #001=QBI$INSPECT File=QBI$INSPECT.:E05TSI                          WED 07/30/97 04:50 Page:8    
      238    18415    3             VARIABLE_TARGET$ = PINCRC ( QI_1.OP.DATA.T$,
      239    18416    3               QI_1.OFFSET.T
      240    18417    3               + QI_1.OP.INX.T );
      241    18418
      242    18419    3             IF VARIABLE_SOURCE$ -> QI$VARIABLE_STRING_SOURCE.TEXT =
      243    18420    3               VARIABLE_TARGET$ -> QI$VARIABLE_STRING_TARGET.TEXT
      244    18421    3             THEN
      245    18422    4               DO;
      246    18423    4                 IF INSTR$ -> QI$OP_INSPECT.ATTRIBUTE.BEFORE_AFTER =
      247    18424    4                   %QI#INSPECT_AFTER_INITIAL
