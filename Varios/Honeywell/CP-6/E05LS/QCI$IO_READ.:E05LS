             20389    1     DCL 1 QCI@IO_READ ALIGNED,
             20390    1           2 FRAME_START_INDEX UBIN,
             20391                          /* Starting index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             20392    1           2 FRAME_STOP_INDEX UBIN,
             20393                          /* Stopping index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             20394    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             20395                          /* If IS_FIELD_INDEXES_VALID, use them;
             20396                             else use all the fields in the frame.  */
             20397    1           2 FIELD_START_INDEX UBIN,
             20398                          /* Starting index into QI$GZ_FIELD table.  */
             20399    1           2 FIELD_STOP_INDEX UBIN,
             20400                          /* Stopping index into QI$GZ_FIELD table.  */
             20401    1           2 IS_CURSOR_POSITION_VALID BIT(1) ALIGNED,
             20402                          /* If IS_CURSOR_POSITION_VALID, use it;
             20403                             else allow cursor to default to upper-left-most field.  */
             20404    1           2 CURSOR_POSITION BIT(16) ALIGNED,
             20405                          /* POSition of the field on which to place the cursor.  */
             20406    1           2 STATUS UBIN,
             20407                          /* Returned status code:  0 -> Immediate return to level 0;
             20408                                                   >0 -> Status is ok.  */
             20409    1           2 FIELD_ERROR_STOP_INDEX UBIN;
             20410                          /* Passed index for QCG$GET_FIELD processing.  */
             20417            %ENDIF;
