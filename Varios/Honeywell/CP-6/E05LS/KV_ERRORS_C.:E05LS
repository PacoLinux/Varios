    7      /* KV_ERRORS_C range for codes is 1400 - 1499. */
    8      %EQU E$NO_FLD = 1400;  /* Field not found */
    9      %EQU E$ILGFLDPST = 1401;  /* Illegal field position */
   10      %EQU E$OVRLAPFLD = 1402;  /* Overlapping fields */
   11      %EQU E$NO_FLDSLC = 1403;  /* No fields selected */
   12      %EQU E$IMP_ILGCMNTYP = 1404;  /* IMP:  Illegal command type */
   13      %EQU E$IMP_ILGID_TKNTYP = 1405;  /* IMP:  Illegal ID token type */
   14      %EQU E$IMP_ILGINTTKNTYP = 1406;  /* IMP:  Illegal introducer token type */
   15      %EQU E$IMP_CNTGETMMR = 1407;  /* IMP:  Can't get memory */
   16      %EQU E$IMP_TBLSIZLMTEXC = 1408;  /* IMP:  Table size limit exceeded */
   17      %EQU E$DATDLT_DLT = 1409;   /* Data deleted because we're deleting */
   18      %EQU E$DATDLT_DSC = 1410;   /* Data deleted because we're disconnecting */
   19      %EQU E$OPRDSC = 1411;   /* Operator Disconnect (or Kill). */
   20      %EQU E$HRDERR = 1412;   /* Hardware error */
   21      %EQU E$SFTERR = 1413;   /* Software error (1 line abort). */
   22      %EQU E$PRTERR = 1414;   /* Protocol error */
   23      %EQU E$X364_P_CNTEXC = 1415;  /* X3.64 P count exceeded */
   24      %EQU E$X364_P_MAXVALEXC = 1416;  /* X3.64 P maximum value exceeded */
   25      %EQU E$X364_ILGF_CHR = 1417;  /* X3.64 F character is illegal */
   26      %EQU E$X364_ILGCE_CHR = 1418;  /* X3.64 CE character is illegal */
   27      %EQU E$X364_ILGFE_CHR = 1419;  /* X3.64 FE character is illegal */
   28      %EQU E$X364_ILGCE_AFTESC = 1425;  /* illegal CE after ESC */
   29      %EQU E$X364_ILGCE_AFTP = 1426;  /* illegal CE after P */
   30      %EQU E$X364_ILGCE_CSIAFTESC = 1427;  /* illegal CE CSI after ESC */
   31      %EQU E$X364_ILGCE_CSIAFTP = 1428;  /* illegal CE CSI after P */
   32      %EQU E$X364_ILGCE_CSIIN_CTRSQN = 1429;  /* illegal CE CSI in control sequence */
   33      %EQU E$X364_ILGCE_IN_CTRSQN = 1430;  /* illegal CE in control sequence */
   34      %EQU E$X364_ILGCHRAFTESC = 1431;  /* illegal character after ESC */
   35      %EQU E$X364_ILGCHRAFTP = 1432;  /* illegal character after P */
   36      %EQU E$X364_ILGCHRIN_CTRSQN = 1433;  /* illegal character in control sequence */
   37      %EQU E$X364_ILGESCAFTP = 1434;  /* illegal escape after P */
