PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:1    
        1        1        /*M* LLG$GROW - Tree "growing" routine                              */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* PLM=4,STI=0,IND=4,CTI=3,DCI=4,ECI=4,CSI=0,THI=0,DTI=0,ENI=4,
        8        8             CLM=0,SQB,NTB,MOC,CCI=3,NLB,MCL=0,EDMC*/
        9        9        /*F* NAME:         LLG$GROW
       10       10             PURPOSE:      Provide a common routine through which the
       11       11                           internal tree structured symbol tables may
       12       12                           be constructed.
       13       13             DESCRIPTION:  LLG$GROW is entered at LLG$GROW and is passed
       14       14                           6 parameters.  The first param. is labeled
       15       15                           ROOT$ and points to the root node of the tree.
       16       16                           The second param. is labeled NEW$ and points
       17       17                           to the first node to be added to the tree.  The
       18       18                           thirs param. is labeled N and contains an integer
       19       19                           specifying the number of nodes to be added to the
       20       20                           tree.  The fourth parameter is labeled CUR_OU$
       21       21                           and points to the OU_ENTRY for the group of nodes.
       22       22                           The fifth parameter is labeled AVAIL$ and it
       23       23                           points to the next available word of memory.  The
       24       24                           sixth parameter is labeled NAME$ and it points to
       25       25                           the def names table.
       26       26
       27       27                           Enough core should be allocated prior to calling
       28       28                           LLG$GROW so that a memory fault does not occur.
       29       29
       30       30                           Pointer descriptions:
       31       31                                TREE.OU$       pointer to obj. unit ref.
       32       32                                TREE.BROTHER$  pointer to doubly def. sym.
       33       33                                TREE.PARENT$   pointer to predecessor node
       34       34                                TREE.L_CHILD$  pointer to left child
       35       35                                TREE.R_CHILD$  pointer to right child
       36       36                                TREE.NODE$     pointer to node
       37       37
PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:2    
       38       38                           If the new node has been previously entered, then
       39       39                           a brother link is established to the new node.
       40       40                           The first node retains all pointers, but the new
       41       41                           node has only 3 significant pointers, the OU$,
       42       42                           NODE$ and BROTHER$ pointers.  This is to allow the
       43       43                           capability of very explicit diagnostics in the
       44       44                           event of a multiply defined symbol.  The OU$
       45       45                           pointer allows retrieval of the object unit name
       46       46                           as well as the object file name from which the
       47       47                           multiply defined symbol declaration occured.     */
       48       48        %EJECT;
PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:3    
       49       49        LLG$GROW: PROC (ROOT$, NEW$, N, CUR_OU$, AVAIL$, NAME$, TYPE);
       50       50        %INCLUDE LL_MACS_C;
       51      404        %INCLUDE B$OBJECT_C;
       52     3834        %INCLUDE LL_DATA_R;
       53     3870        %B$EDEF ( NAME = NODE );
       54     3940        %B$EDEF ( NAME = ENTDEF_NODE );
       55     4010        %B$SDEF ( NAME = SYMDEF_NODE );
       56     4064        %B$HEAD ( NAME = LL_HEAD,
       57     4065         STCLASS = SYMREF );
       58     4265        %B$RECORDSUBS;
       59     4293        %LL$NODE ( NAME = TREE );
       60     4325        %SUB_LINKFLAGS;
       61     4334        %LL$OU_ENTRY (NAME = OU_ENTRY);
       62     4382    1   DCL REGION UBIN BYTE;
       63     4383    1   DCL PRE_REGION UBIN BYTE;
       64     4384    1   DCL SOURCE UBIN;
       65     4385    1   DCL TYPE UBIN WORD ALIGNED;
       66     4386    1   DCL ROOT$ PTR;
       67     4387    1   DCL NEW$ PTR;
       68     4388    1   DCL CUR_OU$ PTR;
       69     4389    1   DCL AVAIL$ PTR;
       70     4390    1   DCL NAME$ PTR;
       71     4391    1   DCL PREDECESSOR$ PTR;
       72     4392    1   DCL N UBIN WORD ALIGNED;
       73     4393    1   DCL I UBIN WORD ALIGNED;
       74     4394    1   DCL TPTR$ PTR;
       75     4395    1   DCL ERR# BIT(1) UNAL;
       76     4396    1   DCL 1 NAME BASED ALIGNED,
       77     4397    1         2 COUNT UBIN HALF UNAL,
       78     4398    1         2 TEXT CHAR(NAME.COUNT) UNAL;
       79     4399    1   DCL TEMP1 UBIN WORD ALIGNED;
       80     4400    1   DCL TEMP2 UBIN WORD ALIGNED;
       81     4401        %EJECT;
PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:4    
       82     4402    2       DO I = 0 TO N - 1;
       83     4403    2           IF TYPE = %TYPEDEF
       84     4404    2           THEN SOURCE = NEW$ -> ENTDEF_NODE.LFLAGS.SOURCE;
       85     4405    2           ELSE SOURCE = NEW$ -> SYMDEF_NODE.LFLAGS.SOURCE;
       86     4406    2           TPTR$ = ROOT$;
       87     4407    2           ERR# = '0'B;
       88     4408    3           IF (TPTR$ = ADDR(NIL)) THEN DO;
       89     4409    3               IF NOT LL_FLAGS.BIAS_OU
       90     4410    4               THEN DO;
       91     4411    4                   IF SOURCE = %SOURCE_REMOVED#
       92     4412    4                   THEN GOTO NEXT;
       93     4413    4                   END;
       94     4414    3               ROOT$ = AVAIL$;
       95     4415    3               PREDECESSOR$ = ADDR(NIL);
       96     4416    3               END;
       97     4417    3           ELSE DO;
       98     4418    3               TEMP1 = NEW$ -> NODE.NPOINTER;
       99     4419    4               DO WHILE (TPTR$ ~= ADDR(NIL) AND ~ERR#);
      100     4420    4                   IF NOT LL_FLAGS.BIAS_OU
      101     4421    5                   THEN DO;
      102     4422    5                       IF SOURCE = %SOURCE_REMOVED#
      103     4423    5                       THEN GOTO NEXT;
      104     4424    5                       END;
      105     4425    4                   IF NOT LL_FLAGS.BIAS_OU
      106     4426    4                   THEN
      107     4427    4                       IF SOURCE = %SOURCE_SLIB#
      108     4428    5                       THEN DO;
      109     4429    5                           IF PINCRW(NAME$,TEMP1)->NAME.TEXT = LL_PATCH_NAMES.N1.TEXT
      110     4430    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = LL_PATCH_NAMES.N2.
              4430                                       TEXT
      111     4431    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = LL_PATCH_NAMES.N3.
              4431                                       TEXT
      112     4432    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = LL_PATCH_NAMES.N4.
              4432                                       TEXT
      113     4433    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = 'G_VERSION'
      114     4434    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = 'G$ROS$'
      115     4435    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = 'G$TCB$'
PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:5    
      116     4436    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = 'G$PIA$'
      117     4437    5                              OR PINCRW(NAME$,TEMP1)->NAME.TEXT = 'G$JIT$'
      118     4438    5                           THEN GOTO NEXT;
      119     4439    5                           END;
      120     4440    4                   TEMP2 = TPTR$ -> TREE.NODE$ -> NODE.NPOINTER;
      121     4441    4                   IF (PINCRW(NAME$, TEMP1) -> NAME.TEXT >
      122     4442    5                      PINCRW(NAME$, TEMP2) -> NAME.TEXT ) THEN DO;
      123     4443    5                       PREDECESSOR$ = TPTR$;
      124     4444    5                       TPTR$ = TPTR$ -> TREE.R_CHILD$;
      125     4445    5                       IF TPTR$ = ADDR(NIL) THEN
      126     4446    5                           PREDECESSOR$ -> TREE.R_CHILD$ = AVAIL$;
      127     4447    5                       END;
      128     4448    4                   ELSE
      129     4449    4                       IF (PINCRW(NAME$, TEMP1) -> NAME.TEXT <
      130     4450    5                          PINCRW(NAME$, TEMP2) -> NAME.TEXT) THEN DO;
      131     4451    5                           PREDECESSOR$ = TPTR$;
      132     4452    5                           TPTR$ = TPTR$ -> TREE.L_CHILD$;
      133     4453    5                           IF (TPTR$ = ADDR(NIL)) THEN
      134     4454    5                               PREDECESSOR$ -> TREE.L_CHILD$ = AVAIL$;
      135     4455    5                           END;
      136     4456    5                       ELSE DO;
      137     4457    5                           IF NOT LL_FLAGS.BIAS_OU
      138     4458    5                           THEN
      139     4459    6                           DO;
      140     4460    6                               IF SOURCE = %SOURCE_SLIB#
      141     4461    6                                  OR SOURCE = %SOURCE_LINKER#
      142     4462    6                               THEN GOTO NEXT;
      143     4463    6                               END;
      144     4464    5                           ERR# = '1'B;
      145     4465    5                           IF ( TYPE = %TYPEDEF ) THEN
      146     4466    5                               TPTR$ -> TREE.NODE$ ->
      147     4467    5                                  ENTDEF_NODE.LFLAGS.MULTIPLE = '1'B;
      148     4468    5                           ELSE
      149     4469    5                               TPTR$ -> TREE.NODE$ ->
      150     4470    5                                  SYMDEF_NODE.LFLAGS.MULTIPLE = '1'B;
      151     4471    5                           END;
      152     4472    4                   END;
PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:6    
      153     4473    3               END;
      154     4474    2           AVAIL$ -> TREE.OU$ = CUR_OU$;
      155     4475    2           AVAIL$ -> TREE.NODE$ = NEW$;
      156     4476    2           IF AVAIL$->TREE.OU$ = ADDR(NIL)
      157     4477    2           THEN
      158     4478    2               REGION = 0;
      159     4479    2           ELSE
      160     4480    2               REGION = AVAIL$->TREE.OU$->OU_ENTRY.REGION;
      161     4481    3           IF (~ERR#) THEN DO;
      162     4482    3               IF ( TYPE = %TYPEDEF ) THEN
      163     4483    3                   AVAIL$ -> TREE.NODE$ -> ENTDEF_NODE.LFLAGS.MULTIPLE = '0'B;
      164     4484    3               ELSE
      165     4485    3                   AVAIL$ -> TREE.NODE$ ->
      166     4486    3                      SYMDEF_NODE.LFLAGS.MULTIPLE = '0'B;
      167     4487    3               AVAIL$ -> TREE.PARENT$ = PREDECESSOR$;
      168     4488    3               AVAIL$ -> TREE.R_CHILD$ = ADDR(NIL);
      169     4489    3               AVAIL$ -> TREE.L_CHILD$ = ADDR(NIL);
      170     4490    3               AVAIL$ -> TREE.BROTHER$ = ADDR(NIL);
      171     4491    3               END;
      172     4492    3           ELSE DO;
      173     4493    4               DO UNTIL (TPTR$->TREE.BROTHER$ = ADDR(NIL));
      174     4494    4                   IF TPTR$->TREE.OU$ = ADDR(NIL)
      175     4495    4                   THEN
      176     4496    4                       PRE_REGION = 0;
      177     4497    4                   ELSE
      178     4498    4                       PRE_REGION = TPTR$->TREE.OU$->OU_ENTRY.REGION;
      179     4499    4                   IF PRE_REGION = REGION
      180     4500    4                   THEN
      181     4501    5                   DO;
      182     4502    5                       IF LL_HEAD.SEV < 4
      183     4503    5                       THEN LL_HEAD.SEV = 4;
      184     4504    5                       IF (TYPE = %TYPEDEF)
      185     4505    5                       THEN
      186     4506    6                       DO;
      187     4507    6                           AVAIL$->TREE.NODE$->ENTDEF_NODE.LFLAGS.MULTIPLE = '1'B;
      188     4508    6                           TPTR$->TREE.NODE$->ENTDEF_NODE.LFLAGS.MULTIPLE = '1'B;
      189     4509    6                           END;
PL6.E3A0      #001=LLG$GROW File=LLG$GROW.:E05TSI                                WED 07/30/97 03:15 Page:7    
      190     4510    5                       ELSE
      191     4511    6                       DO;
      192     4512    6                           AVAIL$->TREE.NODE$->SYMDEF_NODE.LFLAGS.MULTIPLE = '1'B;
      193     4513    6                           TPTR$->TREE.NODE$->SYMDEF_NODE.LFLAGS.MULTIPLE = '1'B;
      194     4514    6                           END;
      195     4515    5                       END;
      196     4516    4                   ELSE
      197     4517    5                   DO;
      198     4518    5                       IF (TYPE = %TYPEDEF)
      199     4519    5                       THEN
      200     4520    6                       DO;
      201     4521    6                           AVAIL$->TREE.NODE$->ENTDEF_NODE.LFLAGS.MULTIPLE = '0'B;
      202     4522    6                           TPTR$->TREE.NODE$->ENTDEF_NODE.LFLAGS.MULTIPLE = '0'B;
      203     4523    6                           END;
      204     4524    5                       ELSE
      205     4525    6                       DO;
      206     4526    6                           AVAIL$->TREE.NODE$->SYMDEF_NODE.LFLAGS.MULTIPLE = '0'B;
