      211     4531    4                   THEN
      212     4532    4                       TPTR$ = TPTR$ -> TREE.BROTHER$;
      213     4533    4                   END;
      214     4534    3               TPTR$ -> TREE.BROTHER$ = AVAIL$;
      215     4535    3               AVAIL$ -> TREE.BROTHER$ = ADDR(NIL);
      216     4536    3               END;
      217     4537    2   NEXT:
      218     4538    2           AVAIL$ = PINCRW(AVAIL$, SIZEW(TREE));
      219     4539    2           NEW$ = PINCRW(NEW$, SIZEW(NODE));
      220     4540    2           END;
      221     4541    1       RETURN;
      222     4542    1   END LLG$GROW;

