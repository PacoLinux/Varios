      701     3229    3        LLEN=FONT_HEIGHT(ENT_FONT)*FONT_WIDTH(ENT_FONT);
      702     3230    3        PRT_POS=SCOL;
      703     3231    3        I=0;
      704     3232    4        DO WHILE (PRT_POS+FONT_WIDTH(ENT_FONT)<=FS$WRSP.WIDTH);
      705     3233    4             LETTER=SUBSTR(WORKA.TWORK.TEXT,I,1);
      706     3234    4             INDX=ASCBIN(LETTER)-32;
      707     3235    4             IF ENT_FONT~=FS_PLAIN_TEXT
      708     3236    4             THEN
      709     3237    5               DO;
      710     3238    5                    IF ENTRIES.STRIKES>1
      711     3239    5                    THEN
      712     3240    5                         LETTER=CSET(FS$WRSP.STRIKES-1);
      713     3241    5                  END;
      714     3242            %EJECT;
PL6.E3A0      #001=FSK$BANNER File=FSK$BANNER.:E05TSI                            TUE 07/29/97 17:32 Page:26   
      715     3243    4             IF INDX=-24
      716     3244    4             THEN
      717     3245    5               DO;
      718     3246    5                    IF PRT_POS~=SCOL THEN
      719     3247    5                         PRT_POS=PRT_POS-(FONT_WIDTH(ENT_FONT)+ENTRIES.ICSH);
      720     3248    5                    GOTO BACKSPACE;
      721     3249    5                  END;
      722     3250    4             IF INDX<0 OR INDX>96
      723     3251    4             THEN
      724     3252    5               DO;
      725     3253    5   UNPRINT:         INDX=0;
      726     3254    5                    IF ENT_FONT ~=0
      727     3255    5                    THEN
      728     3256    5                         LETTER=' ';
      729     3257    5                  END;
      730     3258    4             LET$=FONT_ADDRS(ENT_FONT);
      731     3259    4             POS=FONT_OFF(ENT_FONT)+INDX*LLEN+FONTINDX*FONT_WIDTH(ENT_FONT);
      732     3260    5             DO L=0 TO FONT_WIDTH(ENT_FONT)-1;
      733     3261    5                  IF LET$->MAT$LET(POS)='1'B
      734     3262    5                  THEN
      735     3263    5                       BUFFER.NBUFF(PRT_POS)=LETTER;
      736     3264    5                  POS=POS+1;
      737     3265    5                  PRT_POS=PRT_POS+1;
      738     3266    5                END;
      739     3267    4             PRT_POS=PRT_POS+ENTRIES.ICSH;
      740     3268    4   BACKSPACE: I=I+1;
      741     3269    4             IF I>=WORKA.TWORK.COUNT
      742     3270    4             THEN
      743     3271    4                  GOTO BYE;
      744     3272    4           END;
      745     3273    3   BYE: RETURN;
      746     3274    3   END ADD_TO_REC;
      747     3275    2   END LOOK;
      748     3276            %EJECT;
PL6.E3A0      #001=FSK$BANNER File=FSK$BANNER.:E05TSI                            TUE 07/29/97 17:32 Page:27   
      749     3277    1   END FSK$BANNER;
      750     3278        %EOD;

