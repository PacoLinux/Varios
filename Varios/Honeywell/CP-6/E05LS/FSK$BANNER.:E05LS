      752        2        /*T*                                                         */
      753        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      754        4        /*T*                                                         */
      755        5        /*T***********************************************************/
      756        6        FSK$GETBANNERTEXT: PROC (RD$,TEXTWORK,READ_ALREADY, ITEM#) ALTRET;
      757        7    1   DCL RD$ PTR;
      758        8    1   DCL 1 TEXTWORK,
      759        9    1            2 COUNT UBIN BYTE UNAL,
      760       10    1            2 TEXT CHAR (256);
      761       11    1   DCL READ_ALREADY BIT (1) ALIGNED;
      762       12    1   DCL ITEM# UBIN;
      763       13
      764       14        %INCLUDE CP_6;
      765     5573
      766     5574        %INCLUDE FS_BANNER_C;
      767     5615        %FS$BANNER;
      768     5637        %INCLUDE AS_SUPER_C;
      769     6072        %AS_USERS;
      770     6250        %INCLUDE FS_SUPER_C;
      771     7013        %FS$WORKAUTO;
      772     7032    1   DCL FMP$CLSJ ENTRY OPTIONAL;
      773     7033    1   DCL FMM$DFLOPNP ENTRY OPTIONAL;         /* Default open, Privileged. */
      774     7034    1   DCL FMM$SCRUB ENTRY(1) OPTIONAL;
      775     7035    1   DCL FRS$STRABT ENTRY ALTRET OPTIONAL;
      776     7036    1   DCL M$MREAD ENTRY(1)ALTRET OPTIONAL;
      777     7037
      778     7038    1   DCL RDBUF CHAR (%BTEXT_REC_SIZE) BASED (RD$);
      779     7039    1   DCL PTR2RD$ PTR;
      780     7040    1   DCL PTR2$ PTR;
      781     7041
      782     7042    1   DCL WOFF SBIN;
      783     7043
      784     7044    1        PTR2RD$ = ADDR (RDBUF);
      785     7045    1        IF NOT READ_ALREADY
      786     7046    2        THEN DO;
      787     7047    2               CALL FSK$READ_STAR_S (PTR2RD$) ALTRET (READ_ERR);
PL6.E3A0      #002=FSK$GETBANNERTEXT File=FSK$BANNER.:E05TSI                     TUE 07/29/97 17:33 Page:115  
      788     7048    2               READ_ALREADY = '1'B;
      789     7049    2             END;
      790     7050
      791     7051    1        WOFF = PTR2RD$->FS$BANNER.TEXT_OFF (ITEM#);
      792     7052    1        IF WOFF ~= -1
      793     7053    2        THEN DO;
      794     7054    2               PTR2$ = PINCRC (PTR2RD$,WOFF);
      795     7055    2               TEXTWORK.COUNT = PTR2$->TEXT.LENGTH_ ;
      796     7056    2               TEXTWORK.TEXT = PTR2$->TEXT.VAL ;
      797     7057    2             END;
      798     7058    2        ELSE DO;
      799     7059    2               TEXTWORK.COUNT = 0;
      800     7060    2               TEXTWORK.TEXT = ' ';
      801     7061    2             END;
      802     7062
      803     7063    1        RETURN;
      804     7064    1   READ_ERR: ;
      805     7065    1        ALTRETURN;
      806     7066    1   FSK$READ_STAR_S: PROC(BUF$) ALTRET;
      807     7067        /* PARAMETERS */
      808     7068    2   DCL BUF$ PTR;
      809     7069        /* INCLUDE FILES */
      810     7070        %INCLUDE B$JIT;
      811     7673        %INCLUDE F$DCB;
      812     7722        %INCLUDE CP_6_SUBS;
      813     8262        /* EXTERNAL STORAGE */
      814     8263    2   DCL B$JIT$ PTR SYMREF;
      815     8264        /* LOCAL AUTOMATIC STORAGE */
      816     8265    2   DCL STAR$ PTR;
      817     8266    2   DCL JDCB$ PTR;
      818     8267    2   DCL SVERR BIT (36) ALIGNED;
      819     8268    2   DCL 1 BANKEY CONSTANT,
      820     8269    2            2 L UBIN BYTE UNAL INIT (10),
      821     8270    2            2 C CHAR(10) INIT ('BANNERTEXT');
      822     8271        /* CONSTANT STORAGE */
      823     8272        %FPT_READ(FPTN=CONSTREAD,STCLASS=STATIC,KEYS=YES,KEY=BANKEY);
      824     8311        %FPT_READ(FPTN=AUTOREAD,STCLASS=);
PL6.E3A0      #002=FSK$GETBANNERTEXT File=FSK$BANNER.:E05TSI                     TUE 07/29/97 17:33 Page:116  
      825     8350        /**/
      826     8351    2        SVERR = B$JIT.ERR;
      827     8352    2        JDCB$ = B$JIT.DCB$;
      828     8353    2        STAR$ = DCBADDR(%M$STAR#);
      829     8354    2        B$JIT.DCB$ = STAR$;
      830     8355    2        IF STAR$->F$DCB.FCD THEN CALL FMP$CLSJ;
      831     8356    2        CONSTREAD.V.DCB# = %M$STAR#;
      832     8357    2        AUTOREAD = CONSTREAD;
      833     8358        /*         *   AUTOREAD.KEY_ = VECTOR(BANKEY);      */
      834     8359    2        AUTOREAD.BUF_.BUF$ = BUF$;
      835     8360    2        AUTOREAD.BUF_.BOUND = %BAN_REC_SZ;
      836     8361          /* PREPARE THE DCB FO AN OPEN  */
      837     8362    2        CALL FMM$SCRUB(STAR$);
      838     8363    2        STAR$->F$DCB.ASN = %FILE#;
      839     8364    2        STAR$->F$DCB.FUN = %IN#;
      840     8365    2        STAR$->F$DCB.SHARE = %ALL#;
      841     8366    2        STAR$->F$DCB.ACCT = B$JIT$->B$JIT.ACCN;
      842     8367    2        STAR$->F$DCB.ACS = %DIRECT#;
      843     8368    2        STAR$->F$DCB.ORG = %KEYED#;
      844     8369    2        STAR$->F$DCB.NAME.L = 2;
      845     8370    2        STAR$->F$DCB.NAME.C = '*S';
      846     8371    2   REOPN: ;
      847     8372    2        B$JIT.ERR = '0'B;
      848     8373    2        CALL FMM$DFLOPNP;
      849     8374    2        IF B$JIT.ERR THEN GOTO ERR;
      850     8375    2        CALL M$MREAD (AUTOREAD) ALTRET (ERR);
      851     8376           /* CLOSE FILE IF OPEN, RESTORE DCB & ERR CODE */
      852     8377    2        IF STAR$->F$DCB.FCD THEN CALL FMP$CLSJ;
      853     8378    2        B$JIT.ERR = SVERR;
      854     8379    2        B$JIT.DCB$ = JDCB$;
      855     8380    2        RETURN;
      856     8381    2   ERR: ;
      857     8382    2        B$JIT.DCB$ = JDCB$;
      858     8383    2        ALTRETURN;
      859     8384    2   END FSK$READ_STAR_S;
      860     8385    1   END FSK$GETBANNERTEXT;

