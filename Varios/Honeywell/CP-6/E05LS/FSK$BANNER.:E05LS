        9        9        /*
       10       10                     FFFFF  SSS  K   K   $   BBBB   AAA  N   N N   N EEEEE RRRR
       11       11                     F     S   S K  K   $$$  B   B A   A N   N N   N E     R   R
       12       12                     F     S     K K   $ $   B   B A   A NN  N NN  N E     R   R
       13       13                     FFFF   SSS  KK     $$$  BBBB  AAAAA N N N N N N EEEE  RRRR
       14       14                     F         S K K     $ $ B   B A   A N  NN N  NN E     R R
       15       15                     F     S   S K  K   $$$  B   B A   A N   N N   N E     R  R
       16       16                     F      SSS  K   K   $   BBBB  A   A N   N N   N EEEEE R   R
       17       17        */
       18       18            %INCLUDE FS_SUPER_C;
       19      781            %FS$BAN_INIT(NAME=FS$FINST,STCLASS="");
       20      838            %FS$WRSP(STCLASS="");
       21      856            %FS$WORKAUTO (NAME=WORKA, STCLASS="");
       22      875            %INCLUDE B$JIT_C;
       23     1189            %B$JIT0;
       24     1278            %U$JIT1;
       25     1352            %A$JIT6X;
       26     1355    1   DCL B$JIT$ PTR SYMREF;
       27     1356            %INCLUDE UM_CP6;
       28     2208            %VLR_SITEINFO(FPTN=SITEINFO,STCLASS=BASED);
       29     2227            %INCLUDE FS$FIT;
       30     2270            %FS_OSFN(STCLASS=BASED);
       31     2284            %INCLUDE F$DCB;
       32     2333            %INCLUDE FM$FIT;
       33     2554            %CODE16;
       34     2562    1   DCL FSK$GETBANNERTEXT ENTRY (4) ALTRET;
       35     2563    1   DCL BUFFA CHAR(256);
       36     2564    1   DCL 1 BUFFER REDEF BUFFA,
       37     2565    1            2 BUFF,
