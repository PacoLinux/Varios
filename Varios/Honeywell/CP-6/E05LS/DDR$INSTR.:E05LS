      476     6076    2          REGLEN = VDSC.VI;
      477     6077    2          CALL MOVE_REGLEN ALTRET (TOO_BIG);
      478     6078    2          IF VDSC.RN > 0
      479     6079    2            THEN CALL MOVE (TAGS(VDSC.RN),3) ALTRET (TOO_BIG);
      480     6080    2            ELSE IF YFIELD_HAS_REG THEN CALL MOVE (',',1) ALTRET (TOO_BIG);
      481     6081    2       CASE (D_ARG);       /* ARG */
      482     6082    2   DECODE_ARG:;
      483     6083    2          CALL MOVE ('ARG ', 4) ALTRET (TOO_BIG);
      484     6084    2          YFIELD_HAS_REG = ARG.AR;
      485     6085    2          CALL SYMBOLIC_YFIELD ALTRET (TOO_BIG);
      486     6086    2          IF ARG.REG > 0
      487     6087    2            THEN CALL MOVE (TAGS(ARG.REG),3) ALTRET (TOO_BIG);
      488     6088    2            ELSE IF YFIELD_HAS_REG THEN CALL MOVE (',',1) ALTRET (TOO_BIG);
      489     6089    2       CASE (D_CDSC);      /* CLIMB extention word */
      490     6090    2          YFIELD_HAS_REG = '0'B;
      491     6091    3          DO CASE (CLIMB1.OP);
      492     6092    3            CASE (0);
      493     6093    3               IF CLIMB1.SID = 1008
      494     6094    3                 THEN CALL MOVE ('pmme ',5);
      495     6095    3                 ELSE CALL MOVE ('climb ',5);
      496     6096    3               REGLEN = 0;
      497     6097    3               IF CLIMB1.E
      498     6098    3                 THEN REGLEN = CLIMB1.P+1;
      499     6099    3               CALL DDR$BINTXT(C$,REGLEN,%UINT_FMT,SIZE);
      500     6100    3               CALL MOVE (CSTRING,SIZE);
      501     6101    3            CASE (1);
      502     6102    3              CALL MOVE ('ret',3);
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:17   
      503     6103    3            CASE (2);
      504     6104    3              CALL MOVE ('ltras',5);
      505     6105    3            CASE (3);
      506     6106    3              CALL MOVE ('ltrad',5);
      507     6107    3          END; /* do case */
      508     6108    2     END; /* case */
      509     6109    1     IF YFIELD_HAS_REG
      510     6110    1       THEN
      511     6111    1          CALL MOVE (PREGS(INSTR.YFIELD.DR),4) ALTRET (TOO_BIG);
      512     6112
      513     6113    1     RETURN;
      514     6114        %EJECT;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:18   
      515     6115    1   MOVE_REGLEN: PROC ALTRET;
      516     6116
      517     6117    2     IF SIZE_IS_REG
      518     6118    3       THEN DO;
      519     6119    3         REGLEN = MOD (REGLEN,16);
      520     6120    3         IF REGLEN > 0
      521     6121    3           THEN CALL MOVE (TAGS(REGLEN), 3) ALTRET (NO_ROOM);
      522     6122    3           ELSE CALL MOVE (',', 1) ALTRET (NO_ROOM);
      523     6123    3       END;
      524     6124    3       ELSE DO;
      525     6125    3         CALL MOVE (',',1) ALTRET (NO_ROOM);
      526     6126    3         CALL DDR$BINTXT (C$,REGLEN,%UINT_FMT,SIZE);
      527     6127    3         CALL MOVE (CSTRING,SIZE) ALTRET (NO_ROOM);
      528     6128    3       END;
      529     6129    2     RETURN;
      530     6130    2   NO_ROOM:
      531     6131    2     ALTRETURN;
      532     6132    2   END;
      533     6133        %EJECT;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:19   
      534     6134    1   DO_MF: PROC (P_MF);
      535     6135
      536     6136    2   DCL P_MF BIT(7) UNAL;
      537     6137    2   DCL 1 MF ALIGNED,
      538     6138    2         2 AR BIT(1) UNAL,
      539     6139    2         2 RL BIT(1) UNAL,
      540     6140    2         2 ID BIT(1) UNAL,
      541     6141    2         2 REG UBIN(4) UNAL,
      542     6142    2         2 * BIT(29);
      543     6143    2   DCL 1 AFTER_AR REDEF MF,
      544     6144    2         2 * BIT(1),
      545     6145    2         2 X BIT(6),
      546     6146    2         2 * BIT(29);
      547     6147    2   DCL 1 AFTER_RL REDEF MF,
      548     6148    2         2 * BIT(2),
      549     6149    2         2 X BIT(5),
      550     6150    2         2 * BIT(29);
      551     6151    2   DCL REGT(0:15) CHAR(4) CONSTANT INIT(')', ',AU)', 'QU)', ',.3)',
      552     6152    2                                      ',IC)', ',A)', ',Q)', ',.7)',
      553     6153    2                                      ',X0)', ',X1)', ',X2)', ',X3)',
      554     6154    2                                      ',X4)', ',X5)', ',X6)', ',X7)');
      555     6155
      556     6156    2        MF = P_MF;
      557     6157    2        CALL MOVE ('(',1);
      558     6158    2        IF MF.AR THEN CALL MOVE('AR',2);
      559     6159    2        IF AFTER_AR.X THEN CALL MOVE (',',1);
      560     6160    2        IF MF.RL THEN CALL MOVE('RL',2);
      561     6161    2        IF AFTER_RL.X THEN CALL MOVE (',',1);
      562     6162    2        IF MF.ID THEN CALL MOVE('ID',2);
      563     6163    2        CALL INDEX (SIZE, ' ', REGT(MF.REG));
      564     6164    2        CALL MOVE(REGT(MF.REG),SIZE);
      565     6165    2        CALL MOVE (',',1);
      566     6166    2        RETURN;
      567     6167
      568     6168    2   END DO_MF;
      569     6169        %EJECT;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:20   
      570     6170    1   MOVE: PROC(STRING,SZ) ALTRET;
      571     6171    2   DCL SZ SBIN;
      572     6172    2   DCL STRING CHAR(SZ);
      573     6173    2   DCL BUFFER CHAR(SZ) BASED;
      574     6174                                 /* Will this new string exceed maximum buffer size ? */
      575     6175    2        FSS = FSS+SZ;
      576     6176    2        IF FSS > BUF_LENGTH THEN ALTRETURN;
      577     6177                                                /* No.  Move it                       */
      578     6178    2        S_D.D$->BUFFER = STRING;
      579     6179    2        S_D.D$ = PINCRC(S_D.D$,SZ);
      580     6180    2        RETURN;
      581     6181                       /* Put out the yfield symbolically if it will fit in the buffer.
      582     6182                          altreturn if not.  */
      583     6183    2   END MOVE;
      584     6184        %EJECT;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:21   
      585     6185        /*** SYMBOLIC_YFIELD: takes the Y field from INSTR, and outputs it.
      586     6186         ***   if YFIELD_HAS_REG, top 3 bits are a register, so use octal.
      587     6187         ***   if YFIELD_IN_OCTAL, don't try to output as ENTDEF+xx.
      588     6188         ***/
      589     6189    1   SYMBOLIC_YFIELD: PROC ALTRET;
      590     6190    2   DCL SAVE_EWSQ# UBIN;
      591     6191    2   DCL MSS SBIN;
      592     6192    2   DCL YY SBIN;
