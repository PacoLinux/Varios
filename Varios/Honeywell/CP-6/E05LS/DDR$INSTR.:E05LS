PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:15   
      429     6029    1   DECODE_DESC:;      /* this is a descriptor following an EIS instruction */
      430     6030    1     YFIELD_HAS_REG = S_D.EIS.MODS.AR;
      431     6031    1     YFIELD_IN_OCTAL = '0'B;
      432     6032    1     SIZE_IS_REG = S_D.EIS.MODS.RL;
      433     6033    1     TAG = 0;
      434     6034    2     DO CASE (S_D.EIS.DESC_TYPE);
      435     6035    2       CASE (D_ADSC);      /* ADSCn */
      436     6036    2          CALL MOVE ('ADSC',4) ALTRET (TOO_BIG);
      437     6037    2          CALL MOVE (EISTAG(ADSC.TA), 2) ALTRET (TOO_BIG);
      438     6038    2          CALL SYMBOLIC_YFIELD ALTRET (TOO_BIG);
      439     6039    2          REGLEN = ADSC.CN;
      440     6040    2          IF (ADSC.TA = 0) THEN REGLEN = REGLEN / 2;
      441     6041    2          CALL MOVE (BINTOCHAR (REGLEN), 2) ALTRET (TOO_BIG);
      442     6042    2          REGLEN = ADSC.N;
      443     6043    2          CALL MOVE_REGLEN ALTRET (TOO_BIG);
      444     6044    2       CASE (D_BDSC);      /* BDSC */
      445     6045    2          CALL MOVE ('BDSC ', 5);
      446     6046    2          CALL SYMBOLIC_YFIELD ALTRET (TOO_BIG);
      447     6047    2          REGLEN = BDSC.N;
      448     6048    2          CALL MOVE_REGLEN ALTRET (TOO_BIG);
      449     6049    2          CALL MOVE (BINTOCHAR(BDSC.C),2) ALTRET (TOO_BIG);
      450     6050    2          SIZE = BDSC.B;
      451     6051    2          CALL MOVE (',',1) ALTRET (TOO_BIG);
      452     6052    2          CALL DDR$BINTXT(C$,SIZE,%OCT_FMT,SIZE);
      453     6053    2          CALL MOVE (CSTRING,SIZE) ALTRET (TOO_BIG);
      454     6054    2       CASE (D_NDSC);      /* NDSCn */
      455     6055    2          CALL MOVE ('NDSC', 4) ALTRET (TOO_BIG);
      456     6056    2          CALL MOVE (NDSC_SIZE(NDSC.TN),2) ALTRET (TOO_BIG);
