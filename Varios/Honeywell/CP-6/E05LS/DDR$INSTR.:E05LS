      459     6059    2          REGLEN = NDSC.N;
      460     6060    2          CALL MOVE_REGLEN ALTRET (TOO_BIG);
      461     6061    2          CALL MOVE (BINTOCHAR(NDSC.SX),3) ALTRET (TOO_BIG);
      462     6062    2          SIZE = NDSC.SF;
      463     6063    2          CALL DDR$BINTXT(C$,SIZE,%UINT_FMT,SIZE);
      464     6064    2          CALL MOVE (CSTRING,SIZE) ALTRET (TOO_BIG);
      465     6065    2       CASE (D_SDSC);      /* SDSCn */
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:16   
      466     6066    2          CALL MOVE ('SDSC', 4) ALTRET (TOO_BIG);
      467     6067    2          CALL MOVE (SDSC_SIZE(SDSC.B),2) ALTRET (TOO_BIG);
      468     6068    2          CALL SYMBOLIC_YFIELD ALTRET (TOO_BIG);
      469     6069    2          CALL MOVE (BINTOCHAR(SDSC.CN),1) ALTRET (TOO_BIG);
      470     6070    2          CALL MOVE (BINTOCHAR(SDSC.L),2) ALTRET (TOO_BIG);
