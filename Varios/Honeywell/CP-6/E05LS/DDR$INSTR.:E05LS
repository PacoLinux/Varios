      594     6194
      595     6195                            /* Save schema state.  Set schema state to 'NO SCHEMA' in
      596     6196                               order to force DDR$POSIT to return ENTDEF+offset. */
      597     6197    2        SAVE_EWSQ# = S_D.EWSQ#;
      598     6198
      599     6199    2        YY = INSTR.Y;
      600     6200    2        IF YFIELD_HAS_REG
      601     6201    3          THEN DO;
      602     6202    3            YY = INSTR.YFIELD.YY;
      603     6203    3            GOTO OCTAL_YFIELD;
      604     6204    3          END;
      605     6205    2        IF YFIELD_IN_OCTAL THEN GOTO OCTAL_YFIELD;
      606     6206                                                /* Compute maximum string size        */
      607     6207    2        MSS = POFFC(S_D.DESCL$,S_D.D$);
      608     6208    2        CALL DDR$POSIT(YY,S_D.D$,MSS,SIZE) ALTRET(OCTAL_YFIELD);
      609     6209    2        S_D.D$ = PINCRC(S_D.D$,SIZE);
      610     6210    2        FSS = FSS + SIZE;
      611     6211                                                /* Restore old schema state           */
      612     6212    2        S_D.EWSQ# = SAVE_EWSQ#;
      613     6213    2        RETURN;
      614     6214
      615     6215    2   OCTAL_YFIELD:;
      616     6216    2        S_D.EWSQ# = SAVE_EWSQ#;
      617     6217    2        CALL DDR$BINTXT(C$,YY,%OCT_FMT,SIZE);
      618     6218    2        CALL MOVE (CSTRING,SIZE) ALTRET (NO_ROOM);
      619     6219    2        RETURN;
      620     6220
      621     6221    2   NO_ROOM:;
