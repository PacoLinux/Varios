PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:1    
        1        1        /*M* DDR$INSTR REPORT INSTRUCTION ROUTINE */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*D*  NAME:  DDR$INSTR
        8        8              CALL:  CALL DDR$INSTR(INS)   ALTRET(TOO_BIG)
        9        9              INPUT: INS is the instruction to be formatted
       10       10                     S_D.D$ is the buffer pointer
       11       11                     S_D.DESCL$ is the pointer to the end of the buffer
       12       12              OUTPUT:The formatted instruction is sent to the buffer
       13       13              DESCRIPTION:  The length of the instruction is determined
       14       14                     from the op code table.  multi-word instructions
       15       15                     are handled separately.  Single word instructions
       16       16                     have two general cases: IT modification and non IT
       17       17                     (I.E., R, RI or IR) modification. An attempt is
       18       18                     made to put out each significant field symbolically in
       19       19                     the form
       20       20                           OP! yfield,tag,PR
       21       21                     if there is not enough buffer for this representation
       22       22                     or if it is inappropriate to put out the yfield
       23       23                  symbolically (IC, DU, DL, or IT, or AR) then octal representation
       24       24                     is used.  An ALTRET is taken if octal cant happen.
       25       25
       26       26                     If we're in MODIFY, then EIS instructions will set a
       27       27                     flag so that <lf>'s will decode the DSC's associated with
       28       28                     the EIS instruction.
       29       29                     */
       30       30        DDR$INSTR: PROC(INS)   ALTRET;
       31       31    1   DCL INS BIT(36) ALIGNED;
       32       32        %INCLUDE DD$BASED;
       33     1335        %HOST_INSTR(NAME=INSTR,STCLASS="");
       34     1357    1   DCL 1 EIS0 REDEF INSTR,
       35     1358    1         2 FP BIT(1) UNAL,
       36     1359    1         2 * BIT(1) UNAL,
       37     1360    1         2 MF3 BIT(7) UNAL,
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:2    
       38     1361    1         2 T BIT(1) UNAL,
       39     1362    1         2 RD BIT(1) UNAL,
       40     1363    1         2 MF2 BIT(7) UNAL,
       41     1364    1         2 OP BIT(10) UNAL,
       42     1365    1         2 I BIT(1) UNAL,
       43     1366    1         2 MF1 BIT(7) UNAL;
       44     1367    1   DCL 1 EIS1 REDEF INSTR,
       45     1368    1         2 FILL UBIN(9) UNAL,
       46     1369    1         2 BOLR REDEF FILL,
       47     1370    1           3 * BIT(5) UNAL,
       48     1371    1           3 X UBIN(4) UNAL,
       49     1372    1         2 * BIT(27) UNAL;
       50     1373    1   DCL 1 CLIMB0 REDEF INSTR,
       51     1374    1         2 ALT BIT(1),
       52     1375    1         2 X0 UBIN(17) UNAL,
       53     1376    1         2 * BIT(18);
       54     1377    1   DCL 1 CLIMB1 REDEF INSTR,
       55     1378    1         2 E BIT(1) UNAL,
       56     1379    1         2 P UBIN(9) UNAL,
       57     1380    1         2 * BIT(8) UNAL,
       58     1381    1         2 X0 BIT(1) UNAL,
       59     1382    1         2 NOSL BIT(1) UNAL,
       60     1383    1         2 * BIT(2) UNAL,
       61     1384    1         2 OP UBIN(2) UNAL,
       62     1385    1         2 SID UBIN(12) UNAL;
       63     1386        %ADSC_INSTR(STCLASS="REDEF INSTR");
       64     1394        %BDSC_INSTR(STCLASS="REDEF INSTR");
       65     1401        %NDSC_INSTR(STCLASS="REDEF INSTR");
       66     1410        %SDSC_INSTR(STCLASS="REDEF INSTR");
       67     1419        %VDSC_INSTR(STCLASS="REDEF INSTR");
       68     1426        %ARG_INSTR(STCLASS="REDEF INSTR");
       69     1434    1   DCL TAGS(0:63) CHAR(4) CONSTANT ALIGNED INIT
       70     1435    1   ('    ',',AU ',',QU ',',DU ',',IC ',',AL ',',QL ',',DL ',
       71     1436    1    ',X0 ',',X1 ',',X2 ',',X3 ',',X4 ',',X5 ',',X6 ',',X7 ',
       72     1437    1    ',N* ',',AU*',',QU*',',.23',',IC*',',AL*',',QL*',',.27',
       73     1438    1    ',X0*',',X1*',',X2*',',X3*',',X4*',',X5*',',X6*',',X7*',
       74     1439    1    ',F  ',',.41',',.42',',.43',',SD ',',SCR',',.46',',.47',
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:3    
       75     1440    1    ',CI ',',I  ',',SC ',',AD ',',DI ',',DIC',',ID ',',IDC',
       76     1441    1    ',*N ',',*AU',',*QU',',*DU',',*IC',',*AL',',*QL',',*DL',
       77     1442    1    ',*X0',',*X1',',*X2',',*X3',',*X4',',*X5',',*X6',',*X7');
       78     1443    1   DCL PREGS(0:7) CHAR(4) CONSTANT ALIGNED INIT
       79     1444    1   (',PR0',',PR1',',PR2',',PR3',',PR4',',PR5',',PR6',',PR7');
       80     1445    1   DCL EISTAG(0:3) CHAR(2) CONSTANT ALIGNED INIT ('9 ','6 ','4 ','x ');
       81     1446    1   DCL SDSC_SIZE(0:1) CHAR(2) CONSTANT ALIGNED INIT ('9 ', '8 ');
       82     1447    1   DCL NDSC_SIZE(0:1) CHAR(2) CONSTANT ALIGNED INIT ('9 ', '4 ');
       83     1448    1   DCL BINTOCHAR(0:7) CHAR(4) CONSTANT ALIGNED INIT
       84     1449    1   (',0, ',',1, ',',2, ',',3, ',',4, ',',5, ',',6, ',',7, ');
       85     1450    1   DCL FSS SBIN;
       86     1451    1   DCL BUF_LENGTH SBIN;
       87     1452    1   DCL A$REA CHAR(BUF_LENGTH) BASED;
       88     1453    1   DCL LENGTH SBIN;
       89     1454    1   DCL YY SBIN;
       90     1455    1   DCL CSTRING CHAR(40);
       91     1456    1   DCL CHAR1 CHAR(1) BASED;
       92     1457    1   DCL C$ PTR;
       93     1458    1   DCL SIZE SBIN;
       94     1459    1   DCL REGLEN UBIN;
       95     1460    1   DCL TM SBIN;
       96     1461    1   DCL TD SBIN;
       97     1462    1   DCL TAG UBIN;
       98     1463    1   DCL TAGG CHAR(4);
       99     1464    1   DCL X0 BIT(18) HALIGNED;
      100     1465    1   DCL YFIELD_HAS_REG BIT(1);
      101     1466    1   DCL YFIELD_IN_OCTAL BIT(1);
      102     1467    1   DCL SIZE_IS_REG BIT(1);
      103     1468    1   DCL SAVE_D$_INIT PTR;
      104     1469        %SUB TM_IR = 3;
      105     1470        %SUB TM_IT = 2;
      106     1471        %SUB TM_RI = 1;
      107     1472        %SUB TD_DU = 3;
      108     1473        %SUB TD_DL = 7;
      109     1474        %SUB TD_IC = 4;
      110     1475             %INCLUDE XUH_MACRO_C;
      111     1559             %INCLUDE DD_SD_M;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:4    
      112     2895             %S_D;
      113     4213        %INCLUDE DD_ERR_C;
      114     4610        %INCLUDE DD_SUBS_C;
      115     4881        %INCLUDE B_STRINGS_C;
      116     5010        %INCLUDE DD$DITEM;
       12+    5113             %LIST;
       13+    5114    1        DCL SDD$ PTR;
       14+    5115    1        DCL ZITEM$ PTR;
       15+    5116    1        DCL SDD_L6$ PTR;
       16+    5117    1        DCL ZITEM_L6$ PTR;
      117     5691        %OP$TAB (STCLASS=AUTO);
      118     5718    1   DCL DDR$BINTXT ENTRY(4);
      119     5719    1   DCL DDB$OCTAL_TO_INSTR ENTRY(2) ALTRET;
      120     5720    1   DCL DDE$ERROR ENTRY;
      121     5721    1        S_D.ACCESS = '1'B;
      122     5722    1        REMEMBER CANT_ACCESS IN S_D.REM_ACCESS;
      123     5723    1        INSTR = INS;
      124     5724    1        S_D.ACCESS = '0'B;
      125     5725                                                /*  Initialize final string size      */
      126     5726    1        FSS = 0;
      127     5727                                 /* Save initial buffer pointer, in case backup to octal
      128     5728                                    is necessary */
      129     5729    1        SAVE_D$_INIT = S_D.D$;
      130     5730    1        C$ = ADDR(CSTRING);                /* Pointer to auto char string        */
      131     5731    1        BUF_LENGTH = POFFC(S_D.DESCL$,S_D.D$);
      132     5732                                 /* Guarantee that octal representation is possible.  */
      133     5733    2        IF BUF_LENGTH <13 THEN DO;
      134     5734    2             S_D.ERROR = E@BUFFULL;           /* E@BUFFULL */
      135     5735    2             ALTRETURN;
      136     5736    2           END;
      137     5737                                                /* Blank the buffer                   */
      138     5738    1        S_D.D$->A$REA = ' ';
      139     5739                                      /* Point to the instruction to be formatted     */
      140     5740                                      /* Locate the instruction in the op code table  */
      141     5741    1        IF S_D.EIS.DESC# AND S_D.MACTIVE THEN GOTO DECODE_DESC;
      142     5742    1        CALL DDB$OCTAL_TO_INSTR(INSTR,OP$TAB) ALTRET(PURE_OCTAL);
      143     5743                            /* Distinguish between single and multi-word instructions */
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:5    
      144     5744    1        LENGTH = OP$TAB.LENGTH;
      145     5745                                                /* Move op mnemonic into buffer       */
      146     5746    1        CALL INDEX (SIZE, ' ', OP$TAB.MNE);
      147     5747    1        CALL MOVE (OP$TAB.MNE, SIZE) ALTRET(TOO_BIG);
      148     5748    1        IF OP$TAB.NIS.AWDX AND NOT INSTR.AR
      149     5749    1        THEN CALL MOVE( 'X', 1 ) ALTRET( TOO_BIG );
      150     5750                       /* If the inhibit bit is set put out '! ' else put out ' '     */
      151     5751    1        IF INSTR.I THEN CALL MOVE('! ',2)ALTRET(TOO_BIG);
      152     5752    1        ELSE CALL MOVE(' ',1) ALTRET(TOO_BIG);
      153     5753    1        IF LENGTH > 1
      154     5754    1        THEN GOTO MULTI_WORD_OP;
      155     5755                                                /* Convert yfield from bin to text    */
      156     5756                                                /* Get TM type                        */
      157     5757    1        TM = INSTR.TAG.TM;
      158     5758    1        TD = INSTR.TAG.TD;
      159     5759    1        TAG = INSTR.TAGG;
      160     5760    1        YFIELD_HAS_REG = INSTR.AR | OP$TAB.NIS.AWDX;
      161     5761    1        YFIELD_IN_OCTAL = OP$TAB.NIS.OCTALY;
      162     5762                       /* Decide whether to put Y field out symbolically or in octal,
      163     5763                          that IS: Put out y field symbolically only if tm is not it
      164     5764                          and TD is not DU,DL or IC.  */
      165     5765    1        IF (TM=TM_IT) OR (TD=TD_DU) OR (TD=TD_DL) OR (TD=TD_IC) OR YFIELD_HAS_REG
      166     5766    1          THEN
      167     5767    1             YFIELD_IN_OCTAL = '1'B;
      168     5768                                                /* Try for symbolic yfield            */
      169     5769    1        CALL SYMBOLIC_YFIELD ALTRET (TOO_BIG);
      170     5770                                                /* Put out TAG field                  */
      171     5771    1        IF OP$TAB.NIS.STCA
      172     5772    1          THEN
      173     5773    1             CALL CONCAT (TAGG, ',.', BINASC(TAG/8+48), BINASC(MOD(TAG,8)+48));
      174     5774    1          ELSE
      175     5775    1             TAGG = TAGS(TAG);
      176     5776    1        CALL INDEX (SIZE, ' ', TAGG);
      177     5777    1        CALL MOVE (TAGG, SIZE) ALTRET (TOO_BIG);
      178     5778                                                /* see if we need a ,PRx on instr     */
      179     5779    1        IF YFIELD_HAS_REG
      180     5780    2          THEN DO;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:6    
      181     5781    2             IF TAG = 0 THEN CALL MOVE (',',1) ALTRET (TOO_BIG);
      182     5782    2             CALL MOVE(PREGS(INSTR.YFIELD.DR),4) ALTRET(TOO_BIG);
      183     5783    2          END;
      184     5784    1        RETURN;
      185     5785        %EJECT;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:7    
      186     5786                            /* Multi word instruction. */
      187     5787    1   MULTI_WORD_OP:
      188     5788    1        S_D.KILLEDEF = '0'B;
      189     5789    1        IF S_D.MACTIVE
      190     5790    2          THEN DO;
      191     5791    2             S_D.EIS.DESC1.DESC# = '1'B;
      192     5792    2             S_D.EIS.DESC1.DESC_TYPE = OP$TAB.DESC1;
      193     5793    2             S_D.EIS.DESC1.MODS = EIS0.MF1;
      194     5794    2          END;
      195     5795    1        IF OP$TAB.TYPE=4 THEN GOTO CLIMB_INSTR;  /* special */
      196     5796    1        CALL DO_MF (EIS0.MF1);
      197     5797    1        IF (OP$TAB.LENGTH > 2)
      198     5798    2          THEN DO;
      199     5799    2             IF S_D.MACTIVE
      200     5800    3               THEN DO;
      201     5801    3                  S_D.EIS.DESC2.DESC# = '1'B;
      202     5802    3                  S_D.EIS.DESC2.DESC_TYPE = OP$TAB.DESC2;
      203     5803    3                  S_D.EIS.DESC2.MODS = EIS0.MF2;
      204     5804    3               END;
      205     5805    2             IF OP$TAB.DESC2 ~= 6 /*arg*/ THEN CALL DO_MF (EIS0.MF2);
      206     5806    2          END;
      207     5807    1        IF (OP$TAB.LENGTH > 3)
      208     5808    2          THEN DO;
      209     5809    2             IF S_D.MACTIVE
      210     5810    3               THEN DO;
      211     5811    3                  S_D.EIS.DESC3.DESC# = '1'B;
      212     5812    3                  S_D.EIS.DESC3.DESC_TYPE = OP$TAB.DESC3;
      213     5813    3                  S_D.EIS.DESC3.MODS = EIS0.MF3;
      214     5814    3               END;
      215     5815    2             IF OP$TAB.DESC3 ~= 6 /*arg*/ THEN CALL DO_MF (EIS0.MF3);
      216     5816    2          END;
      217     5817    2        IF OP$TAB.EIS.FILL OR OP$TAB.EIS.MASK THEN DO;
      218     5818    2           YY=EIS1.FILL;
      219     5819    2           CALL DDR$BINTXT(C$,YY,%OCT_FMT,SIZE);
      220     5820    2           CALL MOVE (CSTRING,SIZE);
      221     5821    2           CALL MOVE (',',1);
      222     5822    2           END;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:8    
      223     5823    2        IF OP$TAB.EIS.BOLR THEN DO;
      224     5824    2           YY=EIS1.BOLR.X;
      225     5825    2           CALL DDR$BINTXT(C$,YY,%OCT_FMT,SIZE);
      226     5826    2           CALL MOVE (CSTRING,SIZE);
      227     5827    2           CALL MOVE (',',1);
      228     5828    2           END;
      229     5829    1        IF OP$TAB.EIS.RD THEN IF EIS0.RD THEN CALL MOVE ('RD,',3);ELSE CALL MOVE('0,',2
              5829                 );
      230     5830    1        IF OP$TAB.EIS.P THEN IF EIS0.FP THEN CALL MOVE ('P,',2);ELSE CALL MOVE('0,',2);
      231     5831    1        IF OP$TAB.EIS.F THEN IF EIS0.FP THEN CALL MOVE ('F,',2);ELSE CALL MOVE('0,',2);
      232     5832    1        IF OP$TAB.EIS.T THEN IF EIS0.T THEN CALL MOVE ('T,',2);ELSE CALL MOVE('0,',2);
      233     5833    1        S_D.D$=PINCRC(S_D.D$,-1);
      234     5834    1        RETURN;
      235     5835        %EJECT;
PL6.E3A0      #001=DDR$INSTR File=DDR$INSTR.:E05TSI                              TUE 07/29/97 14:07 Page:9    
      236     5836    1   CLIMB_INSTR:;
      237     5837    1        IF CLIMB0.ALT THEN CALL MOVE ('alt,',4) ALTRET (TOO_BIG);
      238     5838    1        X0 = BINBIT(CLIMB0.X0,18);
      239     5839    2        DO SELECT(X0);
      240     5840    2          SELECT('000001'O); CALL MOVE('M$EXIT',SIZEC('M$EXIT'));
      241     5841    2          SELECT('000002'O); CALL MOVE('M$ERR',SIZEC('M$ERR'));
      242     5842    2          SELECT('000003'O); CALL MOVE('M$XXX',SIZEC('M$XXX'));
