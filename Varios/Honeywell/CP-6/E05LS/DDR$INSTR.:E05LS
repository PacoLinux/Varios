      417     6017    1   PURE_OCTAL:
      418     6018                            /* Put out instruction in pure octal (12 octal characters)
      419     6019                            reset the buffer pointer and convert.  */
      420     6020    1        S_D.KILLEDEF = '0'B;
      421     6021    1        S_D.D$ = SAVE_D$_INIT;
      422     6022    1        CALL DDR$BINTXT(S_D.D$,INSTR,%OCT_FMT,SIZE);
      423     6023    1        S_D.D$ = PINCRC(S_D.D$,SIZE);
