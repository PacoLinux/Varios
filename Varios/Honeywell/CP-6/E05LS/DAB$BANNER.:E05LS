VERSION E05

PL6.E3A0      #001=DAB$BANNER File=DAB$BANNER.:E05TSI                            TUE 07/29/97 12:34 Page:1    
        1        1        /*M* DAB$BANNER - PRODUCE CRASH DUMP BANNER */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        DAB$BANNER: PROC;
        8        8        /**/
        9        9    1   DCL DAB$BLKPRT ENTRY(5) ALTRET;
       10       10    1   DCL DAF$WRITE EPTR SYMREF;
       11       11    1   DCL DAF$INIT ENTRY(1) ALTRET;
       12       12        /**/
       13       13        %INCLUDE F$DCB;
       14       62        %INCLUDE DA_FORMAT_C;
       15      304        %INCLUDE DA_MAP_R;
       16      334        /**/
       17      335    1   DCL M$DUMPFILE DCB;
       18      336        %DA_HDR (NAME=DA_HDR,STCLASS=SYMREF);
       19      367    1   DCL DFNAME CHAR(10);
       20      368    1   DCL SCODE CHAR(13);
       21      369    1   DCL DATE CHAR(10);
       22      370        /**/
       23      371    1   DCL 1 TTL STATIC,
       24      372    1         2 L UBIN BYTE UNAL INIT(SIZEC(TTL.T)),
       25      373    1         2 T,
       26      374    1           3 * CHAR(0) INIT('DUMPFILE '),
       27      375    1           3 DFNAME CHAR(9);
       28      376        %REPORT (NAME=BANRPT,CONTENTS=NO,TRUNC=YES,TITLEADR=TTL);
       29      442        %EJECT;
PL6.E3A0      #001=DAB$BANNER File=DAB$BANNER.:E05TSI                            TUE 07/29/97 12:34 Page:2    
       30      443    2           IF DA_DUMPFILE THEN DO;
       31      444    2              DFNAME=DCBADDR(DCBNUM(M$DUMPFILE))->F$DCB.NAME.C;
       32      445    2              TTL.T.DFNAME=DFNAME;
       33      446    2              END;
       34      447    2           ELSE DO;
       35      448    2              DFNAME='RUNNING';
       36      449    2              TTL.T.DFNAME='HUH?';
       37      450    2              END;
       38      451    1           CALL DAF$INIT(BANRPT);
       39      452    1           CALL BLKPRT(DFNAME,SIZEC(DFNAME));
       40      453    1           SCODE=DA_HDR.TITLE.SCODEA;
       41      454    1           CALL BLKPRT(SCODE,SIZEC(SCODE));
       42      455    1           DATE=DA_HDR.TITLE.DT;
       43      456    1           CALL BLKPRT(DATE,SIZEC(DATE));
       44      457    1           RETURN;
       45      458        %EJECT;
PL6.E3A0      #001=DAB$BANNER File=DAB$BANNER.:E05TSI                            TUE 07/29/97 12:34 Page:3    
       46      459    1   BLKPRT: PROC(TXT,SIZE);
       47      460    2   DCL TXT CHAR(SIZE);
       48      461    2   DCL SIZE SBIN;
       49      462        /**/
       50      463    2   DCL I SBIN;
       51      464    2   DCL B CHAR(132);
       52      465    2   DCL BS SBIN;
       53      466    2   DCL B$ PTR;
       54      467    2   DCL BUF CHAR(BS) BASED(B$);
       55      468        /**/
       56      469    2           B$=ADDR(B);
       57      470    3           DO I=0 TO 9;
       58      471    3              CALL DAB$BLKPRT(I,TXT,SIZE,B,BS) ALTRET(DO_BLANKS);
       59      472    3              CALL DAF$WRITE(VECTOR(BUF));
       60      473    3              END;
       61      474    2   DO_BLANKS:
       62      475    3           DO I=0 TO 2;
       63      476    3              CALL DAF$WRITE(VECTOR(BUF));
       64      477    3              END;
       65      478    2           RETURN;
       66      479    2   END BLKPRT;
       67      480    1   END DAB$BANNER;
       68      481        %EOD;

