       35     1077    1   DCL 1 DUPCHANERR_TEXT,
       36     1078    1         2 COUNT UBIN(9) UNAL,
       37     1079    1         2 CHANNEL CHAR(8) UNAL,
PL6.E3A0      #001=ITS$SYSCON File=ITS$SYSCON.:E05TSI                            TUE 07/29/97 23:08 Page:2    
       38     1080    1         2 IOMTEXT CHAR(1) UNAL,
       39     1081    1         2 DASH CHAR(1) UNAL,
       40     1082    1         2 CHANTEXT CHAR(2) UNAL;
       41     1083        /**/
       42     1084        /*  BASED  */
       43     1085        /**/
       44     1086        %NI$DVT;
       45     1093        %NI$SQH;
       46     1106        %NI$DQH;
       47     1129        %NI$CHT;
       48     1132        %NI$DCT;
       49     1180        %NI$IOM;
       50     1185        %NI$IOSTAT;
       51     1194        %NI$MBX;
       52     1213        %OC$INFO (FPTN=OC$INFO,STCLASS=BASED);
       53     1250        %IT$CONFIG (NAME=IT_CONFIG,STCLASS=SYMREF);
       54     1311        %NI$PARTITION (NAME=IT_PARTITION, STCLASS=SYMREF);
       55     1319        /**/
       56     1320        /*  ENTRY DCLS  */
       57     1321        /**/
       58     1322    1   DCL ITE$ERROR ENTRY(2);
       59     1323    1   DCL NIC$POSTER ENTRY(3);
       60     1324    1   DCL ITH$ALLOCATE ENTRY(2) ALTRET;
