PL6.E3A0      #001=ITS$SYSCON File=ITS$SYSCON.:E05TSI                            TUE 07/29/97 23:08 Page:3    
       74     1454        /*D*     NAME:     ITS$SYSCON
       75     1455                 PURPOSE:  To build the necessary tables for the system console
       76     1456        *
       77     1457                 INPUT:    PTR$ - parameter - pointer to next available location
       78     1458                           in the TIGR table area
       79     1459                           Command table - built in DS5 by ITC$CHECK_CON
       80     1460        *
       81     1461                 OUTPUT:   The initialized tables
       82     1462        *
       83     1463                 DESCRIPTION:  ITS$SYSCON aquirs space for and initializes the
       84     1464                           NI$SQH,NI$DQH and NI$DCT entries for this console. In
       85     1465                           addition the cell B_SCDCTX is initialized to the DCT
       86     1466                           index of this console if this is the first console
       87     1467                           defined. The channel entry in the NI$CHT table is
       88     1468                           initialized to point to the DQH.        */
       89     1469        /**/
       90     1470    1           CMD$ = B$DS5$;
       91     1471    1           CMD$ = PINCRW(CMD$,SIZEW(IT$CMDWD));
       92     1472        /**/
       93     1473    1           CALL ITH$SQH_INIT(PTR$,SQH$) ALTRET(ITS$SYSCON_ERR);
       94     1474    1           SQH$ -> NI$SQH.NDCT = 1;
       95     1475        /**/
       96     1476    1           DQH$ = PTR$;
       97     1477    1           CALL ITH$ALLOCATE(PTR$,SIZEW(NI$DQH)) ALTRET(ITS$SYSCON_ERR);
       98     1478        /**/
       99     1479    1           DQH$ -> NI$DQH = NI$DQH_C;     /* set to constant inited structure    */
      100     1480        /**/
      101     1481    1           DQH$ -> NI$DQH.FL$ = DQH$;
      102     1482        /*      DQH$ -> NI$DQH.IO$ = ADDR(NIL);            */
      103     1483    1           DQH$ -> NI$DQH.SQH$ = SQH$;
      104     1484    1           DQH$ -> NI$DQH.IOCHAN.IOM = CMD$ -> IT$CONSOLE.IOM#;
      105     1485    1           DQH$ -> NI$DQH.IOCHAN.CHANNEL = CMD$ -> IT$CONSOLE.CHAN;
      106     1486        /*      DQH$ -> NI$DQH.MPC$ = ADDR(NIL);           */
      107     1487    2           IF HW_IMX THEN DO;
      108     1488        /*         DQH$ -> NI$DQH.MBX$ = ADDR(NIL);        */
      109     1489        /*         DQH$->NI$DQH.IOSTATUS$ = ADDR(NIL);     */
      110     1490    2              END;
PL6.E3A0      #001=ITS$SYSCON File=ITS$SYSCON.:E05TSI                            TUE 07/29/97 23:08 Page:4    
      111     1491    2           ELSE DO;
      112     1492    2              DQH$ -> NI$DQH.MBX$ = PINCRW (NI$IOM$(CMD$->IT$CONSOLE.IOM#) -> NI$IOM.
              1492                       MBX$,
      113     1493    2                SIZEW(NI$MBX(0)) * CMD$->IT$CONSOLE.CHAN);
      114     1494    2              DQH$ -> NI$DQH.IOSTATUS$ = NI$FQ$ -> NI$FQ.HD$;
              1494                       /* STATUS IN LOW REAL MEM */
      115     1495    3              IF HW_IOP THEN DO;
      116     1496    3                 DQH$ -> NI$DQH.IOSTATUS$ -> NI$IOSTAT.AESDCW = %IOP_AESDCW_INIT;
      117     1497    3                 DQH$ -> NI$DQH.IOSTATUS$ -> NI$IOSTAT.AESDCW.ADDRESS =
      118     1498    3                   POFFW (ADDR (DQH$ -> NI$DQH.IOSTATUS$ -> NI$IOSTAT.EXTST), B$REAL$);
      119     1499    3                 END;
      120     1500
      121     1501    2              NI$FQ$ -> NI$FQ.HD$ = PINCRW(NI$FQ$ -> NI$FQ.HD$,SIZEW(NI$IOSTAT));
      122     1502    2              END;
      123     1503        /**/
      124     1504    1         IF ((IT_CONFIG.IOM.PORT = NI$IOM$(CMD$->IT$CONSOLE.IOM#)->NI$IOM.ADDRESS) AND
      125     1505    1             (IT_CONFIG.SC01.CHAN = CMD$->IT$CONSOLE.CHAN)) THEN AARDCONFOUND = '1'B;
      126     1506        /**/
      127     1507    1           CHTX = ((CMD$ -> IT$CONSOLE.IOM#)*%N_MAXCHANS) + CMD$ -> IT$CONSOLE.CHAN;
      128     1508    2           IF (NI$CHT$ -> NI$CHT.DQH$(CHTX) ~= ADDR(NIL)) THEN DO;
      129     1509    2              DUPCHANERR_TEXT.COUNT = SIZEC(DUPCHANERR_TEXT) - 1;
      130     1510    2              DUPCHANERR_TEXT.CHANNEL = 'CHANNEL ';
      131     1511    2              CALL BINCHAR(DUPCHANERR_TEXT.IOMTEXT,CMD$ -> IT$CONSOLE.IOM#);
      132     1512    2              DUPCHANERR_TEXT.DASH = '-';
      133     1513    2              CALL BINCHAR(DUPCHANERR_TEXT.CHANTEXT,CMD$ -> IT$CONSOLE.CHAN);
      134     1514    2              CALL ITE$ERROR(%E$DUPCHAN,ADDR(DUPCHANERR_TEXT));
      135     1515    2              RETURN;
      136     1516    2              END;
      137     1517        /**/
      138     1518    1           NI$CHT$ -> NI$CHT.DQH$(CHTX) = DQH$;
      139     1519    1           NI$CHT$ -> NI$CHT.POSTER(CHTX) = ENTADDR(NIC$POSTER);
      140     1520        /**/
      141     1521    1           CALL ITH$DCT_INIT(PTR$,DEV$) ALTRET(ITS$SYSCON_ERR);
      142     1522        /**/
      143     1523    1           IF (B_SCDCTX = 0) THEN B_SCDCTX = N_DCTXMAX;
      144     1524        /**/
      145     1525    1           DEV$ -> NI$DCT.DEVNM = CMD$ -> IT$CONSOLE.NAME;
PL6.E3A0      #001=ITS$SYSCON File=ITS$SYSCON.:E05TSI                            TUE 07/29/97 23:08 Page:5    
      146     1526    1           DEV$ -> NI$DCT.DVT$ = NI$DVT$(CMD$ -> IT$CONSOLE.TYPE);
      147     1527    1           DEV$ -> NI$DCT.TYPE = DEV$ -> NI$DCT.DVT$ -> NI$DVT.TYPE;
      148     1528    1           DEV$ -> NI$DCT.MODX = CMD$ -> IT$CONSOLE.MODX;
      149     1529    1           DEV$ -> NI$DCT.FX = CMD$ -> IT$CONSOLE.FX;
      150     1530    1           DEV$ -> NI$DCT.SQH$ = SQH$;
      151     1531    1           DEV$ -> NI$DCT.DQH$ = DQH$;
      152     1532    1           DEV$ -> NI$DCT.DFLG.INPUT = YES;
      153     1533    1           DEV$ -> NI$DCT.DFLG.OUTPUT = YES;
      154     1534    1           DEV$ -> NI$DCT.DFLG.CONSOLE = YES;
      155     1535    1           IF HW_TYPE ~= 0 THEN DEV$ -> NI$DCT.DFLG.PSIA = YES;
      156     1536    1           IT_PARTITION.DEVICE(N_DCTXMAX) = CMD$->IT$CONSOLE.DOWN;
      157     1537        /**/
      158     1538    1           DEV$ -> NI$DCT.SC.OCINFO$ = PTR$;
      159     1539    1           PTR$ -> OC$INFO = CMD$ -> IT$CONSOLE.OCINFO;
      160     1540    1           CALL ITH$ALLOCATE(PTR$,SIZEW(OC$INFO)) ALTRET(ITS$SYSCON_ERR);
      161     1541    1           RETURN;
      162     1542    1   ITS$SYSCON_ERR:
      163     1543    1           ALTRETURN;
      164     1544    1   END;

