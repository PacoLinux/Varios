    9      %EQU S$DUXCON=726/* S$DUXCON */; /* DELTA's XCON routine entered for no good reason */
   10      %EQU S$DUTRAPDS=727/* S$DUTRAPDS */; /* DELTA's TRAP routine entered & S_D.DO_SCAN set */
   11      %EQU S$DUTRAP=728/* S$DUTRAP */; /* DELTA's TRAP routine entered for no good reason */
   12      %EQU S$DUEVENT=729/* S$DUEVENT*/; /* DELTA's EVENT routine entered for no good reason */
   13      %EQU S$DUMESSAGE=730/* S$DUMESSAGE*/; /* DELTA received a messages from the FD it didn't expect */
   14      %EQU S$USRREQSCR=731/* S$USRREQSCR */; /* Front end DELTA has received a CRASH command */

