    8      /**/
    9      /**/
   10      %EQU E$BADSER=650 /*E$BADSER*/;    /*SER DOESNT MATCH THAT MOUNTED ON DRIVE*/
   11      %EQU E$DUPREEL=651 /*E$DUPREEL*/;       /*THIS SER MOUNTED ON ANOTHER DRIVE*/
   12      %EQU E$REOPN=652 /*E$REOPN*/;           /*DCB ALREADY OPEN TO THIS TAPE*/
   13      %EQU E$NORES=653 /*E$NORES*/; /*ALLOCATED RESOURCES OF THIS TYPE EXHAUSTED*/
   14      %EQU E$BADRES=654 /*E$BADRES*/; /*THIS RES TYPE REQUIRES FULL VIRTUAL RES NAME*/
   15      %EQU E$NODP=655 /*E$NODP*/;             /*PRIVATE PACKS NOT IMPLEMENTED YET*/
   16      %EQU E$MNTBRK=656 /*E$MNTBRK*/;         /*BREAK OR CTL-Y DURING MOUNT*/
   17      %EQU E$NRLOPN=657 /*E$NRLOPN*/;    /*CANT RELEASE OR REMOVE AN OPEN RESOURCE*/
   18      %EQU E$NOTUR=658 /*E$NOTUR*/;           /*YOU DONT OWN SUCH A RESOURCE*/
   19      %EQU E$CANT=659 /*E$CANT*/;             /* OPERATOR CANT MOUNT YOUR TAPE      */
   20      %EQU E$SUOUT=660/*E$SUOUT*/;  /* AVAILABLE PSEUDO RESOURCES ARE ALL IN USE*/
   21      %EQU E$NORSS=661/*E$NORSS*/;  /* AVAILABLE PHYSICAL RESOURCES ARE ALL IN USE*/
   22      %EQU E$BSUNM=662/*E$BSUNM*/;            /* ILLEGAL PSEUDO RESOURCE NAME       */
   23      %EQU E$NORQ=663/*E$NORQ*/;    /* REQUIRED PSEUDO RESOURCES NOT OWNED BY USER*/
   24      %EQU E$NORSM=664/*E$NORSM*/;   /* NO MORE OF THIS RES LEFT FOR YOUR MODE */
   25      %EQU E$BDATTR=665/*E$BDATTR*/;  /* YOU ASKED FOR ATTRS ON OPEN THAT YOU DIDNT ON RESOURCE */
   26      %EQU E$NORSNN=666/*E$NORSNN*/;  /* YOU ASKED FOR A DRIVE YOU DONT HAVE */
   27      %EQU E$NORMRM=667/*E$NORMRM*/; /* NOT ENOUGH ROOM FOR M$DISPRES */
   28      %EQU E$LESMEM=668/*E$LESMEM*/; /* CANT GIVE USER MEM HE WANTS */
   29      %EQU E$PRTBSY=669/*E$PRTBSY*/; /* CANT PARTITION A BUSY DEVICE */
   30      %EQU E$UPNLDT=670/*E$UPNLDT*/; /* CANT UNPARTITION SYMBIONT - NO LDCTS */
   31      %EQU E$MAXACCT=671;   /*cant get entry in LCP-6 acct resource table*/
   32      %EQU E$MAXFPRGS=672;  /*max user slots per account exceeded */
   33      %EQU E$NORESSLOT=673; /* No resource slot for resource */

