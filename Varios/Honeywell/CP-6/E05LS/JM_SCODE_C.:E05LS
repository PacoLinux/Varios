    7      /* The batch scheduler (MBS) owns screech codes fron 225 to 249.              */
    8      /**/
    9      %SUB S$JM225 = 225;    /* Can't get mbs data segments                         */
   10      %SUB S$JM226 = 226;    /* I/O error on central systems comgroup               */
   11      %SUB S$JM227 = 227;    /* Inconsistency in batch scheduler queues             */
   12      %SUB S$JM228 = 228;    /* No fit information in input job file                */
   13      %SUB S$JM229 = 229;    /* Job info table in fit is wrong size                 */
   14      %SUB S$JM230 = 230;    /* I/O error on test open of job file                  */
   15      %SUB S$JM231 = 231;    /* Terminated job not in partition's job list          */
   16      %SUB S$JM232 = 232;    /* Terminated job not in running job list              */
   17      %SUB S$JM233 = 233;    /* Illegal event id recieved by MBS                    */
   18      %SUB S$JM234 = 234;    /* Illegal event code passed in MBS citation           */
   19      %SUB S$JM235 = 235;    /* I/O error opening/closing job file                  */
   20      %SUB S$JM236 = 236;    /* I/O error creating job status file                  */
   21      %SUB S$JM237 = 237;    /* I/O error accessing job status file                 */
   22      %SUB S$JM238 = 238;    /* Can't get free resource list for job scheduling     */
   23      %SUB S$JM239 = 239;    /* M$JOBSTATS failed mapping MBS tables                */
   24      %SUB S$JM240 = 240;    /* M$MBS failed getting PSN list                       */
   25      %SUB S$SLUGX = 241;    /* SLUG has aborted                                    */
   26      %SUB S$JM242 = 242;    /* The :JOBSTATS file has been managed improperly      */
   27      %SUB S$JM243 = 243;    /* The :JOBSTATS file contains an unknown final status */
   28      %SUB S$JM244 = 244;    /* MBS received an unknown CANCEL subcode              */

