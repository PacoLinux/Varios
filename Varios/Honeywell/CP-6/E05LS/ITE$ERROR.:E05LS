        8        8        /*P*     NAME:     ITE$ERROR
        9        9                 PURPOSE:  ITE$ERROR outputs TIGR error messages   */
       10       10        ITE$ERROR: PROC(ERR_CODE,SUB$);
       11       11        /**/
       12       12    1   DCL ERR_CODE UBIN(36);
       13       13    1   DCL SUB$ PTR;
       14       14        /**/
       15       15        /*  INCLUDE FILES  */
       16       16        /**/
       17       17        %INCLUDE IT_SUBS_C;
       18      284        /**/
       19      285        /*  MACROS  */
       20      286        /**/
       21      287            %MACRO ITERR (NAME=ITERR,C=0,T=' ',LAST=",");
       22      288                 2 NAME UBIN BYTE INIT(C) UNAL,
       23      289                  2 * UNAL,
       24      290                  3 COUNT UBIN(9) INIT(LENGTHC(T)) UNAL,
       25      291                  3 TEXT CHAR(0) INIT(T) UNAL LAST
       26      292            %MEND;
       27      293        /**/
       28      294        /*  AUTOMATIC  */
       29      295        /**/
       30      296    1             DCL ERR$ PTR;
       31      297    1   DCL ERR_INDEX SBIN;
       32      298    1   DCL LEFT SBIN;
       33      299    1   DCL SAVEOUT UBIN WORD;
       34      300        /**/
       35      301        /*  SYMREFS  */
       36      302    1   DCL IT_OUTDEV UBIN WORD SYMREF;
       37      303    1   DCL IT_ECHODEV UBIN WORD SYMREF;
PL6.E3A0      #001=ITE$ERROR File=ITE$ERROR.:E05TSI                              TUE 07/29/97 23:03 Page:2    
       38      304        /**/
       39      305        /*  STATIC  */
       40      306    1   DCL TIGR_BUF CHAR(80) SYMREF;
       41      307        /**/
       42      308        /*  ENTRY DCLS  */
       43      309        /**/
       44      310    1   DCL ITR$WRITE ENTRY(2) ALTRET;
       45      311        /**/
       46      312        /*  CONSTANTS  */
       47      313        /**/
       48      314    1   DCL ERR_END$ PTR CONSTANT INIT(ADDR(IT_ERR.ERREND));
       49      315    1   DCL 1 IT_ERR CONSTANT,                  /* ERROR MESSAGE TEXT                 */
       50      316        %INCLUDE IT_ERR_C;
       51      774        %ITERR (NAME=ERREND,C=511,LAST=";",T='**** NO MESSAGE FOUND');
       52      780        /**/
       53      781        /*  BASED  */
       54      782    1         DCL 1 IT$ERRTEXT BASED UNAL,
       55      783    1         2 CODE UBIN BYTE UNAL,
       56      784    1         2 TEXTC,
       57      785    1           3 COUNT UBIN(9) UNAL,
       58      786    1           3 TEXT CHAR(IT$ERRTEXT.TEXTC.COUNT) UNAL;
       59      787    1   DCL 1 TEXT$C BASED UNAL,
       60      788    1         2 COUNT UBIN(9) UNAL,
       61      789    1         2 TEXT CHAR(TEXT$C.COUNT) UNAL;
       62      790        %EJECT;
PL6.E3A0      #001=ITE$ERROR File=ITE$ERROR.:E05TSI                              TUE 07/29/97 23:03 Page:3    
       63      791        /*D*     NAME:     ITE$ERROR
       64      792                 PURPOSE:  To output TIGR error messages.
       65      793                 DESCRIPTION: The macro %ITERR is used to build TIGR's error
       66      794                           messages in constant data. Each error message has an
       67      795                           associated error code which is passed as a parameter
       68      796                           to ITE$ERROR and is used to find the correct message.
       69      797                           The parameter SUB$ if not null points to a TEXTC
       70      798                           string which is to precede the error message text.
       71      799                 INPUT:    ERR_CODE - code which identifies message
       72      800                           SUB$ - pointer to text to precede message
       73      801                 OUTPUT:   Error message written         */
       74      802        /**/
       75      803    1           TIGR_BUF = ' ';
       76      804    1           ERR_INDEX = 0;
       77      805    1           LEFT = LENGTHC(TIGR_BUF);
       78      806    1           ERR$ = ADDR(IT_ERR);
       79      807        /**/
       80      808    1           IF (ADDR(SUB$) ~= ADDR(NIL)) AND (SUB$  ~= ADDR(NIL))
       81      809    1           THEN CALL M_TADD(SUB$ -> TEXT$C);
       82      810        /**/
       83      811    2           DO WHILE (ERR$ < ERR_END$);
       84      812    2              IF (ERR$ -> IT$ERRTEXT.CODE = ERR_CODE) THEN GOTO GOT_IT;
       85      813    2              ERR$ = PINCRC(ERR$,SIZEC(IT$ERRTEXT.CODE) +
       86      814    2                SIZEC(IT$ERRTEXT.TEXTC.COUNT) +
       87      815    2                ERR$ -> IT$ERRTEXT.TEXTC.COUNT);
       88      816    2              END;
       89      817        /**/
       90      818    1   GOT_IT:
       91      819    1           CALL M_TADD (ERR$ -> IT$ERRTEXT.TEXTC);
       92      820    1           CALL M_TRUNC;
       93      821    1           RETURN;
       94      822        /**/
       95      823        %EJECT;
PL6.E3A0      #001=ITE$ERROR File=ITE$ERROR.:E05TSI                              TUE 07/29/97 23:03 Page:4    
       96      824        /**/
       97      825    1   M_TADD: PROC (TEXTC);
       98      826        /**/
       99      827    2   DCL 1 TEXTC UNAL,
      100      828    2         2 COUNT UBIN(9) UNAL,
      101      829    2         2 TEXT CHAR(TEXTC.COUNT) UNAL;
      102      830        /**/
      103      831        /*  AUTOMATIC  */
      104      832        /**/
      105      833    2   DCL START SBIN;
      106      834    2   DCL LE SBIN;
      107      835        /**/
      108      836        /**/
      109      837    2           START = 0;
      110      838    2           LE = TEXTC.COUNT;
      111      839        /**/
      112      840    2   MORE:
      113      841    2           CALL INSERT (TIGR_BUF,ERR_INDEX,LEFT,SUBSTR(TEXTC.TEXT,START,LE))
      114      842    2             ALTRET (TRUNC);
      115      843    2           LEFT = LEFT - LE -1;            /* LEAVE A BLANK                      */
      116      844    2           ERR_INDEX = ERR_INDEX + LE + 1;
      117      845    2           RETURN;
      118      846        /**/
      119      847    2   TRUNC:
      120      848    2           CALL ITR$WRITE (VECTOR(TIGR_BUF),LENGTHC(TIGR_BUF));
      121      849    2           START = START + LEFT;
      122      850    2           LE = LE - LEFT;
      123      851    2           GOTO MORE;
      124      852        /**/
      125      853    2   END;
      126      854        /**/
      127      855        %EJECT;
PL6.E3A0      #001=ITE$ERROR File=ITE$ERROR.:E05TSI                              TUE 07/29/97 23:03 Page:5    
      128      856        /**/
      129      857    1   M_TRUNC: PROC;
      130      858        /**/
      131      859    2           CALL ITR$WRITE (VECTOR(TIGR_BUF),ERR_INDEX);
      132      860    3           IF (IT_OUTDEV ~= IT_ECHODEV) THEN DO;
      133      861    3              SAVEOUT = IT_OUTDEV;
      134      862    3              IT_OUTDEV = IT_ECHODEV;
      135      863    3              CALL ITR$WRITE (VECTOR(TIGR_BUF),ERR_INDEX);
      136      864    3              IT_OUTDEV = SAVEOUT;
      137      865    3              END;
      138      866        /**/
      139      867    2           TIGR_BUF = ' ';
      140      868    2           LEFT = LENGTHC(TIGR_BUF);
      141      869    2           ERR_INDEX = 0;
      142      870    2           RETURN;
      143      871    2   END;
      144      872    1   END;

