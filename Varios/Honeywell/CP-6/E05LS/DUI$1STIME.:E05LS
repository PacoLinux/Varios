      730    17467    3                  TEMP$ -> DU$TREE.NEXT# = -1;
      731    17468    3                  TEMP$ -> DU$TREE.NOW_IN = '0'B;
      732    17469    3                  TEMP$ = PINCRW( TEMP$, SIZEW( DU$TREE ) );
      733    17470    3                END;
      734    17471        /* Sort the tree backwards by load address */
      735    17472    3             DO I = 0 TO S_D.NODE#M1;
      736    17473    3                  DU$NODE.INDX#( I ) = I;  /* Init sort keys */
      737    17474    3                  DU$NODE.GOTDEFS( I ) = '0'B;
      738    17475    3                  DU$NODE.EDEF$( I ) = ADDR( NIL );
      739    17476    3                  DU$NODE.EDEF_END$( I ) = ADDR( NIL );
      740    17477    3                  DU$NODE.SDEF$( I ) = ADDR( NIL );
      741    17478    3                  DU$NODE.SDEF_END$( I ) = ADDR( NIL );
      742    17479    3                  DU$NODE.DNAME$( I ) = ADDR( NIL );
      743    17480    3                END;
      744    17481    2             K = S_D.NODE#M1;
      745    17482    3             DO WHILE ( K > 0 );
      746    17483    3                  J = K - 1;
      747    17484    3                  K = 0;
      748    17485    4                  DO I = 0 TO J;
      749    17486    4                       T1 = DU$NODE.INDX#( I );
      750    17487    4                       T2 = DU$NODE.INDX#( I+1 );
      751    17488    4                       IF S_D.B$TREE$ -> B$NODE.PADDR( T1 ) < S_D.B$TREE$ -> B$NODE.
             17488                                PADDR( T2 )
      752    17489    5                       THEN DO;
PL6.E3A0      #001=DUI$1STIME File=DUI$1STIME.:E05TSI                            TUE 07/29/97 14:53 Page:22   
      753    17490    5                            DU$NODE.INDX#( I ) = T2;
      754    17491    5                            DU$NODE.INDX#( I+1 ) = T1;
      755    17492    5                            K = I;
      756    17493    5                          END;
      757    17494    4                     END;
      758    17495    3                END;
      759    17496    2             IF DU$NODE.INDX#( S_D.NODE#M1 ) ~= 0
      760    17497    3             THEN DO;
      761    17498    3                  S_D.ERROR = E@DELBUG;    /* E@DELBUG - Root should be last */
      762    17499    3                  CALL DDE$ERROR;
      763    17500    3                END;
      764    17501    3             ELSE DO;
      765    17502    3                  DU$NODE.EDEF$( 0 ) = S_D.EDEF$;
      766    17503    3                  DU$NODE.EDEF_END$( 0 ) = S_D.EDEF_END$;
      767    17504    3                  DU$NODE.SDEF$( 0 ) = S_D.SDEF$;
