      721    17458    3                END;
      722    17459    2             S_D.OVERLAID = '1'B;
      723    17460    2             S_D.MTREE$ = PINCRW( S_D.NODESEG$, SIZEW( DU$NODE ) * TSIZE );
      724    17461    2             S_D.UTREE$ = PINCRW( S_D.MTREE$, SIZEW( DU$TREE ) * (TSIZE+1) );
      725    17462    2             S_D.NODESEG_NEXT$ = PINCRW( S_D.UTREE$, SIZEW( DU$TREE ) * (TSIZE+1) );
      726    17463    2             NODESEG$ = S_D.NODESEG$;
