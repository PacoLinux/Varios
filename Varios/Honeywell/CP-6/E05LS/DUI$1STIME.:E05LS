      773    17510    3                END;
      774    17511    3             DO WHILE ( '0'B );
      775    17512    3   NOOV1:
      776    17513    3                  S_D.ERROR = E@OVTABLES;  /* E@OVTABLES */
      777    17514    3                  CALL DDE$ERROR;
      778    17515    3                END;
      779    17516    2           END;
      780    17517                                                /**/
      781    17518    1   RETN:
      782    17519    1        RETURN;
      783    17520                                                /**/
