      681    17418    3                     END;
      682    17419    3                  DO IX = 1 TO DCBRES;
      683    17420    3                       DCBPTR$ -> DCB$PTR$ = ADDR( NIL );
      684    17421    3                       DCBPTR$ = PINCRW( DCBPTR$, SIZEW( DCBPTR$ ) );
      685    17422    3                     END;
      686    17423                                                /* Setup tree table pointers */
      687    17424    2                  IF BHEAD.OVRL
      688    17425    3                    THEN DO;              /* program is overlayed */
      689    17426    3                       TEMP$ = PINCRW( TEMP$, BHEAD.TREEDISP );
      690    17427    3                       S_D.B$ROS$ -> B$ROSEG.TREE$ = TEMP$;
      691    17428    3                       TEMP1$ = PINCRW( TEMP$, SIZEW( B$TREEHDR ) );
      692    17429    3                       TEMP1$ -> B$NODE.FLGS.NOW_IN( 0 ) = '1'B; /* Root as in */
