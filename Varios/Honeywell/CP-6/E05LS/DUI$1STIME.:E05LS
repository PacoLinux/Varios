      702    17439    3                    END;
      703    17440    2                END;
      704    17441    1        TEMP$ = S_D.B$ROS$ -> B$ROSEG.TREE$;
      705    17442    1        IF GOTHEAD AND BHEAD.OVRL AND
      706    17443    1           NOT (S_D.ANLZ AND NOT(S_D.ANLZ_FILE)) AND
      707    17444    1           NOT S_D.ANLZ_KLUDGE
      708    17445    2        THEN DO;
      709    17446    2             S_D.B$TREEHDR$ = TEMP$;
      710    17447    2             TSIZE = TEMP$ -> B$TREEHDR.NODES;
      711    17448    2             TEMP$ = PINCRW( TEMP$, SIZEW( B$TREEHDR ) );
      712    17449    2             S_D.B$TREE$ = TEMP$;
      713    17450    2             S_D.NODE#M1 = TSIZE - 1;
      714    17451    2             ARS = 2 * (TSIZE+1) * SIZEW(DU$TREE) + TSIZE * SIZEW(DU$NODE);
      715    17452    2             NEED = POFFW( S_D.NODESEG_USED$, S_D.NODESEG$ ) - ARS;
      716    17453    2             IF NEED < 0
PL6.E3A0      #001=DUI$1STIME File=DUI$1STIME.:E05TSI                            TUE 07/29/97 14:53 Page:21   
      717    17454    3             THEN DO;
      718    17455    3                  ARS = ( ( -NEED + @PSIZ - 1 ) / @PSIZ ) * @PSIZ;
