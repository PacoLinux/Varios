PL6.E3A0      #001=QBV$VERI_PRES File=QBV$VERI_PRES.:E05TSI                      WED 07/30/97 04:57 Page:7    
      143    19057    1   QBV$PRESENT: ENTRY;
      144    19058
      145    19059        /*D*  NAME:         QBV$PRESENT
      146    19060              CALL:         CALL QBV$PRESENT;
      147    19061              PARAMETERS:   None.
      148    19062              INTERFACE:    Called by QAT$TALLYHO when a PRESENT op-code is
      149    19063                            encountered.
      150    19064              ENVIRONMENT:  Nothing unusual.
      151    19065              INPUT:        QI_1,
      152    19066                            FPL rununit static data following the op-code.
      153    19067              OUTPUT:       Moves data from the COBOL DAD(s) to the FIELD DAD(s).
      154    19068                            Also sets QI$GZ_FIELD.INDICATORS$ ->
      155    19069                                      QI$INDICATORS.STATE_FLAGS.PRESENTED.
      156    19070              SCRATCH:      Various otter data.
      157    19071              DESCRIPTION:  Move data from COBOL (FILE or WORKING-STORAGE SECTION) DADs
      158    19072                            to FIELD (FRAME SECTION) DADs.  That means setting up not
      159    19073                            only the COBOL DAD addresses but also QI$GZ_FIELD
      160    19074                            stuff.  About the only real oddity is that the compiler
      161    19075                            gens only one argument@P for each COBOL DAD, so if the
      162    19076                            DAD is an array (has an OCCURS clause associated with it),
      163    19077                            the location of each array element has to be devined
      164    19078                            by this routine.  Ish.
      165    19079        */
      166    19080
      167    19081    1       THIS_IS_A_VERIFY = %NO;
      168    19082    1       GOTO SO_DO_THE_VERI_PRES_ALREADY;
      169    19083
      170    19084    1   QBV$VERIFY: ENTRY;
      171    19085
      172    19086        /*D*  NAME:         QBV$VERIFY
      173    19087              CALL:         CALL QBV$VERIFY;
      174    19088              PARAMETERS:   None.
      175    19089              INTERFACE:    Called by QAT$TALLYHO when a VERIFY op-code is
      176    19090                            encountered.
      177    19091                            Calls QCG$GET_FIELD if a LEGAL clause is not satisfied.
      178    19092              ENVIRONMENT:  Nothing unusual.
      179    19093              INPUT:        QI_1,
PL6.E3A0      #001=QBV$VERI_PRES File=QBV$VERI_PRES.:E05TSI                      WED 07/30/97 04:57 Page:8    
      180    19094                            FPL rununit static data following the op-code.
      181    19095              OUTPUT:       Moves data from the FIELD DAD(s) to the COBOL DAD(s).
      182    19096                            Also sets QI$GZ_FIELD.INDICATORS$ ->
      183    19097                                      QI$INDICATORS.STATE_FLAGS.AVAILABLE.
      184    19098              SCRATCH:      Various otter data.
      185    19099              DESCRIPTION:  Move data from FIELD (FRAME SECTION) DADs to COBOL
      186    19100                            (FILE or WORKING-STORAGE SECTION) DADs.  That means setting
      187    19101                            up not only the COBOL DAD addresses but also QI$GZ_FIELD
      188    19102                            stuff.  About the only real oddity is that the compiler
      189    19103                            gens only one argument@P for each COBOL DAD, so if the
      190    19104                            DAD is an array (has an OCCURS clause associated with it),
      191    19105                            the location of each array element has to be devined
      192    19106                            by this routine.  Ish.
      193    19107                            For VERIFYs, the data move is made only if the FIELD DAD
      194    19108                            is AVAILABLE.  Also, if the COBOL DAD contains a LEGAL
      195    19109                            clause (of one sort or another), then the FIELD DAD's
      196    19110                            data is verified against the clause.  If an error is
      197    19111                            detected, QCG$GET_FIELD is called to re-prompt for the
      198    19112                            FIELD DAD.
      199    19113        */
      200    19114
      201    19115    1       THIS_IS_A_VERIFY = %YES;
      202    19116
      203    19117    1   SO_DO_THE_VERI_PRES_ALREADY:
      204    19118
      205    19119    1       INSTR$ = QI_1.INSTR$;
      206    19120    1       VERI_PRES_COUNT = INSTR$ -> QI$OP_VERI_PRES.DAD_PAIR_COUNT;
      207    19121    1       QI_1.INSTR$ = PINCRW ( QI_1.INSTR$, ( VERI_PRES_COUNT * 2 ) + 2 );
      208    19122
      209    19123    2       DO VERI_PRES_INDEX = 0 TO VERI_PRES_COUNT - 1;
      210    19124
      211    19125    2         QI_1.SOURCE$ = ADDR ( INSTR$ -> QI$OP_VERI_PRES.DAD_PAIR_A.SOURCE@P (
             19125                  VERI_PRES_INDEX ) );
      212    19126
      213    19127    2         CALL QCS$SETUP_SOURCE;
      214    19128    2         CALL QCS$SETUP_TARGET;
      215    19129
PL6.E3A0      #001=QBV$VERI_PRES File=QBV$VERI_PRES.:E05TSI                      WED 07/30/97 04:57 Page:9    
      216    19130    2         IF THIS_IS_A_VERIFY
      217    19131    2         THEN
      218    19132    3           DO;
      219    19133    3             COBOL_DAD$ = QI_1.DAD.T$;
      220    19134    3             FIELD_DAD$ = QI_1.DAD.S$;
      221    19135    3           END;
      222    19136    2         ELSE
      223    19137    3           DO;
      224    19138    3             COBOL_DAD$ = QI_1.DAD.S$;
      225    19139    3             FIELD_DAD$ = QI_1.DAD.T$;
      226    19140    3           END;
      227    19141
      228    19142              %IF QI#DEBUG;
      229    19143
      230    19144    2         IF COBOL_DAD$ -> QI$COBOL_DAD.WORD0_FLAGS.IS_FORM_VARIABLE
      231    19145    2         THEN
      232    19146    2           CALL QCE$ERROR_INTERNAL ( %E$INTERNAL_COMPILER_ERROR );
      233    19147
      234    19148    2         IF NOT FIELD_DAD$ -> QI$FIELD_DAD.WORD0_FLAGS.IS_FORM_VARIABLE
      235    19149    2         THEN
      236    19150    2           CALL QCE$ERROR_INTERNAL ( %E$INTERNAL_COMPILER_ERROR );
      237    19151
      238    19152              %ENDIF;
      239    19153
      240    19154    2         QI_1.CURRENT.NODE$ = FIELD_DAD$;
      241    19155
      242    19156    2   RETRY_THE_CURRENT_PAIR:
      243    19157    2         CALL QCF$FIND_CURRENTS;
      244    19158
      245    19159    2         IF NOT QI_1.CURRENT.FRAME_STATUS$ -> QI$FRAME_STATUS.INDICATORS.IS_ACTIVE
      246    19160    2         THEN
      247    19161    3           DO;
      248    19162    3             QI_1.ERROR_STATUS = %E$FRAME_NOT_ACTIVE;
      249    19163    3             GOTO CHECK_USE_PROCEDURE_OR_INVALID;
      250    19164    3           END;
      251    19165
      252    19166    2         IF NOT QI_1.CURRENT.FORM_STATUS$ -> QI$FORM_STATUS.INDICATORS.OPEN_FLAGS.
PL6.E3A0      #001=QBV$VERI_PRES File=QBV$VERI_PRES.:E05TSI                      WED 07/30/97 04:57 Page:10   
             19166                  IS_OUTPUT
      253    19167    2           AND NOT QI_1.CURRENT.FORM_STATUS$ -> QI$FORM_STATUS.INDICATORS.OPEN_FLAGS.
             19167                    IS_INPUT
      254    19168    2         THEN
      255    19169    3           DO;
      256    19170    3             QI_1.ERROR_STATUS = %E$FILE_NOT_OPEN;
      257    19171    3             GOTO CHECK_USE_PROCEDURE_OR_INVALID;
      258    19172    3           END;
      259    19173
      260    19174        %EJECT;
PL6.E3A0      #001=QBV$VERI_PRES File=QBV$VERI_PRES.:E05TSI                      WED 07/30/97 04:57 Page:11   
      261    19175    2         DIMENSION_COUNT = FIELD_DAD$ -> QI$FIELD_DAD.WORD0_FLAGS.DIMENSION_COUNT;
      262    19176
      263    19177    2   ESTABLISH_DIMENSION_BOUNDS:
      264    19178    3         DO CASE ( DIMENSION_COUNT );
      265    19179
      266    19180    3           CASE ( %QI#ELEMENTARY_ITEM );
      267    19181    3             SUBSCRIPT_BOUND_A (1) = 0;
      268    19182    3             SUBSCRIPT_BOUND_A (0) = 0;
      269    19183
      270    19184    3           CASE ( %QI#ONE_DIMENSIONAL_ITEM );
      271    19185    3             SUBSCRIPT_BOUND_A (1) = 0;
      272    19186    3             SUBSCRIPT_INDEX = 0;
      273    19187
      274    19188    3             GOTO CONTINUE_ESTABLISHMENT;
      275    19189
      276    19190    3           CASE ( %QI#TWO_DIMENSIONAL_ITEM );
      277    19191    3             SUBSCRIPT_INDEX = 1;
      278    19192
      279    19193    3   CONTINUE_ESTABLISHMENT:
      280    19194    3             IF THIS_IS_A_VERIFY
      281    19195    3             THEN
      282    19196    4               DO;
      283    19197    4                 INITIAL_COBOL_OFFSET = QI_1.OP.CIP.T.CD.OFFSET;
      284    19198    4                 INITIAL_COBOL_DATA$ = QI_1.OP.DATA.T$;
      285    19199    4               END;
      286    19200    3             ELSE
      287    19201    4               DO;
      288    19202    4                 INITIAL_COBOL_OFFSET = QI_1.OP.CIP.S.CD.OFFSET;
      289    19203    4                 INITIAL_COBOL_DATA$ = QI_1.OP.DATA.S$;
      290    19204    4               END;
      291    19205
      292    19206                  /*  Increment past any level_multipliers and get to the
      293    19207                      P.whatever flags.  */
      294    19208    3             FIELD_DAD_P$ = ADDR ( FIELD_DAD$ -> QI$FIELD_DAD.VARIABLE_LENGTH_ITEMS.
             19208                      LEVEL_MULTIPLIER_A ( DIMENSION_COUNT ) );
      295    19209    3             IF FIELD_DAD_P$ -> QI$FIELD_DAD_P.P.REPEATS
      296    19210    3             THEN
PL6.E3A0      #001=QBV$VERI_PRES File=QBV$VERI_PRES.:E05TSI                      WED 07/30/97 04:57 Page:12   
      297    19211    4               DO;
      298    19212                      %PRESENCE_DISPLACEMENT (
      299    19213                      PRESENCE_THING$ = FIELD_DAD_P$,
      300    19214                      INDEX = P_ITEM_INDEX,
      301    19215                      START = %QI#P_FIELD_FIRST_INDEX,
      302    19216                      STOP = %QI#P_REPEATS_INDEX,
      303    19217                      RESULT = REPEATS_INDEX
      304    19218                      );
