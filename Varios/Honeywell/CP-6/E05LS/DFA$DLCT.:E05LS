PL6.E3A0      #003=DFA$MLCT_ACTIVE File=DFA$DLCT.:E05TSI                         TUE 07/29/97 14:29 Page:88   
      370        1        /*T***********************************************************/
      371        2        /*T*                                                         */
      372        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      373        4        /*T*                                                         */
      374        5        /*T***********************************************************/
      375        6        /*P* NAME: DFA$MLCT_ACTIVE
      376        7             PURPOSE:  To provide the MODIFY ACTIVE LCTs command
      377        8        */
      378        9        /*D* NAME: DFA$MLCT
      379       10             DESCRIPTION: This routine is called by DDA$DO to perform the MLCT
      380       11                  (active ) directive.
      381       12        */
      382       13        DFA$MLCT_ACTIVE: PROC ALTRET;
      383       14
      384       15        /* Local auto */
      385       16    1   DCL TEMP$ PTR;
      386       17    1   DCL TEMP1 REDEF TEMP$ UBIN(36) ALIGNED;
      387       18    1   DCL 1 TEMP1B REDEF TEMP$ ALIGNED,
      388       19    1         2 * UBIN(4) UNAL,
      389       20    1         2 B1 UBIN(8) UNAL,
