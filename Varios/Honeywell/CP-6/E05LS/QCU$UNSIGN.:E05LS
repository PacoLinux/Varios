        1        1        /*M* QCU$UNSIGN:  Make an ALPHANUMERIC-looking item out of something.  */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QCU$UNSIGN
       10       10              PURPOSE:      Make an ALPHANUMERIC-looking item out of something
       11       11                            which may be DISPLAY or PACKED, signed, and
       12       12                            possibly P-scaled.
       13       13              DESCRIPTION:  At various times, the compiler gens op-codes which
       14       14                            "mix" numeric and alphanumeric items, knowing full
       15       15                            well that the interpreter's going to have to turn
       16       16                            cartwheels to make everything work.  This thing is
       17       17                            called to alphanumericize numeric items so such
       18       18                            operations will work.
       19       19        */
       20       20
       21       21        QCU$UNSIGN: PROC;
       22       22
       23       23          %INCLUDE LCP_6;
       24      214
       25      215          %INCLUDE QG_MACRO_M;
       26      352          %INCLUDE QI_EQUATES_E;
       27      711          %INCLUDE QI_MACROS_M;
       28     5769          %QI$ALL;
       29    16882
       30    16883    1     DCL CIP$ PTR;
       31    16884    1     DCL FUNNY_ADDRESS_SYLLABLE_OR_BITS BIT(16);
       32    16885    1     DCL INX UBIN;
       33    16886    1     DCL LENGTH_DECREMENT_FOR_SIGN UBIN;
       34    16887          %QI@TYPE_DISPLAY_TARGET ( STCLASS = "AUTO" );
       35    17552
       36    17553    1     DCL QCC$DMC ENTRY(1) ALTRET;
       37    17554        %EJECT;
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:2    
       38    17555    1   QCU$UNSIGN_SOURCE: ENTRY;
       39    17556
       40    17557        /*D*  NAME:         QCU$UNSIGN_SOURCE
       41    17558              CALL:         CALL QCU$UNSIGN_SOURCE;
       42    17559              PARAMETERS:   None.
       43    17560              INTERFACE:    Called by QBM$MOVxx, QCI$IFxx;
       44    17561                            may call QCC$DMC for some numeric items.
       45    17562              ENVIRONMENT:  Nothing unusual.
       46    17563              INPUT:        QI_1.OP.xx.S., all flags of which have been
       47    17564                            QCS$SETUP_SOURCE'd.
       48    17565              OUTPUT:       Changes some things in QI_1.OP.xx.S., notably
       49    17566                            QI_1.OP.DATA.S$ -> the_data.
       50    17567              SCRATCH:      Various otter data.
       51    17568              DESCRIPTION:  Just make QI_1.OP.xx.S. look as alphanumeric as
       52    17569                            possible:  get rid of packing, signs and P-scaling.
       53    17570        */
       54    17571
       55    17572    1       INX = %QI#SOURCE;
       56    17573    1       FUNNY_ADDRESS_SYLLABLE_OR_BITS = '0010'X;
       57    17574    1       CIP$ = ADDR ( QI_1.OP.CIP_A (%QI#SOURCE) );
       58    17575    1       GOTO SO_UNSIGN_IT_ALREADY;
       59    17576
       60    17577    1   QCU$UNSIGN_TARGET: ENTRY;
       61    17578
       62    17579        /*D*  NAME:         QCU$UNSIGN_TARGET
       63    17580              CALL:         CALL QCU$UNSIGN_TARGET;
       64    17581              PARAMETERS:   None.
       65    17582              INTERFACE:    Called by QBM$MOVxx, QCI$IFxx;
       66    17583                            may call QCC$DMC for some numeric items.
       67    17584              ENVIRONMENT:  Nothing unusual.
       68    17585              INPUT:        QI_1.OP.xx.T., all flags of which have been
       69    17586                            QCS$SETUP_TARGET'd.
       70    17587              OUTPUT:       Changes some things in QI_1.OP.xx.T., notably
       71    17588                            QI_1.OP.DATA.T$ -> the_data.
       72    17589              SCRATCH:      Various otter data.
       73    17590              DESCRIPTION:  Just make QI_1.OP.xx.T. look as alphanumeric as
       74    17591                            possible:  get rid of packing, signs and P-scaling.
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:3    
       75    17592        */
       76    17593
       77    17594    1       INX = %QI#TARGET;
       78    17595    1       FUNNY_ADDRESS_SYLLABLE_OR_BITS = '0020'X;
       79    17596    1       CIP$ = ADDR ( QI_1.OP.CIP_A (%QI#TARGET) );
       80    17597    1       GOTO SO_UNSIGN_IT_ALREADY;
       81    17598
       82    17599    1   QCU$UNSIGN_AUX: ENTRY;
       83    17600
       84    17601        /*D*  NAME:         QCU$UNSIGN_AUX
       85    17602              CALL:         CALL QCU$UNSIGN_AUX;
       86    17603              PARAMETERS:   None.
       87    17604              INTERFACE:    Called by QBM$MOVxx, QCI$IFxx;
       88    17605                            may call QCC$DMC for some numeric items.
       89    17606              ENVIRONMENT:  Nothing unusual.
       90    17607              INPUT:        QI_1.OP.xx.AUX., all flags of which have been
       91    17608                            QCS$SETUP_AUX'd.
       92    17609              OUTPUT:       Changes some things in QI_1.OP.xx.AUX., notably
       93    17610                            QI_1.OP.DATA.AUX$ -> the_data.
       94    17611              SCRATCH:      Various otter data.
       95    17612              DESCRIPTION:  Just make QI_1.OP.xx.AUX. look as alphanumeric as
       96    17613                            possible:  get rid of packing, signs and P-scaling.
       97    17614        */
       98    17615
       99    17616    1       INX = %QI#AUX;
      100    17617    1       FUNNY_ADDRESS_SYLLABLE_OR_BITS = '0030'X;
      101    17618    1       CIP$ = ADDR ( QI_1.OP.CIP_A (%QI#AUX) );
      102    17619
      103    17620    1   SO_UNSIGN_IT_ALREADY:
      104    17621
      105    17622            /*  These next few cases contain lots of code that could be
      106    17623                "compressed" into fewer statements.  However, after about
      107    17624                the 17th rewrite of this module, I have come to appreciate
      108    17625                the subtlety of some of these conversions, and have decided
      109    17626                to re-code it in the most obvious fashion.  JJL, C01.  */
      110    17627    2       DO CASE ( QI_1.TYPE_A (INX) );
      111    17628
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:4    
      112    17629    2         CASE ( %QI#TYPE_ALPHANUMERIC );
      113    17630    2           RETURN;
      114    17631
      115    17632    2         CASE ( %QI#TYPE_DISPLAY );
      116    17633    3           DO CASE ( CIP$ -> QI$CIP.CD.SIGN );
      117    17634
      118    17635    3             CASE ( %QI#CD_UNSIGNED );
      119    17636    3               IF QI_1.OP.DEC_A (INX) < 0
      120    17637    3               THEN
      121    17638    4                 DO;
      122    17639    4                   LENGTH_DECREMENT_FOR_SIGN = 0;
      123    17640    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      124    17641    4                 END;
      125    17642    3               ELSE
      126    17643    4                 DO;
      127    17644    4                   QI_1.OP.INX_A (INX) = 0;
      128    17645    4                 END;
      129    17646
      130    17647    3             CASE ( %QI#CD_SIGN_LEADING_SEPARATE );
      131    17648    3               IF QI_1.OP.DEC_A (INX) < 0
      132    17649    3               THEN
      133    17650    4                 DO;
      134    17651    4                   LENGTH_DECREMENT_FOR_SIGN = 1;
      135    17652    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      136    17653    4                 END;
      137    17654    3               ELSE
      138    17655    4                 DO;
      139    17656    4                   QI_1.OP.INX_A (INX) = 1;
      140    17657    4                   QI_1.OP.LEN_A (INX) = QI_1.OP.LEN_A (INX) - 1;
      141    17658    4                 END;
      142    17659
      143    17660    3             CASE ( %QI#CD_SIGN_TRAILING_SEPARATE );
      144    17661    3               IF QI_1.OP.DEC_A (INX) < 0
      145    17662    3               THEN
      146    17663    4                 DO;
      147    17664    4                   LENGTH_DECREMENT_FOR_SIGN = 1;
      148    17665    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:5    
      149    17666    4                 END;
      150    17667    3               ELSE
      151    17668    4                 DO;
      152    17669    4                   QI_1.OP.INX_A (INX) = 0;
      153    17670    4                   QI_1.OP.LEN_A (INX) = QI_1.OP.LEN_A (INX) - 1;
      154    17671    4                 END;
      155    17672
      156    17673    3             CASE ( %QI#CD_SIGN_TRAILING_OVER );
      157    17674    3               IF QI_1.OP.DEC_A (INX) < 0
      158    17675    3               THEN
      159    17676    4                 DO;
      160    17677    4                   LENGTH_DECREMENT_FOR_SIGN = 0;
      161    17678    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      162    17679    4                 END;
      163    17680    3               ELSE
      164    17681    4                 DO;
      165    17682    4                   QI_1.OP.DEC_A (INX) = 0;
      166    17683    4                   LENGTH_DECREMENT_FOR_SIGN = 0;
      167    17684    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      168    17685    4                 END;
      169    17686
      170    17687    3           END;
      171    17688
      172    17689    2         CASE ( %QI#TYPE_PACKED );
      173    17690    3           DO CASE ( QI_1.OP.CIP.S.CP.SIGN );
      174    17691
      175    17692    3             CASE ( %QI#CP_UNSIGNED );
      176    17693    3               IF QI_1.OP.DEC_A (INX) < 0
      177    17694    3               THEN
      178    17695    4                 DO;
      179    17696    4                   LENGTH_DECREMENT_FOR_SIGN = 0;
      180    17697    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      181    17698    4                 END;
      182    17699    3               ELSE
      183    17700    4                 DO;
      184    17701    4                   QI_1.OP.DEC_A (INX) = 0;
      185    17702    4                   LENGTH_DECREMENT_FOR_SIGN = 0;
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:6    
      186    17703    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      187    17704    4                 END;
      188    17705
      189    17706    3             CASE ( %QI#CP_SIGN_TRAILING );
      190    17707    3               IF QI_1.OP.DEC_A (INX) < 0
      191    17708    3               THEN
      192    17709    4                 DO;
      193    17710    4                   LENGTH_DECREMENT_FOR_SIGN = 1;
      194    17711    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      195    17712    4                 END;
      196    17713    3               ELSE
      197    17714    4                 DO;
      198    17715    4                   QI_1.OP.DEC_A (INX) = 0;
      199    17716    4                   LENGTH_DECREMENT_FOR_SIGN = 1;
      200    17717    4                   CALL MAKE_A_SCALED_DISPLAY_ITEM;
      201    17718    4                 END;
      202    17719
      203    17720    3           END;
      204    17721
      205    17722    2       END;
      206    17723
      207    17724            /* By the time we get here (even if we had such a monster as a
      208    17725               packed field with P-scaling) we now have a ALPANUMERIC-
      209    17726               looking item whose real length can be found in
      210    17727               QI_1.OP.LEN_A (INX).  Sooo, we'll just randomly stomp
      211    17728               on the length in the CIP descriptor itself, stick
      212    17729               in an appropriate address syllable, and remember to FILL.  */
      213    17730    1       CIP$ -> QI$CIP.LEN = 0;
      214    17731    1       CIP$ -> QI$CIP.AS = BITBIN ( %BINBIT ( %QI#CIP_DEFAULT_AS, 16 ) |
             17731                FUNNY_ADDRESS_SYLLABLE_OR_BITS );
      215    17732    1       CIP$ -> QI$CIP.CA.FILL = 1;
      216    17733
      217    17734    1       QI_1.TYPE_A (INX) = %QI#TYPE_ALPHANUMERIC;
      218    17735
      219    17736    1       RETURN;
      220    17737
      221    17738        %EJECT;
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:7    
      222    17739    1   MAKE_A_SCALED_DISPLAY_ITEM: PROC;
      223    17740
      224    17741        /*I*  NAME:         MAKE_A_SCALED_DISPLAY_ITEM
      225    17742              PURPOSE:      Make an alphanumeric-looking item out of a
      226    17743                            DISPLAY or PACKED item.
      227    17744              CALL:         CALL MAKE_A_SCALED_DISPLAY_ITEM;
      228    17745              PARAMETERS:   None.
      229    17746              DATA:         QI_1.stuff,
      230    17747                            CIP$ from above.
      231    17748              DESCRIPTION:  Basically just common code for DISPLAY or PACKED items
      232    17749                            to get them unpacked, unsigned and/or unP-scaled.
      233    17750        */
      234    17751
      235    17752    2       QI@TYPE_DISPLAY_TARGET = QI_TYPE_DISPLAY_TARGET;
      236    17753    2       QI@TYPE_DISPLAY_TARGET.CIP.S_A (0) = CIP$ -> QI$CIP_A (0);
      237    17754    2       QI@TYPE_DISPLAY_TARGET.DATA.S$ = QI_1.OP.DATA_A$ (INX);
      238    17755    2       QI@TYPE_DISPLAY_TARGET.DEC.S = QI_1.OP.DEC_A (INX);
      239    17756
      240    17757            /* Get the address of where to stuff the soon-to-be unsigned
      241    17758               item.  Normally, OTTER_SPACE$ will point to an
      242    17759               OTTER_SPACE item a couple of frames back.  */
      243    17760    2       QI_1.OP.DATA_A$ (INX) = QI_1.OTTER_SPACE_A$ (INX);
      244    17761    2       QI@TYPE_DISPLAY_TARGET.DATA.T$ = QI_1.OTTER_SPACE_A$ (INX);
      245    17762
      246    17763            /* Since the target's DEC is 0, P-scaled items will
      247    17764               automatically be re-aligned here. */
      248    17765
      249    17766    2       QI@TYPE_DISPLAY_TARGET.CIP.T.CD.SIGN = %QI#CD_UNSIGNED;
      250    17767
      251    17768    2       QI@TYPE_DISPLAY_TARGET.SIZE_ERROR = %QI#DONT_CHECK_FOR_SIZE_ERROR;
      252    17769
      253    17770    2       CALL QCC$DMC ( QI@TYPE_DISPLAY_TARGET );
      254    17771
      255    17772    2       CIP$ -> QI$CIP_A (0) = 0;
      256    17773    2       QI_1.OP.LEN_A (INX) = QI_1.OP.LEN_A (INX)
      257    17774    2         - QI_1.OP.DEC_A (INX)
      258    17775    2         - LENGTH_DECREMENT_FOR_SIGN;
PL6.E3A0      #001=QCU$UNSIGN File=QCU$UNSIGN.:E05TSI                            WED 07/30/97 05:15 Page:8    
      259    17776    2       QI_1.OP.INX_A (INX) = %QI#MAX_DISPLAY_LENGTH
      260    17777    2         - QI_1.OP.LEN_A (INX);
      261    17778    2       QI_1.OFFSET_A (INX) = 0;
      262    17779
      263    17780    2       RETURN;
      264    17781
      265    17782    2   END MAKE_A_SCALED_DISPLAY_ITEM;
      266    17783
      267    17784    1   END QCU$UNSIGN;

