        7        7
        8        8        /*P* NAME: B_ALTTBLS_D
        9        9        PURPOSE: Common SEARCH and XLATE tables.
       10       10        DESCRPTION:
       11       11                This module contains the alternate set of common SEARCH and
       12       12                XLATE tables.  This set of tables is intended for a non-English
       13       13                language.  B_TABLES_D contains the standard set of tables
       14       14                intended for use with English.
       15       15
       16       16                The alternate tables are initially identical to the standard
       17       17                set.  Language-specific changes must be applied with patches
       18       18                at boot time.
       19       19        */
       20       20
       21       21        B_ALTTBLS_D: PROC;
       22       22
       23       23        %INCLUDE B_TABLES_C;
       24      714        %INCLUDE B$TABLES_C;
       25      822
       26      823        %LIST;
       27      824        %B_TABLES( NAME=B_ALTTBLS );
               825    1   DCL 1 NAME CONSTANT SYMDEF DALIGNED,
               826            %B_SR_NON_BLANK;    /*   0  Search for non-blank                */
               827    1     2 B_SR_NON_BLANK( 0:511 ) BIT(9) CALIGNED INIT(
               828    1       %B$SR#CTL * 32,                 /*  NULL thru US                */
               829    1       '0'B,                           /*  SP                          */
               830    1       %B$SR#PUNC * 2,                 /*  ! "                         */
               831    1       %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
               832    1       %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
               833    1       %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
               834    1       %B$SR#PUNC,                     /*  ,                           */
PL6.E3A0      #001=B_ALTTBLS_D File=B_ALTTBLS_D.:E05TSI                          TUE 07/29/97 11:06 Page:2    
               835    1       %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
               836    1       %B$SR#PUNC,                     /*  /                           */
               837    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 8,
               838                                            /*  0 thru 7                    */
               839    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
               840    1       %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
               841    1       %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
               842    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
               843    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      G thru Z                */
               844    1       %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
               845    1       %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
               846    1       %B$SR#PUNC,                     /*  `                           */
               847    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
               848    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      g thru z                */
               849    1       %B$SR#PUNC * 4,                 /*  { | } ~                     */
               850    1       %B$SR#CTL,                      /*  DEL                         */
               851    1       %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
               853            %B_SR_NON_ZERO;     /*   1  Search for non-zero                 */
               854    1     2 B_SR_NON_ZERO( 0:511 ) BIT(9) CALIGNED INIT(
               855    1       %B$SR#CTL * 32,                 /*  NULL thru US                */
               856    1       %B$SR#PUNC * 3,                 /*  SP ! "                      */
               857    1       %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
               858    1       %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
               859    1       %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
               860    1       %B$SR#PUNC,                     /*  ,                           */
               861    1       %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
               862    1       %B$SR#PUNC,                     /*  /                           */
               863    1       '0'B,                           /*  0                           */
               864    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 7,
               865                                            /*  1 thru 7                    */
               866    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
               867    1       %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
               868    1       %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
               869    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
               870    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      G thru Z                */
               871    1       %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
               872    1       %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
PL6.E3A0      #001=B_ALTTBLS_D File=B_ALTTBLS_D.:E05TSI                          TUE 07/29/97 11:06 Page:3    
               873    1       %B$SR#PUNC,                     /*  `                           */
               874    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
               875    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      g thru z                */
               876    1       %B$SR#PUNC * 4,                 /*  { | { ~                     */
               877    1       %B$SR#CTL,                      /*  DEL                         */
               878    1       %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
