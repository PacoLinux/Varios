PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:4    
       68     6356        /* Variables */
       69     6357
       70     6358    1      DCL CHAR_COUNT SBIN STATIC ALIGNED;
       71     6359    1      DCL DATE_BUF CHAR(8) STATIC;
       72     6360    1      DCL WRITE_BUF_BCD(0:10)  UBIN(6) STATIC UNAL;
       73     6361    1      DCL WRITE_BUF_ASCII(0:10)  CHAR(1) STATIC UNAL;
       74     6362    1      DCL READ_BUF_ASCII(0:10) CHAR(1) STATIC UNAL;
       75     6363    1      DCL READ_BUF_BCD(0:10)  UBIN(6) STATIC UNAL;
       76     6364    1      DCL TIME_BUF CHAR(11) STATIC;
       77     6365    1      DCL 1 B_VERSION SYMREF CALIGNED,
       78     6366    1            2 LGTH    UBIN BYTE UNAL,
       79     6367    1            2 TEXT    CHAR (11) UNAL ;
       80     6368
       81     6369        /* External Entry Points */
