       37     6134                     PROGRAMMED=TOR$FAULTS,
       38     6135                     MME=TRAP,
       39     6136                     DERAIL= TRAP,
       40     6137                     FAULT_TAG=TRAP,
       41     6138                     ERRORS= TOR$FAULTS,
       42     6139                     MEMORY=TRAP,
       43     6140                     COMMAND=TRAP,
       44     6141                     LOCKUP=TRAP,
       45     6142                     IPR=TRAP,
