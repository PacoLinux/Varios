PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:5    
       97     6385        /*D* NAME:         TOR$CONSOLE_OUT_PTR
       98     6386                           TOR$WRAPUP_PTR
       99     6387                           TOR$EXIT_PTR
      100     6388         **********************************************************
      101     6389         **     SATISFYING ENTRY POINTS OF INTERNAL              **
      102     6390         **     PROCEDURES CALLED FROM EXTERNALLY DECLARED       **
      103     6391         **     PROCEDURES                                       **
      104     6392         **********************************************************
      105     6393        */
      106     6394
      107     6395    1   TOR$CONSOLE_OUT_PTR: ENTRY;
      108     6396    1      CALL TOR$CONSOLE_OUT;
      109     6397    1      RETURN;
      110     6398
      111     6399    1   TOR$WRAPUP_PTR: ENTRY;
      112     6400    1      CALL TOR$WRAPUP;
      113     6401
      114     6402    1   TOR$EXIT_PTR: ENTRY;
      115     6403    1      CALL TOR$EXIT;
      116     6404        %EJECT;
PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:6    
      117     6405        /*F* NAME:         TOR$INITIALIZE
      118     6406             PURPOSE:      Does all the required system initialization
      119     6407                           required for the TURTLE program.
      120     6408                           the main sequence of events in this routine is the
      121     6409                           establishing of the Exceptional condition handlers
      122     6410                           with the aid of CP6 Monitor services routines.
      123     6411                             Conditions handled:
      124     6412                              1.) Hardware, and programed faults.
      125     6413                              2.) BREAK Key depression.
      126     6414                              3.) Line disconnect and exit cleanup.
      127     6415        */
      128     6416
      129     6417    1   TOR$INITIALIZE: PROC;
      130     6418
      131     6419    2      CALL M$OPEN(OPEN_FPT) ;
      132     6420    2      CALL M$INT(INT_FPT) ;
      133     6421    2      CALL M$TRAP(TRAP_FPT) ;
      134     6422    2      RETURN;
      135     6423
      136     6424    2   END TOR$INITIALIZE;
      137     6425        %EJECT;
PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:7    
      138     6426        /*F* NAME:         TOR$START_UP
      139     6427             PURPOSE:      Issues the message telling the user that the TURTLE
      140     6428                           program has been activated.  The message will be edited to
      141     6429                           contain the current time, date and version date of TURTLE.
      142     6430        */
      143     6431
      144     6432    1   TOR$START_UP: PROC;
      145     6433
      146     6434    2      DCL BUFFER  CHAR (80) STATIC CALIGNED ;
      147     6435
      148     6436        /* Code */
      149     6437
      150     6438    2      CALL M$TIME(START_FPT) ;
      151     6439    2      CALL CONCAT (BUFFER,
      152     6440    2              'TURTLE Version ',
      153     6441    2              SUBSTR (B_VERSION.TEXT, 0, B_VERSION.LGTH),
      154     6442    2              ' on at ',
      155     6443    2              SUBSTR (TIME_BUF, 0, 5),
      156     6444    2              ' ',
      157     6445    2              DATE_BUF) ;
      158     6446    2      CONSOLE_FPT.BUF_ = VECTOR (BUFFER) ;
      159     6447    2      CALL TOR$CONSOLE_OUT;
      160     6448    2      RETURN;
      161     6449
      162     6450    2   END TOR$START_UP;
      163     6451        %EJECT;
PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:8    
      164     6452        /*F* NAME:        TOR$EXIT
      165     6453             PURPOSE:     Performs the functions of exiting from the TURTLE program
      166     6454                          upon normal termination.
      167     6455                          Outputs a TURTLE termination message, telling the user that
      168     6456                          TURTLE has terminated, which includes the current date and
      169     6457                          time of day. TURTLE terminates via M$EXIT.
      170     6458        */
      171     6459
      172     6460    1   TOR$EXIT: PROC;
      173     6461
      174     6462    2      DCL BUFFER  CHAR (80) STATIC CALIGNED ;
      175     6463
      176     6464        /* Code */
      177     6465
      178     6466    2      BUFFER = ' ' ;
      179     6467    2      CONSOLE_FPT.BUF_ = VECTOR (BUFFER) ;
      180     6468    2      CALL TOR$CONSOLE_OUT;
      181     6469    2      CALL M$TIME(START_FPT) ;
      182     6470    2      CALL CONCAT (BUFFER,
      183     6471    2              'TURTLE off at ',
      184     6472    2              SUBSTR (TIME_BUF, 0, 5),
      185     6473    2              ' ',
      186     6474    2              DATE_BUF) ;
      187     6475    2      CALL TOR$CONSOLE_OUT;
      188     6476    2      CALL M$EXIT ;
      189     6477
      190     6478    2   END TOR$EXIT;
      191     6479        %EJECT;
PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:9    
      192     6480        /*F* NAME:        TOR$CONSOLE_OUT
      193     6481             PURPOSE:     Is called to do just console output.
      194     6482                          Before calling this routine,
      195     6483                          the parameters for the console output FPT, CONSOLE_FPT, must
      196     6484                          be established, or reset.
      197     6485                          The parameters which must be established or reset are:
      198     6486                            1.)  Output buffer address
      199     6487                            2.)  Wait  option
      200     6488        */
      201     6489
      202     6490    1   TOR$CONSOLE_OUT: PROC;
      203     6491
      204     6492    2      CALL M$WRITE(CONSOLE_FPT) ;
      205     6493    2      RETURN;
      206     6494
      207     6495    2   END TOR$CONSOLE_OUT;
      208     6496        %EJECT;
PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:10   
      209     6497        /*F* NAME:       TOR$WRAPUP
      210     6498             PURPOSE:    Called by the Asynchronous Fault Handlerwhen ever the
      211     6499                         fault is to be processed.
      212     6500                         The processing that is performed in this routine is the
      213     6501                         the outputting of a TURTLE wrapup message and abnormal
      214     6502                         termination of the TURTLE program.
      215     6503                         The abnormal termination will be done with aid of a
      216     6504                         Monitor Services Routine M$MERCS, which turns control
      217     6505                         over to the monitor who intern continues to process the
      218     6506                         the fault, and abnormally abort the program.
      219     6507        */
      220     6508
      221     6509    1   TOR$WRAPUP: PROC;
      222     6510
      223     6511    2      DCL BUFFER  CHAR (80) STATIC CALIGNED ;
      224     6512
      225     6513        /* Code */
      226     6514
      227     6515    2      CALL M$TIME(START_FPT) ;
      228     6516    2      CALL CONCAT (BUFFER,
      229     6517    2              'TURTLE Version ',
      230     6518    2              SUBSTR (B_VERSION.TEXT, 0, B_VERSION.LGTH),
      231     6519    2              ' wrapup at ',
      232     6520    2              SUBSTR (TIME_BUF, 0, 5),
      233     6521    2              ' ',
      234     6522    2              DATE_BUF) ;
      235     6523    2      CONSOLE_FPT.BUF_ = VECTOR (BUFFER) ;
      236     6524    2      CALL TOR$CONSOLE_OUT;
      237     6525    2      RETURN;
      238     6526
      239     6527    2   END TOR$WRAPUP;
      240     6528    1   END TOR$TURTLE;

