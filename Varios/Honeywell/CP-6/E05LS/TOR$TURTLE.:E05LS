       11       11        /*F* NAME:         TOR$TURTLE
       12       12             PURPOSE:      Calls a system initialization routine that
       13       13                           will initialize all TURTLE variables, tables,
       14       14                           and  flags, plus establish all the exceptional
       15       15                           condition  handler routines.
       16       16             LAST UPDATE:  08/17/82
       17       17        */
       18       18
       19       19        TOR$TURTLE: PROC;
       20       20
       21       21        /* Include Files */
       22       22
       23       23           %INCLUDE CP_6;
       24     5582           %INCLUDE CP_6_SUBS;
       25     6122
       26     6123        /* Data Control Blocks */
       27     6124
       28     6125    1      DCL M$LO DCB;
       29     6126    1      DCL M$SI DCB;
       30     6127           %EJECT;
PL6.E3A0      #001=TOR$TURTLE File=TOR$TURTLE.:E05TSI                            WED 07/30/97 06:34 Page:2    
       31     6128        /* Function Parameter Tables */
