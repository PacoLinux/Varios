      785    11711    4                  CALL DDE$ERROR;
      786    11712    4                  S_D.ERROR = 502;    /* E@DELBUG */
      787    11713    4                  CALL DDE$ERROR;
      788    11714    4                  GOTO DERR;
      789    11715    4                END;
      790    11716    3             SIZE = GD$MSG.CHAR# * 8;
      791    11717    3             IF SIZE/8 > 255
      792    11718    4             THEN DO;
      793    11719    4                  S_D.ERROR = 41;    /* E@NODISP */
