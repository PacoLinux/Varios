      103     9917    1   DCL TOP$ PTR;
      104     9918        %VLP_ERRCODE (STCLASS = "REDEF TOP$");
      105     9963    1   DCL PARSE_CODE REDEF TOP$ BIT(36);
      106     9964    1   DCL M$LO DCB;
      107     9965    1   DCL M$SI DCB;
      108     9966    1   DCL M$DO DCB;
      109     9967    1   DCL M$HLPCG DCB;
      110     9968    1   DCL M$NETCON DCB;
      111     9969    1   DCL M$HLPF DCB;
      112     9970    1   DCL M$PROF DCB;
      113     9971    1        CALL CONCAT(HI_FMT.T_,'NETCON ',ADDR(B_VERSION)->
      114     9972    1             B$VERSIONB.T_,' HERE');
      115     9973    2        CALL M$SPRIV(PPR_NETCON) WHENALTRETURN DO;
      116     9974    2           CALL KSE$ERRMSG(%E$KS_NO_PRIV);
      117     9975                /*E* ERROR: KSN-E$KS_NO_PRIV-0
      118     9976                     MESSAGE: Insufficient privilege to run NETCON.
      119     9977                     MESSAGE1: User must have NETCON processor privilege to use NETCON.
      120     9978                     DESCRIPTION: SEE MESSAGES */
      121     9979    2           RETURN;
      122     9980    2           END;  /* WHENALTRETURN */
      123     9981    1        CALL XUR$INIT(XUR_INIT,P_PCB$);
      124     9982    1        CALL M$SPRIV ( FPT_PRIV );
      125     9983    1        CALL BINCHAR(KS_MYSTATION.SYSID,B$JIT.SYSID);
      126     9984    1        VLP_SETSTA.MYSTATION# = KS_MYSTATION_C;
      127     9985    2        CALL M$OPEN(OPEN_HLPCG) WHENALTRETURN DO;
      128     9986    2           CALL KSE$ERRMSG(0);
      129     9987    2           END;  /* WHENALTRETURN */
      130     9988    2        CALL M$OPEN(OPEN_NETCON) WHENALTRETURN DO;
      131     9989    2           CALL KSE$ERRMSG(0);
      132     9990    2           GOTO BAG_IT;
      133     9991    2           END;  /* WHENALTRETURN */
PL6.E3A0      #001=KSN$NETCON File=KSN$NETCON.:E05TSI                            WED 07/30/97 01:43 Page:7    
      134     9992    1           OPEN_FILE.V.DCB# = DCBNUM(M$PROF);
      135     9993    1           OPEN_FILE.NAME_ = VECTOR(PRONAME);
      136     9994    1           CALL M$OPEN (OPEN_FILE)
      137     9995    2           WHENALTRETURN DO;
      138     9996    2              CALL KSE$ERRMSG(0);
      139     9997    2              END;
      140     9998    1           OPEN_FILE.V.DCB# = DCBNUM(M$HLPF);
      141     9999    1           OPEN_FILE.NAME_ = VECTOR(HLPNAME);
      142    10000    1           CALL M$OPEN (OPEN_FILE)
      143    10001    2           WHENALTRETURN DO;
      144    10002    2              CALL KSE$ERRMSG(0);
      145    10003    2              END;
      146    10004    1        CALL M$OPEN(OPEN_LO);
      147    10005    1        CALL KSW$WRITE(HI_VEC);
      148    10006    1        REMEMBER ERR_RETURN IN KS_ERR_RETURN;
      149    10007    1        CALL M$INT(FPT_INT);
      150    10008    1        KS_END_FLG = '0'B;
      151    10009    2        CALL M$MONINFO(FPT_MONINFO) WHENRETURN DO;
      152    10010    2           PROMPT.NODENAME = VLR_MONINFO.NODE_NAME;
      153    10011    2           END;
      154    10012    2        WHENALTRETURN DO;
      155    10013    2           PROMPT.NODENAME = 'HOST';
      156    10014    2           END;
      157    10015    2        DO WHILE(NOT KS_END_FLG);
      158    10016    3           CALL XUR$GETCMD(KS_CMD,TOP$,VECTOR(PROMPT)) WHENRETURN DO;
      159    10017    3              CALL XUR$ECHOIF;
      160    10018    3              CALL KSC$COMMANDS(TOP$);
      161    10019    3              END;  /* WHENRETURN */
      162    10020    3           WHENALTRETURN DO;
      163    10021    3                  CALL XUR$ERRMSG(TOP$);
      164    10022    3                  IF VLP_ERRCODE.ERR# = %E$EOF
      165    10023    3                    OR VLP_ERRCODE.ERR# = %E$NOFILE THEN
      166    10024    3                     GOTO BAG_IT;
      167    10025    3                  UNWIND TO KS_ERR_RETURN;
      168    10026    3               END;
      169    10027    2   ERR_RETURN:;
      170    10028    2           END;  /* DO WHILE NOT KS_END_FLG */
PL6.E3A0      #001=KSN$NETCON File=KSN$NETCON.:E05TSI                            WED 07/30/97 01:43 Page:8    
      171    10029    1   BAG_IT:;
      172    10030    1        CALL M$CLOSE(CLOSE_NETCON);
      173    10031    1        CALL M$CLOSE(CLOSE_HLPCG);
      174    10032    1        CALL M$CLOSE(CLOSE_LO);
      175    10033    1        RETURN;
      176    10034    1   END KSN$NETCON;
      177    10035        %EOD;

