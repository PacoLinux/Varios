 15:10 07/29/97  FILE=DXC$MISC.:E05TSI           DXC$MISC - DXC$PATCHENT                                 10   BMAP - B00     
                                       124           TTL      DXC$MISC - DXC$PATCHENT
 0 000000                              125            USE       DXC$MISC,1
                                       126  *
          (0)000000000000              127  DXC$MISC  EQU       *              PRIMARY ENTDEF
                                       128  *
                                       129  *D*  NAME:   DXC$PATCHENT
                                       130  *D*  CALL:   AARDVARK TRANSFERS CONTROL TO DXC$PATCHENT BY POINTING THE
                                       131  *D*          SSR TO XDELTA'S SAFE STORE (WITH IC= DXC$PATCHENT) AND
                                       132  *D*          DOING A RET
                                       133  *D*  DESCRIPTION: INTIALIZE XDELTA'S AUTO AND FAULT ENTRY DESCRIPTORS
                                       134  *D*               IN LINKAGE SEGMENT. SAVE FAULT LOCATIONS . SAVE
                                       135  *D*               MONITOR IC. OTHER MISCELLANY TOO
                                       136  *
                                       137  *
          (0)000000000000              138  DXC$PATCHENT  EQU   *
                                       139  *
   000000  X 000000 7400 00            140           STX0     SAVEX0            REMEMBER PATCH-TIME FLAG
   000001  X 000000 4410 00            141           SXL1     D_CPU_TYPE
                                       142  *                                   BIT0 OF X0 SET IF PATCHING
   000002    006001 4724 07            143           LDP2     XDLSSID,DL        XD LS INTO DR2
                                       144  *
                                       145  * INITIALIZE XDELTA'S AUTO DESCRIPTOR IN LINKAGE SEGMENT ($LS3)
                                       146  *
   000003  X 000000 2350 07            147           LDA      AUTOSTART,DL      AUTO OFFSET FROM IS BASE
   000004    000002 7350 00            148           ALS      2                 INTO BYTES
   000005    2 00001 0751 00           149           ADA XL.ISR*2+1,,PR2      ADD IN IS BASE
   000006    2 00007 7551 00           150           STA      XL.AUTO*2+1,,PR2     STORE BASE
   000007  X 000000 7210 00            151           LXL1     AUTOSIZEW
   000010    000001 1610 03            152           SBX1     1,DU              PREVIOUS WORD
   000011    2 00006 7411 00           153           STX1     XL.AUTO*2,,PR2
                                       154  *
                                       155  * INITIALIZE FAULT ENTRY DESCRIPTOR IN LINKAGE SEGMENT ($LS18)
                                       156  *
   000012    2 00045 2351 00           157           LDA      XL.FLT*2+1,,PR2 STORE LS BASE
   000013    777600 3750 03            158           ANA      =O777600,DU       INTO SECOND
   000014    2 00003 2751 00           159           ORA      XL.LSR*2+1,,PR2
   000015    2 00045 7551 00           160           STA      XL.FLT*2+1,,PR2 WORD OF DESCRIPTOR
 15:10 07/29/97  FILE=DXC$MISC.:E05TSI           DXC$MISC - DXC$PATCHENT                                 11   BMAP - B00     
   000016 (0)000074 6210 00            161           EAX1     DXC$FAULTENT
   000017    2 00044 7411 00           162           STX1     XL.FLT*2,,PR2   STORE ENTRY POINT (IC)
   000020  X 000000 4734 00            163           LDP3     MLS$              PTR TO MON LS
   000021    001761 4734 07            164           LDP3     CHNGTYPESID,DL    GET AS DATA
   000022    2 00044 2371 00           165           LDAQ     XL.FLT*2,,PR2
   000023    3 00054 7571 00           166           STAQ     LS.XD*2,,PR3    STUFF XD ENTRY DESC INTO MON LS
   000024  X 000000 7570 00            167           STAQ     XDFAULTDESC       SAVE ACTUAL XD FAULT ENTRY DESC
                                       168  *
                                       169  * CHANGE MON SAFE STORE DESCRIPTOR TO TYPE 0
                                       170  *
   000025    006013 4734 07            171           LDP3     MSSSID,DL         GET MON SS
   000026    001761 4734 07            172           LDP3     CHNGTYPESID,DL
   000027    2 00050 0535 00           173           STD3     XL.MSSR0*2,,PR2
                                       174  *
                                       175  *  CHANGE XDELTA SS DESCRIPTOR TO TYPE 0
                                       176  *
   000030    006002 4734 07            177           LDP3     XDSSSID,DL
   000031    001761 4734 07            178           LDP3     CHNGTYPESID,DL
   000032    2 00046 0535 00           179           STD3     XL.SSR0*2,,PR2
                                       180  *
                                       181  * PICK UP MONITOR IC FROM MON SS AND PUT INTO S_D.SAVEIC
                                       182  *
   000033  X 000000 4774 00            183           LDP7     S_D_GOIC$         PTR TO S_D.GOIC
