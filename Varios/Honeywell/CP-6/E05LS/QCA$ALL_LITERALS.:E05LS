  17202   0 0001D0  EFC7 0010            MOVE_LOTS_TO_X_*STB,B6   SOURCE_INDEX_FROM_LAST_TIME+1,AUTO

      350    17203
      351    17204        /*I*  NAME:         MOVE_LOTS_TO_X_FIELDS
      352    17205              PURPOSE:      To "unfold" an ALL "literal" source within
      353    17206                            an ALPHANUMERIC-type target.
      354    17207              CALL:         CALL MOVE_LOTS_TO_X_FIELDS;
      355    17208              PARAMETERS:   None.
      356    17209              DATA:         Uses QI_1.OP.LEN.S,
      357    17210                                 QI_1.OP.LEN.T,
      358    17211                                 QI_1.OP.INX.S (always zero, turns out),
      359    17212                                 QI_1.OP.INX.T
      360    17213                            to move QI_1.OP.CIP.S
      361    17214                                 to QI_1.OP.CIP.T.
      362    17215              DESCRIPTION:  Well, we got this ALL "literal" source, see, and
      363    17216                            we move it into the target.  If it just fit, or
      364    17217                            if it had to be truncated to fit, we're done.
      365    17218                            Otherwise...
      366    17219                            we start moving the target within the target,
      367    17220                            "unfolding" the ALL "literal".
      368    17221                            For exemplum:
      369    17222                            MOVE "123" TO TARGET.
      370    17223                            Step 1, source to target:
      371    17224                                "123"    "                             "
      372    17225                                 ^        ^
      373    17226                                 S$       T$
      374    17227
      375    17228                            Step 2, target to target:
      376    17229                                "123"    "123                          "
      377    17230                                          ^  ^
      378    17231                                          S$ T$
      379    17232
      380    17233                            Repeat step 2:
      381    17234                                "123"    "123123                       "
      382    17235                                          ^     ^
PL6.E3A0      #001=QCA$ALL_LITERAL_MOVES File=QCA$ALL_LITERALS.:E05TSI           WED 07/30/97 05:01 Page:43   
      383    17236                                          S$    T$
      384    17237
      385    17238                            Repeat step 2:
      386    17239                                "123"    "123123123123                 "
      387    17240                                          ^           ^
      388    17241                                          S$          T$
      389    17242
      390    17243                            Repeat step 2:
      391    17244                                "123"    "123123123123123123123123     "
      392    17245                                          ^                       ^
      393    17246                                          S$                      T$
      394    17247
      395    17248                            All done:
      396    17249                                "123"    "12312312312312312312312312312"
      397    17250        */
      398    17251
      399    17252    2     DCL SIGNED_TARGET_LENGTH SBIN;
      400    17253
      401    17254            /* First we'll move the original ALL string (e.g., ALL "ABC")
      402    17255               to the target... */
      403    17256    2       IF QI_1.OP.LEN.S > QI_1.OP.LEN.T

  17256   0 0001D2  E800 0000 0026  xsym                 LDR,R6   QI_1+38
          0 0001D5  E900 0000 0027  xsym                 CMR,R6   QI_1+39
          0 0001D8  0381 0007                            BLE      s:17260,PREL

      404    17257    2       THEN
      405    17258    2         QI_1.OP.LEN.S = QI_1.OP.LEN.T;

  17258   0 0001DA  E800 0000 0027  xsym                 LDR,R6   QI_1+39
          0 0001DD  EF00 0000 0026  xsym                 STR,R6   QI_1+38

      406    17259
