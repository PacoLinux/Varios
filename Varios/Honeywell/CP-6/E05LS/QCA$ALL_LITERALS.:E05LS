      131    16984    2           QI_1.OP.LEN.S = OLD_SOURCE_LENGTH;
      132    16985    2           QI_1.OP.LEN.T = OLD_TARGET_LENGTH
      133    16986    2             - LEFT_TRUNCATION_LENGTH;
      134    16987
      135    16988                /* If the target length is zero, then the
      136    16989                   bimbo did something like 'MOVE ALL "777" TO PIC9.'
      137    16990                   (where PIC9 has a PICture of PIC 9).  Gratefully,
      138    16991                   we're finished in this case. */
      139    16992    2           IF QI_1.OP.LEN.T = 0
      140    16993    2           THEN
