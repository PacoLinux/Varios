PL6.E3A0      #001=QCA$ALL_LITERAL_MOVES File=QCA$ALL_LITERALS.:E05TSI           WED 07/30/97 05:01 Page:13   
      349    17202    1   MOVE_LOTS_TO_X_FIELDS: PROC;
      350    17203
      351    17204        /*I*  NAME:         MOVE_LOTS_TO_X_FIELDS
      352    17205              PURPOSE:      To "unfold" an ALL "literal" source within
      353    17206                            an ALPHANUMERIC-type target.
      354    17207              CALL:         CALL MOVE_LOTS_TO_X_FIELDS;
      355    17208              PARAMETERS:   None.
      356    17209              DATA:         Uses QI_1.OP.LEN.S,
      357    17210                                 QI_1.OP.LEN.T,
      358    17211                                 QI_1.OP.INX.S (always zero, turns out),
      359    17212                                 QI_1.OP.INX.T
      360    17213                            to move QI_1.OP.CIP.S
      361    17214                                 to QI_1.OP.CIP.T.
      362    17215              DESCRIPTION:  Well, we got this ALL "literal" source, see, and
      363    17216                            we move it into the target.  If it just fit, or
      364    17217                            if it had to be truncated to fit, we're done.
      365    17218                            Otherwise...
      366    17219                            we start moving the target within the target,
      367    17220                            "unfolding" the ALL "literal".
      368    17221                            For exemplum:
      369    17222                            MOVE "123" TO TARGET.
      370    17223                            Step 1, source to target:
      371    17224                                "123"    "                             "
      372    17225                                 ^        ^
      373    17226                                 S$       T$
      374    17227
      375    17228                            Step 2, target to target:
      376    17229                                "123"    "123                          "
      377    17230                                          ^  ^
      378    17231                                          S$ T$
      379    17232
      380    17233                            Repeat step 2:
      381    17234                                "123"    "123123                       "
      382    17235                                          ^     ^
      383    17236                                          S$    T$
      384    17237
      385    17238                            Repeat step 2:
PL6.E3A0      #001=QCA$ALL_LITERAL_MOVES File=QCA$ALL_LITERALS.:E05TSI           WED 07/30/97 05:01 Page:14   
      386    17239                                "123"    "123123123123                 "
      387    17240                                          ^           ^
      388    17241                                          S$          T$
      389    17242
      390    17243                            Repeat step 2:
      391    17244                                "123"    "123123123123123123123123     "
      392    17245                                          ^                       ^
      393    17246                                          S$                      T$
      394    17247
      395    17248                            All done:
      396    17249                                "123"    "12312312312312312312312312312"
      397    17250        */
      398    17251
      399    17252    2     DCL SIGNED_TARGET_LENGTH SBIN;
      400    17253
      401    17254            /* First we'll move the original ALL string (e.g., ALL "ABC")
      402    17255               to the target... */
      403    17256    2       IF QI_1.OP.LEN.S > QI_1.OP.LEN.T
      404    17257    2       THEN
      405    17258    2         QI_1.OP.LEN.S = QI_1.OP.LEN.T;
      406    17259
      407    17260    2       OLD_TARGET_LENGTH = QI_1.OP.LEN.T;
      408    17261    2       QI_1.OP.LEN.T = QI_1.OP.LEN.S;
      409    17262
      410    17263    2       CALL QCC$ALR ( QI_1.OP );
      411    17264
      412    17265            /* Now we'll start moving the target string within itself,
      413    17266               doubling the length each time, until the target's filled
      414    17267               up... */
      415    17268    2       QI_1.OP.DATA.S$ = QI_1.OP.DATA.T$;
      416    17269    2       QI_1.OP.CIP.S.CA.OFFSET = QI_1.OP.CIP.T.CA.OFFSET;
      417    17270    2       QI_1.OP.INX.S = QI_1.OP.INX.T;
      418    17271    2       QI_1.OP.INX.T = QI_1.OP.INX.T + QI_1.OP.LEN.S;
      419    17272    2       SIGNED_TARGET_LENGTH = OLD_TARGET_LENGTH - QI_1.OP.LEN.S;
      420    17273
      421    17274    3       DO WHILE ( SIGNED_TARGET_LENGTH > 0 );
      422    17275
PL6.E3A0      #001=QCA$ALL_LITERAL_MOVES File=QCA$ALL_LITERALS.:E05TSI           WED 07/30/97 05:01 Page:15   
      423    17276    3         IF QI_1.OP.LEN.S > SIGNED_TARGET_LENGTH
      424    17277    3         THEN
      425    17278    4           DO;
      426    17279    4             QI_1.OP.LEN.S = SIGNED_TARGET_LENGTH;
      427    17280    4             QI_1.OP.LEN.T = SIGNED_TARGET_LENGTH;
      428    17281    4           END;
      429    17282
      430    17283    3         QI_1.OP.FILL_CHAR.TEXT = %QI#CIP_BLANK_FILL;
      431    17284
      432    17285    3         CALL QCC$ALR ( QI_1.OP );
      433    17286
      434    17287    3         QI_1.OP.INX.T = QI_1.OP.INX.T + QI_1.OP.LEN.S;
      435    17288    3         SIGNED_TARGET_LENGTH = SIGNED_TARGET_LENGTH - QI_1.OP.LEN.T;
      436    17289    3         QI_1.OP.LEN.S = QI_1.OP.LEN.S * 2;
      437    17290    3         QI_1.OP.LEN.T = QI_1.OP.LEN.S;
      438    17291
      439    17292    3       END;
      440    17293
      441    17294    2       RETURN;
      442    17295
      443    17296    2   END MOVE_LOTS_TO_X_FIELDS;
      444    17297
      445    17298    1   END QCA$ALL_LITERAL_MOVES;
