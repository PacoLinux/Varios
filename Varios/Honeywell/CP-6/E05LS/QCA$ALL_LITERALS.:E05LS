SOURCE_INDEX_FROM_LAST_TIME
     16896**DCL     17089<<ASSIGN  17137>>IF      17143>>ASSIGN  17147>>ASSIGN  17160<<ASSIGN
SO_MOVE_ALL_TO_X_FIELD_ALREADY
     17039**LABEL   17001--GOTO
STICK_IT
     17188**LABEL   17178--GOTO    17182--GOTO

PL6.E3A0      #002=QCA$ALL_LITERAL_IFS File=QCA$ALL_LITERALS.:E05TSI             WED 07/30/97 05:04 Page:67   
      447        1        /*M* QCA$ALL_LITERAL_IFS:  'IF' an 'ALL "literal"'.  */
      448        2        /*T***********************************************************/
      449        3        /*T*                                                         */
      450        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      451        5        /*T*                                                         */
      452        6        /*T***********************************************************/
      453        7
      454        8        /*P*  NAME:         QCA$ALL_LITERAL_IFS
      455        9              PURPOSE:      Handle ALL "literal" sources or targets compared to
      456       10                            various types of sources or targets.
      457       11              DESCRIPTION:  Handle ALL "literals" compared to various sources
      458       12                            and targets via calls from QCI$IFA and QCI$IFG
      459       13                            who find themselves up against such strange things.
      460       14        */
      461       15
      462       16        QCA$ALL_LITERAL_IFS: PROC;
      463       17
      464       18          %INCLUDE LCP_6;
      465      209
      466      210          %INCLUDE QG_MACRO_M;
      467      347          %INCLUDE QI_EQUATES_E;
      468      706          %INCLUDE QI_MACROS_M;
      469     5764          %QI$ALL;
      470    16877
      471    16878          %SET LISTINC = '1'B;
      472    16879          %SET LISTMAC = '1'B;
      473    16880
      474    16881    1     DCL QCC$ACM ENTRY (1);
      475    16882    1     DCL OLD_OTHER_THINGS_LENGTH UBIN;
      476    16883    1     DCL THE_ALL_LITERALS_TABLE_INDEX UBIN;
      477    16884    1     DCL THE_OTHER_THINGS_TABLE_INDEX UBIN;
      478    16885
      479    16886    1   QCA$ALL_LITERAL_IF: ENTRY;
      480    16887
      481    16888        /*D*  NAME:         QCA$ALL_LITERAL_IF
      482    16889              CALL:         CALL QCA$ALL_LITERAL_IF;
      483    16890              PARAMETERS:   None.
PL6.E3A0      #002=QCA$ALL_LITERAL_IFS File=QCA$ALL_LITERALS.:E05TSI             WED 07/30/97 05:04 Page:68   
      484    16891              INTERFACE:    Called by QCI$IFA or QCI$IFG when an ALL "literal"
      485    16892                            source or target is encountered.
      486    16893              ENVIRONMENT:  Nothing unusual.
      487    16894              INPUT:        QI_1.
      488    16895              OUTPUT:       QI_1.OP.COMP_RESULT will be checked by the caller
      489    16896                            (set by QCC$ACM).
      490    16897              SCRATCH:      Various otter data.
      491    16898              DESCRIPTION:  QCI$IFA or QCI$IFG has given us this ALL "literal" if.
      492    16899                            We'll just bop along comparing the offending ALL "literal"
      493    16900                            to a piece of the source or target (which ever one the
      494    16901                            ALL "literal" isn't) as long as the ALL "literal" at
      495    16902                            a time, waiting for a "not equal" result.
      496    16903                            Admittedly, the ALL "literal" could be expanded into
      497    16904                            dynamic memory, and then compared at one whack, but
      498    16905                            that's sort of like cutting off your memory to spite
      499    16906                            your CIP.
      500    16907        */
      501    16908
      502    16909    1       IF QI_1.IS_ALL_LIT.S
      503    16910    1       THEN
      504    16911    2         DO;
      505    16912    2           THE_ALL_LITERALS_TABLE_INDEX = %QI#SOURCE;
      506    16913    2           THE_OTHER_THINGS_TABLE_INDEX = %QI#TARGET;
      507    16914    2         END;
      508    16915    1       ELSE
      509    16916    2         DO;
      510    16917    2           THE_OTHER_THINGS_TABLE_INDEX = %QI#SOURCE;
      511    16918    2           THE_ALL_LITERALS_TABLE_INDEX = %QI#TARGET;
      512    16919    2         END;
      513    16920
      514    16921            /*  If we're lucky, somebody said something like:
      515    16922                IF TRASH = SPACES THEN...
      516    16923                and so we can use the ALL-literal as the FILL character
      517    16924                for the comparison.  */
      518    16925    1       IF QI_1.OP.LEN_A (THE_ALL_LITERALS_TABLE_INDEX) = 1
      519    16926    1       THEN
      520    16927    2         DO;
PL6.E3A0      #002=QCA$ALL_LITERAL_IFS File=QCA$ALL_LITERALS.:E05TSI             WED 07/30/97 05:04 Page:69   
      521    16928
      522    16929                /* Sometimes, on a lark, the compiler'll gen a DISPLAY
      523    16930                   figurative constant, for stuff like IF GROUP = ZEROES,
      524    16931                   and so the CIP's length gotta get zapped, too.  */
      525    16932    2           QI_1.OP.CIP.S.LEN = 0;
