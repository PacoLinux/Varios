             17115                EDIT_STRING_COUNT - 1;
      263    17116
      264    17117    2         QI_1.OP.INX.S = 0;
      265    17118    2         QI_1.OP.LEN.T = PIC$ -> QI$ALPHANUMERIC_PICTURE_STRING.EDIT_STRING_A.LENGTH (
             17118                  MOP_STRING_INDEX);
      266    17119
      267    17120    3         DO CASE ( PIC$ -> QI$ALPHANUMERIC_PICTURE_STRING.EDIT_STRING_A.FUNCTION (
             17120                  MOP_STRING_INDEX) );
      268    17121
      269    17122    3           CASE ( %QI#PIC_M_9_A_X );
      270    17123    3             IF QI_1.OP.LEN.S > 1
      271    17124    3             THEN
      272    17125    4               DO;
      273    17126
PL6.E3A0      #001=QCA$ALL_LITERAL_MOVES File=QCA$ALL_LITERALS.:E05TSI           WED 07/30/97 05:01 Page:10   
      274    17127                      /* SOURCE_INDEX_FROM_LAST_TIME > 0 means we had something
      275    17128                         like 'MOVE ALL "ABCDEFG" TO PICX5B2X3.' (where
      276    17129                         PICX5B2X3 has a PICture clause of 'PIC X(5)B(2)X(3)').
      277    17130                         The first time through CASE %QI#PIC_M_9_A_X, we'd
      278    17131                         move "ABCDE" to the X(5) part.  Then we'd go through
      279    17132                         CASE %QI#PIC_M_B, and stick in the two blanks.
      280    17133                         Supposing we're now in CASE %QI#PIC_M_9_A_X again,
      281    17134                         we'll make QI_1.OP.INX.S = 5, and then go through
      282    17135                         the following in order to squeeze "FG" into the
      283    17136                         leftmost two characters of the remaining X(3)... */
      284    17137    4                 IF SOURCE_INDEX_FROM_LAST_TIME > 0
      285    17138    4                 THEN
      286    17139    5                   DO;
      287    17140
      288    17141    5                     OLD_SOURCE_LENGTH = QI_1.OP.LEN.S;
      289    17142    5                     OLD_TARGET_LENGTH = QI_1.OP.LEN.T;
      290    17143    5                     LEFT_TRUNCATION_LENGTH = QI_1.OP.LEN.S
      291    17144    5                       - SOURCE_INDEX_FROM_LAST_TIME;
      292    17145    5                     QI_1.OP.LEN.S = LEFT_TRUNCATION_LENGTH;
      293    17146    5                     QI_1.OP.LEN.T = LEFT_TRUNCATION_LENGTH;
      294    17147    5                     QI_1.OP.INX.S = SOURCE_INDEX_FROM_LAST_TIME;
      295    17148
      296    17149    5                     CALL QCC$ALR ( QI_1.OP );
      297    17150
      298    17151    5                     QI_1.OP.LEN.S = OLD_SOURCE_LENGTH;
      299    17152    5                     QI_1.OP.LEN.T = OLD_TARGET_LENGTH
      300    17153    5                       - LEFT_TRUNCATION_LENGTH;
      301    17154    5                     QI_1.OP.INX.S = 0;
      302    17155    5                     QI_1.OP.INX.T = QI_1.OP.INX.T
      303    17156    5                       + LEFT_TRUNCATION_LENGTH;
      304    17157
      305    17158    5                   END;
      306    17159
      307    17160    4                 SOURCE_INDEX_FROM_LAST_TIME = MOD ( QI_1.OP.LEN.T, QI_1.OP.LEN.S );
      308    17161
      309    17162    4                 OLD_SOURCE_DATA$ = QI_1.OP.DATA.S$;
      310    17163    4                 OLD_SOURCE_CIP_WORD0 = QI_1.OP.CIP.S_A (0);
PL6.E3A0      #001=QCA$ALL_LITERAL_MOVES File=QCA$ALL_LITERALS.:E05TSI           WED 07/30/97 05:01 Page:11   
      311    17164    4                 OLD_SOURCE_LENGTH = QI_1.OP.LEN.S;
      312    17165
      313    17166    4                 CALL MOVE_LOTS_TO_X_FIELDS;
      314    17167
      315    17168    4                 QI_1.OP.DATA.S$ = OLD_SOURCE_DATA$;
      316    17169    4                 QI_1.OP.CIP.S_A (0) = OLD_SOURCE_CIP_WORD0;
      317    17170    4                 QI_1.OP.LEN.S = OLD_SOURCE_LENGTH;
      318    17171
      319    17172    4               END;
      320    17173    3             ELSE
      321    17174    3               QI_1.OP.INX.T = QI_1.OP.INX.T + QI_1.OP.LEN.T;
      322    17175
      323    17176    3           CASE ( %QI#PIC_M_B );
      324    17177    3             QI_1.OP.FILL_CHAR.TEXT = %QI#CIP_BLANK_FILL;
      325    17178    3             GOTO STICK_IT;
