       28     5769          %QI$ALL;
       29    16882
       30    16883    1     DCL QCS$SETUP_SOURCE ENTRY;
       31    16884    1     DCL QCS$SETUP_TARGET ENTRY;
       32    16885    1     DCL QCC$ALR ENTRY (1);
       33    16886
       34    16887    1     DCL LEFT_TRUNCATION_LENGTH UBIN;
       35    16888    1     DCL MOP_STRING_INDEX UBIN;
