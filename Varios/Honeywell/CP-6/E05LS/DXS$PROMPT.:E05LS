      144     3385    2             OUT_DEV = SAVE_OUT;
      145     3386    2             IF OUT_DEV = DCTX_ME
      146     3387    2             THEN S_D.OUT_WIDTH = 79;
      147     3388    2             ELSE S_D.OUT_WIDTH = 132;
      148     3389    2           END;
PL6.E3A0      #001=DDS$PROMPTC File=DXS$PROMPT.:E05TSI                           TUE 07/29/97 15:14 Page:5    
      149     3390    1        IF ( S_D.PATCH ) AND ( S_D.INPUT = '!EOD' )
      150     3391    2        THEN DO;
      151     3392    2             S_D.INPUT = 'GO        "Supplied by DXS$PROMPT';
      152     3393    2             S_D.NCHARS = LENGTHC( 'GO        "Supplied by DXS$PROMPT' );
      153     3394    2             BOS = 0;
      154     3395    2             IN_DEV = DCTX_ME;
      155     3396    2             OUT_DEV = OUT_DEV_SAVE;
      156     3397    2           END;
      157     3398    1        IF S_D.PATCH AND ADDR(S_D.INPUT)->CHAR1 ~= 'M'
      158     3399    2        THEN DO;                           /* Echo input, avoiding most modifies */
      159     3400    2             S_D.OUTPUT = ' ';
      160     3401    2             S_D.XDP.IMAGE.INP = S_D.INPUT;
      161     3402    2             CALL DXQ$WRTPI (ADDR(S_D.XDP), SIZEC(S_D.XDP.IMAGE));
      162     3403    2           END;
      163     3404    1        RETURN;
      164     3405                                                /**/
      165     3406    1   ERR:
      166     3407    1        S_D.ERROR = E@IN_DEV_ERROR;        /* E@IN_DEV_ERROR */
      167     3408    1        CALL DDE$ERROR;
      168     3409    1        GOTO WRITEIT;
      169     3410        %EJECT;
PL6.E3A0      #001=DDS$PROMPTC File=DXS$PROMPT.:E05TSI                           TUE 07/29/97 15:14 Page:6    
      170     3411                                                /**/
      171     3412    1   EDDIT: PROC;
      172     3413                                                /**/
      173     3414    2        OUTPT = 0;
      174     3415    3        DO IX = 0 TO SIZEREAD - 1;
      175     3416    3             LOCAR = PINCRC( EBUF$, IX ) -> CHAR1;
      176     3417    3             IF ( LOCAR = BS ) OR ( LOCAR = DEL )
      177     3418    4             THEN DO;
      178     3419    4                  OUTPT = OUTPT - 1;
      179     3420    4                  IF ( OUTPT < 0 )
      180     3421    4                  THEN OUTPT = 0;
      181     3422    4                  ELSE PINCRC( EBUF$, OUTPT ) -> CHAR1 = ' ';
      182     3423    4                END;
      183     3424    3             ELSE IF ( LOCAR = CAN )
      184     3425    3                  THEN OUTPT = 0;
      185     3426    3                  ELSE IF ( IN_DEV = DCTX_CR )
      186     3427    4                       THEN DO;
      187     3428    4                            CALL INDEX( IXX, LOCAR, PUNCHIN ) ALTRET( NOTSPEC );
      188     3429    4                            PINCRC( EBUF$, OUTPT ) -> CHAR1 = PUNCHOUT( IXX );
      189     3430    4                            OUTPT = OUTPT + 1;
      190     3431    4                          END;
      191     3432    3                       ELSE
      192     3433    4   NOTSPEC:            DO;
      193     3434    4                            PINCRC( EBUF$, OUTPT ) -> CHAR1 = B$XL_UPPER.C( ASCBIN(
              3434                                     LOCAR ) );
      194     3435    4                            OUTPT = OUTPT + 1;
      195     3436    4                          END;
      196     3437    3           END;
      197     3438                                                /**/
      198     3439    2        IX = OUTPT - 1;
      199     3440    3        DO WHILE ( ( IX >= 0 ) AND ( PINCRC( EBUF$, IX ) -> CHAR1 = ' ' ) );
      200     3441    3             IX = IX - 1;
      201     3442    3           END;
      202     3443    2        OUTPT = IX + 1;
      203     3444                                                /**/
      204     3445    2        RETURN;
      205     3446                                                /**/
PL6.E3A0      #001=DDS$PROMPTC File=DXS$PROMPT.:E05TSI                           TUE 07/29/97 15:14 Page:7    
      206     3447    2   END EDDIT;
      207     3448                                                /**/
      208     3449    1   END DDS$PROMPTC;

