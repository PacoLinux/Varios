       10       10        DDS$PROMPTC: PROC;
       11       11                                                /**/
       12       12    1   DCL IX SBIN;                            /* Must be SBIN */
       13       13    1   DCL IXX UBIN;
       14       14    1   DCL PROS UBIN;
       15       15    1   DCL SIZEREAD UBIN;
       16       16    1   DCL OUTPT SBIN;                         /* Must be SBIN */
       17       17    1   DCL LOCAR CHAR(1);
       18       18    1   DCL SAVE_OUT UBIN;
       19       19    1   DCL BNS UBIN;
       20       20    1   DCL CHAR1 CHAR(1) BASED;
       21       21    1   DCL BN$EW CHAR(BNS) BASED(BN$);
       22       22    1   DCL BO$LD CHAR(BOS) BASED(BO$);
       23       23    1   DCL PR$OM CHAR(PROS) BASED(PROM$);
       24       24    1   DCL PUNCHINT(0:3) CHAR(1) UNAL CONSTANT INIT( ']', '^', '|', BITASC('331'O) );
       25       25    1   DCL PUNCHIN REDEF PUNCHINT CHAR(4);
       26       26    1   DCL PUNCHOUT(0:3) CHAR(1) UNAL CONSTANT INIT( '!', '~', '^', '\' );
       27       27    1   DCL BS CHAR(1) CONSTANT INIT(BITASC('010'O));
       28       28    1   DCL CAN CHAR(1) CONSTANT INIT(BITASC('030'O));
       29       29    1   DCL DEL CHAR(1) CONSTANT INIT(BITASC('177'O));
       30       30    1   DCL ESC(0:3) CHAR(1) UNAL CONSTANT SYMDEF INIT(BITASC('033'O), /* ESC */
       31       31    1                           BITASC('133'O), /* [ */
       32       32    1                           BITASC('173'O), /* { */
       33       33    1                          BITASC('033'O)); /* Filler */
       34       34    1   DCL EESC REDEF ESC CHAR(4);
       35       35    1   DCL BO$ PTR CONSTANT INIT(ADDR(BUFOLD));
       36       36    1   DCL BUFOLE(0:255) CHAR(1) UNAL STATIC INIT( ' '*256 );
       37       37    1   DCL BUFOLD REDEF BUFOLE CHAR(256);
PL6.E3A0      #001=DDS$PROMPTC File=DXS$PROMPT.:E05TSI                           TUE 07/29/97 15:14 Page:2    
       38       38    1   DCL BOS UBIN STATIC INIT( 0 );
       39       39    1   DCL BUFNEW CHAR(256) STATIC;
       40       40    1   DCL BN$ PTR CONSTANT INIT( ADDR ( BUFNEW ) );
       41       41    1   DCL EBUF$ PTR;
       42       42    1   DCL PROMPT CHAR(30) STATIC;
       43       43    1   DCL PROM$ PTR CONSTANT INIT( ADDR( PROMPT ) );
       44       44    1   DCL DCTX_ME SBIN SYMREF;
       45       45    1   DCL DCTX_CR UBIN SYMREF;
       46       46    1   DCL DCTX_PATCH UBIN SYMREF;
       47       47    1   DCL IN_DEV UBIN SYMREF;
       48       48    1   DCL OUT_DEV UBIN SYMREF;
