       57     3298    1   DCL DDQ$WRITENC ENTRY(2);
       58     3299    1   DCL DDQ$WRITE ENTRY(2);
       59     3300    1   DCL DDQ$READ ENTRY(2) ALTRET;
       60     3301    1   DCL DDE$ERROR ENTRY;
       61     3302    1   DCL DXQ$WRTPI ENTRY(2);
       62     3303        %SUB @MACTP="' M'/* @MACTP */";
       63     3304        %SUB @CONTP="'99'/* @CONTP */";
       64     3305                                                /**/
       65     3306    1        CALL BINCHAR( SUBSTR( PROMPT, 0, LENGTHC( @CONTP ) ), S_D.STORED );
       66     3307    1        CALL INSERT( PROMPT, LENGTHC( @CONTP ), , S_D.UPROMPT );
       67     3308    1        PROS = LENGTHC( @CONTP ) + S_D.UPROMPT_SIZE;
       68     3309    1        GOTO WRITEIT;
       69     3310                                                /**/
       70     3311    1   DDS$PROMPT: ENTRY;
       71     3312                                                /**/
       72     3313    1        IF S_D.MACTIVE
       73     3314    2        THEN DO;
       74     3315    2             CALL CONCAT( PROMPT, @MACTP, S_D.UPROMPT );
PL6.E3A0      #001=DDS$PROMPTC File=DXS$PROMPT.:E05TSI                           TUE 07/29/97 15:14 Page:3    
       75     3316    2             PROS = LENGTHC( @MACTP ) + S_D.UPROMPT_SIZE;
       76     3317    2           END;
       77     3318    2        ELSE DO;
       78     3319    2             PROMPT = S_D.UPROMPT;
       79     3320    2             PROS = S_D.UPROMPT_SIZE;
       80     3321    2           END;
       81     3322                                                /**/
       82     3323    1   WRITEIT:
       83     3324    1        S_D.OUTPUT = PR$OM;
       84     3325    1        S_D.INPUT = ' ';                   /* Blank out input buffer */
       85     3326    1        IF IN_DEV = DCTX_ME
       86     3327    2        THEN DO;
       87     3328    2             SAVE_OUT = OUT_DEV;
       88     3329    2             OUT_DEV = DCTX_ME;
       89     3330    2             S_D.OUT_WIDTH = 79;
       90     3331    2             IF S_D.MACTIVE OR S_D.STEP
       91     3332    2             THEN CALL DDQ$WRITENC ( ADDR( S_D.OUTPUT ), PROS );
       92     3333    2             ELSE CALL DDQ$WRITE( ADDR( S_D.OUTPUT ), PROS );
       93     3334    2             OUT_DEV = SAVE_OUT;
       94     3335    2             IF OUT_DEV = DCTX_ME
       95     3336    2             THEN S_D.OUT_WIDTH = 79;
       96     3337    2             ELSE S_D.OUT_WIDTH = 132;
       97     3338    2           END;
       98     3339                                                /**/
       99     3340    1        S_D.STEP = '0'B;
      100     3341    1        SIZEREAD = LENGTHC( S_D.INPUT );
      101     3342    1        EBUF$ = ADDR(S_D.INPUT);
      102     3343    1        CALL DDQ$READ ( EBUF$, SIZEREAD ) ALTRET( ERR );
      103     3344    1        CALL EDDIT;
      104     3345    1        IF OUTPT > 0
      105     3346    1        THEN CALL INDEX( IXX, PINCRC(EBUF$,IX)->CHAR1, EESC ) ALTRET( ACCEPT );
      106     3347    1        ELSE GOTO ACCEPT;                  /* Wipeout BO$LD */
      107     3348    1   AGAIN:
      108     3349    2        DO;
      109     3350    2             CALL CONCAT( S_D.OUTPUT, PR$OM, BO$LD );
      110     3351    2             OUTPT = PROS + BOS;
      111     3352    2             CALL DDQ$WRITE( ADDR( S_D.OUTPUT ), OUTPT );
PL6.E3A0      #001=DDS$PROMPTC File=DXS$PROMPT.:E05TSI                           TUE 07/29/97 15:14 Page:4    
      112     3353    2             BUFNEW = ' ';
      113     3354    2             BNS = LENGTHC( BUFNEW );
      114     3355    2             CALL DDQ$READ( BN$, BNS ) ALTRET( ERR );
      115     3356    2             CALL CONCAT( S_D.INPUT, BO$LD, BN$EW );
      116     3357    2             CALL EDDIT;
      117     3358    2             IF OUTPT > 0
      118     3359    2             THEN CALL INDEX( IXX, PINCRC(EBUF$,IX)->CHAR1, EESC ) ALTRET( GO1 );
      119     3360    2             ELSE GOTO GO1;
      120     3361    2             GOTO AGAIN;                   /* Hit Ctrl-X, ESC or xxxx, ESC */
      121     3362    2   GO1:
      122     3363    2             BUFOLD = S_D.INPUT;
      123     3364    2             BOS = OUTPT;
      124     3365    2             S_D.NCHARS = BOS;
      125     3366                                                /**/
      126     3367    2             GOTO RETN;
      127     3368                                                /**/
      128     3369    2           END AGAIN;
      129     3370                                                /**/
      130     3371    1   ACCEPT:
      131     3372    1        S_D.NCHARS = OUTPT;
      132     3373    1        BUFOLD = S_D.INPUT;
      133     3374    1        BOS = OUTPT;
      134     3375                                                /**/
      135     3376    1   RETN:
      136     3377    1        IF IN_DEV ~= DCTX_ME
