      157      350                TSTAMP=UTS_BUF);
      158      369        /**/
      159      370        %EJECT;
PL6.E3A0      #001=IPE$LIST File=IPE$LIST.:E05TSI                                TUE 07/29/97 22:41 Page:7    
      160      371        /**/
      161      372        /*F*  NAME:     IPE$LIST1_INFO                                     */
      162      373        /*F*  PURPOSE:  To print a line of LIST information for a given    */
      163      374        /*,*            shared processor.                                  */
      164      375    1   IPE$LIST1_INFO: ENTRY(PNUM);
      165      376        /**/
      166      377    1           IPNUM = PNUM;
      167      378    1           IF NOT(B$P.FLGS(PNUM) & %P_OVRLY) THEN
      168      379    2               DO WHILE (IPNUM ~= 0);
      169      380    2                 LIST1_STR = ' ';
      170      381    2                 CALL IPD$BINTXT(ADDR(LIST1.PNUM),IPNUM,DEC,SIZEC(LIST1.PNUM));
      171      382    2                 TP$ = ADDR(B$P.NAME(IPNUM));
      172      383    2                 LIST1.NAME = TP$->B$TEXTC.TEXT;
      173      384    2                 TP$ = ADDR(B$P.ACCT(IPNUM));
      174      385    2                 LIST1.ACCT = TP$->B$CHAR8;
      175      386    2                 UVAL = 0;
      176      387    2                 BVAL.LOWHALF = B$P.FLGS(IPNUM);
      177      388    2                 CALL IPD$BINTXT(ADDR(LIST1.FLGS),UVAL,OCT,SIZEC(LIST1.FLGS));
      178      389    2                 UVAL = BITBIN(B$P.PRIV(IPNUM));
      179      390    2                 CALL IPD$BINTXT(ADDR(LIST1.PRIV),UVAL,OCT,SIZEC(LIST1.PRIV));
      180      391    2                 UVAL = B$P.UC(IPNUM);
      181      392    2                 CALL IPD$BINTXT(ADDR(LIST1.UC),UVAL,DEC,SIZEC(LIST1.UC));
      182      393    2                 UVAL = B$P.PPC(IPNUM);
      183      394    2                 CALL IPD$BINTXT(ADDR(LIST1.PPC),UVAL,DEC,SIZEC(LIST1.PPC));
      184      395    2                 UVAL = B$P.PVA(IPNUM);
      185      396    2                 CALL IPD$BINTXT(ADDR(LIST1.PVA),UVAL,DEC,SIZEC(LIST1.PVA));
      186      397    2                 UVAL = B$P.DVA(IPNUM);
      187      398    2                 CALL IPD$BINTXT(ADDR(LIST1.DVA),UVAL,DEC,SIZEC(LIST1.DVA));
      188      399    2                 UVAL = B$P.HVA(IPNUM);
      189      400    2                 CALL IPD$BINTXT(ADDR(LIST1.HVA),UVAL,DEC,SIZEC(LIST1.HVA));
      190      401    2                 UVAL = B$P.LNK(IPNUM);
      191      402    2                 CALL IPD$BINTXT(ADDR(LIST1.LNK),UVAL,DEC,SIZEC(LIST1.LNK));
      192      403                      %PRINT(STR=LIST1);
      193      407    2                 IPNUM = B$P.LNK(IPNUM);
      194      408    2               END;
      195      409    1           RETURN;
      196      410        /**/
PL6.E3A0      #001=IPE$LIST File=IPE$LIST.:E05TSI                                TUE 07/29/97 22:41 Page:8    
      197      411        %EJECT;
PL6.E3A0      #001=IPE$LIST File=IPE$LIST.:E05TSI                                TUE 07/29/97 22:41 Page:9    
      198      412        /**/
      199      413        /*F*    NAME:   IPE$LIST2_INFO
      200      414                PURPOSE: To print the second line of LIST information for
      201      415                        a given shared program.                            */
      202      416    1   IPE$LIST2_INFO: ENTRY(PNUM);
      203      417        /**/
      204      418    1           IPNUM = PNUM;
      205      419    1           IF NOT(B$P.FLGS(PNUM) & %P_OVRLY) THEN
      206      420    2               DO WHILE (IPNUM ~= 0);
      207      421    2                 LIST2_STR = ' ';
      208      422    2                 LIST2.MODTIME.DASH = '-';
      209      423    2                 LIST2.MODTIME.COLON = ':';
      210      424    2                 CALL IPD$BINTXT(ADDR(LIST2.PNUM),IPNUM,DEC,SIZEC(LIST2.PNUM));
      211      425    2                 TP$ = ADDR(B$P.NAME(IPNUM));
      212      426    2                 LIST2.NAME = TP$->B$TEXTC.TEXT;
      213      427    2                 UVAL = B$P.DSZ(IPNUM);
      214      428    2                 CALL IPD$BINTXT(ADDR(LIST2.DSZ),UVAL,OCT,SIZEC(LIST2.DSZ));
      215      429    2                 UVAL = B$P.DADDR(IPNUM);
      216      430    2                 CALL IPD$BINTXT(ADDR(LIST2.DADDR),UVAL,OCT,SIZEC(LIST2.DADDR));
      217      431    2                 UVAL = B$P.TCBSZ(IPNUM);
      218      432    2                 CALL IPD$BINTXT(ADDR(LIST2.TCBSZ),UVAL,DEC,SIZEC(LIST2.TCBSZ));
      219      433    2                 UVAL = B$P.SA(IPNUM);
      220      434    2                 CALL IPD$BINTXT(ADDR(LIST2.SA),UVAL,OCT,SIZEC(LIST2.SA));
      221      435    2                 UVAL = B$P.HPP(IPNUM);
      222      436    2                 CALL IPD$BINTXT(ADDR(LIST2.HPP),UVAL,OCT,SIZEC(LIST2.HPP));
      223      437    2                 UVAL = B$P.FRQ(IPNUM);
      224      438    2                 CALL IPD$BINTXT(ADDR(LIST2.FRQ),UVAL,DEC,SIZEC(LIST2.FRQ));
      225      439    2                 UTS_BUF = B$P.INSTDATE(IPNUM);
      226      440    2                 CALL M$TIME(FPT_TIME);
      227      441    2                 LIST2.MODTIME.YY = DATE_BUF;
      228      442    2                 LIST2.MODTIME.HH = TIME_BUF.HH;
      229      443    2                 LIST2.MODTIME.MM = TIME_BUF.MM;
      230      444    2                 UVAL = B$P.LIB_LNK(IPNUM);
      231      445    2                 CALL IPD$BINTXT(ADDR(LIST2.LIB_LNK),UVAL,DEC,SIZEC(LIST2.LIB_LNK));
      232      446                      %PRINT(STR=LIST2);
      233      450    2                 IPNUM = B$P.LNK(IPNUM);
      234      451    2               END;
PL6.E3A0      #001=IPE$LIST File=IPE$LIST.:E05TSI                                TUE 07/29/97 22:41 Page:10   
      235      452    1           RETURN;
