      131      294        /**/
      132      295    1   DCL M$LO DCB;
      133      296    1   DCL P$ PTR SYMREF;
      134      297        /**/
      135      298    1   DCL 1 B$TEXTC BASED ALIGNED,
      136      299    1         2 COUNT UBIN(9) UNAL,
      137      300    1         2 TEXT CHAR(B$TEXTC.COUNT) UNAL;
      138      301    1   DCL B$CHAR8 CHAR(8) UNAL BASED;
      139      302        /**/
      140      303    1   DCL IPD$BINTXT ENTRY(4) ALTRET;
      141      304        /**/
      142      305        %MACRO PRINT(STR=NIL);
      143      306                WRITE_LO.BUF_ = VECTOR(STR);
      144      307                CALL M$WRITE(WRITE_LO);
      145      308        %MEND;
      146      309        /**/
      147      310                %SUB B$P=P$->B$P;
      148      311                %SUB OCT='1'B;
      149      312                %SUB DEC='0'B;
