       50    14370        %XUH_PARAM (NAME = XUH_PARAM ,
       51    14371                    PROCNAME = VLP_SUPER,
       52    14372                    PROCACCT = FILE_ACCT ,
       53    14373                    OUTDCB1 = M$LO ,
       54    14374                    OUTDCB2 = M$DO ,
       55    14375                    STCLASS = STATIC) ;
       56    14436        %VLP_NAME (FPTN = VLP_SUPER,
       57    14437                    NAME = 'SUPER',
       58    14438                    LEN = 5 ,
       59    14439                    STCLASS = CONSTANT) ;
