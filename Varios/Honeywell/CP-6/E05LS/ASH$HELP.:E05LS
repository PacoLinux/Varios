       30    14267        /**********************************************************************/
       31    14268        /*   Based data   based data   based data   based data   based data   */
       32    14269        /**********************************************************************/
       33    14270        %PARSE$OUT (NAME = OUT$BLK, STCLASS = BASED) ;
       34    14315        %PARSE$SYM (NAME=OUT$SYM, STCLASS="BASED (BLK$)");
       35    14353        /*****************************************************************/
       36    14354        /*    Entry dcls   entry dcls   entry dcls   entry dcls   entry dcls   */
