       69    14675        %EJECT;
PL6.E3A0      #001=ASH$HELP File=ASH$HELP.:E05TSI                                TUE 07/29/97 09:51 Page:3    
       70    14676        /*******************************************************************/
       71    14677        /*   Auto data   auto data   auto data   auto data   auto data     */
       72    14678        /*******************************************************************/
       73    14679    1   DCL I  SBIN;
       74    14680    1   DCL J  SBIN;
       75    14681    1   DCL BLK$  PTR;
       76    14682    1   DCL 1 HELP_STRING,
       77    14683    1          2 COUNT UBIN  BYTE UNAL,
       78    14684    1          2 TXT   CHAR(31) CALIGNED;
       79    14685    1   DCL 1 PROCNAME,
       80    14686    1          2 COUNT UBIN  BYTE UNAL,
       81    14687    1          2 TXT   CHAR(24) CALIGNED;
       82    14688    1   DCL 1 SUP_NAME CONSTANT,
       83    14689    1          2 COUNT UBIN BYTE UNAL INIT(5),
       84    14690    1          2 TXT  CHAR(5) CALIGNED INIT('SUPER');
       85    14691        %EJECT;
PL6.E3A0      #001=ASH$HELP File=ASH$HELP.:E05TSI                                TUE 07/29/97 09:51 Page:4    
       86    14692        /*****************************************************************************/
       87    14693        /*   Procedure   procedure   procedure   procedure   procedure   procedure   */
       88    14694        /*****************************************************************************/
       89    14695    1   ASH_HELP = '1'B;
       90    14696    1   BLK$ = P_PCB.OUT$;
       91    14697    1   XUH_PARAM.TOPICS# = '0'B;
       92    14698    2   DO WHILE (OUT$SYM.CODE ~= HELP_VAL);
       93    14699    2      IF (OUT$SYM.NSUBLKS = 0) THEN RETURN;
       94    14700    2      BLK$ = BLK$ -> OUT$BLK.SUBLK$(0);
       95    14701    2      END;  /* While */
       96    14702    1   BLK$ = BLK$ -> OUT$BLK.SUBLK$(0);
       97    14703        /*     BLK$ = P_PCB.OUT$ -> OUT$BLK.SUBLK$(0); */
       98    14704    1   CALL INDEX (I, '(', OUT$SYM.TEXT)
       99    14705    2   WHENRETURN DO;
      100    14706    2      CALL INDEX (J,')',OUT$SYM.TEXT)
      101    14707    3      WHENRETURN DO;
      102    14708    4         IF (J>I) THEN DO;
      103    14709    4            PROCNAME.TXT = SUBSTR (OUT$SYM.TEXT,I+1,J-1);
      104    14710    4            PROCNAME.COUNT = J - I + 1;
      105    14711
      106    14712    4            XUH_PARAM.PROCNAME_ = VECTOR (PROCNAME);
      107    14713    4            XUH_PARAM.HELP$ = BLK$ -> OUT$BLK.SUBLK$(0);
      108    14714    4            XUH_PARAM.KEYWORD_ = VECTOR (NIL) ;
      109    14715    4            END; /* If then */
      110    14716    3         ELSE RETURN;
      111    14717    3         END; /* Whenreturn */
      112    14718
      113    14719    3      WHENALTRETURN DO;
      114    14720    3         RETURN;
      115    14721    3         END; /* Whenaltreturn */
      116    14722    2      END; /*  Whenreturn  */
      117    14723
      118    14724    2   WHENALTRETURN DO;
      119    14725    2      HELP_STRING.TXT = SUBSTR (OUT$SYM.TEXT,0,OUT$SYM.COUNT);
      120    14726    2      HELP_STRING.COUNT = OUT$SYM.COUNT;
      121    14727    2      XUH_PARAM.PROCNAME_ = VECTOR (SUP_NAME);
      122    14728    2      XUH_PARAM.HELP$ = ADDR(HELP_STRING);
PL6.E3A0      #001=ASH$HELP File=ASH$HELP.:E05TSI                                TUE 07/29/97 09:51 Page:5    
      123    14729    2      XUH_PARAM.KEYWORD_ = VECTOR(NIL);
      124    14730    2      END; /*  Whenaltreturn  */
      125    14731
      126    14732    1   CALL X$HELP (XUH_PARAM) ALTRET (HELP_ERR) ;
      127    14733    1   RETURN;
      128    14734
      129    14735    1   ASH$TOPICS: ENTRY ;
      130    14736
      131    14737    1   XUH_PARAM.TOPICS# = '1'B;
      132    14738    1   CALL X$HELP (XUH_PARAM) ALTRET (HELP_ERR) ;
      133    14739    1   RETURN;
      134    14740
      135    14741    1   ASH$MORE_HELP: ENTRY ;
      136    14742
      137    14743    1   XUH_PARAM.MORE# = '1'B ;
      138    14744    1   CALL X$HELP (XUH_PARAM) ALTRET (HELP_ERR);
      139    14745    1   RETURN;
      140    14746
      141    14747    1   ASH$ALL_HELP: ENTRY ;
      142    14748
      143    14749    1   XUH_PARAM.ALL# = '1'B ;
      144    14750    1   CALL X$HELP (XUH_PARAM) ALTRET (HELP_ERR);
      145    14751    1   RETURN;
      146    14752
      147    14753    1   HELP_ERR:
      148    14754    1   CALL ASH$ERROR( XUH_PARAM.ERR );
      149    14755    1   IF NOT XUH_PARAM.ERR.MON AND XUH_PARAM.ERR.ERR# = %E$BDTOPIC THEN
      150    14756    1      ASH_HELP = '11'B;
      151    14757    1   RETURN;
      152    14758
      153    14759
      154    14760    1   END ASH$HELP;
