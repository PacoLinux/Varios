        1        1        /*M* DUS$PROMPT Prompt & read input commands for User Delta. */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MOC,DCI=5,CRT=5 */
        8        8        /*F* NAME: DUS$PROMPT
        9        9             PURPOSE: To prompt the user & then read his command.
       10       10             DESCRIPTION: Builds prompt, if it has changed, then calls M$PROMPT
       11       11                          to set new prompt. Sets transition VFC if DELTA was just
       12       12                          entered. */
       13       13        /*D* NAME: DUS$PROMPT
       14       14             ENTRY: DDS$PROMPT
       15       15             CALL: CALL DDS$PROMPT;
       16       16             INPUT: S_D.PROMPT - Current prompt, including VFC.
       17       17                    S_D.PROMPT_SIZE - Size of S_D.PROMPT.
       18       18                    S_D.UPROMPT - User specified prompt.
       19       19                    S_D.UPROMPT_SIZE - Size of user specified prompt.
       20       20                    S_D.READPOS - Record position of record to be read.
       21       21             OUTPUT: S_D.PROMPT - Current prompt, including VFC.
       22       22                     S_D.PROMPT_SIZE - Size of S_D.PROMPT.
       23       23                     S_D.READPOS - Record position of record to be read.
       24       24             INTERFACE: DDQ$READ
       25       25                        DUE$ERRMSG;
       26       26                        M$PROMPT;
       27       27             ENVIRONMENT: User Delta.
       28       28             SCRATCH: Local AUTO only.
       29       29             DESCRIPTION: Builds prompt, if it has changed, then calls M$PROMPT
       30       30                          to set new prompt. Sets transition VFC if DELTA was just
       31       31                          entered. */
       32       32                                                /**/
       33       33        DDS$PROMPTC: PROC;
       34       34                                                /**/
       35       35        %INCLUDE CP_6;
       36      116        /* Auto */
       37      117    1   DCL 1 PROMPT,
PL6.E3A0      #001=DDS$PROMPTC File=DUS$PROMPT.:E05TSI                           TUE 07/29/97 15:00 Page:2    
       38      118    1         2 VFC CHAR(1) UNAL,
       39      119    1         2 TEXT CHAR(30) UNAL;
       40      120    1   DCL PRONEW_SIZE UBIN;
       41      121    1   DCL PRONEW$ PTR;
       42      122    1   DCL PROOLD_SIZE UBIN;
       43      123        /* Based */
       44      124    1   DCL 1 PROM$NEW BASED UNAL,
       45      125    1         2 * CHAR( PRONEW_SIZE );
       46      126    1   DCL 1 PROM$OLD BASED UNAL,
       47      127    1         2 * CHAR( PROOLD_SIZE );
       48      128        /* Delta's static data */
       49      129        %INCLUDE XUH_MACRO_C;
       50      213        %INCLUDE DD_SD_M;
       51     1549        %S_D;
       52     2867        %INCLUDE DD$BASED;
       53     4170        %FPRG$CONTEXT;
       54     4233        /* Constant */
       55     4234        %FPT_PROMPT (FPTN=FPT_PROMPT,
       56     4235                     STCLASS=CONSTANT SYMDEF,
       57     4236                     VFC=YES);
       58     4264        /* Symref */
       59     4265        %FPT_PROMPT (FPTN=FPTPROMPT,
       60     4266                     STCLASS=);
       61     4294        /* Entry */
       62     4295    1   DCL DDQ$READ ENTRY(2);
       63     4296    1   DCL DUE$ERRMSG ENTRY(0);
       64     4297        /* Misc */
       65     4298        %INCLUDE NK_VFC_C;
       66     4372        %SUB @MACTP="' M'/* @MACTP */";
       67     4373        %SUB @CONTP="'99'/* @CONTP */";
       68     4374        %SUB @MLCTP="' MLCT'/* @MLCTP */";
       69     4375                                                /**/
       70     4376    1        IF S_D.FPRG
       71     4377    2        THEN DO;
       72     4378    2             CALL CONCAT(S_D.UFPROMPT, ' ',SUBSTR(S_D.CURRENT_FPRG_CONTEXT$ ->
              4378                      FPRG$CONTEXT.PROMPT,
       73     4379    2               0, S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.PROMPT_SIZE), '>');
PL6.E3A0      #001=DDS$PROMPTC File=DUS$PROMPT.:E05TSI                           TUE 07/29/97 15:00 Page:3    
       74     4380    2             S_D.UFPROMPT_SIZE = S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.PROMPT_SIZE
              4380                      + 2;
       75     4381    2           END;
       76     4382    1        CALL BINCHAR( SUBSTR( PROMPT.TEXT, 0, LENGTHC( @CONTP ) ), S_D.STORED );
       77     4383    1        IF S_D.FPRG
       78     4384    2        THEN DO;
       79     4385    2             CALL INSERT( PROMPT.TEXT, LENGTHC( @CONTP ), , S_D.UFPROMPT );
       80     4386    2             PRONEW_SIZE = LENGTHC( @CONTP ) + S_D.UFPROMPT_SIZE;
       81     4387    2           END;
       82     4388    2        ELSE DO;
       83     4389    2             CALL INSERT( PROMPT.TEXT, LENGTHC( @CONTP ), , S_D.UPROMPT );
       84     4390    2             PRONEW_SIZE = LENGTHC( @CONTP ) + S_D.UPROMPT_SIZE;
       85     4391    2           END;
       86     4392    1        GOTO SETIT;
       87     4393                                                /**/
       88     4394    1   DDS$PROMPT: ENTRY;
       89     4395                                                /**/
       90     4396    1        IF S_D.FPRG
       91     4397    2        THEN DO;
       92     4398    2             CALL CONCAT(S_D.UFPROMPT, ' ', SUBSTR(S_D.CURRENT_FPRG_CONTEXT$ ->
              4398                      FPRG$CONTEXT.PROMPT,
       93     4399    2               0, S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.PROMPT_SIZE), '>');
       94     4400    2             S_D.UFPROMPT_SIZE = S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.PROMPT_SIZE
              4400                      + 2;
       95     4401    2           END;
       96     4402    1        IF S_D.MACTIVE
       97     4403    2        THEN DO;
       98     4404    2             IF S_D.MLCT
       99     4405    3             THEN DO;
      100     4406    3                  IF S_D.FPRG
      101     4407    4                  THEN DO;
      102     4408    4                       CALL CONCAT( PROMPT.TEXT, ' ', SUBSTR(S_D.CURRENT_FPRG_CONTEXT$
              4408                                -> FPRG$CONTEXT.PROMPT,
      103     4409    4                         0, S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.PROMPT_SIZE),
              4409                                  @MLCTP, '>');
      104     4410    4                       PRONEW_SIZE = LENGTHC( @MLCTP ) + S_D.CURRENT_FPRG_CONTEXT$ ->
              4410                                FPRG$CONTEXT.PROMPT_SIZE + 2;
PL6.E3A0      #001=DDS$PROMPTC File=DUS$PROMPT.:E05TSI                           TUE 07/29/97 15:00 Page:4    
      105     4411    4                     END;
      106     4412    4                  ELSE DO;
      107     4413    4                       CALL CONCAT( PROMPT.TEXT, @MLCTP, S_D.UPROMPT );
      108     4414    4                       PRONEW_SIZE = LENGTHC( @MLCTP ) + S_D.UPROMPT_SIZE;
      109     4415    4                     END;
      110     4416    3                END;
      111     4417    3             ELSE DO;
      112     4418    3                  IF S_D.FPRG
      113     4419    4                  THEN DO;
      114     4420    4                       CALL CONCAT( PROMPT.TEXT, ' ', SUBSTR(S_D.CURRENT_FPRG_CONTEXT$
              4420                                -> FPRG$CONTEXT.PROMPT,
      115     4421    4                         0, S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.PROMPT_SIZE),
              4421                                  @MACTP, '>');
      116     4422    4                       PRONEW_SIZE = LENGTHC( @MACTP ) + S_D.CURRENT_FPRG_CONTEXT$ ->
              4422                                FPRG$CONTEXT.PROMPT_SIZE + 2;
      117     4423    4                     END;
      118     4424    4                  ELSE DO;
      119     4425    4                       CALL CONCAT( PROMPT.TEXT, @MACTP, S_D.UPROMPT );
      120     4426    4                       PRONEW_SIZE = LENGTHC( @MACTP ) + S_D.UPROMPT_SIZE;
      121     4427    4                     END;
      122     4428    3                END;
      123     4429    2           END;
      124     4430    2        ELSE DO;
      125     4431    2             IF S_D.FPRG
      126     4432    3             THEN DO;
      127     4433    3                  PROMPT.TEXT = S_D.UFPROMPT;
      128     4434    3                  PRONEW_SIZE = S_D.UFPROMPT_SIZE;
      129     4435    3                END;
      130     4436    3             ELSE DO;
      131     4437    3                  PROMPT.TEXT = S_D.UPROMPT;
      132     4438    3                  PRONEW_SIZE = S_D.UPROMPT_SIZE;
      133     4439    3                END;
      134     4440    2           END;
      135     4441        /****GOTO SETIT;****/
      136     4442                                                /**/
      137     4443    1   SETIT: ;
      138     4444    1        PRONEW_SIZE = PRONEW_SIZE + LENGTHC( PROMPT.VFC );
PL6.E3A0      #001=DDS$PROMPTC File=DUS$PROMPT.:E05TSI                           TUE 07/29/97 15:00 Page:5    
      139     4445    1        PROOLD_SIZE = S_D.PROMPT_SIZE;
      140     4446    1        IF S_D.USVFC
      141     4447    2        THEN DO;
      142     4448    2             PROMPT.VFC = %VFC_TRANSITION0;
      143     4449    2             S_D.USVFC = '0'B;
      144     4450    2           END;
      145     4451    1        ELSE PROMPT.VFC = %VFC_EXT;
      146     4452    1        PRONEW$ = ADDR( PROMPT );
      147     4453    1        IF ( PROOLD_SIZE = 0 ) OR
      148     4454    1          ( PRONEW$ -> PROM$NEW ~= ADDR( S_D.PROMPT ) -> PROM$OLD )
      149     4455    2        THEN DO;
      150     4456    2             S_D.PROMPT = PROMPT;
      151     4457    2             S_D.PROMPT_SIZE = PRONEW_SIZE;
      152     4458    2             IF NOT S_D.BATCH
      153     4459    3             THEN DO;
      154     4460    3                  FPTPROMPT = FPT_PROMPT;
      155     4461    3                  FPTPROMPT.V_ = VECTOR( FPTPROMPT.V );
      156     4462    3                  FPTPROMPT.PROMPT_ = VECTOR( PRONEW$ -> PROM$NEW );
      157     4463    3                  CALL M$PROMPT( FPTPROMPT ) ALTRET( ARETP );
      158     4464    4                  DO WHILE ( '0'B );
      159     4465    4   ARETP:              CALL DUE$ERRMSG;
      160     4466    4                     END;
      161     4467    3                END;
      162     4468    2           END;
      163     4469                                                /**/
      164     4470    1        CALL DDQ$READ( ADDR( S_D.INPUT ), LENGTHC( S_D.INPUT ) );
      165     4471    1        S_D.READPOS = S_D.READPOS + 1;
      166     4472                                                /**/
      167     4473    1        RETURN;
      168     4474                                                /**/
      169     4475    1   END DDS$PROMPTC;

