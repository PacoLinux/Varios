        7        7        %MACRO CHK_DUP_OPT(
        8        8              STRUCT=STRUCT,
        9        9              STRUCT_NAME$=STRUCT_NAME$,
       10       10              ERROR="GOTO SKIP_OPTION;");
       11       11              IF STRUCT THEN DO;
       12       12                  CALL VYP$DUP_OPT( STRUCT_NAME$ );
       13       13                  ERROR
       14       14                 END;
       15       15              ELSE STRUCT = @TRUE_B;
