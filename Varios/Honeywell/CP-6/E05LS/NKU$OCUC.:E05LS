PL6.E3A0      #001=NKU$UCCLS File=NKU$OCUC.:E05TSI                               WED 07/30/97 03:45 Page:1    
        1        1        /*M* NKU$OCUC Open and close M$UC - disconnect T.S. lines at logoff*/
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X*DMC,PLM=6,IND=2,ENU=2,DCI=4,CSU=2,ECU=2,THI=2,DTI=0*/
        8        8        /**/
        9        9        /**/
       10       10        NKU$UCCLS: PROC ALTRET;
       11       11        /**/
       12       12        /**/
       13       13        %PLIST;
       14       14        %INCLUDE F$DCB;
       15       63        %INCLUDE NK$LDCT;
       16      165        %INCLUDE NK_LDCT_R;
       17      174        %INCLUDE CP_6_SUBS;
       18      714        %INCLUDE NK_SUBS;
       19      739        %INCLUDE KV$VDO;
       20     1754        %INCLUDE K$RWPARM;
       21     2140        %INCLUDE HF_LOCK_C;
       22     2154        %KV_VDO_E;
       23     2190        /**/
       24     2191        %NK$LDCT;
       25     2237        %K$RWPARM (NAME=K_RWPARM_CON, STCLASS=SYMREF);
       26     2553        /**/
       27     2554        /**/
       28     2555    1   DCL LDCT$ PTR;
       29     2556    1   DCL DONE BIT(1);
       30     2557        %K$RWPARM (NAME=RWPARM,STCLASS=AUTO);
       31     2873    1   DCL KIA$DSCU ENTRY(3) ALTRET;
       32     2874    1   DCL NKL$RELDCTL ENTRY(1) ALTRET;
       33     2875        /**/
       34     2876    1   DCL SC_NTSDCT ENTRY CONV(2,0);
       35     2877        /**/
       36     2878        /**/
       37     2879    1         LDCT$=NK$LDCT$(DCBADDR(%M$UC#)->F$DCB.LDCTX);
PL6.E3A0      #001=NKU$UCCLS File=NKU$OCUC.:E05TSI                               WED 07/30/97 03:45 Page:2    
       38     2880    1         IF LDCT$=ADDR(NIL) THEN CALL SC_NTSDCT;
       39     2881        /*S*  SCREECH_CODE:  NKU-S$NTSDCT
       40     2882              TYPE:          SUA
       41     2883              MESSAGE:       LDCT for timesharing users terminal lost (at disconnect)
       42     2884              REMARKS:       NKU$UCCLS is called by step when running down
       43     2885                             timesharing users.  This SUA occurs when the LDCT
       44     2886                             that should be pointed to by the users M$UC DCB
       45     2887                             does not exist. This has happened in the past because
       46     2888                             the disconnect occured before the user was fully
       47     2889                             started, and this timing hole could reappear.  This
       48     2890                             code is also reported by KIA$UCIO (KIA-S$NTSDCT)
       49     2891                             when the LDCT disappears during I/O                     */
       50     2892    1         IF LDCT$->LDCT.DFLG.OLDSC='0'B THEN ALTRETURN;
       51     2893              %LOCK (G=LDCT$->LDCT.LOCK);
       52     2896    1         RWPARM = K_RWPARM_CON;
       53     2897    1         CALL KIA$DSCU(LDCT$->LDCT,%KV_VDO_FNC_DSC_RSP,RWPARM)
       54     2898    2           WHENALTRETURN DO;
       55     2899    2           LDCT$->LDCT.DFLG.PRLS = '1'B;
       56     2900    2         END;
       57     2901              %UNLOCK (G=LDCT$->LDCT.LOCK);
       58     2904    1         CALL NKL$RELDCTL (LDCT$);
       59     2905    1         DCBADDR(%M$UC#)->F$DCB.LDCTX = 0;
       60     2906    1         DCBADDR(%M$UC#)->F$DCB.SETX = 0;
       61     2907    1         RETURN;
       62     2908        /**/
       63     2909    1   END;
       64     2910        /**/
       65     2911        /**/
       66     2912        %EOD;

