        8        8        %SET LISTSUB='1'B;
        9        9                                                /**/
       10       10                                                /**/
       11       11        /*M* OUT$TEMPLAT entry point for creating templates */
       12       12        /*
       13       13         ###   #   #  #####   #   #####  #####  #   #  ####   #       ###   #####
       14       14        # # #  #   #    #    ###    #    #      ## ##  #   #  #      #   #    #
