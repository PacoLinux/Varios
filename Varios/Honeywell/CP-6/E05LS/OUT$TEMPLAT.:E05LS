       17       17        #   #  #   #    #     # #   #    #      #   #  #      #      #   #    #
       18       18        #   #  #   #    #    ###    #    #      #   #  #      #      #   #    #
       19       19         ###    ###     #     #     #    #####  #   #  #      #####  #   #    #
       20       20                                                                          */
       21       21                                                /**/
       22       22        OUT$TEMPLAT: PROC;
       23       23                                                /**/
       24       24                                                /**/
       25       25                                           /*
       26       26                                                SYMREFS
       27       27                                                                          */
       28       28    1   DCL ZWI$M$NS# UBIN SYMREF;
       29       29    1   DCL OUT$TOPNODE UBIN SYMREF;
       30       30    1   DCL B$TCB$ PTR SYMREF;
       31       31    1   DCL OUX_BUF CHAR(300) SYMREF;
       32       32    1   DCL OUX_CSIZE SBIN SYMREF;
       33       33    1   DCL ZWI$RECNO UBIN SYMREF;
       34       34    1   DCL 1 ZWI$INPUT_KEY ALIGNED SYMREF,
       35       35    1            2 TCHAR UBIN(8) UNAL,
       36       36    1            2 EDIT_VAL SBIN(28) UNAL;
       37       37    1   DCL ZWH$ERR ENTRY(11);
PL6.E3A0      #001=OUT$TEMPLAT File=OUT$TEMPLAT.:E05TSI                          WED 07/30/97 04:15 Page:2    
       38       38    1   DCL OUT$BUILD ENTRY(1) ALTRET;
       39       39                                                /**/
       40       40                                           /*
       41       41                                                INCLUDES
       42       42                                                                          */
       43       43        %INCLUDE OUX_ENTRY;
       44       61        %INCLUDE XU_PERR_C;
       45      252        %INCLUDE OUT_DATA_R;
       46      267        %INCLUDE OU_PERR_C;
       47      288        %INCLUDE CP_6;
       48      369                                                /**/
       49      370             %FPT_GDS       (FPTN=GET_COMMON_PAGES,
       50      371                                 COMMON=YES,
       51      372                                 RESULTS=OUX_VECTOR,
       52      373                                 SEGSIZE=16384);
       53      389                                                /**/
       54      390                  %VLP_VECTOR (FPTN=OUX_VECTOR);
       55      405                                                /**/
       56      406
       57      407                                                /**/
       58      408                                           /*
       59      409                                                LOCAL STORAGE
       60      410                                                                          */
       61      411    1   DCL I SBIN WORD STATIC;
       62      412    1   DCL J SBIN WORD STATIC;
       63      413    1   DCL K SBIN WORD STATIC;
       64      414    1   DCL TOPNODE$ STATIC PTR;
       65      415    1   DCL ERROR_CODE REDEF TOPNODE$ UBIN WORD;
       66      416    1   DCL 1 ERROR_CODED REDEF TOPNODE$,
       67      417    1            2 MID BIT(12) UNAL,
       68      418    1            2 FCG BIT(6) UNAL,
       69      419    1            2 MON BIT(1) UNAL,
       70      420    1            2 CODE UBIN(14) UNAL,
       71      421    1            2 SEV UBIN(3) UNAL;
       72      422    1   DCL STRING CHAR(140) STATIC UNAL;
       73      423                                                /**/
       74      424                                           /*
PL6.E3A0      #001=OUT$TEMPLAT File=OUT$TEMPLAT.:E05TSI                          WED 07/30/97 04:15 Page:3    
       75      425                                                LOCAL CONSTANTS
       76      426                                                                          */
       77      427    1   DCL XUP_MID BIT(12) CONSTANT INIT('3025'O);
       78      428    1   DCL XUP_FCG BIT(6) CONSTANT INIT('20'O);
       79      429        %EJECT;
PL6.E3A0      #001=OUT$TEMPLAT File=OUT$TEMPLAT.:E05TSI                          WED 07/30/97 04:15 Page:4    
       80      430        /*****************************************************************
       81      431
       82      432             Begin     OUT$TEMPLAT        main
       83      433
       84      434        *******************************************************************/
       85      435                                                /**/
       86      436                                                /**/
       87      437    1        CALL M$GDS (GET_COMMON_PAGES) ALTRET (ALT_ERROR);
       88      438                                                /**/
       89      439    1        CALL OUX$SETMEM (OUX_VECTOR) ALTRET (ALT_INT_ERROR);
       90      440                                                /**/
       91      441    1        CALL OUX$SETDCBS (ZWI$M$NS#, DCBNUM(NIL));
       92      442                                                /**/
       93      443    2        DO WHILE('1'B);
       94      444                                                /**/
       95      445    2   TRY_TRY_AGAIN: ;
       96      446    2             CALL OUX$GETCMD (OUT$TOPNODE, TOPNODE$,
       97      447    2                  ,                        /* NIL PROMPT */
       98      448    2                  ,                        /* NIL TEXT */
       99      449    2                  ,                        /* NIL TSIZ */
      100      450    2                  )                        /* NIL ROUTINE */
      101      451    2                  ALTRET (ALT_PARSE);
      102      452                                                /**/
      103      453    2             CALL OUT$BUILD (TOPNODE$) ALTRET (ALT_BUILD);
      104      454    2           END;                            /* DO WHILE */
      105      455                                                /**/
      106      456                                                /**/
      107      457                                                /**/
      108      458    1   ALT_PARSE: ;
      109      459    1        IF ERROR_CODED.MID = XUP_MID
      110      460    1        THEN
      111      461    2        DO;
      112      462    3             DO CASE(ERROR_CODED.CODE);
      113      463                                                /**/
      114      464    3             CASE (%E$SYNERR);
      115      465    3                CALL OUX$ERRPTR ( ,DCBNUM(NIL)); /* Use PCB.HI_CHAR */
      116      466                                                /* return %U1 */
PL6.E3A0      #001=OUT$TEMPLAT File=OUT$TEMPLAT.:E05TSI                          WED 07/30/97 04:15 Page:5    
      117      467                                                /**/
      118      468        /*E*      ERROR: OUT-E$OUTSYNERR-3
      119      469                  MESSAGE: Syntax error at %U1.
      120      470                                                                          */
      121      471
      122      472    3                OUT_RECNO = ZWI$RECNO;
      123      473    3                OUT_INPUT_KEY = ZWI$INPUT_KEY;
      124      474    3                OUT_MIDD.MID = '24'O /* T_MID# */;
      125      475    3                OUT_MIDD.FCG = '1725'O /* OU_FCG# */;
      126      476    3                OUT_ERROR_CODE = %E$OUTSYNERR;
      127      477    3                OUT_SEV = 3;
      128      478    3                OUT_ERR_CNT = OUX_CSIZE;
      129      479    3                OUT_ERR_STRING = SUBSTR(OUX_BUF,0,OUX_CSIZE);
      130      480                                                /**/
      131      481    3                CALL ZWH$ERR (OUT_RECNO,
      132      482    3                     OUT_INPUT_KEY,
      133      483    3                     OUT_MID,
      134      484    3                     OUT_ERROR_CODE,
      135      485    3                     OUT_SEV,
      136      486    3                     OUT_ERR_CNT,
      137      487    3                     OUT_ERR_STRING);
      138      488    3             CASE (ELSE);
      139      489    3             END;                          /* DO CASE ERROR CODE */
      140      490    2           END;                            /* DO IF XUP ERROR */
      141      491    1        IF ERROR_CODED.MID ~= XUP_MID      /* TEMPORARY ONLY! */
      142      492    1        THEN                               /* TEMPORARY ONLY! */
      143      493    1             RETURN;
      144      494    1        GOTO TRY_TRY_AGAIN;
      145      495                                                /**/
      146      496    1   ALT_BUILD: ;                            /* COME HERE IF BAD BUILD */
      147      497    1        OUT_RECNO = ZWI$RECNO;
      148      498    1        OUT_INPUT_KEY = ZWI$INPUT_KEY;
      149      499    1        OUT_MIDD.MID = '24'O /* T_MID# */;
      150      500    1        OUT_MIDD.FCG = '1725'O /* OU_FCG# */;
      151      501    1        OUT_ERROR_CODE = OUT_ERRORED;
      152      502    1        OUT_SEV = 3;
      153      503                                                /**/
PL6.E3A0      #001=OUT$TEMPLAT File=OUT$TEMPLAT.:E05TSI                          WED 07/30/97 04:15 Page:6    
      154      504    1        CALL ZWH$ERR (OUT_RECNO,
      155      505    1             OUT_INPUT_KEY,
      156      506    1             OUT_MID,
      157      507    1             OUT_ERROR_CODE,
      158      508    1             OUT_SEV);
      159      509    1        GOTO TRY_TRY_AGAIN;
      160      510
      161      511    1   ALT_INT_ERROR: ;                        /* COME HERE IF INTERNAL ERROR */
      162      512    1   ALT_ERROR: ;                            /* COME HERE IF NO PAGES */
      163      513    1        RETURN;                            /* GO BACK FOR NOW */
      164      514    1   END OUT$TEMPLAT;
      165      515        %EOD;

