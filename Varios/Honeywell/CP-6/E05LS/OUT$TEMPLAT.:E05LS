      575     3399
      576     3400    2        DO CASE ( PTR$ -> OU$OUTBLK.CODE);
      577     3401
      578     3402    2        CASE (71 /* T_NUMSEL# */);
      579     3403        /*E*      ERROR: OUT-E$OUT_BADTYP
      580     3404                  MESSAGE: Selection criteria does not match data type of selector.
      581     3405                           Text was requested and numeric was specified, or
      582     3406                           numeric was requested and text was specified.
      583     3407                                                                          */
      584     3408    2           IF TYPE_IND ~= 1 /* NUMERIC */
      585     3409    2           THEN
      586     3410    2                CALL OUT$ERROR (%E$OUT_BADTYP);
      587     3411
      588     3412    2           CALL OUT$MAKESEL (PTR$ -> OU$OUTBLK.SUBLK$(0));
      589     3413
      590     3414    2        CASE (73 /* T_NUMSELLIST# */);
      591     3415
      592     3416    2           IF TYPE_IND ~= 1 /* NUMERIC */
      593     3417    2           THEN
      594     3418    2                CALL OUT$ERROR (%E$OUT_BADTYP);
      595     3419
      596     3420    3           DO I = 0 TO PTR$ -> OU$OUTBLK.NSUBLKS - 1;
      597     3421
      598     3422    3                CALL OUT$MAKESEL (PTR$ -> OU$OUTBLK.SUBLK$(I));
      599     3423    3              END;                         /* DO I */
      600     3424
      601     3425    2        CASE (74 /* T_TEXTSEL# */);
      602     3426
      603     3427    2           IF TYPE_IND ~= 2 /* TEXTYPE */
      604     3428    2           THEN
PL6.E3A0      #006=OUT$CHLIST File=OUT$TEMPLAT.:E05TSI                           WED 07/30/97 04:16 Page:95   
      605     3429    2                CALL OUT$ERROR (%E$OUT_BADTYP);
      606     3430
      607     3431    2           CALL OUT$MAKESEL (PTR$ -> OU$OUTBLK.SUBLK$(0));
      608     3432
      609     3433    2        CASE (76 /* T_TEXTSELLIST# */);
      610     3434
      611     3435    2           IF TYPE_IND ~= 2 /* TEXTYPE */
      612     3436    2           THEN
      613     3437    2                CALL OUT$ERROR (%E$OUT_BADTYP);
      614     3438
      615     3439    3           DO I = 0 TO PTR$ -> OU$OUTBLK.NSUBLKS - 1;
      616     3440
      617     3441    3                CALL OUT$MAKESEL (PTR$ -> OU$OUTBLK.SUBLK$(I));
      618     3442
      619     3443    3              END;                         /* DO I = 0 TO NSUBLKS */
      620     3444    2        CASE (78 /* T_ELSESEL# */);
      621     3445    2           CALL OUT$ADDBYTE(8 /* TYP_OTHERWISE# */);
