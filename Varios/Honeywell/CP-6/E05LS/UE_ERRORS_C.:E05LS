   12      %EQU E$BADPTR=494 /* E$BADPTR */; /*BAD PTR SPECIFIED ON TRTN OR RETRY PMME-756*/
   13      %EQU E$NOEVENT=495/* E$NOEVENT */;      /* NO EVENT PROCEDURE FOR THIS EVENT  */
   14      %EQU E$NOTPMME=496 /* E$NOTPMME */;     /*TOP TCB FRAME NOT A PMME FRAME-760*/
