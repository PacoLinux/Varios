    8      %EQU E$NOTCB=490;   /*PMME not allowed if program has no TCB */
    9      %EQU E$DLTALT=491 /*E$DLTALT*/;         /*DELTA ALTRETED FROM M$ALIB             */
   10      %EQU E$DRTN=492 /* E$DRTN */;           /*DELTA M$DRTN WITH NO USER FRAME-754*/
