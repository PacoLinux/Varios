        1        1        /*M* DDS$SDO EVALUATE "SIMBLE", "DECIMAL", "OCTIL", OR "CNR1C" NODES */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MOC,DCI=5,CRT=5 */
        8        8        %SET LISTSUB='1'B;
        9        9        /*F* NAME: DDS$SDO
       10       10             PURPOSE: TO EVALUATE "SIMBLE", "DECIMALS", "OCTIL", OR "CNR1C" NODES.
       11       11             DESCRIPTION: DETERMINES WHICH NODE IT IS, THEN CALLS THE APPROPRIATE
       12       12             ROUTINE TO EVALUATE IT. THE NODES "SIMBLE" AND "CNR1C" ARE
       13       13             EVALUATED INLINE. */
       14       14        /*D* NAME: DDS$SDO
       15       15             CALL: CALL DDS$SDO( NODE$, V ) ALTRET( ERROR );
       16       16             INTERFACE: DDS$UD_SRCH
       17       17                        DDS$POS_SP
       18       18                        DDS$ED_SRCH
       19       19                        DDS$SD_SRCH
       20       20             ENVIRONMENT:
       21       21             INPUT: N$ - POINTER TO NODE.
       22       22                    S_D.SIMBLEOK - WHETHER SIMBLES ARE ALLOWED
       23       23                    S_D.SDOSDEF - WHETHER TO SEARCH SYMDEFs BEFORE ENTDEFs
       24       24             OUTPUT: V - VALUE OUTPUT.
       25       25                     S_D.SDOS - WHETHER SIMBLE WAS ENCOUNTERED
       26       26                     S_D.SDOD - WHETHER DECIMAL WAS ENCOUNTERED
       27       27                     S_D.SDOO - WHETHER OCTAL WAS ENCOUNTERED
       28       28                     S_D.SDOB - WHETHER BIT-STRING WAS ENCOUNTERED
       29       29                     S_D.SDOOB - WHETHER OCTAL BIT-STRING WAS ENCOUNTERED
       30       30                     S_D.SDOT - WHETHER TEXT-STRING WAS ENCOUNTERED
       31       31             SCRATCH: LOCAL AUTO ONLY.
       32       32             DESCRIPTION: DETERMINES WHICH NODE IT IS, THEN CALLS THE APPROPRIATE
       33       33             ROUTINE TO EVALUATE IT. RETURNS UNLESS AN ERROR CONDITION ENCOUNTERED,
       34       34             THEN ALTRETURNS. */
       35       35        /**/
       36       36        DDS$SDO: PROC( NODE$, V ) ALTRET;
       37       37        /**/
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:2    
       38       38    1   DCL NODE$ PTR;                          /* POINTER TO NODE INPUT */
       39       39    1   DCL V UBIN;                             /* VALUE OUTPUT */
       40       40    1   DCL T REDEF V CHAR(4);
       41       41    1   DCL VBIT (0:35) REDEF V UBIN(1) UNAL;
       42       42    1   DCL VOCT (0:11) REDEF V UBIN(3) UNAL;
       43       43    1   DCL VHEX (0:8)  REDEF V UBIN(4) UNAL;
       44       44    1   DCL 1 L6BIT REDEF V,
       45       45    1         2 * BIT(4) UNAL,
       46       46    1         2 X (0:31) UBIN(1) UNAL;
       47       47    1   DCL 1 L6OCT REDEF V,
       48       48    1         2 * BIT(4) UNAL,
       49       49    1         2 X (0:9) UBIN(3) UNAL,
       50       50    1         2 * BIT(2) UNAL;
       51       51    1   DCL 1 L6HEX REDEF V,
       52       52    1         2 * BIT(4) UNAL,
       53       53    1         2 X (0:7) UBIN(4) UNAL;
       54       54    1   DCL 1 L6TXT REDEF V,
       55       55    1         2 * BIT(4) UNAL,
       56       56    1         2 X (0:3) UBIN(8) UNAL;
       57       57    1   DCL N$ PTR;                             /* TEMP FOR CURRENT NODE */
       58       58    1   DCL NCD SBIN;                           /* CURRENT NODE CODE */
       59       59    1   DCL SYM$ PTR;                           /* SYMBOL TEXTC POINTER */
       60       60    1   DCL ENTRY$ PTR;                         /* EDEF/SYMDEF POINTER */
       61       61    1   DCL ERROR BIT(1) ALIGNED;
       62       62    1   DCL IX UBIN;
       63       63    1   DCL TSIZ UBIN;
       64       64    1   DCL ACCUM UBIN;
       65       65    1   DCL TEMP UBIN;
       66       66    1   DCL TCHARS CHAR(4);
       67       67    1   DCL BCHARS(0:3) REDEF TCHARS UBIN BYTE CALIGNED;
       68       68    1   DCL C CHAR(1);
       69       69    1   DCL CHAR$1 CHAR(1) BASED UNAL;
       70       70    1   DCL SCHARS (0:0) CHAR(1) CALIGNED BASED (SYM$);
       71       71    1   DCL SSTR CHAR(TSIZ) CALIGNED BASED (SYM$);
       72       72    1   DCL XCHARS CHAR(16) CONSTANT INIT('0123456789ABCDEF');
       73       73
       74       74        /* Use ASCII character value as an index to get an EBCDIC character
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:3    
       75       75           value.
       76       76           For the ASCII character value -
       77       77               only use 8 bits to distinguish an ASCII character, the 9th bit
       78       78               can be either '0'B or '1'B.
       79       79           For the EBCDIC character value -
       80       80               the 8th bit is always '0'B. */
       81       81
       82       82    1   DCL 1 ASC2EBC CONSTANT DALIGNED,
       83       83    1         2 C(0:511) UBIN BYTE UNAL INIT(
       84       84        /*Most sig-       Least significant digits         */
       85       85        /*nificant  000  001  010  011  100  101  110  111 */
       86       86    1   /* 00000 */   0,   1,   2,   3,  55,  45,  46,  47,
