      269    12008    5                                           CALL INDEX (TEMP, SCHARS(IX), '01234567')
      270    12009    5                                             ALTRET (BADDIG);
      271    12010    5                                           L6OCT.X(IX) = TEMP;
      272    12011    5                                         END;
      273    12012    4                                   CASE ( ELSE );
      274    12013    4                                      S_D.ERROR = 502; /* E@DELBUG */
      275    12014    4                                      GOTO ALTRT;
      276    12015    4                                   END;
      277    12016    3                              CASE ( 472 ); /* C@FSPECC - CHARACTER STRING */
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:9    
      278    12017    3                                 S_D.SDOT = '1'B;
      279    12018    3                                 CALL INSERT (TCHARS,,, SSTR) ALTRET (CONSTOOBIG);
      280    12019    4                                 DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
      281    12020    4                                   CASE ( %HOST );
      282    12021    4                                      T = TCHARS;
      283    12022    4                                   CASE ( %L6 );
      284    12023    4                                      L6TXT.X (0) = BCHARS (0);
      285    12024    4                                      L6TXT.X (1) = BCHARS (1);
      286    12025    4                                      L6TXT.X (2) = BCHARS (2);
      287    12026    4                                      L6TXT.X (3) = BCHARS (3);
      288    12027    4                                   CASE ( ELSE );
      289    12028    4                                      S_D.ERROR = 502; /* E@DELBUG */
      290    12029    4                                      GOTO ALTRT;
      291    12030    4                                   END;
      292    12031    3                              CASE ( 474 ); /* C@FSPECX - HEX STRING */
