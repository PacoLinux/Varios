      164    11903    1   DCL DDS$POS_SP ENTRY(3) ALTRET;         /* EVALUATES SPECIAL SYMBOLS */
      165    11904    1   DCL DDS$UD_SRCH ENTRY(2) ALTRET;        /* EVALUATES USER DEFINED SYMBOLS */
      166    11905    1   DCL DDS$ED_SRCH ENTRY(2) ALTRET;        /* EVALUATES ENTDEFS */
      167    11906    1   DCL DDS$SD_SRCH ENTRY(2) ALTRET;        /* EVALUATES SYMDEFS */
      168    11907        %EJECT;
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:6    
      169    11908        /**/
      170    11909    1        N$ = NODE$ -> POB.SUBLK$( 0 );
      171    11910    1        NCD = N$ -> POB.CODE;
      172    11911    1        IF ( NCD = 14 ) OR ( NCD = 15 ) /* SYM_CODE | QTSTRING_CODE */
      173    11912    2        THEN DO;                           /* "SIMBLE" */
      174    11913    2             SYM$ = N$ -> POB.SUBLK$( 0 ); /* POINT TO "SIMBLE" TEXT */
      175    11914    2             IF NOT S_D.SIMBLEOK
      176    11915    3             THEN DO;                      /* SIMBLE NOT ALLOWED HERE */
      177    11916    3                  S_D.ERROR = 51; /* E@NOSYMBHERE */
      178    11917    3                  GOTO ARETSYM;
      179    11918    3                END;
      180    11919    2             ELSE ;
      181    11920    2             S_D.SDOS = '1'B;
      182    11921    2             CALL DDS$UD_SRCH( SYM$, V ) ALTRET( RETN );
      183    11922    2             IF NCD = 14             /* SYM_CODE */
      184    11923    3             THEN DO;
      185    11924    3                  CALL DDS$POS_SP( SYM$, V, ERROR ) ALTRET( RETN );
      186    11925    3                  IF ERROR
      187    11926    3                  THEN GOTO ARETSYM;
      188    11927    3                END;
      189    11928    2             ELSE ;              /* BYPASS SPECIAL SYMBOL CHECK IF QTSTRINGS */
      190    11929    2             IF S_D.SDOSDEF
      191    11930    3             THEN DO;                      /* SEARCH SYMDEFs FIRST */
      192    11931    3                  CALL DDS$SD_SRCH( SYM$, ENTRY$ ) ALTRET( SD_FOUND );
      193    11932    3                  CALL DDS$ED_SRCH( SYM$, ENTRY$ ) ALTRET( ED_FOUND );
      194    11933    3                END;
      195    11934    3             ELSE DO;                      /* SEARCH ENTDEFs FIRST */
      196    11935    3                  CALL DDS$ED_SRCH( SYM$, ENTRY$ ) ALTRET( ED_FOUND );
      197    11936    3                  CALL DDS$SD_SRCH( SYM$, ENTRY$ ) ALTRET( SD_FOUND );
      198    11937    3                END;
      199    11938                                                /* CAN'T RESOLVE SYMBOL */
      200    11939    2             S_D.ERROR = 3;   /* E@SYM_NOTFOUND */
      201    11940    2   ARETSYM:
      202    11941    2             S_D.ERROR_SYM$ = SYM$;
      203    11942    2             GOTO ALTRT;
      204    11943    2   ED_FOUND:
      205    11944    2             V = ENTRY$ -> B$EDEF.ISADDR;
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:7    
      206    11945    2             GOTO RETN;
      207    11946    2   SD_FOUND:
      208    11947    2             IF ENTRY$ -> B$SDEF.CONSTNT
      209    11948    2             THEN V = ENTRY$ -> B$SDEF.VALUE;
      210    11949    2             ELSE V = ENTRY$ -> B$SDEF.AD.ISADDR;
      211    11950    2             GOTO RETN;
      212    11951    2           END;                            /* "SIMBLE" */
      213    11952    1        ELSE IF NCD = 27             /* OCT_CODE */
      214    11953    2             THEN DO;                      /* "OCTIL" */
      215    11954    2                  S_D.SDOO = '1'B;
      216    11955    2                  GOTO OCTAL;
      217    11956    2                END;                       /* "OCTIL" */
      218    11957    1             ELSE IF NCD = 8        /* DEC_CODE */
      219    11958    2                  THEN DO;                 /* "DECIMAL" */
      220    11959    2                       S_D.SDOD = '1'B;
      221    11960    2                       N$ = N$ -> POB.SUBLK$( 0 );
      222    11961    2                       GOTO DECIMAL;
      223    11962    2                     END;                  /* "DECIMAL" */
      224    11963    1                  ELSE IF NCD = 200  /* CNR1_CODE */
      225    11964    2                       THEN DO;            /* "CNR1C" */
      226    11965    2                            NCD = 472 ; /* C@SPECC - DEFAULT IS CHARACTER STRING */
      227    11966    2                            IF ( N$ -> POB.NSB = 2 )
      228    11967    2                            THEN NCD = N$ -> POB.SUBLK$( 1 ) -> POB.CODE;
      229    11968    2                            SYM$ = ADDR (N$ -> POB.SUBLK$(0) -> POS.TEXT);
             11968                                     /* QUOTE STRING TEXT POINTER */
      230    11969    2                            TSIZ = N$ -> POB.SUBLK$(0) -> POS.COUNT;
      231    11970    2                            V = 0;
      232    11971    3                            DO CASE( NCD );
      233    11972    3                              CASE ( 470 ); /* C@FSPECB - BIT STRING */
      234    11973    3                                 S_D.SDOB = '1'B;
      235    11974    4                                 DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
      236    11975    4                                   CASE ( %HOST );
      237    11976    4                                      IF ( TSIZ > LENGTHB( V ) ) THEN GOTO CONSTOOBIG;
      238    11977    5                                      DO IX = 0 TO TSIZ-1;
      239    11978    5                                           CALL INDEX (TEMP, SCHARS(IX), '01')
      240    11979    5                                             ALTRET (BADDIG);
      241    11980    5                                           VBIT(IX) = TEMP;
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:8    
      242    11981    5                                         END; /* "DO IX" */
