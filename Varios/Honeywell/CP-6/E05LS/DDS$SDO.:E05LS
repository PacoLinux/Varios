      244    11983    4                                      IF  TSIZ > LENGTHB( L6BIT.X )
      245    11984    4                                      THEN GOTO CONSTOOBIG;
      246    11985    5                                      DO IX = 0 TO TSIZ-1;
      247    11986    5                                           CALL INDEX (TEMP, SCHARS(IX), '01')
      248    11987    5                                             ALTRET (BADDIG);
      249    11988    5                                           L6BIT.X(IX) = TEMP;
      250    11989    5                                         END;
      251    11990    4                                   CASE ( ELSE );
      252    11991    4                                      S_D.ERROR = 502; /* E@DELBUG */
      253    11992    4                                      GOTO ALTRT;
      254    11993    4                                   END;
      255    11994    3                              CASE ( 471 ); /* C@FSPECO - OCTAL BIT STRING */
      256    11995    3                                 S_D.SDOOB = '1'B;
      257    11996    4                                 DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
