      307    12046    5                                             ALTRET (BADDIG);
      308    12047    5                                           L6HEX.X (IX) = TEMP;
      309    12048    5                                         END;
      310    12049    4                                   CASE ( ELSE );
      311    12050    4                                      S_D.ERROR = 502; /* E@DELBUG */
      312    12051    4                                      GOTO ALTRT;
      313    12052    4                                   END;
      314    12053    3                              CASE ( 475 ); /* C@FSPECEB - EBCDIC CHARACTER */
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:10   
      315    12054    3                                 CALL XLATE (TCHARS, ASC2EBC, SSTR) ALTRET (CONSTOOBIG)
             12054                                          ;
      316    12055    4                                 DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
      317    12056    4                                   CASE ( %HOST );
      318    12057    4                                      T = TCHARS;
      319    12058    4                                   CASE ( %L6 );
      320    12059    4                                      L6TXT.X (0) = BCHARS (0);
      321    12060    4                                      L6TXT.X (1) = BCHARS (1);
      322    12061    4                                      L6TXT.X (2) = BCHARS (2);
      323    12062    4                                      L6TXT.X (3) = BCHARS (3);
      324    12063    4                                   END;
      325    12064    3                              END;         /* "DO CASE( NCD )" */
      326    12065    2                          END;             /* "CNR1C" */
      327    12066    1                       ELSE IF NCD = 104 /* HEX_CODE */
      328    12067    2                            THEN DO;       /* "HEXIL" */
      329    12068    2                                 S_D.SDOH = '1'B;
      330    12069    2                                 GOTO HEX;
      331    12070    2                               END;
      332    12071    2                            ELSE DO;       /* UNKNOWN CODE */
      333    12072    2                                 S_D.ERROR = 501; /* E@DERR1 */
      334    12073    2                                 GOTO ALTRT;
      335    12074    2                               END;        /* UNKNOWN CODE */
      336    12075        /**/
      337    12076    1        RETURN;
      338    12077        %EJECT;
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:11   
      339    12078        /**/
      340    12079    1   DDS$DECIMAL: ENTRY( NODE$, V ) ALTRET;
      341    12080        /**/
      342    12081    1        IF NODE$ -> POB.CODE ~= 8   /* DEC_CODE */
      343    12082    1        THEN GOTO NOTDECERR;
      344    12083    1        N$ = NODE$ -> POB.SUBLK$( 0 );
      345    12084    1   DECIMAL: ;
      346    12085    1        TSIZ = N$ -> TEXT$C.SIZE;
      347    12086    2        DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
      348    12087    2          CASE ( %HOST );
      349    12088    2             IF TSIZ > LENGTHC( '68719476735' ) THEN GOTO TOOLARGE;
      350    12089    2             IF TSIZ = LENGTHC( '68719476735' ) AND
      351    12090    2               N$ -> TEXT$C.TXT > '68719476735'
      352    12091    2             THEN GOTO TOOLARGE;
      353    12092    2          CASE ( %L6 );
      354    12093    2             IF TSIZ > LENGTHC( '4294967295' ) THEN GOTO TOOLARGE;
      355    12094    2             IF TSIZ = LENGTHC( '4294967295' ) AND
      356    12095    2               N$ -> TEXT$C.TXT > '4294967295'
      357    12096    2             THEN GOTO TOOLARGE;
      358    12097    2          CASE ( ELSE );
      359    12098    2             S_D.ERROR = 502;         /* E@DELBUG */
      360    12099    2             GOTO ALTRT;
      361    12100    2          END;
      362    12101    1        CALL CHARBIN( V, N$ -> TEXT$C.TXT );
      363    12102    1        RETURN;
      364    12103        %EJECT;
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:12   
      365    12104        /**/
      366    12105    1   DDS$OCTAL: ENTRY( NODE$, V ) ALTRET;
      367    12106        /**/
      368    12107    1        IF NODE$ -> POB.CODE ~= 27   /* OCT_CODE */
      369    12108    1        THEN GOTO NOTOCTERR;
      370    12109    1        N$ = NODE$;
      371    12110    1   OCTAL: ;
      372    12111    1        IF N$ -> POB.NDTYPE = %KONCAT#
      373    12112    1        THEN N$ = N$ -> POB.SUBLK$( 0 );
      374    12113    1        N$ = N$ -> POB.SUBLK$( 0 );
      375    12114    1        TSIZ = N$ -> TEXT$C.SIZE;
      376    12115    2        DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
      377    12116    2          CASE ( %HOST );
      378    12117    2             IF TSIZ > 12 THEN GOTO TOOLARGE;
      379    12118    2          CASE ( %L6 );
      380    12119    2             IF TSIZ > 10 THEN GOTO TOOLARGE;
      381    12120    2          CASE ( ELSE );
      382    12121    2             S_D.ERROR = 502;
      383    12122    2             GOTO ALTRT;
      384    12123    2          END;
      385    12124        /* PICK UP EACH DIGIT LEFT TO RIGHT & ACCUMULATE INTO SUM */
      386    12125    1        ACCUM = 0;
      387    12126    2        DO IX = 1 TO TSIZ;
      388    12127    2             N$ = PINCRC( N$, LENGTHC( CHAR$1 ) );
      389    12128    2             ACCUM = ( ACCUM * 8 ) + ASCBIN( N$ -> CHAR$1 ) - ASCBIN( '0' );
      390    12129    2           END;                            /* "DO IX" */
      391    12130    1        V = ACCUM;
      392    12131    1        RETURN;
      393    12132        %EJECT;
PL6.E3A0      #001=DDS$SDO File=DDS$SDO.:E05TSI                                  TUE 07/29/97 14:21 Page:13   
      394    12133        /**/
      395    12134    1   DDS$HEX: ENTRY(NODE$, V) ALTRET;
      396    12135        /**/
      397    12136    1        IF NODE$ -> POB.CODE ~= 104   /* HEX_CODE */
      398    12137    1        THEN GOTO NOTHEXERR;
      399    12138    1        N$ = NODE$;
      400    12139    1   HEX: ;
      401    12140    1        IF N$ -> POB.NDTYPE = %KONCAT#
      402    12141    1        THEN N$ = N$ -> POB.SUBLK$(0);
      403    12142    1        N$ = N$ -> POB.SUBLK$(0);
      404    12143    1        TSIZ = N$ -> TEXT$C.SIZE;
      405    12144    1        IF TSIZ > 8
      406    12145    1        THEN GOTO TOOLARGE;
      407    12146                            /* Pick up each digit left to right and accumulate in sum */
      408    12147    1        ACCUM = 0;
      409    12148    2        DO IX = 1 TO TSIZ;
      410    12149    2             N$ = PINCRC(N$, LENGTHC(CHAR$1));
      411    12150    2             CALL INDEX(TEMP, N$ -> CHAR$1, XCHARS) ALTRET(BADDIG);
      412    12151    2             ACCUM = ACCUM*16 + TEMP;
      413    12152    2           END;
      414    12153    1        V = ACCUM;
      415    12154    1        RETURN;
      416    12155        /**/
      417    12156    1   RETN: RETURN;
      418    12157        /**/
      419    12158    1   CONSTOOBIG: S_D.ERROR = 64; GOTO ALTRT; /* E@CONSTOOBIG */
      420    12159    1   NOTDECERR: S_D.ERROR = 511; GOTO ALTRT; /* E@NOTDECIMAL */
      421    12160    1   NOTOCTERR: S_D.ERROR = 603; GOTO ALTRT; /* E@NOTOCTIL */
      422    12161    1   NOTHEXERR: S_D.ERROR = 604; GOTO ALTRT; /* E@NOTHEX */
      423    12162    1   TOOLARGE: S_D.ERROR = 46; GOTO ALTRT; /* E@TOOLARGE */
      424    12163    1   BADDIG: S_D.ERROR = 65;           /* E@BADDIG */
      425    12164    1   ALTRT: ALTRETURN;
      426    12165        /**/
      427    12166    1   END DDS$SDO;

