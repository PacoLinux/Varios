       90    12155    2                                                   ALTRET ( RET ) ;
       91    12156    2               IF  DCT.TYPE = %DV_DP
       92    12157    3               THEN DO ;
       93    12158    3                   CALL NMO$DCT ( 0, I )           ALTRET ( DCTERR ) ;
       94    12159    3                   CALL LIST ( NMD_PIT.DEV.DCT$(0) ) ;
       95    12160    3                   END ;
       96    12161    2               ELSE
       97    12162    2                   ;
       98    12163    2   DCTERR:     END ;
       99    12164
      100    12165    2           ELSE DO  I = 0 TO OUT.NSUBLKS-1 ;
      101    12166    2               CALL NMO$DEV ( 0, OUT.SUBLK$(I) )   ALTRET ( DEVERR ) ;
      102    12167    2               CALL LIST ( NMD_PIT.DEV.DCT$(0) ) ;
      103    12168    2   DEVERR:     END ;
      104    12169        /*
      105    12170        Return to the calling routine.
      106    12171        */
      107    12172    1   RET:    RETURN ;
