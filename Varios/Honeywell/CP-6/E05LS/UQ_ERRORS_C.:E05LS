    7                                              /***********************************/
    8                                              /*       ENQ/DEQ ERROR CODES          */
    9                                              /***********************************/
   10      /*                                                                  */
   11      /*                                                                  */
   12      /*                                                                  */
   13                                              /* ENQ/DEQ OWNS CODES 800-824         */
   14      %EQU E$DSFULL=800 /* E$DSFULL */;       /* ENQ/DEQ OUT OF DATA SPACE          */
   15      %EQU E$DLOCK=801 /* E$DLOCK */;         /* ENQ/DEQ DEADLOCK DETECTED          */
   16      %EQU E$TIMELIMIT=802 /* E$TIMELIMIT */; /* ENQ/DEQ WAIT TIME LIMIT EXCEEDED   */
   17      %EQU E$NOEADDR=803 /* E$NOEADDR */;     /* REQUIRED EVENT ADDRESS MISSING     */
   18      %EQU E$CRD_ERR=804 /* E$CRD_ERR */; /* TYPE OF DEQ INCOMPATIBLE WITH STATE    */
   19      %EQU E$PDOMAIN=805 /* E$PDOMAIN */; /* PREVIOUSLY REQUESTED BY ANOTHER DOMAIN */
   20      %EQU E$MISSING_PARAM=806 /* E$MISSING_PARAM */;/* REQUIRED FPT PARAMETER MISSING */
   21      %EQU E$BAD_DCB=807 /* E$BAD_DCB */;     /* DCB NOT OPEN TO A FILE             */
   22      %EQU E$BAD_TIME=808 /* E$BAD_TIME */;   /* ILLEGAL VALUE FOR WAIT_TIME PARAM  */
   23      %EQU E$BREAK=809 /* E$BREAK */;         /* BREAK, CTRL Y, ETC. OCCURRED       */
   24      %EQU E$BAD_UPGRADE=810 /* E$BAD_UPGRADE */; /* MUST BE ALLOCATED BEFORE UPGRADE */
   25      %EQU E$ENQ_LIMIT=812 /* E$ENQ_LIMIT */; /* MAX ALLOWED ENQ'S EXCEEDED         */
   26      %EQU E$UQ_NOCHANGE=813 /* E$UQ_NOCHANGE */; /* M$ENQ W NO CHANGE IN OPTIONS   */
   27      %EQU E$UQ_DOWNGRADE=814 /* E$UQ_DOWNGRADE */; /* ILLEGAL DOWNGRADE REQUEST    */
   28      %EQU E$NO_UPGRADE=815 /* E$NO_UPGRADE */; /* NO_UPGRADE UPGRADE REQUESTED    */

