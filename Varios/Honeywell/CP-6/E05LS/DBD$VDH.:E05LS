      158     3311           '%-%RVrtLin=%.#:8/:1-KD'
      159     3312           '%1#[%?%3.1#E'                       /* If there is a KV$FLDLST, fetch it  */
      160     3313           '%/---KV$FLDLST @ .%.1#KH:%2.1P'     /* And display it                     */
      161     3314           '%.16-%RAtrCnt%...' %KV$FLDLST_ATRCNT# 'P=%@.16D'
