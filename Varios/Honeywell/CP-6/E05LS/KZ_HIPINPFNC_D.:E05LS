       20     1064        %INCLUDE KV$INPFNC;
       21     1623        %INCLUDE KV$GLB;
       22     2042        %INCLUDE KV$VDH;
       23     2473        %KV_INPPRC_E;
       24     2525    1   DCL KZ_INPFNCTBLHED$ PTR CONSTANT SYMDEF INIT(ADDR(KZ_INPFNC.CP5));
       25     2526        %EJECT;
PL6.E3A0      #001=KZ_INPFNC_D File=KZ_HIPINPFNC_D.:E05TSI                       WED 07/30/97 03:06 Page:2    
       26     2527        /*
       27     2528           CP-5 compatible (almost) system input function table.
       28     2529           Contains all of the CP-5 ESCAPE and control sequences, with
       29     2530           the following changes:
       30     2531
       31     2532           ESC-STX (ESC-CTRL-B) is the same as ESC-B except
       32     2533           that type-ahead is not deleted.
       33     2534
       34     2535           ESC-EOT (ESC-D) is the same as ESC-D except that the last
       35     2536           saved edit buffer for the current stream is recalled.
       36     2537           Host 'domain' is mapped into stream in B02.
       37     2538
       38     2539           ESC-A in CP5 is ESC-$ in CP6.
       39     2540
       40     2541           ESC-D in CP-6 always recalls the last saved input edit buffer.
       41     2542
       42     2543           ESC-G in CP-6 displays internal communications processor
       43     2544           information.
       44     2545
       45     2546           ESC-K in CP-6 deletes from the edit point thru the end of the
       46     2547           input record.
       47     2548
       48     2549           ESC-M (enter column replacement mode) resets ESC-O (enter
       49     2550           logical overstrike mode).  Since the architecture of the
       50     2551           CP-6 HIP handler is significantly different than the CP-5
       51     2552           COC handler, input editing in CP-6 is much more powerful.
       52     2553           As a result, the meaning of ESC-O has changed significantly.
       53     2554
       54     2555           ESC-N in CP-6 positions to the end of the input record.  ESC-D
       55     2556           in CP-5 did this, under some circumstances.
       56     2557
       57     2558           ESC-O has changed.  See ESC-M.
       58     2559
       59     2560           ESC-P (half-duplex paper tape mode) is exited upon receipt of
       60     2561           BREAK, CTRL-Y, ESC-ESC, or ESC-Y.
       61     2562
       62     2563           ESC-Q issues a response from the communications processor
PL6.E3A0      #001=KZ_INPFNC_D File=KZ_HIPINPFNC_D.:E05TSI                       WED 07/30/97 03:06 Page:3    
       63     2564           (!HI!) and also (if logged on) from the host (E.G., !SCU!).
       64     2565
       65     2566           ESC-V-CHARACTER in CP-6 positions to the specified character in
       66     2567           the input record.
       67     2568
       68     2569           ESC-Z is not in CP-6 because coupling isn't.
       69     2570
       70     2571           ESC-$ selects the proper translate table.  CP-6 supports
       71     2572           bit-paired APL terminals, in addition to the normal ASCII and
       72     2573           and typewriter-paired APL terminals supported by CP-5.
       73     2574           As a result of ESC-$, two 'other' escapes appear to the normal
       74     2575           ASCII terminal user; they are ESC-X'7C' (ESC-VERTICAL-LINE)
       75     2576           and ESC-X'7E' (ESC-TILDE).  They will put the line into
       76     2577           one of the APL modes.  ESC-$ will return the line to normal
       77     2578           ASCII mode.
       78     2579
       79     2580           ESC-HYPHEN in CP-6 tells the handler that the terminal is
       80     2581           positioned at the perf.
       81     2582        */
       82     2583        %EJECT;
PL6.E3A0      #001=KZ_INPFNC_D File=KZ_HIPINPFNC_D.:E05TSI                       WED 07/30/97 03:06 Page:4    
       83     2584        %KV_INPOPR (NAME=KZ_INPFNC, TYP=MSC, RED=); /* Dummy to get started           */
       84     2622        /*
       85     2623          INPOPRs used by routines that directly call the
       86     2624          input function interpreter with INPOPR indexes. These INPOPRs couldn't
       87     2625          be expressed in one word; these are "branched to" via
       88     2626          JMP INPOPRs below.
       89     2627        */
       90     2628        %EQU Y#INPOPR_PRTERR =KV_INPOPRX;
       91     2629        %KV_INPOPR (TYP=USRINPPRC, IMD=, ID=KZ#INPPRC_PRTERR, CHN=);
       92     2667        %KV_INPOPR (TYP=TXT, RED=, DSP=WRT,
       93     2668                       TXT="%CONCAT (BITASC (ASCIIBEL), BITASC (ASCIINMB))", CHN=);
       94     2706        %KV_INPOPR (TYP=ACTRED, RED=, CMPCOD=KV_CMPCOD_PRTERR);
       95     2744        %EQU Y#INPOPR_RE_RED =KV_INPOPRX;
       96     2745        %KV_INPOPR (TYP=VDHINPPRC, RED=, ID=KV_INPPRC_COPSTREDTBFR, CHN=);
       97     2783        %KV_INPOPR (TYP=VDHINPPRC, RED=, ID=KV_INPPRC_RE_TYP);
       98     2821        %EQU Y#INPOPR_RSTINSMOD =KV_INPOPRX;
       99     2822        %KV_INPOPR (TYP=VDHINPPRC, RED=, ID=KV_INPPRC_RSTINSMOD, CHN=);
      100     2860        %EQU Y#INPOPR_RE_TYP =KV_INPOPRX;
      101     2861        %KV_INPOPR (TYP=VDHINPPRC, RED=, ID=KV_INPPRC_RE_PSTRED, CHN=);
      102     2899        %KV_INPOPR (TYP=VDHINPPRC, RED=, ID=KV_INPPRC_RE_TYP);
      103     2937        %EJECT;
PL6.E3A0      #001=KZ_INPFNC_D File=KZ_HIPINPFNC_D.:E05TSI                       WED 07/30/97 03:06 Page:5    
