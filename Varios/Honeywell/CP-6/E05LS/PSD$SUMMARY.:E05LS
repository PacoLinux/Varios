       86     6053    1      CALL PSL$TIME (PS_OLD_COLLECT.UTS, THEN_TIME);
       87     6054
       88     6055    1      IF PS_MODE = REPLAY_MODE#
       89     6056    1        THEN
       90     6057    1         IF SUMMARY_TIME.DATE@ ~= THEN_TIME.DATE@
       91     6058    1           THEN
       92     6059    1            CALL X$WRITE (PS_FDS, VECTOR(DAY_FORMAT),
       93     6060    1             VECTOR(SUMMARY_TIME.DAY@), VECTOR(SUMMARY_TIME.DATE@));
       94     6061
       95     6062    2      DO I = 0 TO PS_SUMMARY_ITEMS - 1;
       96     6063
       97     6064    2         ITEM = PS_SUMMARY_LIST(I);
       98     6065
       99     6066    2         IF V_TABLE.VAR$(ITEM) = ADDR(NIL)
      100     6067    3           THEN DO;
      101     6068
      102     6069    3            J = V_TABLE.INDX(ITEM);
      103     6070
      104     6071    3            CALL PSL$LOCATE (J, P$, Q$);
      105     6072
      106     6073    3            VALUE = P$ -> BLOCK.DATA(V_TABLE.OFFSET(ITEM));
      107     6074
      108     6075    3            IF Q$ = ADDR(NIL)
      109     6076    3              THEN
      110     6077    3               OLD_VALUE = 0;
      111     6078    3              ELSE
PL6.E3A0      #001=PSD$SUMMARY File=PSD$SUMMARY.:E05TSI                          WED 07/30/97 04:34 Page:5    
      112     6079    3               OLD_VALUE = Q$ -> BLOCK.DATA(V_TABLE.OFFSET(ITEM));
      113     6080
      114     6081    3            END;
      115     6082
      116     6083    3           ELSE DO;
      117     6084
      118     6085    3            VALUE = V_TABLE.VAR$(ITEM) -> SBIN_;
      119     6086    3            OLD_VALUE = PINCRW(ADDR(PS_OLD_COLLECT), POFFW(V_TABLE.VAR$(ITEM),
      120     6087    3             ADDR(PS_COLLECT))) -> SBIN_;
      121     6088
      122     6089    3            IF V_TABLE.MODE(ITEM) = ABS#
      123     6090    3             AND PS_OLD_PMDAT$ = ADDR(NIL)
      124     6091    3              THEN
      125     6092    3               VALUE = OLD_VALUE;
      126     6093
      127     6094    3            END;
      128     6095
      129     6096    2         IF V_TABLE.MODE(ITEM) = PERCENT#
      130     6097    3           THEN DO;
      131     6098
      132     6099    3            CALL PSL$CHOMP (OLD_VALUE * 10, VALUE * 10, PERCENT#,
      133     6100    3             WASTE_CAN, TEMP);
      134     6101
      135     6102    3            SUMMARY_DATA (SUM_INDX) = TEMP / 10;
      136     6103    3            SUMMARY_DATA (SUM_INDX + 1) = MOD(TEMP, 10);
      137     6104    3            SUM_INDX = SUM_INDX + 2;
      138     6105
      139     6106    3            END;
      140     6107
      141     6108    3           ELSE DO;
      142     6109
      143     6110    3            J = V_TABLE.MODE (ITEM);
      144     6111
      145     6112    3            CALL PSL$CHOMP (OLD_VALUE, VALUE, J, WASTE_CAN,
      146     6113    3             SUMMARY_DATA (SUM_INDX));
      147     6114
      148     6115    3            SUM_INDX = SUM_INDX + 1;
PL6.E3A0      #001=PSD$SUMMARY File=PSD$SUMMARY.:E05TSI                          WED 07/30/97 04:34 Page:6    
      149     6116
      150     6117    3            END;
      151     6118
      152     6119    2         END;
      153     6120
      154     6121    1      LEN = SUM_INDX * 4;
      155     6122
      156     6123    1      CALL X$WRITE (PS_FDS, FORMAT_,
      157     6124    1       VECTOR(SUMMARY_TIME.TIME@), VECTOR(ADDR(SUMMARY_DATA) -> TEXT));
      158     6125
      159     6126    1      IF PS_WORKING_FLAGS ~= PS_DISPLAY_FLAGS
      160     6127    1        THEN CALL PSD$SKIP;
      161     6128
      162     6129    1      RETURN;
      163     6130
      164     6131    1   PSD$SUM_HEAD: ENTRY;
      165     6132
      166     6133    1      CALL GEN_VECTOR (PS_SUMMARY_HEADER, HEADER_);
      167     6134
      168     6135    1      CALL X$WRITE (PS_FDS, VECTOR(SUM_HEADER),
      169     6136    1       HEADER_);
