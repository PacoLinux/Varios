    7      /**/
    8      /*  MM owns all screech codes from 9 to 49.                        */
    9      /**/
   10      %EQU   S$MM9 = 9;                       /* Impossible WSQ passed to MM        */
   11      %EQU   S$MM10 = 10;                     /* Page chain inconsistant            */
   12      %EQU   S$MM11 = 11;                     /* Bad virtual page # passed to MM    */
   13      %EQU   S$MM12 = 12;                     /* Attempt to free a monitor page     */
   14      %EQU   S$MM13 = 13;                     /* RSPP called for a non-stolen page     */
   15      %EQU   S$MM14 = 14;                /* I/O cache page count has gone negative     */
   16      %EQU   S$MM15 = 15;                /* Can't get page guaranteed by CHKSIZE    */
   17      %EQU   S$MM16 = 16;                     /* Illegal monitor GVP request        */
   18      %EQU   S$MM17 = 17;                     /* Illegal monitor FVP request        */
   19      %EQU   S$MM18 = 18;           /* Page being freed not in user's page chain    */
   20      %EQU   S$MM19 = 19;           /* XMAP called for pg not in buffer segment     */
   21      %EQU   S$MM20 = 20;                /* XMAP called for pg not currently in use    */
   22      %EQU   S$MM21 = 21;      /* SEGID of non-standard descriptor passed to MMF    */
   23      %EQU   S$MM22 = 22;                     /* Inconsistancy between GDS and SGVP    */
   24      %EQU   S$MM23 = 23;                     /* Inconsistancy between FDS and SFVP    */
   25      %EQU   S$MM24 = 24;                     /* Illegal data segment size in MMH   */
   26      %EQU   S$MM25 = 25;                /* SEGID invalid for memory allocation     */
   27      %EQU   S$MM26 = 26;                     /* Illegal monitor-mode call on MMI   */
   28      %EQU   S$MM27 = 27;           /* FDP and SFVP mismatch on allocated ISR size     */
   29      %EQU   S$MM28 = 28;                /* Couldn't trunc enough FPOOL's     */
   30      %EQU   S$MM29 = 29;                     /* CVM failed in MAKUSER              */
   31      %EQU   S$MM30 = 30;                     /* WSQINIT called more than once      */
   32      %EQU   S$MM31 = 31;                /* Bad # of WSQs requested from WSQINIT    */
   33      %EQU   S$MM32 = 32;           /* Insufficient memory for CWSQ page tables     */
   34      %EQU   S$MM33 = 33;                     /* MMS called with bad CWSQ number    */
   35      %EQU   S$MM34 = 34;           /* More than max # pages requested for queue    */
   36      %EQU   S$MM35 = 35;                     /* CVM failed in WSQINIT              */
   37      %EQU   S$MM36 = 36;                /* Attempt to free invalid CWSQ buffer     */
07:29 JUL 29 '97 MM_SCODE_C.:E05SI                                         2    
   38      %EQU   S$MM37 = 37;                     /* Queue page is already allocated    */
   39      %EQU   S$MM38 = 38;                /* GOODPP called for page which isn't bad     */
   40      %EQU    S$MM39 = 39;               /* User CURRCORE count has gone negative   */
   41      %EQU    S$MM40 = 40;          /* Not enough pgs reserved for COMGROUP by TIGR */
   42      %EQU    S$MM41 = 41;          /* Invalid request to MMQ$RECON    */
   43      %EQU    S$MM42 = 42;                    /* Buffer too big for MMS$SETUP_BUF   */
   44      %EQU    S$MM43 = 43;                    /* Bad parameters for MMH$MAPDS       */
   45      %EQU    S$MM44 = 44;                    /* CVM failure in MMH$INIT_DS         */
   46      %EQU    S$MM45 = 45;               /* Pg chain count inconsistancy in MMJ     */
   47      %EQU  S$MM47  = 47;                     /* Virtual page inconsitency          */
   48      %EQU S$MM48 = 48;              /* GMA segment parameter errors */
   49      %EQU S$MM49 = 49;              /* Memory shuffle error    */

