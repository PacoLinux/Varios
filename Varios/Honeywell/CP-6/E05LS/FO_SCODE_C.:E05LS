    7      /*X* MCL=1,PLM=4,IND=0,SDI=2,CTI=0,ENU=0,AND,DCI=4,CSU=2,ECU=4,THI=0,DTI=2 */
    8      %EQU S$BADEXTEV=275/*BADEXTEV*/;        /* UNKN EXTERNAL EVENT                */
    9      %EQU S$BADEVS=278/*BADEVS*/;            /* DEV_EVENT: BAD EV-ST COMBO         */
   10      %EQU S$DELACTFIL=279/*DELACTFIL*/;      /* DELETING FILE WHILE PRINTING       */
   11      %EQU S$BADSYMBDA=283/*BADSYMBDA*/;      /* BAD SR DISK ADDR IN MOOS           */
   12      %EQU S$DBLCON=285/*DBLCON*/;            /* DOUBLE DEVICE CONNECT              */
   13      %EQU S$NODEV=286/*NODEV*/;              /* CAN'T FIND EVENTED DEVICE          */
   14      %EQU S$XCON=287/*XCON*/;                /* OUTSYM RECEIVED EXIT CONTROL       */
   15      %EQU S$BADSYMBINFCN=289/*BADSYMBINFCN*/;/* UNKN FCN TO FOM$SYMBIN             */
   16      %EQU S$BADSYMBEVST=290/*BADSYMBEVST*/;  /* FOM$EVENT: BAD EV-ST COMBO         */
   17      %EQU S$BADEV=291/*BADEV*/;              /* UNKN EVENT TO FOE$EVNT             */
   18      %EQU S$BADSYMBEVENT=292/*BADSYMBEVENT*/;/* UNKN EVENT TO FOM$EVENT            */
   19      %EQU S$BADSYMBSTATE=293/*BADSYMBSTATE*/;/* UNKN SYMB STATE IN FOM$EVENT       */
   20      %EQU S$BADSYMBIO=294/*BADSYMBIO*/;      /* IMPOSSIBLE ALTRET FROM M$SYMBIO    */
   21      %EQU S$BADOSAUEV=295/*BADOSAUEV*/;      /* UNKN OSCG AU EVENT                 */
   22      %EQU S$BADEXTCHN=296/*BADEXTCHN*/;      /* BAD EXTENT CHAIN FOR FILE          */
   23      %EQU S$BADCNUM=299/*BADCNUM*/;          /* BAD COMMODE CNUM                   */
   24      %EQU S$DBLADD=602/*DBLADD*/;            /* FILE ADDED TWICE                   */
   25      %EQU S$OFFADD=604/*OFFADD*/;            /* FILE ADDED BY OFFED BATCH JOB      */
   26      %EQU S$BADGLC=605/*BADGLC*/;            /* COMMODE CHUNK ADDED TO ENDED FILE  */
   27      %EQU S$CSCGERR=606/*CSCGERR*/;          /* ERR READING CSCG                   */
   28      %EQU S$FOBFORM=608/*S$FOBFORM*/;        /* :FORM FILE BROKEN                  */

