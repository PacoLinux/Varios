      305     8996    3             CALL EDITSTR (STR,,, TRIM_MOP, SUBSTR (CHAR16,2,6));
      306     8997    3           END;
      307     8998
      308     8999    2        RETURN;
      309     9000
      310     9001    2   END CONVERT_TO_WADDR;
