      247     8767    1   ADD_STR: PROC (STR, STRL);
      248     8768
      249     8769    2   DCL STR CHAR(STRL);
      250     8770    2   DCL STRL SBIN;
      251     8771
      252     8772    2        IF POS<LENGTHC(VARIABLE.EXTRA) THEN POS = POS+1; /* insert a space */
      253     8773
      254     8774    2   ADD_STR_NS: ENTRY (STR, STRL);
      255     8775
      256     8776    2        IF POS+STRL<=LENGTHC(VARIABLE.EXTRA)
      257     8777    3        THEN DO;
      258     8778    3             SUBSTR (VARIABLE.EXTRA, POS, STRL) = STR;
      259     8779    3             POS = POS + STRL;
      260     8780    3           END;
      261     8781    2        ELSE IF POS<LENGTHC(VARIABLE.EXTRA)
      262     8782    3             THEN DO;
      263     8783    3                  SUBSTR (VARIABLE.EXTRA, POS) = STR;
      264     8784    3                  POS = LENGTHC(VARIABLE.EXTRA);
      265     8785    3                END;
      266     8786    2   END;
      267     8787
