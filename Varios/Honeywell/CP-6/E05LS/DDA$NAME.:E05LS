PL6.E3A0      #001=DDA$NAME File=DDA$NAME.:E05TSI                                TUE 07/29/97 13:34 Page:12   
      312     9003        /*D*   NAME:   WILDCMPR
      313     9004               CALL:   CALL WILDCMPR(str_textc, wild_textc) ALTRET (unequal);
      314     9005               INTERFACE: none.
      315     9006               INPUT:  STR_TEXTC - a textc string of the string to match.
      316     9007                       WILD_TEXTC - the wildcarded textc string to match.
      317     9008               OUTPUT: none.
      318     9009               DESCRIPTION: The specified character string is checked to see
      319     9010                       if it matches the wildcarded string - ('?' matched).
      320     9011                       If so return, else altreturn.
      321     9012        */
      322     9013        /****************************************************************************/
      323     9014    1   WILDCMPR: PROC(SRC_STR,WILD_STR) ALTRET;
      324     9015                   %INCLUDE B_MACROS_C;
      325     9126
      326     9127        %B$DNAME (NAME=SRC_STR, STCLASS=);
      327     9146        %TEXTC (NAME=WILD_STR, STCLASS=);
      328     9151
      329     9152    2   DCL SRCL SBIN;
      330     9153    2   DCL WILDL SBIN;
      331     9154    2   DCL SRC$ PTR;
      332     9155    2   DCL WILD$ PTR;
      333     9156    2   DCL SRC CHAR(SRCL) BASED (SRC$);
      334     9157    2   DCL WILD CHAR(WILDL) BASED (WILD$);
      335     9158
      336     9159    2   DCL QI SBIN;
      337     9160    2   DCL SI SBIN;
      338     9161    2   DCL WL SBIN;
      339     9162
      340     9163    2        UNMATCHED_OR_WILD = '0'B;
      341     9164    2        SRCL = SRC_STR.COUNT;
      342     9165    2        WILDL = WILD_STR.SIZ;
      343     9166    2        SRC$ = ADDR (SRC_STR.TEXT);
      344     9167    2        WILD$ = ADDR (WILD_STR.TEXT);
      345     9168
      346     9169    2        CALL INDEX (QI, '?', WILD)
      347     9170    3        WHENALTRETURN DO;
      348     9171    3             IF SRC ~= WILD THEN GOTO FAIL;
PL6.E3A0      #001=DDA$NAME File=DDA$NAME.:E05TSI                                TUE 07/29/97 13:34 Page:13   
      349     9172    3             RETURN;
      350     9173    3           END;
      351     9174    2        UNMATCHED_OR_WILD = '1'B;          /* for the WILD part */
      352     9175    2        IF (QI>0) AND (SUBSTR(SRC,0,QI) ~= SUBSTR(WILD,0,QI)) THEN GOTO FAIL;
      353     9176    2        SI = QI;
      354     9177    3        DO WHILE (QI < WILDL);
      355     9178    3             QI = QI+1;                    /* SKIP OVER TO '?' */
      356     9179    3             IF QI = WILDL THEN RETURN;    /* '?' IN LAST POSITION */
      357     9180    3             CALL INDEX (WL, '?', SUBSTR (WILD, QI))
      358     9181    4             WHENRETURN DO;                /* a '?' still exists */
      359     9182    4                  IF (WL > 0)
      360     9183    5                  THEN DO;
      361     9184    5                       CALL INDEX (SI, SUBSTR (WILD,QI,WL), SRC, SI) ALTRET (FAIL);
      362     9185    5                       SI = SI+WL;
      363     9186    5                       QI = QI+WL;
      364     9187    5                     END;
      365     9188    4                END;
      366     9189    4             WHENALTRETURN DO;             /* no more '?'s in string */
      367     9190    4                  IF (WL > 0) AND
      368     9191    4                    ((WL > SRCL - SI) OR
      369     9192    4                    (SUBSTR (SRC, SRCL - WL, WL) ~= SUBSTR (WILD, QI, WL)))
      370     9193    4                  THEN
      371     9194    4                       GOTO FAIL;
      372     9195    4                  RETURN;
      373     9196    4                END;
      374     9197    3           END;                            /* DO WHILE (QI < WILDL); */
      375     9198    2        RETURN;
      376     9199
      377     9200    2   FAIL:
      378     9201    2        UNMATCHED_OR_WILD = '1'B;          /* for the UNMATCHED part */
      379     9202    2        ALTRETURN;
      380     9203
      381     9204    2   END WILDCMPR;
      382     9205
      383     9206    1   END DDA$NAME;

