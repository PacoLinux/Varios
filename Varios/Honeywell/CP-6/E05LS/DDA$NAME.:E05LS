      288     8808    1   CONVERT_TO_WADDR: PROC (VALUE, STR);
      289     8809    2   DCL VALUE SBIN;
      290     8810    2   DCL STR CHAR(7) CALIGNED;
      291     8811
      292     8812        %INCLUDE B_MICROPS_C;
      293     8984    2   DCL TRIM_MOP(0:4) BIT(9) UNAL CONSTANT INIT (%LTE5,'056'O,%MFLC5,%ENF10,%MVC1);
      294     8985
      295     8986    2   DCL DDR$BINOCT ENTRY(2);
      296     8987    2   DCL DFR$BINHEX ENTRY(4);
      297     8988
      298     8989    2        IF S_D.ONE_TO_TWO.EXEC_DOMAIN = %HOST
      299     8990    3        THEN DO;
