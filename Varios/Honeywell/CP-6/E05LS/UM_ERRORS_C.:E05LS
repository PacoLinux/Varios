    7      /*
    8         This module contains error codes for MISC PMMEs.
    9         Numerical range = 825 - 859
   10      */
   11      /*************** ERRORS 850-852,856-859 are used by M$FID *********************/
   12      %EQU E$NOFID    = 850 /* E$NOFID */;    /* NO FID RECEIVED BY M$FID           */
   13      %EQU E$BADFID   = 851 /* E$BADFID */;   /* INVALID FID                        */
   14      %EQU E$NOFIELD  = 852 /* E$NOFIELD */;  /* NO RECIEVING FIELD                 */
   15      %EQU E$FIDOFLOW = 856 /* E$FIDOFLOW */; /* FID OVERFLOW                       */
   16      %EQU E$BADACCT  = 857 /* E$BADACCT */; /* ACCOUNT NAME IS MALFORMED           */
   17      %EQU E$BADPASS  = 858 /* E$BADPASS */; /* PASSWORD IS MALFORMED               */
   18      %EQU E$SERTOOSMALL  = 859 /* E$SERTOOSMALL */; /* TOO MANY SNS */
   19      /**/
   20      /*************** ERROR 853 IS USED BY M$ERRMSG ********************************/
   21      %EQU E$ERRMSGALT = 853 /* E$ERRMSGALT */; /* M$ERRMSG USES THIS CODE TO ALTRET */
   22      /**/
   23      /*************** ERROR 854 USED BY M$TIME *************************************/
   24      %EQU E$BADDT     = 854 /* E$BADDT */;
   25      /**/
   26      /*************** ERROR 825 USED BY M$TIME ***********************/
   27
   28      %EQU E$YEARERR   = 825 /* E$YEARERR */;
   29      /**/
   30      /*************** ERROR 826 USED BY M$RUE ***********************/
   31
   32      %EQU E$EVENTERR   = 826 /* E$EVENTERR */;
   33
   34      /*************** ERRORS 840-849 ARE USED BY M$HELP ****************************/
   35      /*      THE FOLLOWING CODES INDICATE ERRORS BY THE TERMINAL OPERATOR.  THE    */
   36      /*      SHOULD BE REPORTED AS OBSERVED                                        */
   37      %EQU E$NOHPROC  = 840 /* E$NOHPROC */; /* NO SUCH PROCESSOR HAS HELP DATABASE */
07:34 JUL 29 '97 UM_ERRORS_C.:E05SI/UM_ERRORS_C.:E05TUI                    2    
   38      %EQU E$BADHOPT  = 841 /* E$BADHOPT */;  /* BADH COMBINATION OF HELP OPTIONS    */
   39      %EQU E$NOMOREHELP=842 /* E$NOMOREHELP */; /* NO HELP LEFT FOR THAT TOPIC      */
   40      %EQU E$BADHFID  = 843 /* E$BADHFID */;  /* BADH FID FOR HELP PROCESSOR         */
   41      %EQU E$NOTOPIC  = 844 /* E$NOTOPIC */;  /* NO SUCH HELP TOPIC                 */
   42      %EQU E$ILLWILD  = 845 /* E$ILLWILD */;  /* WILDCARDING NOT LEGAL HERE         */
   43      /*      THE FOLLOWING ERRORS SHOULD BE CAUGHT BY THE CALLING PROGRAMME AND    */
   44      /*      NOT REPORTED TO THE TERMINAL OPERATOR                                 */
   45      %EQU E$HERROR   = 846 /* E$HERROR */;   /* BAD CALL TO M$HELP, OR ERROR COND. */
   46      %EQU E$NEEDRESULTS = 847 /* E$NEEDRESULTS */; /* THAT OPERATION REQUIRES THE  */
   47                                              /* RESULTS FIELD                      */
   48      %EQU E$HELPDERR = 848 /* E$HELPDERR */; /* HELP DATABASE ERROR                */
   49      %EQU E$BADHPARAM    = 849 /* E$BADHPARAM  */;  /*BAD M$HELP PARAMETER         */
   50      /**/
   51      /************ ERRORS 836 - 839 are used by M$CMDVAR ***************************/
   52      %EQU E$BADCMDCALL   = 839 /* E$BADCMDCALL */;  /* BAD CALL TO M$CMDVAR        */
   53      %EQU E$BADCMDNAME   = 838 /* E$BADCMDNAME */;  /* BAD COMMAND VAR NAME        */
   54      %EQU E$NOTCMDNAME   = 837 /* E$NOTCMDNAME */;  /* NO SUCH COMMAND VARIABLE    */
   55      /*********** ERRORS 830 - 835 are used by M$OCMSG *****************************/
   56      %EQU E$BADOCMSG1    = 830 /* E$BADOCMSG1 */; /* BAD USER PARM1 ON M$OCMSG */
   57      %EQU E$BADOCMSG2    = 831 /* E$BADOCMSG2 */; /* BAD USER PARM2 ON M$OCMSG */
   58    * /*********** ERRORS - ARE USED BY UMD$DRIBBLE *********************************/
   59    * %EQU E$DRIBINTOKEYED = 855;            /* DRIBBLE INTO EXISTING KEYED FILE */

