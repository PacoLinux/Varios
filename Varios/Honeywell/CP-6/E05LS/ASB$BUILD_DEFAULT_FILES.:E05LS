      151    15439    1   KL$TREC.KL$FETS = KL_TSREC.KL_FETS ;
      152    15440    1   KL$TREC.NWINFO = SIZEW( KL$TREC.KL$FETS) ;
      153    15441    1   ARS = LENGTHC( KL_DREC_CNT ) + KL$TREC.NWINFO * 4 ;
      154    15442    1   KL$TREC.PASSWORD = AS_BLANK ;
      155    15443    1   CALL ASZ$SCRAMBLE( KL$TREC.PASSWORD ) ;
      156    15444    1   CALL ASB$WRITE_HLP( TREC$, ARS ) ;
      157    15445                                                  /* Setup default project */
      158    15446    1   USERS_KEY.COUNT = 21;
      159    15447    1   USERS_KEY.TEXT(7) = 'P';
      160    15448    1   USERS_KEY.TEXT(20) = 'S';
      161    15449    1   AS_DEFAULT.BUDGET.PROJ_ALLOWED = 0;
      162    15450    1   AS_DEFAULT.SETUP_CHRA.SIZE = 0;
      163    15451    1   CALL ASB$WRITE_USERS( AS_DEFAULT );
      164    15452    1   CALL ASB$WRITE_HLP( TREC$, ARS );
      165    15453    1   USERS_KEY.COUNT = 8;
      166    15454    2   DO I = 0 TO 2;
      167    15455    2      AS_USERS.PPRIV(I) = AS_USERS.PPRIV(I) | %PPR_PADMIN#;
PL6.E3A0      #001=ASA$B_USERS_HLP File=ASB$BUILD_DEFAULT_FILES.:E05TSI          TUE 07/29/97 08:55 Page:15   
      168    15456    2      END;
