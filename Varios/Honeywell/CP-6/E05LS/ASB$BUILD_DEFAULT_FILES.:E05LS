     1013    23018        /* Routine to read in a charset and place it in the form record
     1014    23019             for either stdlp or STD**.  For STD** Place the 513 chain
     1015    23020             and attributes in it.  For stdlp, get the attributes of the
     1016    23021             LP01 out of the :CONFIG file, find a chain with the same
     1017    23022             attributes and place that chains info into stdlp.  If no
     1018    23023             chain matches the attributes for LP@LOCAL, the default is
     1019    23024             600.
     1020    23025        */
     1021    23026
     1022    23027    1   INSERT_CHARSET: PROC;
     1023    23028
     1024    23029    2   DCL LP_ATTRS BIT (18) ALIGNED;
     1025    23030
     1026    23031    2   IF FS$FORMREC.FNAME = 'STD**'
     1027    23032    3   THEN DO;
     1028    23033    3      ARS = SIZEB(CHARSET);
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:191  
     1029    23034    3      CALL ASF$READ_FORM (KEY_513, CHARSET$, ARS)
     1030    23035    4      WHENRETURN DO;
     1031    23036    4         FS$FORMREC.LGLTYPS = CHARSET.LGLTYPS;
     1032    23037    4         FS$FORMREC.ATTR = CHARSET.ATTRS;
     1033    23038    4         FS$FORMREC.CHAINIMAGE = CHARSET.CHAINIMAGE;
     1034    23039    4         FS$FORMREC.CHAIN_NAME = '513';
     1035    23040    4         END;
     1036    23041    4      WHENALTRETURN DO;
     1037    23042    4         IF MERR_CODE.CODE ~= 86
     1038    23043    5         THEN DO;
     1039    23044        /*E* ERROR: ASF-E$513CHAINNOTTHERE-C
     1040    23045             MESSAGE: STD** cannot be build - Charset 513 does not exist.
     1041    23046             MESSAGE1: When STD** is built, it uses as default the 513 charset,
     1042    23047                       if 513 is not there (it is built as a default) STD**
     1043    23048                       cannot be build.
     1044    23049        */
     1045    23050                 %SCREAM(ERR#=E$513CHAINNOTTHERE, MID='F', LEVEL=ERROR);
     1046    23065    5            END;
     1047    23066    5         ELSE DO;
     1048    23067    5            CALL ASH$ERROR (P_PCB.ERROR);
     1049    23068    5            RETURN;
     1050    23069    5            END;
     1051    23070    4         END;
     1052    23071    3      END; /* STD** */
     1053    23072    2   IF FS$FORMREC.FNAME = 'STDGR'
     1054    23073    3   THEN DO;
     1055    23074    3      ARS = SIZEB(CHARSET);
     1056    23075    3      CALL ASF$READ_FORM (KEY_600, CHARSET$, ARS)
     1057    23076    4      WHENRETURN DO;
     1058    23077    4         FS$GFORMREC.LGLTYPS = CHARSET.LGLTYPS;
     1059    23078    4         FS$GFORMREC.ATTR = CHARSET.ATTRS;
     1060    23079    4         FS$GFORMREC.ATTR = FS$GFORMREC.ATTR | '000200'O;
     1061    23080    4         FS$GFORMREC.ATTR = FS$GFORMREC.ATTR | '000400'O;
     1062    23081    4         END;
     1063    23082    4      WHENALTRETURN DO;
     1064    23083    4         IF MERR_CODE.CODE ~= 86
     1065    23084    5         THEN DO;
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:192  
     1066    23085    5            AS_FIELD1 = STGR;
     1067    23086                 %SCREAM(ERR#=E$600CHAINNOTTHERE, MID='F', LEVEL=ERROR);
     1068    23101    5            END;
     1069    23102    5         ELSE DO;
     1070    23103    5            CALL ASH$ERROR (P_PCB.ERROR);
     1071    23104    5            RETURN;
     1072    23105    5            END;
     1073    23106    4         END;
     1074    23107    3      END; /* Stdgr */
     1075    23108    2   IF FS$FORMREC.FNAME = 'STDLP'
     1076    23109    3   THEN DO;
     1077    23110    3      ARS = SIZEB(CHARSET);
     1078    23111    3      CALL GET_CONFIGS_LP01_ATTRS (LP_ATTRS)
     1079    23112    4      WHENRETURN DO;
     1080    23113    4         USERS_KEY.COUNT = 16;
     1081    23114    4         USERS_KEY.TXTC = '~        CHARSET';
     1082    23115             /* This is to assure us that the next record read will be a
     1083    23116              charset record.
     1084    23117             */
     1085    23118    4         CALL ASF$PREC_FORM (USERS_KEY) ALTRET (GO_ON);
     1086    23119    4   GO_ON: ;
     1087    23120
     1088    23121             /* Start the loop.  Just keep reading in charset records until
     1089    23122              the attrs match,  then we have a valid charset.
     1090    23123              If we get an end of file on a read, we
     1091    23124              go back to the 600 charset.  Any other altret does this too.
     1092    23125             */
     1093    23126    5         DO UNTIL ( LP_ATTRS = CHARSET.ATTRS );
     1094    23127    5            CALL ASF$READ_FORM_SEQ (USERS_KEY, CHARSET$, ARS) ALTRET (USE_600);
     1095    23128    5            END;
     1096    23129    4         END;
     1097    23130    4      WHENALTRETURN DO;
     1098    23131    4   USE_600: ;
     1099    23132    4         USERS_KEY = KEY_600;
     1100    23133    4         CALL ASF$READ_FORM (KEY_600, CHARSET$, ARS)
     1101    23134    5         WHENALTRETURN DO;
     1102    23135    5            IF MERR_CODE.CODE ~= 86
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:193  
     1103    23136    6            THEN DO;
     1104    23137                   /*E* ERROR: ASF-E$600CHAINNOTTHERE-C
     1105    23138                        MESSAGE: %U1 cannot be build - Charset 600 does not exist.
     1106    23139                        MESSAGE1: When %U1 is built, it uses as default the 600 charset,
     1107    23140                                  if 600 is not there (600 is built as a default) %U1
     1108    23141                                  cannot be build.
     1109    23142        */
     1110    23143    6               AS_FIELD1 = STLP;
     1111    23144                    %SCREAM(ERR#=E$600CHAINNOTTHERE, MID='F', LEVEL=ERROR);
     1112    23159    6               END;
     1113    23160    6            ELSE DO;
     1114    23161    6               CALL ASH$ERROR (P_PCB.ERROR);
     1115    23162    6               RETURN;
     1116    23163    6               END;
     1117    23164    5            END;
     1118    23165    4         END;
     1119    23166          /* Now CHARSET$ is pointing at the charset to use, just stick it in!*/
     1120    23167    3      FS$FORMREC.LGLTYPS = CHARSET.LGLTYPS;
     1121    23168    3      FS$FORMREC.ATTR = CHARSET.ATTRS;
     1122    23169    3      FS$FORMREC.CHAINIMAGE = CHARSET.CHAINIMAGE;
     1123    23170    3      FS$FORMREC.CHAIN_NAME = SUBSTR (USERS_KEY.TXTC,1,USERS_KEY.COUNT-8);
     1124    23171    3      END; /* Stdlp */
     1125    23172    2   RETURN;
     1126    23173    2   END INSERT_CHARSET;
     1127    23174    1   REVERSE_BANNER_TEXT: PROC;
     1128    23175    2   DCL BNR$ PTR;
     1129    23176    2   DCL SPARE CHAR (240) ALIGNED; /* Hope the text of a banner never
     1130    23177                                         exceeds 240 chars. */
     1131    23178    2   DCL ENTRIES UBIN;
     1132    23179    2   DCL TEXT$ PTR;
     1133    23180    2   DCL I SBIN ALIGNED;
     1134    23181    2   DCL J UBIN ALIGNED;
     1135    23182    2   DCL FIRST UBIN ALIGNED;
     1136    23183    2   DCL LAST UBIN ALIGNED;
     1137    23184    2   DCL F UBIN ALIGNED;
     1138    23185    2   DCL L UBIN ALIGNED;
     1139    23186    2   DCL MODIFIER UBIN ALIGNED;  /* Offset to first element */
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:194  
     1140    23187    2   DCL CHRTEXT CHAR (240) BASED;
     1141    23188
     1142    23189    2   BNR$ = PINCRW (PTR$, PTR$->FS$FORMREC.BANRDISP);
     1143    23190    2   ENTRIES = BNR$-> FS$BANNER.NUM_ENTRIES - 1; /* Make it 0 based */
     1144    23191
     1145    23192    3   DO I = 0 TO ENTRIES;
     1146    23193    3      IF (BNR$->FS$BANNER.ENTRIES.TYPE (I) = 0/*TEXT*/)
     1147    23194    4      THEN DO;
     1148    23195    4         TEXT$ = PINCRW (BNR$, BNR$->FS$BANNER.ENTRIES.DISP (I));
     1149    23196    4         MODIFIER = BNR$->FS$BANNER.ENTRIES.DISP (I);
     1150    23197    4         I = ENTRIES + 1; /* Force us out of the loop */
     1151    23198    4         END;
     1152    23199    3      END;
     1153    23200
     1154    23201        /* We now have the text, we must reverse the order of the entries */
     1155    23202    2   J = 0;  /* J will indicate where to insert the text into spare */
     1156    23203    2   LAST = 0; /* Last will track our new disp values */
     1157    23204    2   FIRST = 0; /* First will track the new entry to get the count fixed.*/
     1158    23205    2   SPARE = ' ';
     1159    23206    3   DO I = ENTRIES DOWNTO 0;
     1160    23207    3      IF (BNR$->FS$BANNER.ENTRIES.TYPE (I) = 0/*TEXT*/)
     1161    23208    4      THEN DO;
     1162    23209              /* Must move whole words */
     1163    23210    4         L = BNR$->FS$BANNER.ENTRIES.COUNT(I);
     1164    23211    5         DO WHILE (MOD (L,4) ~= 0);
     1165    23212    5            L = L + 1;
     1166    23213    5            END;
     1167    23214    4         F = (BNR$->FS$BANNER.ENTRIES.DISP (I) - MODIFIER) * 4;
     1168    23215
     1169    23216    4         SUBSTR (SPARE, J, L) = SUBSTR (TEXT$->CHRTEXT, F, L);
     1170    23217    4         J = J + L;
     1171    23218
     1172    23219    4         IF (LAST = 0)
     1173    23220    5         THEN DO;
     1174    23221    5            BNR$-> FS$BANNER.ENTRIES.DISP (I) = MODIFIER;
     1175    23222    5            LAST = BNR$-> FS$BANNER.ENTRIES.COUNT (I);
     1176    23223    5            END;
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:195  
     1177    23224    5         ELSE DO;
     1178    23225    6            DO WHILE (MOD (LAST,4) ~= 0);
     1179    23226    6               LAST = LAST + 1;
     1180    23227    6               END;
     1181    23228    5            BNR$-> FS$BANNER.ENTRIES.DISP (I) = MODIFIER + (LAST/4);
     1182    23229    5            LAST = LAST + BNR$-> FS$BANNER.ENTRIES.COUNT (I);
     1183    23230    5            END;
     1184    23231
     1185    23232    4         END;
     1186    23233    3      END;
     1187    23234    2   J = J - 1;  /* J has the count of text that has to be moved */
     1188    23235    2   SUBSTR (TEXT$->CHRTEXT, 0, J) = SUBSTR (SPARE, 0, J);
     1189    23236
     1190    23237    2   RETURN;
     1191    23238    2   END REVERSE_BANNER_TEXT;
     1192    23239    1   END ASF$BUILD_FORM;
     1193    23240        %EOD;

