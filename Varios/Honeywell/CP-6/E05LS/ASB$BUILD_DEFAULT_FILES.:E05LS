             21475    1             'BDFGHJ7x0noprstwLz123456K89.,()M0PQUVWXY',
             21476    1             'Z:;?!%gSyACEINOR0Tabcdef#hijklm@$=&/''\^_',
             21477    1             '0"<>[]7x~noprstw{z123456089.,()}|`-*+quv')
