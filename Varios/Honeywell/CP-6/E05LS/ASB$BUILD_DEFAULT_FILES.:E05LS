             21580    1           3 VLPPLATEN,                    /* SEE KI_CP6_C FOR DESC              */
             21581    1             4 TL,
             21582    1               5 T UBIN BYTE UNAL /*(8)/**/,
             21583    1               5 L UBIN BYTE UNAL /*(16)/**/,
             21584    1             4 WIDTH UBIN BYTE UNAL /*(0)/**/,
             21585    1             4 LENGTH UBIN BYTE UNAL /*(0)/**/,
             21586    1             4 LIMBO UBIN BYTE UNAL /*(0)/**/,
             21587    1             4 FIRSTLINE UBIN BYTE UNAL /*(0)/**/,
             21588    1             4 * BIT(8) /*('00000000'B)/**/,
             21589    1             4 EXTWID BIT(1) /*('0'B)/**/,
             21590    1             4 LPI UBIN BYTE UNAL /*(6)/**/,
             21591    1             4 * (0:9) UBIN BYTE UNAL /*(0*0)/**/,
