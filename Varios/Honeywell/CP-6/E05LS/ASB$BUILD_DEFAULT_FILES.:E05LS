      170    15458    2      KL$TREC.KL$FETS.PPRIV(I) = KL$TREC.KL$FETS.PPRIV(I) | %PPR_PADMIN#;
      171    15459    2      END;
      172    15460    1   CALL ASB$WRITE_USERS( AS_USERS );
      173    15461    1   CALL ASB$WRITE_HLP( TREC$, ARS );
