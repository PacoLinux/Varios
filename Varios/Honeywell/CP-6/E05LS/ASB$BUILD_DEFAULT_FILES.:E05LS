      877    21482        %FS$BAN_INIT (NAME = FS$BANNER,
      878    21483                      STCLASS = "BASED");
             21484        %LSET STR='BASED        ';
             21485        %IF
             21486         '1'B;
             21487    1   DCL 1 FS$BANNER BASED,
             21488    1         2 CHARGE UBIN BYTE UNAL,          /* DO WE CHARGE FOR BANNER            */
             21489    1         2 ALIGNCTL REDEF CHARGE UNAL,
             21490    1           3 EVERY BIT(1) UNAL,
             21491    1           3 * BIT(8) UNAL,
             21492    1         2 REPEATS UBIN BYTE UNAL,         /* NUM OF TIMES TO PRINT THIS BANNER  */
             21493    1         2 INTR UBIN BYTE UNAL,            /* ARE INTRINSICS  ALLOWED            */
             21494    1         2 NUM_ENTRIES UBIN BYTE UNAL,     /* NUM OF ENTRIES IN THE TABLE        */
             21495    1         2 ENTRIES(0:0),
             21496    1           3 ROW UBIN BYTE UNAL,           /* START ROW                          */
             21497    1           3 COL UBIN BYTE UNAL,           /* START COL                          */
             21498    1           3 FONT UBIN BYTE UNAL,          /* FONT TO BE USED                    */
             21499    1           3 STRIKES UBIN BYTE UNAL,       /* NUMBER OF PRINT STRIKES            */
             21500    1           3 ICSH UBIN BYTE UNAL,          /* INTER CHAR SPACING HORZ.           */
             21501    1           3 REPS UBIN BYTE UNAL,          /* NUM OF TIME TO PRINT THIS ENTRY    */
             21502    1           3 RINC UBIN BYTE UNAL,          /* ROW INCERMENT PER REPEAT           */
             21503    1           3 CINC UBIN BYTE UNAL,          /* COL INCERMENT PER REPEAT           */
             21504    1           3 TYPE UBIN BYTE UNAL,     /* TYPE OF ENTRY(PLAIN TEXT, INTRINIC)     */
             21505    1           3 DISP UBIN HALF UNAL,          /* DISP(IN WORDS) TO TEXT(IF ANY)     */
             21506    1           3 B REDEF DISP,
             21507    1             4 START UBIN BYTE UNAL,
             21508    1             4 LEN UBIN BYTE UNAL,
             21509    1           3 COUNT UBIN BYTE UNAL,         /* COUNT OF TEXT(IF ANY)              */
             21510    1           3 CHR REDEF COUNT CHAR(1),      /* OR CHAR TO BE REPEATED ACROSS PAGE */
             21511    1           3 TOTLEN UBIN(27) UNAL,         /* TOTLEN OF ENTRY(INTERNAL)          */
             21512    1           3 MARK UBIN BYTE UNAL;          /* MARK(INTERNAL)                     */
             21513        %ELSE;
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:207  
             21538        %ENDIF;
      879    21540        %FS$FORMREC (NAME = FS$FORM,
      880    21541                     STCLASS = "BASED");
             21542        %LSET STR='BASED        ';
             21543        %IF
             21545         '0'B;
             21547        %ELSE;
             21548        %LSET INIT=/*;
             21549        %ENDIF;
             21550        /*
             21551        */
             21552    1   DCL 1 FS$FORM BASED ALIGNED,
             21553    1         2 FNAME CHAR(6) /*('  ')/**/, /* FORM NAME           */
             21554    1         2 ACCTDISP UBIN HALF UNAL /*(0)/**/,
             21555    1         2 SETUPDISP UBIN HALF UNAL /*(0)/**/, /* WD DISP TO SETUP */
             21556    1         2 ACCSDISP UBIN HALF UNAL /*(0)/**/, /* DISP TO ACCESS */
             21557    1         2 BANRDISP UBIN HALF UNAL /*(0)/**/, /* DISP TO BANNER */
             21558    1         2 ALGNDISP UBIN HALF UNAL /*(0)/**/, /* DISP TO ALGNMENT */
             21559    1         2 FLAGS BIT(7) /*('0000000'B) /**/, /* SETABLE BUT NOT USED */
             21560    1         2 * BIT(1) /*('0'B)/**/, /* THIS BIT IS FREE            */
             21561    1         2 GRAPHIC BIT(1) /*('0'B)/**/, /* GRAPHIC FORM   */
             21562        /**/
             21563        /*  LEGAL TYPE OF DEVICES FOR THIS FORM */
             21564        /*   The LGLTYPS field here is duplicated in the FS$CHARSET record
             21565             and when this macro is changed, this one should be checked!
             21566        */
             21567        /**/
             21568    1         2 LGLTYPS,
             21569    1           3 LPOK BIT(1) /*('0'B)/**/, /* LEGAL ON LP           */
             21570    1           3 TSOK BIT(1) /*('0'B)/**/, /* LEGAL ON TS           */
             21571    1           3 ELSOK BIT(1) /*('0'B)/**/, /* LEGAL ON ALL BUT TS+LP */
             21572    1           3 ONEOK BIT(1) /*('0'B)/**/, /* LEGAL ON TYP=LTYP   */
             21573    1           3 * BIT(5) /*('00000'B)/**/,
             21574    1           3 LTYP CHAR(2) /*(' ')/**/, /* LEGAL IF ONEOK=YES   */
             21575    1         2 ATTR BIT(18) UNAL /*('000006'O) /**/,
             21576    1         2 SOFTFORM,                       /*SOFT VFU/PLATEN INFO                */
             21577        /**/
PL6.E3A0      #003=ASF$BUILD_FORM File=ASB$BUILD_DEFAULT_FILES.:E05TSI           TUE 07/29/97 08:58 Page:208  
