      136    12429    5               DO_FLAG(L_ALTERABLE_BANNER9) = '1'B;
      137    12430    5               SELECT(L_ALL);
      138    12431    5               DOFLAG.USER = DOFLAG.USER | '003777777777777777777777'O;
      139    12432    5               DOFLAG.FE_MODE = DOFLAG.FE_MODE | '777777777760'O;
      140    12433    5               DO_FLAG(L_MAXENQ) = '1'B;
      141    12434    5               MORE = '0'B;
      142    12435    5               SELECT(L_END);
      143    12436    5               MORE = '0'B;
      144    12437    5               SELECT(ELSE);
      145    12438    5            DO_FLAG(ND$->P$B.CODE) = '1'B;
      146    12439    5               END;                  /* End case                           */
      147    12440    4         I = I + 1;
      148    12441    4            END;
      149    12442    3         SELECT( HELP_VAL );
      150    12443    3         CALL ASH$HELP ;
      151    12444    3         SELECT( QUESTION_VAL );
      152    12445    3         CALL ASH$QUEST;
      153    12446    3         SELECT( DQUESTION_VAL );
      154    12447    3         CALL ASH$DQUEST ;
      155    12448    3         END;
      156    12449    3      DO WHILE('0'B);
      157    12450    3   PARSE_ALT: MERR_CODE = P_PCB.ERROR;
      158    12451    3         MERR_CODE.SEV = 0;
      159    12452    3         CALL ASH$ERROR(MERR_CODE);
      160    12453    3         END;
      161    12454    2      END;
      162    12455    1   IF NOT (DO_FLAG(L_BATCH)
      163    12456    1        OR DO_FLAG(L_GHOST)
      164    12457    1        OR DO_FLAG(L_INT)
      165    12458    1        OR DO_FLAG(L_TP)
      166    12459    1        OR DO_FLAG(L_FE))
      167    12460    2   THEN DO;                       /* Default is all modes get reported  */
      168    12461    2      DO_FLAG(L_BATCH) = AS_TRUE;
      169    12462    2      DO_FLAG(L_GHOST) = AS_TRUE;
      170    12463    2      DO_FLAG(L_INT) = AS_TRUE;
      171    12464    2      DO_FLAG(L_TP) = AS_TRUE;
      172    12465    2      DO_FLAG(L_FE) = AS_TRUE;
PL6.E3A0      #001=ASL$LIST_OPTS File=ASL$EXT.:E05TSI                            TUE 07/29/97 09:57 Page:6    
      173    12466    2      END;
      174    12467    1   IF NOT (DO_FLAG(L_FEUSER)
      175    12468    1        OR DO_FLAG(L_FECG)
      176    12469    1        OR DO_FLAG(L_FEHANDLR)
      177    12470    1        OR DO_FLAG(L_FEGHOST))
      178    12471    2   THEN DO;                       /* Default is all modes get reported  */
      179    12472    2      DO_FLAG(L_FEUSER) = AS_TRUE;
      180    12473    2      DO_FLAG(L_FECG) = AS_TRUE;
      181    12474    2      DO_FLAG(L_FEHANDLR) = AS_TRUE;
      182    12475    2      DO_FLAG(L_FEGHOST) = AS_TRUE;
      183    12476    2      END;
      184    12477    1   RETURN;
      185    12478    1   END ASL$LIST_OPTS;
