      707    13627    1   BUILD_HDR: PROC( HDR_LEN, STOP );
      708    13628    2   DCL HDR_LEN SBIN;
      709    13629    2   DCL STOP UBIN;
      710    13630    2   DCL CUR_IDX SBIN;
      711    13631    2   DCL MORE BIT(1) ALIGNED;
      712    13632    2   DCL NEXT UBIN WORD ;
      713    13633    2   CUR_IDX = ST_IDX;
      714    13634    2   CALL INSERT(OUT_BUF,0,3,' ');
      715    13635    2   HDR_LEN = 3;
      716    13636    2   MORE = '1'B;
      717    13637    3   DO WHILE(MORE);
      718    13638    3      NEXT = OPT_TBL.ORDR( CUR_IDX ) ;
      719    13639    4      DO WHILE (~DO_FLAG(NEXT) OR OPT_TBL.LEN(NEXT)=0);
      720    13640    4         CUR_IDX = CUR_IDX + 1;
      721    13641    4         NEXT = OPT_TBL.ORDR( CUR_IDX ) ;
      722    13642    4         END;
      723    13643    3      IF CUR_IDX < STOP
      724    13644    4      THEN DO;
      725    13645    4         IF HDR_LEN=3
      726    13646    4         THEN
      727    13647    4            BOGU = OPT_TBL.BOGU(NEXT);
      728    13648    4         IF ((HDR_LEN + OPT_TBL.LEN(NEXT)) <= LO_WIDTH
      729    13649    4              AND BOGU = OPT_TBL.BOGU(NEXT))
      730    13650    4              OR HDR_LEN = 3
      731    13651    5         THEN DO;
      732    13652    5            CALL INSERT(OUT_BUF,HDR_LEN,OPT_TBL.LEN(NEXT),
      733    13653    5                 OPT_TBL.HDR(NEXT));
      734    13654    5            HDR_LEN = HDR_LEN + OPT_TBL.LEN(NEXT);
      735    13655    5            FIELDS = FIELDS + 1;
      736    13656    5            CUR_IDX = CUR_IDX + 1;
      737    13657    5            END;
      738    13658    4         ELSE
      739    13659    4            MORE = '0'B;
      740    13660    4         END;
      741    13661    3      ELSE
      742    13662    3         MORE = '0'B;
      743    13663    3      END;
PL6.E3A0      #002=ASL$LIST_OUT File=ASL$EXT.:E05TSI                             TUE 07/29/97 09:59 Page:60   
      744    13664    2   NXT_IDX = CUR_IDX;
      745    13665    2   RETURN;
      746    13666    2   END BUILD_HDR;
