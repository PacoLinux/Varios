     2762    15742    6               ELSE
     2763    15743    6                  FMT_VEC(BOG_IDX*2+1) = VECTOR(NO_STR);
     2764    15744    6               END; /*  Loop through femodes  */
     2765    15745    5            END;  /*  TBL.LEN > 0  */
     2766    15746    4         FDS.NVECS# = 8;
     2767    15747    4         CLASS = C_MODE_TEXT;
     2768    15748    4         CALL OUTPUT_FMT;
     2769    15749    4         END; /*  Non blank hdr  */
     2770    15750    3      ST_IDX = ST_IDX + 1;
