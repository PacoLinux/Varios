      786    13706    6               NEXT = OPT_TBL.ORDR(CUR_IDX) ;
      787    13707    6               END;
      788    13708    5            SBINS(SBIN_IDX) = NEXT_TAB;
      789    13709    5            FMT_VEC(FDS.NVECS#) = VECTOR(SBINS(SBIN_IDX)); /* Next tab stop */
      790    13710    5            SBIN_IDX = SBIN_IDX + 1;
      791    13711    5            FDS.NVECS# = FDS.NVECS# + 1;
      792    13712    5            CALL INSERT(FMT_BUF,FDS.FMT_.BOUND+1,3,'%N-'); /* Tab over   */
      793    13713    5            FDS.FMT_.BOUND = FDS.FMT_.BOUND+3;
      794    13714    5            CALL INSERT_FIELD;
      795    13715    5            NEXT_TAB = NEXT_TAB + OPT_TBL.LEN(NEXT);
      796    13716    5            CUR_IDX = CUR_IDX + 1;
      797    13717    5            END;
      798    13718    4         CALL X$FORMAT(FDS);
      799    13719    4         END;
      800    13720    3      END;
      801    13721    2   RETURN;
      802    13722        %EJECT;
