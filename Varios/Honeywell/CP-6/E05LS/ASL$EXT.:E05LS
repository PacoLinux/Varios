     1744    14724    4         FDS.FMT_.BOUND = CNT1 + 2;
     1745    14725    4         END;   /*  Suboption mode  */
     1746    14726    4      ELSE DO;
     1747    14727    4         FMT_BUF = SUBSTR (JCL_OPT_TBL.HDR(NEXT),0,CNT1) ;
     1748    14728    4         FDS.FMT_.BOUND = CNT1 - 1;
     1749    14729    4         END;   /*  Option mode  */
     1750    14730
     1751    14731    4      DO SELECT(NEXT);
     1752    14732
     1753    14733    4         SELECT(L_ACCESS);
             14733                  /* Access mode=yesno,mode=yesno,mode=yesno,mode=yesno,mode=yesno  */
