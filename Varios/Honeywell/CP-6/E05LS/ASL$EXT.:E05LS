PL6.E3A0      #002=ASL$LIST_OUT File=ASL$EXT.:E05TSI                             TUE 07/29/97 09:59 Page:74   
     1200    14120    1   LIST_KEY: PROC;
     1201    14121
     1202    14122    2   DCL KEYS BIT(72) STATIC;
     1203    14123    2   DCL KEY_A(0:71) REDEF KEYS BIT(1) UNAL;
     1204    14124    2   DCL TAB UBIN STATIC INIT( 4 );
     1205    14125    2   DCL LEN UBIN STATIC;
     1206    14126    2   DCL I UBIN STATIC;
     1207    14127    2   DCL 1 ALL_KEYS CONSTANT,
     1208    14128    2          2 * BIT(36) INIT('777777777777'O),
     1209    14129    2          2 * BIT(36) INIT('777777777777'O);
     1210    14130
     1211    14131
     1212    14132    2   CALL ASB$WRITE_LO( AS_BLANK, 1 );
     1213    14133    2   CALL ASB$WRITE_LO( OPT_TBL.HDR(L_KEY), 6 );
     1214    14134    2   KEYS = AS_USERS.KEY;
     1215    14135    2   IF KEYS = AS_FALSE THEN
     1216    14136    2      CALL ASB$WRITE_LO('    NONE', 8);
     1217    14137    2   ELSE
     1218    14138    2      IF KEYS = ALL_KEYS THEN
     1219    14139    2         CALL ASB$WRITE_LO('    ALL', 7);
     1220    14140    3      ELSE DO;
     1221    14141    3         CALL FLUSH;
     1222    14142    4         DO I = 1 TO 72;
     1223    14143    5            IF KEY_A(I-1) THEN DO;
     1224    14144                    %NUMBER( NUM1=I );
             14152    5         CALL ASB$BINCHAR( AS_FIELD1.TEXT, 0, -1, I );
     1225    14174    5               LEN = AS_FIELD1.COUNT + 1;
     1226    14175    5               IF TAB + LEN > LO_WIDTH THEN CALL FLUSH;
     1227    14176    5               CALL INSERT( OUT_BUF, TAB, LEN, SUBSTR(AS_FIELD1.TEXT,0,AS_FIELD1.COUNT)
             14176                        , ',' );
     1228    14177    5               TAB = TAB + LEN;
     1229    14178    5               END;
     1230    14179    4            END;
     1231    14180    3         CALL FLUSH;
     1232    14181    3         END;
     1233    14182    2   RETURN;
     1234    14183
PL6.E3A0      #002=ASL$LIST_OUT File=ASL$EXT.:E05TSI                             TUE 07/29/97 09:59 Page:75   
     1235    14184
     1236    14185    2   FLUSH: PROC;
     1237    14186
     1238    14187    4   IF TAB > 4 THEN DO;
     1239    14188    4      TAB = TAB - 1;
     1240    14189    4      CALL ASB$WRITE_LO( OUT_BUF, TAB );
     1241    14190    4      END;
     1242    14191    3   OUT_BUF = AS_BLANK;
     1243    14192    3   TAB = 4;
     1244    14193
     1245    14194    3   END FLUSH;
     1246    14195
     1247    14196    2   END LIST_KEY;
