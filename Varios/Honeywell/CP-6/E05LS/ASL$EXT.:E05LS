     1159    14079    4      FMT_VEC(FDS.NVECS#) = VECTOR( AS$PROJ.SKELETON );
     1160    14080    4      CLASS = C_TEXT;
     1161    14081    4      END;                           /* End case ... finally               */
     1162    14082    4   DO SELECT(CLASS);
     1163    14083    4      SELECT(C_TEXT);
     1164    14084    4      FDS.NVECS# = FDS.NVECS# + 1;
     1165    14085    4      CALL INSERT(FMT_BUF,FDS.FMT_.BOUND+1,3,'%>A');
     1166    14086    4      FDS.FMT_.BOUND = FDS.FMT_.BOUND + 3;
     1167    14087    4      SELECT(C_SBIN);
     1168    14088    4      FMT_VEC(FDS.NVECS#) = VECTOR(SBINS(SBIN_IDX));
     1169    14089    4      FDS.NVECS# = FDS.NVECS# + 1;
PL6.E3A0      #002=ASL$LIST_OUT File=ASL$EXT.:E05TSI                             TUE 07/29/97 09:59 Page:73   
     1170    14090    4      SBIN_IDX = SBIN_IDX + 1;
     1171    14091    4      CALL INSERT(FMT_BUF,FDS.FMT_.BOUND+1,3,'%+D');
     1172    14092    4      FDS.FMT_.BOUND = FDS.FMT_.BOUND + 3;
     1173    14093    4      SELECT(C_YESNO);
     1174    14094    4      IF YES_NO
     1175    14095    4      THEN
     1176    14096    4         FMT_VEC(FDS.NVECS#) = VECTOR(YES_STR);
     1177    14097    4      ELSE
     1178    14098    4         FMT_VEC(FDS.NVECS#) = VECTOR(NO_STR);
     1179    14099    4      FDS.NVECS# = FDS.NVECS# + 1;
     1180    14100    4      CALL INSERT(FMT_BUF,FDS.FMT_.BOUND+1,3,'%>A');
     1181    14101    4      FDS.FMT_.BOUND = FDS.FMT_.BOUND + 3;
     1182    14102    4      SELECT(C_MONEY);
     1183    14103    4      SVAL = SVAL/100;             /* in pennies                         */
     1184    14104    4      SBINS(SBIN_IDX) = SVAL/100;  /* Dollar amount                      */
     1185    14105    4      SBINS(SBIN_IDX+1) = SVAL - SBINS(SBIN_IDX)*100;
     1186    14106    4      IF SBINS(SBIN_IDX+1)<0
     1187    14107    4      THEN
     1188    14108    4         SBINS(SBIN_IDX+1) = SBINS(SBIN_IDX+1)*-1;
     1189    14109    4      FMT_VEC(FDS.NVECS#) = VECTOR(SBINS(SBIN_IDX));
     1190    14110    4      FMT_VEC(FDS.NVECS#+1) = VECTOR(SBINS(SBIN_IDX+1));
     1191    14111    4      FDS.NVECS# = FDS.NVECS# + 2;
     1192    14112    4      SBIN_IDX = SBIN_IDX + 2;
     1193    14113    4      CALL INSERT(FMT_BUF,FDS.FMT_.BOUND+1,8,'$%D.%2ZD');
     1194    14114    4      FDS.FMT_.BOUND = FDS.FMT_.BOUND + 8;
     1195    14115    4      END;                           /* End case                           */
     1196    14116    3   RETURN;
     1197    14117    3   END INSERT_FIELD;
     1198    14118    2   END LIST_FIELDS;
