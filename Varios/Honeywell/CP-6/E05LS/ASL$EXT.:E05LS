     2428    15408    3      SVAL = SVAL/100;             /* in pennies              */
     2429    15409    3      SBINS ( 0 ) = SVAL/100;          /* Dollar amount           */
     2430    15410    3      SBINS ( 1 ) = SVAL - SBINS(0)*100;
     2431    15411    3      IF SBINS(1) < 0 THEN
     2432    15412    3         SBINS ( 1 ) = SBINS ( 1 )*-1;
     2433    15413    3      FMT_VEC ( 0 ) = VECTOR ( SBINS ( 0 ) );
     2434    15414    3      FMT_VEC ( 1 ) = VECTOR ( SBINS ( 1 ) );
     2435    15415    3      FDS.NVECS# = 2;
