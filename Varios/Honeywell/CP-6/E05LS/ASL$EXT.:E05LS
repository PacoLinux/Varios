     1721    14701    1   BUILD_JCL_FMT: PROC  (START_INDEX, SUBOPTION_MODE );
     1722    14702    2   DCL START_INDEX            UBIN;
     1723    14703    2   DCL SUBOPTION_MODE         BIT(1);
