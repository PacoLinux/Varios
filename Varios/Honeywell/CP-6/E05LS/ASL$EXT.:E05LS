     1759    14739    4         IF KL_DREC.FLAGS.GHOSTOK THEN
     1760    14740    4            FMT_VEC ( 3 ) = VECTOR ( YES_STR );
     1761    14741    4         ELSE
     1762    14742    4            FMT_VEC ( 3 ) = VECTOR ( NO_STR );
     1763    14743    4         IF KL_DREC.FLAGS.OLOK THEN
     1764    14744    4            FMT_VEC ( 5 ) = VECTOR ( YES_STR );
