       63    12356        /*   Procedure       procedure       procedure       procedure      */
       64    12357        /********************************************************************/
       65    12358    1   DO_FLAG = '0'B;
       66    12359    1   MORE = '1'B;
       67    12360    1   LIST1: P_PCB.ROOT$ = ADDR(LIST_S_OPT);
       68    12361    2   DO WHILE(MORE);
       69    12362    2      REMEMBER LIST1 IN REMEM_VAR;
       70    12363                                                /*  Set prompt                        */
       71    12364    2      CALL ASB$SET_PROMPT ( %PROMPT_OPT ) ;
       72    12365    2      CALL ASB$READ_SI(IN_BUF,ARS);
       73    12366    2      P_PCB.NCHARS = ARS;
       74    12367    2      CALL X$PARSE(P_PCB) ALTRET(PARSE_ALT);
       75    12368    3      DO SELECT(P$B.SUBLK$(0)->P$B.CODE);
       76    12369    3         SELECT(BLANK_LINE_VAL);
       77    12370    3         MORE = '0'B;
       78    12371    3         SELECT(LIST_OPTIONS_VAL);
       79    12372    3         I = 0;
       80    12373    4         DO WHILE(MORE AND I<P$B.SUBLK$(0)->P$B.NSUBLKS);
       81    12374    4            ND$ = P$B.SUBLK$(0)->P$B.SUBLK$(I);
       82    12375    5            DO SELECT(ND$->P$B.CODE);
       83    12376    5               SELECT(L_EXPIRE);
       84    12377    5               DO_FLAG(L_MEXP) = '1'B;
       85    12378    5               DO_FLAG(L_DEXP) = '1'B;
       86    12379    5               SELECT(L_BUDGET);
       87    12380    5               DO_FLAG(L_MAXC) = '1'B;
       88    12381    5               DO_FLAG(L_ACCUM) = '1'B;
       89    12382    5               DO_FLAG(L_MACCT) = '1'B;
       90    12383    5               DO_FLAG(L_BKACC) = '1'B;
       91    12384    5               DO_FLAG(L_BUDLIM) = '1'B;
       92    12385    5               DO_FLAG(L_ICHARGE) = '1'B;
       93    12386    5               DO_FLAG(L_PCHARGE) = '1'B;
       94    12387    5               DO_FLAG(L_BLINDACCTNG) = '1'B;
       95    12388    5               SELECT( L_OUTPUTPRIO );
       96    12389    5               DO_FLAG(L_OUTPUTPRIO) = '1'B;
       97    12390    5               SELECT(L_MEMORY);
       98    12391    5               DO_FLAG(L_MMEM) = '1'B;
