PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:129  
     4515    22094    1   DO_SETUP_CHR_JCL: PROC (PTR$, LENGTH);
     4516    22095
     4517    22096        /*
     4518    22097           In setting up for this routine,  there will be some indexing into
     4519    22098           the KL$CCHR.TXT_UBIN array.  If the index is 0, it means that the
     4520    22099           field we are currently trying to display was not set, or was set
     4521    22100           to none.  So, we will leave the pointer, PTR$, pointing at the
     4522    22101           index and not at the place in the KL$CCHR.TXT_UBIN array where
     4523    22102           the field starts.
     4524    22103           I also assume that PTR$ is pointing at a textc type structure,
     4525    22104           ie. a string with a count as the first byte followed by count
     4526    22105           number of bytes that represent the field we are interested in.
     4527    22106        */
     4528    22107    2   DCL PTR$ PTR;
     4529    22108    2   DCL I  UBIN;
     4530    22109    2   DCL LENGTH   UBIN;
     4531    22110    2   DCL COUNT UBIN;
     4532    22111    2   DCL ABYTE  UBIN BYTE BASED CALIGNED;
     4533    22112    2   DCL OLD_POS UBIN STATIC INIT (0);
     4534    22113    2   DCL NEW_POS UBIN STATIC INIT (0);
     4535    22114    2   DCL STRING  CHAR(512)  STATIC INIT(' ');
     4536    22115
     4537    22116    2   OLD_POS = 0;
     4538    22117    2   NEW_POS = 0;
     4539    22118    2   STRING = ' ';
     4540    22119    2   COUNT = PTR$ -> ABYTE;
     4541    22120    2   PTR$ = PINCRC(PTR$,1);
     4542    22121
     4543    22122    3   IF COUNT ~= 0 THEN DO;
     4544    22123    3      I = 1;
     4545    22124    4      DO WHILE I <= COUNT;
     4546    22125    4         IF (PTR$ -> ABYTE = 127) OR
     4547    22126    5              (PTR$->ABYTE < 32) THEN DO;
     4548    22127                                            /*  Unprintable characters
     4549    22128                 case (  0,  1,  2,  3,  4,  5,  6,  7,
     4550    22129                         8,  9, 10, 11, 12, 13, 14, 15,
     4551    22130                        16, 17, 18, 19, 20, 21, 22, 23,
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:130  
     4552    22131                        24, 25, 26, 27, 28, 29, 30, 31, 127);    */
     4553    22132
     4554    22133    6            IF (LENGTH+4) > LO_WIDTH THEN DO;
     4555    22134    6               FDS.FMT_.BOUND = FDS.FMT_.BOUND - 1;
     4556    22135    6               CALL X$FORMAT (FDS) ;
     4557    22136    6               FDS.NVECS# = 0;
     4558    22137    6               SUBSTR (FMT_BUF,0,17) = '%4-SETUP_CHR = + ';
     4559    22138    6               FDS.FMT_.BOUND = 16;
     4560    22139    6               LENGTH = 18;
     4561    22140    6               END;   /*  out of space on the output line  */
     4562    22141    5            FMT_VEC (FDS.NVECS#) = VECTOR (UNPRINT_TABLE.TXT(PTR$->ABYTE));
     4563    22142    5            FDS.NVECS# = FDS.NVECS# + 1;
     4564    22143    5            SUBSTR (FMT_BUF,FDS.FMT_.BOUND+1,4) = '%>A,';
     4565    22144    5            FDS.FMT_.BOUND = FDS.FMT_.BOUND + 4;
     4566    22145    5            I = I + 1;
     4567    22146    5            PTR$ = PINCRC (PTR$,1);
     4568    22147    5            LENGTH = LENGTH + 4;
     4569    22148
     4570    22149    5            END;   /*  Unprintable characters  */
     4571    22150    4         ELSE
     4572    22151    5            IF (PTR$->ABYTE < 127) THEN DO;
     4573    22152                                         /*  Printable characters  */
     4574    22153    6               IF LENGTH + 3 > LO_WIDTH THEN DO;
     4575    22154    6                  FDS.FMT_.BOUND = FDS.FMT_.BOUND - 1;
     4576    22155    6                  CALL X$FORMAT (FDS) ;
     4577    22156    6                  FDS.NVECS# = 0;
     4578    22157    6                  SUBSTR (FMT_BUF,0,16) = '%4-SETUP_CHR = + ';
     4579    22158    6                  FDS.FMT_.BOUND = 16;
     4580    22159    6                  LENGTH = 18;
     4581    22160    6                  END;   /*  out of space on the output line  */
     4582    22161                            /*  OLD_POS is the last position in string
     4583    22162                                NEW_POS is the next position in string  */
     4584    22163    5               NEW_POS = OLD_POS + 1;
     4585    22164    6               DO WHILE PTR$->ABYTE > 31 AND PTR$ -> ABYTE < 127;
     4586    22165    6                  SUBSTR (STRING,NEW_POS,1) = BINASC (PTR$->ABYTE);
     4587    22166    6                  PTR$ = PINCRC (PTR$,1);
     4588    22167    6                  NEW_POS = NEW_POS + 1;
