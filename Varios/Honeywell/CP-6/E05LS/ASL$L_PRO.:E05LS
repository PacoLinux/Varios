     2757    20336        %EJECT;
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:79   
     2758    20337    1   DO_HEX_LIST: PROC (PTR$);
     2759    20338
     2760    20339        /*
     2761    20340           In setting up for this routine,  there will be some indexing into
     2762    20341           the KL$CCHR.TXT_UBIN array.  If the index is 0, it means that the
     2763    20342           field we are currently trying do display was not set, or was set
     2764    20343           to none.  So, we will leave the pointer, PTR$, pointing at the
     2765    20344           index and not at the place in the KL$CCHR.TXT_UBIN array where
     2766    20345           the field starts.
     2767    20346           I also assume that PTR$ is pointing at a textc type structure,
     2768    20347           ie. a string with a count as the first byte followed by count
     2769    20348           number of bytes that represent the field we are interested in.
     2770    20349        */
     2771    20350    2   DCL PTR$ PTR;
     2772    20351    2   DCL TEMP$ PTR;
     2773    20352    2   DCL I  UBIN;
     2774    20353    2   DCL COUNT UBIN;
     2775    20354    2   DCL POS UBIN ;
     2776    20355    2   DCL STRING  CHAR(256)  BASED;
     2777    20356    2   DCL ABYTE  UBIN BYTE BASED CALIGNED;
     2778    20357
     2779    20358    2   COUNT = PTR$ -> ABYTE;
     2780    20359    2   PTR$ = PINCRC(PTR$,1);
     2781    20360
     2782    20361    3   IF COUNT ~= 0 THEN DO;
     2783    20362    3      I = 1;
     2784    20363    4      DO WHILE I <= COUNT;
     2785    20364    4         IF (PTR$ -> ABYTE = 127) OR
     2786    20365    5              ((PTR$->ABYTE > 0) AND (PTR$->ABYTE < 32)) THEN DO;
     2787    20366                                            /*  Unprintable characters
     2788    20367                 case (  0,  1,  2,  3,  4,  5,  6,  7,
     2789    20368                         8,  9, 10, 11, 12, 13, 14, 15,
     2790    20369                        16, 17, 18, 19, 20, 21, 22, 23,
     2791    20370                        24, 25, 26, 27, 28, 29, 30, 31, 127);    */
     2792    20371
     2793    20372    6            IF FDS.FMT_.BOUND + 4 > SIZEC (FMT_BUF) THEN DO;
     2794    20373    6               FDS.FMT_.BOUND = FDS.FMT_.BOUND - 1;
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:80   
     2795    20374    6               CALL X$FORMAT (FDS) ;
     2796    20375    6               FDS.NVECS# = 0;
     2797    20376    6               SUBSTR (FMT_BUF,0,3) = '%4-';
     2798    20377    6               FDS.FMT_.BOUND = 2;
     2799    20378    6               END;   /*  out of space in FMT_BUF  */
     2800    20379    5            FMT_VEC (FDS.NVECS#) = VECTOR (UNPRINT_TABLE.TXT(PTR$->ABYTE));
     2801    20380    5            FDS.NVECS# = FDS.NVECS# + 1;
     2802    20381    5            SUBSTR (FMT_BUF,FDS.FMT_.BOUND+1,4) = '%>A,';
     2803    20382    5            FDS.FMT_.BOUND = FDS.FMT_.BOUND + 4;
     2804    20383    5            I = I + 1;
     2805    20384    5            PTR$ = PINCRC (PTR$,1);
     2806    20385
     2807    20386    5            END;   /*  Unprintable characters  */
     2808    20387    4         ELSE
     2809    20388    5            IF ((PTR$->ABYTE > 31) AND (PTR$->ABYTE < 127)) THEN DO;
     2810    20389                                         /*  Printable characters  */
     2811    20390    6               IF FDS.FMT_.BOUND + 5 > SIZEC (FMT_BUF) THEN DO;
     2812    20391    6                  FDS.FMT_.BOUND = FDS.FMT_.BOUND - 1;
     2813    20392    6                  CALL X$FORMAT (FDS) ;
     2814    20393    6                  FDS.NVECS# = 0;
     2815    20394    6                  SUBSTR (FMT_BUF,0,3) = '%4-';
     2816    20395    6                  FDS.FMT_.BOUND = 2;
     2817    20396    6                  END; /*  out of space in FMT_BUF  */
     2818    20397    5               POS = 0;
     2819    20398    5               TEMP$ = PTR$;
     2820    20399    6               DO WHILE PTR$->ABYTE > 31 AND PTR$ -> ABYTE < 127;
     2821    20400    6                  PTR$ = PINCRC (PTR$,1);
     2822    20401    6                  POS = POS + 1;
     2823    20402    6                  I = I + 1;
     2824    20403    6                  IF I > COUNT THEN
     2825    20404    6                     EXIT;
     2826    20405    6                  END; /*   Do while a printable character  */
     2827    20406    5               CALL INSERT (FMT_BUF,FDS.FMT_.BOUND+1,5,'''%A'',');
     2828    20407    5               FDS.FMT_.BOUND = FDS.FMT_.BOUND + 5;
     2829    20408    5               FMT_VEC (FDS.NVECS#) = VECTOR (SUBSTR(TEMP$->STRING,0,POS));
     2830    20409    5               FDS.NVECS# = FDS.NVECS# + 1;
     2831    20410    5               END;   /*  Printable characters  */
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:81   
     2832    20411    5            ELSE DO;   /*  Character values > 127  */
     2833    20412    6               IF FDS.FMT_.BOUND + 7 > SIZEC (FMT_BUF) THEN DO;
     2834    20413    6                  FDS.FMT_.BOUND = FDS.FMT_.BOUND - 1;
     2835    20414    6                  CALL X$FORMAT (FDS) ;
     2836    20415    6                  FDS.NVECS# = 0;
     2837    20416    6                  SUBSTR (FMT_BUF,0,3) = '%4-';
     2838    20417    6                  FDS.FMT_.BOUND = 2;
     2839    20418    6                  END; /*  out of space in FMT_BUF  */
     2840    20419    5               CALL INSERT (FMT_BUF,FDS.FMT_.BOUND+1,7,'X''%2H'',');
     2841    20420    5               FDS.FMT_.BOUND = FDS.FMT_.BOUND + 7;
     2842    20421    5               FMT_VEC (FDS.NVECS#) = VECTOR (PTR$->ABYTE);
     2843    20422    5               FDS.NVECS# = FDS.NVECS# + 1;
     2844    20423    5               I = I + 1;
     2845    20424    5               END;
     2846    20425    4         END;  /*  Do while i < count  */
     2847    20426    3      FDS.FMT_.BOUND = FDS.FMT_.BOUND - 1;
     2848    20427    3      END; /*  If not 0  */
     2849    20428    3   ELSE DO;
     2850    20429    3      FMT_VEC (FDS.NVECS#) = VECTOR (NONE_STR);
     2851    20430    3      FDS.NVECS# = FDS.NVECS# + 1;
     2852    20431    3      CALL INSERT (FMT_BUF, FDS.FMT_.BOUND+1,3,'%>A');
     2853    20432    3      FDS.FMT_.BOUND = FDS.FMT_.BOUND + 3;
     2854    20433    3      END;   /*  Else  */
     2855    20434    2   RETURN;
     2856    20435    2   END DO_HEX_LIST;
     2857    20436        %EJECT ;
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:82   
     2858    20437    1   LIST_TABS: PROC ;
     2859    20438    2   DCL I UBIN ;
     2860    20439    2   DCL TABS_HDR CHAR(0) CONSTANT INIT('    ') ;
     2861    20440    2   FMT_VEC(0) = VECTOR(TABS_HDR) ;
