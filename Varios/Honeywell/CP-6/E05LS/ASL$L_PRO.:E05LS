     2674    20253    5         END ;                     /* Do i                               */
     2675    20254    4      CALL EDITSTR( HEX_BUF, 0, 4, MOP_STR, HEX_BUF );
     2676    20255    4      CALL INDEX1R( I, AS_BLANK, HEX_BUF )
     2677    20256    5      WHENRETURN DO;
     2678    20257    5         I = I + 1;
     2679    20258    5         END;
     2680    20259    5      WHENALTRETURN DO;
     2681    20260    5         I = 0;
     2682    20261    5         END ;
     2683    20262    4      CALL INSERT(FMT_BUF,FDS.FMT_.BOUND+1,7-I,'X''',SUBSTR(HEX_BUF,I,4-I),'''') ;
