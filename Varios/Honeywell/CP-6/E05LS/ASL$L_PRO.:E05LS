     4589    22168    6                  I = I + 1;
     4590    22169    6                  LENGTH = LENGTH + 1;
     4591    22170    6                  IF (I > COUNT) OR ((LENGTH+2) >= LO_WIDTH) THEN
     4592    22171    6                     EXIT;
     4593    22172    6                  END; /*   Do while a printable character  */
     4594    22173    5               SUBSTR (STRING,OLD_POS,1) = BINASC(NEW_POS-OLD_POS-1);
