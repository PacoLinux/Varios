PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:75   
     2659    20238    4      SVAL = SBINS(SBIN_IDX)/10 ;
     2660    20239    4      SBINS(SBIN_IDX+1) = SBINS(SBIN_IDX) - SVAL*10 ;
     2661    20240    4      SBINS(SBIN_IDX) = SVAL ;
     2662    20241    4      IF SBINS(SBIN_IDX+1)<0
     2663    20242    4      THEN
     2664    20243    4         SBINS(SBIN_IDX+1) = SBINS(SBIN_IDX+1)*-1 ;
