     2905    20484    2   DCL SOFTWARE CHAR (0) CONSTANT INIT ('SOFTWARE');
     2906    20485    2   DCL POS UBIN; /* Position in array of next element to output */
     2907    20486    2   DCL LINE_POS UBIN;
     2908    20487    2   DCL I UBIN;
     2909    20488    2   DCL FMT_LEN UBIN;
     2910    20489
     2911    20490    3   DO I = 0 TO 280 /* L_PRO_END */;
     2912    20491    3      IF ASL_PRO_OPT_TBL.ORDR (I) = 242 /* L_GR_ESCAPE */
     2913    20492    3      THEN EXIT;
     2914    20493    3      END;  /* That finds where the escape is in the order */
     2915    20494
     2916    20495        /* Now we have to find the next element to output after the escape. */
     2917    20496    3   DO NEXT = I+1 TO 280 /* L_PRO_END */;
     2918    20497    3      IF (PRO_FLAGS (ASL_PRO_OPT_TBL.ORDR (NEXT)) AND
     2919    20498    3           (ASL_PRO_OPT_TBL.LEN (NEXT) ~= 0))
     2920    20499    3      THEN EXIT;
     2921    20500    3      END;
     2922    20501
     2923    20502    2   CUR_IDX = NEXT;
     2924    20503
     2925    20504    2   IF (NEXT = 280 /* L_PRO_END */)
     2926    20505    3   THEN DO;
     2927    20506    3      NXT_IDX = CUR_IDX + 1 ;
     2928    20507    3      END;
     2929    20508    3   ELSE DO;
