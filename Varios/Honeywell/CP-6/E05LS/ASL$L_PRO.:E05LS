     2930    20509    3      NXT_IDX = CUR_IDX ;
     2931    20510    3      END;
     2932    20511        /* That was to set up for the next element, now put out escapes. */
     2933    20512
     2934    20513    2   NUM_ESCAPES = GRA$->GRA$REC.NUM_ESCAPES - 1;
     2935    20514    2   IF (NUM_ESCAPES < 0)
     2936    20515    3   THEN DO;
