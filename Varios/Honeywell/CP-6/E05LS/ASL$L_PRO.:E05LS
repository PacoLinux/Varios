     3011    20590    3      CALL X$FORMAT (FDS);   /* Write out the headings. */
     3012    20591
     3013    20592    3      FDS.NVECS# = 0;
     3014    20593    3      FMT_LEN = 0;
     3015    20594    3      FMT_BUF = ' ';
     3016    20595    3      FDS.FMT_.BOUND = 0;
     3017    20596    3      LINE_POS = 0;
     3018    20597    4      DO I = 0 TO NEXT - 1; /* Next is the number of entries in second */
     3019    20598    4         CALL INSERT (FMT_BUF, FMT_LEN, SIZEC (SINGLHORS_FMT), SINGLHORS_FMT);
     3020    20599    4         FMT_VEC (LINE_POS) = VECTOR (SECOND.TAB (I));
     3021    20600    4         FMT_VEC (LINE_POS + 1) = VECTOR (SECOND.HORS (I));
     3022    20601    4         LINE_POS = LINE_POS + 2;
     3023    20602    4         FDS.NVECS# = FDS.NVECS# + 2;
     3024    20603    4         FMT_LEN = FMT_LEN + SIZEC (SINGLHORS_FMT);
     3025    20604    4         FDS.FMT_.BOUND = FDS.FMT_.BOUND + LENGTHC (SINGLHORS_FMT);
     3026    20605    4         END;
     3027    20606    3      CALL X$FORMAT (FDS);
     3028    20607    3      END;
     3029    20608
     3030    20609    2   FMT_LEN = 0;
     3031    20610    2   FMT_BUF = ' ';
     3032    20611    2   FDS.FMT_.BOUND = 0;
     3033    20612    2   FDS.NVECS# = 0;
     3034    20613    2   RETURN;
     3035    20614    2   END LIST_GR_ESCAPES;
     3036    20615        %EJECT;
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:87   
     3037    20616        /* This routine lists the key definition block of profiles.  See the
     3038    20617           file AS_FUNKEY_DEFS_C fo a description of this block.  This routine
     3039    20618           is self contained.  To add new key definitions, a equ must be added
     3040    20619           to KV$INPFNC, the name must be added to the list of FUNKEY_NAMES in
     3041    20620           the position corresponding to the equate for the key (the equate
     3042    20621           must be less than 256) and the nodes in ASP$FUNKEY_NODS must be
     3043    20622           added.  That is all.
     3044    20623        */
     3045    20624    1   LIST_FUNKEY_DEFS: PROC;
     3046    20625    2   DCL J UBIN WORD;
     3047    20626    2   DCL NUM_ON_LINE UBIN WORD;
     3048    20627    2   DCL CUR_KEY SBIN WORD;
     3049    20628    2   DCL ENTRY_LENGTH UBIN STATIC INIT (46);
     3050    20629    2   DCL KEYNAME (0:5)  CHAR (24) ALIGNED;
     3051    20630    2   DCL START UBIN WORD ALIGNED;
     3052    20631    2   DCL LAST UBIN WORD ALIGNED;
     3053    20632    2   DCL KEYNMOD UBIN WORD ALIGNED;
