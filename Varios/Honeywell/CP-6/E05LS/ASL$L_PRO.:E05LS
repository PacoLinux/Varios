     2685    20264    4      SELECT(15 /*C_HEX_LIST */) ;
     2686    20265    4      IF PTR$ = SETUP_CHR$ THEN
     2687    20266    4         PTR$ = PINCRC (PTR$,1);
     2688    20267    4      ELSE
     2689    20268    4         IF PTR$ -> ABYTE ~= 0 THEN
