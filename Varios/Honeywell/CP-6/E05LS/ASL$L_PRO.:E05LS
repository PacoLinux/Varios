     3323    20902    2                    'IGNORE' );
     3324    20903
     3325    20904    2   CALL ASB$WRITE_LO ('   FUNCTION_KEY', 15);
     3326    20905    2   IF (KL$FUNKEY.NUM_DEFS = 0)
     3327    20906    3   THEN DO;
     3328    20907    3      IF NOT JCL THEN
     3329    20908    3         CALL ASB$WRITE_LO ('    None', 8);
     3330    20909    3      RETURN;
     3331    20910    3      END;
     3332    20911
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:95   
     3333    20912    2   TAB_POS (0) = 5;
     3334    20913    3   DO J = 1 TO 5;
     3335    20914    3      TAB_POS (J) = ENTRY_LENGTH + TAB_POS (J - 1);
     3336    20915    3      END;
     3337    20916    2   CUR_KEY = -1;  /* This sets up to inc the current key before each new
     3338    20917                          key is taken. */
     3339    20918    3   DO WHILE (CUR_KEY < KL$FUNKEY.NUM_DEFS - 1);
     3340    20919
     3341    20920    3      FMT_VEC ( 0 ) = VECTOR ( TAB_POS ( 0 ) );
     3342    20921    3      SBIN_IDX = 0;
     3343    20922    3      NUM_ON_LINE = 0;
     3344    20923    3      FDS.NVECS# = 1;
     3345    20924    3      FMT_BUF = '%N-';
     3346    20925    3      FDS.FMT_.BOUND = 2;
     3347    20926    4      DO WHILE ((((NUM_ON_LINE + 1) * ENTRY_LENGTH) <= LO_WIDTH) AND
     3348    20927    4           (CUR_KEY < KL$FUNKEY.NUM_DEFS - 1) );
     3349    20928
     3350    20929    4         MODIFIER(NUM_ON_LINE) = -1;
     3351    20930    4         CUR_KEY = CUR_KEY + 1; /* This starts us with the next key */
     3352    20931              /* Frist we get the name of the key. */
     3353    20932    4         IF (KL$FUNKEY.FCNKEY.FCN_BYTE2 (CUR_KEY) = %KV_INPFNCTYP_CTR)
     3354    20933    5         THEN DO;  /* The key is a line function */
     3355    20934    5            IF (KL$FUNKEY.FCNKEY.FCN_BYTE3 (CUR_KEY) = 1)
     3356    20935    6            THEN DO;  /* code for break */
     3357    20936    6               KEYNAME (NUM_ON_LINE) = 'BREAK';
     3358    20937    6               END;   /* If */
     3359    20938    6            ELSE DO;
     3360    20939    6               KEYNAME (NUM_ON_LINE) = 'Unrecognized key';
     3361    20940    6               END;   /*  Else  */
     3362    20941    5            END;
     3363    20942    4         ELSE IF (KL$FUNKEY.FCNKEY.FCN_BYTE2 (CUR_KEY) = %KV_INPFNCTYP_SPCCHR)
     3364    20943    5            THEN DO;
     3365    20944    5               IF (KL$FUNKEY.FCNKEY.FCN_BYTE3(CUR_KEY) < 33) OR
     3366    20945    5                    (KL$FUNKEY.FCNKEY.FCN_BYTE3(CUR_KEY) = 127) THEN
     3367    20946    5                  KEYNAME (NUM_ON_LINE) = UNPRINT_TABLE.TXT (KL$FUNKEY.FCNKEY.FCN_BYTE3
             20946                           (CUR_KEY));
     3368    20947    5               ELSE
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:96   
     3369    20948    5                  IF (KL$FUNKEY.FCNKEY.FCN_BYTE3(CUR_KEY) < 127)
     3370    20949    6                  THEN DO;  /* It is printable */
     3371    20950    6                     TEMP = BINASC (KL$FUNKEY.FCNKEY.FCN_BYTE3(CUR_KEY));
     3372    20951    6                     CALL CONCAT (KEYNAME (NUM_ON_LINE), '''', SUBSTR (TEMP, 0, 1),
             20951                              '''');
     3373    20952    6                     END; /*  Printable BYTE3  */
     3374    20953    6                  ELSE DO;
     3375    20954    6                     UHEX_NUM = KL$FUNKEY.FCNKEY.FCN_BYTE3 (CUR_KEY);
     3376    20955    7                     DO I = 5 TO 8 ;
     3377    20956    7                        RHEX_BUF(I-5) = HEX_CHARS(HEX_NUM(I)) ;
     3378    20957    7                        END ;                  /* Do i */
     3379    20958    6                     CALL EDITSTR( HEX_BUF, 0, 4, MOP_STR, HEX_BUF );
     3380    20959    6                     CALL INDEX1R( I, AS_BLANK, HEX_BUF )
     3381    20960    7                     WHENRETURN DO;
     3382    20961    7                        I = I + 1;
     3383    20962    7                        END; /*  Whenreturn  */
     3384    20963    7                     WHENALTRETURN DO;
     3385    20964    7                        I = 0;
     3386    20965    7                        END ; /*  Whenaltreturn  */
     3387    20966    6                     CALL CONCAT (KEYNAME (NUM_ON_LINE), 'X''',SUBSTR(HEX_BUF,I,4-I),
             20966                              '''') ;
     3388    20967    6                     END; /*  Hex number  */
     3389    20968    5               END;  /*  BYTE2 = spcchr  */
     3390    20969    4            ELSE IF (KL$FUNKEY.FCNKEY.FCN_BYTE2 (CUR_KEY) = %KV_INPFNCTYP_FNCKEY)
     3391    20970    5               THEN DO;
     3392    20971    5                  KEYNAME (NUM_ON_LINE) = FUNKEY_NAMES (KL$FUNKEY.FCNKEY.FCN_BYTE3 (
             20971                           CUR_KEY));
     3393    20972    5                  END;  /*  BYTE2 = fnckey  */
     3394    20973    4               ELSE IF (KL$FUNKEY.FCNKEY.FCN_BYTE2 (CUR_KEY) = %KV_INPFNCKEY_IGNORE)
     3395    20974    5                  THEN DO;
     3396    20975    5                     KEYNAME (NUM_ON_LINE) = FUNKEY_NAMES (256);
     3397    20976    5                     END;  /*  BYTE2 = ignore  */
     3398    20977    5                  ELSE DO; /* Deal with a modifier for the key. */
     3399    20978    5                     KEYNMOD = KL$FUNKEY.FCNKEY.FCN_BYTE2 (CUR_KEY);
     3400    20979    5                     IF ((KEYNMOD >= %KV_FCNKEY_MODIFIER_BASE) AND
     3401    20980    5                          (KEYNMOD < %KV_SPCCHR_MODIFIER_BASE) )
     3402    20981    6                     THEN DO; /* This is a modifier for a function key */
