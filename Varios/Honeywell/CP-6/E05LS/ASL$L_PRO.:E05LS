PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:22   
      725    18304    1   BUILD_HDR: PROC(HDR_LEN, CMP_CNT) ;
      726    18305    2   DCL HDR_LEN SBIN ;
      727    18306    2   DCL CMP_CNT SBIN ;
      728    18307    2   DCL CUR_IDX SBIN ;
      729    18308    2   DCL MORE BIT(1) ALIGNED ;
      730    18309    2   DCL NEXT UBIN WORD ;
      731    18310    2   CUR_IDX = ST_IDX ;
      732    18311    2   CALL INSERT(OUT_BUF,0,3,' ') ;
      733    18312    2   HDR_LEN = 3 ;
      734    18313    2   MORE = '1'B ;
      735    18314    3   DO WHILE(MORE) ;
      736    18315    3      NEXT = ASL_PRO_OPT_TBL.ORDR( CUR_IDX ) ;
      737    18316    4      DO WHILE ((NEXT < CMP_CNT) AND
      738    18317    4           ((~ PRO_FLAGS (NEXT)) OR
      739    18318    4           (ASL_PRO_OPT_TBL.LEN (NEXT) = 0)));
      740    18319    4         CUR_IDX = CUR_IDX + 1 ;
      741    18320    4         NEXT = ASL_PRO_OPT_TBL.ORDR(CUR_IDX) ;
      742    18321    4         END ;                     /* Do while                           */
      743    18322    3      IF (CUR_IDX = GR_ESC_IDX)
      744    18323    4      THEN DO;
      745    18324    4         NXT_IDX = CUR_IDX;
      746    18325    4         RETURN;
      747    18326    4         END;
      748    18327    3      IF NEXT < CMP_CNT
      749    18328    3      THEN
      750    18329    4      DO ;
      751    18330    4         IF (HDR_LEN + ASL_PRO_OPT_TBL.LEN(NEXT)) <= LO_WIDTH
      752    18331    4              OR HDR_LEN = 3
      753    18332    4         THEN
      754    18333    5         DO ;
      755    18334    5            CALL INSERT(OUT_BUF,HDR_LEN,ASL_PRO_OPT_TBL.LEN(NEXT),
      756    18335    5                 ASL_PRO_OPT_TBL.HDR(NEXT)) ;
      757    18336    5            HDR_LEN = HDR_LEN + ASL_PRO_OPT_TBL.LEN(NEXT) ;
      758    18337    5            FIELDS = FIELDS + 1 ;
      759    18338    5            CUR_IDX = CUR_IDX + 1 ;
      760    18339    5            END ;                 /* Then do                            */
      761    18340    4         ELSE
PL6.E3A0      #001=ASL$DISP_ONE_PROFILE File=ASL$L_PRO.:E05TSI                   TUE 07/29/97 10:26 Page:23   
      762    18341    4            MORE = '0'B ;
      763    18342    4         END ;                    /* Then do                            */
      764    18343    3      ELSE
      765    18344    3         MORE = '0'B ;
      766    18345    3      END ;                        /* Do while                           */
      767    18346    2   IF NEXT = 280 /* L_PRO_END */ - 1
      768    18347    2   THEN NXT_IDX = CUR_IDX + 1 ;
      769    18348    2   ELSE NXT_IDX = CUR_IDX ;
      770    18349    2   RETURN ;
      771    18350    2   END   BUILD_HDR ;
      772    18351        %EJECT ;
