        8        8        %SET LISTSUB='1'B;
        9        9        /*P* NAME: DUA$PMD
       10       10             PURPOSE: HANDLES THE PMD FUNCTION FOR DELTA.
       11       11        */
       12       12        DUA$PMD: PROC;
       13       13    1   DCL I UBIN;
       14       14                                                /**/
       15       15        %INCLUDE XUH_MACRO_C;
       16       99        %INCLUDE DD_SD_M;
       17     1435        %S_D;
       18     2753        %INCLUDE DD_SUBS_C;
       19     3024        %INCLUDE DD_PSUBS_C;
       20     3499        %INCLUDE DD_ERR_C;
       21     3896        %INCLUDE DD$BASED;
       22     5199        %KEEP$CMD (NAME=PMD$CMD);
       23     5218    1   DCL DDA$PLUGH_PMD ENTRY(0);
       24     5219                                                /**/
       25     5220    1        IF S_D.DO$->PMD$CMD.OPTARRY(0)
       26     5221    2        THEN DO;                           /* Stand back, he wants it all! */
       27     5222    3             DO I = (490 + 1) TO 514; /*P@FIRSTSUB+1-P@LASTSUB*/
       28     5223    3                  CALL PMD_DOIT(I) ALTRET(BREAK_HIT);
       29     5224    3                END;
       30     5225    2             RETURN;
       31     5226    2           END;
       32     5227    1        IF S_D.DO$->PMD$CMD.OPTARRY(499 - 490) /*P@DS - P@ALL*/
       33     5228    2        THEN DO;
       34     5229    3             DO I = 491 TO 498;       /*P@AUTO to P@DS8*/
       35     5230    3                  CALL PMD_DOIT(I) ALTRET(BREAK_HIT);
       36     5231    3                END;
       37     5232    2           END;
PL6.E3A0      #001=DUA$PMD File=DUA$PMD.:E05TSI                                  TUE 07/29/97 14:48 Page:2    
       38     5233    1        IF S_D.DO$->PMD$CMD.OPTARRY(503 - 490)
       39     5234    2        THEN DO;
       40     5235    2             CALL PMD_DOIT (505) ALTRET(BREAK_HIT);
       41     5236    2             CALL PMD_DOIT (500) ALTRET(BREAK_HIT);
       42     5237    2             CALL PMD_DOIT (502) ALTRET(BREAK_HIT);
       43     5238    2           END;
       44     5239    2        DO I = (490 + 1) TO 514; /*P@FIRSTSUB-P@LASTSUB*/
       45     5240    2             IF S_D.DO$->PMD$CMD.OPTARRY(I - 490) THEN CALL PMD_DOIT(I) ALTRET(
              5240                      BREAK_HIT);
       46     5241    2           END;
       47     5242    1   BREAK_HIT:
       48     5243    1        RETURN;
       49     5244                                                /**/
       50     5245        /*D* NAME: PMD_DOIT
       51     5246             DESCRIPTION: Called by DUA$PMD to format the sections of the
       52     5247                  PMD report.  Formats headers and calls DDA$DUMP to output
       53     5248                  data.
       54     5249        */
       55     5250    1   PMD_DOIT: PROC(I) ALTRET;
       56     5251    2   DCL TEMP UBIN;
       57     5252    2   DCL J UBIN;
       58     5253    2   DCL DCB$ PTR;
       59     5254    2   DCL 1 DCBADR REDEF DCB$,
       60     5255    2         2 ADR UBIN(18) UNAL,
       61     5256    2         2 * UBIN(18) UNAL;
       62     5257    2   DCL I UBIN;
       63     5258    2   DCL OUT$ PTR;
       64     5259        %INCLUDE CP_6;
       65     5340        %F$DCB;
       66     5397        %INCLUDE B$ROSEG;
       67     5460        %INCLUDE B$JIT;
       68     5750    2   DCL B$ROS$ PTR SYMREF;
       69     5751    2   DCL B$JIT$ PTR SYMREF;
       70     5752    2   DCL DDB$BREAK ENTRY ALTRET;
       71     5753    2   DCL DDQ$WRITE ENTRY(2);
       72     5754    2   DCL DDQ$WRTOF ENTRY;
       73     5755    2   DCL DUA$UNFID ENTRY(3) ALTRET;
PL6.E3A0      #001=DUA$PMD File=DUA$PMD.:E05TSI                                  TUE 07/29/97 14:48 Page:3    
       74     5756    2   DCL DDA$DUMP ENTRY(6) ALTRET;
       75     5757    2   DCL DDE$ERROR ENTRY;
       76     5758                                                /**/
       77     5759    2        OUT$ = ADDR(S_D.OUTPUT);
       78     5760    2        CALL DDQ$WRTOF;
       79     5761    3        DO CASE(I);
       80     5762    3          CASE(491);                    /*P@AUTO*/
       81     5763    3             S_D.OUTPUT = 'AUTO (DS1)';
       82     5764    3             CALL DDQ$WRITE (OUT$, SIZEC('AUTO (DS1)'));
       83     5765    3             GOTO DUMP_DS;
       84     5766    3          CASE(492);                  /*P@COMMON*/
       85     5767    3             S_D.OUTPUT = 'COMMON (DS2)';
       86     5768    3             CALL DDQ$WRITE(OUT$, SIZEC('COMMON (DS2)'));
       87     5769    3             GOTO DUMP_DS;
       88     5770    3          CASE(493,                      /*P@DS3*/
       89     5771    3               494,                      /*P@DS4*/
       90     5772    3               495,                      /*P@DS5*/
       91     5773    3               496,                      /*P@DS6*/
       92     5774    3               497,                      /*P@DS7*/
       93     5775    3               498);                     /*P@DS8*/
       94     5776    3             CALL CONCAT (S_D.OUTPUT, 'DATA SEGMENT ',BINASC(I-493+3+48));
       95     5777    3             CALL DDQ$WRITE(OUT$, SIZEC('DATA SEGMENT 8'));
       96     5778    3   DUMP_DS:  CALL DDA$DUMP(0, 3, I-493+6, '1'B, '1'B, 17000000/*DD_BIGNUM*/) ALTRET(ERR
              5778            );
       97     5779    3          CASE(499);                      /*P@DS*/
       98     5780                                                /* Ignore this one */
       99     5781    3          CASE(500);                    /*P@DATA*/
      100     5782    3             IF B$JIT.PCD = 0
      101     5783    4             THEN DO;
      102     5784    4                  S_D.OUTPUT = 'No DATA present.';
      103     5785    4                  CALL DDQ$WRITE(OUT$, SIZEC('NO DATA PRESENT.'));
      104     5786    4                END;
      105     5787    4             ELSE DO;
      106     5788    4                  S_D.OUTPUT = 'DATA';
      107     5789    4                  CALL DDQ$WRITE(OUT$, SIZEC('DATA'));
      108     5790    4                  CALL DDA$DUMP((B$JIT.DLL-64)*1024, 3, 0, '0'B, '1'B, B$JIT.PCD*1024)
              5790                           ALTRET(ERR);
PL6.E3A0      #001=DUA$PMD File=DUA$PMD.:E05TSI                                  TUE 07/29/97 14:48 Page:4    
      109     5791    4                END;
      110     5792    3          CASE(502);                      /*P@DD*/
      111     5793    3             IF B$JIT.PCDD = 0
      112     5794    4             THEN DO;
      113     5795    4                  S_D.OUTPUT = 'No DYNAMIC DATA present.';
      114     5796    4                  CALL DDQ$WRITE(OUT$, SIZEC('NO DYNAMIC DATA PRESENT.'));
      115     5797    4                END;
      116     5798    4             ELSE DO;
      117     5799    4                  S_D.OUTPUT = 'DYNAMIC DATA';
      118     5800    4                  CALL DDQ$WRITE(OUT$, SIZEC('DYNAMIC DATA'));
      119     5801    4                  CALL DDA$DUMP((B$JIT.DDLL-64)*1024, 3, 0, '0'B, '1'B, B$JIT.PCDD*1024
              5801                           ) ALTRET(ERR);
      120     5802    4                END;
      121     5803    3          CASE(501);                    /*P@DCBS*/
      122     5804    4             DO J = 1 TO B$ROS$->B$ROSEG.NUMDCBS;
      123     5805    4                  DCB$ = B$ROS$->B$ROSEG.DCBPTR$->B$RODCB$(J);
      124     5806    4                  IF DCB$ ~= ADDR(NIL)
      125     5807    5                  THEN DO;
      126     5808    5                       IF J>1 THEN CALL DDQ$WRTOF;
      127     5809    5                       CALL DUA$UNFID(J);
      128     5810    5                       S_D.OUTPUT = ' ';
      129     5811    5                       CALL DDQ$WRITE (OUT$, 1);
      130     5812    5                       TEMP = DCBADR.ADR;
      131     5813    5                       CALL DDA$DUMP(TEMP, 3, 3, '1'B, '1'B, SIZEW(DCB$->F$DCB)) ALTRET
              5813                                (ARET);
      132     5814    6                       DO WHILE('0'B);
      133     5815    6   ARET:                    CALL DDE$ERROR;
      134     5816    6                          END;
      135     5817    5                       CALL DDB$BREAK ALTRET(ALT);
      136     5818    5                     END;
      137     5819    4                END;
      138     5820    3          CASE(503);                     /*P@IISR*/
      139     5821                                                /* Ignore this one */
      140     5822    3          CASE(504);                     /*P@JIT*/
      141     5823    3             S_D.OUTPUT = 'JIT';
      142     5824    3             CALL DDQ$WRITE(OUT$, SIZEC('JIT'));
      143     5825    3             CALL DDA$DUMP(0, 3, 1, '1'B, '1'B, 17000000/*DD_BIGNUM*/) ALTRET(ERR);
PL6.E3A0      #001=DUA$PMD File=DUA$PMD.:E05TSI                                  TUE 07/29/97 14:48 Page:5    
      144     5826    3          CASE(505);               /*P@PROCEDURE*/
      145     5827    3             IF B$JIT.PCP = 0
      146     5828    4             THEN DO;
      147     5829    4                  S_D.OUTPUT = 'No PROCEDURE present.';
      148     5830    4                  CALL DDQ$WRITE(OUT$, SIZEC('NO PROCEDURE PRESENT.'));
      149     5831    4                END;
      150     5832    4             ELSE DO;
      151     5833    4                  S_D.OUTPUT = 'PROCEDURE';
      152     5834    4                  CALL DDQ$WRITE(OUT$, SIZEC('PROCEDURE'));
      153     5835    4                  CALL DDA$DUMP((B$JIT.PLL-64)*1024, 3, 0, '0'B, '0'B, B$JIT.PCP*1024)
              5835                           ALTRET(ERR);
      154     5836    4                END;
      155     5837    3          CASE(506);                   /*P@ROSEG*/
      156     5838    3             S_D.OUTPUT = 'READ ONLY SEGMENT';
      157     5839    3             CALL DDQ$WRITE(OUT$, SIZEC('READ ONLY SEGMENT'));
      158     5840    3             CALL DDA$DUMP(0, 3, 3, '1'B, '1'B, 17000000/*DD_BIGNUM*/) ALTRET(ERR);
      159     5841    3          CASE(507);                     /*P@TCB*/
      160     5842    3             S_D.OUTPUT = 'TCB';
      161     5843    3             CALL DDQ$WRITE(OUT$, SIZEC('TCB'));
      162     5844    3             CALL DDA$DUMP(0, 3, 15, '1'B, '0'B, 17000000/*DD_BIGNUM*/) ALTRET(ERR);
      163     5845    3          CASE(508);                   /*P@PLUGH*/
      164     5846    3             S_D.OUTPUT = 'PLUGH';
      165     5847    3             CALL DDQ$WRITE(OUT$, LENGTHC('PLUGH'));
      166     5848    3             CALL DDA$PLUGH_PMD;
      167     5849    3          CASE(ELSE);
      168     5850                                           /* Do nothing if option as yet undefined */
      169     5851    3          END;                             /* Do case */
      170     5852    2        CALL DDB$BREAK ALTRET(ALT);
      171     5853    2        RETURN;
      172     5854    2   ERR: CALL DDE$ERROR;
      173     5855    2        RETURN;
      174     5856    2   ALT: ALTRETURN;
      175     5857    2   END PMD_DOIT;
      176     5858    1   END DUA$PMD;

