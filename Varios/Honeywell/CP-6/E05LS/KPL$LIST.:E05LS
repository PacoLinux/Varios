      155     4778    1                         VECTOR(FREE_VSECTOR) );
      156     4779    1           CALL KPI$IO_SVC( %KP_LO_WRITE# );
      157     4780        /**/
      158     4781    1           DUMP_VSECTOR_ = '0'B;
      159     4782    1           DUMP_VSECTOR_.HOB = DISKDIR.DUMP_VSEC.HOB;
      160     4783    1           DUMP_VSECTOR_.LOB = DISKDIR.DUMP_VSEC.LOB;
      161     4784    1           IF DUMP_VSECTOR > 0 THEN
