       26     4473        %INCLUDE XUF_ENTRY;
       27     4481        /**/
       28     4482        /*  Entry declarations  */
       29     4483        /**/
       30     4484        %KP_ENTRIES (OMIT=KPL$LIST);
       31     4590        /**/
       32     4591        /*  BASED DATA  */
       33     4592        /**/
       34     4593        %PARSE$OUT    ( NAME    = OUT$BLK,
       35     4594                        STCLASS = BASED );
       36     4639        %EJECT;
PL6.E3A0      #001=KPL$LIST File=KPL$LIST.:E05TSI                                WED 07/30/97 01:20 Page:2    
       37     4640        /**/
       38     4641        /* Format data structure for magic X$WRITE routine */
       39     4642        /**/
       40     4643        /**/
       41     4644        /*  Local goodies  */
       42     4645        /**/
       43     4646        %KP_DISKID (FPTN=DISKID, STCLASS=AUTO);
       44     4658        %KP_DISKDIR (FPTN=DISKDIR, STCLASS=AUTO);
       45     4668    1   DCL FEP_NUM UBIN WORD;
       46     4669    1   DCL DRIVE_NUM UBIN WORD;
       47     4670    1   DCL DUMP_VSECTOR SBIN WORD;
       48     4671    1   DCL 1 DUMP_VSECTOR_ REDEF DUMP_VSECTOR,
       49     4672    1         2 * BIT(20) UNAL,
       50     4673    1         2 HOB UBIN(8) UNAL,
       51     4674    1         2 LOB UBIN(8) UNAL;
       52     4675    1   DCL FREE_VSECTOR SBIN WORD;
       53     4676    1   DCL 1 FREE_VSECTOR_ REDEF FREE_VSECTOR,
       54     4677    1         2 * BIT(20) UNAL,
       55     4678    1         2 HOB UBIN(8) UNAL,
       56     4679    1         2 LOB UBIN(8) UNAL;
       57     4680    1   DCL REV_LEVEL SBIN WORD;
       58     4681    1   DCL 1 REV_LEVEL_ REDEF REV_LEVEL,
       59     4682    1         2 * BIT(28) UNAL,
       60     4683    1         2 LOB UBIN(8) UNAL;
