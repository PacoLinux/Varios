       93     4716    1             FEP_NUM,DRIVE_NUM) ALTRET(CANT_OPEN);
       94     4717        /**/
       95     4718        /*  Open the diskette, get special data areas, and close it.  */
       96     4719        /**/
       97     4720    1           CALL KPX$OPEN( FEP_NUM, DRIVE_NUM ) ALTRET( BAD_ALTRET );
       98     4721    1           CALL KPU$READ_BLOCK( FEP_NUM,
       99     4722    1                                DRIVE_NUM,
      100     4723    1                                %KP_VSEC_DISKID#,
      101     4724    1                                ADDR(DISKID),
      102     4725    1                                %KP_NSEC_DISKID#)
      103     4726    1                                ALTRET( BAD_ALTRET );
      104     4727    1           CALL KPU$READ_BLOCK( FEP_NUM,
      105     4728    1                                DRIVE_NUM,
      106     4729    1                                %KP_VSEC_DISKDIR#,
      107     4730    1                                ADDR(DISKDIR),
      108     4731    1                                %KP_NSEC_DISKDIR#)
      109     4732    1                                ALTRET( BAD_ALTRET );
      110     4733    1           CALL KPX$CLOSE( FEP_NUM, DRIVE_NUM ) ALTRET( BAD_ALTRET );
      111     4734        %EJECT;
PL6.E3A0      #001=KPL$LIST File=KPL$LIST.:E05TSI                                WED 07/30/97 01:20 Page:5    
      112     4735        /**/
      113     4736        /* Pluck out the interesting info from all those structures */
      114     4737        /**/
      115     4738    1           CALL X$WRITE( F_FDS, VECTOR(FMT1),
      116     4739    1                         VECTOR(FEP_NUM),
      117     4740    1                         VECTOR(DRIVE_NUM) );
      118     4741    1           CALL KPI$IO_SVC( %KP_LO_WRITE# );
      119     4742    1           IF DISKID.VERITY ~= 'FUZZY-PIGS' THEN
      120     4743    2              DO;
      121     4744    2                CALL KPE$ERROR (E$NOT_BOOT_DISK);
      122     4745    2                RETURN;
      123     4746    2                END;
      124     4747        /**/
      125     4748    1           IF DISKID.REVISION_LEVEL ~= %KP_REVISION_LEVEL# THEN
      126     4749    2              DO;
      127     4750    2                REV_LEVEL_ = '0'B;
      128     4751    2                REV_LEVEL_.LOB = DISKID.REVISION_LEVEL;
      129     4752    2                CALL X$WRITE( F_FDS, VECTOR(FMT6),
      130     4753    2                              VECTOR(REV_LEVEL),
      131     4754    2                              VECTOR(KP_REVISION_LEVEL));
      132     4755    2                CALL KPI$IO_SVC( %KP_LO_WRITE# );
      133     4756    2                END;
      134     4757        /**/
      135     4758    1           CALL X$WRITE( F_FDS, VECTOR(FMT2),
      136     4759    1                         VECTOR(DISKID.BUILD_DAY),
      137     4760    1                         VECTOR(DISKID.BUILD_DATE),
      138     4761    1                         VECTOR(DISKID.BUILD_TIME) );
      139     4762    1           CALL KPI$IO_SVC( %KP_LO_WRITE# );
      140     4763        /**/
      141     4764    1           CALL X$WRITE( F_FDS, VECTOR(FMT3),
      142     4765    1                         VECTOR(DISKID.DESTINATION_NODE),
      143     4766    1                         VECTOR(SUBSTR(DISKID.SITE_ID,0,36)));
      144     4767    1           CALL KPI$IO_SVC( %KP_LO_WRITE# );
      145     4768        /**/
      146     4769    1           CALL X$WRITE( F_FDS, VECTOR(FMT7),
      147     4770    1                         VECTOR(DISKID.PATCH_WEEK),
      148     4771    1                         VECTOR(DISKID.SYS_VERSION));
PL6.E3A0      #001=KPL$LIST File=KPL$LIST.:E05TSI                                WED 07/30/97 01:20 Page:6    
      149     4772    1           CALL KPI$IO_SVC( %KP_LO_WRITE# );
      150     4773        /**/
      151     4774    1           FREE_VSECTOR_ = '0'B;
      152     4775    1           FREE_VSECTOR_.HOB = DISKDIR.FREE_VSEC.HOB;
      153     4776    1           FREE_VSECTOR_.LOB = DISKDIR.FREE_VSEC.LOB;
