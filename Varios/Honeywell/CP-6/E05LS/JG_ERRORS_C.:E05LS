    8      /* JG ERRORS ARE 1125-1134  */
    9      /**/
   10      /**/
   11      %EQU E$NGHMEM=1125/*E$NGHMEM*/;         /* NO MEMORY AVAILABLE TO START GHOST */
   12      %EQU E$NGHUSR=1126/*E$NGHUSR*/;    /* NO USER SLOTS AVAILABLE TO START GHOST  */
   13      %EQU E$MOCBSY=1127/*E$MOCBSY*/;    /* CANT START GHOST WITH M$KEYIN PENDING   */
   14      %EQU E$BGHNM=1128/*E$BGHNM*/;           /* GHOST NAME 0 OR >12 CHARACTERS     */
   15      %EQU E$CCGT256=1129/*E$CCGT256*/;  /* CCBUF ON M$GJOB EXCEEDS 256 CHARACTERS  */
   16      %EQU E$NGLGN=1130/*E$NGLGN*/;      /* No such user logon */
   17      %EQU E$NGAUTH=1131/*E$NGAUTH*/;    /* User not authorized for that mode */
