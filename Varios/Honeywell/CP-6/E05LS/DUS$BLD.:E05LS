      335     9713    3                  THEN DO;
      336     9714    3                       CALL DDD$SIMEXP( NC$, V,TEMP_FLAG ) ALTRET( ARET );
PL6.E3A0      #001=DUS$BLD File=DUS$BLD.:E05TSI                                  TUE 07/29/97 14:58 Page:11   
      337     9715    3                       DUMP$CMD.NUM = V;
