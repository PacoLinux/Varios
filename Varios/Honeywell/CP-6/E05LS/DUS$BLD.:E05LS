      311     9689             DESCRIPTION: */
      312     9690                                                /**/
      313     9691    1   DDS$BLD_DUMP: ENTRY ALTRET;
      314     9692                                                /**/
      315     9693    1        NCMD$ = S_D.NCMD$;
      316     9694    1        LASTBLT$ = S_D.LASTBLT$;
      317     9695    1        CALL DDS$ATTDIRA( SIZEW( DUMP$CMD ), C_DUMP ) ALTRET( ARET ); /* C_DUMP */
      318     9696    1        NSB = NCMD$ -> POB.NSB - 1;
      319     9697    1        DUMP$CMD.NUM = DD_BIGNUM;          /* DEFAULT SIZE > 16 MEGAWORDS */
      320     9698    2        DO IX = 0 TO NSB;
      321     9699    2             L$ = S_D.D$;
      322     9700    2             NC$ = NCMD$ -> POB.SUBLK$ ( IX );
      323     9701    2             CODE = NC$ -> POB.CODE;
      324     9702    2             IF ( CODE = VREF_CODE )       /* VREF_CODE */
      325     9703    3             THEN DO;
      326     9704    3                  CALL DDS$VREFCOM( NC$ ) ALTRET( ARET );
      327     9705    3                  DUMP$CMD.VREF = '1'B;
      328     9706    3                  IF ( NOT L$ -> ZITEM.FLAGS.MDSE.M )
      329     9707    4                  THEN DO;
