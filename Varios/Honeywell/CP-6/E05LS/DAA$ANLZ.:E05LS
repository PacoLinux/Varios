     1599     3848    5                    END;
     1600     3849    4                 T$=V1$->PBASE$;
     1601     3850    4                 V1$=PINCRW(T$,V2);
     1602     3851    4               CASE(%DA_OP_MULT);
     1603     3852    4                 V1P.WA=V1P.WA*V2;
