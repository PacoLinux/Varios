     1372     3992    1           OUT$=ADDR(TOP);
     1373     3993    2           DO WHILE(OUT.CODE~=%DA_NUMBER_CODE AND OUT.NSUBLKS>0);
     1374     3994    2              OUT$=OUT.SUBLK$(0);
     1375     3995    2              END;
     1376     3996    1           OUT$=OUT.SUBLK$(0);
     1377     3997    2           DO CASE(OUT.CODE);
     1378     3998    2            CASE(%DA_NUMERIC_POS);
     1379     3999    2              NEG='0'B;
     1380     4000    2            CASE(%DA_NUMERIC_NEG);
     1381     4001    2              NEG='1'B;
     1382     4002    2            CASE(%DA_NUMERIC_CUN);
     1383     4003    2              CALL DAA$CUN(VALUE);
     1384     4004    2              IF ADDR(R)~=ADDR(NIL) THEN R=8;
     1385     4005    2              RETURN;
     1386     4006    2            CASE(%DA_NUMERIC_ME);
     1387     4007    2              IF NOT DA_DUMPFILE THEN VALUE=B$JIT.USER;
     1388     4008    2              ELSE VALUE=0;
     1389     4009    2              IF ADDR(R)~=ADDR(NIL) THEN R=8;
     1390     4010    2              RETURN;
     1391     4011    2            END;
     1392     4012    1           OUT$=OUT.SUBLK$(0);
     1393     4013    2           IF OUT.CODE=10 THEN DO;
     1394     4014    2              RADIX=10;
     1395     4015    2              CALL CHARBIN(TEMP,SYM.TEXT);
     1396     4016    2              END;
     1397     4017    2           ELSE DO;
     1398     4018    2              RADIX=OUT.CODE;
     1399     4019    2              IF RADIX=0 THEN RADIX=DA_DFLT_RADIX;
     1400     4020    2              TEMP=0;
     1401     4021    3              DO I=0 TO SYM.COUNT-1;
     1402     4022    3                 CALL INDEX(X,SUBSTR(SYM.TEXT,I,1),SUBSTR(DIGITS,0,RADIX)) ALTRET(
              4022                          BAD_DIGIT);
     1403     4023    3                 TEMP=TEMP*RADIX+X;
     1404     4024    3                 END;
     1405     4025    2              END;
     1406     4026    1           IF NEG THEN VALUE=-TEMP;
     1407     4027    1           ELSE VALUE=TEMP;
PL6.E3A0      #003=DAA$VALUE File=DAA$ANLZ.:E05TSI                               TUE 07/29/97 12:36 Page:259  
     1408     4028    1           IF ADDR(R)~=ADDR(NIL) THEN R=RADIX;
     1409     4029    1           RETURN;
     1410     4030    1   BAD_DIGIT: ;
     1411     4031    1           TEMP=OUT.CPOS+I;
     1412     4032    1           CALL XUR$ERRPTR(TEMP,DCBNUM(M$DO));
     1413     4033    1           CALL DAA$ERRMSG(%E$DA_BAD_DIGIT);
     1414     4034        /*E*    ERROR:   DAA-E$DA_BAD_DIGIT
     1415     4035                MESSAGE: Bad digit.
     1416     4036                                                                           */
