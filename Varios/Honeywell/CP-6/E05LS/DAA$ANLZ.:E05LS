     1552     3801    1   VAL_EXP: PROC ALTRET;
     1553     3802        /**/
     1554     3803    2           I=1;
     1555     3804    3           CALL VALUE(V1,0) WHENALTRETURN DO; ALTRETURN; END;
     1556     3805    3           DO WHILE(I<OUT.NSUBLKS);
     1557     3806    4              CALL VALUE(V2,I+1) WHENALTRETURN DO; ALTRETURN; END;
     1558     3807    4              DO CASE(OUT.SUBLK$(I)->OUT.CODE);
     1559     3808    4               CASE(%DA_OP_PLUS);
     1560     3809    4                 V1=V1+V2;
     1561     3810    4               CASE(%DA_OP_MINUS);
     1562     3811    4                 V1S=V1S-V2S;
     1563     3812    4               CASE(%DA_OP_POINT);
     1564     3813    4                 T$=PINCRW(B$IS$,V1);
