     1566     3815    5                    ALTRETURN;
     1567     3816    5                    END;
     1568     3817    4                 V1=V1$->UBIN36BASE+V2;
     1569     3818    4               CASE(%DA_OP_MULT);
     1570     3819    4                 V1=V1*V2;
     1571     3820    4               CASE(%DA_OP_DIV);
     1572     3821    4                 V1=V1S/V2S;
     1573     3822    4               END;
     1574     3823    3              I=I+2;
     1575     3824    3              END;
     1576     3825    2           VAL=V1;
