     1874        6        DAA$WILDCOMP: PROC(WILDSTR,STR,STRSIZ) ALTRET;
     1875        7    1   DCL 1 WILDSTR,
     1876        8    1         2 L UBIN BYTE UNAL,
     1877        9    1         2 C CHAR(WILDSTR.L);
     1878       10    1   DCL STR CHAR(STRSIZ);
     1879       11    1   DCL STRSIZ UBIN;
     1880       12    1   DCL WX UBIN;
     1881       13    1   DCL WXP UBIN;
     1882       14    1   DCL CX UBIN;
     1883       15    1   DCL CXP UBIN;
     1884       16        /**/
     1885       17        /**/
     1886       18        /**/
     1887       19    1           WX=0;
     1888       20    1           WXP=WILDSTR.L;
     1889       21    1           CX=0;
     1890       22    1           CXP=0;
     1891       23    2           DO WHILE(WX<WILDSTR.L AND SUBSTR(WILDSTR.C,WX)~=' '
     1892       24    2             OR CX<STRSIZ AND SUBSTR(STR,CX)~=' ');
     1893       25    3              IF WX<WILDSTR.L AND SUBSTR(WILDSTR.C,WX,1)='?' THEN DO;
     1894       26    3                 WXP=WX;
     1895       27    3                 END;
     1896       28    3              ELSE DO;
     1897       29    3                 IF WX=WXP+1 THEN CXP=CX;
     1898       30    3                 IF WX>=WILDSTR.L OR CX>=STRSIZ
     1899       31    4                   OR SUBSTR(WILDSTR.C,WX,1) ~= SUBSTR(STR,CX,1) THEN DO;
     1900       32    4                    IF WXP=WILDSTR.L OR CXP=STRSIZ THEN ALTRETURN;
     1901       33    4                    WX=WXP;
     1902       34    4                    CX=CXP;
     1903       35    4                    END;
     1904       36    3                 CX=CX+1;
     1905       37    3                 END;
PL6.E3A0      #008=DAA$WILDCOMP File=DAA$ANLZ.:E05TSI                            TUE 07/29/97 12:37 Page:395  
     1906       38    2              IF WX<WILDSTR.L THEN WX=WX+1;
     1907       39    2              END;
     1908       40    1           RETURN;
     1909       41    1   END DAA$WILDCOMP;
     1910       42        %EOD;

