       21       21    1         2 SIZE UBIN,
       22       22    1         2 OPS(0:15) UBIN,
       23       23    1         2 OPSB(0:15) REDEF OPS,
       24       24    1           3 * BIT(20) UNAL,
       25       25    1           3 UH UBIN(8) UNAL,
       26       26    1           3 LH UBIN(8) UNAL;
       27       27    1   DCL ADDRESS SBIN;
       28       28    1   DCL SEG UBIN;
       29       29    1   DCL DESC# UBIN;
