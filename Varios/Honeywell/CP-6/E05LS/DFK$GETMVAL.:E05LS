      130     5743    3                  S_D.ERROR = E@NOTIMP;    /* E@NOTIMP */
      131     5744    3               CASE(%GD#RSPBADPTR);
      132     5745    3                  S_D.ERROR = E@BADPTR;    /* E@BADPTR */
      133     5746    3               CASE(ELSE);
      134     5747    3                  IF S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.XDELTA AND
      135     5748    3                    GD$MSG.FNC = %GD#RSPBADMSG
      136     5749    4                  THEN DO;
      137     5750    4                       S_D.ERROR = E@BADMSG; /* E@BADMSG */
      138     5751    4                       GOTO ARET;
      139     5752    4                     END;
      140     5753    3                  IF GD$MSG.FNC = %GD#RSPNAK
      141     5754    4                  THEN DO;
      142     5755    4                       IF S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.XDELTA
      143     5756    5                       THEN DO;
      144     5757    5                            TEMP1 = BITBIN(GD$MSG.ERROR_D.ERRCODE);
      145     5758    5                            TEMP2B.B1 = TEMP1B.B1;
      146     5759    5                            TEMP2B.B2 = TEMP1B.B2;
      147     5760    5                            TEMP2B.B3 = TEMP1B.B3;
