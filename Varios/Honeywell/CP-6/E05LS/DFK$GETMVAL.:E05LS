       92     5705    2               DESC# = S_D.MOD_L6.DESC# AND
       93     5706    2               NOT_IS = S_D.MOD_L6.NOT_IS AND
       94     5707    2               ADDRESS >= S_D.MOD_L6.ADDRESS AND
       95     5708    2               ADDRESS < (S_D.MOD_L6.ADDRESS + S_D.MOD_L6.VALUE.SIZE -6 )
       96     5709    3             THEN DO;
       97     5710    3                  OFFSET = ADDRESS - S_D.MOD_L6.ADDRESS;
       98     5711    4                  DO I = OFFSET TO 15;
       99     5712    4                       NVALUE.OPS(I - OFFSET) = S_D.MOD_L6.VALUE.OPS(I);
      100     5713    4                     END;
