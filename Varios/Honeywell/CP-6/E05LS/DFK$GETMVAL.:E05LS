PL6.E3A0      #001=DFK$GETMVAL File=DFK$GETMVAL.:E05TSI                          TUE 07/29/97 14:40 Page:3    
       74     5687    1   DCL DFQ$WRITE_FD ENTRY(1) ALTRET;
       75     5688    1   DCL DUE$ERRMSG   ENTRY;
       76     5689    1   DCL DUE$GERRMSG   ENTRY;
       77     5690
       78     5691    1        IF SEG = 6
       79     5692    2        THEN DO;
       80     5693    2             IF DESC# ~= 0 OR ADDRESS ~= 12
       81     5694    2             THEN
       82     5695    2                  GOTO NOTFEP;
       83     5696    2             NVALUE.SIZE = 2;
       84     5697    2             NVALUE.OPS(0) = S_D.SAVEIC/65536;
       85     5698    2             NVALUE.OPS(1) = MOD (S_D.SAVEIC,65536);
       86     5699    2             RETURN;
       87     5700    2           END;
       88     5701         /* Check to see if contents of evaluated address are already in cache in S_D. */
