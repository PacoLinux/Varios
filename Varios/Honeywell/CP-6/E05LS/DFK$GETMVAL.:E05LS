      115     5728    1          S_D.DO$ -> ALL_ATTACH.CMD = C_MODIFYDW /* C_MODIFYDW */ OR
      116     5729    1          S_D.DO$ -> ALL_ATTACH.CMD = C_EVAL) /* C_EVAL */
      117     5730    1        THEN GD$MSG.DISPMEM_D.DUMPCMD = '1'B;
