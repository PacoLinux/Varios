      111     5724    1        GD$MSG.DISPMEM_D.NOT_IS = NOT_IS;
      112     5725    1        IF S_D.DO$ ~= ADDR(NIL) AND SEG = 3 AND
      113     5726    1          (S_D.DO$ -> ALL_ATTACH.CMD = C_DUMP /* C_DUMP */ OR
