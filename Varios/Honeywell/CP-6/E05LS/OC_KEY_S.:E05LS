     3617     4563        X2S2 (902 /* XTXT2# */) = .Q-
     3618     4564        XTXT3 = (X3S | X3D)
     3619     4565        X3D ( 903 /* XTXT3# */ ) = .DEC-
     3620     4566        X3S = ( X3S1 | X3S2 )
     3621     4567        X3S1 (903 /* XTXT3# */) = .AN+'$_#*:-/?'-
     3622     4568        X3S2 (903 /* XTXT3# */) = .Q-
     3623     4569        XTXT4 = (X4S | X4D)
