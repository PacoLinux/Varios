     3676     4622        XHELP ( 908 /* XTXTC# */ ) = .SYM+'&!.%*+,/@:<=^?'
     3677     4623                                                /**/
     3678     4624                                           /* Dashes are not allowed
     3679     4625                                                on HELP keyins. The dash
     3680     4626                                                is special, representing a range.
     3681     4627                                                (therefore it should not be used
     3682     4628                                                 in a help text string */
     3683     4629                                           /*
     3684     4630                                                XDECn lexemes accept an
     3685     4631                                                unsigned decimal string from
     3686     4632                                                the input command, convert
     3687     4633                                                it to a UBIN(36) value, and
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               112  
     3688     4634                                                then check it to see if the
     3689     4635                                                resultant value is greater
     3690     4636                                                than n bits in length.  If
     3691     4637                                                the size is not exceeded, then
     3692     4638                                                the number is stored in the
     3693     4639                                                output block.  Only the
     3694     4640                                                least significant n bits are
     3695     4641                                                stored, starting on the next
     3696     4642                                                character boundary.       */
     3697     4643
     3698     4644        XDEC9 ( 909 /* XDEC9# */ ) = .DEC-
     3699     4645        XDEC18 ( 910 /* XDEC18# */ ) = .DEC-
     3700     4646        XDEC27 ( 911 /* XDEC27# */ ) = .DEC-
     3701     4647        XDEC36 ( 912 /* XDEC36# */ ) = .DEC-
     3702     4648                                           /*
     3703     4649                                                XDECSn lexemes are similar to
     3704     4650                                                the XDECn lexemes described
     3705     4651                                                above, except for these
     3706     4652                                                differences: If a minus sign
     3707     4653                                                is input then the number is
     3708     4654                                                converted to a SBIN(36)
     3709     4655                                                quantity.  Overflow in n
     3710     4656                                                bits is checked for and
     3711     4657                                                disallowed.  No plus sign is
     3712     4658                                                allowed for positive numbers.
     3713     4659                                                A positive number may NOT
     3714     4660                                                set the sign bit.         */
     3715     4661
     3716     4662        XDECS9 = (XDECS91 | XDECS92)
     3717     4663        XDECS91 ( 914 /* XDECS9# */ ) = .DEC+'-'-
     3718     4664        XDECS92 ( 914 /* XDECS9# */ ) = .DEC-
     3719     4665        XDECS18 = (XDECS181 | XDECS182)
     3720     4666        XDECS181 ( 915 /* XDECS18# */ ) = .DEC+'-'-
     3721     4667        XDECS182 ( 915 /* XDECS18# */ ) = .DEC-
     3722     4668        XDECS27 = (XDECS271 | XDECS272)
     3723     4669        XDECS271 ( 916 /* XDECS27# */ ) = .DEC+'-'-
     3724     4670        XDECS272 ( 916 /* XDECS27# */ ) = .DEC-
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               113  
     3725     4671        XDECS36 = (XDECS361 | XDECS362)
     3726     4672        XDECS361 ( 917 /* XDECS36# */ ) = .DEC+'-'-
     3727     4673        XDECS362 ( 917 /* XDECS36# */ ) = .DEC-
     3728     4674                                           /*
     3729     4675                                                XOCTn lexemes accept valid
     3730     4676                                                octal character strings from
     3731     4677                                                the input command and convert
     3732     4678                                                them to the appropriate binary
     3733     4679                                                equivalent in n bits in the
     3734     4680                                                output block. No negation is
     3735     4681                                                allowed. Overflow over n
     3736     4682                                                bits is not allowed.      */
     3737     4683
     3738     4684        XOCT9 ( 919 /* XOCT9# */ ) = .OCT-
     3739     4685        XOCT18 ( 920 /* XOCT18# */ ) = .OCT-
