     3779     4725        XNUL36 ( 950 /* XNUL36# */ ) = .END
     3780     4726                                           /*
     3781     4727                                                XWSN builds 8 bytes into the
     3782     4728                                                output block, consisting of
     3783     4729                                                4 bytes of the originating
     3784     4730                                                console's TWSN, followed by
     3785     4731                                                4 blanks.                 */
     3786     4732
     3787     4733        XWSN ( 960 /* XWSN# */ ) = .END
     3788     4734                                           /*
     3789     4735                                                XDEV accepts 4 characters
     3790     4736                                                from the input line, validates
     3791     4737                                                it to see if it is of the
     3792     4738                                                form (2 characters of device
     3793     4739                                                type, 2 characters of device
     3794     4740                                                number), concatenates those
     3795     4741                                                4 characters with the TWSN of
     3796     4742                                                 the originating console, and
     3797     4743                                                packs those 8 bytes into the
     3798     4744                                                output block.             */
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               115  
     3799     4745
     3800     4746        XDEV ( 961 /* XDEV# */ ) = .AN-
     3801     4747                                           /*
     3802     4748                                                XGNDEV is just like XDEV
     3803     4749                                                except that it also allows
     3804     4750                                                a generic 2-char device
     3805     4751                                                form (2 characters of device
     3806     4752                                                type, device number blank.) */
     3807     4753
     3808     4754        XGNDEV ( 974 /* XGNDEV# */ ) = .AN-
     3809     4755                                           /*
     3810     4756                                                XPRIO places the mxprio of
     3811     4757                                                the originating console into
     3812     4758                                                the next byte into the
     3813     4759                                                output block.             */
     3814     4760
     3815     4761        XPRIO (962 /* XPRIO# */) = .END
     3816     4762                                           /*
     3817     4763                                                XTERM places the TERMINAL
     3818     4764                                                name of the originating
     3819     4765                                                console into the next 8 bytes
     3820     4766                                                of the output block. If the
     3821     4767                                                console is a SYSTEM console,
     3822     4768                                                then the TERM name returned
     3823     4769                                                will be blanks.           */
     3824     4770
     3825     4771        XTERM (963 /* XTERM# */) = .END
     3826     4772                                           /*
     3827     4773                                                XCATTR places the 36 bits of
     3828     4774                                                the originating console's
     3829     4775                                                input and output attributes
     3830     4776                                                into the next 4 characters
     3831     4777                                                in the output block.      */
     3832     4778
     3833     4779        XCATTR (964 /* XCATTR# */) = .END
     3834     4780                                           /*
     3835     4781                                                XCWSN places the WSN name
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               116  
     3836     4782                                                of the originating console
     3837     4783                                                into the next 8 bytes in the
     3838     4784                                                output block. Note that these
     3839     4785                                                characters may contain a
     3840     4786                                                single trailing question mark
     3841     4787                                                indicating that wildcarding
     3842     4788                                                has been authorized for this
     3843     4789                                                console.                  */
     3844     4790
     3845     4791        XCWSN (965 /* XCWSN# */) = .END
     3846     4792                                           /*
     3847     4793                                                XNAME places the 8 character
     3848     4794                                                LOGON string for the
     3849     4795                                                originating console into the
     3850     4796                                                next 8 characters in the
     3851     4797                                                output block.  Note that it
     3852     4798                                                is possible for these
     3853     4799                                                characters to be blank if
     3854     4800                                                the console was logged on as
     3855     4801                                                part of a terminal.       */
     3856     4802
     3857     4803        XNAME (966 /* XNAME# */) = .END
     3858     4804                                           /*
     3859     4805                                                XBWSN places both the
     3860     4806                                                WSN name and TERM name of the
     3861     4807                                                originating console into the
     3862     4808                                                output block (in 16 bytes).
     3863     4809                                                This lexeme is similar to
     3864     4810                                                saying XCWSN XTERM, except
     3865     4811                                                that if the console is not
     3866     4812                                                authorized for device access
     3867     4813                                                the TERM in the output block
     3868     4814                                                becomes binary zeroes, and
     3869     4815                                                if the console is not
     3870     4816                                                authorized for administrative
     3871     4817                                                access then the WSN in the
     3872     4818                                                output block becomes binary
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               117  
     3873     4819                                                zeroes. Note again that the
     3874     4820                                                TERM name of a SYSTEM console
     3875     4821                                                is blanks.                */
     3876     4822
     3877     4823        XBWSN (967 /* XBWSN# */) = .END
     3878     4824                                           /*
     3879     4825                                                XBTWSN accepts an '@' qualified
     3880     4826                                                string from the input command,
     3881     4827                                                checks to see if it is a legal
     3882     4828                                                TERM name, and builds it into
     3883     4829                                                the next 8 characters in the
     3884     4830                                                output block.  If it is
     3885     4831                                                a valid terminal name, the
     3886     4832                                                TWSN corresponding to that
     3887     4833                                                TERM are built into the
     3888     4834                                                next 4 bytes in the output
     3889     4835                                                block, followed by 4 spaces.*/
     3890     4836
     3891     4837        XBTWSN (968 /* XBTWSN# */) = '@'- .AN+'$_#*:-/'-
     3892     4838                                           /*
     3893     4839                                                XSTA accepts 8 characters
     3894     4840                                                from the input command and
     3895     4841                                                places them in the array
     3896     4842                                                containing the station names
     3897     4843                                                that KEYIN distributes the
     3898     4844                                                resultant output blocks to.
     3899     4845                                                XSTA is therefore interpreted|
     3900     4846                                                to be a valid station on the
     3901     4847                                                CSCG ComGroup.            */
     3902     4848
     3903     4849        XSTA = (XSTAS | XSTAQ)
     3904     4850        XSTAQ (969 /* XSTA# */) = .Q
     3905     4851        XSTAS (969 /* XSTA# */) = .AN+'$_#*:-/'
     3906     4852                                           /*
     3907     4853                                                XNATL places the single byte
     3908     4854                                                of the originating console's
     3909     4855                                                native language identifier
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               118  
     3910     4856                                                into the next byte of the
     3911     4857                                                output block.             */
     3912     4858
     3913     4859        XNATL (970 /* XNATL# */) = .END
     3914     4860                                           /*
     3915     4861                                                XNOT is an unusual lexeme.
     3916     4862                                                If NOT is specified on the
     3917     4863                                                input line where XNOT is
     3918     4864                                                seen, then a 1 is built into
     3919     4865                                                the byte specified by XNOTs
     3920     4866                                                sub_item number.  If XNOT
     3921     4867                                                is used with failure processing
     3922     4868                                                then failure to get a NOT at the
     3923     4869                                                specified point will build a
     3924     4870                                                zero into the output block at
     3925     4871                                                the specified point.  A single
     3926     4872                                                byte is built.  The anomaly
     3927     4873                                                is that 1 means NOT was
     3928     4874                                                specified, 0 means NOT was NOT
     3929     4875                                                specified.  The tilde (~) may
     3930     4876                                                be used in deference to the
     3931     4877                                                word NOT.                 */
     3932     4878
     3933     4879        XNOT = (XNOT1 | XNOT2)
     3934     4880        XNOT1 ( 971 /* XNOT# */) = 'NOT'-
     3935     4881        XNOT2 ( 971 /* XNOT# */) = '~'-
     3936     4882                                           /*
     3937     4883                                                XCGFID accepts a valid
     3938     4884                                                ComGroup fid from the input
     3939     4885                                                command and builds it into the
     3940     4886                                                structure defined in
     3941     4887                                                the INCLUDE file KC$CGBLK
     3942     4888                                                and the macro KC$CGID.  If
     3943     4889                                                the resultant fid is not
     3944     4890                                                ASN=COMGROUP, and error is
     3945     4891                                                printed and the keyin is
     3946     4892                                                rejected.                 */
PARTRGE version E04  Source listing      04:09:55.96 JUL 30 '97 OC_KEY_S                               119  
     3947     4893
     3948     4894        XCGFID (972 /* XCGFID# */) = .FID-
     3949     4895                                           /*
     3950     4896                                                XLPWSN places the 8 character
     3951     4897                                                LP_WSN field from the console
     3952     4898                                                table into the output block.
     3953     4899                                                                          */
     3954     4900        XLPWSN (973 /* XLPWSN# */) = .END
     3955     4901
