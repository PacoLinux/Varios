        1        1        /*M* QBI$IF_INDICATOR:  IF frame-or-field-indicator.  */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QBI$IF_INDICATOR
       10       10              PURPOSE:      Transfer control according to the settings of
       11       11                            various frame and field STATUSes.
       12       12              DESCRIPTION:  Execute an IF_INDICATOR (IFINDR) op-code, whose
       13       13                            argument may be a frame or field.
       14       14        */
       15       15
       16       16        QBI$IF_INDICATOR: PROC ALTRET;
       17       17
       18       18        /*D*  NAME:         QBI$IF_INDICATOR
       19       19              CALL:         CALL QBI$IF_INDICATOR;
       20       20              PARAMETERS:   None.
       21       21              INTERFACE:    Called by QAT$TALLYHO when an IF_INDICATOR (IFINDR)
       22       22                            op-code is encountered.
       23       23              ENVIRONMENT:  Nothing unusual.
       24       24              INPUT:        QI_1,
       25       25                            FPL rununit static data following the op-code.
       26       26              OUTPUT:       Transfers control within the FPL rununit according
       27       27                            to the status which was checked for.
       28       28              SCRATCH:      Various otter stuff.
       29       29              DESCRIPTION:  A block of op-codes is either executed or skipped
       30       30                            according to the status of the argument variable:
       31       31                            the block of op-codes is plunged into if the
       32       32                            condition checked for is true; otherwise, the
       33       33                            block is skipped (there may be an ELSE block, too).
       34       34                            For a frame argument, the status checked is for:
       35       35                            1)  IS ACTIVE.
       36       36                            For a field argument, the status checked is for:
       37       37                            1)  IS ENABLED FOR INPUT;
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:2    
       38       38                            2)  IS ENABLED FOR OUTPUT;
       39       39                            3)  IS ENABLED FOR I-O;
       40       40                            4)  IS PRESENTED;
       41       41                            5)  IS AVAILABLE.
       42       42        */
       43       43
       44       44          %INCLUDE LCP_6;
       45      235
       46      236          %INCLUDE QG_MACRO_M;
       47      373          %INCLUDE QI_EQUATES_E;
       48      732          %INCLUDE QI_PERR_C;
       49      900          %INCLUDE QI_MACROS_M;
       50     5958          %QI$ALL;
       51    17071
       52    17072    1     DCL GZ_FIELD$ PTR;
       53    17073    1     DCL INSTR$ PTR;
       54    17074
       55    17075    1     DCL QCE$ERROR_INTERNAL ENTRY (4);
       56    17076    1     DCL QCF$FIND_CURRENTS ENTRY;
       57    17077    1     DCL QCU$USE_IO ENTRY (1);
       58    17078
       59    17079          %SET LISTMAC = '1'B;
       60    17080
       61    17081          %INCLUDE QCU_USE_M;
       62    17135          %QCU@USE;
             17136            %BEGIN_MACRO;
             17137            %SET LISTDIR = '0'B;
             17150    1     DCL 1 QCU@USE AUTO ALIGNED,
             17151    1           2 CURR_FRAME$ PTR,
             17152                          /* Pointer to DAD of current frame or ADDR (NIL).  */
             17153    1           2 CURR_NODE$ PTR,
             17154                          /* Pointer to current node or ADDR (NIL).  */
             17155    1           2 USE_DEFAULT_IO UBIN,
             17156                          /* For USE procedures which default applies
             17157                             (see corresponding EQUs).  */
             17158    1           2 IS_TIME_TO_SAVE_AWAY_FIELDS BIT(1) ALIGNED,
             17159                          /* If IS_TIME_TO_SAVE_AWAY_FIELDS, then
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:3    
             17160                             save the fields within the bounds of
             17161                             FRAME_START_INDEX, FRAME_STOP_INDEX,
             17162                             FIELD_START_INDEX, and FIELD_STOP_INDEX
             17163                             before calling the USE procedure.  */
             17164    1           2 FRAME_START_INDEX UBIN,
             17165                          /* Foist frame of the CURRENT.FORM$
             17166                             involved with the I-O error.  */
             17167    1           2 FRAME_STOP_INDEX UBIN,
             17168                          /* Larst frame of the CURRENT.FORM$
             17169                             involved with the I-O error.  */
             17170    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             17171                          /* If IS_FIELD_INDEXES_VALID, use them;
             17172                             else use all the fields in the frame.  */
             17173    1           2 FIELD_START_INDEX UBIN,
             17174                          /* Starting index into QI$GZ_FIELD
             17175                             which may have values "pushed".  */
             17176    1           2 FIELD_STOP_INDEX UBIN,
             17177                          /* Stopping index into QI$GZ_FIELD
             17178                             which may have values "pushed".  */
             17179    1           2 IS_CURSOR_POSITION_VALID BIT(1) ALIGNED,
             17180                          /* If IS_CURSOR_POSITION_VALID, use it;
             17181                             else allow cursor to default to upper-left-most field.  */
             17182    1           2 CURSOR_POSITION BIT(16) ALIGNED,
             17183                          /* POSition of the field on which to place the cursor.  */
             17184    1           2 STATUS UBIN;
             17185                          /* Returned action code; see corresponding EQUs.  */
             17192            %ENDIF;
       63    17195
       64    17196          %SET LISTMAC = '0'B;
       65    17197        %EJECT;
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:4    
       66    17198    1       INSTR$ = QI_1.INSTR$;
       67    17199    1       QI_1.INSTR$ = ADDR ( INSTR$ -> QI$OP_IFINDR.NEXT_CODE@P );
       68    17200
       69    17201    1   RETRY_WHATEVER_FLAGS:
       70    17202            %PROC_ADDRESS ( THING@P = "INSTR$ -> QI$OP_IFINDR.FRAME_OR_FIELD@P",
       71    17203            CHECK_FOR_INDIRECTION = YES,
       72    17204            RESULT$ = "QI_1.CURRENT.NODE$" );
       73    17241
       74    17242    1       CALL QCF$FIND_CURRENTS;
       75    17243
       76    17244            /*  If QI_1.CURRENT.FORM_STATUS$ = ADDR (NIL), we were unable
       77    17245                to trace backwards from the given NODE to its corresponding
       78    17246                FORM.  This is because the FRAME which was at or above the
       79    17247                given NODE was not ACTIVATEd.
       80    17248                We can't assume that the FORM is closed (we don't know
       81    17249                which FORM the FRAME was intended to be used under), so we'll
       82    17250                fall through to code that will result in a return status
       83    17251                of FALSE for ACTIVE or ENABLED checks.  */
       84    17252    1       IF QI_1.CURRENT.FORM_STATUS$ ~= ADDR (NIL)
       85    17253    1       THEN
       86    17254    1         IF NOT QI_1.CURRENT.FORM_STATUS$ -> QI$FORM_STATUS.INDICATORS.OPEN_FLAGS.
             17254                  IS_INPUT
       87    17255    1           AND NOT QI_1.CURRENT.FORM_STATUS$ -> QI$FORM_STATUS.INDICATORS.OPEN_FLAGS.
             17255                    IS_OUTPUT
       88    17256    1         THEN
       89    17257    2           DO;
       90    17258    2             QI_1.ERROR_STATUS = %E$FILE_NOT_OPEN;
       91    17259    2             GOTO CHECK_USE_PROCEDURE;
       92    17260    2           END;
       93    17261
       94    17262    1       IF INSTR$ -> QI$OP_IFINDR.ATTRIBUTE.STATUS_TYPE = %QI#IF_INDICATOR_ACTIVE
       95    17263    1       THEN
       96    17264    1   WE_GOTTA_FRAME:
       97    17265    2         DO;
       98    17266
       99    17267    2           IF QI_1.CURRENT.FRAME_STATUS$ -> QI$FRAME_STATUS.INDICATORS.IS_ACTIVE
      100    17268    2           THEN
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:5    
      101    17269    2             GOTO SUCCEED;
      102    17270    2           ELSE
      103    17271    2             GOTO FAIL;
      104    17272
      105    17273    2         END WE_GOTTA_FRAME;
      106    17274    1       ELSE
      107    17275    1   WE_GOTTA_FIELD:
      108    17276    2         DO;
      109    17277
      110    17278    2           IF NOT QI_1.CURRENT.FRAME_STATUS$ -> QI$FRAME_STATUS.INDICATORS.IS_ACTIVE
      111    17279    2           THEN
      112    17280    2             GOTO FAIL;
      113    17281
      114    17282    2           GZ_FIELD$ = PINCRW ( QI_1.CURRENT.FRAME_STATUS$ -> QI$FRAME_STATUS.GZ.START$
             17282                    ,
      115    17283    2             QI_1.CURRENT.NODE_STATUS$ -> QI$MAJOR_STATUS.GZ.FIELD_START_INDEX * SIZEW
             17283                      (QI$GZ_FIELD) );
      116    17284
      117    17285    3           DO CASE ( INSTR$ -> QI$OP_IFINDR.ATTRIBUTE.STATUS_TYPE );
      118    17286
      119    17287    3             CASE ( %QI#IF_INDICATOR_ENABLED_INPUT );
      120    17288    3               IF GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.ENABLED_FLAGS.
             17288                        IS_INPUT
      121    17289    3               THEN
      122    17290    3                 GOTO SUCCEED;
      123    17291    3               ELSE
      124    17292    3                 GOTO FAIL;
      125    17293
      126    17294    3             CASE ( %QI#IF_INDICATOR_ENABLED_OUTPUT );
      127    17295    3               IF GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.ENABLED_FLAGS.
             17295                        IS_OUTPUT
      128    17296    3               THEN
      129    17297    3                 GOTO SUCCEED;
      130    17298    3               ELSE
      131    17299    3                 GOTO FAIL;
      132    17300
      133    17301    3             CASE ( %QI#IF_INDICATOR_ENABLED_IO );
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:6    
      134    17302    3               IF GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.ENABLED_FLAGS.
             17302                        IS_INPUT
      135    17303    3                 AND GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.
             17303                          ENABLED_FLAGS.IS_OUTPUT
      136    17304    3               THEN
      137    17305    3                 GOTO SUCCEED;
      138    17306    3               ELSE
      139    17307    3                 GOTO FAIL;
      140    17308
      141    17309    3             CASE ( %QI#IF_INDICATOR_PRESENTED );
      142    17310    3               IF GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.STATE_FLAGS.
             17310                        IS_PRESENTED
      143    17311    3               THEN
      144    17312    3                 GOTO SUCCEED;
      145    17313    3               ELSE
      146    17314    3                 GOTO FAIL;
      147    17315
      148    17316    3             CASE ( %QI#IF_INDICATOR_AVAILABLE );
      149    17317    3               IF GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.STATE_FLAGS.
             17317                        IS_AVAILABLE
      150    17318    3               THEN
      151    17319    3                 GOTO SUCCEED;
      152    17320    3               ELSE
      153    17321    3                 GOTO FAIL;
      154    17322
      155    17323    3           END;
      156    17324
      157    17325    2         END WE_GOTTA_FIELD;
      158    17326
      159    17327    1   SUCCEED:
      160    17328    1       IF INSTR$ -> QI$OP_IFINDR.ATTRIBUTE.IS_NOT
      161    17329    1       THEN
      162    17330    1         GOTO BRANCH_AROUND_TRUE_STATEMENTS;
      163    17331
      164    17332    1       RETURN;
      165    17333
      166    17334    1   FAIL:
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:7    
      167    17335    1       IF INSTR$ -> QI$OP_IFINDR.ATTRIBUTE.IS_NOT
      168    17336    1       THEN
      169    17337    1         RETURN;
      170    17338
      171    17339    1   BRANCH_AROUND_TRUE_STATEMENTS:
      172    17340            %PROC_ADDRESS ( THING@P = "INSTR$ -> QI$OP_IFINDR.FALSE_CONDITION@P",
      173    17341            CHECK_FOR_INDIRECTION = NO,
      174    17342            RESULT$ = "QI_1.INSTR$" );
      175    17379
      176    17380    1       ALTRETURN;
      177    17381
      178    17382        %EJECT;
PL6.E3A0      #001=QBI$IF_INDICATOR File=QBI$IF_INDICATOR.:E05TSI                WED 07/30/97 04:49 Page:8    
      179    17383    1   CHECK_USE_PROCEDURE:
      180    17384    1       QCU@USE.USE_DEFAULT_IO = %QI#USE_DEFAULT_NONE;
      181    17385    1       QCU@USE.IS_TIME_TO_SAVE_AWAY_FIELDS = %NO;
      182    17386    1       QCU@USE.CURR_FRAME$ = QI_1.CURRENT.FRAME$;
      183    17387    1       QCU@USE.CURR_NODE$ = QI_1.CURRENT.NODE$;
      184    17388
      185    17389    1       CALL QCU$USE_IO ( QCU@USE );
      186    17390
      187    17391    2       DO CASE ( QCU@USE.STATUS );
      188    17392
      189    17393    2         CASE ( %QI#USE_INVALID_RETURN );
      190    17394    2           RETURN;
      191    17395
      192    17396    2         CASE ( %QI#USE_INVALID_RETRY );
      193    17397    2           GOTO RETRY_WHATEVER_FLAGS;
      194    17398
      195    17399    2         CASE ( %QI#USE_INVALID_SKIP_TO_NEXT );
      196    17400    2           RETURN;
      197    17401
      198    17402    2         CASE ( ELSE );
      199    17403    2           CALL QCE$ERROR_INTERNAL ( %E$INTERNAL_INTERPRETER_ERROR );
      200    17404
      201    17405    2       END;
      202    17406
      203    17407    1   END QBI$IF_INDICATOR;

