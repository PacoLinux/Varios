  903      /*D* NAME:         B_CPULS
  904           PURPOSE:      To generate the command processor portion of the user LS. */
  905      %MACRO B_CPULS (FPTN=B_CPULS,STCLASS=BASED,LVL=1,LAST=";");
  906
  907       %LSET LISTDIR='0'B;
  908       %LSET LISTEXP='1'B;
  909       %LSET LISTCOM='0'B;
  910       %LSET LISTSUB='1'B;
  911       %LSET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
  912       %IF (SUBSTR(STR,0,6)='STATIC') OR
  913       (SUBSTR(STR,0,8)='CONSTANT') OR
  914       (SUBSTR(STR,0,3)='EXT');
  915       %LSET INIT=CHARTEXT('INIT');
  916       %ELSE;
  917       %LSET INIT=CHARTEXT('/*');
  918       %ENDIF;
  919
  920       %IF LVL=1;
  921      DCL 1 FPTN STCLASS DALIGNED,
  922       %ELSE;
  923          %LVL FPTN DALIGNED,
  924       %ENDIF;
  925          %(LVL+1) IS,                           /* CP     ISR                         */
