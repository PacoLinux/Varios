  481      /*D* NAME:         B_DBULS
14:14 JUL 28 '97 B_ULSMAC_C.:E05SI                                         14   
  482           PURPOSE:      To generate the debugger portion of the user LS. */
  483      %MACRO B_DBULS (FPTN=B_DBULS,STCLASS=BASED,LVL=1,LAST=";");
  484
  485       %LSET LISTDIR='0'B;
  486       %LSET LISTEXP='1'B;
  487       %LSET LISTCOM='0'B;
  488       %LSET LISTSUB='1'B;
  489       %LSET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
  490       %IF (SUBSTR(STR,0,6)='STATIC') OR
  491       (SUBSTR(STR,0,8)='CONSTANT') OR
  492       (SUBSTR(STR,0,3)='EXT');
  493       %LSET INIT=CHARTEXT('INIT');
  494       %ELSE;
  495       %LSET INIT=CHARTEXT('/*');
  496       %ENDIF;
  497
  498       %IF LVL=1;
  499      DCL 1 FPTN STCLASS DALIGNED,
  500       %ELSE;
  501          %LVL FPTN DALIGNED,
  502       %ENDIF;
  503          %(LVL+1) IS,                           /* DEBUGGER ISR                       */
  504          %(LVL+2) BOUND UBIN(20) UNAL %INIT(0)%CHARTEXT('/**/'),
  505          %(LVL+2) FLGS BIT(9) UNAL %INIT('553'O)%CHARTEXT('/**/'),
  506          %(LVL+2) WSR UBIN(3) UNAL %INIT(5)%CHARTEXT('/**/'),
  507          %(LVL+2) TYP UBIN(4) UNAL %INIT(0)%CHARTEXT('/**/'),
  508          %(LVL+2) BASE UBIN(36) %INIT(0)%CHARTEXT('/**/'),
  509
