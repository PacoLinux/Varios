  247      /*D* NAME:         B_USRULS_8000
  248           PURPOSE:      To generate the user portion of the user LS for DPS8000. */
  249      %MACRO B_USRULS_8000 (FPTN=B_USRULS_8000,STCLASS=BASED,LVL=1,LAST=";");
  250
  251       %LSET LISTDIR='0'B;
  252       %LSET LISTEXP='1'B;
  253       %LSET LISTCOM='0'B;
