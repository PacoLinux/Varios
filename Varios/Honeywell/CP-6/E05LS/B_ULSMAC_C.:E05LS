   13      /*D* NAME:         B_USRULS
   14           PURPOSE:      To generate the user portion of the user LS. */
   15      %MACRO B_USRULS (FPTN=B_USRULS,STCLASS=BASED,LVL=1,LAST=";");
   16
   17       %LSET LISTDIR='0'B;
   18       %LSET LISTEXP='1'B;
   19       %LSET LISTCOM='0'B;
   20       %LSET LISTSUB='1'B;
   21       %LSET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
   22       %IF (SUBSTR(STR,0,6)='STATIC') OR
   23       (SUBSTR(STR,0,8)='CONSTANT') OR
   24       (SUBSTR(STR,0,3)='EXT');
   25       %LSET INIT=CHARTEXT('INIT');
   26       %ELSE;
   27       %LSET INIT=CHARTEXT('/*');
   28       %ENDIF;
   29
   30       %IF LVL=1;
   31      DCL 1 FPTN STCLASS DALIGNED,
   32       %ELSE;
   33          %LVL FPTN DALIGNED,
   34       %ENDIF;
   35          %(LVL+1) IS,                           /* USER   ISR                         */
   36          %(LVL+2) BOUND UBIN(20) UNAL %INIT(IS_BOUND)%CHARTEXT('/**/'),
   37          %(LVL+2) FLGS BIT(9) UNAL %INIT('753'O)%CHARTEXT('/**/'),
14:14 JUL 28 '97 B_ULSMAC_C.:E05SI                                         2    
   38          %(LVL+2) WSR UBIN(3) UNAL %INIT(7)%CHARTEXT('/**/'),
   39          %(LVL+2) TYP UBIN(4) UNAL %INIT(0)%CHARTEXT('/**/'),
   40          %(LVL+2) BASE UBIN(36) %INIT(IS_BASE)%CHARTEXT('/**/'),
   41
   42          %(LVL+1) JIT,                          /* USER JIT                           */
   43          %(LVL+2) BOUND UBIN(20) UNAL %INIT(JIT_BOUND)%CHARTEXT('/**/'),
   44          %(LVL+2) FLGS BIT(9) UNAL %INIT('543'O)%CHARTEXT('/**/'),
   45          %(LVL+2) WSR UBIN(3) UNAL %INIT(7)%CHARTEXT('/**/'),
   46          %(LVL+2) TYP UBIN(4) UNAL %INIT(0)%CHARTEXT('/**/'),
   47          %(LVL+2) BASE UBIN(36) %INIT(JIT_BASE)%CHARTEXT('/**/'),
   48
   49          %(LVL+1) BUF,                          /* FILE & COOP BUFS                   */
   50          %(LVL+2) BOUND UBIN(20) UNAL %INIT(FMS_BOUND)%CHARTEXT('/**/'),
   51          %(LVL+2) FLGS BIT(9) UNAL %INIT('143'O)%CHARTEXT('/**/'),
