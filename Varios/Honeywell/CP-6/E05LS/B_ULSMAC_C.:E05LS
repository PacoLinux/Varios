  692      /*D* NAME:         B_ASLULS
  693           PURPOSE:      To generate the ASL portion of the user LS. */
  694      %MACRO B_ASLULS (FPTN=B_ASLULS,STCLASS=BASED,LVL=1,LAST=";");
  695
  696       %LSET LISTDIR='0'B;
  697       %LSET LISTEXP='1'B;
  698       %LSET LISTCOM='0'B;
  699       %LSET LISTSUB='1'B;
  700       %LSET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
  701       %IF (SUBSTR(STR,0,6)='STATIC') OR
  702       (SUBSTR(STR,0,8)='CONSTANT') OR
  703       (SUBSTR(STR,0,3)='EXT');
14:14 JUL 28 '97 B_ULSMAC_C.:E05SI                                         20   
  704       %LSET INIT=CHARTEXT('INIT');
  705       %ELSE;
  706       %LSET INIT=CHARTEXT('/*');
  707       %ENDIF;
  708
  709       %IF LVL=1;
  710      DCL 1 FPTN STCLASS DALIGNED,
  711       %ELSE;
  712          %LVL FPTN DALIGNED,
  713       %ENDIF;
  714          %(LVL+1) IS,                           /* ASL    ISR                         */
  715          %(LVL+2) BOUND UBIN(20) UNAL %INIT(0)%CHARTEXT('/**/'),
  716          %(LVL+2) FLGS BIT(9) UNAL %INIT('553'O)%CHARTEXT('/**/'),
  717          %(LVL+2) WSR UBIN(3) UNAL %INIT(6)%CHARTEXT('/**/'),
