  256       %IF (SUBSTR(STR,0,6)='STATIC') OR
  257       (SUBSTR(STR,0,8)='CONSTANT') OR
  258       (SUBSTR(STR,0,3)='EXT');
  259       %LSET INIT=CHARTEXT('INIT');
14:14 JUL 28 '97 B_ULSMAC_C.:E05SI                                         8    
  260       %ELSE;
  261       %LSET INIT=CHARTEXT('/*');
  262       %ENDIF;
  263
  264       %IF LVL=1;
  265      DCL 1 FPTN STCLASS DALIGNED,
  266       %ELSE;
  267          %LVL FPTN DALIGNED,
  268       %ENDIF;
