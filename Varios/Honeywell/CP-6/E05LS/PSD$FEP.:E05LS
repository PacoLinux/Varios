PL6.E3A0      #001=PSD$FEP File=PSD$FEP.:E05TSI                                  WED 07/30/97 04:27 Page:61   
      503    18531    1   GENERATE: PROC (RESULT, VALUE, DELTA_T, SCALE);
      504    18532
      505    18533    2   DCL RESULT CHAR (7);
      506    18534    2   DCL VALUE SBIN;
      507    18535    2   DCL DELTA_T UBIN;
      508    18536    2   DCL DELTA_TS REDEF DELTA_T SBIN;
      509    18537    2   DCL SCALE UBIN;
      510    18538    2   DCL TEMP UBIN;
      511    18539    2   DCL TEMPCHAR CHAR (7);
      512    18540    2   DCL PATTERN (0:1) BIT (9) CONSTANT INIT (%MVZB6, %MVC1);
      513    18541
      514    18542    3      DO CASE (SCALE);
      515    18543
      516    18544    3      CASE (%/* abs# */ 0);
      517    18545    3         TEMP = VALUE;
      518    18546
      519    18547    3      CASE (%PER_MINUTE#);
      520    18548    3         CALL CRUNCH (TEMP, VALUE, 1, DELTA_T, %FEP_TICS_PER_MINUTE);
      521    18549
      522    18550    3      CASE (%PER_SECOND#);
      523    18551    3         CALL CRUNCH (TEMP, VALUE, 1, DELTA_T, %FEP_TICS_PER_SECOND);
      524    18552
      525    18553    3      CASE (%PCT_TIME#);
      526    18554    3         TEMP = (VALUE * 100 + DELTA_T / 2) / DELTA_TS;
      527    18555
      528    18556    3      END;
      529    18557
      530    18558    2      CALL BINCHAR (TEMPCHAR, TEMP);
      531    18559    2      CALL EDITCHAR (RESULT, , , PATTERN, TEMPCHAR);
      532    18560    2      RETURN;
      533    18561
      534    18562    2   END GENERATE;
