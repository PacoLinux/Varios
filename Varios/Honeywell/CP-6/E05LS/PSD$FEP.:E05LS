      343    18371    5               ALTRETURN;
      344    18372    5            END; ELSE DO;
      345    18373    5               P.PTR$ = PINCRW(P.PTR$, P.PTR$->PS_FEPSTATS.WORDS);
      346    18374    5               ANOTHER = '0'B;
      347    18375    5            END;
      348    18376    4         END;
      349    18377    3      END;
