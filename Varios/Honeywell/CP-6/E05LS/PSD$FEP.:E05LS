      335    18363    4           OR NOT P.PTR$->PS_FEPSTATS.DISP.BITS(ENT)
      336    18364    4           OR P.PTR$->PS_FEPSTATS.HANDLER ~= FEP$->PS_FEPSTATS.HANDLER
