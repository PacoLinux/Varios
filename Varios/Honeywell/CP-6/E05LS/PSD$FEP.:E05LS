PL6.E3A0      #001=PSD$FEP File=PSD$FEP.:E05TSI                                  WED 07/30/97 04:27 Page:57   
      403    18431    1   CRUNCH: PROC (RESULT, ITEM, DIVISOR, TICS, TICS_PER_UNIT);
      404    18432
      405    18433    2   DCL RESULT SBIN;
      406    18434    2   DCL ITEM SBIN;
      407    18435    2   DCL DIVISOR SBIN;
      408    18436    2   DCL TICS SBIN;
      409    18437    2   DCL TICS_PER_UNIT SBIN;
      410    18438
      411    18439    2      IF (DIVISOR*TICS)=0
      412    18440    2      THEN RESULT=0;
      413    18441    2      ELSE IF TICS < TICS_PER_UNIT * 100
      414    18442    2      THEN IF (ITEM * TICS_PER_UNIT) / TICS_PER_UNIT = ITEM
      415    18443    2      THEN RESULT = (ITEM * TICS_PER_UNIT) / (DIVISOR * TICS);
      416    18444    2      ELSE RESULT = (ITEM / TICS) * (TICS_PER_UNIT / DIVISOR);
      417    18445    2      ELSE
      418    18446    2      RESULT = ITEM / (DIVISOR * TICS / TICS_PER_UNIT);
      419    18447
      420    18448    2      RETURN;
      421    18449
      422    18450    2   END CRUNCH;
      423    18451
      424    18452        %EJECT;
PL6.E3A0      #001=PSD$FEP File=PSD$FEP.:E05TSI                                  WED 07/30/97 04:27 Page:58   
      425    18453    1   CRUNCHUP: PROC (RESULT, ITEM, DIVISOR, TICS, TICS_PER_UNIT);
      426    18454
      427    18455    2   DCL RESULT SBIN;
      428    18456    2   DCL ITEM SBIN;
      429    18457    2   DCL DIVISOR SBIN;
      430    18458    2   DCL TICS SBIN;
      431    18459    2   DCL TICS_PER_UNIT SBIN;
      432    18460
      433    18461    2      IF (DIVISOR*TICS)>0
      434    18462    2      THEN IF TICS < TICS_PER_UNIT * 100
      435    18463    2      THEN IF (ITEM * TICS_PER_UNIT) / TICS_PER_UNIT = ITEM
      436    18464    2      THEN RESULT = RESULT + (ITEM * TICS_PER_UNIT) / (DIVISOR * TICS);
      437    18465    2      ELSE RESULT = RESULT + (ITEM / TICS) * (TICS_PER_UNIT / DIVISOR);
      438    18466    2      ELSE
      439    18467    2      RESULT = RESULT + ITEM / (DIVISOR * TICS / TICS_PER_UNIT);
      440    18468
      441    18469    2      RETURN;
      442    18470
      443    18471    2   END CRUNCHUP;
      444    18472
      445    18473        %EJECT;
PL6.E3A0      #001=PSD$FEP File=PSD$FEP.:E05TSI                                  WED 07/30/97 04:27 Page:59   
      446    18474    1   CALC_TICS: PROC (STARTING, ENDING, RESULT);
      447    18475
      448    18476    2   DCL STARTING UBIN;
      449    18477    2   DCL ENDING UBIN;
      450    18478    2   DCL RESULT UBIN;
      451    18479    2   DCL INTERVAL UBIN;
      452    18480
      453    18481    2      CALL XUD$UTS_DIFF (INTERVAL, ENDING, STARTING);
      454    18482
      455    18483    2      IF ENDING <= STARTING
      456    18484    2      THEN
      457    18485    2      RESULT = 0;
      458    18486    2      ELSE
      459    18487
      460    18488         /* Convert UTS interval to FEP TICS, by converting the
      461    18489            interval into seconds and then dividing by 120. */
      462    18490
      463    18491    2      IF (INTERVAL) > 1000000
      464    18492    2      THEN
      465    18493    2      RESULT = INTERVAL / %MICROSECONDS_PER_FEP_TIC * 10000;
      466    18494    2      ELSE
      467    18495    2      RESULT = INTERVAL * 10000 / %MICROSECONDS_PER_FEP_TIC;
      468    18496
      469    18497    2      RETURN;
      470    18498
      471    18499    2   END CALC_TICS;
      472    18500
      473    18501        %EJECT;
PL6.E3A0      #001=PSD$FEP File=PSD$FEP.:E05TSI                                  WED 07/30/97 04:27 Page:60   
      474    18502    1   GEN: PROC (RESULT, NEW, OLD, SCALE);
      475    18503
      476    18504    2   DCL 1 RESULT CHAR (7);
      477    18505    2   DCL NEW UBIN;
      478    18506    2   DCL OLD UBIN;
      479    18507    2   DCL SCALE UBIN;
      480    18508    2      IF SCALE = %/* abs# */ 0
      481    18509    3      THEN DO;
      482    18510
      483    18511    3         CALL GENERATE (RESULT, NEW, 1, SCALE);
      484    18512    3      END;
      485    18513
      486    18514    3      ELSE DO;
      487    18515
      488    18516
      489    18517    3         IF DELTA_TICS_ELAPSED=0 OR SECOND_BOOT>INTERVAL_START
      490    18518    3         THEN
      491    18519    3         RESULT = ' ';
      492    18520    3         ELSE
      493    18521    3         CALL GENERATE (RESULT, NEW - OLD, DELTA_TICS_ELAPSED,
      494    18522    3           SCALE);
      495    18523    3      END;
      496    18524
      497    18525
      498    18526    2      RETURN;
      499    18527
      500    18528    2   END GEN;
      501    18529
