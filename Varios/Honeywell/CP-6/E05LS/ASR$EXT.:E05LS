PL6.E3A0      #002=ASR$R_DEVWSN File=ASR$EXT.:E05TSI                             TUE 07/29/97 10:47 Page:31   
      171    14073        /*********************************************************************/
      172    14074        /*   Procedure       procedure       procedure       procedure       */
      173    14075        /*********************************************************************/
      174    14076                                                              /**/
      175    14077        /*
      176    14078            ***            Begin procedure                    ***
      177    14079        */
      178    14080    1   FDS.NVECS# = 0;
      179    14081    1   WSN_WRIT = '0'B;
      180    14082    1   DEVWSN_KEY.ACCT = USERS_KEY.TXTC ;
      181    14083    1   USERS_KEY = DEVWSN_KEY ;
      182    14084    1   DEVWSN$ = DSEG1$ ;
      183    14085    1   DEVWSN_ARS = DSEG1SIZE ;
      184    14086    1   CALL ASB$ENQ_HLP ALTRET(ENQ_ERR) ;
      185    14087    1   CALL ASB$READ_HLP (DEVWSN$, DEVWSN_ARS) ALTRET(DEV_ERR) ;
      186    14088    1   IF KL$DEVWSN.NDEV > 0
      187    14089    2   THEN DO;
      188    14090    3      DO I = 0 TO KL$DEVWSN.NDEV - 1 ;
      189    14091    3         WSNDEV_KEY.ACCT = KL$DEVWSN.DEV(I) ;
      190    14092    3         USERS_KEY = WSNDEV_KEY ;
      191    14093    3         CALL ASW$ENQ_WSN ALTRET( ENQ_ERR);
      192    14094    3         WSNDEV$ = PINCRW( DSEG1$, 1000 );
      193    14095    3         WSNDEV_ARS = DSEG1SIZE - 1000*4 ;
      194    14096    3         CALL ASW$READ_WSN (WSNDEV$, WSNDEV_ARS) ALTRET( WSN_ERR) ;
      195    14097    3         IF WSN_MSG
      196    14098    4         THEN DO;
      197    14099    5            IF I = 0 THEN DO;
