       25     9768        %INCLUDE    AS_SUPER_R ;
       26    12784    1   DCL PSEUDO_FORM_REC UBIN;
       27    12785    1   DCL FORM CHAR(6);
       28    12786    1   DCL BUFFER CHAR(80);
       29    12787    1   DCL 1 PSEUDO,
       30    12788    1          2 CNT UBIN BYTE UNAL,
       31    12789    1          2 TXT CHAR(6) UNAL;
       32    12790    1   DCL TABS UBIN STATIC INIT(10);
