       17       17        %INCLUDE    AS_ENTRIES_M ;
       18     1067        %INCLUDE    AS_SUPER_C ;
       19     1502        %INCLUDE    AS_SUBS_C  ;
       20     2916        %INCLUDE    AS_MAC_C   ;
