       37    12799        %ASF$DELREC_FORM ;
PL6.E3A0      #001=ASR$R_PSEUDO_FORMS File=ASR$EXT.:E05TSI                       TUE 07/29/97 10:46 Page:2    
       38    12802        %ASB$WRITE_LO ;
       39    12805
       40    12806    1   FORM = PSEUDO_FORM_KEY.TXT;
       41    12807    1   PSEUDO.CNT = 6;
       42    12808    1   CALL ASB$WRITE_LO( AS_BLANK, 1 );
       43    12809    1   CALL FLUSH;
       44    12810    1   CALL ASF$PREC_FORM( PSEUDO_FORM_KEY );
       45    12811    1   ARS = SIZEC(PSEUDO_FORM_REC);
       46    12812    1   CALL ASF$READ_FORM_SEQ(, ADDR(PSEUDO_FORM_REC), ARS ) ALTRET(EXIT#);
       47    12813    1   COUNT = 0;
       48    12814    2   DO WHILE( USERS_KEY.COUNT = 12  AND  SUBSTR(USERS_KEY.TXTC,0,6) = FORM );
       49    12815    2      PSEUDO_FORM_KEY = USERS_KEY;
       50    12816    2      PSEUDO.TXT = SUBSTR(USERS_KEY.TXTC,6,6);
       51    12817    2      CALL ASF$DELREC_FORM;
       52    12818    2      CALL LIST;
       53    12819    2      USERS_KEY = PSEUDO;
       54    12820    2      CALL ASF$DELREC_FORM;
       55    12821    2      COUNT = COUNT + 1;
       56    12822    2      CALL ASF$PREC_FORM( PSEUDO_FORM_KEY );
       57    12823    2      ARS = SIZEC(PSEUDO_FORM_REC);
       58    12824    2      CALL ASF$READ_FORM_SEQ(, ADDR(PSEUDO_FORM_REC), ARS ) ALTRET(EXIT#);
       59    12825    2      END;
       60    12826    1   EXIT#:
       61    12827    1   CALL FLUSH;
       62    12828    1   CALL ASB$WRITE_LO( AS_BLANK, 1 );
       63    12829    2   IF COUNT > 0 THEN DO;
       64    12830    2      IF COUNT = 1 THEN BUFFER = '.. 1 pseudo form removed.';
       65    12831    3      ELSE DO;
       66    12832              %NUMBER( NUM1=COUNT );
             12840    3         CALL ASB$BINCHAR( AS_FIELD1.TEXT, 0, -1, COUNT );
       67    12862    3         CALL CONCAT( BUFFER, '.. ', SUBSTR(AS_FIELD1.TEXT,0,AS_FIELD1.COUNT),
             12862                  ' pseudo forms removed.' );
       68    12863    3         END;
       69    12864    2      CALL ASB$WRITE_LO( BUFFER, LENGTHC(BUFFER) );
       70    12865    2      END;
       71    12866    1   RETURN;
       72    12867        %EJECT;
PL6.E3A0      #001=ASR$R_PSEUDO_FORMS File=ASR$EXT.:E05TSI                       TUE 07/29/97 10:46 Page:3    
       73    12868    1   LIST: PROC;
       74    12869
       75    12870    2   IF TABS = 80 THEN CALL FLUSH;
       76    12871    2   CALL INSERT( BUFFER, TABS, , PSEUDO.TXT );
       77    12872    2   TABS = TABS + 10;
       78    12873
       79    12874    2   END LIST;
       80    12875
       81    12876    1   FLUSH: PROC;
       82    12877
       83    12878    2   IF TABS > 10 THEN CALL ASB$WRITE_LO( BUFFER, LENGTHC(BUFFER) );
       84    12879    2   BUFFER = ' ';
       85    12880    2   TABS = 10;
       86    12881
       87    12882    2   END FLUSH;
       88    12883
       89    12884
       90    12885    1   END ASR$R_PSEUDO_FORMS;
