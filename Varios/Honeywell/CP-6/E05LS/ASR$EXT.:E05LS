       95        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
       96        5        /*T*                                                         */
       97        6        /*T***********************************************************/
       98        7        /*X*  DMC,DMR,CCC,PLM=0,IND=3,CTI=5,DCI=6,IAD=0,DTI=0,IDT=0,ENI=3,
       99        8              ECU=3,CSU=3,SDI=3                                           */
      100        9                                                              /**/
      101       10        ASR$R_DEVWSN: PROC (WSN_MSG);
