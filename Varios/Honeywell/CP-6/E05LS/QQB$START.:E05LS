       84     7101    1           IF QQ_CMDBLK.OUT$ -> OUT$BLK.NSUBLKS > 0 THEN
       85     7102    2             DO;                           /* GET TYPE                           */
       86     7103    2               FID = QQ_CMDBLK.OUT$ -> OUT$BLK.SUBLK$(0) -> OUT$SYM.TEXT;
       87     7104    2               CALL M$FID(FPT_FID) ALTRET(FID_ERR);
       88     7105    2               IF FPT_OPEN.V.ASN# ~= 1 OR
