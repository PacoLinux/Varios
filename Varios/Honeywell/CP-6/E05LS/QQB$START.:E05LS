       93     7110    2                   VLR_FID.RES THEN
       94     7111    3                 DO;
       95     7112    3                   QQ_CMDBLK.MINOR_ERR = CODE_IFP;
       96     7113    3                   CALL PRINT_ERR;
       97     7114    3                   RETURN;
       98     7115    3                 END;
       99     7116    2               FP = VLP_NAME.NAME#;
      100     7117    2             END;
      101     7118    1           ELSE FP = '*DFLT';
