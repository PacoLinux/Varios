      107     7124    2               CALL QQC$RLS_BLK(QQ_GLOBAL.POOL$, TP$STATION.QT$FORM$);
      108     7125    2               TP$STATION.QT$FORM$ = ADDR(NIL);
      109     7126    2               TP$STATION.FLAGS.IS_QT$FORM = '0'B;
      110     7127    2               RETURN;
      111     7128    2             END;
      112     7129    1           TP$STATION.FLAGS.IS_QT$FORM = '1'B;
