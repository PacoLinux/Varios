      134     7151    3                 END;
      135     7152    2             END;
      136     7153    1           VLP_STATION.STATION# = QQ_CMDBLK.S$ -> TP$STATION.ID;
      137     7154    1           CALL M$WRITE(FPT_WRITE) ALTRET(MON_ERR);
