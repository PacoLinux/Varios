    8      /*B*
    9      NMC#            define the nodes used in the PARTRGE metalanguage
   10              to define the commands accepted by the MIRROR program.
   11
   12              Constant                Command
   13              --------------------    ---------                          */
   14      /*B*    NMC#ALL                 ALL ( DISPLAY )                    */
   15      /*B*    NMC#COPY                COPY                               */
   16      /*B*    NMC#DEV                 device                             */
   17      /*B*    NMC#DISPLAY             DISPLAY                            */
   18      /*B*    NMC#DONT                DONT                               */
   19      /*B*    NMC#END                 END                                */
   20      /*B*    NMC#FAIL                FAIL                               */
   21      /*B*    NMC#KOMPARE             COMPARE                            */
   22      /*B*    NMC#LOCK                LOCK                               */
   23      /*B*    NMC#MIRROR              MIRROR                             */
   24      /*B*    NMC#SWITCH              SWITCH                             */
   25      /*B*    NMC#UNLOCK              UNLOCK                             */
   26      /*                                                                 */
   27      %SUB    NMC#KOMPARE             = " 1 /* NMC#KOMPARE  */" ;
   28      %SUB    NMC#DISPLAY             = " 2 /* NMC#DISPLAY  */" ;
   29      %SUB    NMC#DONT                = " 3 /* NMC#DONT     */" ;
   30      %SUB    NMC#MIRROR              = " 4 /* NMC#MIRROR   */" ;
   31      %SUB    NMC#END                 = " 5 /* NMC#END      */" ;
   32      %SUB    NMC#FAIL                = " 6 /* NMC#FAIL     */" ;
   33      %SUB    NMC#SWITCH              = " 7 /* NMC#SWITCH   */" ;
   34      %SUB    NMC#COPY                = " 8 /* NMC#COPY     */" ;
   35      %SUB    NMC#LOCK                = " 9 /* NMC#LOCK     */" ;
   36      %SUB    NMC#UNLOCK              = "10 /* NMC#UNLOCK   */" ;
