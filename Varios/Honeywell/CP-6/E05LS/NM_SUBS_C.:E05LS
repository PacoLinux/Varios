   41      /*B*
   42      NMO#?           define the nodes used in the PARTRGE metalanguage
   43              to define the options accepted by the MIRROR program.
   44
   45              Constant                Option
   46              --------------------    ---------                          */
   47      /*B*    NMO#GRANS               Grans                              */
   48      /*B*    NMO#MS                  Milliseconds                       */
   49      /*B*    NMO#MXDIFF              Maximum Differences                */
   50      /*B8    NMO#NREPORT             No report                          */
   51      /*B*    NMO#PAUSE               Pause                              */
   52      /*B*    NMO#REPORT              Report                             */
   53      /*B*    NMO#SECONDS             Seconds                            */
   54      /*                                                                 */
   55      %SUB    NMO#GRANS               = "201 /* NMO#GRANS   */" ;
   56      %SUB    NMO#MS                  = "202 /* NMO#MS      */" ;
   57      %SUB    NMO#PAUSE               = "203 /* NMO#PAUSE   */" ;
   58      %SUB    NMO#SECONDS             = "204 /* NMO#SECONDS */" ;
   59      %SUB    NMO#MXDIFF              = "205 /* NMO#MXDIFF  */" ;
   60      %SUB    NMO#REPORT              = "206 /* NMO#REPORT  */" ;
   61      %SUB    NMO#NREPORT             = "207 /* NMO#NREPORT */" ;

