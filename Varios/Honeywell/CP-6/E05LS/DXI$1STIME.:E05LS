      330    13718    1        S_D.DNAME$ = ADNAME$;
      331    13719    1        S_D.OVERLAID = '0'B;
      332    13720    1        CALL DDS$PD_SRCH(DD_XOU$,TEMP$) ALTRET(GOTAXOU);
      333    13721    2        DO WHILE ('0'B);
      334    13722    2   GOTAXOU:
      335    13723    2             D_SCHEMA$ = ASCHEMA$;
      336    13724    2             CALL DDS$EQCS#(POFFW(TEMP$,AEDEF$)/SIZEW(B$EDEF)) ALTRET(NOAXOU);
      337    13725    2             CALL DDS$SET_Q2AX ALTRET(NOAXOU);
      338    13726    2           END;
      339    13727    1   NOAXOU:
      340    13728        /* Establish USE MON in use frame, setup for MON SCHEMA */
      341    13729    1        S_D.OVERLAID = '1'B;
      342    13730    1        CALL DXB$CUSE;
      343    13731        /* Setup default syntax */
      344    13732    1        S_D.SYNTAX = %PL6ID;
      345    13733    1        S_D.SYNTAX_DEFAULT = %PL6ID;
      346    13734    2        DO I=0 TO SIZEW(DD_COMP_NODE_PTR$)-1;
      347    13735    2             DD_COMP_NODE_PTR$(I) -> DD_NODE.SUBNDS$(0) = DD_SYN_PL6(I);
      348    13736    2           END;
      349    13737        /* Establish @ and # patch space, if any */
      350    13738    1        S_D.GOT@ = '0'B;
      351    13739    1        S_D.GOT# = '0'B;
      352    13740    1        CALL DDI$GET_PATCH_SPACE;
      353    13741    1        IF NOT S_D.PATCH
      354    13742    1        THEN CALL DDR$REPORT( 11/*R_DELTA*/ );
      355    13743                                                /**/
      356    13744    1        RETURN;
      357    13745                                                /**/
      358    13746                                                /**/
      359    13747    1   END DXI$1STIME;
      360    13748        %EOD;

