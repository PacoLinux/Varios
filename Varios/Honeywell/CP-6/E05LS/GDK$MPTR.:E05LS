        1        1        /*M*  GDK$MPTR - Converts ADDRESS, SEG, DESC#, NOT_IS into a PTR.              */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MCL=4,DCI=5,CRT=5 */
        8        8        /*F*  NAME: GDK$MPTR
        9        9              PURPOSE: This routine evaluates ADDRESS, SEG, DESC#, and NOT_IS.
       10       10                       resolving the whole mess into a genuine L6 pointer into
       11       11                       the user's memory.
       12       12
       13       13                       Note that only special symbols usable as pointers need
       14       14                       be decoded.
       15       15
       16       16              DESCRIPTION:  Input is ADDRESS - word offset in segment
       17       17                                     SEG - described in GDK$EVAL
       18       18                                     DESC# the encoded segment ID
       19       19                                     NOT_IS - set if segment not INSTRUCTION
       20       20                                              SEGMENT (seg=3, DESC#=0)
       21       21
       22       22                                                     */
       23       23        GDK$MPTR: PROC(PTR$,ADDRESS,SEG,DESC#,NOT_IS,IS_PTR,DSIZE);
       24       24             %INCLUDE GD_STATIC_DATA;
       25      497          /* Parms */
       26      498    1   DCL PTR$ PTR;
       27      499    1   DCL PTR_ REDEF PTR$ BIT(32);
       28      500    1   DCL ADDRESS UBIN(32);
       29      501    1   DCL SEG UBIN WORD;
       30      502    1   DCL DESC# UBIN WORD;
       31      503    1   DCL NOT_IS BIT(1) ALIGNED;
       32      504    1   DCL IS_PTR BIT(1) ALIGNED;
       33      505    1   DCL DSIZE UBIN;
       34      506    1   DCL G$JIT$ PTR SYMREF;
       35      507        %INCLUDE G_ROS_M;
       36      589        %G$ROS (STCLASS="BASED(G$ROS$)");
       37      633        %INCLUDE GM_VIRTUAL_E;
