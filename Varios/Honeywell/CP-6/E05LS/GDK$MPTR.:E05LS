      121     1503        /**/
      122     1504    1   GOT_SPEC_SYM:
      123     1505    2        DO CASE(SEG);
      124     1506    2          CASE(1);
      125     1507    2             IF DESC# = 7 /* $JIT */ THEN
      126     1508    2                  PTR$ = PINCRW(G$JIT$,ADDRESS);
