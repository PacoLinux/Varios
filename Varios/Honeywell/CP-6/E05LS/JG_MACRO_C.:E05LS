   10      %MACRO JG_RSLTS (NAME=JG_RSLTS,STCLASS=STATIC);
   11      DCL 1 NAME STCLASS,
   12            2 SYSID UBIN WORD,                /* SYSID of the started user          */
   13            2 ERR UBIN WORD;                  /* Error code if non-zero (out of     */
   14                                              /* JG_ERRORS_C)                       */
   15      %MEND;

