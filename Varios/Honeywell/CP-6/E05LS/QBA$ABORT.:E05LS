        1        1        /*M* QBA$ABORT:  "ABORT dataname", "ABORT 'literal'", and just plain ABORT.  */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QBA$ABORT
       10       10              PURPOSE:      Optional write to the MESSAGE LINE, and ABORT.
       11       11              DESCRIPTION:  Execute an ABORT op-code, whose argument may include
       12       12                            a FILE or WORKING-STORAGE SECTION DAD.
       13       13        */
       14       14
       15       15        QBA$ABORT: PROC;
       16       16
       17       17        /*D*  NAME:         QBA$ABORT
       18       18              CALL:         CALL QBA$ABORT;
       19       19              PARAMETERS:   None.
       20       20              INTERFACE:    Called by QAT$TALLYHO when an ABORT
       21       21                            op-code is encountered;
       22       22                            calls QCM$MESS_whatever
       23       23                            to do any actual MESSAGE LINE I-O;
       24       24                            calls QCE$ERROR.
       25       25              ENVIRONMENT:  Ready to abandon ship.
       26       26              INPUT:        QI_1,
       27       27                            FPL rununit static data following the op-code.
       28       28              OUTPUT:       May write to, read from, and erase from the MESSAGE LINE.
       29       29              SCRATCH:      Not a whole lot.
       30       30              DESCRIPTION:  For ABORT statements with dataname or "literal" arguments,
       31       31                            QCM$MESS is called to write the data to the default
       32       32                            MESSAGE LINE (see commentary in QCM$MESS).  QCM$MESS_PAUSE
       33       33                            is called to read 1 byte from the MESSAGE LINE (hence
       34       34                            the "pause"), and finally QCM$MESS_ERASE is called to,
       35       35                            yes, erase the data after the pause.
       36       36                            Then halt and catch fire.
       37       37        */
PL6.E3A0      #001=QBA$ABORT File=QBA$ABORT.:E05TSI                              WED 07/30/97 04:39 Page:2    
       38       38
       39       39          %INCLUDE LCP_6;
       40      230
       41      231          %INCLUDE QG_MACRO_M;
       42      368          %INCLUDE QI_EQUATES_E;
       43      727          %INCLUDE QI_PERR_C;
       44      895          %INCLUDE QI_MACROS_M;
       45     5953          %QI$ALL;
       46    17066
       47    17067    1     DCL INSTR$ PTR;
       48    17068
       49    17069    1     DCL QCE$ERROR ENTRY (4);
       50    17070    1     DCL QCM$MESS ENTRY (1);
       51    17071    1     DCL QCM$MESS_ERASE ENTRY (1);
       52    17072    1     DCL QCM$MESS_PAUSE ENTRY (1);
       53    17073    1     DCL QCS$SETUP_SOURCE ENTRY;
       54    17074
       55    17075          %SET LISTMAC = '1'B;
       56    17076
       57    17077          %INCLUDE QCM_MESS_M;
       58    17119          %QCM@MESS;
             17120            %BEGIN_MACRO;
             17121            %SET LISTDIR = '0'B;
             17134    1     DCL 1 QCM@MESS ALIGNED,
             17135    1           2 MESSAGE_LINE_GZ_HEADER$ PTR,
             17136                           /* Pointer to QI$GZ_HEADER for the
             17137                              MESSAGE LINE to use.  */
             17138    1           2 IS_MESSAGE_A_DAD BIT(1) ALIGNED,
             17139                           /* Set if MESSAGE$ points to a DAD,
             17140                              reset if MESSAGE$ points to text
             17141                              (in which case MESSAGE_LENGTH is valid).  */
             17142    1           2 MESSAGE_LENGTH UBIN,
             17143                           /* Length, in bytes, of the error message.  */
             17144    1           2 MESSAGE$ PTR,
             17145                           /* See IS_MESSAGE_A_DAD.  */
             17146    1           2 FRAME_INDEX UBIN,
             17147                           /* Index of the current frame.  */
PL6.E3A0      #001=QBA$ABORT File=QBA$ABORT.:E05TSI                              WED 07/30/97 04:39 Page:3    
             17148    1           2 FIELD_INDEX UBIN,
             17149                           /* Index of the field to re-get (if any).  */
             17150    1           2 STATUS UBIN,
             17151                           /* Returned status code:  0 -> Immediate return to level 0;
             17152                                                    >0 -> Status is ok.  */
             17153    1           2 FIELD_ERROR_STOP_INDEX UBIN,
             17154                           /* Passed index for QCG$GET_FIELD processing.  */
             17155    1           2 VISUAL_ATTRIBUTE UBIN;
             17156                           /* Matches %EQUs for QI#xxx_VISUAL_ATTR
             17157                              (currently, blinking or non-blinking message line).  */
             17164            %ENDIF;
       59    17167
       60    17168          %SET LISTMAC = '0'B;
       61    17169        %EJECT;
PL6.E3A0      #001=QBA$ABORT File=QBA$ABORT.:E05TSI                              WED 07/30/97 04:39 Page:4    
       62    17170    1       INSTR$ = QI_1.INSTR$;
       63    17171    1       QI_1.INSTR$ = ADDR ( INSTR$ -> QI$OP_ABORT.NEXT_CODE@P );
       64    17172
       65    17173    1       QI_1.A_REG = 0;
       66    17174
       67    17175    1       IF INSTR$ -> QI$OP_ABORT.OPERAND@P ~= 0
       68    17176    1       THEN
       69    17177    1   WE_GOTTA_STOP:
       70    17178    2         DO;
       71    17179
       72    17180    2           CALL QCS$SETUP_SOURCE;
       73    17181
       74    17182    2           QCM@MESS.MESSAGE_LINE_GZ_HEADER$ = QI_1.MESSAGE_LINE_GZ.HEADER$;
       75    17183    2           QCM@MESS.IS_MESSAGE_A_DAD = %YES;
       76    17184    2           QCM@MESS.MESSAGE$ = QI_1.DAD.S$;
       77    17185    2           QCM@MESS.VISUAL_ATTRIBUTE = %QI#OUTPUT_VISUAL_ATTRIBUTE;
       78    17186
       79    17187    2           CALL QCM$MESS ( QCM@MESS );
       80    17188
       81    17189    2           IF QCM@MESS.STATUS = %QI#USE_INVALID_RETURN
       82    17190    2           THEN
       83    17191    3             DO;
       84    17192                /*  Just like STOP_PAUSE, but ignore the ALTRETs.  */
       85    17193    3             END;
       86    17194
       87    17195    2           CALL QCM$MESS_PAUSE ( QCM@MESS );
       88    17196
       89    17197    2           IF QCM@MESS.STATUS = %QI#USE_INVALID_RETURN
       90    17198    2           THEN
       91    17199    3             DO;
       92    17200                /*  Just like STOP_PAUSE, but ignore the ALTRETs.  */
       93    17201    3             END;
       94    17202
       95    17203    2           CALL QCM$MESS_ERASE ( QCM@MESS );
       96    17204
       97    17205    2         END WE_GOTTA_STOP;
       98    17206
PL6.E3A0      #001=QBA$ABORT File=QBA$ABORT.:E05TSI                              WED 07/30/97 04:39 Page:5    
       99    17207    1       CALL QCE$ERROR ( %E$ABORT_EXECUTED );
      100    17208
      101    17209    1   END QBA$ABORT;
      102    17210

