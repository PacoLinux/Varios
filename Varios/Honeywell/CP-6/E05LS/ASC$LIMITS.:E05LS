      653    13742    2      IF AS_MOD_USER.LIMITS.FPOOLS_A(MODE) ~= '0'B
      654    13743    3      THEN DO;
      655    13744    3         IF AS_MOD_USER.LIMITS.FPOOLS_A.MFPOOLS(MODE) THEN
