PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:30   
      682    13771    1   CHECK_DEFAULT_MAX: PROC( TYPE, DEFAULT, MAX, SERVICE_NAME );
      683    13772
      684    13773
      685    13774
      686    13775        /*********************************************************************/
      687    13776        /*   Parameters      parameters      parameters      parameters      */
      688    13777        /*********************************************************************/
      689    13778
      690    13779    2   DCL TYPE          UBIN;
      691    13780    2   DCL DEFAULT       UBIN    ALIGNED;
      692    13781    2   DCL DEFAULT_SBIN  REDEF   DEFAULT SBIN ALIGNED;
      693    13782    2   DCL MAX           UBIN    ALIGNED;
      694    13783    2   DCL MAX_SBIN      REDEF   MAX     SBIN ALIGNED;
      695    13784    2   DCL 1 SERVICE_NAME ALIGNED,
      696    13785    2          2 CNT UBIN BYTE UNAL,
      697    13786    2          2 TXT CHAR(SERVICE_NAME.CNT);
      698    13787
      699    13788
      700    13789
      701    13790        %EJECT ;
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:31   
      702    13791                                                              /* Check that default
      703    13792                                                                 is not greater than
      704    13793                                                                  max.  */
      705    13794    2   IF TYPE = 0 /* Expire */
      706    13795    3   THEN DO;
      707    13796    3      IF DEFAULT <= MAX THEN RETURN;
      708    13797    3      IF DEFAULT_SBIN = -1 THEN AS_FIELD2 = NEVER_TEXTC;
      709    13798    4      ELSE DO;
      710    13799              %NUMBER( NUM2=DEFAULT );
             13815    4         CALL ASB$BINCHAR( AS_FIELD2.TEXT, 0, -2, DEFAULT );
      711    13829    4         END;
      712    13830           %NUMBER( NUM3=MAX );
             13854    3         CALL ASB$BINCHAR( AS_FIELD3.TEXT, 0, -3, MAX );
      713    13860    3      AS_FIELD1 = SERVICE_NAME;
      714    13861    3      END;
      715    13862    3   ELSE DO;
      716    13863    3      IF DEFAULT <= MAX THEN RETURN;
      717    13864    3      CALL CONCAT( AS_FIELD1.TEXT,
      718    13865    3           SUBSTR( AS_MODE_ARRY.TXT(MODE), 0, AS_MODE_ARRY.CNT(MODE) ),
      719    13866    3           ' ', SERVICE_NAME.TXT );
      720    13867    3      AS_FIELD1.COUNT = AS_MODE_ARRY.CNT(MODE) + SERVICE_NAME.CNT + 1;
      721    13868    3      IF TYPE = 1 /* Time */
      722    13869    4      THEN DO;
      723    13870              %NUMBER( NUM2="DEFAULT/6000", NUM3="MAX/6000" );
             13886    4         CALL ASB$BINCHAR( AS_FIELD2.TEXT, 0, -2, DEFAULT/6000 );
             13894    4         CALL ASB$BINCHAR( AS_FIELD3.TEXT, 0, -3, MAX/6000 );
      724    13900    4         END;
      725    13901    4      ELSE DO;
      726    13902              %NUMBER( NUM2=DEFAULT, NUM3=MAX );
             13918    4         CALL ASB$BINCHAR( AS_FIELD2.TEXT, 0, -2, DEFAULT );
             13926    4         CALL ASB$BINCHAR( AS_FIELD3.TEXT, 0, -3, MAX );
      727    13932    4         END;
      728    13933    3      END;
      729    13934
      730    13935    2   IF AS_WILD_USER THEN CALL ASB$FLUSH_ERR;
      731    13936        /**/
      732    13937        /*E*  ERROR:    ASC-E$DEFAULT_GT_MAX-C
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:32   
      733    13938              MESSAGE: ***  Default value is greater than maximum value for% %U1%.
