      660    13749    3         MAX     = AS_USERS.DFPOOLS(MODE);
      661    13750    3         CALL CHECK_DEFAULT_MAX(3, DEFAULT, MAX, FPOOLS_TEXTC );
      662    13751    3         AS_USERS.DFPOOLS(MODE) = DEFAULT;
      663    13752    3         END;
