      142    12996
      143    12997        %SET LISTSUB='1'B;
      144    12998        %LIST ;
      145    12999            %PARSE$OUT( NAME=P$NODE, STCLASS="BASED(NODE$)",
      146    13000                        NDTYPE=TYPE, NSUBLKS=SUBNODES) ;
             13001        /*F*  NAME:     PARSE$OUT
             13002              FUNCTION: a structure built by X$PARSE which corresponds to a "list",
             13003                  "alternative", "reducing alternative", or "koncat" node in
             13004                  the user's parse tree.
             13005        */
             13006    1   DCL 1 P$NODE BASED(NODE$) ALIGNED,
             13007    1         2 TEMP BIT(1) UNAL,
             13008    1         2 * BIT (11) UNAL,
             13009    1         2 TYPE UBIN (6) UNAL,
             13010        /*K* NDTYPE - specifies the type of parse node to which this PARSE$OUT
             13011                  block corresponds.  See the XU_SUBS_C file for the
             13012                  %EQU symbols whose values may appear in this field.
             13013        */
             13014    1         2 CODE UBIN HALF HALIGNED,
             13015        /*K* CODE - Contains the numeric "node code" specified by the user when
             13016                  the corresponding parse-node was defined.
             13017        */
             13018    1         2 SUBNODES UBIN HALF HALIGNED,
             13019        /*K* NSUBLKS - "number of sub-blocks".  This field contains the count of
             13020                  pointers actually available in the PARSE$OUT.SUBLK$ array.
             13021        */
             13022    1         2 SCRITCH UBIN HALF HALIGNED,
             13023        /*K* SCRITCH - an 18-bit scratch field reserved for the user.  X$PARSE
             13024                  initializes this field to zero when the PARSE$OUT structure
             13025                  is allocated, and never sets or examines it thereafter.
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:11   
             13026                  The name of this field may be changed when the PARSE$OUT macro
             13027                  is invoked by the user (say "SCRITCH=yourname"), and its data
             13028                  type may also be changed (say "SCRMODE=pl6datatype" and
             13029                  "SCRALGN=alignment").  BEWARE: this field MUST be 18-bits long,
             13030                  and be UNALIGNED, CALIGNED, or HALIGNED.
             13031        */
             13032    1         2 CPOS UBIN WORD,
             13033        /*K* CPOS - "character position" of this block.  This field will contain
             13034                  a character index (starting at 0) into the original parse
             13035                  string;  the index identifies the first character of the
             13036                  string which was accepted by this node (or by any of its
             13037                  sub-nodes).
             13038        */
             13039    1         2 SUBLK$(0:0) PTR UNAL;
             13040        /*K* SUBLK$ - an array of PL-6 PTRs.  The number of elements in this array
             13041                  is found in OUT$BLK.NSUBLKS;  each PTR will point to another
             13042                  OUT$BLK structure, or to an OUT$SYM structure.
             13043        */
      147    13045              %PARSE$SYM( NAME = OUT$SYM ,
      148    13046                          STCLASS="BASED(BLK$)") ;
             13047        /*F*  NAME:     PARSE$SYM
             13048              FUNCTION: holds "parse symbols".  Structures of this form are created
             13049                  by X$PARSE to hold those portions of the string being parsed
             13050                  which must be returned to the user - quoted strings, decimal
             13051                  or octal numbers, PL-6 identifiers, etc.
             13052        */
             13053    1   DCL 1 OUT$SYM BASED(BLK$) ALIGNED,
             13054    1         2 TEMP BIT(1) UNAL,
             13055    1         2 * BIT (11) UNAL,
             13056    1         2 NDTYPE UBIN (6) UNAL,
             13057        /*K* NDTYPE - see OUT$BLK.NDTYPE for details.
             13058        */
             13059    1         2 CODE UBIN HALF HALIGNED,
             13060        /*K* CODE - see OUT$BLK.CODE for details.
             13061        */
             13062    1         2 NSUBLKS UBIN HALF HALIGNED,
             13063        /*K* NSUBLKS - always contains the value 1.
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:12   
             13064        */
             13065    1         2 SCRITCH UBIN HALF HALIGNED,
             13066        /*K* SCRITCH - see OUT$BLK.SCRITCH for details.
             13067        */
             13068    1         2 CPOS UBIN WORD,
             13069        /*K* CPOS - see OUT$BLK.CPOS for details.
             13070        */
             13071    1         2 TEXTC$ PTR,
             13072        /*K* TEXTC$ - a pointer to a TEXTC-type string (a 1-byte count field
             13073                  followed by a variable number of characters).  Always
             13074                  points to OUT$SYM.COUNT in the current OUT$SYM block.
             13075        */
             13076    1         2 COUNT UBIN BYTE CALIGNED,
             13077        /*K* COUNT - contains the number of characters in OUT$SYM.TEXT.
             13078        */
             13079    1         2 TEXT CHAR(OUT$SYM.COUNT) CALIGNED;
             13080        /*K* TEXT - contains the text of a string, decimal number, PL-6
             13081                  identifier, or some other parser "symbol".
             13082        */
      149    13084        %PLIST ;
      150    13085        %EJECT ;
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:13   
      151    13086    1   ASC$EXPIRE: ENTRY( SUBND$ );
      152    13087
      153    13088        /*
      154    13089              ***             Begin procedure                 ***
      155    13090        */
      156    13091
      157    13092    1   CODE = SUBND$ -> P$NODE.SUBLK$(0) -> P$NODE.CODE;
      158    13093    1   BLK$ = SUBND$ -> P$NODE.SUBLK$(1);
      159    13094    2   DO SELECT( OUT$SYM.CODE );
      160    13095    2      SELECT( 13   /*  NEVER_VAL  */ );
      161    13096    2      TOTAL = -1;
      162    13097
      163    13098    2      SELECT( 18  /*  DEC_VAL  */ );
      164    13099    2      CALL CHARBIN( TOTAL, OUT$SYM.TEXT ) ;
      165    13100    3      IF TOTAL > BITBIN('777777'O) THEN DO;
      166    13101        /*E*  ERROR:    ASC-E$EXPIRE_DATE_TOO_LARGE-C
      167    13102              MESSAGE:  EXPIRE date too large.
      168    13103              MESSAGE1: Maximum of 262143 days.
      169    13104        */
      170    13105              %SCREAM( ERR#=E$EXPIRE_DATE_TOO_LARGE, LEVEL=ERROR, MID='A' );
      171    13120    3         RETURN ;
      172    13121    3         END ;  /* Then do */
      173    13122                                                                   /**/
      174    13123    2      END ; /* Do case( code ) */
      175    13124
      176    13125    2   DO SELECT( CODE );
      177    13126    2      SELECT( 54 /* MEXP */ );
      178    13127                                                            /*
      179    13128                                                                Check that max.
      180    13129                                                                does not exceed the
      181    13130                                                                project maximum */
      182    13131    2      AS_USERS.MEXP = TOTAL;
      183    13132    2      IF AS_CR_MOD_PROJ > 0
      184    13133    2      THEN CALL CHECK_PROJ_MAX( 0, AS_DFLT.MEXP, AS_USERS.MEXP, EXPIRE_TEXTC );
      185    13134    2      AS_MOD_USER.LIMITS.EXPIRE.MEXP = AS_TRUE;
      186    13135
      187    13136    2      SELECT( 55 /* DEXP */ );
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:14   
      188    13137    2      AS_USERS.DEXP = TOTAL;
      189    13138    2      AS_MOD_USER.LIMITS.EXPIRE.DEXP = AS_TRUE;
      190    13139
      191    13140    2      END;
      192    13141
      193    13142                                                              /* Test that default
      194    13143                                                                 not greater than max. */
      195    13144    1   CALL CHECK_DEFAULT_MAX( 0, AS_USERS.DEXP, AS_USERS.MEXP, EXPIRE_TEXTC );
      196    13145
      197    13146    1   RETURN;
      198    13147
      199    13148        %EJECT;
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:15   
      200    13149    1   ASC$TIME: ENTRY( SUBND$ );
      201    13150
      202    13151        /*
      203    13152              ***             Begin procedure                 ***
      204    13153        */
      205    13154
      206    13155    1   CODE = SUBND$ -> P$NODE.SUBLK$(0) -> P$NODE.CODE;
      207    13156    1   PTR$ = SUBND$ -> P$NODE.SUBLK$(1);
      208    13157    1   SUBS = PTR$ -> P$NODE.SUBNODES;
      209    13158
      210    13159    2   DO I = 0 TO SUBS - 1;
      211    13160    2      MODE = PTR$ -> P$NODE.SUBLK$(I) -> P$NODE.SUBLK$(0) -> P$NODE.CODE;
      212    13161    2      BLK$ = PTR$ -> P$NODE.SUBLK$(I) -> P$NODE.SUBLK$(1) -> P$NODE.SUBLK$(0);
      213    13162    2      CALL CHARBIN( VAL, OUT$SYM.TEXT );
      214    13163    2      VAL = VAL * 6000;
      215    13164    3      DO SELECT( CODE );
      216    13165    3         SELECT( 30 /* TIME_MAX */ );
      217    13166    3         AS_USERS.MTIME(MODE) = VAL;
      218    13167    3         IF AS_CR_MOD_PROJ > 0
      219    13168    3         THEN CALL CHECK_PROJ_MAX( 1, AS_DFLT.MTIME(MODE),
      220    13169    3                 AS_USERS.MTIME(MODE), TIME_TEXTC );
      221    13170    3         AS_MOD_USER.LIMITS.TIME_A.MTIME(MODE) = AS_TRUE;
      222    13171
      223    13172    3         SELECT( 31 /* TIME_DEF */ );
      224    13173    3         AS_USERS.DTIME(MODE) = VAL;
      225    13174    3         AS_MOD_USER.LIMITS.TIME_A.DTIME(MODE) = AS_TRUE;
      226    13175
      227    13176    3         END;
      228    13177
      229    13178    2      CALL CHECK_DEFAULT_MAX( 1, AS_USERS.DTIME(MODE), AS_USERS.MTIME(MODE), TIME_TEXTC
             13178               );
      230    13179    2      END;
      231    13180    1   RETURN;
      232    13181
      233    13182        %EJECT;
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:16   
      234    13183    1   ASC$MEMORY: ENTRY( SUBND$ );
      235    13184
      236    13185        /*
      237    13186              ***             Begin procedure                 ***
      238    13187        */
      239    13188
      240    13189    1   CODE = SUBND$ -> P$NODE.SUBLK$(0) -> P$NODE.CODE;
      241    13190    1   PTR$ = SUBND$ -> P$NODE.SUBLK$(1);
      242    13191    1   SUBS = PTR$ -> P$NODE.SUBNODES;
      243    13192
      244    13193    2   DO I = 0 TO SUBS - 1;
      245    13194    2      MODE = PTR$ -> P$NODE.SUBLK$(I) -> P$NODE.SUBLK$(0) -> P$NODE.CODE;
      246    13195    2      BLK$ = PTR$ -> P$NODE.SUBLK$(I) -> P$NODE.SUBLK$(1) -> P$NODE.SUBLK$(0);
      247    13196    2      CALL CHARBIN( VAL, OUT$SYM.TEXT );
      248    13197    3      DO SELECT( CODE );
      249    13198    3         SELECT( 31 /* MEM_MAX */ );
      250    13199    3         IF AS_CR_MOD_PROJ > 0
      251    13200    4         THEN DO;
      252    13201    4            PROJ_MAX = AS_DFLT.MMEM(MODE);
      253    13202    4            CALL CHECK_PROJ_MAX( 2, PROJ_MAX, VAL, MEMORY_TEXTC );
      254    13203    4            END;
      255    13204    3         IF VAL > 262143
      256    13205    4         THEN DO;
      257    13206        /*E* ERROR:   ASC-E$MEM_VAL_TOO_BIG-C
      258    13207             MESSAGE: Specified memory value is too large, using 262143.
      259    13208             MESSAGE1:262143 is the largest value that fits in a max or default mem field.
      260    13209        */
      261    13210                 %SCREAM (ERR#=E$MEM_VAL_TOO_BIG,LEVEL=WARN,MID='C');
      262    13225    4            AS_USERS.MMEM (MODE) = 262143;
      263    13226    4            END;
      264    13227    4         ELSE DO;
      265    13228    4            AS_USERS.MMEM (MODE) = VAL;
      266    13229    4            END;
      267    13230    3         AS_MOD_USER.LIMITS.MEM_A.MMEM(MODE) = AS_TRUE;
      268    13231
      269    13232    3         SELECT( 32 /* MEM_DEF */ );
      270    13233    3         IF VAL > 262143
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:17   
      271    13234    4         THEN DO;
      272    13235                 %SCREAM (ERR#=E$MEM_VAL_TOO_BIG,LEVEL=WARN,MID='C');
      273    13250    4            AS_USERS.DMEM (MODE) = 262143;
      274    13251    4            END;
      275    13252    4         ELSE DO;
      276    13253    4            AS_USERS.DMEM (MODE) = VAL;
      277    13254    4            END;
      278    13255    3         AS_MOD_USER.LIMITS.MEM_A.DMEM(MODE) = AS_TRUE;
      279    13256
      280    13257    3         END;
      281    13258
      282    13259    2      DEFAULT = AS_USERS.DMEM(MODE);
      283    13260    2      MAX     = AS_USERS.MMEM(MODE);
      284    13261    2      CALL CHECK_DEFAULT_MAX( 2, DEFAULT, MAX, MEMORY_TEXTC );
      285    13262    2      AS_USERS.DMEM(MODE) = DEFAULT;
      286    13263    2      END;
      287    13264    1   RETURN;
      288    13265
      289    13266        %EJECT;
PL6.E3A0      #001=ASC$LIMITS File=ASC$LIMITS.:E05TSI                            TUE 07/29/97 09:35 Page:18   
      290    13267    1   ASC$SERVICE: ENTRY;
      291    13268                                                              /**/
      292    13269        /*
      293    13270            ***            Begin procedure                    ***
      294    13271        */
      295    13272                                                              /* Save some of P_PCB. */
      296    13273    1   WORK$ = P_PCB.WORK$ ;
      297    13274    1   WKSZ = P_PCB.WKSZ ;
      298    13275    1   WK_USED = P_PCB.WK_USED ;
      299    13276    1   ROOT$ = P_PCB.ROOT$ ;
      300    13277                                                              /* Bump past area used. */
      301    13278    1   P_PCB.WORK$ = PINCRW( WORK$, P_PCB.WK_USED ) ;
      302    13279    1   P_PCB.WKSZ = WKSZ - P_PCB.WK_USED ;
      303    13280                                                              /* Save NODE$ */
      304    13281    1   T_NODE$ = NODE$ ;
      305    13282                                                              /* Set ROOT$ */
      306    13283    1   P_PCB.ROOT$ = ADDR( SERV_OPT_LIST ) ;
      307    13284                                                              /* Set prompt */
      308    13285    1   CALL ASB$SET_PROMPT ( %PROMPT_SUB ) ;
      309    13286    1   MORE = AS_TRUE ;
      310    13287    1   REMEMBER SERV_LOOP IN REMEM_VAR ;
      311    13288                                                              /**/
      312    13289    1   SERV_LOOP: ;
      313    13290                                                              /**/
