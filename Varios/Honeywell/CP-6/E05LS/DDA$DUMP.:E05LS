      461     7135    1   SAME_OUT: PROC;
      462     7136    3        IF OFFSET_SAVE + MAX_CELL = OFFSET THEN DO; /* Only one line */
      463     7137    3             SAVE_CALLED = '1'B;
      464     7138    3             CALL LINE_OUT;
      465     7139    3             SAVE_CALLED = '0'B;
