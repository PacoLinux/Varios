    7      %EQU S$NOTIOINT=171;     /* INTERRUPT HANDLER CALL ON OTHER THAN IO INTERRUPT */
    8      %EQU S$BADINTLVL=172;    /* INCOMPATIBLE INTERRUPT TYPE AND LEVEL             */
    9      %EQU S$ELFABORT=173;     /* ELF ABORT (SNAP)                                  */
   10      %EQU S$INVDLA=178;       /* INVALID DLA                                       */
   11      %EQU S$INVMIR=179 ;      /* Invalid DCTX for Mirrored Disk device.            */
   12      %EQU S$BADFC=180;        /* ILLEGAL LOGICAL FUNCTION CODE OR DEVICE COMMAND   */
   13      %EQU S$NOPMIR=181 ;      /* No operational Mirrored Disk device               */
   14      %EQU S$BADSTATE=191;     /* ILLEGAL DCT OR DQH STATE (SOFTWARE ERROR)         */
   15      %EQU S$BADSQHCQ=192;     /* CHAIN OF IDLE CHANNELS BROKEN (SOFTWARE ERROR)    */
   16      %EQU S$NRD=196;          /* NO REMOTE DEVICES                                 */
   17      %EQU S$BADIOQ=197 ;      /* BAD IOQ GET/RELEASE/SCHEDULE/POST                 */
   18      /* END NI_SCODE_C */

