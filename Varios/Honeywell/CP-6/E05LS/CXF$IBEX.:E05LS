PL6.E3A0      #002=CXF$FIND_STREAM File=CXF$IBEX.:E05TSI                         TUE 07/29/97 12:28 Page:106  
      514        1        /*M* Locate the requested stream for the TERMINAL and LDEV cmds */
      515        2        /*T***********************************************************/
      516        3        /*T*                                                         */
      517        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      518        5        /*T*                                                         */
      519        6        /*T***********************************************************/
      520        7        /*X*  DMC,DFC   */
      521        8        /*P*   NAME:   CXF$FIND_STREAM
      522        9               PURPOSE: To locate a stream that has the requested RES.
      523       10               DESCRIPTION: CXF$FIND_STREAM looks though the table of DCBs
      524       11                      to locate a DCB with requested RES.  CXF$FIND_STREAM will
      525       12                      return the DCB# of the dcb found, otherwise it will
      526       13                      return to the calling routine with CPSL_PARAM.ERRCODE set.
      527       14               REFERENCE: Programmers Reference Manual
      528       15        */
      529       16        /**********************************************************************/
      530       17        CXF$FIND_STREAM:
      531       18          PROC(STRM_NAME, STRM, DCB#, CPSL_PARAM) ;
      532       19              %INCLUDE CP_6 ;
      533      100              %INCLUDE B$ROSEG ;
      534      163              %INCLUDE B$JIT ;
      535      453              %INCLUDE CX_PERR_C ;
      536      524              %INCLUDE CX_MACROS_M ;
      537      575        /*
      538      576          *** Parameters
      539      577        */
      540      578    1   DCL STRM_NAME CHAR(2) ;
      541      579    1   DCL STRM CHAR(2) ;
      542      580    1   DCL DCB# UBIN WORD ;
      543      581        %CPSL_COMMAND_PARAMETERS (NAME=CPSL_PARAM, STCLASS=PARAM) ;
      544      599        /*
      545      600          *** Auto
      546      601        */
      547      602    1   DCL I UBIN WORD ;
      548      603    1   DCL RES CHAR(4) ;
      549      604        /*
      550      605          *** Symrefs
PL6.E3A0      #002=CXF$FIND_STREAM File=CXF$IBEX.:E05TSI                         TUE 07/29/97 12:28 Page:107  
      551      606        */
      552      607    1   DCL B$ROS$ PTR SYMREF ;
      553      608    1   DCL B$JIT$ PTR SYMREF ;
      554      609        %F$DCB ;
      555      666             /***** Error codes *****/
      556      667        %VLP_ERRCODE (FPTN=E_NOSUCHSTRM,FCG=CX,MID=F,ERR#=E$NOSUCHSTRM,STCLASS=CONSTANT);
      557      712        /**/
      558      713    1         CALL CONCAT(RES, STRM_NAME, STRM) ;
      559      714    2         DO I=1 TO B$ROS$->B$ROSEG.NUMDCBS ;
      560      715    2             IF DCBADDR(I)~=ADDR(NIL)
      561      716    2               THEN
      562      717    2                 IF DCBADDR(I)->F$DCB.FCD# AND DCBADDR(I)->F$DCB.RES#=RES
      563      718    2                   THEN
      564      719    3                     DO ;
      565      720    3                       DCB# = I ;
      566      721    3                       RETURN ;
      567      722    3                     END ; /* Then Do */
      568      723    2         END ; /* Do I */
      569      724        /**/
      570      725    1     CPSL_PARAM.ERRCODE = E_NOSUCHSTRM ;
      571      726    1     CPSL_PARAM.FLAGS.REPORTED_TO_USER = '0'B ;
      572      727    1         RETURN ;
      573      728
      574      729        /*E*    ERROR: CXF-E$NOSUCHSTRM-C
      575      730                MESSAGE: No such stream defined.
      576      731        */
      577      732    1   END   CXF$FIND_STREAM ;

