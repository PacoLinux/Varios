    6      /*T***********************************************************/
    7      /* KH_ERRORS_C range for codes is 1576 - 1599. */
    8      %EQU E$ERR_IO = 1576;  /* IONAK on IO (not IOLD) instruction */
    9      %EQU E$ERR_IOLD_OTPCCBCTR = 1577;  /* IONAK on IO (output CCB control) instruction */
   10      %EQU E$ERR_IOLD = 1578;  /* IONAK on IOLD or IOLD/(ram data transfer) instruction */
   11      %EQU E$ERR_BADBLKWRT = 1579;  /* Status error on block mode write */

