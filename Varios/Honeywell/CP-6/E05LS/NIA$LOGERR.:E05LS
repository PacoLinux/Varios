      124     1176    1           CALL NIF$DCWLIST(N$REQ,NI$IOERR.IDCW,NI$IOERR.DCW,NI$IOERR.MEMADR);
      125     1177    1           NI$IOERR.COUNT=NI$IOERR.COUNT+1;
      126     1178    1           N$REQ.ERR$=ERR$;
      127     1179    1   NOPKTS: RETURN;
      128     1180    1   NIA$FWLOAD: ENTRY(N$REQ,TYPE);
      129     1181    1           DCT$=ADDR(N$REQ);
      130     1182    1           EL$FWLOAD.DEVNM=NI$DCT.DEVNM;
      131     1183    1           EL$FWLOAD.LOADED=~NI$DCT.DFLG.RELOAD;
      132     1184    1           CALL ELA$SYSLOG(TYPE,EL$FWLOAD,SIZEW(EL$FWLOAD),'0'B,1,ERRCD);
      133     1185    1           RETURN;
      134     1186    1   NIA$SPURINT: ENTRY(N$REQ,TYPE,INTLVL,STATUS);
      135     1187    2           DO INHIBIT ;
