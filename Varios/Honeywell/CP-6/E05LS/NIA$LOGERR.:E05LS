       64     1116    1           ERR$=N$REQ.ERR$;
       65     1117    1           DCT$=N$REQ.DCT$;
       66     1118    1           IF NI$DCT.STATUS.DOWN THEN RETURN;
       67     1119    1           DQH$=NI$DCT.DQH$;
       68     1120    2           IF ERR$=ADDR(NIL) THEN DO;
       69     1121    2              IOERR='0'B;
       70     1122    2              IOERR.THRESH = '1111'B;
       71     1123    2              IOERR.MPC$ = NI$DQH.MPC$;
