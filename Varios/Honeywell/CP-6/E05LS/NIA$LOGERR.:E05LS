      142     1194    2              EL$SPURINT.STATUS      = NI$DQH.STATUS ;
      143     1195    2              CALL ELA$SYSLOG ( TYPE, EL$SPURINT, SIZEW(EL$SPURINT), '0'B, 1, ERRCD ) ;
      144     1196    2              RETURN ;
