       81     1133    1           IOERR.IOCHAN.DVN=NI$DCT.DVN;
       82     1134    2           IF ERR$=ADDR(NIL) THEN DO;
       83     1135    2              CALL NIQ$GET(ERR$) ALTRET(NOPKTS);
       84     1136    3              IF REQ.IOFLG.IOS THEN DO;
       85     1137    3                 CALL NIQ$RELS(REQ.IOS$);
       86     1138    3                 REQ.IOFLG.IOS='0'B;
       87     1139    3                 END;
       88     1140    2              NI$IOERR=IOERR;
       89     1141    2              END;
       90     1142    2           ELSE DO;
       91     1143    2              IF (NI$IOERR.STATUS.MAJOR = IOERR.STATUS.MAJOR) AND
       92     1144    2                (NI$IOERR.STATUS.MINOR = IOERR.STATUS.MINOR) AND
       93     1145    2                (NI$IOERR.STATUS.IOM = IOERR.STATUS.IOM)
       94     1146    2              THEN                         /* REPEAT OF THIS ERROR               */
       95     1147    3              DO;
       96     1148    3                 IF IOERR.IOCHAN.IOM ~= NI$IOERR.IOCHAN.IOM
       97     1149    3                 THEN NI$IOERR.THRESH.IOM = '0'B;
       98     1150    3                 IF (IOERR.IOCHAN.CHAN ~= NI$IOERR.IOCHAN.CHAN) OR
       99     1151    3                   (IOERR.IOCHAN.IOM ~= NI$IOERR.IOCHAN.IOM)
      100     1152    3                 THEN NI$IOERR.THRESH.CHANNEL = '0'B;
PL6.E3A0      #001=NIA$LOGERR File=NIA$LOGERR.:E05TSI                            WED 07/30/97 03:27 Page:4    
      101     1153    3                 IF NI$DQH.MPC$ ~= ADDR(NIL) THEN
      102     1154    3                    IF NI$IOERR.MPC$->NI$DCT.DEVNM ~= NI$DQH.MPC$->NI$DCT.DEVNM
      103     1155    3                    THEN NI$IOERR.THRESH.MPC = '0'B;
      104     1156    3                 END;                      /* IF REPEAT                          */
      105     1157    2              ELSE
      106     1158    3              DO;
      107     1159    3                 CALL NIA$RELERR(N$REQ);
      108     1160    3                 CALL NIQ$GET(ERR$) ALTRET(NOPKTS);
      109     1161    4                 IF REQ.IOFLG.IOS THEN DO;
      110     1162    4                    REQ.IOFLG.IOS='0'B;
      111     1163    4                    CALL NIQ$RELS(REQ.IOS$);
      112     1164    4                    END;
