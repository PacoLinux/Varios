        8        8        NIA$LOGERR: PROC(N$REQ,TYPE,INTLVL,STATUS);
        9        9        /*D*    NAME:    NIA$LOGERR
       10       10                INTERFACE:  NIQ$GET
       11       11                            ELA$SYSLOG
       12       12                INPUT:   N$REQ - request packet for which error occured.
       13       13                         TYPE - Type code for error log.
       14       14                DESCRIPTION:
       15       15                   If an error packet has already been obtained for this request
       16       16                a check is made to see if the status and type are the same as the previous
       17       17                error.  If they are the same the error count in the error packet
       18       18                is incremented and no further action is taken.  If the status
       19       19                is not the same the old error packet is sent to the error logger.
       20       20                If necessary a new error packet is obtained through NIQ$GET and
       21       21                the fields in the EL$IOERR packet are filled in.  This packet is
       22       22                then attached to the NI$REQ packet.                        */
       23       23        /*
       24       24                Includes
       25       25        */
       26       26        %INCLUDE                EL$TABLES ;
       27      349        %INCLUDE                EL_SUBS_C ;
       28      433        %INCLUDE                N$REQ ;
       29      507        %INCLUDE                NI$ERRLOG ;
       30      573        %INCLUDE                NI$TABLES ;
       31      789        /*
       32      790                Parameters
       33      791        */
       34      792    1   DCL 1 INTLVL            UBIN(18) PARAM ;
       35      793        %N$REQ                  ( NAME=N$REQ, STCLASS=PARAM ) ;
       36      851    1   DCL 1 STATUS            BIT(72) DALIGNED PARAM ;
       37      852    1   DCL 1 TYPE              UBIN WORD ALIGNED PARAM ;
PL6.E3A0      #001=NIA$LOGERR File=NIA$LOGERR.:E05TSI                            WED 07/30/97 03:27 Page:2    
