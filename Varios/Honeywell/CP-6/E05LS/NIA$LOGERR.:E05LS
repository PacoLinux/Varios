       43      858    1   DCL 1 NIF$DCWLIST       ENTRY(4) ;
       44      859    1   DCL 1 NIQ$GET           ENTRY(1) ALTRET ;
       45      860    1   DCL 1 NIQ$RELS          ENTRY(1) ALTRET ;
       46      861        /*
       47      862                Auto
       48      863        */
       49      864    1   DCL 1 DCT$              PTR ALIGNED AUTO ;
       50      865    1   DCL 1 DQH$              PTR ALIGNED AUTO ;
       51      866        %EL$FWLOAD              ( NAME=EL$FWLOAD, STCLASS=AUTO ) ;
       52      869        %EL$SPURINT             ( NAME=EL$SPURINT, STCLASS=AUTO ) ;
       53      873    1   DCL 1 ERR$              PTR ALIGNED AUTO ;
       54      874    1   DCL 1 ERRCD             SBIN WORD ALIGNED AUTO ;
       55      875        %NI$IOERR               ( NAME=IOERR, STCLASS=AUTO ) ;
