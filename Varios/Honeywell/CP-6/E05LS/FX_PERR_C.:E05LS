    8      %SUB E$NOVOL=12/*E$NOVOL*/;             /* NO VOLUMES IN SET ARE UP*/
    9      %SUB E$VOLDOWN=13/*E$VOLDOWN*/;         /* VOLUME XX IS NOT UP*/
   10      %SUB E$2RECON=14/*E$2RECON*/;      /* CAN ONLY RECONSTRUCT ONE SET AT ONCE    */
   11      %SUB E$NOVSN=15/*E$NOVSN*/;             /* VSN REQUIRED TO CTG UNMOUNTED SET  */
   12      %SUB E$DUPVSN=16/*E$DUPVSN*/;           /* SAME VSN IN SNLIST TWICE           */
   13      %SUB E$NEEDSCR=17/*E$NEEDSCR*/; /* DONT USE ALL OF SPLITVOL */
   14      %SUB E$NOPROJ=18/*E$NOPROJ*/; /* NO PROJECT OR NOT YOURS */
   15      %SUB E$PROJCMD=19/*E$PROJCMD*/; /* NON-PROJECT-MODE COMMAND */
   16      %SUB E$PROJPSN=20/*E$PROJPSN*/; /* NO PSN FOR PROJECT CMD   */
   17      %SUB E$PROJDFT=21/*E$PROJDFT*/; /* ERR ON PROJECT DFLT ATTR */
   18      %SUB E$PROJNA=22/*E$PROJNA*/; /* ACCOUNT NO IN PROJECT */
   19      %SUB E$PROJOVER=23/*E$PROJOVER*/; /* PROJECT FULL OF GRANS OR ACCTS */
   20      %SUB E$PROJEX=24/*E$PROJEX*/; /* ACCOUNT EXISTS, CANNOT CREATE */
   21      %SUB E$PROJSK=25/*E$PROJSK*/; /* ACCT NAME DOESNT MATCH SKELETON */
   22      %SUB E$DEFER=26/*E$DEFER*/; /* SUPER REQUEST DEFERRED-SET NOT UP */
   23      %SUB E$NLIST=27/*E$NLIST*/; /* SUPER REQUEST DENIED-SET NOT UP */
   24      %SUB E$FILECNT=40/*E$FILECNT*/;         /* BAD DIRECTORY IN RECONSTRUCT       */
   25      %SUB E$MAXERR=41/*E$MAXERR*/;           /* TOO MANY ERRORS IN RECONSTRUCT     */
   26      %SUB E$BIGSECT=42/*E$BIGSECT*/;        /* SECTION >262143 ABORTS SCRATCH CMD */
