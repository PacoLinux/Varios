        1        1        /*M* KYK$KEYIN  Handles all URP-directed KEYINs */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X*   DMR,PLM=7,IND=3,CTI=3,SDI=3,MCL=10,CSI=3,DTI=3,DCI=6,
        8        8               ECI=0,DIN=7,CSU=3,AND      */
        9        9        KYK$KEYIN: PROC(KY$CTX,KEYIN);
       10       10        /*D* NAME:        KYK$KEYIN
       11       11             PURPOSE:     To handle KEYINs sent to URP devices
       12       12             CALL:        CALL KYK$KEYIN (KY$CTX,KEYIN);
       13       13                             where: KY$CTX - context block for the KEYINed device
       14       14                                    KEYIN - Value specifying which KEYIN
       15       15             DESCRIPTION: KYK$KEYIN first DISABLEs to the URP device interrupt
       16       16                          level. If the target device is not expecting a KEYIN,
       17       17                          the KEYIN will be ignored,
       18       18                          otherwise, the KEYIN is processed as follows:
       19       19
       20       20                          o Stop the current timed message
       21       21                          o Zero the RETRY count
       22       22                          o Change the STATE to IDLE if currently in OPWAIT_ON.
       23       23                            Otherwise change the STATE to RDYWAIT and put out
       24       24                            a new timed message.
       25       25
       26       26                          PROCEED KEYIN:
       27       27                             o Set the PRINTED bit for each remaing record
       28       28                               in the queue for output devices, or
       29       29                               set the BUF_VALID bit for input devices
       30       30                             o Call the appropriate scheduler. The output unit
       31       31                               record scheduler is not called unless the new
       32       32                               state is IDLE.
       33       33                               RDYWAIT. Otherwise cancel timed msg,
       34       34                               set state to IDLE and call the appropriate
       35       35                               scheduler
       36       36
       37       37                          RETRY KEYIN:
PL6.E3A0      #001=KYK$KEYIN File=KYK$KEYIN.:E05TSI                              WED 07/30/97 02:42 Page:2    
       38       38                             o Call the appropriate scheduler. The output unit
       39       39                               record scheduler is not called unless the new
       40       40                               state is IDLE.
       41       41
       42       42                          Finally, ENABLE back to the previous level and RETURN.
       43       43        */
       44       44        %INCLUDE GH_HARDWARE_M;
       45      142        %INCLUDE KY_URP_M;
       46      437        %INCLUDE KY_SUBS_C;
       47      526        %INCLUDE KH$STT;
       48      556        %INCLUDE OC_SUBS_C;
       49     1287        %INCLUDE KV_GLBCNS_E;
       50     1628        /**/
       51     1629        %KY$CTX;
       52     1806    1          DCL KEYIN UBIN WORD;
       53     1807        /**/
       54     1808    1          DCL OUTPUT$ PTR;
       55     1809    1          DCL SIZE UBIN WORD;
       56     1810        %KY$REC (STCLASS="BASED(OUTPUT$)");
       57     1849        %KY$OUTMRK (STCLASS="BASED(OUTPUT$)");
       58     1866        /**/
       59     1867        %KV_USR_EVT_ID_E;
       60     1887        %KV_USR_FNC_E;
       61     1903        /**/
       62     1904    1          DCL KYN$ENABLE ENTRY;
       63     1905    1          DCL KYN$DISABLE ENTRY;
       64     1906    1          DCL KYK$OPCON ENTRY(2);
       65     1907    1          DCL KYS$IUR_SCHED ENTRY(2);
       66     1908    1          DCL KYS$OUR_SCHED ENTRY(1) ALTRET;
       67     1909    1          DCL KYP$PROCESS ENTRY(1);
       68     1910        /**/
       69     1911        %EJECT;
PL6.E3A0      #001=KYK$KEYIN File=KYK$KEYIN.:E05TSI                              WED 07/30/97 02:42 Page:3    
       70     1912    1          CALL KYN$DISABLE;
       71     1913    1          IF KY$CTX.DVC_STATE=%KY#ST_OPWAIT_ON
       72     1914    1             OR KY$CTX.DVC_STATE=%KY#ST_OPWAIT_OFF
       73     1915    1             OR KY$CTX.DVC_STATE=%KY#ST_RDYWAIT THEN
       74     1916    2             DO;
       75     1917    2                CALL KYK$OPCON(KY$CTX,OC_STTM);
       76     1918    2                KY$CTX.RTRYNUM = 0;
       77     1919    2                IF KY$CTX.DVC_STATE=%KY#ST_OPWAIT_ON AND
       78     1920    2                   KY$CTX.DVCTYP ~= %KY#TYP_CR THEN
       79     1921    2                   KY$CTX.DVC_STATE=%KY#ST_IDLE;
       80     1922    2                ELSE
       81     1923    3                   DO;
       82     1924    3                      KY$CTX.DVC_STATE = %KY#ST_RDYWAIT;
       83     1925    3                      CALL KYK$OPCON(KY$CTX,OC_WRTM);
       84     1926    3                   END;
       85     1927    3                DO CASE(KY$CTX.DVCTYP);
       86     1928    3                   CASE(%KY#TYP_LP,%KY#TYP_CP);
       87     1929    3                      IF KEYIN=KEY_PROCEED# THEN
       88     1930    4                         DO;
       89     1931    4                            KY$CTX.Q.OUTPUT$->KY$REC.PRINTED='1'B;
       90     1932    4                         END;              /* IF KEYIN = PROCEED                 */
       91     1933    3                      IF KY$CTX.DVC_STATE=%KY#ST_IDLE THEN
       92     1934    3                         CALL KYS$OUR_SCHED(KY$CTX) ALTRET(GO_RDYWAIT);
       93     1935    3                   CASE(%KY#TYP_CR);
       94     1936    3                      IF KEYIN=KEY_PROCEED# THEN
       95     1937    3                         CALL KYP$PROCESS(KY$CTX);
       96     1938    3                      ELSE
       97     1939    3                         CALL KYS$IUR_SCHED(KY$CTX,%KY#READ);
       98     1940    3                END;
       99     1941    3                DO WHILE('0'B);
      100     1942    3   GO_RDYWAIT:     KY$CTX.DVC_STATE=%KY#ST_RDYWAIT;
      101     1943    3                   CALL KYK$OPCON(KY$CTX,OC_WRTM);
      102     1944    3                END;
      103     1945    2             END;
      104     1946    1          CALL KYN$ENABLE;
      105     1947    1          RETURN;
      106     1948    1   END KYK$KEYIN;
