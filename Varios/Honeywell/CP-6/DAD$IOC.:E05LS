PL6.E3A0      #001=DAD$IOC File=DAD$IOC.:E05TSI                                  TUE 07/29/97 12:44 Page:5    
      122     4469    1   PRINTSTATS: PROC;
      123     4470        /* */
      124     4471    2          CALL DAF$INIT(STATHDR);
      125     4472
      126     4473    2          CALL DAS$SDMONPTR(%M#FMC_TYPTBL$, TYPTBL$);
      127     4474    2          CALL DAM$MAP(TYPTBL$, TYPTBL$,,, DA_MONISDATA);
      128     4475    2          TYPTBL$=TYPTBL$->PTR$;
      129     4476    2          CALL DAM$MAP(TYPTBL$, TYPTBL$);
      130     4477
      131     4478    2          CALL DAS$SDMONPTR(%M#FMC_STATS$, STATS$);
      132     4479    2          CALL DAM$MAP(STATS$, STATS$,,, DA_MONISDATA);
      133     4480    2          STATS$=STATS$->PTR$;
      134     4481    2          CALL DAM$MAP(STATS$, STATS$);
      135     4482
      136     4483    3          DO I=0 TO %CACHETYPES-1;
      137     4484    3             OLDEST=FMC$TYPTBL.OLDEST(I);
      138     4485    3             NEWEST=FMC$TYPTBL.NEWEST(I);
      139     4486    3             HITZERO=FMC$STATS.HITZERO(I);
      140     4487    3             HITNONZERO=FMC$STATS.HITNONZERO(I);
      141     4488    3             FAILED=FMC$STATS.FAILED(I);
      142     4489    3             ATTGET=FMC$STATS.ATTGET(I);
      143     4490    3             ATTPUT=FMC$STATS.ATTPUT(I);
      144     4491    3             UNUSED=FMC$STATS.UNUSED(I);
      145     4492
      146     4493    3             CALL DAF$WRITE(VECTOR(STATSFMT),
      147     4494    3                VECTOR(GTYP(I)),
      148     4495    3                VECTOR(OLDEST),
      149     4496    3                VECTOR(NEWEST),
      150     4497    3                VECTOR(UNUSED),
      151     4498    3                VECTOR(ATTGET),
      152     4499    3                VECTOR(ATTPUT),
      153     4500    3                VECTOR(FAILED),
      154     4501    3                VECTOR(HITZERO),
      155     4502    3                VECTOR(HITNONZERO));
      156     4503
      157     4504    3          END;  /* DO I */
      158     4505
