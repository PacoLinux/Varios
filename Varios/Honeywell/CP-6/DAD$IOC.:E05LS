      165     4512    2          CALL DAM$MAP(CACHE$, CACHE$,,, DA_MONISDATA);
      166     4513    2          CACHE$=CACHE$->PTR$;
      167     4514    2          CALL DAM$MAP(CACHE$, CACHE$);
      168     4515
      169     4516    2          CALL DAF$INIT(CACHEHDR);
      170     4517
      171     4518    2          IF CACHEFLG = 1 THEN
      172     4519    3             DO;   /* ENTRIES SPECIFIED */
      173     4520    4             DO I=0 TO TOP.NSUBLKS-1;
      174     4521    5                DO CASE(TOP.SUBLK$(I)->OUT.CODE);
      175     4522    5                   CASE(%DA_IOC_RANGE);
      176     4523    5                      CALL DAA$VALUE(TOP.SUBLK$(I)->OUT.SUBLK$(0)->OUT, N1);
      177     4524    5                      CALL DAA$VALUE(TOP.SUBLK$(I)->OUT.SUBLK$(1)->OUT, N2);
      178     4525    5                      IF N2 >= N1 THEN
      179     4526    6                         DO J=N1 TO N2;
      180     4527    6                            CALL PRINTONE(J);
      181     4528    6                         END;
      182     4529
      183     4530    5                   CASE(%DA_IOC_NUM);
      184     4531    5                      CALL DAA$VALUE(TOP.SUBLK$(I)->OUT.SUBLK$(0)->OUT, N1);
      185     4532    5                      CALL PRINTONE(N1);
      186     4533
      187     4534    5                END;  /* CASE */
      188     4535    4             END;  /* DO I */
      189     4536    3             END;  /* ENTRIES SPECIFIED */
      190     4537    2          ELSE
      191     4538    3             DO I=1 TO CACHESIZ;
      192     4539    3                IF FMC$CACHETBL.SRDA(I) > 0 THEN
      193     4540    3                   CALL PRINTONE(I);
      194     4541    3             END;
      195     4542
      196     4543    2          RETURN;
      197     4544    2          END PRINTCACHE;
      198     4545               %EJECT;
PL6.E3A0      #001=DAD$IOC File=DAD$IOC.:E05TSI                                  TUE 07/29/97 12:44 Page:8    
      199     4546    1   PRINTONE: PROC(N);
      200     4547    2          DCL N SBIN;
      201     4548        /* */
      202     4549    2          IF N < 1 OR N > CACHESIZ THEN
      203     4550    2             RETURN;
      204     4551
      205     4552    2          TYPE=GTYP(FMC$CACHETBL.TYPE(N));
      206     4553    2          SRDA=FMC$CACHETBL.SRDA(N);
      207     4554    2          FLINK=FMC$CACHETBL.FLINK(N);
      208     4555    2          BLINK=FMC$CACHETBL.BLINK(N);
      209     4556    2          USECNT=FMC$CACHETBL.USECNT(N);
      210     4557    2          UPCOUNT=FMC$CACHETBL.UPCOUNT(N);
      211     4558    2          SETX=FMC$CACHETBL.SETX(N);
      212     4559    2          IO=BITBIN(FMC$CACHETBL.FLAGS.IO(N));
      213     4560    2          WAIT=BITBIN(FMC$CACHETBL.FLAGS.WAIT(N));
      214     4561    2          ERR=BITBIN(FMC$CACHETBL.FLAGS.ERR(N));
      215     4562    2          BUFUP=BITBIN(FMC$CACHETBL.FLAGS.BUFUP(N));
      216     4563    2          AGE=FMC$CACHETBL.AGE(N);
      217     4564
      218     4565    2          CALL DAF$WRITE(VECTOR(CACHEFMT),
      219     4566    2             VECTOR(N),
      220     4567    2             VECTOR(IO),
      221     4568    2             VECTOR(WAIT),
      222     4569    2             VECTOR(ERR),
      223     4570    2             VECTOR(BUFUP),
      224     4571    2             VECTOR(TYPE),
      225     4572    2             VECTOR(SRDA),
      226     4573    2             VECTOR(FLINK),
      227     4574    2             VECTOR(BLINK),
      228     4575    2             VECTOR(USECNT),
