        6         6             /* EVALUATION OF MONADIC OPERATORS */
        7         7
        8         8             #include "Lpars:h"
        9         9
       10        10             ch7mon(oper, pval)
       11        11              register int *pval;
       12        12             {
       13        13    1         switch (oper)  {
       14        14    2         case '~':
       15        15    2            *pval = ~(*pval);
       16        16    2            break;
       17        17    2         case '-':
       18        18    2            *pval = -(*pval);
       19        19    2            break;
       20        20    2         case '!':
       21        21    2            *pval = !(*pval);
       22        22    2            break;
       23        23    2         }
       24        24    1        }
       25        25
