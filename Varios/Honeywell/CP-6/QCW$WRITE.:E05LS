        1        1        /*M* QCW$WRITE:  Screen WRITE or DISPLAY pre-processing.  */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QCW$WRITE
       10       10              PURPOSE:      Do pre-M$WRITE processing of fields.
       11       11              DESCRIPTION:  Edits the contents of fields, then calls QCI$IO_WRITE
       12       12                            before returning to higher-level callers.
       13       13        */
       14       14
       15       15        QCW$WRITE: PROC ( QCW@WRITE );
       16       16
       17       17        /*D*  NAME:         QCW$WRITE
       18       18              CALL:         CALL QCW$WRITE ( QCW@WRITE );
       19       19              PARAMETERS:   QCW@WRITE is a structure which contains information
       20       20                            concerning the fields to be edited and written; it's
       21       21                            generated by invoking the QCW@WRITE %MACRO within the
       22       22                            QCW_WRITE_M %INCLUDE file (see commentary within
       23       23                            that file).
       24       24              INTERFACE:    Called by routines wishing to perform perform
       25       25                            non-message-line terminal M$WRITEs, and further
       26       26                            wishing the output to be edited before the write in
       27       27                            typical FPL fashion (QBD$DISPLAY, QBW$WRITE_FORM).
       28       28              ENVIRONMENT:  Nothing unusual.
       29       29              INPUT:        QI_1, etc.
       30       30              OUTPUT:       QI_1, etc.
       31       31              SCRATCH:      Various otter stuff.
       32       32              DESCRIPTION:  The calling routine (DISPLAY, or WRITE_FORM)
       33       33                            sets up the starting and stopping indices for fields
       34       34                            within the form to be written.
       35       35                            The editing includes:
       36       36                            1)  filling the fields with any applicable
       37       37                                FILL CHARACTERS.
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:2    
       38       38                            QCI$IO_WRITE is then called to do the actual I-O.
       39       39                              (This module may seem like a waste, since only FILL
       40       40                            CHARACTER filling is done, but it makes the terminal
       41       41                            read and write paths even.  JJL, C00.)
       42       42        */
       43       43
       44       44          %INCLUDE LCP_6;
       45      235
       46      236          %INCLUDE QG_MACRO_M;
       47      373          %INCLUDE QI_EQUATES_E;
       48      732          %INCLUDE QI_PERR_C;
       49      900          %INCLUDE QI_MACROS_M;
       50     5958          %QI$ALL;
       51    17071
       52    17072    1     DCL FILL_INCREMENT SBIN;
       53    17073    1     DCL FILL_INDEX SBIN;
       54    17074    1     DCL FILL_INDEX_STOP SBIN;
       55    17075    1     DCL FIELD_DAD$ PTR;
       56    17076    1     DCL FIELD_DAD_P$ PTR;
       57    17077    1     DCL FIELD_INDEX UBIN;
       58    17078    1     DCL FILL_CHARACTER UBIN;
       59    17079    1     DCL 1 FILL_CHARACTER_MINIMUM_SIZE REDEF FILL_CHARACTER,
       60    17080    1           2 FILL_CHARACTER UBIN(8) UNAL,
       61    17081    1           2 MINIMUM_SIZE UBIN(8) UNAL;
       62    17082    1     DCL GZ_FIELD$ PTR;
       63    17083
       64    17084          %SET LISTMAC = '1'B;
       65    17085
       66    17086          %INCLUDE QCI_IO_WRITE_M;
       67    17121          %QCI@IO_WRITE;
             17122            %BEGIN_MACRO ( STCLASS = STCLASS );
             17123            %SET LISTDIR = '0'B;
             17136    1     DCL 1 QCI@IO_WRITE ALIGNED,
             17137    1           2 FRAME_START_INDEX UBIN,
             17138                          /* Starting index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             17139    1           2 FRAME_STOP_INDEX UBIN,
             17140                          /* Stopping index into QI$FORM_FILE.FRAME_LIST_A@P.  */
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:3    
             17141    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             17142                          /* If IS_FIELD_INDEXES_VALID, use them;
             17143                             else use all the fields in the frame.  */
             17144    1           2 FIELD_START_INDEX UBIN,
             17145                          /* Starting index into QI$GZ_FIELD table.  */
             17146    1           2 FIELD_STOP_INDEX UBIN,
             17147                          /* Stopping index into QI$GZ_FIELD table.  */
             17148    1           2 STATUS UBIN,
             17149                          /* Returned status code:  0 -> Immediate return to level 0;
             17150                                                   >0 -> Status is ok.  */
             17151    1           2 FIELD_ERROR_STOP_INDEX UBIN;
             17152                          /* Passed index for QCG$GET_FIELD processing.  */
             17159            %ENDIF;
       68    17162
       69    17163          %INCLUDE QCW_WRITE_M;
       70    17198          %QCW@WRITE;
             17199            %BEGIN_MACRO ( STCLASS = STCLASS );
             17200            %SET LISTDIR = '0'B;
             17213    1     DCL 1 QCW@WRITE ALIGNED,
             17214    1           2 FRAME_START_INDEX UBIN,
             17215                          /* Starting index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             17216    1           2 FRAME_STOP_INDEX UBIN,
             17217                          /* Stopping index into QI$FORM_FILE.FRAME_LIST_A@P.  */
             17218    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             17219                          /* If IS_FIELD_INDEXES_VALID, use them;
             17220                             else use all the fields in the frame.  */
             17221    1           2 FIELD_START_INDEX UBIN,
             17222                          /* Starting index into QI$GZ_FIELD table.  */
             17223    1           2 FIELD_STOP_INDEX UBIN,
             17224                          /* Stopping index into QI$GZ_FIELD table.  */
             17225    1           2 STATUS UBIN,
             17226                          /* Returned status code:  0 -> Immediate return to level 0;
             17227                                                   >0 -> Status is ok.  */
             17228    1           2 FIELD_ERROR_STOP_INDEX UBIN;
             17229                          /* Passed index for QCG$GET_FIELD processing.  */
             17236            %ENDIF;
       71    17239
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:4    
       72    17240          %SET LISTMAC = '0'B;
       73    17241
       74    17242    1     DCL QCE$ERROR_INTERNAL ENTRY (4);
       75    17243    1     DCL QCI$IO_WRITE ENTRY (1);
       76    17244
       77    17245        %EJECT;
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:5    
       78    17246
       79    17247            %IF QI#DEBUG;
       80    17248
       81    17249    1       IF QCW@WRITE.FIELD_START_INDEX >
       82    17250    1         QI_1.CURRENT.FRAME_STATUS$ -> QI$FRAME_STATUS.GZ.FIELD_STOP_INDEX
       83    17251    1       THEN
       84    17252    1         CALL QCE$ERROR_INTERNAL ( %E$INTERNAL_INTERPRETER_ERROR );
       85    17253
       86    17254            %ENDIF;
       87    17255
       88    17256    1       GZ_FIELD$ = PINCRW (
       89    17257    1         QI_1.CURRENT.FRAME_STATUS$ -> QI$FRAME_STATUS.GZ.START$,
       90    17258    1         QCW@WRITE.FIELD_START_INDEX * SIZEW (QI$GZ_FIELD)
       91    17259    1         );
       92    17260
       93    17261    1   PRE_EDIT_THEM_FIELDS:
       94    17262    2       DO FIELD_INDEX = QCW@WRITE.FIELD_START_INDEX TO QCW@WRITE.FIELD_STOP_INDEX;
       95    17263
       96    17264    2   RETRY_FIELD_PRE_EDITING:
       97    17265    2         FIELD_DAD$ = GZ_FIELD$ -> QI$GZ_FIELD.DESCRIPTION.FIELD_DAD$;
       98    17266              /*  Increment past any level_multipliers to get to the
       99    17267                  P_ITEM_A stuff.  */
      100    17268    2         FIELD_DAD_P$ = ADDR (
      101    17269    2           FIELD_DAD$ -> QI$FIELD_DAD.VARIABLE_LENGTH_ITEMS.LEVEL_MULTIPLIER_A
      102    17270    2           ( FIELD_DAD$ -> QI$FIELD_DAD.WORD0_FLAGS.DIMENSION_COUNT )
      103    17271    2           );
      104    17272
      105    17273    2         IF FIELD_DAD_P$ -> QI$FIELD_DAD_P.P.FILL_CHARACTER_MINIMUM_SIZE
      106    17274    2         THEN
      107    17275    2   WE_GOTTA_FILL_CHARACTER:
      108    17276    3           DO;
      109    17277    3             FILL_CHARACTER = FIELD_DAD_P$ -> QI$FIELD_DAD_P.P_ITEM_A (%
             17277                      QI#P_FILL_SIZE_INDEX);
      110    17278    3             IF FILL_CHARACTER_MINIMUM_SIZE.FILL_CHARACTER ~= %QI#BLANK
      111    17279    3             THEN
      112    17280    3   WE_GOTTA_REAL_FILL_CHARACTER:
      113    17281    4               DO;
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:6    
      114    17282                      /*  Do real different stuff according to
      115    17283                          rightwing-leftwing justification.  */
      116    17284    4                 IF FIELD_DAD$ -> QI$FIELD_DAD.WORD0_FLAGS.IS_JUSTIFIED_RIGHT
      117    17285    4                 THEN
      118    17286    5                   DO;
      119    17287    5                     FILL_INDEX = GZ_FIELD$ -> QI$GZ_FIELD.STATUS.OFFSET;
      120    17288    5                     FILL_INDEX_STOP = GZ_FIELD$ -> QI$GZ_FIELD.DESCRIPTION.LENGTH
      121    17289    5                       + GZ_FIELD$ -> QI$GZ_FIELD.STATUS.OFFSET
      122    17290    5                       - 1;
      123    17291    5                     FILL_INCREMENT = 1;
      124    17292    5                   END;
      125    17293    4                 ELSE
      126    17294    5                   DO;
      127    17295    5                     FILL_INDEX = GZ_FIELD$ -> QI$GZ_FIELD.DESCRIPTION.LENGTH
      128    17296    5                       + GZ_FIELD$ -> QI$GZ_FIELD.STATUS.OFFSET
      129    17297    5                       - 1;
      130    17298    5                     FILL_INDEX_STOP = GZ_FIELD$ -> QI$GZ_FIELD.STATUS.OFFSET;
      131    17299    5                     FILL_INCREMENT = -1;
      132    17300    5                   END;
      133    17301
      134    17302                      /*  This is kind of embarrassing.
      135    17303                          First, there was B03 and previous, in which the output
      136    17304                          side of FILL CHARACTERing was forgotten completely.
      137    17305                          No module equivalent to QCW$WRITE even existed, and this
      138    17306                          FILL stuff on output was patched in.
      139    17307                          Then, there came C00/C01, in which the implementor was
      140    17308                          overly concerned about using CIP routines, since COBOL
      141    17309                          variables can legally be up to 16K characters long.
      142    17310                          The problem is that, in this case, we're working with a
      143    17311                          FRAME SECTION variable, which will be at most 256 characters
      144    17312                          long, practically less than 80, and almost always about 10.
      145    17313                          I apologize for my previous hubris in this area.  After
      146    17314                          whipping up a worst-case testcase (20 fields, 80 characters
      147    17315                          apiece, all to be FILLed completely, alternating LEFT and
      148    17316                          RIGHT JUSTIFIED:  1 PRESENT, 5000 WRITEs, 1 CLEAR so as to
      149    17317                          avoid actually updating the screen), the following loop
      150    17318                          runs in about 1/3 the time the old CIP stuff (which was
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:7    
      151    17319                          about 3 times as big, too) took.  JJL, D00.  */
      152    17320    4   FILL_LOOP:
      153    17321    5                 DO WHILE (
      154    17322    5                   ( FIELD_DAD$ -> QI$FIELD_DAD.WORD0_FLAGS.IS_JUSTIFIED_RIGHT
      155    17323    5                   AND FILL_INDEX <= FILL_INDEX_STOP )
      156    17324    5                   OR
      157    17325    5                   ( NOT FIELD_DAD$ -> QI$FIELD_DAD.WORD0_FLAGS.IS_JUSTIFIED_RIGHT
      158    17326    5                   AND FILL_INDEX >= FILL_INDEX_STOP ) );
      159    17327
      160    17328    5                   IF GZ_FIELD$ -> QI$GZ_FIELD.STATUS.DATA$ -> QI$UBYTE_A ( FILL_INDEX
             17328                            )
      161    17329    5                     = %QI#BLANK
      162    17330    5                   THEN
      163    17331    5                     GZ_FIELD$ -> QI$GZ_FIELD.STATUS.DATA$ -> QI$UBYTE_A ( FILL_INDEX )
      164    17332    5                       = FILL_CHARACTER_MINIMUM_SIZE.FILL_CHARACTER;
      165    17333    5                   ELSE
      166    17334    5                     EXIT FILL_LOOP;
      167    17335    5                   FILL_INDEX = FILL_INDEX + FILL_INCREMENT;
      168    17336
      169    17337    5                 END FILL_LOOP;
      170    17338    4               END WE_GOTTA_REAL_FILL_CHARACTER;
      171    17339    3           END WE_GOTTA_FILL_CHARACTER;
      172    17340
      173    17341    2   NEXT_FIELD_PLEASE:
      174    17342    2         GZ_FIELD$ = PINCRW ( GZ_FIELD$, SIZEW (QI$GZ_FIELD) );
      175    17343
      176    17344    2       END PRE_EDIT_THEM_FIELDS;
      177    17345
      178    17346    1       QCI@IO_WRITE = QCW@WRITE;
      179    17347
      180    17348    1       CALL QCI$IO_WRITE ( QCI@IO_WRITE );
      181    17349
      182    17350    1       QCW@WRITE.STATUS = QCI@IO_WRITE.STATUS;
      183    17351
      184    17352        /*  IF QCI@IO_WRITE.STATUS = %QI#USE_INVALID_RETURN
      185    17353            THEN
      186    17354              RETURN;  */
PL6.E3A0      #001=QCW$WRITE File=QCW$WRITE.:E05TSI                              WED 07/30/97 05:17 Page:8    
      187    17355
      188    17356    1       RETURN;
      189    17357
      190    17358    1   END QCW$WRITE;

