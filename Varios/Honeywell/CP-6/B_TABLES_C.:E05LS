   10      /*P* NAME: B_TABLES_C
   11      PUROPSE: Macros and EQUs to define common SEARCH and XLATE tables.
   12      DESCRIPTION:
   13              This module contains preprocessor definitions needed to define
   14              common SEARCH and XLATE tables.  Definitions that are useful
   15              only for defining tables are contained in this module.  The
   16              module B$TABLES_C contains preprocessor definitions needed to
   17              access tables.  Both of these must be %INCLUDEd in the modules
   18              that actually define the tables.
   19
   20              There are two sets of tables: the "standard" set that is
   21              intended for use with English, and the "alternate" set that
   22              can be used for foreign languages.
   23
   24              For each table there is:
   25               -- an EQU of the form "B#tablename" in this module that
   26                  defines the starting offset of the table in the table set.
   27               -- a macro named "B_tablename" in this module that generates
   28                  the table.  These macros are all invoked in the B_TABLES
   29                  macro (also in this module) in the order indicated by the
   30                  "B#tablename" EQUs.
   31               -- a CONSTANT SYMDEF pointer named "B$tablename$" that points
   32                  to the start of the table.  These pointers are defined in
   33                  both B_POINTER_D and B_USRPTRS_D.
   34               -- a macro named "B$tablename" that generates a BASED array
   35                  that can be used to reference the table.  These macros
   36                  are defined in B$TABLES_C.
   37      */
14:14 JUL 28 '97 B_TABLES_C.:E05SI                                         2    
   38
   39
   40      /****** EQUs for table offsets
   41
   42              The following EQUs define word offsets of each table from the
   43              beginning of the structure defined by the B_TABLES macro.
   44      */
   45      %EQU B#SR_NON_BLANK = 0 * 128;
   46      %EQU B#SR_NON_ZERO  = 1 * 128;
   47      %EQU B#SR_NON_DEC   = 2 * 128;
   48      %EQU B#SR_NON_ALPHA = 3 * 128;
   49      %EQU B#XL_ASC_EBC   = 4 * 128;
   50      %EQU B#XL_EBC_ASC   = 5 * 128;
   51      %EQU B#XL_LOWER     = 6 * 128;
   52      %EQU B#XL_UPPER     = 7 * 128;
   53      %EQU B#XL_UR        = 8 * 128;
   54      %EQU B#XL_UPPER_UR  = 9 * 128;
   55      %EQU B#XL_LOWER_UR  = 10 * 128;
   56      %EQU B#XL_MOD_256   = 11 * 128;
   57      %EQU B#XL_ASC_DUMP  = 12 * 128;
   58      %EQU B#XL_EBC_DUMP  = 13 * 128;
   59      %EQU B#TABLE_LIMIT  = 14 * 128;
   60
   61
   62      /*F* NAME: B_TABLES
   63      PURPOSE: Macro to generate a set of standard tables.
   64      DESCRIPTION:
   65              This macro generates a set of standard SEARCH and XLATE tables
   66              in a fixed order, as indicated by the "B#tablename# EQUs.
   67      */
   68      %MACRO B_TABLES( NAME=B_TABLES );
   69
   70      DCL 1 NAME CONSTANT SYMDEF DALIGNED,
   71          %B_SR_NON_BLANK;    /*   0  Search for non-blank                */
   72          %B_SR_NON_ZERO;     /*   1  Search for non-zero                 */
   73          %B_SR_NON_DEC;      /*   2  Search for non-decimal              */
   74          %B_SR_NON_ALPHA;    /*   3  Search for non-alphabetic           */
14:14 JUL 28 '97 B_TABLES_C.:E05SI                                         3    
   75          %B_XL_ASC_EBC;      /*   4  Translate ASCII to EBCDIC           */
   76          %B_XL_EBC_ASC;      /*   5  Translate EBCDIC to ASCII           */
   77          %B_XL_LOWER;        /*   6  Translate upper to lower case       */
   78          %B_XL_UPPER;        /*   7  Translate lower to upper case       */
   79          %B_XL_UR;           /*   8  Translate to Unit Record char set   */
   80          %B_XL_UPPER_UR;     /*   9  Translate to uppercase UR char set  */
   81          %B_XL_LOWER_UR;     /*  10  Translate to lowercase UR char set  */
   82          %B_XL_MOD_256;      /*  11  Strip hi-order bit from each char   */
   83          %B_XL_ASC_DUMP;     /*  12  Translate nonprintable ASCII to dot */
   84          %B_XL_EBC_DUMP;     /*  13  Translate nonprintable EBCDIC to dot*/
   85          2 * UBIN;
   86
   87      %MEND;
   88
   89
   90      /*F* NAME: B_SR_NON_BLANK
   91      PURPOSE: Table to search for a non-blank character.
   92      */
   93      %MACRO B_SR_NON_BLANK;
   94
   95        2 B_SR_NON_BLANK( 0:511 ) BIT(9) CALIGNED INIT(
   96          %B$SR#CTL * 32,                 /*  NULL thru US                */
   97          '0'B,                           /*  SP                          */
   98          %B$SR#PUNC * 2,                 /*  ! "                         */
   99          %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
  100          %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
  101          %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
  102          %B$SR#PUNC,                     /*  ,                           */
  103          %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
  104          %B$SR#PUNC,                     /*  /                           */
  105          %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 8,
  106                                          /*  0 thru 7                    */
  107          %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
  108          %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
  109          %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
  110          %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
  111          %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      G thru Z                */
14:14 JUL 28 '97 B_TABLES_C.:E05SI                                         4    
  112          %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
  113          %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
  114          %B$SR#PUNC,                     /*  `                           */
  115          %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
  116          %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      g thru z                */
  117          %B$SR#PUNC * 4,                 /*  { | } ~                     */
  118          %B$SR#CTL,                      /*  DEL                         */
  119          %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
  120
  121      %MEND;
  122
  123
  124      /*F* NAME: B_SR_NON_ZERO
  125      PURPOSE: Table to search for a non-zero character.
  126      */
  127      %MACRO B_SR_NON_ZERO;
  128
  129        2 B_SR_NON_ZERO( 0:511 ) BIT(9) CALIGNED INIT(
  130          %B$SR#CTL * 32,                 /*  NULL thru US                */
  131          %B$SR#PUNC * 3,                 /*  SP ! "                      */
  132          %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
  133          %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
  134          %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
  135          %B$SR#PUNC,                     /*  ,                           */
  136          %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
  137          %B$SR#PUNC,                     /*  /                           */
  138          '0'B,                           /*  0                           */
  139          %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 7,
  140                                          /*  1 thru 7                    */
  141          %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
  142          %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
  143          %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
  144          %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
  145          %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      G thru Z                */
  146          %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
  147          %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
  148          %B$SR#PUNC,                     /*  `                           */
14:14 JUL 28 '97 B_TABLES_C.:E05SI                                         5    
  149          %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
  150          %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      g thru z                */
  151          %B$SR#PUNC * 4,                 /*  { | { ~                     */
  152          %B$SR#CTL,                      /*  DEL                         */
  153          %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
  154
  155      %MEND;
