  200          %B$SR#PUNC,                     /*  ,                           */
  201          %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
  202          %B$SR#PUNC,                     /*  /                           */
  203          %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 8,
  204                                          /*  0 thru 7                    */
  205          %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
  206          %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
  207          %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
  208          '0'B * 26,                      /*  A thru Z                    */
  209          %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
  210          %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
  211          %B$SR#PUNC,                     /*  `                           */
  212          '0'B * 26,                      /*  a thru z                    */
  213          %B$SR#PUNC * 4,                 /*  { | } ~                     */
  214          %B$SR#CTL,                      /*  DEL                         */
  215          %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
  216
  217      %MEND;
