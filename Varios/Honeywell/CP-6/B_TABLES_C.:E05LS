  631      /*F* NAME: B_XL_ASC_DUMP
  632      PURPOSE: Table to translate non-printable ASCII characters to a dot.
  633      DESCRIPTION:
  634              This table translates all non-printable ASCII characters to
  635              dot.  It is typically used in producing dumps of ASCII data.
  636              The two hi-order (parity) bits are disregarded (i.e., characters
  637              are translated as modulo 128 values).
  638      */
  639      %MACRO B_XL_ASC_DUMP;
  640
  641        2 B_XL_ASC_DUMP( 0:15 ) CHAR(32) INIT(
  642          '................................',
  643          ' !"#$%&''()*+,-./0123456789:;<=>?',
  644          '@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_',
  645          '`abcdefghijklmnopqrstuvwxyz{|}~.',
  646          '................................',
  647          ' !"#$%&''()*+,-./0123456789:;<=>?',
  648          '@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_',
  649          '`abcdefghijklmnopqrstuvwxyz{|}~.',
  650          '................................',
  651          ' !"#$%&''()*+,-./0123456789:;<=>?',
  652          '@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_',
  653          '`abcdefghijklmnopqrstuvwxyz{|}~.',
  654          '................................',
  655          ' !"#$%&''()*+,-./0123456789:;<=>?',
  656          '@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_',
  657          '`abcdefghijklmnopqrstuvwxyz{|}~.' ),
  658
  659      %MEND;
  660
  661
  662      /*F* NAME: B_XL_EBC_DUMP
  663      PURPOSE: Table to translate non-printable EBCDIC characters to a dot.
  664      DESCRIPTION:
  665              This table translates all non-printable EBCDIC characters to
  666              dot.  It is typically used in producing dumps of EBCDIC data.
14:14 JUL 28 '97 B_TABLES_C.:E05SI                                         19   
  667              The hi-order (parity) bit is disregarded (i.e., characters
  668              are translated as modulo 256 values).
  669      */
  670      %MACRO B_XL_EBC_DUMP;
  671
  672        2 B_XL_EBC_DUMP( 0:31 ) CHAR(16) INIT(
  673          '................', '................', /* 00 - 1F */
  674          '................', '................', /* 20 - 3F */
  675          ' .........`.<(+|', '&.........!$*);~', /* 40 - 5F */
  676          '-/........^,%_>?','..........:#@''="', /* 60 - 7F */
  677          '.abcdefghi......', '.jklmnopqr......', /* 80 - 9F */
  678          '..stuvwxyz......', '.\{}[]..........', /* A0 - BF */
  679          '{ABCDEFGHI......', '}JKLMNOPQR......', /* C0 - DF */
  680          '..STUVWXYZ......', '0123456789......', /* E0 - FF */
  681          '................', '................', /* 00 - 1F */
  682          '................', '................', /* 20 - 3F */
  683          ' .........`.<(+|', '&.........!$*);~', /* 40 - 5F */
  684          '-/........^,%_>?','..........:#@''="', /* 60 - 7F */
  685          '.abcdefghi......', '.jklmnopqr......', /* 80 - 9F */
  686          '..stuvwxyz......', '.\{}[]..........', /* A0 - BF */
  687          '{ABCDEFGHI......', '}JKLMNOPQR......', /* C0 - DF */
  688          '..STUVWXYZ......', '0123456789......'  /* E0 - FF */ ),
  689
  690      %MEND;

