        6         6             /* append t to s, upto n characters
        7         7             */
        8         8             char *
        9         9             strncat(s, t, n)
       10        10              register char *s, *t;
       11        11              register int n;
       12        12             {
       13        13    1         register char *b = s;
       14        14    1
       15        15    1         while (*s++)
       16        16    1            ;
       17        17    1         s--;
       18        18    1         while ((n-- > 0) && (*s++ = *t++))
       19        19    1            ;
       20        20    1         return b;
       21        21    1        }
       22        22

