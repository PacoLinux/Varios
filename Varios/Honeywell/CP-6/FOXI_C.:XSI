/*
**       These are the Entry DCLs for the user-callable FOXI routines
*/
%Macro   Foxi$Ents;
       Dcl  Fast$Open     Entry (08)  AltRet  Conv (0);
       Dcl  Fast$Read     Entry (09)  AltRet;
       Dcl  Fast$Close    Entry (03)  AltRet;
       Dcl  Fast$PFil     Entry (03)  AltRet;
       Dcl  Fast$PRecord  Entry (05)  AltRet;
       Dcl  Fast$Write    Entry (07)  AltRet;
 
       Dcl  Find$DCB      Entry (04)  AltRet;
       Dcl  Get_Arg_Len   Entry (04)  AltRet;
%MEnd;
