        6        6        /****************************************************************/
        7        7        /*   PROC:  ZIU$PGRG
        8        8             PURPOSE:  To get page ranges initialized by the user in the
        9        9                       driver program, and return those ranges to calling
       10       10                       programs with the following conditions:
       11       11             MAX_PAGE: All error checking on page ranges are done by this
       12       12                       routine, therefore a max page variable is needed to
       13       13                       check the high range against. If this value is not
       14       14                       known, an arbitrarily high value may be used.
       15       15         INTRVL_SIZE:  If you desire to have a page range limited to a certain
       16       16                       interval size by breaking it up into smaller ranges,
       17       17                       pass that interval size as the third parameter.
       18       18                       Ex:
       19       19                           If the next range was to be a low range of 1 and
       20       20                       a high range of 10 but you would like that passed to
       21       21                       you as two smaller ranges of 1 to 5 and 6 to 10, you
       22       22                       would pass a INTRVL_SIZE of 5. The first call will
       23       23                       return 1 and 5, the second call will return 6 and 10.
       24       24          ACT_INTRVL: This will return the actual interval size of present
       25       25                        range.
       26       26          Last RANGE:  If this routine is called and there are no more ranges,
       27       27                       the altreturn will be taken.
       28       28        */
       29       29        /*****************************************************************/
       30       30        PAGE_RANGE: PROC(LOW,HIGH,INTRVL_SIZE,ACT_INTRVL,MAX_PAGE,FINISHED) NOAUTO;
       31       31    1   DCL LOW UBIN(36) CALIGNED;
       32       32    1   DCL HIGH UBIN(36) CALIGNED;
       33       33    1   DCL TEMP UBIN(36) UNAL;
       34       34    1   DCL INDX SBIN(36) STATIC INIT(0);
       35       35    1   DCL INTRVL_SIZE UBIN(36) CALIGNED;
       36       36    1   DCL ACT_INTRVL UBIN(36) CALIGNED;
       37       37    1   DCL NUM_INTRVLS UBIN(36) UNAL;
PL6.E3A0      #001=PAGE_RANGE File=ZIU$PGRG.:ZIC0TSI                             FRI 09/05/97 13:19 Page:2    
       38       38    1   DCL MAX_PAGE UBIN(36) ALIGNED;
       39       39    1   DCL TEMP_LOW SBIN(36) STATIC INIT(-1);
       40       40    1   DCL TEMP_ICNT SBIN(36) STATIC;
       41       41    1   DCL FINISHED BIT(1);
       42       42        /**/
       43       43    1   DCL SYS_FUNC_ERRMSG ENTRY ALTRET;
       44       44        /**/
       45       45        %INCLUDE ZI$DIR;
       46      202         %DIR;
       47      346        %INCLUDE ZI_GLOBAL_DBUTIL;
       48      524         %MISC_DCLS;
       49      577         %SYS_FUNC;
       50      669        /*
       51      670              If user did not specify a page range, we will make a
       52      671              page range to cover the complete file size.
       53      672        */
       54      673    1       IF ZI$DIR.RNG_LIST = '0'B THEN
       55      674    2       DO;
       56      675    2           INIT_RG.FIRNG(0) = 1;
       57      676    2           INIT_RG.SECRNG(0) = MAX_PAGE;
       58      677    2           END;
       59      678    2       IF ZI$DIR.PGRG_RESET THEN DO; /* Reset each new directive */
       60      679    2           TEMP_ICNT = 0; /* Zero means one level of ranges */
       61      680    2           TEMP_LOW = -1;
       62      681    2           INDX = 0;
       63      682    2           ZI$DIR.PGRG_RESET = '0'B;
       64      683    2           END;
       65      684        /*
       66      685               Set low and high values to values recieved by driver.
       67      686        */
       68      687    1       IF TEMP_LOW = -1 THEN
       69      688    2       DO;
       70      689    2           LOW = INIT_RG.FIRNG(0);
       71      690    2           IF ICNT >= 0 THEN
       72      691    2               TEMP_ICNT = ICNT;
       73      692    2           END;
       74      693    1       ELSE
PL6.E3A0      #001=PAGE_RANGE File=ZIU$PGRG.:ZIC0TSI                             FRI 09/05/97 13:19 Page:3    
       75      694    1           LOW = TEMP_LOW;
       76      695    1       HIGH = INIT_RG.SECRNG(INDX);
       77      696    1       IF INDX > TEMP_ICNT THEN
       78      697    2       DO;
       79      698    2           FINISHED = '1'B;
       80      699    2           ACT_INTRVL = 1;
       81      700    2           TEMP_LOW = -1; /* Reset flags and counters */
       82      701    2           INDX = 0;
       83      702    2           RETURN; /* Page ranges are all done */
       84      703    2           END;
       85      704        /*
       86      705               Examine interval size option. 0,1 or nil turns option off.
       87      706        */
       88      707    1       IF INTRVL_SIZE < 1 THEN INTRVL_SIZE = 1;
       89      708    1       IF ADDR(INTRVL_SIZE) = ADDR(NIL) THEN
       90      709    1           INTRVL_SIZE = 1;
       91      710        /*
       92      711               Check for possible range specification errors.
       93      712        */
       94      713    1       CALL CHK_RNG_ERR;
       95      714        /**/
       96      715    1       ACT_INTRVL = HIGH - LOW + 1;
       97      716        /*
       98      717               Calculate the number of intervals of INTRVL_SIZE that
       99      718               this page range will make
      100      719        */
      101      720    1       NUM_INTRVLS = (ACT_INTRVL + INTRVL_SIZE -1)/INTRVL_SIZE;
      102      721        /*
      103      722               If calling routine asked for interval sizes greater than
      104      723               1, and this range is greater than interval size asked for
      105      724               then reduce the range to interval size but don't incriment
      106      725               indx pointer and do bump up low range for next call.
      107      726        */
      108      727    1       IF NUM_INTRVLS > 1 AND INTRVL_SIZE > 1 THEN
      109      728    2       DO;
      110      729    2           TEMP_LOW = LOW + INTRVL_SIZE;
      111      730    2           HIGH = LOW + INTRVL_SIZE -1;
PL6.E3A0      #001=PAGE_RANGE File=ZIU$PGRG.:ZIC0TSI                             FRI 09/05/97 13:19 Page:4    
      112      731    2           ACT_INTRVL = INTRVL_SIZE;
      113      732    2           RETURN;
      114      733    2           END;
      115      734        /*
      116      735               If the above was not the case then just return the low
      117      736               and high values of page range and incriment indx.
      118      737        */
      119      738    1       INDX = INDX + 1;
      120      739    1       TEMP_LOW = INIT_RG.FIRNG(INDX);
      121      740    1       RETURN;
      122      741        /*
      123      742        --------------------- CHK_RN_ERR ----------------------------
      124      743        */
      125      744    1   CHK_RNG_ERR: PROC;
      126      745    2       IF LOW > MAX_PAGE THEN
      127      746    3       DO;
      128      747    3           LOW = 0;
      129      748    3           ERRCODESF.ERR# = 1001;
      130      749    3           MSGCODE = ERRCODE_RDEF;
      131      750    3           CALL SYS_FUNC_ERRMSG;
      132      751         /*E* ERROR: ZIU-01001-C
      133      752                             MESSAGE: ...Pick another page please - that one is a bit large.
      134      753                               */
      135      754    3           UNWIND TO RESTART;
      136      755    3           END;
      137      756    2       ELSE
      138      757    2           IF LOW < 1 THEN
      139      758    3           DO;
      140      759    3               IF ZI$DIR.LOAD OR ZI$DIR.UNLOAD
      141      760    3               THEN LOW = 1;
      142      761    3               END;
      143      762        /**/
      144      763    2       IF HIGH > MAX_PAGE THEN
      145      764    3       DO;
      146      765    3           HIGH = MAX_PAGE;
      147      766    3           INIT_RG.SECRNG(INDX) = HIGH;
      148      767    3           ERRCODESF.ERR# = 1002;
PL6.E3A0      #001=PAGE_RANGE File=ZIU$PGRG.:ZIC0TSI                             FRI 09/05/97 13:19 Page:5    
      149      768    3           MSGCODE = ERRCODE_RDEF;
      150      769    3           CALL SYS_FUNC_ERRMSG;
      151      770         /*E* ERROR: ZIU-01002-C
      152      771                          MESSAGE: ...Adjusting high range to end of file.
      153      772                       */
      154      773    3           END;
      155      774    2       ELSE
      156      775    2           IF HIGH < 1 THEN
      157      776    3           DO;
      158      777    3               IF ZI$DIR.LOAD OR ZI$DIR.UNLOAD
      159      778    3               THEN HIGH = 1;
      160      779    3               END;
      161      780        /**/
      162      781    2       IF LOW > HIGH THEN
      163      782    3       DO;
      164      783    3           TEMP = HIGH;
      165      784    3           HIGH = LOW;
      166      785    3           LOW = TEMP;
      167      786    3           INIT_RG.FIRNG(INDX) = LOW;
      168      787    3           INIT_RG.SECRNG(INDX) = HIGH;
      169      788    3           ERRCODESF.ERR# = 1003;
