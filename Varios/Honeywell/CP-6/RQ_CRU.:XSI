!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB  NAME=RQ,WSN=LOCAL
!RESOURCE MEM=256,TIME=10,NHPSN,ACCOUNT=(.SIACCT,.GNACCT,.LBACCT)
!ROUTE Lee Hallin, LADC
!"
!"  By default, the output created by this job is EDGEMARKed.
!"  However, if you do NOT want it EDGEMARKed, do a DEST=LP substitution on
!"  your BATCH or XEQ command such as:
!"    !BATCH RQ_CRU.XACCT DEST=LP
!"
!DEFAULT DEST=*LS
!IF $FID_EXIST('TO.:SYS') & $MODE='BATCH' THEN TO AC=%$ACCT Your RQ compile just started.
!LET LISTING = 'DEST'
!LET MONNUM=$SUBSTR($DATE,2,2)
!LET MONTHS='JanFebMarAprMayJunJulAugSepOctNovDec'
!LET RIGHT=$SUBSTR(MONTHS,(MONNUM*3)-3,3) || ' ' || $SUBSTR($DATE,4,2)
!IF $SUBSTR(RIGHT,4,1) ~= '0' THEN GOTO DATE_OK
!LET RIGHT= $SUBSTR(RIGHT,0,4) || $SUBSTR(RIGHT,5,1)
!DATE_OK:
!PL6 RQ_SI61.SIACCT OVER *RQ_OU61.GNACCT,DEST(NSYS,OU,LS,SR(.SIACCT,.LBACCT),SC)
!PL6 RQ_SI62.SIACCT INTO *RQ_OU62.GNACCT,DEST(NSYS,OU,LS,SR(.SIACCT,.LBACCT))
!PARTRGE RQ_SIN1.SIACCT INTO *RQ_OUN.GNACCT,DEST(NSYS,SR(.SIACCT,.LBACCT))
!IF LISTING = 'LP' THEN GOTO DONT_EDGEMARK
!EDGEMARK.XACCT DEST TO LP@(F='PERF',L='RQ.X',R='%RIGHT')
!DONT_EDGEMARK:
!LINK ;
      B_COPYRIGHT.LBACCT, ;  " Bull copyright notice "
      *RQ_OU61.GNACCT,;      " Read Queue main program "
      *RQ_OU62.GNACCT,;      " Misc routines "
      *RQ_OUN.GNACCT ;       " Read Queue parse nodes "
 OVER RQ.GNACCT(SIDCB=M$SI,LODCB=M$LO,IPATCH=100,DPATCH=10)
!EXTRACT.XACCT
DATA_BASE *RQ
EXTRACT RQ_C2.SIACCT
BUILD INTO RQ.GNACCT PROCESSOR
END
!IF $FID_EXIST('TO.:SYS') & $MODE='BATCH' THEN TO AC=%$ACCT Your RQ compile just finished.
