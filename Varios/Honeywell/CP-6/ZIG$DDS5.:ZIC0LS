      109        9        PURPOSE: PUT A LINE OUT TO M$LO;
      110       10        */
      111       11        PUT_OUT_LINE:
      112       12            PROC(COUNT,REC_OUT);
      113       13    1   DCL COUNT UBIN (36);
      114       14    1   DCL REC_OUT CHAR (80);
      115       15    1   DCL LAST_COUNT UBIN (36) STATIC SYMDEF; /*COUNT OF CHARACTERS IN LINE*/
      116       16    1   DCL LIST_STRING CHAR (128) EXT;
      117       17    1   DCL LINE_NUM CHAR (7);
      118       18    1   DCL LIST ENTRY (1);
      119       19    1   DCL I UBIN (36);
      120       20    1       CALL BINCHAR(LINE_NUM,COUNT);
      121       21    1       I=0;
      122       22    2       DO WHILE (SUBSTR(LINE_NUM,I,1)='0' AND I<6);
      123       23    2           CALL INSERT(LINE_NUM,I,1,' ');
      124       24    2           I=I+1;
      125       25    2           END;
      126       26    1       LIST_STRING = ' ';
      127       27    1       CALL CONCAT(LIST_STRING,LINE_NUM,'    ',REC_OUT);
      128       28    1       CALL LIST (LIST_STRING);
      129       29    1   END PUT_OUT_LINE;
      130       30        %EOD;

