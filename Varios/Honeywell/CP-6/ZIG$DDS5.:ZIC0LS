        1        1        /*M*     ZIG$DDS5       TO READ THROW M$SI */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8            NAME:     COMPRD.
        9        9            PURPOSE:  IT IS TO READ THROW M$SI BY CALLING EXREAD AND SET THE
       10       10                 FLAG NCS TO SHOW IF WE HAVE THE START OF A NEW DIRECTIVE.
       11       11        */
       12       12        COMPRD:
       13       13            PROC;
       14       14        %INCLUDE B$TABLES_C;
       15      122        %B$SR_NON_BLANK;
       16      126        %B$XL_UPPER;
       17      130    1   DCL EOFFLG UBIN (36) EXT;
       18      131    1   DCL EXREAD ENTRY;
       19      132    1   DCL NCS UBIN (36) EXT;
       20      133    1   DCL REC CHAR (80) EXT;
       21      134    1   DCL UPPER_REC CHAR (80);
       22      135    1   DCL NUM_CHAR_READ UBIN (36) EXT;
       23      136    1   DCL I UBIN (36) ;
       24      137    1   DCL J UBIN WORD;
       25      138    1   DCL DISP_STR CHAR(9) CONSTANT INIT('DISPLAY  ');
       26      139    1   DCL GEN_STR CHAR(9) CONSTANT INIT('GENERATE ');
       27      140    1   DCL TRANS_STR CHAR(9) CONSTANT INIT('TRANSLATE');
       28      141    1   DCL VALID_STR CHAR(9) CONSTANT INIT('VALIDATE ');
       29      142    1   DCL RDWORD BIT (1) EXT;
       30      143    1   DCL FOUND_PER BIT (1) STATIC SYMDEF;
       31      144         %EJECT;
PL6.E3A0      #001=COMPRD File=ZIG$DDS5.:ZIC0TSI                                 FRI 09/05/97 11:49 Page:2    
       32      145    1   ZIG$DDS5: ENTRY;
       33      146    1       IF EOFFLG = 0 & RDWORD
       34      147    2       THEN DO;
       35      148        /**************
       36      149                 Read the next line if we don't already have a
       37      150                 unused line in rec.
       38      151                 Rdword tells use if we have used the line or not.
       39      152        **************/
       40      153    2           CALL EXREAD;
       41      154    2           END;
       42      155    1       IF EOFFLG ~= 0
       43      156    2       THEN DO;
       44      157    2           NCS=1;
       45      158    2           RETURN;
       46      159    2           END;
       47      160    1       CALL SEARCH(I,J,B$SR_NON_BLANK,SUBSTR(REC,0,NUM_CHAR_READ))
       48      161    2       WHENALTRETURN DO;
       49      162    2           NCS = 0;
       50      163    2           RETURN;
       51      164    2           END;
       52      165    1       CALL INDEX(J,' ',SUBSTR(REC,I,NUM_CHAR_READ-I));
       53      166        /*
       54      167            The longest directive keyword at this time is 9 chars.
       55      168        */
       56      169    1       IF J > 9
       57      170    2       THEN DO;
       58      171    2           NCS = 0;
       59      172    2           RETURN;
       60      173    2           END;
       61      174    1       CALL XLATE(UPPER_REC,B$XL_UPPER,REC);
       62      175        /*
       63      176            REC has been converted to all capital letters for comparison to
       64      177            keywords either in their entirety or with at least the minimum
       65      178            number of letters in abbreviations.
       66      179        */
       67      180    1       IF SUBSTR(UPPER_REC,I,1) = '?' OR
       68      181    1          SUBSTR(UPPER_REC,I,J) = 'ASSIGN' OR
PL6.E3A0      #001=COMPRD File=ZIG$DDS5.:ZIC0TSI                                 FRI 09/05/97 11:49 Page:3    
       69      182    1          SUBSTR(UPPER_REC,I,J) = 'DBACS' OR
       70      183    1          SUBSTR(UPPER_REC,I,J) = SUBSTR(DISP_STR,0,MAXIMUM(4,J)) OR
       71      184    1          SUBSTR(UPPER_REC,I,J) = SUBSTR(GEN_STR,0,MAXIMUM(3,J)) OR
       72      185    1          SUBSTR(UPPER_REC,I,J) = 'HELP' OR
       73      186    1          SUBSTR(UPPER_REC,I,J) = 'PURGE' OR
       74      187    1          SUBSTR(UPPER_REC,I,J) = 'REPLACE' OR
       75      188    1          SUBSTR(UPPER_REC,I,J) = SUBSTR(TRANS_STR,0,MAXIMUM(5,J)) OR
       76      189    1          SUBSTR(UPPER_REC,I,J) = SUBSTR(VALID_STR,0,MAXIMUM(5,J))
       77      190    1       THEN
       78      191    1           NCS = 1;
       79      192    1       ELSE
       80      193    1           NCS = 0;
       81      194    1       RETURN;
       82      195    1   END COMPRD;
       83      196         %EOD;

