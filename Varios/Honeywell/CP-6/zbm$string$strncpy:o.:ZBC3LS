        6         6             /* Copy t into s, upto n characters
        7         7             */
        8         8             char *
        9         9             strncpy(s, t, n)
       10        10              register char *s, *t;
       11        11              register int n;
       12        12             {
       13        13    1         register char *b = s;
       14        14    1
       15        15    1         while ((n-- > 0) && (*s++ = *t++))
       16        16    1            ;
       17        17    1         return b;
       18        18    1        }
       19        19

