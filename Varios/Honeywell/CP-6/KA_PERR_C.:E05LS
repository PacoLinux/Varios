    8      %EQU E$ASN_BADLGNSTR = 1; /* bad logon string   */
    9      %EQU E$ASN_BADCHNSTT = 2; /* channel status is uninitialized or released   */
   10      %EQU E$ASN_INVINT = 4; /* invalid interrupts */
   11      %EQU E$ASN_IO_ERR = 3; /* CCB indicates IO error */
   12      %EQU E$ASN_LINERRDSC = 5; /* disconnect because of excessive line error */
   13      %EQU E$ASN_LINERRLOG = 9; /* logged because of excessive line error */
   14      %EQU E$ASN_NNZRNG = 6; /* non-zero range on completed IOs */
   15      %EQU E$ASN_OTPASNRQSFAL = 7; /* output asnrqs to MLCP failed */
   16      %EQU E$ASN_UN_EXPINT = 8; /* un-expected interrupt */
   17      %EQU E$ASN_INVRNG = 10; /* Bad range returned for input interrupt */
