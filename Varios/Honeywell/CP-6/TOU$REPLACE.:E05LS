PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:1    
        1        1        /*M* TOU$REPLACE     DEckfile Editor Routine String Replacement */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7
        8        8        /*X* PLM=3,DCI=6,SQB=N,PRB=N,DTI=5,ENI=0,AND=Y,ECI=0,CSU=3,DIN=3,
        9        9             CTI=8,CRM=109,CRT=1,NLB=N,NTB=Y,MCL=110 */
       10       10
       11       11        /*P*  NAME:        TOU$REPLACE
       12       12              AUTHORS:     D. Augustine
       13       13                           S. Sheppard
       14       14              PURPOSE:     DEckfile Editor Routine String Replacement
       15       15              DESCRIPTION: Error messages and general information messages are
       16       16                           fetched from the msg data base as string constants.
       17       17                           However, many of  these  messages  contain variable
       18       18                           fields whose values must be plugged in before being
       19       19                           printed. Such fields are stored in the original msg
       20       20                           as ~strings. This procedure accepts a message which
       21       21                           contains ~strings and  replaces the  ~strings  with
       22       22                           the correct values.
       23       23              REFERENCE:   CP-6 Release C0L0
       24       24              LAST UPDATE: 09/29/83
       25       25        */
       26       26        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:2    
       27       27        /*F*  NAME:        TOU$REPLACE
       28       28              DESCRIPTION: Procedure Nesting Hierarchy
       29       29
       30       30              TOU$REPLACE: procedure (MSG, MSG_LGTH)
       31       31                 SELECT_LETTER: procedure
       32       32                    SUB_FOR_LETTER_A: procedure
       33       33                    SUB_FOR_LETTER_B: procedure
       34       34                    SUB_FOR_LETTER_C: procedure
       35       35                    SUB_FOR_LETTER_D: procedure
       36       36                    SUB_FOR_LETTER_E: procedure
       37       37                    SUB_FOR_LETTER_F: procedure
       38       38                    SUB_FOR_LETTER_G: procedure
       39       39                    SUB_FOR_LETTER_H: procedure
       40       40                    SUB_FOR_LETTER_I: procedure
       41       41                    SUB_FOR_LETTER_J: procedure
       42       42                    SUB_FOR_LETTER_L: procedure
       43       43                    SUB_FOR_LETTER_M: procedure
       44       44                    SUB_FOR_LETTER_O: procedure
       45       45                    SUB_FOR_LETTER_P: procedure
       46       46                    SUB_FOR_LETTER_R: procedure
       47       47                    SUB_FOR_LETTER_S: procedure
       48       48                    SUB_FOR_LETTER_T: procedure
       49       49                    SUB_FOR_LETTER_W: procedure
       50       50                    SUB_FOR_LETTER_X: procedure
       51       51                    SUB_FOR_LETTER_Y: procedure
       52       52                    SUB_FOR_LETTER_Z: procedure
       53       53                 BINARY_2_ASCII: procedure (SIZE, BINARY)
       54       54                 MCA_2_ASCII: procedure (SIZE, BINARY)
       55       55        */
       56       56        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:3    
       57       57        /*F*  NAME:        TOU$REPLACE
       58       58              AUTHORS:     D. Augustine
       59       59                           S. Sheppard
       60       60              SUMMARY:     Replacement Strings
       61       61
       62       62              --------   ------------  -------------------
       63       63              Original   Replace Size  Variable Referenced
       64       64              --------   ------------  -------------------
       65       65              ~A             3         'BOS' 'GMAP' 'Level6' 'MPC'
       66       66                                       (CASE OBJECT.BCD.ASSEMBLER)
       67       67              ~ARS           6         IFAD_ARS#-1
       68       68              ~BLK           6         BLOCK.BSN
       69       69              ~BLS           4         BLOCK_SEQ
       70       70              ~BCZ           6         BLOCK.BCW.SIZE
       71       71              ~BSZ           6         BLOCK.CONTROL
       72       72              ~BCT           6         OBJECT.BINARY_COUNT
       73       73              ~CAT          23         CATALOG_KEY.DATA
       74       74              ~CD            6         CATALOG_DATA.DECK#
       75       75              ~CLAS          5         'ITR' 'FW' 'MDR'
       76       76                                       (case OBJECT.BCD.CLASS)
       77       77              ~CLS           4         OBJECT.BCD.CLASS
       78       78              ~COL           4         CUR#+1
       79       79              ~CUR           4         CUR#
       80       80              ~DATE          6         OBJECT.BCD.DATE
       81       81              ~DECK          6         OBJECT.DECK#
       82       82              ~DF_FID        40        DUP.DF.FID
       83       83              ~EC            6         CATALOG_DATA.ENTRY_COUNT
       84       84              ~EM            6         MASTER_ENTRY_KEY.ENTRY#
       85       85              ~EN            6         CATALOG_ENTRY_KEY.ENTRY#
       86       86              ~ENT          23         OBJECT_KEY.DATA
       87       87              ~FN            3         FILE_FILE
       88       88              ~FM            3         FILE_FILE - 1
       89       89              ~F2            3         SUBCATALOG_DATA.IFAD_FN
       90       90              ~GECAL         6         OBJECT.BCD.GECALL
       91       91              ~HLGDATE        8        LOG_B.ACCESSES.DATE(LOG#)
       92       92              ~HLGTIME       11        LOG_B.ACCESSES.TIME(LOG#)
       93       93              ~HLGCATALOG    21        LOG_B.ACCESSES.CATALOG(LOG#)
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:4    
       94       94              ~HLGDECK       23        LOG_B.ACCESSES.DECK(LOG#)
       95       95              ~HLGIFAD       40        LOG_B.ACCESSES.IFAD(LOG#)
       96       96              ~ID            6         ID.ID
       97       97              ~IMX           4         DUP.IFAD.IMU_NUMBER
       98       98              ~J             2         DUP.IFAD.DRIVE_NUMBER
       99       99              ~IMAGE       6-80        DUP.ERROR_IMAGE
      100      100              ~IMBLK        80         DUP.ERROR_IMAGE
      101      101              ~IFAD_FID      40        DUP.IFAD.FID
      102      102              ~LBL           4         OBJECT.BCD.LBL
      103      103              ~LIB           6         OBJECT.BCD.LIBRARY
      104      104              ~LOAD          5         'RELOC' 'PROG' 'DATA'
      105      105                                       (case OBJECT.BCD.LOAD_TYPE)
      106      106              ~LTIME        11         CURRENT.TIME (long time)
      107      107              ~LDATE         8         CURRENT.DATE
      108      108              ~MSZ           6         OBJECT.LOAD_SIZE
      109      109              ~MODEL         6         OBJECT.BCD.MODEL
      110      110              ~OBJ          110        OBJECT.BCD_
      111      111              ~PAGE          5         DUP.CURRENT_PAGE#
      112      112              ~PATCH        80         BCD_B(CUR#)
      113      113              ~R             2         ID.REV.LEVEL
      114      114              ~REVIS         2         ID.REVISION
      115      115              ~RCW          12         BCD_ASCII(RCW_OCTAL(CUR#))
      116      116              ~RU           10         B_VERSION
      117      117              ~RLINK         6         TOU$DEER.LINK_DATE.ASCII
      118      118              ~SE            4         'PACT' 'COLT' 'MOLT' 'SOLT' 'POLT'
      119      119                                       (case OBJECT.BCD.CLASS)
      120      120              ~SI            40        SUBCATALOG_DATA.IFAD_FID
      121      121              ~TYP           4         OBJECT.BCD.LOAD_TYPE
      122      122              ~TIME          5         CURRENT.TIME (time)
      123      123              ~TCT           6         OBJECT.TOTAL_COUNT
      124      124              ~WD1           6         BLOCK_OFFSET+1
      125      125              ~WSN           94        CUSTOMER.SITE_NAME#.C
      126      126              ~WSI           6         CUSTOMER.SITE_ID#
      127      127              ~WIR           6         REVISION_B.VERSION
      128      128              ~XTI            8        DISKETTE_HEADER.TITLE
      129      129              ~XUI            8        DISKETTE_HEADER.UNIQUE_ID
      130      130              ~XDS            6        DISKETTE_HEADER.DIR_SIZE
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:5    
      131      131              ~XCN            8        DISKETTE_HEADER.CONFIG_NAME
      132      132              ~XET            4        DISKETTE_HEADER.EQUIP_TYPE
      133      133              ~XIN           12        DISKETTE_HEADER.IPI_NUM
      134      134              ~XDN           12        DISKETTE_HEADER.DISK_DWG_NUM
      135      135              ~XPT            3        DISKETTE_HEADER.IPI_NUM.PRD_NUM_TAB
      136      136              ~XXX            1        DISKETTE_HEADER.X_OF_N.X
      137      137              ~XXN            1        DISKETTE_HEADER.X_OF_N.N
      138      138              ~YPN            8        DIRECTORY_ENTRY.PATH_NAME
      139      139              ~YFS            6        DIRECTORY_ENTRY.FILE_SIZE
      140      140              ~ZMT            4        DISKETTE_FILE.MODULE_TYPE
      141      141              ~ZET            4        DISKETTE_FILE.EQUIP_TYPE
      142      142              ~ZUN            8        DISKETTE_FILE.UNIQUE_NAME
      143      143              ~ZDN           12        DISKETTE_FILE.MOD_DWG_NUM
      144      144              ~ZTD            6        DISKETTE_FILE.TTL_DATE
      145      145              ~ZPR            4        DISKETTE_FILE.PGM_REV
      146      146              ~ZDT            4        DISKETTE_FILE.DISK_TAB
      147      147              ~ZRT            4        DISKETTE_FILE.LD_CTL.REC_TYPE
      148      148              --------   ------------  -------------------
      149      149
      150      150        */
      151      151        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:6    
      152      152        TOU$REPLACE: PROC (MSG, MSG_LGTH) ;
      153      153
      154      154        /* Include Files */
      155      155
      156      156           %INCLUDE B_ERRORS_C;
      157     1283           %INCLUDE CP_6;
      158     6842           %INCLUDE CP_6_SUBS;
      159     7382           %INCLUDE TOU_DEER_M ;
      160     8793
      161     8794        /* Parameters */
      162     8795
      163     8796    1      DCL MSG                        CHAR (MSG_LGTH) CALIGNED ;
      164     8797    1      DCL MSG_LGTH                   UBIN WORD ALIGNED ;
      165     8798
      166     8799        /* Macros */
      167     8800
      168     8801           %DUP (STCLASS = SYMREF) ;
      169     8944           %CATALOG_DATA (STCLASS = SYMREF) ;
      170     8958           %CATALOG_ENTRY_KEY (STCLASS = SYMREF) ;
      171     8972           %CATALOG_KEY (STCLASS = SYMREF) ;
      172     8986           %DKEND (STCLASS = SYMREF) ;
      173     9010           %LOG_B (STCLASS = "BASED (DUP.LOG_V.PTR$)") ;
      174     9036           %MASTER_ENTRY_KEY (STCLASS = SYMREF) ;
      175     9050           %MCA_B (STCLASS = "BASED (DUP.BINARY_V.PTR$)") ;
      176     9120           %OBJECT (STCLASS = SYMREF) ;
      177     9183           %OBJECT_KEY (STCLASS = SYMREF) ;
      178     9196           %BCD_B (STCLASS = "BASED (DUP.BCD_V.PTR$)") ;
      179     9208           %SUBCATALOG_DATA (STCLASS = SYMREF) ;
      180     9221           %REVISION_B (STCLASS = SYMREF) ;
      181     9242           %TOU$DEER (STCLASS = SYMREF) ;
      182     9256           %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:7    
      183     9257        /* Variables */
      184     9258
      185     9259    1      DCL BCD_ASCII_TABLE CHAR (64) CALIGNED SYMREF;
      186     9260    1      DCL BCD_ASCII (0:63) REDEF BCD_ASCII_TABLE CHAR (1) CALIGNED ;
      187     9261
      188     9262    1      DCL 1 BLOCK        SYMREF,
      189     9263    1            2 CONTROL        UBIN WORD ALIGNED,
      190     9264    1            2 OFFSET         UBIN WORD ALIGNED,
      191     9265    1            2 BSN            UBIN WORD ALIGNED,
      192     9266    1            2 BCW,
      193     9267    1              3 BSN          UBIN HALF UNAL,
      194     9268    1              3 SIZE         UBIN HALF UNAL,
      195     9269    1            2 DATA,
      196     9270    1              3 RCW,
      197     9271    1                4 SIZE       UBIN HALF UNAL,
      198     9272    1                4 FILE_MARK  BIT  (6)  UNAL,
      199     9273    1                4 *          UBIN (12) UNAL,
      200     9274    1              3 DATA (0:317) UBIN WORD ALIGNED;
      201     9275
      202     9276    1      DCL BLOCK_SEQ CHAR (4) SYMREF ALIGNED;
      203     9277
      204     9278    1      DCL FILE_FILE UBIN WORD SYMREF ALIGNED;
      205     9279
      206     9280    1      DCL 1 ID SYMREF CALIGNED,
      207     9281    1            2 ID      CHAR (6) UNAL,
      208     9282    1            2 REVISION CHAR (6) UNAL,
      209     9283    1            2 REV REDEF REVISION UNAL,
      210     9284    1              3 *     CHAR (4) UNAL,
      211     9285    1              3 LEVEL CHAR (2) UNAL,
      212     9286    1            2 USAGE   CHAR (6) UNAL;
      213     9287           %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:8    
      214     9288    1      DCL IFAD_ARS# UBIN WORD SYMREF ALIGNED ;
      215     9289
      216     9290    1      DCL CUR#   UBIN WORD SYMREF ALIGNED;
      217     9291    1      DCL LOG#   UBIN WORD SYMREF ALIGNED;
      218     9292
      219     9293    1      DCL 1 RCW SYMREF ALIGNED,
      220     9294    1            2 SIZE        UBIN HALF UNAL,
      221     9295    1            2 FILE_MARK   BIT  (6)  UNAL,
      222     9296    1            2 MEDIA_CODE  UBIN (6) UNAL,
      223     9297    1            2 REPORT_CODE UBIN (6) UNAL;
      224     9298    1      DCL RCW_OCTAL (0:11) REDEF RCW UBIN (3) UNAL;
      225     9299    1      DCL R_START UBIN WORD ALIGNED;
      226     9300           %EJECT;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:9    
      227     9301        /* ********************
      228     9302           ********************
      229     9303           **  Main Program  **
      230     9304           ********************
      231     9305           ******************** */
      232     9306
      233     9307    1      CALL INDEX (R_START, '~', MSG, 0) ;
      234     9308    2      DO WHILE (R_START < MSG_LGTH);
      235     9309    2         CALL SELECT_LETTER ;
      236     9310    2         CALL INDEX (R_START, '~', MSG, R_START+1) ;
      237     9311    2      END /* WHILE */ ;
      238     9312    1      RETURN;
      239     9313
      240     9314        /* ********************
      241     9315           ********************
      242     9316           **  End Of  Main  **
      243     9317           ********************
      244     9318           ******************** */
      245     9319        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:10   
      246     9320    1   SELECT_LETTER: PROC ;
      247     9321
      248     9322        /* Code */
      249     9323
      250     9324    3      DO SELECT (SUBSTR (MSG, R_START + 1, 1)) ;
      251     9325    3         SELECT ('A') ;
      252     9326    3         CALL SUB_FOR_LETTER_A ;
      253     9327    3         SELECT ('B') ;
      254     9328    3         CALL SUB_FOR_LETTER_B ;
      255     9329    3         SELECT ('C') ;
      256     9330    3         CALL SUB_FOR_LETTER_C ;
      257     9331    3         SELECT ('D') ;
      258     9332    3         CALL SUB_FOR_LETTER_D ;
      259     9333    3         SELECT ('E') ;
      260     9334    3         CALL SUB_FOR_LETTER_E ;
      261     9335    3         SELECT ('F') ;
      262     9336    3         CALL SUB_FOR_LETTER_F ;
      263     9337    3         SELECT ('G') ;
      264     9338    3         CALL SUB_FOR_LETTER_G ;
      265     9339    3         SELECT ('H') ;
      266     9340    3         CALL SUB_FOR_LETTER_H ;
      267     9341    3         SELECT ('I') ;
      268     9342    3         CALL SUB_FOR_LETTER_I ;
      269     9343    3         SELECT ('J') ;
      270     9344    3         CALL SUB_FOR_LETTER_J ;
      271     9345    3         SELECT ('L') ;
      272     9346    3         CALL SUB_FOR_LETTER_L ;
      273     9347    3         SELECT ('M') ;
      274     9348    3         CALL SUB_FOR_LETTER_M ;
      275     9349                 %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:11   
      276     9350    3         SELECT ('O') ;
      277     9351    3         CALL SUB_FOR_LETTER_O ;
      278     9352    3         SELECT ('P') ;
      279     9353    3         CALL SUB_FOR_LETTER_P ;
      280     9354    3         SELECT ('R') ;
      281     9355    3         CALL SUB_FOR_LETTER_R ;
      282     9356    3         SELECT ('S') ;
      283     9357    3         CALL SUB_FOR_LETTER_S ;
      284     9358    3         SELECT ('T') ;
      285     9359    3         CALL SUB_FOR_LETTER_T ;
      286     9360    3         SELECT ('W') ;
      287     9361    3         CALL SUB_FOR_LETTER_W ;
      288     9362    3         SELECT ('X') ;
      289     9363    3         CALL SUB_FOR_LETTER_X ;
      290     9364    3         SELECT ('Y') ;
      291     9365    3         CALL SUB_FOR_LETTER_Y ;
      292     9366    3         SELECT ('Z') ;
      293     9367    3         CALL SUB_FOR_LETTER_Z ;
      294     9368    3      END /* SELECT */ ;
      295     9369    2      RETURN ;
      296     9370
      297     9371        /* END SELECT_LETTER */
      298     9372        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:12   
      299     9373    2   SUB_FOR_LETTER_A: PROC ;
      300     9374
      301     9375        /* Code */
      302     9376
      303     9377    3      IF SUBSTR (MSG, R_START, 3) = '~A '
      304     9378    4      THEN DO SELECT (OBJECT.BCD.ASSEMBLER) ;
      305     9379    4              SELECT ('B') ;
      306     9380    4              CALL INSERT (MSG, R_START, 6, 'BOS') ;
      307     9381    4              SELECT ('G') ;
      308     9382    4              CALL INSERT (MSG, R_START, 6, 'GMAP') ;
      309     9383    4              SELECT ('L') ;
      310     9384    4              CALL INSERT (MSG, R_START, 6, 'Level6') ;
      311     9385    4              SELECT ('M') ;
      312     9386    4              CALL INSERT (MSG, R_START, 6, 'MPC') ;
      313     9387    4              SELECT ('P', '.') ;
      314     9388    4              CALL INSERT (MSG, R_START, 6, 'PLM') ;
      315     9389    4              SELECT ('N') ;
      316     9390    4              CALL INSERT (MSG, R_START, 6, 'None') ;
      317     9391    4           END /* THEN SELECT */ ;
      318     9392    3      ELSE CALL BINARY_2_ASCII (6, IFAD_ARS# - 1); /* ~ARS */
      319     9393    3      RETURN ;
      320     9394
      321     9395    3   END SUB_FOR_LETTER_A ;
      322     9396        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:13   
      323     9397    2   SUB_FOR_LETTER_B: PROC ;
      324     9398
      325     9399        /* Code */
      326     9400
      327     9401    3      IF SUBSTR (MSG, R_START, 4) = '~BLK'
      328     9402    3      THEN CALL BINARY_2_ASCII (6, BLOCK.BSN);
      329     9403    3      ELSE IF SUBSTR (MSG, R_START, 4) = '~BLS'
      330     9404    3         THEN CALL INSERT (MSG, R_START, 4, BLOCK_SEQ);
      331     9405    3         ELSE IF SUBSTR (MSG, R_START, 4) = '~BCZ'
      332     9406    3            THEN CALL BINARY_2_ASCII (6, BINBIT (BLOCK.BCW.SIZE, 36)) ;
      333     9407    3            ELSE IF SUBSTR (MSG, R_START, 4) = '~BCT'
      334     9408    3               THEN CALL BINARY_2_ASCII (6, BINBIT (OBJECT.BINARY_COUNT, 36)) ;
      335     9409    3               ELSE CALL BINARY_2_ASCII (6, BLOCK.CONTROL); /* ~BSZ */
      336     9410    3      RETURN ;
      337     9411
      338     9412    3   END SUB_FOR_LETTER_B ;
      339     9413        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:14   
      340     9414    2   SUB_FOR_LETTER_C: PROC ;
      341     9415
      342     9416        /* Code */
      343     9417
      344     9418    4      DO SELECT (SUBSTR (MSG, R_START, 5)) ;
      345     9419    4         SELECT ('~CAT ') ;
      346     9420    4         CALL INSERT (MSG, R_START, 21, CATALOG_KEY.DATA) ;
      347     9421    4         SELECT ('~CD  ') ;
      348     9422    4         CALL BINARY_2_ASCII (6, BINBIT (CATALOG_DATA.DECK#, 36));
      349     9423    4         SELECT ('~CLAS') ;
      350     9424    5         DO SELECT (OBJECT.BCD.CLASS) ;
      351     9425    5            SELECT ('I') ;
      352     9426    5            CALL INSERT (MSG, R_START, 5, 'ITR');
      353     9427    5            SELECT ('F', 'N', 'H') ;
      354     9428    5            IF OBJECT.BCD.LIBRARY = 'HNIOA0'
      355     9429    5            THEN CALL INSERT (MSG, R_START, 5, 'NIO');
      356     9430    5            ELSE CALL INSERT (MSG, R_START, 5, 'FW') ;
      357     9431    5            SELECT (ELSE) ;
      358     9432    5         CALL INSERT (MSG, R_START, 5, 'MDR');
      359     9433    5         END /* SELECT */ ;
      360     9434    4      SELECT ('~CLS ') ;
      361     9435    4      CALL INSERT (MSG, R_START, 4, OBJECT.BCD.CLASS);
      362     9436    4      SELECT ('~COL ') ;
      363     9437    4      CALL BINARY_2_ASCII (4, CUR# + 1);
      364     9438    4      SELECT ('~CUR ') ;
      365     9439    4      CALL BINARY_2_ASCII (4, CUR#) ;
      366     9440    4      END /* SELECT */ ;
      367     9441    3      RETURN ;
      368     9442
      369     9443    3   END SUB_FOR_LETTER_C ;
      370     9444        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:15   
      371     9445    2   SUB_FOR_LETTER_D: PROC ;
      372     9446
      373     9447        /* Code */
      374     9448
      375     9449    3      IF SUBSTR (MSG, R_START, 5) = '~DATE'
      376     9450    3      THEN CALL INSERT (MSG, R_START, 6, OBJECT.BCD.DATE);
      377     9451    3      ELSE IF SUBSTR (MSG, R_START, 5) = '~DECK'
      378     9452    3         THEN CALL BINARY_2_ASCII (6, BINBIT (OBJECT.DECK#, 36));
      379     9453    3         ELSE CALL INSERT (MSG, R_START, 40, DUP.DF.FID) ; /* ~DF_FID */
      380     9454    3      RETURN ;
      381     9455
      382     9456    3   END SUB_FOR_LETTER_D ;
      383     9457        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:16   
      384     9458    2   SUB_FOR_LETTER_E: PROC ;
      385     9459
      386     9460        /* Code */
      387     9461
      388     9462    3      IF SUBSTR (MSG, R_START, 3) = '~EC'
      389     9463    3      THEN CALL BINARY_2_ASCII (6, BINBIT (CATALOG_DATA.ENTRY_COUNT, 36));
      390     9464    3      ELSE IF SUBSTR (MSG, R_START, 3) = '~EM'
      391     9465    3         THEN CALL BINARY_2_ASCII (6, BINBIT (MASTER_ENTRY_KEY.ENTRY#, 36));
