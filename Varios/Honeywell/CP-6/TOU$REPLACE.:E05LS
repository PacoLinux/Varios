PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:33   
      710     9922    1   BINARY_2_ASCII: PROC (SIZE, BINARY) ;
      711     9923
      712     9924    2      DCL BINARY  UBIN WORD     ALIGNED;
      713     9925    2      DCL M       UBIN WORD     ALIGNED;
      714     9926    2      DCL SIZE    UBIN WORD     ALIGNED;
      715     9927    2      DCL STRING  CHAR (8)    ALIGNED;
      716     9928    2      DCL STRING_CHAR (0:7) REDEF STRING CHAR (1) UNAL;
      717     9929
      718     9930        /* Code */
      719     9931
      720     9932    2      CALL BINCHAR (STRING, BINARY) ;
      721     9933    2      M = 0;
      722     9934    3      DO WHILE (STRING_CHAR(M) = '0' & M < 7);
      723     9935    3         STRING_CHAR(M) = ' ';
      724     9936    3         M = M + 1;
      725     9937    3      END /* WHILE */ ;
      726     9938    2      CALL INSERT (MSG, R_START, SIZE, SUBSTR (STRING, 8 - SIZE)) ;
      727     9939    2      RETURN;
      728     9940
      729     9941    2   END BINARY_2_ASCII;
      730     9942        %EJECT ;
PL6.E3A0      #001=TOU$REPLACE File=TOU$REPLACE.:E05TSI                          WED 07/30/97 06:50 Page:34   
      731     9943    1   MCA_2_ASCII: PROC (SIZE, BINARY) ;
      732     9944
      733     9945    2      DCL BINARY  UBIN WORD ALIGNED;
      734     9946    2      DCL TEMP_   UBIN WORD ALIGNED ;
      735     9947    2      DCL 1 TEMP REDEF TEMP_ ALIGNED,
      736     9948    2            2 *   UBIN (18) UNAL,
      737     9949    2            2 U   UBIN (10) UNAL,
      738     9950    2            2 L   UBIN (8)  UNAL ;
      739     9951    2      DCL M       UBIN WORD ALIGNED;
      740     9952    2      DCL SIZE    UBIN WORD ALIGNED;
      741     9953    2      DCL STRING  CHAR (8)  ALIGNED;
      742     9954    2      DCL STRING_CHAR (0:7) REDEF STRING CHAR (1) CALIGNED ;
      743     9955
      744     9956        /* Code */
      745     9957
      746     9958    2      TEMP_ = BINARY ;
      747     9959    2      TEMP.U = TEMP.U / 2 ;
      748     9960    2      CALL BINCHAR (STRING, TEMP_) ;
      749     9961    2      M = 0;
      750     9962    3      DO WHILE (STRING_CHAR(M) = '0' & M < 7);
      751     9963    3         STRING_CHAR(M) = ' ';
      752     9964    3         M = M + 1;
      753     9965    3      END /* WHILE */ ;
      754     9966    2      CALL INSERT (MSG, R_START, SIZE, SUBSTR (STRING, 8 - SIZE)) ;
      755     9967    2      RETURN;
      756     9968
      757     9969    2   END MCA_2_ASCII;
      758     9970    1   END TOU$REPLACE;

