PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:13   
      309     1112    1   INSERT_SLICE_IN_ARRAY : PROC (INDX, DEFAULT, SLICE, SLICELEN, DEF_BASE, MAX_INDX);
      310     1113
      311     1114        /* INSERT_SLICE_IN_ARRAY WILL ACCEPT SPECIFICATIONS FOR
      312     1115           ENCODING AN ENTRY INTO THE TRANSITION TABLE.  FOR A DESCRIPTION
      313     1116           OF THIS TECHNIQUE, SEE AHO & ULLMAN - PRINCIPLES OF
      314     1117           COMPILER DESIGN, SECTIONS 6.8 & 3.8.
      315     1118
      316     1119           THE PARAMETERS TO THIS PROCEDURE INCLUDE:
      317     1120
      318     1121             INDX
      319     1122                  - THE INDEX INTO THE DEF_BASE ARRAY AT WHICH TO INSERT
      320     1123                    THE ENTRY.
      321     1124
      322     1125             DEFAULT
      323     1126                  - THE DEFAULT ACTION TO TAKE BY THE PARSER.  THIS VALUE
      324     1127                    IS INSERTED INTO THE DEF_BASE ARRAY AT ELEMENT INDEX.
      325     1128
      326     1129             SLICE
      327     1130                  - THE SLICE ARRAY ENCODES THE ACTIONS TO BE TAKEN BY
      328     1131                    THE PARSER UPON RECOGNITION OF CERTAIN INDICIES.  THE
      329     1132                    SLICE ARRAY IS INSERTED INTO NULL ELEMENTS OF
      330     1133                    THE NEXT_CHECK STRUCTURE.  THE BASE OF THE DEF_BASE
      331     1134                    ARRAY WILL POINT TO THE INDEX INTO THE NEXT_CHECK
      332     1135                    STRUCTURE WHERE THE SLICE WAS INSERTED.
      333     1136
      334     1137             SLICELEN
      335     1138                  - LENGTH OF THE ARRAY SLICE.
      336     1139
      337     1140            DEF_BASE
      338     1141                  - THE DEF_BASE ARRAY ENCODES THE DEFAULT AND BASE
      339     1142                    INDEX INTO THE NEXT_CHECK STRUCTURE.
      340     1143
      341     1144            MAX_INDX
      342     1145                  - The maximum index into the array which may be checked
      343     1146                    during the parse.
      344     1147
      345     1148        */
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:14   
      346     1149
      347     1150    2   DCL 1 DEF_BASE (0:%NR_CONFIG_MAX) ALIGNED,
      348     1151    2         2 DEFAULT SBIN HALF HALIGNED,
      349     1152    2         2 BASE    SBIN HALF HALIGNED;
      350     1153
      351     1154    2   DCL INDX     SBIN WORD;
      352     1155    2   DCL DEFAULT  SBIN WORD;
      353     1156    2   DCL SLICELEN SBIN WORD;
      354     1157    2   DCL MAX_INDX SBIN WORD;
      355     1158
      356     1159    2   DCL 1 SLICE (0:0),   /* VARIABLE LENGTH*/
      357     1160    2         2 INDX SBIN HALF,
      358     1161    2         2 VAL SBIN HALF;
      359     1162
      360     1163
      361     1164    2   DCL BASE_FOUND_NUM SBIN WORD;  /* BASE FOUND BY MATCH SLICE */
      362     1165
      363     1166
      364     1167
      365     1168
      366     1169        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:15   
      367     1170    2   TRANSFER_SLICE : PROC;  /* IN INSERT_SLICE_IN_ARRAY */
      368     1171
      369     1172        /* A SLICE HAS BEEN MATCHED AGAINST THE NEXT_CHECK ARRAY.  WE
      370     1173           NOW NEED TO TRANSFER ITS CONTENTS.
      371     1174        */
      372     1175
      373     1176    3   DCL I SBIN WORD;
      374     1177
      375     1178        %MACRO ENSURE_NC_LEN (N=0);
      376     1179
      377     1180          DO;
      378     1181            IF NEXT_CHECK_LEN < BASE_FOUND_NUM + SLICE.INDX(I) THEN
      379     1182              NEXT_CHECK_LEN = BASE_FOUND_NUM + SLICE.INDX(I);
      380     1183            IF NEXT_CHECK_LEN > %MAX_NC_LEN THEN
      381     1184             CALL DIAG (15,'ERROR => Output ACTION table overflow.%%');
      382     1185          END;
      383     1186
      384     1187        %MEND;
      385     1188
      386     1189
      387     1190
      388     1191    4   DO I = 1 TO SLICELEN;
      389     1192
      390     1193    4         IF NOT (SLICE.VAL(I) = UNIQUE_FINAL_STATE) THEN
      391     1194                /* IF FINAL STATE, LEAVE NEXT AT ZERO */
      392     1195    5           DO;
      393     1196                %ENSURE_NC_LEN;
      394     1204    5             NEXT_CHECK.NEXT(BASE_FOUND_NUM + SLICE.INDX(I)) = SLICE.VAL(I);
      395     1205    5           END;
      396     1206    4         NEXT_CHECK.CHECK(BASE_FOUND_NUM + SLICE.INDX(I)) = INDX;
      397     1207
      398     1208    4   END;
      399     1209
      400     1210    3   IF NEXT_CHECK_LEN < BASE_FOUND_NUM + MAX_INDX THEN
      401     1211    3     NEXT_CHECK_LEN = BASE_FOUND_NUM + MAX_INDX;
      402     1212    3   IF NEXT_CHECK_LEN > %MAX_NC_LEN THEN
      403     1213    3     CALL DIAG (15, 'ERROR => Output action table overflow.%%');
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:16   
      404     1214
      405     1215    3   END TRANSFER_SLICE;
      406     1216
      407     1217
      408     1218        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:17   
      409     1219    2   MATCH_SLICE : PROC;  /* IN INSERT_SLICE_IN_ARRAY */
      410     1220
      411     1221        /* TAKE THE SLICE AND MATCH IT UP WITH ALL ELEMENTS IN NEXT_CHECK.
      412     1222           RETURN BASE_FOUND_NUM .
      413     1223        */
      414     1224
      415     1225    3   DCL I SBIN WORD;
      416     1226
      417     1227    3   BASE_FOUND_NUM = 0;
      418     1228
      419     1229
      420     1230    4   DO WHILE ('1'B);
      421     1231
      422     1232    4     BASE_FOUND_NUM = BASE_FOUND_NUM + 1;
      423     1233    4     I = 1;
      424     1234
      425     1235    4     IF BASE_FOUND_NUM + SLICE.INDX(I) > %MAX_NC_LEN THEN
      426     1236    4       CALL DIAG(15,'ERROR => Output ACTION Table Overflow%%');
      427     1237
      428     1238    5     DO WHILE NEXT_CHECK_CLEAR.B(SLICE.INDX(I) + BASE_FOUND_NUM) = '0'B;
      429     1239
      430     1240    5       I = I + 1;
      431     1241    5       IF I > SLICELEN THEN
      432     1242    5         RETURN;   /* Found Base */
      433     1243
      434     1244    5       IF BASE_FOUND_NUM + SLICE.INDX(I) > %MAX_NC_LEN THEN
      435     1245    5         CALL DIAG(15,'ERROR => Output ACTION Table Overflow%%');
      436     1246
      437     1247    5     END;
      438     1248
      439     1249          /* We did not get a match */
      440     1250
      441     1251    4   END;
      442     1252
      443     1253    3   END MATCH_SLICE;
      444     1254
      445     1255
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:18   
      446     1256        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:19   
      447     1257             /* MAIN BODY OF INSERT_SLICE_IN_ARRAY */
      448     1258
      449     1259    2   DEF_BASE.DEFAULT(INDX) = DEFAULT;
      450     1260
      451     1261    2   IF SLICELEN > 0 THEN
      452     1262    3     DO;
      453     1263    3       CALL MATCH_SLICE;
      454     1264    3       CALL TRANSFER_SLICE;
      455     1265    3     END;
