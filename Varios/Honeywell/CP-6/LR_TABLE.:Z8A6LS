               565            OT_OPT))) THEN
     1621      566    2     DO;
     1622      567    2       ABSV = MAXIMUM (TS.MAXV, TS.MINV, - TS.MINV);
     1623      568    2       TS.SIGNED = (TS.MINV < 0);
     1624      569    2       TS.NBITS = 0;
     1625      570    3       DO UNTIL ABSV = 0;
     1626      571    3         TS.NBITS = TS.NBITS + 1;
     1627      572    3         ABSV = ABSV/2;
     1628      573    3       END;
     1629      574
