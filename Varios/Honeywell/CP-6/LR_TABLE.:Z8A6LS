      273     1052    4           IF OPTIONS(AN_OPT) THEN
      274     1053    4             CALL NUM_ACT(SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(I) + PROD.RIGHT_CNT(I))
              1053                      ));
      275     1054    4           ELSE
      276     1055    5             DO;
      277     1056                    %OUT_STR(S="'%'");
      278     1059    5               CALL OUT_STR(SUBSTR(SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(I) +
      279     1060    5                                         PROD.RIGHT_CNT(I))) -> SYMBOL.TEXTCHR,
      280     1061    5                                   1,
      281     1062    5                                   SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(I) +
      282     1063    5                                   PROD.RIGHT_CNT(I)))-> SYMBOL.SYMLEN -2),
      283     1064    5                      SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(I) +
      284     1065    5                                  PROD.RIGHT_CNT(I))) -> SYMBOL.SYMLEN - 2);
      285     1066    5             END;
      286     1067    4         END;
      287     1068    3     ELSE
      288     1069    3       CALL OUT_DEC (0);
      289     1070          %OUT_STR(S="');'");
      290     1073    3     CALL FLUSH;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:12   
      291     1074
