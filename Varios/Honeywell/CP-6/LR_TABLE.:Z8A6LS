       18       18        %INCLUDE LR_GRAM_EXT;
       19      557        %INCLUDE LR_CNFG_EXT;
       20      710
       21      711    1   DCL DIAG                 ENTRY(6);
       22      712    1   DCL OUT_STR              ENTRY(2);
       23      713    1   DCL OUT_TOKEN            ENTRY(2);
       24      714    1   DCL FLUSH                ENTRY;
       25      715    1   DCL OUT_DEC              ENTRY(1);
       26      716    1   DCL OUTPUT_PARSE_SYMREFS ENTRY;
       27      717    1   DCL OUT_SYMBOL ENTRY(1);
       28      718    1   DCL OUT_SYMBOL_TOKEN ENTRY(1);
       29      719    1   DCL OUTPUT_TERMINAL_STRINGS ENTRY;
       30      720
       31      721
       32      722        /* This module will output to two files both
       33      723           the tables generated for the grammar and
       34      724           a set of %equ's and symref's to use those
       35      725           tables */
       36      726
       37      727
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:2    
       38      728        %SUB UNIQUE_FINAL_STATE = 99999;
       39      729
       40      730
       41      731
       42      732
       43      733
       44      734    1   DCL 1 STATE (0:%NR_CONFIG_MAX),
       45      735    1         2 DEFAULT_ACTION SBIN HALF HALIGNED,
       46      736    1         2 BASE           SBIN HALF HALIGNED;   /* INTO ACTIONS */
       47      737
       48      738    1   DCL 1 GOTO_ (0:NR_SYMS_MAX),
       49      739    1         2 DEFAULT_ACTION SBIN HALF HALIGNED,
       50      740    1         2 BASE SBIN HALF HALIGNED;  /* INTO GOTO_LIST */
       51      741
       52      742        %EQU MAX_NC_LEN = MAX(13*NR_SYMS_MAX, 13*NR_CONFIG_MAX);
       53      743
       54      744        /* NEXT_CHECK IS USED BY BOTH THE ACTION & GOTO ROUTINES  TO BUILD
       55      745           THE FSM STRUCTURE. */
       56      746    1   DCL 1 NEXT_CHECK (0:%MAX_NC_LEN),
       57      747    1         2 NEXT  SBIN HALF HALIGNED,
       58      748    1         2 CHECK SBIN HALF HALIGNED;
       59      749    1   DCL 1 NEXT_CHECK_CLEAR REDEF NEXT_CHECK,
       60      750    1         2 B (0:%MAX_NC_LEN) BIT(36);
       61      751
       62      752    1   DCL NEXT_CHECK_LEN SBIN WORD;
       63      753
       64      754        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:3    
       65      755    1   OUT_EQU:PROC(STR,LEN,N);
       66      756
       67      757    2   DCL LEN SBIN;
       68      758    2   DCL STR CHAR(12);
       69      759    2   DCL N SBIN;
       70      760
       71      761
       72      762        /* STR & LEN DEFINE A NAME TO BE USED FOR A CONSTANT DECLARATION */
       73      763
       74      764    2           CALL FLUSH;
       75      765
       76      766                %OUT_STR(S="'%EQU '");
       77      769
       78      770    2           CALL OUT_STR(STR,LEN);
       79      771
       80      772                %OUT_STR(S="'='");
       81      775
       82      776    2           CALL OUT_DEC(N);
       83      777
       84      778                %OUTLINE(S="';'");
       85      784    2   END OUT_EQU;
       86      785
       87      786        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:4    
       88      787    1   OUTPUT_ACTIONS : PROC;
       89      788
       90      789        /* Go thru all actions and output a list of PL6
       91      790           EQUS which will associate integers with the names
       92      791           of the actions.  These names should be used in a
       93      792           CASE statement in the parser to execute the
       94      793           appropriate semantic actions.
       95      794        */
       96      795
       97      796    2   DCL I          SBIN WORD;
       98      797    2   DCL ACTION_NUM SBIN WORD;
       99      798    2   DCL ACTION_VAL SBIN WORD;
      100      799
      101      800        %SELECT_SUBS;
      102      803
      103      804    2   ACTION_NUM = 0;
      104      805
      105      806        %OUTLINE(S="'/* Actions */'");
      106      812        %OUTLINE(S="' '");
      107      818
      108      819    3   DO I = NR_NT+1 TO LAST_ACT;
      109      820
      110      821    3     ACTION_NUM = ACTION_NUM + 1;
      111      822    3     IF SYM_PTRS(I) -> SYMBOL.ORDER_SPECIFIED THEN
      112      823    4       DO;
      113      824    4         ACTION_VAL = SYM_PTRS(I) -> SYMBOL.ORDER;
      114      825    4         CALL OUT_EQU (SUBSTR(SYM_PTRS(I) -> SYMBOL.TEXTCHR,
      115      826    4                              1,
      116      827    4                              SYM_PTRS(I) -> SYMBOL.SYMLEN - 2),
      117      828    4                       SYM_PTRS(I) -> SYMBOL.SYMLEN - 2,
      118      829    4                       ACTION_VAL);
      119      830              %BOUNDS(T=SEM, V=ACTION_VAL);
      120      836    4       END;
      121      837    3     ELSE
      122      838    4       DO;
      123      839    4         CALL OUT_EQU (SUBSTR(SYM_PTRS(I)->SYMBOL.TEXTCHR,
      124      840    4                               1,
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:5    
      125      841    4                               SYM_PTRS(I)->SYMBOL.SYMLEN-2),
      126      842    4                       SYM_PTRS(I) -> SYMBOL.SYMLEN-2,
      127      843    4                       ACTION_NUM);
      128      844              %BOUNDS(T=SEM, V=ACTION_NUM);
      129      850    4       END;
      130      851
      131      852    3   END;
      132      853
      133      854        %OUTLINE(S="' '");
      134      860
      135      861
      136      862    2   END OUTPUT_ACTIONS;
      137      863        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:6    
      138      864    1   OUTPUT_TERM_INFO : PROC;
      139      865
      140      866        /* This module will output equates corresponding to certain
      141      867           important terminals, including EOF & ERROR, so that
      142      868           the parser may use them.
      143      869        */
      144      870
      145      871        %SELECT_SUBS;
      146      874
      147      875        /* output EOF */
      148      876
      149      877        %OUT_STR(S="'%EQU TERM_EOF_SYM = '");
      150      880    2   CALL OUT_DEC(EOF_SYMBOL_PTR -> SYMBOL.SYM_INDX);
      151      881        %OUT_STR(S="';'");
      152      884    2   CALL FLUSH;
      153      885
      154      886        /* output error, if present */
      155      887
      156      888        %OUT_STR(S="'%EQU TERM_ERROR_SYM = '");
      157      891    2   IF ERROR_SYMBOL_PTR = ADDR(NIL) THEN
      158      892    2     CALL OUT_DEC(0);
      159      893    2   ELSE
      160      894    2     CALL OUT_DEC(ERROR_SYMBOL_PTR -> SYMBOL.SYM_INDX);
      161      895        %OUT_STR(S="';'");
      162      898    2   CALL FLUSH;
      163      899
      164      900    2   IF OPTIONS(AN_OPT) THEN
      165      901          %OUTLINE(S="'%EQU ACTIONS_NUMERIC_OPT = ''1''B;'");
      166      907    2   ELSE
      167      908          %OUTLINE(S="'%EQU ACTIONS_NUMERIC_OPT = ''0''B;'");
      168      914
      169      915    2   END OUTPUT_TERM_INFO;
      170      916        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:7    
      171      917    1   OUTPUT_PRODUCTIONS : PROC;
      172      918
      173      919        /* This routine will output a table of production information
      174      920           for use by the parser.  This information includes the
      175      921           number of states to be popped off the state stack
      176      922           upon a reduction, along with the number of terminals
      177      923           that may be required to be popped off the token stack.
      178      924           The semantic action to be performed upon a reduction is
      179      925           also given.
      180      926        */
      181      927
      182      928    2   DCL NUM_RED_STACK SBIN WORD;
      183      929    2   DCL I             SBIN WORD;
      184      930    2   DCL R             SBIN WORD;
      185      931
      186      932
      187      933
      188      934        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:8    
      189      935    2   CALC_PROD_INFO : PROC (N);  /* OF OUTPUT_PRODUCTIONS */
      190      936
      191      937        /* Calculate the number of states to be popped and the
      192      938           number of terminals to be popped in a reduce operation.
      193      939        */
      194      940
      195      941    3   DCL I SBIN WORD;
      196      942    3   DCL N SBIN WORD;  /* Production Number */
      197      943
      198      944    3   NUM_RED_STACK = 0;
      199      945
      200      946    3   IF SYM_PTRS(PROD.GOAL_SYM_INDX(N)) -> SYMBOL.SYM_TYPE ~= SYM_TYPE_ACT THEN
      201      947    4     DO;
      202      948
      203      949    4       NUM_RED_STACK = PROD.RIGHT_CNT(N);
      204      950    4   END;
      205      951
      206      952    3   END CALC_PROD_INFO;
      207      953        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:9    
      208      954    2   NUM_ACT : PROC (SYM$);
      209      955
      210      956    3     DCL SYM$ PTR;
      211      957
      212      958    3     IF SYM$ -> SYMBOL.ORDER_SPECIFIED THEN
      213      959    3       CALL OUT_DEC (BINBIT(SYM$ -> SYMBOL.ORDER, 36));
      214      960    3     ELSE
      215      961    3       CALL OUT_DEC(SYM$ -> SYMBOL.SYM_INDX - NR_NT);
      216      962    3   END NUM_ACT;
      217      963        %EJECT;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:10   
      218      964            /* Main body of OUTPUT_PRODUCTIONS */
      219      965
      220      966
      221      967        %SELECT_OU;
      222      970
      223      971        /* Output header for declaration */
      224      972
      225      973        %OUTLINE(S="'%START_TABLE_PRODR;'");
      226      979
      227      980    3   DO I = 0 TO NR_PROD-1;
      228      981
      229      982          %OUT_STR(S="'  /* '");
      230      985    3     CALL OUT_DEC(I + 1);
      231      986          %OUT_STR(S="' '");
      232      989    3     CALL OUT_SYMBOL(SYM_PTRS(PROD.GOAL_SYM_INDX(I)));
      233      990          %OUT_STR(S="' ::='");
      234      993    4     DO R = 1 TO PROD.RIGHT_CNT(I);
      235      994    4       CALL OUT_SYMBOL_TOKEN(SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(I) + R - 1)));
      236      995    4     END;
      237      996
      238      997    3     IF PROD.TRAILING_SEM_ACT(I) THEN
      239      998    3       CALL OUT_SYMBOL_TOKEN(SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(I) +
      240      999    3                            PROD.RIGHT_CNT(I))));
      241     1000
      242     1001          %OUT_STR(S="' */'");
      243     1004    3     CALL FLUSH;
      244     1005
      245     1006    3     CALL CALC_PROD_INFO(I);
      246     1007          %OUT_STR(S="'    %PRODM(GOAL='");
      247     1010    3     CALL OUT_DEC(PROD.GOAL_SYM_INDX(I) - NR_TERM);
      248     1011
      249     1012          %BOUNDS(T=GOAL, V="PROD.GOAL_SYM_INDX(I) - NR_TERM");
      250     1018
      251     1019          %OUT_STR(S="',PSS='");
      252     1022    3     CALL OUT_DEC(NUM_RED_STACK);
      253     1023
      254     1024          %BOUNDS(T=PSS, V=NUM_RED_STACK);
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:11   
      255     1030
      256     1031          %OUT_STR(S="',SEM='");
      257     1034    3     IF SYM_PTRS(PROD.GOAL_SYM_INDX(I))->SYMBOL.SYM_TYPE = SYM_TYPE_ACT THEN
      258     1035    4       DO;
      259     1036    4         IF OPTIONS(AN_OPT) THEN
      260     1037    4           CALL NUM_ACT(SYM_PTRS(PROD.GOAL_SYM_INDX(I)));
      261     1038    4         ELSE
      262     1039    5           DO;
      263     1040                  %OUT_STR(S="'%'");
      264     1043    5             CALL OUT_STR(SUBSTR(SYM_PTRS(PROD.GOAL_SYM_INDX(I))->SYMBOL.TEXTCHR,
      265     1044    5                                 1,
      266     1045    5                                 SYM_PTRS(PROD.GOAL_SYM_INDX(I)) -> SYMBOL.SYMLEN-2),
      267     1046    5                          SYM_PTRS(PROD.GOAL_SYM_INDX(I))->SYMBOL.SYMLEN - 2);
      268     1047    5           END;
      269     1048    4       END;
      270     1049    3     ELSE
