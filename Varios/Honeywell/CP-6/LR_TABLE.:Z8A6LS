     1143      739
     1144      740              %OUT_STR(S="'A   '");
     1145      743
     1146      744    3     END;
     1147      745
     1148      746    2     CALL OUT_SYMBOL(SYM_PTRS(I));
     1149      747    2     IF LINELEN < 30 THEN
     1150      748    2       CALL OUT_STR('                    ', 30-LINELEN);
     1151      749
     1152      750    2     TPTR = SXRL(I);
     1153      751
     1154      752    3     DO WHILE TPTR ~= ADDR(NIL);
     1155      753
     1156      754            %OUT_STR(S="' '");
     1157      757    3       CALL OUT_DEC(TPTR -> XREF.P + 1);
     1158      758    3       TPTR = TPTR -> XREF.LINK;
     1159      759
     1160      760    3     END;
     1161      761
     1162      762    2     CALL FLUSH;
     1163      763    2   END;
     1164      764
     1165      765        /* RETURN MEMORY USED */
     1166      766
     1167      767    2   DO I = 1 TO LAST_NULL;
     1168      768
     1169      769    3     DO WHILE SXRL(I) ~= ADDR(NIL);
     1170      770    3       TPTR = SXRL(I) -> XREF.LINK;
     1171      771    3       CALL COLLECT_GARBAGE(SXRL(I));
PL6.E3A0      #003=OUTPUT_XREF File=LR_TABLE.:Z8A6TSI                            SAT 08/09/97 09:39 Page:191  
     1172      772    3       SXRL(I) = TPTR;
     1173      773    3     END;
     1174      774
     1175      775    2   END;
