     1630      575    2       IF TS.SIGNED THEN
     1631      576    2         TS.NBITS = TS.NBITS + 1;
     1632      577    2     END;
     1633      578    1   ELSE
     1634      579    2     DO;
     1635      580    2       TS.SIGNED = (TS.MINV < 0);
     1636      581    2       TS.NBITS = 18;
     1637      582    2     END;
     1638      583
     1639      584    1   END GENTYPE;
