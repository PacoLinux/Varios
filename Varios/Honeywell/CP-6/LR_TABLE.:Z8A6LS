     1034      582    1        2 LINK PTR ALIGNED;
     1035      583
     1036      584
     1037      585    1    ADD_REF: PROC (S,P);
     1038      586
     1039      587        /* ADD FACT THAT SYMBOL S WAS REFERENCED IN PRODUCTION P
     1040      588        */
     1041      589
     1042      590    2   DCL GET_STRING_MEM ENTRY(2);
     1043      591
     1044      592    2   DCL S SBIN WORD;
     1045      593    2   DCL P SBIN WORD;
     1046      594
     1047      595    2   IF SXRL(S) ~= ADDR(NIL) THEN
     1048      596    2     IF SXRL(S) -> XREF.P = P THEN
     1049      597    2       RETURN;
     1050      598
     1051      599    2   TPTR = SXRL(S);
     1052      600    2   CALL GET_STRING_MEM(SXRL(S), LENGTHC(XREF) / 2 - 1);
