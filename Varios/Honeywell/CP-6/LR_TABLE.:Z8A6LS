      923        4        /*T*                                                         */
      924        5        /*T***********************************************************/
      925        6        LIST_GRAMMAR : PROC;
      926        7
      927        8        /*D*
      928        9               NAME: LIST_GRAMMAR
      929       10            PURPOSE: LIST THE PRODUCTIONS ACTUALLY USED BY THE PARSER
      930       11                     GENERATOR IN PRODUCING TABLES.  THIS MAY DIFFER FROM
      931       12                     THE INPUT GRAMMAR.  THE CROSS REFERENCE OF SYMBOLS
      932       13                     IS GIVEN AGAINST THIS LIST.
      933       14        */
      934       15
      935       16    1   DCL P SBIN WORD;
      936       17    1   DCL GS SBIN WORD;
      937       18    1   DCL R SBIN WORD;
      938       19    1   DCL S SBIN WORD;
      939       20
      940       21        %INCLUDE LR_GRAM_EXT;
      941      560
      942      561    1   DCL OUT_STR      ENTRY(2);
      943      562    1   DCL OUT_TOKEN    ENTRY(2);
      944      563    1   DCL PRINT_PROD   ENTRY(3);
      945      564    1   DCL OUT_DEC      ENTRY(1);
      946      565    1   DCL FLUSH        ENTRY;
      947      566    1   DCL OUT_SYMBOL ENTRY(1);
      948      567    1   DCL OUT_SYMBOL_TOKEN ENTRY(1);
      949      568
      950      569        %SELECT_LO;
      951      572
      952      573        %OUTLINE(S="' '");
      953      579        %OUTLINE(S="' '");
      954      585        %OUTLINE(S="'==============='");
      955      591        %OUTLINE(S="'GRAMMAR LISTING'");
      956      597        %OUTLINE(S="'==============='");
PL6.E3A0      #002=LIST_GRAMMAR File=LR_TABLE.:Z8A6TSI                           SAT 08/09/97 09:38 Page:164  
      957      603        %OUTLINE(S="' '");
      958      609
      959      610    1   GS = -1;
      960      611
      961      612    2   DO P = 0 TO NR_PROD - 1;
      962      613
      963      614    2     IF PROD.GOAL_SYM_INDX(P) ~= GS THEN
      964      615    3       DO;
      965      616              %OUTLINE(S="' '");
      966      622    3         CALL OUT_DEC(P+1);
      967      623    3         CALL OUT_STR('     ', 5-LINELEN);
      968      624    3         CALL PRINT_PROD(P,0,'0'B);
      969      625    3         GS = PROD.GOAL_SYM_INDX(P);
      970      626    3       END;
      971      627    2     ELSE
      972      628    3       DO;
      973      629    3         CALL OUT_DEC(P+1);
      974      630    3         CALL OUT_STR('     ', 5-LINELEN);
      975      631              %OUT_STR(S="'     |'");
      976      634
      977      635    4         DO R = 1 TO PROD.RIGHT_CNT(P);
      978      636    4           S = RIGHT_PARTS(PROD.RIGHT_START(P) + R -1);
      979      637    4           CALL OUT_SYMBOL_TOKEN(SYM_PTRS(S));
      980      638    4         END;
      981      639
      982      640    3         IF PROD.TRAILING_SEM_ACT(P) THEN
      983      641    3           CALL OUT_SYMBOL_TOKEN(SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(P) +
      984      642    3                              PROD.RIGHT_CNT(P))));
      985      643    3         CALL FLUSH;
      986      644    3       END;
      987      645    2   END;
      988      646
      989      647    1   END LIST_GRAMMAR;
      990      648        %EOD;

