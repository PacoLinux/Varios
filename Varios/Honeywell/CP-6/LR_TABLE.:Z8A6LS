     1096      686    2   DO I = 1 TO LAST_NULL;
     1097      687    2     SXREF(I) = I;
     1098      688    2   END;
     1099      689
PL6.E3A0      #003=OUTPUT_XREF File=LR_TABLE.:Z8A6TSI                            SAT 08/09/97 09:39 Page:189  
     1100      690        /* BUBBLE SORT */
     1101      691    2   DO J = 1 TO LAST_NULL - 2;
     1102      692    3     DO I = 1 TO LAST_NULL - J;
     1103      693    3       C1_LEN = SYM_PTRS(SXREF(I)) -> SYMBOL.SYMLEN;
     1104      694    3       C1$ = ADDR(SYM_PTRS(SXREF(I)) -> SYMBOL.TEXTCHR);
     1105      695    3       C2_LEN = SYM_PTRS(SXREF(I + 1)) -> SYMBOL.SYMLEN;
     1106      696    3       C2$ = ADDR(SYM_PTRS(SXREF(I + 1)) -> SYMBOL.TEXTCHR);
     1107      697    3       IF (SYM_PTRS(SXREF(I+1)) -> SYMBOL.SYM_TYPE < SYM_PTRS(SXREF(I)) -> SYMBOL.
               697                SYM_TYPE)
     1108      698    3             OR
     1109      699    3          ((SYM_PTRS(SXREF(I+1)) -> SYMBOL.SYM_TYPE = SYM_PTRS(SXREF(I)) -> SYMBOL.
               699                   SYM_TYPE)
     1110      700    3                    AND (C1 > C2)) THEN
     1111      701    4         DO;
     1112      702    4           K = SXREF(I);
     1113      703    4           SXREF(I) = SXREF(I+1);
     1114      704    4           SXREF(I+1) = K;
     1115      705    4         END;
     1116      706    3     END;
     1117      707    2   END;
     1118      708
     1119      709
     1120      710    2   DO J =  1 TO LAST_NULL;
     1121      711    2     I = SXREF(J);
     1122      712
     1123      713    2     IF I > NR_TERM THEN
     1124      714    2       CALL OUT_DEC(I - NR_TERM);
     1125      715    2     ELSE
     1126      716    2       CALL OUT_DEC(I);
     1127      717    2     CALL OUT_STR('        ', 7-LINELEN);
     1128      718
     1129      719    3     DO CASE SYM_PTRS(I) -> SYMBOL.SYM_TYPE;
     1130      720
     1131      721    3       CASE(SYM_TYPE_TERM);
     1132      722
     1133      723              %OUT_STR(S="'T   '");
     1134      726
PL6.E3A0      #003=OUTPUT_XREF File=LR_TABLE.:Z8A6TSI                            SAT 08/09/97 09:39 Page:190  
