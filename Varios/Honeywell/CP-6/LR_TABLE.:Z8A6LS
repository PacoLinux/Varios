PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:24   
      580     1474              /* MAIN BODY OF OUTPUT_STATES */
      581     1475
      582     1476    2   NEXT_CHECK_LEN    = 0;
      583     1477    2   NEXT_CHECK_CLEAR.B = '0'B;
      584     1478
      585     1479
      586     1480    3   DO I = 0 TO NR_CONFIG - 1;
      587     1481
      588     1482    3     C$ = C#_TO_$ (I);
      589     1483
      590     1484    3   ACTION_SLICE_LEN = 0;
      591     1485    3   FIRST_ELEM       = 0;
      592     1486
      593     1487    3     IF C$ -> C_SET_B.REDUCE_COUNT = 1 THEN
      594     1488    4       DO;  /* ONLY ONE REDUCTION WHICH IS FIRST IN LIST */
      595     1489    4         FIRST_ELEM = 1;
      596     1490    4         DEFAULT = -(C$->C_SET_B.ELEM.KEY$(0) -> KEY_B.ELEM.PROD(0) + 1);
      597     1491    4       END;
      598     1492    3     ELSE
      599     1493    3       DEFAULT = 0;  /* ERROR */
      600     1494
      601     1495        /* HANDLE ALL OTHER SHIFTS AND REDUCTIONS */
      602     1496
      603     1497    4     DO J = FIRST_ELEM TO C$->C_SET_B.NR_ELEM - 1;
      604     1498
      605     1499    4       KEYN$ = C$ -> C_SET_B.ELEM.KEY$(J);
      606     1500    4       IF KEYN$ -> KEY_B.W.SYM_AFTER = 0 THEN
      607     1501              /* WE HAVE A REDUCTION */
      608     1502    5         DO;
      609     1503    5           FSET$ = ADDR(KEYN$ -> KEY_B.FOLLOW$ ->INCL_B.SET);
      610     1504    5           IF NOT C$ -> C_SET_B.ELEM.AMBIG_SEL_SHIFT(J) THEN
      611     1505    6           DO K = 1 TO NR_TERM;
      612     1506    6             IF AFSET.FSET(K) THEN
      613     1507    7               DO;
      614     1508    7                 ACTION_SLICE_LEN = ACTION_SLICE_LEN + 1;
      615     1509    7                 ACTION_SLICE.VAL(ACTION_SLICE_LEN) = -(KEYN$->KEY_B.ELEM.PROD(0) + 1);
      616     1510    7                 ACTION_SLICE.INDX(ACTION_SLICE_LEN) = K;
PL6.E3A0      #001=TABLES File=LR_TABLE.:Z8A6TSI                                 SAT 08/09/97 09:37 Page:25   
      617     1511    7               END;
      618     1512    6           END;
      619     1513    5         END;
      620     1514    4       ELSE
      621     1515              /* WE HAVE A SHIFT */
      622     1516    5         DO;
      623     1517    5           SYM_INDX = KEYN$ -> KEY_B.W.SYM_AFTER;
      624     1518    5           IF SYM_INDX <= NR_TERM THEN
      625     1519    5           IF NOT C$ -> C_SET_B.ELEM.AMBIG_SEL_REDUCE(J) THEN
      626     1520    6             DO;
      627     1521    6               ACTION_SLICE_LEN = ACTION_SLICE_LEN + 1;
      628     1522    6               ACTION_SLICE.INDX(ACTION_SLICE_LEN) = SYM_INDX;
      629     1523    6               ACTION_SLICE.VAL(ACTION_SLICE_LEN) = KEYN$ ->KEY_B.KEY_VAL$ ->C_SET_B.
              1523                        C_SET#;
      630     1524    6               IF ACTION_SLICE.VAL(ACTION_SLICE_LEN) = 0 THEN
      631     1525                      /* FLAG FINAL STATE */
      632     1526    6               ACTION_SLICE.VAL(ACTION_SLICE_LEN) = UNIQUE_FINAL_STATE;
      633     1527    6             END;
      634     1528    5         END;
      635     1529    4     END;
      636     1530
      637     1531    3     CALL INSERT_SLICE_IN_ARRAY ( I,
      638     1532    3                                  DEFAULT,
      639     1533    3                                  ACTION_SLICE,
      640     1534    3                                  ACTION_SLICE_LEN,
      641     1535    3                                  STATE,
      642     1536    3                                  NR_TERM);
      643     1537    3   END;
      644     1538
      645     1539    2   CALL WRITE_OUTPUT_STATES;
      646     1540
      647     1541    2   END OUTPUT_STATES;
