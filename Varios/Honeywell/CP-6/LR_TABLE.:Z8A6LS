      700     1594
      701     1595    4     IF NT_SHIFT_LIST.NEW_STATE(I) > 0 THEN
      702     1596    5       DO;
      703     1597    5         STATE_COUNT(NT_SHIFT_LIST.NEW_STATE(I)) = STATE_COUNT(NT_SHIFT_LIST.NEW_STATE(
              1597                  I)) + 1;
      704     1598    5         IF (STATE_COUNT (NT_SHIFT_LIST.NEW_STATE(I)) > MOST_COMMON_COUNT)
      705     1599    5             OR ((STATE_COUNT (NT_SHIFT_LIST.NEW_STATE(I)) = MOST_COMMON_COUNT)
      706     1600    5                    AND  (NT_SHIFT_LIST.NEW_STATE(I) > MOST_COMMON_STATE)) THEN
