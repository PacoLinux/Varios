        9        9        /*P*
       10       10             NAME : Z8DQ$QUALIFY_DEPENDING
       11       11             PURPOSE : To qualify and process the OCCURS DEPENDING guys
       12       12               since they need not be defined prior to their reference
       13       13               in the OCCURS CLAUSE.
       14       14        */
       15       15
       16       16        %EQU PHASE = 1;
       17       17
       18       18        %INCLUDE Z8A_DEFC;
       19       86        %INCLUDE Z8A_SUPP_MAC;
       20     2486        %INCLUDE Z8A_COMCOM;
       21     3136        %INCLUDE Z8A_ERR_NUMS;
       22     3900        %INCLUDE Z8A_ST;
       23     7559        %INCLUDE Z8A_ETT;
