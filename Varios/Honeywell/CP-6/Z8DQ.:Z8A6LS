       42    14411    1   DCL DEPENDING_LIST$           PTR;
       43    14412    1   DCL DEPEND$                   PTR;
       44    14413    1   DCL QUAL_ST$                  PTR;
       45    14414    1   DCL QUAL_ETT$                 PTR;
       46    14415
       47    14416        %DCL_AUTO_LX_ITEM (@NAME = DEPEND_GUY);
