PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:3    
       53    14425    1   DEPENDING_CHECKS: PROC (DEPEND$, DEPEND_ETT$, ASSOC_VAR_ST$) ALTRET;
       54    14426
       55    14427    2   DCL DEPEND_ETT$        PTR;
       56    14428    2   DCL DEPEND_ST$         PTR;
       57    14429    2   DCL ASSOC_VAR_ST$      PTR;
       58    14430    2   DCL DEPEND$            PTR;
       59    14431    2   DCL FATHER$            PTR;
       60    14432    2   DCL OCCURS$            PTR;
       61    14433    2   DCL LINE#              SBIN WORD;
       62    14434    2   DCL COLUMN#            SBIN WORD;
       63    14435
       64    14436    2      COLUMN# = DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ ->
       65    14437    2                LX_LIST.HEAD$ -> LX_ITEM.COLUMN;
       66    14438    2      LINE#   = DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ ->
       67    14439    2                LX_LIST.HEAD$ -> LX_ITEM.LINE;
       68    14440           /*
       69    14441           IF DEPEND_ST$ -> SCALAR.B.GLOBAL
       70    14442           OR DEPEND_ST$ -> SCALAR.B.EXTERNAL THEN
       71    14443             CALL Z8AE$ERROR (%E$DEPEND_ATTR_NOT_MATCH_OCCURS);
       72    14444           */
       73    14445
       74    14446           %FISH (TRIAD = DEPEND_ETT$,
       75    14447                  RTN   = DEPEND_ST$);
       76    14455
       77    14456    2      FATHER$ = ASSOC_VAR_ST$ -> ARRAY.QLT$;
       78    14457
       79    14458    3      DO WHILE (FATHER$ -> QLT.FATHER ~= ADDR(NIL));
       80    14459    4        DO WHILE (FATHER$ -> QLT.BROTHER ~= ADDR(NIL));
       81    14460    4          FATHER$ = FATHER$ -> QLT.BROTHER;
       82    14461    4          IF (NOT FATHER$ -> QLT.B.RENAMES)
       83    14462    4          AND (NOT FATHER$ -> QLT.ST$ -> SCALAR.B.REDEF_SUB) THEN
       84    14463    5            DO;
       85    14464    5              CALL Z8AE$ERROR (%E$OCC_DEP_MUST_BE_LAST,
       86    14465    5                               DEPEND$ -> DEFERED_QUAL_ID.LINE,
       87    14466    5                               DEPEND$ -> DEFERED_QUAL_ID.COLUMN);
       88    14467    5              ALTRETURN;
       89    14468    5            END;
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:4    
       90    14469    4        END; /* WHILE FATHER$ -> QLT.BROTHER ~= ADDR(NIL) */
       91    14470
       92    14471    3        IF FATHER$ -> QLT.FATHER ~= ADDR(NIL) THEN
       93    14472    3          IF FATHER$ -> QLT.FATHER -> QLT.ST$ -> ST.ID = %ARRAY THEN
       94    14473    4            DO;
       95    14474    4              CALL Z8AE$ERROR (%E$OCCURS_DEPEND_UNDER_OCCURS,
       96    14475    4                               DEPEND$ -> DEFERED_QUAL_ID.LINE,
       97    14476    4                               DEPEND$ -> DEFERED_QUAL_ID.COLUMN);
       98    14477    4              ALTRETURN;
       99    14478    4            END;
      100    14479
      101    14480    3        FATHER$ = FATHER$ -> QLT.FATHER;
      102    14481    2      END; /* While */;
      103    14482
      104    14483    2      FATHER$ = DEPEND_ST$ -> SCALAR.QLT$ -> QLT.FATHER;
      105    14484
      106    14485    3      DO WHILE (FATHER$ ~= ADDR(NIL));
      107    14486    3        IF FATHER$ = ASSOC_VAR_ST$ -> ARRAY.QLT$ THEN
      108    14487    4          DO;
      109    14488    4            CALL Z8AE$ERROR (%E$DEPEND_IS_UNDER_OCCURS,
      110    14489    4                             LINE#,
      111    14490    4                             COLUMN#,
      112    14491    4                             DEPEND_ST$ -> SCALAR.DNT$);
      113    14492    4            ALTRETURN;
      114    14493    4          END;
      115    14494
      116    14495    3        FATHER$ = FATHER$ -> QLT.FATHER;
      117    14496
      118    14497    3      END;
      119    14498
      120    14499          /* If the array has attribute GLOBAL or EXTERNAL, then the  */
      121    14500          /* DEPENDING ON ID must have this attribute too.            */
      122    14501
      123    14502    2      OCCURS$ = ASSOC_VAR_ST$ -> ARRAY.QLT$;
      124    14503
      125    14504    3      DO WHILE (OCCURS$ -> QLT.FATHER ~= ADDR(NIL));
      126    14505    3        OCCURS$ = OCCURS$ -> QLT.FATHER;
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:5    
      127    14506    2      END; /* While */;
      128    14507
      129    14508    2      IF OCCURS$ -> QLT.ST$ -> ARRAY.B.GLOBAL THEN
      130    14509    3        DO;
      131    14510    3          FATHER$ = DEPEND_ST$ -> SCALAR.QLT$;
      132    14511
      133    14512    4          DO WHILE (FATHER$ ~= ADDR(NIL) AND FATHER$ -> QLT.FATHER ~= ADDR(NIL));
      134    14513    4            FATHER$ = FATHER$ -> QLT.FATHER;
      135    14514    4          END; /* While */
      136    14515
      137    14516    3          IF FATHER$ = ADDR(NIL)  OR
      138    14517    3             NOT FATHER$ -> QLT.ST$ -> VARIABLE.B.GLOBAL THEN
      139    14518    4            DO;
      140    14519                   %CONDITIONAL_ERROR (@LX_ITEM$ =
             14519                       DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ -> LX_LIST.HEAD$,
      141    14520                                       @ERROR#   = %E$DEPEND_ATTR_NOT_MATCH_OCCURS,
      142    14521                                       @LINE     = LINE#,
      143    14522                                       @COLUMN   = COLUMN#,
      144    14523                                       @PLUG1    = DEPEND_ST$ -> SCALAR.DNT$);
      145    14553    4              ALTRETURN;
      146    14554    4            END;
      147    14555    3        END;
      148    14556
      149    14557    2      IF OCCURS$ -> QLT.ST$ -> ARRAY.B.EXTERNAL THEN
      150    14558    3        DO;
      151    14559    3          FATHER$ = DEPEND_ST$ -> SCALAR.QLT$;
      152    14560
      153    14561    4          DO WHILE (FATHER$ ~= ADDR(NIL) AND FATHER$ -> QLT.FATHER ~= ADDR(NIL));
      154    14562    4            FATHER$ = FATHER$ -> QLT.FATHER;
      155    14563    4          END; /* While */
      156    14564
      157    14565    3          IF FATHER$ = ADDR(NIL)  OR
      158    14566    3             NOT FATHER$ -> QLT.ST$ -> VARIABLE.B.EXTERNAL THEN
      159    14567    4            DO;
      160    14568                   %CONDITIONAL_ERROR (@LX_ITEM$ =
             14568                       DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ -> LX_LIST.HEAD$,
      161    14569                                       @ERROR#   = %E$DEPEND_ATTR_NOT_MATCH_OCCURS,
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:6    
      162    14570                                       @LINE     = LINE#,
      163    14571                                       @COLUMN   = COLUMN#,
      164    14572                                       @PLUG1    = DEPEND_ST$ -> SCALAR.DNT$);
      165    14602    4              ALTRETURN;
      166    14603    4            END;
      167    14604    3        END;
      168    14605
      169    14606    2   END DEPENDING_CHECKS;
      170    14607        %EJECT;
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:7    
      171    14608    1   PROCESS_DEPENDING: PROC (DEPEND_ST$, ASS_VAR_ST$);
      172    14609
      173    14610    2   DCL DEPEND_ST$                PTR;
      174    14611    2   DCL ASS_VAR_ST$               PTR;
      175    14612
      176    14613    2   DCL DIMS$                     PTR;
      177    14614
      178    14615           %GET_LTS (@ID     = %DIMS,
      179    14616                     @ST$    = ASS_VAR_ST$,
      180    14617                     @RETURN$= DIMS$);
      181    14660
      182    14661    2      DIMS$ -> DIMS.DN2 = DEPEND_ST$;
      183    14662
      184    14663    2   END PROCESS_DEPENDING;
      185    14664        %EJECT;
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:8    
      186    14665    1   CLEAR_EVIDENCE: PROC (ST$$);
      187    14666
      188    14667    2        DCL ST$$          PTR;
      189    14668    2        DCL FATHER$       PTR;
      190    14669
      191    14670
      192    14671    2        FATHER$ = ST$$ -> SCALAR.QLT$;
      193    14672
      194    14673    3        DO WHILE (FATHER$ ~= ADDR(NIL));
      195    14674    3          FATHER$ -> QLT.ST$ -> SCALAR.B.OCCURS_DEPENDING = FALSE;
      196    14675    3          FATHER$ = FATHER$ -> QLT.FATHER;
      197    14676    3        END;
      198    14677
      199    14678    2   END CLEAR_EVIDENCE;
      200    14679        %EJECT;
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:9    
      201    14680
      202    14681    1      DEPEND$ = DEPENDING_LIST$;
      203    14682
      204    14683    2      DO WHILE (DEPEND$ ~= ADDR(NIL));
      205    14684    2        CALL Z8CO$QUALIFY_VARIABLE (DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$,
      206    14685    2                                    QUAL_ST$);
      207    14686
      208    14687    2        LINE# = DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ -> LX_LIST.HEAD$ ->
      209    14688    2                LX_ITEM.LINE;
      210    14689    2        COLUMN# = DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ -> LX_LIST.HEAD$ ->
      211    14690    2                  LX_ITEM.COLUMN;
      212    14691
      213    14692    2        IF QUAL_ST$ -> VARIABLE.B.OCCURS_DEPENDING THEN
      214    14693    3          DO;
      215    14694                 %GET_LTS (@ID  = %DEPENDING_SIZE,
      216    14695                           @ST$ = QUAL_ST$);
      217    14738    3            IF LTS$ = ADDR(NIL) THEN
      218    14739    4              DO; /* Compiler already has check but this would cause abort
      219    14740                          before compilation gets that far */
      220    14741                     %CONDITIONAL_ERROR (@LX_ITEM$ =
      221    14742                                   DEPEND$ -> DEFERED_QUAL_ID.QUAL_ID_LIST$ -> LX_LIST.HEAD$
             14742                                             ,
      222    14743                                         @ERROR#   = %E$MUST_BE_INTEGER,
      223    14744                                         @LINE     = LINE#,
      224    14745                                         @COLUMN   = COLUMN#);
      225    14775    4                GOTO ERROR;
      226    14776    4              END;
      227    14777    3          END;
      228    14778
      229    14779    2        CALL Z8AF$NEW_BASE (QUAL_ST$, ADDR(NIL), QUAL_ETT$);
      230    14780
      231    14781             %INIT_LX_ETT_ITEM (@POINTER = "ADDR(DEPEND_GUY)",
      232    14782                                @TYPE    = QUAL_ST$ -> VARIABLE.TYPE,
      233    14783                                @ETT$    = QUAL_ETT$,
      234    14784                                @LINE    = LINE#,
      235    14785                                @COLUMN  = COLUMN#);
      236    14807
PL6.E3A0      #001=Z8DQ$QUALIFY_DEPENDING File=Z8DQ.:Z8A6TSI                     SAT 08/09/97 11:17 Page:10   
      237    14808    2        CALL Z8CO$INTEGER_CHECK (ADDR(DEPEND_GUY)) ALTRET (ERROR);
      238    14809    2        CALL Z8EV$CONVERT (%TYI, ADDR(DEPEND_GUY), ADDR(DEPEND_GUY));
      239    14810
      240    14811    2        QUAL_ETT$ = ADDR(DEPEND_GUY) -> LX_ETT_ITEM.ETT$;
      241    14812
      242    14813    2        CALL DEPENDING_CHECKS (DEPEND$, QUAL_ETT$,
      243    14814    2                               DEPEND$ -> DEFERED_QUAL_ID.ASSOC_VAR_ST$)
      244    14815    2                               ALTRET (ERROR);
      245    14816
      246    14817    2        CALL PROCESS_DEPENDING (QUAL_ETT$,
      247    14818    2                                DEPEND$ -> DEFERED_QUAL_ID.ASSOC_VAR_ST$);
      248    14819
      249    14820    2        GOTO NO_ERROR;
      250    14821
      251    14822    2   ERROR:
      252    14823    2        CALL CLEAR_EVIDENCE (DEPEND$ -> DEFERED_QUAL_ID.ASSOC_VAR_ST$);
      253    14824
      254    14825    2   NO_ERROR:
      255    14826
      256    14827
      257    14828    2        DEPEND$ = DEPEND$ -> DEFERED_QUAL_ID.LINK;
      258    14829    2      END; /* While depend ~= addr(nil) */
      259    14830
      260    14831    1      CALL Z8ED$DISPOSE_ABSSYN_TREE (O1COM.OCCURS_DEPENDING$);
      261    14832
      262    14833    1      RETURN;
      263    14834
      264    14835    1   END Z8DQ$QUALIFY_DEPENDING;
      265    14836        %EOD;

