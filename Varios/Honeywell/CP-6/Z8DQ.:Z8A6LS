PL6.E3A0      #002=Z8DQ$QUALIFY_KEYS File=Z8DQ.:Z8A6TSI                          SAT 08/09/97 11:19 Page:53   
      345    16015    1   CHECK_PLACEMENT: PROC (ASSOC_VAR$, KEY$, KEY_ST$) ALTRET;
      346    16016
      347    16017    2   DCL ASSOC_VAR$                PTR;
      348    16018    2   DCL KEY$                      PTR;
      349    16019    2   DCL KEY_ST$                   PTR;
      350    16020
      351    16021    2   DCL FATHER$                   PTR;
      352    16022    2   DCL KEY_DIM                   SBIN WORD;
      353    16023    2   DCL KEY_DIM$                  PTR;
      354    16024    2   DCL ARRAY_DIM                 SBIN WORD;
      355    16025    2   DCL ARRAY_DIM$                PTR;
      356    16026
      357    16027    2      FATHER$ = KEY_ST$ -> VARIABLE.QLT$ -> QLT.FATHER;
      358    16028
      359    16029    3      DO WHILE (FATHER$ ~= ADDR(NIL));
      360    16030    3        IF FATHER$ = ASSOC_VAR$ -> VARIABLE.QLT$ THEN
      361    16031    3          EXIT;
      362    16032    3        ELSE
      363    16033    3          FATHER$ = FATHER$ -> QLT.FATHER;
      364    16034    3      END;
      365    16035
      366    16036    2      IF FATHER$ = ADDR(NIL) THEN
      367    16037    3        DO;
      368    16038               %CONDITIONAL_ERROR (@LX_ITEM$ = KEY$ -> LX_LIST.HEAD$,
      369    16039                     @ERROR#  = %E$KEY_MUST_BE_IN_GROUP,
      370    16040                     @LINE    = KEY$ -> LX_LIST.HEAD$ -> LX_ITEM.LINE,
      371    16041                     @COLUMN  = KEY$ -> LX_LIST.HEAD$ -> LX_ITEM.COLUMN);
      372    16071    3          ALTRETURN;
      373    16072    3        END;
      374    16073
      375    16074    2      CALL Z8DQ$CHECK_FOR_OCCURS (ASSOC_VAR$ -> ARRAY.QLT$ -> QLT.SON,
      376    16075    2                                  KEY_ST$ -> VARIABLE.QLT$);
      377    16076
      378    16077           %GET_LTS (@ID       = %DIMS,
      379    16078                     @ST$      = KEY_ST$,
      380    16079                     @RETURN$  = KEY_DIM$);
      381    16122
PL6.E3A0      #002=Z8DQ$QUALIFY_KEYS File=Z8DQ.:Z8A6TSI                          SAT 08/09/97 11:19 Page:54   
      382    16123           %GET_LTS (@ID       = %DIMS,
      383    16124                     @ST$      = ASSOC_VAR$,
      384    16125                     @RETURN$  = ARRAY_DIM$);
      385    16168
      386    16169           %FIND_LAST_DIM (POINTER = KEY_DIM$,
      387    16170                           VARIABLE = KEY_DIM);
      388    16175
      389    16176           %FIND_LAST_DIM (POINTER  = ARRAY_DIM$,
      390    16177                           VARIABLE = ARRAY_DIM);
      391    16182
      392    16183    2      IF KEY_DIM ~= ARRAY_DIM THEN
      393    16184    3        DO;
      394    16185               /* The dimensions are not equal therefore an extra
      395    16186                  array is above this one -- error */
      396    16187    3          CALL Z8AE$ERROR (%E$KEY_IS_ARRAY,
      397    16188    3                           KEY$ -> LX_LIST.HEAD$ -> LX_ITEM.LINE,
      398    16189    3                           KEY$ -> LX_LIST.HEAD$ -> LX_ITEM.COLUMN);
      399    16190    3          ALTRETURN;
      400    16191    3        END;
      401    16192
