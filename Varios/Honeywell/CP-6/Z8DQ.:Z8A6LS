PL6.E3A0      #003=Z8DQ$CHECK_FOR_OCCURS File=Z8DQ.:Z8A6TSI                      SAT 08/09/97 11:21 Page:91   
      466        1        /*T***********************************************************/
      467        2        /*T*                                                         */
      468        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      469        4        /*T*                                                         */
      470        5        /*T***********************************************************/
      471        6        Z8DQ$CHECK_FOR_OCCURS: PROC (IN_QLT$, TERMINATION_QLT$) ALTRET;
      472        7
      473        8        /*P*
      474        9             NAME : Z8DQ$CHECK_FOR_OCCURS
      475       10             PURPOSE : To check a range of QLT's for an occurs clause.
      476       11        */
      477       12
      478       13
      479       14        %EQU PHASE = 1;
      480       15
      481       16        %INCLUDE Z8A_DEFC;
      482       84        %INCLUDE Z8A_SUPP_MAC;
      483     2484        %INCLUDE Z8A_COMCOM;
      484     3134        %INCLUDE Z8A_ST;
      485     6793        %INCLUDE Z8A_QLT;
      486     6899        %INCLUDE Z8A_ERR_NUMS;
      487     7663
      488     7664    1   DCL Z8AE$ERROR         ENTRY (12);
      489     7665
      490     7666    1   DCL IN_QLT$            PTR;
      491     7667    1   DCL TERMINATION_QLT$   PTR;
      492     7668
      493     7669    1   DCL CURRENT_QLT$       PTR;
      494     7670
      495     7671        %EJECT;
PL6.E3A0      #003=Z8DQ$CHECK_FOR_OCCURS File=Z8DQ.:Z8A6TSI                      SAT 08/09/97 11:21 Page:92   
      496     7672    1      CURRENT_QLT$ = IN_QLT$;
      497     7673
      498     7674    2      DO WHILE (CURRENT_QLT$ ~= ADDR(NIL));
      499     7675    2        IF CURRENT_QLT$ = TERMINATION_QLT$ THEN
      500     7676    2          ALTRETURN;
      501     7677
      502     7678    2        IF CURRENT_QLT$ -> QLT.B.OCCURS THEN
      503     7679    3          DO;
      504     7680    3            CALL Z8AE$ERROR (%E$OCCURS_BETWEEN_KEY_AND_ARRAY,
      505     7681    3                             BINBIT( TERMINATION_QLT$ -> QLT.ST$ ->
      506     7682    3                                     VARIABLE.LINE_NO, 36),
      507     7683    3                             0);
      508     7684    3            ALTRETURN;
      509     7685    3          END;
      510     7686
      511     7687    2        IF CURRENT_QLT$ -> QLT.SON ~= ADDR(NIL) THEN
      512     7688    2          CALL Z8DQ$CHECK_FOR_OCCURS (CURRENT_QLT$ -> QLT.SON,
      513     7689    2                                      TERMINATION_QLT$)
      514     7690    2            WHENALTRETURN
      515     7691    3              DO;
      516     7692    3                ALTRETURN;
      517     7693    3              END;
      518     7694
      519     7695    2        CURRENT_QLT$ = CURRENT_QLT$ -> QLT.BROTHER;
      520     7696    2      END; /* While */
      521     7697
      522     7698    1   END Z8DQ$CHECK_FOR_OCCURS;
      523     7699        %EOD;

