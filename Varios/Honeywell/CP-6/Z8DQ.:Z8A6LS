      311    15791    1   PROCESS_FIRST_KEY: PROC (ASSOC_VAR$, KEY$, LAST_KEY$);
      312    15792
      313    15793    2   DCL ASSOC_VAR$         PTR;
      314    15794    2   DCL LAST_KEY$          PTR;
      315    15795    2   DCL KEY$               PTR;
      316    15796
      317    15797    2   DCL KEY_ST$            PTR;
      318    15798
      319    15799    2      CALL Z8CO$QUALIFY_VARIABLE (KEY$ -> LX_LIST.TAIL$ -> LX_LIST.HEAD$,
      320    15800    2                                  KEY_ST$);
      321    15801
      322    15802    2      IF KEY_ST$ ~= ASSOC_VAR$ THEN
      323    15803    2        CALL CHECK_PLACEMENT (ASSOC_VAR$, KEY$, KEY_ST$)
      324    15804    2          WHENRETURN
      325    15805    3            DO;
      326    15806                   %NEW_KEY (@POINTER = LAST_KEY$,
      327    15807                             @ASCENDING   =
             15807                            "(KEY$ -> LX_LIST.HEAD$ -> LX_ITEM.SYMBOL = %RSV_WRD_ASCENDING)"
             15807                                 ,
      328    15808                             @KEY$    = KEY_ST$);
      329    15904
      330    15905    3              CALL Z8DO$LINK_WITH_LTS (ASSOC_VAR$ -> VARIABLE.LTS$,
      331    15906    3                                       LAST_KEY$);
      332    15907    3            END;
      333    15908    2      ELSE
      334    15909    3        DO;
      335    15910               %NEW_KEY (@POINTER = LAST_KEY$,
      336    15911                         @ASCENDING =
             15911                            "(KEY$ -> LX_LIST.HEAD$ -> LX_ITEM.SYMBOL = %RSV_WRD_ASCENDING)"
             15911                             ,
      337    15912                         @KEY$    = KEY_ST$);
      338    16008
      339    16009    3          CALL Z8DO$LINK_WITH_LTS (ASSOC_VAR$ -> VARIABLE.LTS$,
      340    16010    3                                   LAST_KEY$);
      341    16011    3        END;
      342    16012
      343    16013    2   END PROCESS_FIRST_KEY;
PL6.E3A0      #002=Z8DQ$QUALIFY_KEYS File=Z8DQ.:Z8A6TSI                          SAT 08/09/97 11:19 Page:52   
