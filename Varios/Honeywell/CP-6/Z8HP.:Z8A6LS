       54    000010 ADI   START    PEPD                  {LOP + ROP}
       55    000010        WALK2                   {WALK  EXP FIRST, THEN REFS}
       56    000012
       57    000012 $ADI00 NULL
       58    000012        DSTATE  TYI17,LOPROP,XABS
       59    000014        JMPDV   XABS,16
       60    000016        JMP     $ADIAA          {OPTIMIZED ALIAS}
       61    000017        JMP     $ADIAQ          {LOP=ARA,ROP=ARQ}
       62    000020        JMP     $ADIAX          {LOP=ARA,ROP=XRn}
       63    000021        JMP     $ADIAS          {LOP=ARA,ROP=ST}
       64    000022        JMP     $ADIQA          {LOP=ARQ,ROP=ARA}
       65    000023        JMP     $ADIQQ          {OPTIMIZED ALIAS}
       66    000024        JMP     $ADIQX          {LOP=ARQ,ROP=XRn}
       67    000025        JMP     $ADIQS          {LOP=ARQ,ROP=ST}
       68    000026        JMP     $ADIXA          {LOP=XRn,ROP=ARA}
