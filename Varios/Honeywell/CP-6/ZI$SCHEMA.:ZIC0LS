%MACRO SAS;
DCL 1 SAS BASED UNAL,
      2 RCW UBIN(36) UNAL,
/*  Set pointers    */
      2 CALC,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 AS_AL,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 AS_PG,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 AS_SI,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SF_AS,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
/*  Data fields  */
      2 SS_NAME CHAR(30) UNAL,
      2 SS_TRAN CHAR(12) UNAL,
      2 SS_TRAN_DBCS CHAR(8) UNAL,
      2 * BIT(1) UNAL,
      2 SS_LANG BIT(4) UNAL,
      2 * BIT(4) UNAL;
%MEND;

