%MACRO SSN;
DCL 1 SSN BASED UNAL,
      2 RCW BIT(36) UNAL,
      2 DA_SN,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
      2 SNNR_PT,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_LT,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_NRCO,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_NRWU,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_SE,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_SI,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_TX,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
/*         Data fields       */
      2 * BIT(1) UNAL,
      2 DCLR UBIN(4) UNAL,
      2 ESCAPE UBIN(4) UNAL,
      2 SETYPE UBIN(18) UNAL,
      2 LNO UBIN(23) UNAL,
      2 * BIT(4) UNAL,
      2 ACTLCKS SBIN(36) UNAL,
      2 ARNM CHAR(30) UNAL;
%MEND;

