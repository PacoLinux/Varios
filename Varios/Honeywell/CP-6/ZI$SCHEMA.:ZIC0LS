%MACRO SLT;
DCL 1 SLT BASED UNAL,
                                        /*    Local pointers                  */
      2 RCW BIT(36) UNAL,
      2 LK_LT,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
      2 SN_LT,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
                                        /*    Data fields                     */
      2 SUBENTRIES UBIN(18) UNAL,
      2 * BIT(9) UNAL,
      2 PLOCKS BIT(9) UNAL,
      2 * BIT(18) UNAL,
      2 LNO BIT(23) UNAL,
      2 * BIT(4) UNAL;
%MEND;

