FRI. SEP 05 '97 11:18:50.92  FILE=ZI$SCHEMA

%MACRO SAC;
DCL 1 SAC BASED UNAL,
      2 RCW BIT(36) UNAL,
      2 AC_CA,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 AC_RA,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 AO_AC,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
      2 SN_SE,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 FORG UBIN(4) UNAL,            /*0=INTEGRATED                        */
                                        /*1=INTEGRATED with keyed file*/
                                        /*2=INDEXED                           */
                                        /*3=INDEXED with keyed file           */
        3 ORG REDEF FORG,
          4 * UBIN (2) UNAL,
          4 INDEXED BIT(1) UNAL,
          4 KEYED BIT(1) UNAL,
        3 NCIA BIT(1) UNAL,
        3 PAIA BIT(1) UNAL,
        3 ITHR UBIN(9) UNAL,
        3 PMOD BIT(2) UNAL,
        3 ATMP BIT(1) UNAL,
        3 ANL UBIN(6) UNAL,
      2 FILE_CODE CHAR(2) UNAL,
      2 INDEX_CD CHAR(2) UNAL,
      2 * BIT(1) UNAL,
      2 PRIME_KEY BIT(4) UNAL,
      2 SEC_KEY BIT(4) UNAL,
      2 * BIT(1) UNAL,
      2 PERMTYPE BIT(4) UNAL,
      2 PAGE_TYPE BIT(4) UNAL,
      2 PER_INV UBIN(9) UNAL,
      2 * BIT(1) UNAL,
      2 CALC_NULL BIT(4) UNAL,
      2 * BIT(4) UNAL,
      2 BASE_DBK SBIN(36) UNAL,
      2 KEYS_ALLOC SBIN(36) UNAL,
      2 KEYS_RSRVD SBIN(36) UNAL,
      2 MIN_DBK SBIN(36) UNAL,
      2 MAX_DBK SBIN(36) UNAL,
      2 MIN_PAGE SBIN(36) UNAL,
      2 MAX_PAGE SBIN(36) UNAL,
      2 CALC_HDRS SBIN(36) UNAL,
      2 FRST_INV_PG SBIN(36) UNAL,
      2 DBK_PAGE SBIN(18) UNAL,
      2 DBK_CALC SBIN(36) UNAL,
      2 LINE_BITS SBIN(18) UNAL,
      2 INV_THRESH SBIN(18) UNAL,
      2 LOAD_LIMIT SBIN(18) UNAL,
      2 PGS_INV_PG SBIN(18) UNAL,
      2 INV_UNIT SBIN(18) UNAL,
      2 PAGE_SIZE SBIN(36) UNAL,
      2 SPACE_AVAIL SBIN(36) UNAL,
      2 SECTORS_PAGE SBIN(36) UNAL,
      2 CALC_NEXT SBIN(36) UNAL,
      2 CALC_PRIOR SBIN(36) UNAL;
%MEND;

