%MACRO SSI;
DCL 1 SSI BASED UNAL,
      2 RCW UBIN(36) UNAL,
/*  Set pointers */
      2 AS_SI,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
      2 SI_VB,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 SN_SI,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
/*  Data fields  */
      2 LOCKED BIT(18) UNAL,
      2 * BIT(18) UNAL,
      2 BINDCD,
        3 * UBIN(9) UNAL,
        3 TYPE UBIN(9) UNAL,
        3 CODE UBIN(18) UNAL,
      2 LI_NE BIT(23) UNAL,
      2 * BIT(8) UNAL,
      2 EXCLUDE BIT(1) UNAL,
      2 SETYPE BIT(18) UNAL,
      2 ALIAS BIT(4) UNAL;
%MEND;

