%MACRO SLK;
DCL 1 SLK BASED UNAL,
      2 RCW UBIN(36) UNAL,
                                        /*    Set pointers                    */
      2 CALC,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
      2 DA_LK,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
        3 HEAD UBIN(18) UNAL,
      2 LK_LT,
        3 NEXT UBIN(18) UNAL,
        3 PRIOR UBIN(18) UNAL,
                                        /*    Data fields                     */
      2 PLNAME CHAR(30) UNAL,
      2 PLVALUE CHAR(12) UNAL;
%MEND;

