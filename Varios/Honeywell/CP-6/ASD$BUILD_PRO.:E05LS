      423    20122        /********************************************************************/
      424    20123        /*   Auto data       auto data       auto data       auto data       */
      425    20124        /*********************************************************************/
      426    20125
      427    20126    1   DCL ARS UBIN WORD ;
      428    20127    1   DCL IOCNT UBIN WORD ;
      429    20128    1   DCL EXTWID UBIN WORD ;
      430    20129        %EJECT;
PL6.E3A0      #001=ASD$BUILD_PRO File=ASD$BUILD_PRO.:E05TSI                      TUE 07/29/97 09:44 Page:29   
      431    20130        /********************************************************************/
      432    20131        /*   Procedure       procedure       procedure       procedure       */
      433    20132        /*********************************************************************/
      434    20133
      435    20134        /* In pre C01 times the blocks that made up a profile were all an even
      436    20135           number of bytes, thus assuring that the next block would have the
      437    20136           type byte on an even boundary.  The only part of the profile that
      438    20137           could vary in size was the cchr part at the end, so nothing was
      439    20138           affected.
      440    20139
      441    20140           Now in C01 there are 3 profile blocks that can vary in size (cchr,
      442    20141           functionkey, and SETUP_CHR).  When the funkey or setup blocks get
      443    20142           altered in size, we must make sure the number of bytes added or
      444    20143           removed is even.  The cchr is still at the end....  The solution is
      445    20144           simply to make sure the setup_chr is an even number of characters,
      446    20145           the funkey stuff uses an even number of bytes for each definition,
      447    20146           the record is padded with a byte if necessary.
      448    20147        */
      449    20148
      450    20149    1   ASD$CREATE_PRO: ENTRY;
      451    20150
      452    20151        /*
      453    20152            ***            Begin procedure                    ***
      454    20153        */
      455    20154    1   CALL ASD$OPEN_PRO_CR;
      456    20155    1   CALL ASD$BUILD_$ASYNC( PROFILE_BUF, ARS );
      457    20156    1   CALL ASD$BUILD_$RBT( PROFILE_BUF, ARS );
      458    20157    1   CALL ASD$BUILD_$RBTD( PROFILE_BUF, ARS );
      459    20158    1   CALL ASD$BUILD_$LINK( PROFILE_BUF, ARS );
      460    20159    1   CALL ASD$BUILD_$URP( PROFILE_BUF, ARS );
      461    20160    1   CALL ASD$BUILD_$3270( PROFILE_BUF, ARS );
      462    20161    1   CALL ASD$BUILD_$VIRCIR( PROFILE_BUF, ARS );
      463    20162    1   CALL ASD$BUILD_$POLLVIP( PROFILE_BUF, ARS );
      464    20163    1   CALL ASD$BUILD_$GRA( PROFILE_BUF, ARS );
      465    20164    1   CALL ASD$CLOSE_PRO ;
      466    20165    1   RETURN;
      467    20166
PL6.E3A0      #001=ASD$BUILD_PRO File=ASD$BUILD_PRO.:E05TSI                      TUE 07/29/97 09:44 Page:30   
      468    20167        %SET LISTMAC='1'B;
      469    20168        %SET LISTSUB='1'B;
      470    20169
      471    20170
      472    20171    1   ASD$BUILD_$ASYNC: ENTRY( PRO_BUF, P_ARS );
      473    20172
      474    20173
      475    20174                                                              /* Set up $ASYNC. */
      476    20175    1   PRO_BUF = AS_BLANK ;
      477    20176    1   PP$ = ADDR( PRO_BUF ) ;
      478    20177                                                              /*   Set up header.   */
      479    20178    1   PRO_HDR.TTYP = %KLTY_TTY# ;
      480    20179                                                            /*   Check for extra width.   */
      481    20180    1   IF VLP_ASYNC_PLATEN.EXTWID# = %YES# THEN
      482    20181    1      EXTWID = 1 ;
      483    20182    1   ELSE
      484    20183    1      EXTWID = 0 ;
      485    20184    1   PRO_HDR.WIDTH = VLP_ASYNC_PLATEN.WIDTH# + ( 254*EXTWID ) ;
      486    20185    1   PRO_HDR.LINES = VLP_ASYNC_PLATEN.LENGTH# ;
      487    20186    1   PRO_HDR.ATTR.GRLGL = %NO# ;
      488    20187    1   PRO_HDR.ATTR.TRANSLGL = %NO# ;
      489    20188    1   PRO_HDR.ATTR.NOXLATE = %NO# ;
      490    20189    1   PRO_HDR.ATTR.UPPERCASE = %YES#;
      491    20190    1   PRO_HDR.ATTR.NATL = %NO# ;
      492    20191    1   PRO_HDR.ATTR.TRUOVRPRT = VLP_ASYNC_TRMATTR.TRUOVRPRT# ;
      493    20192    1   PRO_HDR.ATTR.APL = VLP_ASYNC_TRMATTR.APL# ;
      494    20193    1   PRO_HDR.ATTR.BIN = %NO# ;
      495    20194    1   PRO_HDR.ATTR.LCASE = VLP_ASYNC_TRMATTR.LOWERCASEPRINTS# ;
      496    20195    1   PRO_HDR.IO.INPUT = %YES# ;
      497    20196    1   PRO_HDR.IO.OUTPUT = %YES# ;
      498    20197    1   PRO_HDR.DDEV.LP = %YES# ;
      499    20198    1   PRO_HDR.DDEV.CP = %NO# ;
      500    20199    1   PRO_HDR.DDEV.IC = %YES# ;
      501    20200    1   PRO_HDR.DDEV.GRAPHICS_OK = %NO#;
      502    20201                                                              /*   Mover header.   */
      503    20202    1   PP$ -> PRO$HDR = PRO_HDR ;
      504    20203    1   PP$ = PINCRW( PP$, SIZEW( PRO_HDR ) ) ;
PL6.E3A0      #001=ASD$BUILD_PRO File=ASD$BUILD_PRO.:E05TSI                      TUE 07/29/97 09:44 Page:31   
