      403    19641
      404    19642        %LIST ;
      405    19643        %KL_PHDR ( NAME = PRO$HDR  ,
      406    19644                   STCLASS = BASED ) ;
             19645         %LSET LISTDIR='0'B;
             19657    1   DCL 1 PRO$HDR BASED,
             19658    1         2 TTYP UBIN BYTE UNAL /*(2)/**/,
             19659    1         2 WIDTH UBIN BYTE UNAL /*(80)/**/,
             19660    1         2 LINES UBIN HALF UNAL /*(38)/**/,
             19661    1         2 ATTR,
             19662    1           3 * BIT(9) UNAL /*('0'B)/**/,
             19663    1           3 GRLGL BIT(1) UNAL /*('0'B)/**/,
             19664    1           3 TRANSLGL BIT(1) UNAL /*('0'B)/**/,
             19665    1           3 NOXLATE BIT(1) UNAL /*('0'B)/**/,
             19666    1           3 UPPERCASE BIT(1) UNAL /*('1'B)/**/,
             19667    1           3 NATL BIT(1) UNAL /*('0'B)/**/,
