      507    20206        B$VLP = VLP$PNAME ,
      508    20207        T_TYPE = %KV_PRMID_PROFILE ) ;
             20208        %LSET LISTDIR='0'B;

             20211    1   PP$ -> PP$STRCT.TYPE = 3 ;
