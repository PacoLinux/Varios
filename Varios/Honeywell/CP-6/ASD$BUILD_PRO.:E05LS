             19683    1         2 INFO BIT(72) /*('0'B)/**/;
      407    19685        %PLIST ;
      408    19686        %XG$PROFILE (PRON = GRA$REC,
PL6.E3A0      #001=ASD$BUILD_PRO File=ASD$BUILD_PRO.:E05TSI                      TUE 07/29/97 09:44 Page:27   
      409    19687                     STCLASS = BASED) ;
      410    20109    1   DCL 1 PP$STRCT BASED( PP$ ) UNAL ,
