PL6.E3A0      #005=Z8EV$CONVERT_TYA File=Z8EV.:Z8A6TSI                           SAT 08/09/97 12:58 Page:339  
     1262        1        /*T***********************************************************/
     1263        2        /*T*                                                         */
     1264        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
     1265        4        /*T*                                                         */
     1266        5        /*T***********************************************************/
     1267        6        Z8EV$CONVERT_TYA: PROC (FROM$, RETURN$, CONTEXT$) ;
     1268        7
     1269        8        /* This routine is called by Z8EA$INSPECT, Z8EZ$STRING, and
     1270        9           Z8EZ$UNSTRING to determine if a Kanji Space is required
     1271       10           instead of an Ascii Space.  If the identifier being inspected,
     1272       11           strung into, or unstrung is Kanji, then any use of the
     1273       12           figurative constant SPACE will be a Kanji space.
     1274       13        */
     1275       14
     1276       15    1   DCL    FROM$     PTR ; /* Lx_Item */
     1277       16    1   DCL    RETURN$   PTR ; /* Lx_Item */
     1278       17    1   DCL    CONTEXT$  PTR ; /* LX_Item */
     1279       18
     1280       19        %EQU PHASE = 1 ;
     1281       20        %INCLUDE Z8A_DEFC ;
     1282       88        %INCLUDE Z8A_SUPP_MAC ;
     1283     2488        %INCLUDE Z8E_STACKS ;
     1284     4754        %INCLUDE Z8A_ST ;
     1285     8413        %INCLUDE Z8A_DNT ;
     1286     8634        %INCLUDE Z8A_TYPES ;
     1287     8808        %INCLUDE Z8E_WINDOW ;
     1288    10878
     1289    10879
     1290    10880    1   DCL    Z8EV$CONVERT    ENTRY (3) ;
     1291    10881    1   DCL    Z8AH$SEARCH_    ENTRY ;
     1292    10882
     1293    10883    1   DCL    LINK$           PTR ;
     1294    10884
     1295    10885        /* If the context is Kanji, use Kanji Space */
     1296    10886
     1297    10887    1   LINK$ = FROM$ -> LX_ITEM.LINK ;
     1298    10888
PL6.E3A0      #005=Z8EV$CONVERT_TYA File=Z8EV.:Z8A6TSI                           SAT 08/09/97 12:58 Page:340  
     1299    10889    1   IF  CONTEXT$ -> LX_ITEM.TYPE = %TYN
     1300    10890    1   AND FROM$ -> LX_ITEM.ID = %LX_TERMINAL
     1301    10891    2   AND FROM$ -> LX_TERMINAL.SYMBOL = %RSV_WRD_SPACES THEN DO ;
     1302    10892
     1303    10893    2     FROM$ -> LX_TERMINAL.SYMBOL = %ALPHA_LITERAL ;
     1304    10894    2     SUBSTR (LEX_SYMBOL.SYMBOL, 0, 1) = '"' ;
     1305    10895    2     SUBSTR (LEX_SYMBOL.SYMBOL, 1, 1) = %KANJI_START ;
     1306    10896    2     SUBSTR (LEX_SYMBOL.SYMBOL, 2, 1) = %KANJI_SPACE ;
     1307    10897    2     SUBSTR (LEX_SYMBOL.SYMBOL, 3, 1) = %KANJI_SPACE ;
     1308    10898    2     SUBSTR (LEX_SYMBOL.SYMBOL, 4, 1) = %KANJI_END ;
     1309    10899    2     SUBSTR (LEX_SYMBOL.SYMBOL, 5, 1) = '"' ;
     1310    10900    2     LEX_SYMBOL.SIZE = 6 ;
     1311    10901    2     CALL Z8AH$SEARCH_ ; /* Input = LEX_SYMBOL, output = DNT$ */
     1312    10902    2     DNT$ -> DNT.B.ALL = '1'B ;
     1313    10903    2     FROM$ -> LX_TERMINAL.DNT$ = DNT$ ;
     1314    10904    2     CALL Z8EV$CONVERT (%TYN, FROM$, RETURN$) ;
     1315    10905
     1316    10906    2   END ; /* Kanji space */
     1317    10907
     1318    10908    2   ELSE DO ; /* Not Kanji Space */
     1319    10909
     1320    10910    2     IF CONTEXT$ -> LX_ITEM.TYPE = %TYN THEN
     1321    10911    2       CALL Z8EV$CONVERT (%TYN, FROM$, RETURN$) ;
     1322    10912    2     ELSE
     1323    10913    2       CALL Z8EV$CONVERT (%TYA, FROM$, RETURN$) ;
     1324    10914
     1325    10915    2   END ;
     1326    10916
     1327    10917    1   RETURN$ -> LX_ITEM.LINK = LINK$ ;
     1328    10918
     1329    10919    1   END Z8EV$CONVERT_TYA ;

