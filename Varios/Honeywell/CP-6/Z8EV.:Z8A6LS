       82    15134
       83    15135
       84    15136    2      CALL Z8AU$TYPE_DESCRIPTOR(ETT$$, TYPE_DESC);
       85    15137
       86    15138           %UNPACK_NDSC ( @DESC    = TYPE_DESC.DESC ,
       87    15139                          @TN      = TN ,
       88    15140                          @SCALE   = SCALE,
       89    15141                          @SIGN    = SIGN,
       90    15142                          @LENGTH  = LENGTH );
       91    15197
       92    15198    2      ROPCON = TYPE_DESC.DESC;
       93    15199
       94    15200           /* ----------------------------------------------------- */
       95    15201           /* Latest Cobol-85 standard -- PIC 9P(17) -- Length = 18 */
       96    15202           /* ----------------------------------------------------- */
       97    15203    2          IF SCALE > 0
       98    15204    2          OR - SCALE > LENGTH THEN
       99    15205    3            DO;
      100    15206    3              IF SCALE > 0 THEN
      101    15207    3                LENGTH = LENGTH + SCALE;
      102    15208    3              ELSE
      103    15209    3                LENGTH = - SCALE;
      104    15210
      105    15211                   %PACK_NDSC (@DESC   = ROPCON,
      106    15212                               @LENGTH = LENGTH,
      107    15213                               @SCALE  = 0,
