    7      /*X* DMC,DFC */
    8       %EQU PTR_ICODE=1;   /* POINTER */
    9       %EQU SIDX_ICODE=2;  /* SIMPLE INDEX */
   10       %EQU LIDX_ICODE=3;  /* LOWER BOUND INDEX */
   11       %EQU UIDX_ICODE=4;  /* UPPER BOUND INDEX */
   12       %EQU MULT_ICODE=6;  /* MULTIPLIER FOR INDEX */
   13       %EQU EXP_ICODE=8;   /* ADDR EXPRESSION MODIFICATION */
   14       %EQU SIZE_ICODE=9;  /* SIZE OF DISPLAY ITEM (VAR_ICODE) */

