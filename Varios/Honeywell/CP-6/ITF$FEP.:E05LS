PL6.E3A0      #001=ITF$FEP File=ITF$FEP.:E05TSI                                  TUE 07/29/97 23:03 Page:3    
       69     1875        /*D*     NAME:     ITF$FEP
       70     1876                 INPUT:    PTR$ - pointer to next available word in TIGR table
       71     1877                                  area
       72     1878                           Command table - built by ITC$CHECK_FEP in DS5.
       73     1879                 OUTPUT:   FEP tables built and initialized
       74     1880        *
       75     1881                 DESCRIPTION:  ITF$FEP initializes the channel table for the
       76     1882                           channel connected to the FEP, increments KI_MXFE,
       77     1883                           initializes KI_FECH and builds the DQH entry for
       78     1884                           the FEP.
       79     1885        */
       80     1886        /**/
       81     1887    1           CMD$ = B$DS5$;
       82     1888        /**/
       83     1889    1           CMD$ = PINCRW (CMD$,SIZEW(IT$CMDWD));
       84     1890        /**/
       85     1891    1           CHTX = ((CMD$ -> IT$FEP.IOM#) * %N_MAXCHANS) + CMD$ -> IT$FEP.CHAN;
       86     1892    2           IF (NI$CHT$ -> NI$CHT.DQH$(CHTX) ~= ADDR(NIL) ) THEN DO;
       87     1893    2              DUPCHANERR_TEXT.COUNT = SIZEC(DUPCHANERR_TEXT) - 1;
       88     1894    2              DUPCHANERR_TEXT.CHANNEL = 'CHANNEL ';
       89     1895    2              CALL BINCHAR(DUPCHANERR_TEXT.IOMTEXT,CMD$ -> IT$FEP.IOM#);
       90     1896    2              DUPCHANERR_TEXT.DASH = '-';
       91     1897    2              CALL BINCHAR(DUPCHANERR_TEXT.CHANTEXT,CMD$ -> IT$FEP.CHAN);
       92     1898    2              CALL ITE$ERROR(%E$DUPCHAN,ADDR(DUPCHANERR_TEXT));
       93     1899    2              RETURN;
       94     1900    2              END;
       95     1901    2           DO I=1 TO KI_MXFE;
       96     1902    3              IF IT_FEI$$->KI$FEI$(I)->KI$FEI.FEP# = CMD$->IT$FEP.FEP# THEN DO;
       97     1903    3                 CALL ITE$ERROR(%E$DUPFEP#);
       98     1904    3                 RETURN;
       99     1905    3                 END;
      100     1906    2              END;
      101     1907    1           KI_MXFE = KI_MXFE + 1;
      102     1908    1           FEI# = KI_MXFE;
      103     1909    1           FEP# = CMD$ -> IT$FEP.FEP#;
      104     1910        /**/
      105     1911    1           NI$CHT$ -> NI$CHT.DQH$(CHTX) = PTR$;
PL6.E3A0      #001=ITF$FEP File=ITF$FEP.:E05TSI                                  TUE 07/29/97 23:03 Page:4    
      106     1912    1           NI$CHT$ -> NI$CHT.POSTER(CHTX) = ENTADDR(KIQ$FEINT);
      107     1913        /**/
      108     1914    1           DQH$ = PTR$;
      109     1915    1           CALL ITH$ALLOCATE(PTR$,SIZEW(NI$DQH)) ALTRET(IT$FEP_ERR);
      110     1916        /**/
      111     1917    1           DQH$ -> NI$DQH = NI$DQH_C;     /* set to constant inited structure    */
      112     1918        /**/
      113     1919        /*      DQH$ -> NI$DQH.FL$ = ADDR(NIL);         */
      114     1920        /*      DQH$ -> NI$DQH.IO$ = ADDR(NIL);         */
      115     1921    1           IF HW_IMX THEN  ;
      116     1922        /*         DQH$ -> NI$DQH.MBX$ = ADDR(NIL);     */
      117     1923    1           ELSE
      118     1924    1              DQH$ -> NI$DQH.MBX$ = PINCRW (NI$IOM$(CMD$->IT$FEP.IOM#) -> NI$IOM.MBX$,
      119     1925    1                SIZEW(NI$MBX(0)) * CMD$->IT$FEP.CHAN);
      120     1926        /*      DQH$ -> NI$DQH.SQH$ = ADDR(NIL);        */
      121     1927        /*      DQH$ -> NI$DQH.MPC$ = ADDR(NIL);        */
      122     1928        /*      DQH$ -> NI$DQH.IOSTATUS$ = ADDR(NIL);    NO STATUS FOR DIRECT CHANNELS */
      123     1929    1           DQH$ -> NI$DQH.IOCHAN.IOM = CMD$ -> IT$FEP.IOM#;
      124     1930    1           DQH$ -> NI$DQH.IOCHAN.CHANNEL = CMD$ -> IT$FEP.CHAN;
      125     1931    1           DQH$ -> NI$DQH.FEX = FEI#;
      126     1932    1           IT_PARTITION.IOCHAN(CMD$->IT$FEP.IOM#*%N_MAXCHANS+CMD$->IT$FEP.CHAN) = CMD$
              1932                    -> IT$FEP.FLAGS.DOWN;
      127     1933    1           DQH$ -> NI$DQH.STATE = %IDLE;
      128     1934        /**/
      129     1935    1           IT_FEI$$->KI$FEI$(FEI#) = PTR$;
      130     1936    1           CALL ITH$ALLOCATE (PTR$,SIZEW(KI$FEI)) ALTRET (IT$FEP_ERR);
      131     1937    1           IT_FEI$$->KI$FEI$(FEI#)->KI$FEI = KI_FEI;
      132     1938    1           IT_FEI$$->KI$FEI$(FEI#)->KI$FEI.FEP# = FEP#;
      133     1939    1           IT_FEI$$->KI$FEI$(FEI#)->KI$FEI.FECH.IOM = CMD$ -> IT$FEP.IOM#;
