      371    21125    2   CALL M$READ( READ_FPT ) ALTRET( READ_ERR ) ;
      372    21126    2   M$CNFG$ = DCBADDR( DCBNUM( M$CNFG ) ) ;
      373    21127    2   ARS = M$CNFG$ -> F$DCB.ARS ;
      374    21128    2   RETURN ;
      375    21129                                                              /**/
      376    21130    2   READ_ERR: ;
      377    21131    2   ERR_CODE = B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR ;
      378    21132    2   ALTRETURN ;
      379    21133                                                              /**/
      380    21134    2   END   READ_CONFIG ;
      381    21135    1   END   ASC$CONFIG ;

