      167    20821        /*
      168    20822            ***            Begin procedure                    ***
      169    20823        */
      170    20824    1   IF NOT DSEG_FLG THEN
      171    20825    1      CALL ASB$GETSEG
      172    20826    2      WHENALTRETURN DO;
      173    20827              %SCREAM (ERR#=%E$GETSEGERR, LEVEL=ERROR, MID='C');
      174    20842    2         RETURN ;
      175    20843    2         END;   /*  Whenaltreturn from ASB$GETSEG  */
      176    20844                                                              /* Open :CONFIG */
      177    20845    1   CALL M$OPEN( OPEN_FPT ) ALTRET( OPEN_ERR ) ;
      178    20846                                                              /* Open :HLP */
      179    20847    1   CALL ASB$OPEN_HLP_UP ALTRET( OPEN_ERR ) ;
      180    20848                                                              /* Get TD$HEAD */
      181    20849    1   START2$ = PINCRW( DSEG1$, 1024 ) ;
      182    20850    1   START2_SZ = DSEG1SIZE - 4 * 1024 ;
      183    20851    1   TD$HEAD$ = START2$ ;
      184    20852    1   ARS = START2_SZ ;
      185    20853    1   KEY = '0'B ;
      186    20854    1   KEY.CNT = 1 ;
      187    20855    1   CALL READ_CONFIG( KEY, TD$HEAD$, ARS ) ALTRET( READ_CONFIG_ERR ) ;
      188    20856    1   T_ARS = ARS ;                                   /* Total used */
      189    20857    1   ARS = START2_SZ - T_ARS - 4 ;
      190    20858                                                              /* Get TD$WSN */
      191    20859    1   TD$WSN$ = PINCRC( START2$, T_ARS+4 ) ;
      192    20860    1   KEY.CNT = 3 ;
      193    20861    1   KEY.TXT = 'WSN' ;
      194    20862    1   CALL READ_CONFIG( KEY, TD$WSN$, ARS ) ALTRET( READ_CONFIG_ERR ) ;
      195    20863    1   T_ARS = T_ARS + ARS + 4  ;
      196    20864    1   ARS = START2_SZ - T_ARS - 4 ;
      197    20865                                                              /* Get wsn convert */
      198    20866    1   WSNCVT$ = PINCRC( START2$, T_ARS+4 ) ;
      199    20867    1   USERS_KEY.COUNT = 4 ;
PL6.E3A0      #001=ASC$CONFIG File=ASC$CONFIG.:E05TSI                            TUE 07/29/97 09:34 Page:13   
      200    20868    1   USERS_KEY.TXTC = 'TERM' ;
      201    20869    1   CALL ASB$READ_HLP( WSNCVT$, ARS, 1 ) ALTRET( READ_CONFIG_ERR ) ;
      202    20870    1   T_ARS = T_ARS + ARS + 4 ;
      203    20871    1   ARS = START2_SZ - T_ARS - 4 ;
      204    20872                                                              /* Set up TD$DEV$ */
      205    20873    1   TD$DEV$ = PINCRC( START2$, T_ARS+64 ) ;
      206    20874    1   ARS = ARS - 64 ;
      207    20875    1   START$ = ADDR( TD$HEAD.LP_INDX ) ;
      208    20876    1   IF ADDR(WSN_INDEX) = ADDR(NIL)
      209    20877    1   THEN
      210    20878    1      I = 0;
      211    20879    1   ELSE
      212    20880    1      I = WSN_INDEX;
      213    20881    2   DO WHILE ( I < 4 AND TD$WSN.WSNNAME(I) ~= AS_BLANK ) ;
      214    20882    2      TERM_NAME = TD$WSN.WSNNAME(I) ;
      215    20883    2      TWSN_NUM = TWSN_NUM_ARRY(I) ;
      216    20884                                                              /* Set up station record */
      217    20885    2      TREC$ = PINCRW( DSEG2$, 500 ) ;
      218    20886    2      KL$TREC = KL_TREC ;
      219    20887    2      KL$TREC.ATYP = %KLAT_STA# ;
      220    20888    2      KL$TREC.SUPER = AS_STAT#;
      221    20889    2      KL$TREC.WOO = TERM_NAME ;
      222    20890    2      KL$TREC.TWSN# = TWSN_NUM ;
      223    20891    2      TWSN_ARS = LENGTHC( KL$TREC ) ;
      224    20892    2      SS$ = PINCRC( TREC$, TWSN_ARS ) ;
      225    20893    2      TERM_KEY.ACCT = TERM_NAME ;
      226    20894    2      USERS_KEY = TERM_KEY ;
      227    20895    2      CALL ASB$WRITE_HLP( TREC$, TWSN_ARS ) ;
      228    20896                                                              /* Put name in wsncvt */
      229    20897    2      IF I > KL$WSNCVT.LTWSN
      230    20898    2      THEN
      231    20899    3      DO;
      232    20900    3         KL$WSNCVT.LTWSN=I;
      233    20901    3         END; /* Then do */
      234    20902    2      KL$WSNCVT.WSN(I) = TERM_NAME ;
      235    20903    2      TWSNR$ = PINCRC( SS$, 4 ) ;
      236    20904    2      KL$TWSNREC = '0'B ;
PL6.E3A0      #001=ASC$CONFIG File=ASC$CONFIG.:E05TSI                            TUE 07/29/97 09:34 Page:14   
      237    20905    2      KL$TWSNREC.WSN = AS_BLANK ;
      238    20906    2      KL$TWSNREC.STATION = TERM_NAME ;
      239    20907    2      TWSNR_KEY.TWSN# = TWSN_NUM ;
      240    20908    2      TWSN_ARS = LENGTHC( KL$TWSNREC ) - LENGTHC(KL$TWSNREC.DEVS) ;
      241    20909                                                              /* Get devices. */
      242    20910    3      DO J = 0 TO 2 ;
      243    20911    3         HEAD$ = PINCRW( START$, J ) ;
      244    20912    3         INDX = DEV$STRCT.INDX ;
      245    20913    3         CNT = DEV$STRCT.NUM ;
      246    20914                                                              /**/
      247    20915    4         DO K = 0 TO CNT-1 ;
      248    20916    4            DEV_NAME_R = TD$HEAD.DEVNAME( INDX+K ) ;
      249    20917    4            IF DEV_NAME.TWSN = TWSN_NUM
      250    20918    4            THEN
      251    20919    5            DO ;
      252    20920    5               KEY.CNT = 8 ;
      253    20921    5               KEY.TXT = DEV_NAME_R ;
      254    20922    5               CALL READ_CONFIG( KEY, TD$DEV$, ARS )
      255    20923    5                    ALTRET( READ_CONFIG_ERR ) ;
      256    20924    5               NDEV = KL$TWSNREC.NDEV ;
      257    20925    5               KL$TWSNREC.NDEV = KL$TWSNREC.NDEV + 1 ;
      258    20926    5               KL$TWSNREC.DEVS(NDEV) = '0'B;
      259    20927    5               KL$TWSNREC.DEVS.TYP(NDEV) = DEV_NAME.TYPE ;
      260    20928    5               KL$TWSNREC.DEVS.NUM(NDEV) = DEV_NAME.NUM ;
      261    20929    5               KL$TWSNREC.DEVS.ATTR(NDEV) = TD$DEV.ATTR ;
      262    20930    5               KL$TWSNREC.DEVS.ACCT(NDEV) = DEV_NAME_R;
      263    20931    5               KL$TWSNREC.DEVS.IOM_CONNECTED(NDEV) = '1'B;
      264    20932    5               IF DEV_NAME.TYPE = 'LP'
      265    20933    5               THEN
      266    20934    6               DO ;
      267    20935    6                  KL$TWSNREC.DEVS.DDEV(NDEV) = %DDEV_LP# ;
      268    20936    6                  KL$TWSNREC.DEVS.IO.INPUT(NDEV) = AS_FALSE ;
      269    20937    6                  KL$TWSNREC.DEVS.IO.OUTPUT(NDEV) = AS_TRUE ;
      270    20938    6                  KL$TWSNREC.DEVS.FNAME(NDEV) = 'STDLP' ;
      271    20939    6                  END ; /* Then do */
      272    20940    5               IF DEV_NAME.TYPE = 'CP'
      273    20941    5               THEN
PL6.E3A0      #001=ASC$CONFIG File=ASC$CONFIG.:E05TSI                            TUE 07/29/97 09:34 Page:15   
      274    20942    6               DO ;
      275    20943    6                  KL$TWSNREC.DEVS.DDEV(NDEV) = %DDEV_CP# ;
      276    20944    6                  KL$TWSNREC.DEVS.IO.INPUT(NDEV) = AS_FALSE ;;
      277    20945    6                  KL$TWSNREC.DEVS.IO.OUTPUT(NDEV) = AS_TRUE ;
      278    20946    6                  KL$TWSNREC.DEVS.FNAME(NDEV) = 'STD**' ;
      279    20947    6                  END ; /* Then do */
      280    20948    5               IF DEV_NAME.TYPE = 'CR'
      281    20949    5               THEN
      282    20950    6               DO ;
      283    20951    6                  KL$TWSNREC.DEVS.DDEV(NDEV) = '0'B ;
      284    20952    6                  KL$TWSNREC.DEVS.IO.INPUT(NDEV) = AS_TRUE ;
      285    20953    6                  KL$TWSNREC.DEVS.IO.OUTPUT(NDEV) = AS_FALSE ;
      286    20954    6                  KL$TWSNREC.DEVS.FNAME(NDEV) = 'STD**' ;
      287    20955    6                  END ; /* Then do */
      288    20956    5               IF DEV_NAME.TYPE = 'GR'
      289    20957    6               THEN DO;
      290    20958    6                  KL$TWSNREC.DEVS.DDEV(NDEV) = %DDEV_GR#;
      291    20959    6                  KL$TWSNREC.DEVS.IO.INPUT(NDEV) = AS_FALSE ;
      292    20960    6                  KL$TWSNREC.DEVS.IO.OUTPUT(NDEV) = AS_TRUE ;
      293    20961    6                  KL$TWSNREC.DEVS.FNAME(NDEV) = 'STDGR' ;
      294    20962    6                  END ; /* Then do */
      295    20963    5               TWSN_ARS = TWSN_ARS + LENGTHC( KL$TWSNREC.DEVS ) ;
      296    20964    5               END ; /* Then do */
      297    20965    4            END ; /* Do k */
      298    20966    3         END ; /* Do j */
      299    20967    2      USERS_KEY = TWSNR_KEY ;
      300    20968    2      CALL ASB$WRITE_HLP( TWSNR$, TWSN_ARS ) ;
      301    20969                                                              /**/
      302    20970    2      WSN_BUF$ = ADDR( WSN_BUF ) ;
      303    20971    2      WSN_BUF$ = PINCRC( WSN_BUF$, 6 ) ;
      304    20972    2      WSN$ACCT = TERM_NAME ;
      305    20973    2      WSN_BUF$ = PINCRC( WSN_BUF$, 18 ) ;
      306    20974    2      WSN$ACCT = TERM_NAME ;
      307    20975    2      IN_BUF = WSN_BUF ;
      308    20976    2      P_PCB.NCHARS = 32 ;
      309    20977    2      CALL ASW$CONFIG_WSN ;
      310    20978    2      I = I + 1 ;
PL6.E3A0      #001=ASC$CONFIG File=ASC$CONFIG.:E05TSI                            TUE 07/29/97 09:34 Page:16   
      311    20979    2      IF ADDR(WSN_INDEX) ~= ADDR(NIL) THEN
      312    20980    2         EXIT;
      313    20981    2      END ; /* Do while */
      314    20982                                                              /**/
      315    20983    1   CVT_ARS = (KL$WSNCVT.LTWSN + 2) * 8;  /* set bound for wsncvt record */
      316    20984    1   USERS_KEY.COUNT = 4 ;
      317    20985    1   USERS_KEY.TXTC = 'TERM' ;
      318    20986    1   CALL ASB$WRITE_HLP( WSNCVT$, CVT_ARS ) ALTRET( READ_CONFIG_ERR ) ;
      319    20987                                                              /**/
      320    20988    1   CALL ASB$CLOSE_HLP ;
      321    20989    1   CALL M$CLOSE( CLOSE_FPT ) ;
      322    20990                                                              /**/
      323    20991    1   RETURN ;
      324    20992                                                              /* Errors */
      325    20993    1   OPEN_ERR: ;
      326    20994    1   ERR_CODE = B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR ;
      327    20995    1   CALL ASH$ERROR( ERR_CODE ) ;
      328    20996    1   RETURN ;
      329    20997                                                              /**/
      330    20998    1   READ_CONFIG_ERR: ;
      331    20999    1   ERR_CODE = B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR ;
      332    21000    1   CALL ASH$ERROR( ERR_CODE ) ;
      333    21001    1   RETURN ;
      334    21002                                                              /**/
      335    21003        %EJECT ;
