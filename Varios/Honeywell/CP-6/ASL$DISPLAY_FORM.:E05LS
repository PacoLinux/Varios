      243        8        /*X*  DMC,DMR,CCC,PLM=0,IND=3,CTI=5,DCI=6,IAD=0,DTI=0,IDT=0,ENI=3,
      244        9              ECU=3,CSU=3,SDI=3                                           */
      245       10        ASL$DISP_ONE_FORM: PROC  ( OBJECT, FORM_FLAG, JCL) ;
      246       11        /**********************************************************/
      247       12
      248       13        /*D* NAME:     ASL$DISP_ONE_FORM
      249       14             CALL:     CALL ASL$DISP_ONE_FORM (FORM_FLAG, JCL);
      250       15             INPUT:
      251       16                       FORM_FLAG - bit array - where set option is to be
      252       17                                                displayed.
      253       18                       JCL - flag for method of display.
      254       19             OUTPUT:   Nothing is returned.
      255       20             DESCRIPTION:  ASL$DISP_ONE_FORM is the routine that handles
      256       21                           the display of a single form.
      257       22        */
