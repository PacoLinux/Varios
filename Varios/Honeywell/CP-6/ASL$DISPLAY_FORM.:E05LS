PL6.E3A0      #002=ASL$LIST_ONE_FORMSET File=ASL$DISPLAY_FORM.:E05TSI            TUE 07/29/97 09:58 Page:28   
      107        7        ASL$LIST_ONE_FORMSET: PROC (OBJECT, FORM_COUNT, JCL, FLAGS) ALTRET;
      108        8        /*D*
      109        9             NAME: ASL$LIST_ONE_FORMSET
      110       10             CALL: CALL ASL$LIST_ONE_FORMSET (OBJECT, FORM_COUNT, JCL, FLAGS);
      111       11             INPUT:
      112       12                       OBJECT - code for FORMSET/FORM
      113       13                       FORM_COUNT - added to if a form is passed on to the
      114       14                                    display routine.
      115       15                       JCL   - flag for type of display.
      116       16                       FLAGS  - bits for options to display.
      117       17             OUTPUT:   FORM_COUNT is updated and returned.
      118       18             DESCRIPTION:
      119       19                       This routine checks that the form record in data
      120       20                       segment 1 is a master form if the object specified
      121       21                       is a FORMSET.  If object is a FORMSET and the form
      122       22                       is not a master form, this routine altreturns.
      123       23                       If the object is a FORMSET and the form is a master
      124       24                       form, this routine calls the display routine.
      125       25                       pseudo forms for this master form.  When it finds one
      126       26                       it calls the display routine.
      127       27                       If the object is a FORM, this routine calls the display
      128       28                       routine.
      129       29        */
      130       30        /********************************************************************/
      131       31        /*   Include files   include files   include files   include files  */
      132       32        /********************************************************************/
      133       33        %INCLUDE AS_SUPER_C;
      134      468        %INCLUDE AS_SUBS_C;
      135     1882        %INCLUDE AS_MAC_C;
      136     1962        %INCLUDE AS_SSUBS_C;
      137     2054        %INCLUDE AS_ENTRIES_M;
      138     3104        %INCLUDE OCI_SUBS_C ;
      139     3202        %INCLUDE B_ERRORS_C;
      140     4329        %INCLUDE AS_PERR_C;
      141     4931        %INCLUDE FS_SUPER_C;
      142     5694        %INCLUDE KL_SUPER_C;
      143     8596        %INCLUDE XU_MACRO_C;
PL6.E3A0      #002=ASL$LIST_ONE_FORMSET File=ASL$DISPLAY_FORM.:E05TSI            TUE 07/29/97 09:58 Page:29   
      144    11702        %INCLUDE XU_FORMAT_C;
      145    11864        %INCLUDE AS_SUPER_R;
      146    14880        /********************************************************************/
      147    14881        /*   Parameters      parameters      parameters      parameters     */
      148    14882        /********************************************************************/
      149    14883    1   DCL FLAGS (0:8)         BIT (1);
      150    14884    1   DCL JCL                 BIT(1);
      151    14885    1   DCL FORM_COUNT          UBIN;
      152    14886    1   DCL OBJECT              UBIN;
      153    14887        /********************************************************************/
      154    14888        /*   Entry dcls      entry dcls      entry dcls      entry dcls     */
      155    14889        /********************************************************************/
      156    14890        %ASF$READ_FORM_SEQ ;
      157    14893        %ASF$READ_FORM ;
      158    14896        %ASF$PREC_SKIP_FORM ;
      159    14899        %ASH$ERROR ;
      160    14902        %ASL$DISP_ONE_FORM ;
      161    14905        /********************************************************************/
      162    14906        /*   Local based     local based     local based     local based    */
      163    14907        /********************************************************************/
      164    14908        %FS$FORMREC ( NAME = FS$FORMREC, STCLASS = "BASED(DSEG1$)");
      165    14979        /********************************************************************/
      166    14980        /*   Auto data       auto data       auto data       auto data      */
      167    14981        /********************************************************************/
      168    14982    1   DCL ARS                    UBIN;
      169    14983    1   DCL 1 FORM,
      170    14984    1          2 CNT               UBIN BYTE UNAL,
      171    14985    1          2 TXT               CHAR(6) UNAL;
      172    14986    1   DCL I                      UBIN;
      173    14987    1   DCL 1 PSEUDO_FORM_KEY,
      174    14988    1          2 CNT               UBIN BYTE UNAL,
      175    14989    1          2 TXT               CHAR(12) UNAL;
      176    14990    1   DCL PSEUDO_FORM_REC        UBIN;
      177    14991        /********************************************************************/
      178    14992        /*   Procedure       procedure       procedure       procedure      */
      179    14993        /********************************************************************/
      180    14994    1   FORM.TXT = SUBSTR (USERS_KEY.TXTC,0,6);
PL6.E3A0      #002=ASL$LIST_ONE_FORMSET File=ASL$DISPLAY_FORM.:E05TSI            TUE 07/29/97 09:58 Page:30   
      181    14995    1   FORM.CNT = 6;
      182    14996    2   DO SELECT (OBJECT);
      183    14997
      184    14998    2      SELECT (%FORM_OBJ_VAL);
      185    14999
      186    15000    2         CALL ASL$DISP_ONE_FORM ( OBJECT, FLAGS, JCL);
      187    15001    2         FORM_COUNT = FORM_COUNT + 1;
      188    15002
      189    15003    2      SELECT (%FORMSET_OBJ_VAL);
      190    15004
      191    15005    2         IF FS$FORMREC.FNAME ~= SUBSTR (USERS_KEY.TXTC,0,6) THEN
      192    15006    2            ALTRETURN;
      193    15007    2         CALL ASL$DISP_ONE_FORM ( OBJECT, FLAGS, JCL);
      194    15008    2         FORM_COUNT = FORM_COUNT + 1;
      195    15009    3         DO FOREVER;
      196    15010    3            ARS = SIZEC(PSEUDO_FORM_REC);
      197    15011    3            CALL ASF$READ_FORM_SEQ( ,ADDR(PSEUDO_FORM_REC), ARS ) ALTRET(EXIT#);
      198    15012    4            IF USERS_KEY.COUNT = 12 THEN DO;  /* Possible pseudo form  */
      199    15013    5               IF SUBSTR(USERS_KEY.TXTC,0,6) = FORM.TXT THEN DO;
      200    15014                                                     /* Definite pseudo form  */
      201    15015    5                  PSEUDO_FORM_KEY = USERS_KEY;
      202    15016    5                  USERS_KEY.COUNT = 6;
      203    15017    5                  SUBSTR (USERS_KEY.TXTC,0,6) = SUBSTR (PSEUDO_FORM_KEY.TXT,6,6);
      204    15018    5                  I = DSEG1SIZE;
      205    15019    5                  CALL ASF$READ_FORM(USERS_KEY, DSEG1$, I)
      206    15020    6                  WHENALTRETURN DO;
      207    15021    6                     IF MERR_CODE.CODE = %E$NOKEY
      208    15022    7                     THEN DO;
      209    15023                 /*E*  ERROR:  ASL-E$PSEUDONOEXIST-C
      210    15024                       MESSAGE: %U1 does not exist.
      211    15025                       MESSAGE1: A pseudo form of %U2 FORM does not exist.
      212    15026                       MESSAGE2: Your :FORM file is broken.
      213    15027                 */
      214    15028    7                        AS_FIELD1 = FORM;
      215    15029                             %SCREAM( ERR#=E$PSEUDONOEXIST, LEVEL=ERROR, MID='L' );
      216    15044    7                        END;  /*  Error is nokey  */
      217    15045    7                     ELSE DO;
PL6.E3A0      #002=ASL$LIST_ONE_FORMSET File=ASL$DISPLAY_FORM.:E05TSI            TUE 07/29/97 09:58 Page:31   
      218    15046    7                        CALL ASH$ERROR( MERR_CODE );
      219    15047    7                        END;  /*  Error is not nokey  */
      220    15048    6                     RETURN;
      221    15049    6                     END;  /*  Whenaltreturn from ASF$READ_FORM  */
      222    15050    5                  CALL ASL$DISP_ONE_FORM ( OBJECT, FLAGS, JCL);
      223    15051    5                  FORM_COUNT = FORM_COUNT + 1;
      224    15052    5                  USERS_KEY = PSEUDO_FORM_KEY;
      225    15053    5                  CALL ASF$PREC_SKIP_FORM (USERS_KEY) ;
      226    15054    5                  END;  /*  Found pseudo  */
      227    15055    4               END;  /*  Possible pseudo  */
      228    15056    3            END;  /*  Do forever  */
      229    15057    2      END;  /*  Case  */
      230    15058    1   EXIT#:
      231    15059    1   USERS_KEY = FORM;
      232    15060    1   CALL ASF$PREC_SKIP_FORM( USERS_KEY );
      233    15061    1   RETURN;
      234    15062    1   END ASL$LIST_ONE_FORMSET;
      235    15063        %EOD;

