PL6.E3A0      #001=ASL$FORM_LIST_OPTS File=ASL$DISPLAY_FORM.:E05TSI              TUE 07/29/97 09:56 Page:3    
       57    17828        /********************************************************************/
       58    17829        /*   Procedure       procedure       procedure       procedure      */
       59    17830        /********************************************************************/
       60    17831    1   FORM_FLAG = '0'B;
       61    17832    1   MORE = '1'B;
       62    17833    1   L1: P_PCB.ROOT$ = ADDR (LIST_FORM_OPTS_LIST);
       63    17834    2   DO WHILE (MORE);
       64    17835    2      REMEMBER L1 IN REMEM_VAR;
       65    17836                                                /*  Set prompt */
       66    17837    2      CALL ASB$SET_PROMPT ( %PROMPT_OPT ) ;
       67    17838    2      CALL ASB$READ_SI(IN_BUF,ARS);
       68    17839    2      P_PCB.NCHARS = ARS;
       69    17840    2      CALL X$PARSE(P_PCB)
       70    17841    3      WHENRETURN DO;
       71    17842    4         DO I = 0 TO P$B.NSUBLKS - 1;
       72    17843    5            DO SELECT(P$B.SUBLK$(I)->P$B.CODE);
       73    17844    5               SELECT (ALL_VAL);
       74    17845    5                  FORMFLAG = FORMFLAG | '777'O;
       75    17846    5                  MORE = '0'B;
       76    17847    5               SELECT (END_VAL);
       77    17848    5                  MORE = '0'B;
       78    17849    5               SELECT (BLANK_LINE_VAL);
       79    17850    5                  MORE = '0'B;
       80    17851    5               SELECT (HELP_VAL);
       81    17852    5                  CALL ASH$HELP ;
       82    17853    5               SELECT (QUESTION_VAL);
       83    17854    5                  CALL ASH$QUEST;
       84    17855    5               SELECT (DQUESTION_VAL);
       85    17856    5                  CALL ASH$DQUEST ;
       86    17857    5               SELECT (EOL_VAL);
       87    17858    5               SELECT (ELSE);
       88    17859    5                  FORM_FLAG (P$B.SUBLK$(I)->P$B.CODE) = '1'B;
       89    17860    5               END;  /* Do case */
       90    17861    4            END;
       91    17862    3         END; /* Whenreturn from the parse */
       92    17863    3      WHENALTRETURN DO;
       93    17864    3         MERR_CODE = P_PCB.ERROR;
PL6.E3A0      #001=ASL$FORM_LIST_OPTS File=ASL$DISPLAY_FORM.:E05TSI              TUE 07/29/97 09:56 Page:4    
       94    17865    3         MERR_CODE.SEV = 0;
       95    17866    3         CALL ASH$ERROR(MERR_CODE);
       96    17867    3         END;
       97    17868    2      END; /* Do while more */
       98    17869    1   RETURN;
       99    17870    1   END ASL$FORM_LIST_OPTS;
      100    17871        %EOD;

