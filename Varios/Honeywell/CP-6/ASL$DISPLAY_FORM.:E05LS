     1096    16914    2      IF FORMFLAG.ALIGN AND NOT PSEUDO
     1097    16915    3      THEN DO;
     1098    16916    3         IF FS$FORMREC.ALGNDISP~=0
     1099    16917    4         THEN DO;
     1100    16918    4            CALL ASB$WRITE_LO(ALIGNMENT_STR,SIZEC(ALIGNMENT_STR));
     1101    16919    4            P$ = PINCRW(DSEG1$,FS$FORMREC.ALGNDISP);
     1102    16920    4            CALL CONCAT (FMT_BUF,ALIGN_JCL_FMT,YES_NO_FMT);
     1103    16921    4            FDS.FMT_.BOUND = SIZEC(ALIGN_JCL_FMT) + SIZEC(YES_NO_FMT) - 1;
     1104    16922    4            FDS.NVECS# = 1;
     1105    16923    4            UBIN_ARRAY(0) = BITBIN ( FS$BAN.ALIGNCTL.EVERY );
     1106    16924    4            FMT_VEC(0) = VECTOR ( UBIN_ARRAY(0) );
     1107    16925    4            CALL FINISH_BANNER;
     1108    16926        /*    Call ASB$WRITE_LO(' ',1);*/
     1109    16927    4            END; /*  Alignment exists  */
     1110    16928    4         ELSE DO;
     1111    16929    4            CALL ASB$WRITE_LO(REMOVE_ALIGNMENT_STR,SIZEC(REMOVE_ALIGNMENT_STR));
     1112    16930    4            END;   /*  No alignment  */
     1113    16931    3         END; /* If FORMFLAG.ALIGN and not pseudo  */
     1114    16932
     1115    16933        /**
     1116    16934         ** Banner
     1117    16935         **/
     1118    16936    2      IF FORMFLAG.BANNER
     1119    16937    3      THEN DO;
     1120    16938    3         IF FS$FORMREC.BANRDISP~=0
     1121    16939    4         THEN DO;
     1122    16940    4            CALL ASB$WRITE_LO(BANNER_STR,SIZEC(BANNER_STR));
PL6.E3A0      #003=ASL$DISP_ONE_FORM File=ASL$DISPLAY_FORM.:E05TSI               TUE 07/29/97 09:59 Page:82   
     1123    16941    4            P$ = PINCRW(DSEG1$,FS$FORMREC.BANRDISP);
     1124    16942    4            CALL CONCAT (FMT_BUF,CHARGE_JCL_FMT,YES_NO_FMT);
     1125    16943    4            FDS.FMT_.BOUND = SIZEC(CHARGE_JCL_FMT) + SIZEC(YES_NO_FMT) - 1;
     1126    16944    4            FDS.NVECS# = 1;
     1127    16945    4            UBIN_ARRAY(0) = FS$BAN.CHARGE;
     1128    16946    4            FMT_VEC(0) = VECTOR ( UBIN_ARRAY(0) );
     1129    16947    4            CALL FINISH_BANNER;
     1130    16948    4            CALL ASB$WRITE_LO(' ',1);
     1131    16949    4            END; /*  Banner exists  */
     1132    16950    4         ELSE DO;
     1133    16951    4            CALL ASB$WRITE_LO(REMOVE_BANNER_STR,SIZEC(REMOVE_BANNER_STR));
     1134    16952    4            END;   /*  No banner  */
     1135    16953    3         END; /* If FORMFLAG.BANNER */
     1136    16954
     1137    16955    2      END; /* If jcl */
     1138    16956
     1139    16957        /* We are now done with this form... */
