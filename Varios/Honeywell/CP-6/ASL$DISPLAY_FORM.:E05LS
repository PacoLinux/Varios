     1152    16970    1   FINISH_BANNER: PROC;
     1153    16971    2   DCL I SBIN;
     1154    16972    2   DCL 1 X_INT STATIC,
     1155    16973    2          2 * CHAR(1) INIT('%'),
     1156    16974    2          2 XCHAR CHAR(1);
     1157    16975
