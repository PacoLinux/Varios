    6      %SUB E$UTS_FIELD_TOO_BIG  =   1; /* "%U1" field of UTS too big */
    7      %SUB E$NOT_A_FILE         =   2; /* Specified PLOVER fid is not a file */
    8      %SUB E$NOT_AN_OUF         =   3; /* Specified PLOVER fid is not an OU file */
    9      %SUB E$OU_NOT_FOUND       =   4; /* Object unit not found */
   10      %SUB E$UTS_VIOLATION      =   5; /* UTS violation */
   11      %SUB E$NO_FILE            =   6; /* Command issued, but no file named. */
   12      %SUB E$NO_OBJECT_UNIT     =   7; /* Command issued, but no OU named. */
   13      %SUB E$CMD_TOO_LONG       =   8; /* Command more than 256 characters */
   14      %SUB E$PLOVER1            =   9; /* (File=%FN.%AC  -  Object unit = %U1) */
   15      %SUB E$PLOVER2            =  10; /* (File=%FN.%AC  -  No object unit named) */
   16      %SUB E$PLOVER3            =  11; /* (No file named  -  Object unit=%U1) */
   17      %SUB E$PLOVER4            =  12; /* (No file named  -  No object unit named) */
   18      %SUB E$NOTHING_TO_LIST    =  13; /* Nothing to list. */
   19      %SUB E$NOTHING_TO_DEL     =  14; /* Nothing to delete */
   20      %SUB E$ALL_PLOVERS_DEL    =  15; /* All PLOVER records deleted */
   21      %SUB E$ALL_OU_PLOVERS_DEL =  16; /* All PLOVER records for OU %U1 deleted */
   22      %SUB E$ALL_UTS_PLOVERS_DEL=  17; /* All PLOVER records for OU %U1,UTS=%U2 deleted */
   23      %SUB E$SECT_DOES_NOT_EXIST=  18; /* Section %U1 does not exist in OU %U2 */
   24      %SUB E$UTS_BUT_NO_OU      =  19; /* UTS was specified, but OU was not */
   25      %SUB E$BAD_COMMAND_LINE   =  20; /* problem parsing the command */
   26      %SUB E$IGNORE_CMDS        =  21; /* next commands wont be executed */

