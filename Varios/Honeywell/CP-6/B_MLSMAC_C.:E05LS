    7      /*X* DMC,PLM=4,IND=0,SDI=2,CTI=0,ENU=0,AND,DCI=4,CSU=2,ECU=4,THI=0,DTI=2 */
    8      %MACRO B_MLS (FPTN=B_MLS,STCLASS=BASED,LVL=1,LAST=";");
    9
   10       %LSET LISTDIR='0'B;
   11       %LSET LISTEXP='1'B;
   12       %LSET LISTCOM='0'B;
   13       %LSET LISTSUB='1'B;
   14       %LSET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
   15       %IF (SUBSTR(STR,0,6)='STATIC') OR
   16       (SUBSTR(STR,0,8)='CONSTANT') OR
   17       (SUBSTR(STR,0,3)='EXT');
   18       %LSET INIT=CHARTEXT('INIT');
   19       %ELSE;
   20       %LSET INIT=CHARTEXT('/*');
   21      %LSET PAGE_BOUND=0;
   22      %LSET B_CWSQ1=0;
   23      %LSET B_CWSQ2=0;
   24      %LSET B_CWSQ3=0;
   25      %LSET B_CWSQ4=0;
   26      %LSET B_CWSQ5=0;
   27      %LSET B_CWSQ6=0;
   28      %LSET B_CWSQ7=0;
   29      %LSET B_CWSQ8=0;
   30      %LSET B_CWSQ9=0;
   31      %LSET B_CWSQ10=0;
   32      %LSET B_CWSQ11=0;
   33      %LSET B_CWSQ12=0;
   34      %LSET B_CWSQ13=0;
   35      %LSET B_CWSQ14=0;
   36      %LSET B_CWSQ15=0;
   37      %LSET B_CWSQ16=0;
14:13 JUL 28 '97 B_MLSMAC_C.:E05SI                                         2    
   38      %LSET PAGE_BOUND=0;
   39      %LSET IOENDWSQ=0;
   40      %LSET CGWSR=0;
   41      %LSET BYTES#=0;
   42      %LSET PAGE_BOUND=0;  /* Fake out preprocessor */
   43       %ENDIF;
   44
   45       %IF LVL=1;
   46      DCL 1 FPTN STCLASS DALIGNED,
   47       %ELSE;
   48          %LVL FPTN DALIGNED,
   49       %ENDIF;
