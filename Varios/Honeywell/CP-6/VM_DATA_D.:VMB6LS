        9      547          %INCLUDE CP_6;
       10     6106        /***** Canned ptrs *****/
       11     6107        /*
       12     6108             ***** HOW DATA SEGMENTS ARE USED IN MAIL/SEND/MAILMAN ...
       13     6109
       14     6110                   Segments used in all modules
       15     6111                   ----------------------------
       16     6112                   VM_ADDLIST
       17     6113                      - used for building new address list (VMA$EXPAND_LIST).
       18     6114
       19     6115                   VM_HEADER
       20     6116                      - used to contain the HEADER of a message.
       21     6117
       22     6118                   VM_PROFILE
       23     6119                      - used to contain the PROFILE of a folder.
       24     6120
       25     6121                   VM_WORK
       26     6122                      - used to contain LIST and USER definitions when
       27     6123                        expanding an address list.
       28     6124
       29     6125                   Segments used by only MAIL and SEND
       30     6126                   -----------------------------------
       31     6127                   VM_PARSEAREA
       32     6128                      - used to contain input and parse info (XUR$GETCMD)
       33     6129
       34     6130                   VM_ENCLLIST
       35     6131                      - used to hold the enclosure context list
       36     6132
       37     6133                   VM_ENCLREC
PL6.E3A0      #001=VM_DATA_D File=VM_DATA_D.:VMB6TSI                             WED 08/20/97 15:26 Page:2    
       38     6134                      - used to hold enclosure data records
       39     6135
       40     6136                   Segments used only by MAIL
       41     6137                   --------------------------
       42     6138                   VM_SELECT
       43     6139                      - used to contain MESSAGE SELECTION CONTEXTS
       44     6140                        Note: VM_SELECT shares a data segment with VM_PARSEAREA.
       45     6141                        VM_SELECT occupies the first 1024 words and VM_PARSEAREA
       46     6142                        the subsequent 2048 words of the segment.
       47     6143
       48     6144                   Segments used only by MAILMAN
       49     6145                   -----------------------------
       50     6146                   VM_ADDREC
       51     6147                      - used to contain address record to be expanded.
       52     6148
       53     6149                   VM_INPUT
       54     6150                      - used to contain input queue.
       55     6151
       56     6152                   VM_COPY (redef of VM_SELECT)
       57     6153                      - used to move copies of files around.
       58     6154        */
       59     6155    1   DCL 1 VM_HEADER$ CONSTANT SYMDEF,
       60     6156    1         2 OFFSET UBIN(24) INIT(0) UNAL,
       61     6157    1         2 SID BIT(12) INIT(%DS8SID) UNAL;
       62     6158
       63     6159    1   DCL 1 VM_PROFILE$ CONSTANT SYMDEF,
       64     6160    1         2 OFFSET UBIN(24) INIT(0) UNAL,
       65     6161    1         2 SID BIT(12) INIT(%DS3SID) UNAL;
       66     6162
       67     6163    1   DCL 1 VM_WORK$ CONSTANT SYMDEF,
       68     6164    1         2 OFFSET UBIN(24) INIT(0) UNAL,
       69     6165    1         2 SID BIT(12) INIT(%DS4SID) UNAL;
       70     6166
       71     6167    1   DCL 1 VM_ADDLIST$ CONSTANT SYMDEF,
       72     6168    1         2 OFFSET UBIN(24) INIT(0) UNAL,
       73     6169    1         2 SID BIT(12) INIT(%DS5SID) UNAL;
       74     6170
PL6.E3A0      #001=VM_DATA_D File=VM_DATA_D.:VMB6TSI                             WED 08/20/97 15:26 Page:3    
       75     6171    1   DCL 1 VM_ADDREC$ CONSTANT SYMDEF,
       76     6172    1         2 OFFSET UBIN(24) INIT(0) UNAL,
       77     6173    1         2 SID BIT(12) INIT(%DS6SID) UNAL;
       78     6174
       79     6175    1   DCL 1 VM_PARSEAREA$ CONSTANT SYMDEF,
       80     6176    1         2 OFFSET UBIN(24) INIT(4096) UNAL,
       81     6177    1         2 SID BIT(12) INIT(%DS7SID) UNAL;
       82     6178
       83     6179    1   DCL 1 VM_SELECT$ CONSTANT SYMDEF,
       84     6180    1         2 OFFSET UBIN(20) INIT(0) UNAL,
       85     6181    1         2 * UBIN(4) INIT(0) UNAL,
       86     6182    1         2 SID BIT(12) INIT(%DS7SID) UNAL;
       87     6183
       88     6184    1   DCL 1 VM_COPY$ CONSTANT SYMDEF,
       89     6185    1         2 OFFSET UBIN(24) INIT(0) UNAL,
       90     6186    1         2 SID BIT(12) INIT(%DS7SID) UNAL;
       91     6187
