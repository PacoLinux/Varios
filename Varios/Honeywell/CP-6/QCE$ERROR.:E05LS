      170    21059    1       FPT@ERRMSG = QI_FPT_ERRMSG;
      171    21060    1       VBASE ( FPT@ERRMSG.V_ ) = ADDR ( FPT@ERRMSG.V );
      172    21061
      173    21062    2       DO CASE ( ERROR_CODE );
      174    21063
