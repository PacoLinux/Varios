      305    21266    2           Q@FPABT.MSGS.U1 (1) = FILE_IDENTIFIER$ -> QI$VARIABLE_IDENTIFIER;
      306    21267    2           Q@FPABT.MSGS.ERR (2) = G$TCB$ -> G$TCB.ALT$ -> G$EXCFR.TRLR.ERR;
      307    21268
      308    21269    2         CASE ( %E$TRANSACTION_TYPE_WILD,
      309    21270    2             %E$TRANSACTION_WRITE_ERROR,
      310    21271    2             %E$TRANSACTION_RRR_ERROR );
      311    21272    2           Q@FPABT.MSGS.ERR (1) = VLP_ERRCODE_QBW;
      312    21273    2           Q@FPABT.MSGS.U1 (1) = FILE_IDENTIFIER$ -> QI$VARIABLE_IDENTIFIER;
      313    21274    2           Q@FPABT.MSGS.ERR (2) = G$TCB$ -> G$TCB.ALT$ -> G$EXCFR.TRLR.ERR;
      314    21275
      315    21276    2         CASE ( %E$FORM_READ_ERROR,
      316    21277    2             %E$FORM_WRITE_ERROR );
      317    21278    2           Q@FPABT.MSGS.ERR (1) = VLP_ERRCODE_QCI;
      318    21279    2           Q@FPABT.MSGS.U1 (1) = FILE_IDENTIFIER$ -> QI$VARIABLE_IDENTIFIER;
      319    21280                /* In the case of a form read error, the error code was returned
      320    21281                   to us in a RDMULT buffer (not the TCB).  Since both read form
      321    21282                   and write form go through the QGB layer, it's safe to get the
      322    21283                   error code from the QGB_PARAM structure here. */
      323    21284    2           Q@FPABT.MSGS.ERR (2) = QGB_PARAM.ERR;
      324    21285
