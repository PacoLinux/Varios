PL6.E3A0      #003=QCE$EXPERIENCE_M$XBREAK File=QCE$ERROR.:E05TSI                WED 07/30/97 05:12 Page:139  
      704        1        /*M* QCE$EXPERIENCE_M$XBREAK:  Call Carl.  */
      705        2        /*T***********************************************************/
      706        3        /*T*                                                         */
      707        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      708        5        /*T*                                                         */
      709        6        /*T***********************************************************/
      710        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
      711        8
      712        9        /*P*  NAME:         QCE$EXPERIENCE_M$XBREAK
      713       10              PURPOSE:      To CALL M$XBREAK and act appropriately
      714       11                            upon its return.
      715       12              DESCRIPTION:  Basically, this is just a home for the
      716       13                            M$XBREAK call.
      717       14        */
      718       15
      719       16        QCE$EXPERIENCE_M$XBREAK: PROC;
      720       17
      721       18        /*D*  NAME:         QCE$EXPERIENCE_M$XBREAK
      722       19              CALL:         CALL QCE$EXPERIENCE_M$XBREAK;
      723       20              PARAMETERS:   None.
      724       21              INTERFACE:    Called when:
      725       22                            1)  QAT$TALLYHO encounters a BRK op-code;
      726       23                            2)  QAT$TALLYHO sees that QI_VLP_XBREAK.TT is set
      727       24                                and the just-executed op-code has resulted
      728       25                                in a transfer (QI_1.INSTRUCTION_TRANSFERRED);
      729       26                            3)  QCE$ERROR_DBCONTROL was entered.
      730       27              ENVIRONMENT:  Nothing unusual.
      731       28              INPUT:        QI_1.INSTR$, etc.
      732       29              OUTPUT:       None.
      733       30              SCRATCH:      Various otter data.
      734       31              DESCRIPTION:  Call M$XBREAK and act accordingly.
      735       32        */
      736       33
      737       34          %INCLUDE LCP_6;
      738      225          %FPT_XBREAK ( FPTN = FPT@XBREAK,
      739      226                       STCLASS = AUTO );
      740      241          %G$EXCFR;
PL6.E3A0      #003=QCE$EXPERIENCE_M$XBREAK File=QCE$ERROR.:E05TSI                WED 07/30/97 05:12 Page:140  
      741      623          %G$TCB;
      742      644    1     DCL G$TCB$ PTR SYMREF;
      743      645
      744      646          %INCLUDE QG_MACRO_M;
      745      783          %INCLUDE QI_EQUATES_E;
      746     1142          %INCLUDE QI_PERR_C;
      747     1310          %INCLUDE QI_MACROS_M;
      748     6368          %QI$ALL;
      749    17481          %QI_FPTS;
      750    19526
      751    19527    1     DCL QCE$ERROR_M$INTERNAL ENTRY (4);
      752    19528        %EJECT;
PL6.E3A0      #003=QCE$EXPERIENCE_M$XBREAK File=QCE$ERROR.:E05TSI                WED 07/30/97 05:12 Page:141  
      753    19529    1       FPT@XBREAK = QI_FPT_XBREAK;
      754    19530
      755    19531    1       CALL M$XBREAK ( FPT@XBREAK )
      756    19532    1       WHENRETURN
      757    19533    2         DO;
      758    19534    2           IF QI_VLP_XBREAK.GOIC
      759    19535    2           THEN
      760    19536    2             QI_1.INSTR$ = QI_VLP_XBREAK.IC$;
      761    19537    2         END;
      762    19538    1       WHENALTRETURN
      763    19539    2         DO;
      764    19540    2           CALL QCE$ERROR_M$INTERNAL ( G$TCB$ -> G$TCB.ALT$ -> G$EXCFR.TRLR.ERR.ERR# );
      765    19541    2         END;
      766    19542
      767    19543    1       RETURN;
      768    19544
      769    19545    1   END QCE$EXPERIENCE_M$XBREAK;

