      253    21142    1       Q@FPABT.MSGS.U3.LEN# (1) = 0;
      254    21143
      255    21144            /*  OK, first, this was thrown together in about 1 evening.
      256    21145                Second, we're on our way out, so who cares how slow it is.
      257    21146                Third, I sure hope somebody throws that USE stuff out
      258    21147                soon.
      259    21148                The problem's that we might have gotten here from QCU$USE,
      260    21149                so we don't know what the "FCG of origin" was.
      261    21150                It just so happens, however, that in order to have come
      262    21151                from QCU$USE, we must have an error whose U1 is either
      263    21152                in QI_1.CURRENT.FILE$ or QI_1.CURRENT.FRAME$.
      264    21153                Sooo, we can just "devine" the FCG by vectoring off the
      265    21154                error code.  */
      266    21155    1       IF QI_1.CURRENT.FILE$ ~= ADDR (NIL)
      267    21156    1       THEN
PL6.E3A0      #001=QCE$ERRORS File=QCE$ERROR.:E05TSI                             WED 07/30/97 05:05 Page:11   
      268    21157              %PROC_ADDRESS ( THING@P = "QI_1.CURRENT.FILE$ -> QI$FILE.IDENTIFIER@P",
      269    21158                CHECK_FOR_INDIRECTION = NO,
      270    21159                RESULT$ = FILE_IDENTIFIER$ );
      271    21196    1       ELSE
      272    21197    1         FILE_IDENTIFIER$ = ADDR (NIL);
      273    21198
      274    21199    1       IF QI_1.CURRENT.FRAME$ ~= ADDR (NIL)
      275    21200    1       THEN
      276    21201              %PROC_ADDRESS ( THING@P = "QI_1.CURRENT.FRAME$ -> QI$FRAME.IDENTIFIER@P",
      277    21202                CHECK_FOR_INDIRECTION = NO,
      278    21203                RESULT$ = FRAME_IDENTIFIER$ );
      279    21240    1       ELSE
      280    21241    1         FRAME_IDENTIFIER$ = ADDR (NIL);
      281    21242
      282    21243    2       DO CASE ( ERROR_CODE );
      283    21244
      284    21245    2         CASE ( %E$TPAP_ABORT );
      285    21246    2           Q@FPABT.MSGS.ERR (1) = VLP_ERRCODE_QB;
      286    21247
