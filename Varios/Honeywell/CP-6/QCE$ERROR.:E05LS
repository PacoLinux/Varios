PL6.E3A0      #002=QCE$ERRORS_ASYNC File=QCE$ERROR.:E05TSI                       WED 07/30/97 05:10 Page:106  
      642    20262    1   QCE$ERROR_TRAP: ENTRY ASYNC;
      643    20263
      644    20264        /*D*  NAME:         QCE$ERROR_TRAP
      645    20265              CALL:         CALL QCE$ERROR_TRAP;
      646    20266              PARAMETERS:   None.
      647    20267              INTERFACE:    Entered asynchronously.
      648    20268              ENVIRONMENT:  Asynchronous.
      649    20269              INPUT:        None.
      650    20270              OUTPUT:       None.
      651    20271              SCRATCH:      None.
      652    20272              DESCRIPTION:  Set up as the entry to call if a trap
      653    20273                            occurs; call to M$TRAP is in QAT$TALLYHO.
      654    20274        */
      655    20275
      656    20276    1       IF G$JIT$ -> G$JIT.DB# > 0
      657    20277    1       THEN
      658    20278    1   THE_DEBUGGERS_ASSOCIATED:
      659    20279    2         DO;
      660    20280                /*  If the debugger's associated, set QI_VLP_XBREAK.IC$ to:
      661    20281                    1)  QI_1.INSTR$ if it's the user's fault;
      662    20282                    2)  P$ in the trap frame if it's our fault.
      663    20283                    Then call Carl.  */
      664    20284    2           QI_VLP_XBREAK.TRAP# = G$TCB$ -> G$TCB.STK$ -> G$EXCFR.TRLR.TRAP.TRAP#;
      665    20285
      666    20286    3           DO CASE ( QI_VLP_XBREAK.TRAP# );
      667    20287
      668    20288    3             CASE ( %G_C_CHAR#,
      669    20289    3             %G_C_DBZ# );
      670    20290    3               QI_VLP_XBREAK.IC$ = QI_1.INSTR$;
      671    20291
      672    20292    3             CASE ( ELSE );
      673    20293    3               QI_VLP_XBREAK.IC$ = G$TCB$ -> G$TCB.STK$ -> G$EXCFR.P$;
      674    20294
      675    20295    3           END;
      676    20296
      677    20297    2           CALL QCE$EXPERIENCE_M$XBREAK;
      678    20298
PL6.E3A0      #002=QCE$ERRORS_ASYNC File=QCE$ERROR.:E05TSI                       WED 07/30/97 05:10 Page:107  
      679    20299    2         END THE_DEBUGGERS_ASSOCIATED;
      680    20300    1       ELSE
      681    20301    1   NO_DEBUGGER:
      682    20302    2         DO;
      683    20303    3           DO CASE ( G$TCB$ -> G$TCB.STK$ -> G$EXCFR.TRLR.TRAP.TRAP# );
      684    20304
      685    20305    3             CASE ( %G_C_CHAR# );
      686    20306                    /*  Commercial Illegal Character:
      687    20307                        assume (?) it's the user's fault.  */
      688    20308    3               CALL QCE$ERROR ( %E$COMMERCIAL_ILLEGAL_CHARACTER );
      689    20309
      690    20310    3             CASE ( %G_C_DBZ# );
      691    20311                    /*  Commercial Divide By Zero:
      692    20312                        assume (?) it's the user's fault.  */
      693    20313    3               CALL QCE$ERROR ( %E$COMMERCIAL_DIVIDE_BY_ZERO );
      694    20314
      695    20315    3             CASE ( ELSE );
      696    20316                    /*  Oops.  */
      697    20317    3               CALL QCE$ERROR_M$INTERNAL ( %E$INTERPRETER_TRAP );
      698    20318
      699    20319    3           END;
      700    20320    2         END NO_DEBUGGER;
      701    20321
      702    20322    1   END QCE$ERRORS_ASYNC;
      703    20323        %EOD;

