      298    21259
      299    21260    2         CASE ( %E$FILE_ALREADY_OPEN );
      300    21261    2           Q@FPABT.MSGS.ERR (1) = VLP_ERRCODE_QBO;
      301    21262    2           Q@FPABT.MSGS.U1 (1) = FILE_IDENTIFIER$ -> QI$VARIABLE_IDENTIFIER;
