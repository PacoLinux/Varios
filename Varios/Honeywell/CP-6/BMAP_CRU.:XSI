!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY,COUACCT=:E04OU,XACCT=:XSI,UIACCT=NO
!JOB NAME=BMAP_CRU,WSN=LOCAL
!RESOURCE MEM=256,TIME=20
!ROUTE Tom Martin, LADC
!PL6 BMAP_SI64.SIACCT,BMAP_SI64.UIACCT OVER *BMAP_OU64.GNACCT,LP (NSYS,SR(.LBACCT),OPTUI)
!BMAP BMAP_SIG.SIACCT,BMAP_SIG.UIACCT OVER *BMAP_OUG.GNACCT,LP (SR(.LBACCT),XR)
!LINK *BMAP_OU64.GNACCT,*BMAP_OUG.GNACCT ;
       OVER *BMAP_LINKOP.GNACCT (LODCB=M$EO,REPORT=0,SEV=11)
!*BMAP_LINKOP.GNACCT BMAP_DA1.SIACCT,BMAP_DA2.SIACCT OVER *BMAP_SI62.SIACCT,BMAP_C.GNACCT
!RESET
!PL6 *BMAP_SI62.SIACCT OVER *BMAP_OU62.GNACCT,LP (NSYS,SR(.LBACCT))
!SET M$SYSTEM *BMAP_IA.GNACCT
!PL6 BMAP_SI63.SIACCT,BMAP_SI63.UIACCT OVER ,LP (CSYS,NOU,SR(.SIACCT,.LBACCT,.COUACCT,.GNACCT),OPTUI)
!PL6 BMAP_SI61.SIACCT,BMAP_SI61.UIACCT OVER *BMAP_OU61.GNACCT,LP (SR(.SIACCT,.LBACCT,.GNACCT),OPTUI)
!RESET M$SYSTEM
!PARTRGE BMAP_SIN1.SIACCT,BMAP_SIN1.UIACCT OVER *BMAP_OUN.GNACCT,LP (NSYS,SR(.SIACCT,.GNACCT),OPTUI)
!DELETE BMAP_C.GNACCT
!LINK B_COPYRIGHT.LBACCT, *BMAP_OU61.GNACCT,;
       *BMAP_OU62.GNACCT,;
       *BMAP_OUG.GNACCT,;
       *BMAP_OUN.GNACCT ;
     OVER BMAP.GNACCT (PROCACC,REPORT=0,IP=100,DP=10)
!COPY BMAP_SIH1.SIACCT TO ME(K=E)
