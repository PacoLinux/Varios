    7      %EQU MAXMINOR = 31;  /* number of simultaneous connections */
    8      %EQU MAXMAJOR = 111; /* number of clients plus 10*number of servers */
    9      %EQU NUMSRVR = 8;  /* maximum number of servers */
   10      %EQU NUMCLNT = 31; /* maximum number of clients */

