        8        8        %SET LISTSUB='1'B;
        9        9        /*F* NAME: DDS$INSTR
       10       10             PURPOSE: TO ASSEMBLE THE INSTRUCTION POINTED TO BY NODE "INSTR".
       11       11             DESCRIPTION: PICKS UP EACH FIELD & ASSEMBLES IT INTO THE INSTRUCTION. */
       12       12        /*D* NAME: DDS$INSTR
       13       13             CALL: CALL DDS$INSTR( N$ ) ALTRET( ERROR );
       14       14             INTERFACE: SYNTAX TREE FOR DELTA.
       15       15             ENVIRONMENT:
       16       16             INPUT: N$ - POINTER TO NODE "INSTR".
       17       17                    S_D.D$ - POINTER TO INSTRUCTION TEMPLATE.
       18       18             OUTPUT: S_D.D$ - POINTER TO NEXT WORD IN BUILD BUFFER.
       19       19                     S_D.IGNPR - IGNORE PR MODIFICATION FLAG
       20       20                     S_D.AWDX - AWDX TYPE PR MODIFICATION FLAG
       21       21             SCRATCH: S_D.ACCUM
       22       22             DESCRIPTION: PICKS UP EACH FIELD, I.E., OP CODE, INHIBIT INDICATOR,
       23       23             TAG, ADDRESS REGISTER, AND ASSEMBLES THEM IN A GMAP INSTRUCTION TEMPLATE
       24       24             LOCATED AT INS$. THE ADDRESS FIELD OF THE INSTRUCTION IS ENCODED AS
       25       25             A DATA DESCRIPTOR, IMMEDIATELY FOLLOWING THE INSTRUCTION TEMPLATE.
       26       26             ANY FORMAT SPECIFICATION IS HANDLED BY THE CALLING ROUTINE. THE RESULATANT
       27       27             DATA DESCRIPTOR FOR THE ADDRESS MUST BE AN EVALUATABLE ITEM. RETURNS
       28       28             UNLESS AN ERROR CONDITION ENCOUNTERED, THEN ALTRETURNS. */
       29       29        /**/
       30       30        DDS$INSTR: PROC( N$ ) ALTRET;
       31       31        /**/
       32       32    1   DCL N$ PTR;                             /* "INSTR" NODE */
       33       33    1   DCL INS$ PTR;                           /* INSTRUCTION TEMPLATE POINTER */
       34       34    1   DCL INSDD$ PTR;                         /* BEGINNING OF DATA DESCRIPTOR */
       35       35    1   DCL IX SBIN;                            /* LOOP INDEX */
       36       36    1   DCL NC$ PTR;                            /* CURRENT NODE POINTER */
       37       37    1   DCL TEMP SBIN;                          /* TEMP FOR INTERMEDIATE RESULTS */
PL6.E3A0      #001=DDS$INSTR File=DDS$INSTR.:E05TSI                              TUE 07/29/97 14:17 Page:2    
       38       38    1   DCL TAG SBIN;                           /* TAG FOR INTERMEDIATE TAG VALUES */
       39       39    1   DCL VREF_HERE BIT(1) ALIGNED;           /* VREF ENCOUNTERED FLAG */
       40       40    1   DCL GOTZERO BIT(1) ALIGNED;             /* Got ZERO op code */
       41       41    1   DCL TARGET_TEXT CHAR(6);
       42       42        %INCLUDE DD$BASED;
