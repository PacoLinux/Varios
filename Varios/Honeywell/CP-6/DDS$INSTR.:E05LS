PL6.E3A0      #001=DDS$INSTR File=DDS$INSTR.:E05TSI                              TUE 07/29/97 14:17 Page:9    
      273     9485        /* EVAL_VAL - evaluates a "TAG" or "TAGx" node returning a UBIN */
      274     9486        /*    value in the variable TEMP.                               */
      275     9487    1   EVAL_VAL: PROC ALTRET;
      276     9488
      277     9489    2   DCL N$ PTR;
      278     9490
      279     9491    2        N$ = NC$;
      280     9492    2   RECASE:;
      281     9493    3        DO CASE( N$ -> POB.CODE );
      282     9494    3          CASE( 220 );               /* RI MODIFICATION */
      283     9495    3             TEMP = N$->POB.SUBLK$(0)->POB.CODE - 224 + BITBIN( '20'O );
      284     9496    3          CASE( 221 );               /* R MODIFICATION */
      285     9497    3             TEMP = N$->POB.SUBLK$(0)->POB.CODE - 224;
      286     9498    3          CASE( 222 );               /* IR MODIFICATION */
      287     9499    3             TEMP = N$->POB.SUBLK$(0)->POB.CODE - 224 + BITBIN( '60'O );
      288     9500    3          CASE( 223 );               /* IT MODIFICATION */
      289     9501    3             TEMP = N$->POB.SUBLK$(0)->POB.CODE - 224 + BITBIN( '40'O );
      290     9502    3          CASE( 8 );                /* DEC_CODE */
      291     9503    3             CALL CHARBIN (TEMP, N$ -> POS.TEXT);
      292     9504    3          CASE( 27 );                /* OCT_CODE */
      293     9505    3             CALL DDS$OCTAL (N$, TEMP) ALTRET (ALT);
      294     9506    3          CASE( ELSE );                    /* not there yet */
      295     9507    3             N$ = N$ -> POB.SUBLK$(0);
      296     9508    3             GOTO RECASE;
      297     9509    3          END;                             /* do case (N$ -> POB.CODE) */
      298     9510    2        RETURN;
      299     9511        /**/
      300     9512    2   ALT: ALTRETURN;
      301     9513        /**/
      302     9514    2   END EVAL_VAL;
      303     9515        /**/
      304     9516        %EJECT;
PL6.E3A0      #001=DDS$INSTR File=DDS$INSTR.:E05TSI                              TUE 07/29/97 14:17 Page:10   
      305     9517        /* EVAL_MF - evaluates an "EIS_MFx" node returning the MF field.
      306     9518        */
      307     9519    1   EVAL_MF: PROC ALTRET;
      308     9520
      309     9521    2   DCL NCC$ PTR;
      310     9522    2   DCL IX SBIN;
      311     9523
      312     9524    2        MF_FIELD = '0'B;
      313     9525    2        NCC$ = NC$;
      314     9526    3        DO IX = 0 TO NCC$ -> POB.NSB - 1;
      315     9527    3             NC$ = NCC$ -> POB.SUBLK$(IX);
      316     9528    4             DO CASE( NC$ -> POB.CODE );
      317     9529    4               CASE( 244 );           /* AR MODIFICATION */
      318     9530    4                  CALL EVAL_VAL ALTRET (ALT);
      319     9531    4                  IF TEMP > 1 THEN GOTO TOOBIG;
      320     9532    4                  MF_FIELD.AR = TEMP;
      321     9533    4               CASE( 245 );           /* RL MODIFICATION */
      322     9534    4                  CALL EVAL_VAL ALTRET (ALT);
      323     9535    4                  IF TEMP > 1 THEN GOTO TOOBIG;
      324     9536    4                  MF_FIELD.RL = TEMP;
      325     9537    4               CASE( 246 );           /* ID MODIFICATION */
      326     9538    4                  CALL EVAL_VAL ALTRET (ALT);
      327     9539    4                  IF TEMP > 1 THEN GOTO TOOBIG;
      328     9540    4                  MF_FIELD.ID = TEMP;
      329     9541    4               CASE( 247 );           /* REG MODIFICATION */
      330     9542    4                  CALL EVAL_VAL ALTRET (ALT);
      331     9543    4                  IF TEMP > 15 THEN GOTO TOOBIG;
      332     9544    4                  MF_FIELD.REG = TEMP;
      333     9545    4               CASE( 69 );         /* COMMA_CODE - ignore it */
      334     9546    4               END;                        /* do case (NC$ -> POB.CODE) */
      335     9547    3           END;                            /* do IX = ... */
      336     9548    2        RETURN;
      337     9549        /**/
      338     9550    2   TOOBIG: S_D.ERROR = 46;
      339     9551    2   ALT: ALTRETURN;
      340     9552        /**/
      341     9553    2   END EVAL_MF;
