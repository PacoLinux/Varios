        6         6             /* append t to s
        7         7             */
        8         8             char *
        9         9             strcat(s, t)
       10        10              register char *s, *t;
       11        11             {
       12        12    1         register char *b = s;
       13        13    1
       14        14    1         while (*s++)
       15        15    1            ;
       16        16    1         s--;
       17        17    1         while (*s++ = *t++)
       18        18    1            ;
       19        19    1         return b;
       20        20    1        }
       21        21

