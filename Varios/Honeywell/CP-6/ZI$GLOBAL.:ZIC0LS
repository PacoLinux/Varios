   413 - %MACRO DBCAL(NAME=,EXC=,ALT=);
   414 - %ERROR(LENGTHC(TEXTCHAR(NAME))=BITBIN('0'B),'SUBROUTINE NAME MISSING');
   415 - %ERROR(LENGTHC(TEXTCHAR(ALT))=BITBIN('0'B),'ALTRET LABEL MISSING');
   416 -         DO;
   417 -            CALL NAME ALTRET(ALT);
   418 -            IF GLOBAL.RET.ERROR#~=0 THEN
   419 -                %IF LENGTHC(TEXTCHAR(EXC))=BITBIN('0'B);
   420 -               GOTO ALT;
   421 -             %ELSE;
   422 -            DO;
   423 -               GLOBAL.RET.ERROR#=0;
   424 -               GOTO EXC;
   425 -               END;
   426 -             %ENDIF;
   427 - END;
   428 - %MEND;
   429 -          %MACRO DBEXC;
   430 -         GLOBAL.RET.ERROR#=ERROR;
   431 -         RETURN;
   432 - %MEND;
   433 -          %MACRO DBALT(AC=0,SC=0,RC=0,FR=0);
   434 -         IF GLOBAL.RET.ERROR#=0 THEN
   435 -         DO;
   436 -            GLOBAL.RET.AC#=AC;
   437 -            GLOBAL.RET.SC#=SC;
   438 -            GLOBAL.RET.RC#=RC;
   439 -            GLOBAL.RET.FR#=FR;
   440 -            GLOBAL.RET.ERROR#=ERROR;
   441 -            END;
   442 -         IF GLOBAL.RET.ERROR#<100000 THEN
   443 -            ALTRETURN;
   444 -         ELSE
   445 -            CALL ZIB$ERR;
   446 - %MEND;
