%MACRO STATS(NAME=STATS,STCLASS="BASED(STATS$)");
%IF TEXTCHAR(NAME)='STATS';
%SUB LAST#RPT#FCN=50;
%ENDIF;
DCL 1 NAME STCLASS ALIGNED,
      2 TSTAMP UBIN(36) UNAL,           /* Time statistics started            */
      2 VALID BIT(36) UNAL,
      2 REQS UBIN(36) UNAL,             /* # of times buffer req              */
      2 HITS UBIN(36) UNAL,             /* # of times buffer in               */
      2 COUNT(0:60),                    /* Individual counters                */
        3 CALLS UBIN(36) UNAL,          /* # of calles                        */
        3 READS UBIN(36) UNAL,          /* # of reads                         */
        3 WRITES UBIN(36) UNAL,         /* # of writes                        */
        3 ENQS UBIN(36) UNAL,           /* # of enq's issued                  */
        3 LOCKS UBIN(36) UNAL;          /* # of deadlocks                     */
%MEND;

FILE=ZI$GLOBAL                     STATS
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
                                 0!TSTAMP                                                                 !W
                                  I-----------------I-----------------I-----------------I-----------------I
                                 1!VALID                                                                  !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 2!REQS                                                                   !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 3!HITS                                                                   !
                                  I-----------------I-----------------I-----------------I-----------------I
COUNT                            4!CALLS                                                                  !B
                                  I-----------------I-----------------I-----------------I-----------------I
COUNT                            5!READS                                                                  !
                                  I-----------------I-----------------I-----------------I-----------------I
COUNT                            6!WRITES                                                                 !
                                  I-----------------I-----------------I-----------------I-----------------I
COUNT                            7!ENQS                                                                   !
                                  I-----------------I-----------------I-----------------I-----------------I
COUNT                           10!LOCKS                                                                  !
                                  I-----------------I-----------------I-----------------I-----------------I
                                   ARRAY:  '75'O ENTRIES TOTAL.
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO TRCE_SUBS;
%SUB TR#ENTRY=1;
%SUB TR#PREAD=2;
%SUB TR#PWRITE=3;
%SUB TR#PMOD=4;
%SUB TR#RSTO=5;
%SUB TR#RDEL=6;
%SUB TR#RMOD=7;
%SUB TR#RACC=8;
%SUB TR#ISTO=9;
%SUB TR#IDEL=10;
%SUB TR#IMOD=11;
%SUB TR#IREAD=12;
%SUB TR#PREADENQ=13;
%SUB TR#PMODENQ=14;
%SUB TR#PDEQ=15;
%SUB TR#ISTOENQ=16;
%SUB TR#IDELENQ=17;
%SUB TR#IMODENQ=18;
%SUB TR#IREADENQ=19;
%SUB TR#IDEQ=20;
%SUB TR#PDEQALL=21;                     /* WHEN AN IDS AREA IS DEQED          */
%SUB TR#IDEQALL=22;
%MEND;
%MACRO DBCAL(NAME=,EXC=,ALT=);
%ERROR(LENGTHC(TEXTCHAR(NAME))=BITBIN('0'B),'SUBROUTINE NAME MISSING');
%ERROR(LENGTHC(TEXTCHAR(ALT))=BITBIN('0'B),'ALTRET LABEL MISSING');
        DO;
           CALL NAME ALTRET(ALT);
           IF GLOBAL.RET.ERROR#~=0 THEN
               %IF LENGTHC(TEXTCHAR(EXC))=BITBIN('0'B);
              GOTO ALT;
            %ELSE;
           DO;
              GLOBAL.RET.ERROR#=0;
              GOTO EXC;
              END;
            %ENDIF;
END;
%MEND;
         %MACRO DBEXC;
        GLOBAL.RET.ERROR#=ERROR;
        RETURN;
%MEND;
         %MACRO DBALT(AC=0,SC=0,RC=0,FR=0);
        IF GLOBAL.RET.ERROR#=0 THEN
        DO;
           GLOBAL.RET.AC#=AC;
           GLOBAL.RET.SC#=SC;
           GLOBAL.RET.RC#=RC;
           GLOBAL.RET.FR#=FR;
           GLOBAL.RET.ERROR#=ERROR;
           END;
        IF GLOBAL.RET.ERROR#<100000 THEN
           ALTRETURN;
        ELSE
           CALL ZIB$ERR;
%MEND;
%MACRO MEMMOV;
DCL MEM_SIZE UBIN(36);
DCL 1 MEM_PAGES BASED ALIGNED,          /* MEM PAGE TEMPLATE                  */
      2 LOCN CHAR(MEM_SIZE);
%MEND;

