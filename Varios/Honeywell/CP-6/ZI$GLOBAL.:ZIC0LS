FRI. SEP 05 '97 11:18:01.84  FILE=ZI$GLOBAL

%MACRO GLOBALS(STCLASS=SYMREF READONLY);
DCL 1 GLOBAL BASED (GLOBAL$) DALIGNED,
      2 ACTIVE,     /* information associated with the active interaction     */
        3 SSS$ PTR,                /* active schema sub-set ptr being used    */
        3 SSS REDEF SSS$,
          4 * BIT (24) UNAL,
          4 ID BIT (12) UNAL,           /* seg id containing active SSS$      */
        3 DBCB$ PTR,          /* 'in process' data base control block ptr     */
        3 SEG REDEF DBCB$,
          4 * BIT (24) UNAL,
          4 ID BIT (12) UNAL,           /* seg id containing active dbcb      */
        3 DBNO SBIN,               /* 'in process' database segment index     */
      2 CANSSS$ PTR,          /* pointer to canned schema sub-set in global.  */
      2  FCN  UNAL,                     /* FUNCTION CODE OF CALL              */
        3 *  BIT(17) UNAL,              /*Not used*/
        3 DML BIT(1) UNAL,              /* If yes then standard DML call      */
        3 CODE UBIN(18) UNAL,
      2  ETC,                           /* ETC pending information            */
        3 ADR UBIN(18) UNAL,            /* Re-entry address                   */
        3 DBNO UBIN(18) UNAL,           /* DB no of pending ETC               */
      2  RET,                           /* RET STATUS                         */
        3 ERROR# UBIN(36) UNAL,         /* ERROR CODE                         */
        3 SYSERR# BIT(36) UNAL,         /* CP-6 error code                    */
        3 AC# UBIN(36) UNAL,            /* AC OFFSET                          */
        3 FR# UBIN(36) UNAL,            /* FR OFFSET                          */
        3 SC# UBIN(36) UNAL,            /* SC OFFSET                          */
        3 RC# UBIN(36) UNAL,            /* RC OFFSET                          */
        3 DCB# UBIN(18) UNAL,           /* DCB NUMBER                         */
        3 STCD# UBIN(18) UNAL,          /* STATMENT CODE                      */
        3 PAGE# UBIN(36) UNAL,          /* Page number for IO errors          */
      2 INHIB,
        3 * BIT(34) UNAL,
        3 DUMP BIT(1) UNAL,
        3 BREAK BIT(1) UNAL,            /* For break control                  */
      2 PENDING,
        3 RELEASE BIT(1) UNAL,          /* Release all context                */
        3 SHARE UBIN(2) UNAL,           /* Share mode for opens               */
        3 * BIT(1) UNAL,                /* USED TO BE PENDING.TRACE           */
        3 STATS BIT(1) UNAL,            /* Stats on or off                    */
        3 RECVRY BIT(1) UNAL,           /* Recovery in progress               */
        3 XCON BIT(1) UNAL,             /* Xcon in progress                   */
        3 SHANY BIT(1) UNAL,            /* No enques on reads                 */
        3 ROLL BIT(1) UNAL,             /* Rollback in progress               */
        3 EXIT_DML BIT(1) UNAL,         /* Exit when DML complete             */
        3 CURONLY BIT(1) UNAL,          /* CURRENCY ONLY                      */
        3 FORGET BIT(1) UNAL,           /* Forget about buffers for FLUSH     */
        3 ROLL_ABORTED BIT(1) UNAL,
        3 * BIT(23) UNAL,
      2 PARAN,
        3 IO,
          4 AWS$ PTR,                   /* AWS Pointer                        */
          4 PGNO UBIN(36) UNAL,         /* Page number for I/O                */
          4 BUF$ PTR,                   /* Pointer to CP6 page                */
        3 ICKSM,
          4 P$ PTR,                     /* Pointer to page                    */
          4 V UBIN(36) UNAL,            /* Checksum value                     */
        3 RLCW,                         /* Retention list control word        */
          4 OFFSET UBIN(18) UNAL,
          4 A BIT(1) UNAL,              /* Retain Area                        */
          4 S BIT(1) UNAL,              /*        Set                         */
          4 R BIT(1) UNAL,              /*        Record                      */
          4 * BIT(7) UNAL,
          4 M UBIN(8) UNAL,             /* List size                          */
        3 CALLPM0$ PTR,                 /* param for GMAP to PL6 calls        */
        3 GAP1(0:7) UBIN(36) UNAL,
        3 DBFSE,
          4 TYPE UBIN(18) UNAL,
          4 ID UBIN(18) UNAL,
          4 SSOFST UBIN(18) UNAL,
          4 WSOFST UBIN(18) UNAL,
        3 DBPR2,
          4 TYPE UBIN(18) UNAL,         /* Record type                        */
          4 SC UBIN(18) UNAL,
          4 RS UBIN(18) UNAL,           /* Resultant RS                       */
          4 RC UBIN(18) UNAL,           /* Resultant RC                       */
        3 DBXIC,
          4 AC$ PTR,                    /* AC pointer                         */
          4 RWD SBIN(36) UNAL,          /* READ=1,WRITE=2,DELETE=3            */
        3 TRACE,
          4 DCB# UBIN(36) UNAL,
          4 RNAME CHAR(30) UNAL,
          4 PDCB UBIN(18) UNAL,
          4 PNO UBIN(36) UNAL,
          4 RDBK UBIN(36) UNAL,
          4 TYPE UBIN(18) UNAL,
          4 RTYPE UBIN(18) UNAL,
          4 MODE,
            5 ENTRY# BIT(1) UNAL,
            5 PAGE# BIT(1) UNAL,
            5 RUPD# BIT(1) UNAL,
            5 RACC# BIT(1) UNAL,
            5 INDEX# BIT(1) UNAL,
            5 * BIT(31) UNAL,
          4 UOPT(0:35) REDEF MODE BIT(1) UNAL,
        3 REM_RECOVER REMEMBER ALIGNED,
        3 GAP2(0:7) UBIN WORD UNAL,
        3 IDSERR BIT(36) UNAL,
      2 DB,                             /* Database Dependent Information     */
        3 COUNT UBIN(36) UNAL,          /* Number of active DB's              */
        3 SSNAME(0:5) CHAR(30),         /* Subschema name                     */
        3 SSS$(0:5) PTR,                /* Schema Subset                      */
        3 DBCB$(0:5) PTR,               /* Data Base Control Block            */
        3 FLAGS(0:5),
          4 SYSSUB BIT(1) UNAL,         /* System Subschema                   */
          4 NOTRANS BIT(1) UNAL,        /* No translation check               */
          4 PCKD BIT(1) UNAL,           /* Privacy checked                    */
          4 * BIT(33) UNAL,
      2  BCB,                           /* Buffer Control Block               */
        3  HEAD UBIN(36) UNAL,          /* Most recently used buffer          */
        3  TAIL UBIN(36) UNAL,          /* Least recently used buffer         */
        3  COUNT UBIN(36) UNAL,         /* Number of buffers in pool          */
        3  SLOT SBIN(36) UNAL,          /* SLOT FOUND FOR A PAGE              */
        3  LASTP$ PTR,                  /* Last page not in pool              */
        3 BUF(0:128),                   /* buffer dependent info              */
          4  BUF$ PTR,                  /* Pointer to buffer                  */
          4  PGNO UBIN(36) UNAL,        /* Page number in buffer              */
          4  AWS$ PTR,                  /* Pointer to AWS                     */
          4  LINK UBIN(18) UNAL,        /* To more recently used              */
          4 BLINK UBIN(18) UNAL,        /* BACKWRAD BUFFER LINK               */
          4 LOCK  UBIN(18) UNAL,        /* LOCK COUNT                         */
          4 STATUS,                     /* INHIBITED ACCESS                   */
            5 BUSY BIT(1) UNAL,         /* I/O IN PROGRESS                    */
            5 MODIFY BIT(1) UNAL,       /* PAGE HAS BEEN MODIFIED             */
            5 * BIT(1) UNAL,            /* FOR LATER ENHANCEMENT              */
          4 ENQINDX UBIN(15) UNAL,      /* POINTS TO ENQ LIST                 */
/*      ENQ should be a total of 12 words. */
      2 ENQ,
        3 SHARE UBIN(9) CALIGNED,
/*B*        SHARE - Type of ENQ being requested. */
        3 CURONLY BIT(1) UNAL,
/*B*        CURONLY - Currencies only on some area. */
        3 * BIT(8) UNAL,
        3 VALUE,
/*B*            VALUE - The value of the resource to be locked. */
          4 DCB# UBIN(18) HALIGNED,
/*B*            DCB# - The DCB# of the resource being locked. */
          4 RNAME UBIN(36) HALIGNED,
/*B*            RNAME - The resource name being locked. */
        3 LIST,
          4 BASE$ PTR HALIGNED,
/*B*            BASE$ - Base of ENQLIST structure. */
          4 INDX UBIN(18) HALIGNED,
/*B*            INDX - Index to current entry. */
          4 COUNT UBIN(18) HALIGNED,
/*B*            COUNT - Number of entries in list. */
          4 MAXCOUNT UBIN(18) HALIGNED,
/*B*            MAXCOUNT - Maximum count given memory allocated. */
        3 INDXMAP HALIGNED,
          4 INDX UBIN(18) HALIGNED,
/*B*            INDX - Index to current entry. */
          4 COUNT UBIN(18) HALIGNED,
/*B*            COUNT - Number of entries in list. */
          4 MAXCOUNT UBIN(18) HALIGNED,
/*B*            MAXCOUNT - Maximum count given memory allocated. */
          4 BASE$ PTR HALIGNED,
/*B*            BASE$ - Base of INDXMAP structure. */
        3 *(0:4) UBIN WORD,
      2  STATS UNAL,
        3 CURR,                         /* For current call only              */
          4 REQS UBIN(36) UNAL,         /* # of times buffer req              */
          4 HITS UBIN(36) UNAL,         /* # of times buf in this             */
          4 READS UBIN(36) UNAL,        /* Read count                         */
          4 WRITES UBIN(36) UNAL,       /* Write counts                       */
          4 ENQS SBIN,                  /* Current ENQ count                  */
          4 LOCKS SBIN,                 /* Current deadlock count             */
        3 DBCS,                         /* DBCS statistics (see template)     */
          4 * UBIN(36) UNAL,
          4 * BIT(36) UNAL,
          4 * UBIN(36) UNAL,
          4 * UBIN(36) UNAL,
          4 *(0:60),
            5 * BIT(36),
            5 * BIT(36),
            5 * BIT(36),
            5 * BIT(36),
            5 * BIT(36),
        3 USER,                         /* User statistics (see template)     */
          4 * UBIN(36) UNAL,
          4 * BIT(36) UNAL,
          4 * UBIN(36) UNAL,
          4 * UBIN(36) UNAL,
          4 *(0:60),
            5 * BIT(36),
            5 * BIT(36),
            5 * BIT(36),
            5 * BIT(36),
            5 * BIT(36),
        3 DCB# UBIN(36) UNAL,
/* Journal related information */
      2 TJRNL,
        3 DCB# UBIN(36) UNAL,
        3 FIDCOUNT UBIN(36) UNAL,       /* # of FID records in jrnl           */
        3 UNCHECK BIT(1) UNAL,          /* TJRNL NOT IN CHECKPOINT            */
        3 * BIT(35) UNAL,
        3 * BIT(36) UNAL,
      2 CJRNL,
        3 DCB# UBIN(36) UNAL,
      2 CKPTNO UBIN(36) UNAL,           /* Implicit ckpt number               */
      2 ROLLADDR UBIN(36) UNAL,         /* Address to roll back to            */
      2 UPDATE,
        3 TJRNL UBIN(18) UNAL,          /* # open Update journal              */
        3 NOTJRNL UBIN(18) UNAL,        /* # open Update no journal           */
      2 CAC UBIN(36) UNAL,              /* # open with CAC active             */
      2 SHANY UBIN(36) UNAL,            /* # of users open SHANY              */
      2 GAP3(0:2) UBIN(36) UNAL,
      2 FPTCHKR_CODE UBIN(36) UNAL, /* Indicate condition found by FPTCHKR    */
      2 JRECORD,
        3 *(0:1023) UBIN WORD UNAL,
/* FPT related information.  All framed by some vectors */
      2 GRANULE SBIN,
      2 DCBNUMBER SBIN,
      2 KEY,
        3 LENGTH UBIN (9) UNAL,
        3 VALUE CHAR(255) UNAL,
      2 SKEY REDEF KEY,                 /* DBCB.KEYPA$ points here            */
        3 LENGTH UBIN(9) UNAL,
        3 IXID UBIN(18) UNAL,
        3 VALUE CHAR(253) UNAL,
      2 FILENAME,
        3 LENGTH UBIN(9) UNAL,
        3 TEXT CHAR(31) UNAL,
      2 DCBNAME,
        3 LENGTH UBIN(9) UNAL,
        3 TEXT CHAR(31) UNAL,
      2  ACCT CHAR(8),
      2  ERRFIELD_INSERT(0:2),
        3 LENGTH UBIN(9) UNAL,
        3 TEXT CHAR(31) UNAL,
      2 RNAME,                          /* Resource name  E/D                 */
        3 LENGTH UBIN(9) UNAL,          /* TEXTC                              */
        3 VALUE UBIN(36) UNAL,          /* Page no. for E/D                   */
        3 * BIT(27) UNAL,               /* Round to word                      */
      2 ENCRP BIT(36) UNAL,             /* Encription seed                    */
      2 TIME,
        3 TSTAMP UBIN(36) UNAL,         /* UTS from clock                     */
        3 DATE CHAR(10) UNAL,
        3 TIME CHAR(11) UNAL,
        3 DAY CHAR(3) UNAL,
      2 STYPE BIT(36) UNAL,
      2 KEYID,                          /* Two word key ID                    */
        3 * BIT(36) UNAL,               /* Count                              */
        3 RECID UBIN(36) UNAL,          /* Unique record ID                   */
      2 PASSWORD CHAR(8) UNAL,
      2 GAP4(0:7) UBIN WORD UNAL,
      2 PRINTBUF,                       /* Print buffer for general use       */
        3 BUFFER CHAR(132),
        3 LINE (0:131) REDEF BUFFER CHAR(1),
        3 ERRMSG REDEF BUFFER CHAR(132),
        3 ERR REDEF BUFFER,
          4 TITLE CHAR(12) UNAL,
          4 V,
            5 VALUE CHAR(6) UNAL,
            5 * CHAR(114) UNAL,
          4 VAR REDEF V CHAR(120) UNAL,
          4 VCHAR(0:119) REDEF V CHAR(1) UNAL,
          4 V1 REDEF V UNAL,
            5 DBST CHAR(7) UNAL,
            5 * CHAR(113) UNAL,
      2 OPEN$ PTR ALIGNED,
      2 OPNDFLT$ PTR ALIGNED,
      2 OPNSCRB$ PTR ALIGNED,
      2 REOPEN$ PTR ALIGNED,
      2 CLOSE$ PTR ALIGNED,
      2 READD$ PTR ALIGNED,
      2 WRTD$ PTR ALIGNED,
      2 READI$ PTR ALIGNED,
      2 WRTI$ PTR ALIGNED,
      2 READK$ PTR ALIGNED,
      2 WRTK$ PTR ALIGNED,
      2 DELR$ PTR ALIGNED,
      2 PREC$ PTR ALIGNED,
      2 CHECK$ PTR ALIGNED,
      2 GETDCB$ PTR ALIGNED,
      2 RELDCB$ PTR ALIGNED,
      2 GDS$ PTR ALIGNED,
      2 FDS$ PTR ALIGNED,
      2 ENQ$ PTR ALIGNED,
      2 DEQALL$ PTR ALIGNED,
      2 VLP$ PTR ALIGNED,
      2 WRITE$ PTR ALIGNED,
      2 PFIL$ PTR ALIGNED,
      2 WRTJ$ PTR ALIGNED,
      2 READJ$ PTR ALIGNED,
      2 TIME$ PTR ALIGNED,
      2 TIMEC$ PTR ALIGNED,
      2 TRUNC$ PTR ALIGNED,
      2 ERRMSG$ PTR ALIGNED,
      2 ENQS$ PTR ALIGNED,
      2 PRECJ$ PTR ALIGNED,
      2 KEYIN$ PTR ALIGNED,
      2 MONINFO$ PTR ALIGNED,
      2 TRTN$ PTR ALIGNED,
      2 SCREECH$ PTR ALIGNED,
      2 PRECI$ PTR ALIGNED,
      2 DELI$ PTR ALIGNED,
      2 ERR$ PTR ALIGNED,
      2 ALTKEYS$ PTR ALIGNED,
      2 DEQI$ PTR ALIGNED,
      2 ENQI$ PTR ALIGNED,
      2 DEQ$ PTR ALIGNED,
      2 GAP5(0:9) UBIN WORD UNAL,
/*  The actual fpt's associated with the above pointers begin here */
      2 FPTBASE DALIGNED,
        3 * SBIN;
%SUB DB#MAX="(LENGTHW(GLOBAL.DB.SSS$)/LENGTHW(GLOBAL.DB.SSS$(0)))-1";
DCL GLOBAL$ PTR STCLASS;

FILE=ZI$GLOBAL                     GLOBAL
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
ACTIVE                           0!SSS$                                                                   !W
                                  I-----------------I-----------------I-----------------I-----------------I
ACTIVE    SSS                    0!*                                              !ID                     !B
                                  I-----------------I-----------------I-----------------I-----------------I
ACTIVE                           1!DBCB$                                                                  !
                                  I-----------------I-----------------I-----------------I-----------------I
ACTIVE    SEG                    1!*                                              !ID                     !B
                                  I-----------------I-----------------I-----------------I-----------------I
ACTIVE                           2!DBNO                                                                   !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 3!CANSSS$                                                                !
                                  I-----------------I-----------------I-----------------I-----------------I
FCN                 DML          4!*                                !@!CODE                               !B
                                  I-----------------I-----------------I-----------------I-----------------I
ETC                              5!ADR                                !DBNO                               !B
                                  I-----------------I-----------------I-----------------I-----------------I
RET                              6!ERROR#                                                                 !B
                                  I-----------------I-----------------I-----------------I-----------------I
RET                              7!SYSERR#                                                                !
                                  I-----------------I-----------------I-----------------I-----------------I
RET                             10!AC#                                                                    !
                                  I-----------------I-----------------I-----------------I-----------------I
RET                             11!FR#                                                                    !
                                  I-----------------I-----------------I-----------------I-----------------I
RET                             12!SC#                                                                    !
                                  I-----------------I-----------------I-----------------I-----------------I
RET                             13!RC#                                                                    !
                                  I-----------------I-----------------I-----------------I-----------------I
RET                             14!DCB#                               !STCD#                              !
                                  I-----------------I-----------------I-----------------I-----------------I
RET                             15!PAGE#                                                                  !
                                  I-----------------I-----------------I-----------------I-----------------I
INHIB               DUMP        16!*                                                                  !@!.!B
                                  I-----------------I-----------------I-----------------I-----------------I
INHIB               BREAK       16!. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .!@!
                                  I-----------------I-----------------I-----------------I-----------------I

