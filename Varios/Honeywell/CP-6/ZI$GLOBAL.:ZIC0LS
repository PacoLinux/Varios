   452 - %MACRO ENQLIST;
   453 - DCL 1 ENQLIST(0:0) BASED (ENQLIST$) ALIGNED,
   454 -       2 STATUS,
   455 -         3 KEEP BIT(1) UNAL,
   456 - /*B*        KEEP - Set if user did an IDSKEEP call. */
   457 -         3 CURRENT BIT(1) UNAL,
   458 - /*B*        CURRENT - Used during currency only checks. */
   459 -         3 MODIFY BIT(1) UNAL,
   460 - /*B*        MODIFY - Set if pages has been modified. */
   461 -         3 * BIT(6)  UNAL,
   462 -       2 BUFSLOT UBIN BYTE CALIGNED,
   463 - /*B*    BUFSLOT - Mapping into buffer pool.  Zero if not in. */
   464 -       2 VALUE,
   465 - /*B*        VALUE - The value actually ENQed. */
   466 -         3 DCB# UBIN HALF HALIGNED,
   467 -         3 RNAME UBIN WORD HALIGNED;
   468 - DCL ENQLIST$ PTR;
   469 - %MEND;
   470 - %MACRO INDXMAP;
   471 - DCL 1 INDXMAP(0:0) BASED(INDXMAP$) ALIGNED,
   472 -       2 STATUS,
   473 -         3 KEEP BIT(1) UNAL,
   474 - /*B*        KEEP - Set if user did an IDSKEEP call. */
   475 -         3 CURRENT BIT(1) UNAL,
   476 - /*B*        CURRENT - Used during currency only checks. */
   477 -         3 MODIFY BIT(1) UNAL,
   478 - /*B*        MODIFY - Set if record has been modified. */
   479 -         3 * BIT(6) UNAL,
   480 -       2 ENQINDX UBIN(27) CALIGNED,
   481 - /*B*    ENQINDEX - Index into ENQLIST for this entry. */
   482 -       2 RECID UBIN WORD ALIGNED;
   483 - /*B*    RECID - The record id of the indexed record. */
   484 - DCL INDXMAP$ PTR;
