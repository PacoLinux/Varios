DCL GLOBAL$ PTR STCLASS;
DCL ZIB$ERR ENTRY;
%MEND;
%MACRO BUFFERS;
%SUB BUF#MAX="LENGTHW(GLOBAL.BCB.BUF)/LENGTHW(GLOBAL.BCB.BUF(0))-1";
%SUB LOCK#MAX=7/*Max*/;            /* max # times a single buf can be locked  */
%SUB KEY#ATTR=0/*Attr*/;                /* key (granule #) of attribute block */
DCL 1 BLOCK ALIGNED BASED,         /* file mgmt block size for ids file org   */
      2 PSTAMP UBIN(36) UNAL,           /* Page stamp                         */
      2 TSTAMP UBIN(36) UNAL,           /* Time stamp                         */
      2 CKSM UBIN(36) UNAL,             /* Checksum                           */
      2 ACTSZ UBIN(18) UNAL,            /* Active page size                   */
      2 SPAV UBIN(18) UNAL,             /* Space available                    */
      2 PGNO UBIN(36) UNAL,             /* Page number                        */
      2 AVLN UBIN(18) UNAL,             /* Available line no                  */
      2 HILN UBIN(18) UNAL,             /* High line number                   */
      2 * (0:1017) UBIN;                /* Data                               */
%MEND;

FILE=ZI$GLOBAL                     BLOCK
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
                                 0!PSTAMP                                                                 !W
                                  I-----------------I-----------------I-----------------I-----------------I
                                 1!TSTAMP                                                                 !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 2!CKSM                                                                   !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 3!ACTSZ                              !SPAV                               !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 4!PGNO                                                                   !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 5!AVLN                               !HILN                               !
                                  I-----------------I-----------------I-----------------I-----------------I
                                 6!*                                                                      !
                                  I-----------------I-----------------I-----------------I-----------------I
                                   ARRAY:'1772'O ENTRIES TOTAL.
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO RECCW;
DCL 1 RECCW BASED,
      2 TYPE UBIN(18) UNAL,             /* Record type                        */
      2 SIZE UBIN(18) UNAL;             /* Record size                        */
%MEND;

FILE=ZI$GLOBAL                     RECCW
