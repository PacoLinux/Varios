%MACRO ADSC(STCLASS=BASED);
DCL 1 ADSC STCLASS ALIGNED,
      2 REG UBIN (3) UNAL,
      2 OFFSET UNAL,
        3 Y UBIN (15) UNAL,
        3 CN UBIN(2) UNAL,
      2 * BIT(1) UNAL,
      2 TA UBIN (2) UNAL,
      2 * BIT (1) UNAL,
      2 N UBIN (12) UNAL;
%SUB ADSC9#=0/*ADSC9#*/;
%MEND;

