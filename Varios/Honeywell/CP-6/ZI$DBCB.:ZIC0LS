%MACRO RECVAR(STCLASS=BASED);
DCL 1 RECVAR STCLASS ALIGNED, /* record variable witin primitve call block*/
      2 DBKEY SBIN (36) UNAL,           /* data base key of record            */
      2 BADDR,                          /* address of buffer contining record */
        3 OFFSET UBIN (18) UNAL,        /* word offset of record              */
        3 * UBIN (18) UNAL,
      2 BADDR$ REDEF BADDR PTR,
      2 RADDR,                          /* address of the record              */
        3 OFFSET UBIN (18) UNAL,        /* word offset of record              */
        3 * UBIN(2) UNAL,               /* byte number within word            */
        3 * UBIN (16) UNAL,
      2 RADDR$ REDEF RADDR PTR,
      2 RECSZ UBIN (18) UNAL,
      2 RTP,
        3 CONTROL,
          4 * BIT(2) UNAL,
          4 LD BIT(1) UNAL,             /* True = logicaly deleted record     */
          4 * BIT(3) UNAL,
        3 TYPE UBIN (12) UNAL,
      2 SCPTR UBIN (18) UNAL,
      2 RSPTR UBIN (18) UNAL,
      2 ACPTR UBIN (18) UNAL,
      2 RAPTR UBIN (18) UNAL,
      2 RCPTR UBIN (18) UNAL,
      2 * UBIN (18) UNAL,
      2 BLKD SBIN (36) UNAL,            /* lock indicator (0=unlocked)        */
      2 *(0:2) SBIN(36) UNAL,           /* Unused in CP-6                     */
      2 AWS$ PTR UNAL,                  /* Area work space pointer            */
      2 PA UNAL,
        3 CBRCE UBIN(18) UNAL,          /* PA ENTRY OFFSET FOR C-O-S          */
        3 CBRCP UBIN(18) UNAL,          /* PA PTR OFFSET FOR C-O-S            */
        3 CBRPA UBIN(36) UNAL,          /* DBK FOR PA REC GIVING ENTRY OF REC */
      2 * (0:1) SBIN (36) UNAL;         /*  reserved                          */
%MEND;

