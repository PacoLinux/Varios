   508 -         3 CHKSUM BIT(1) UNAL,           /* CHECKSUM REQUIREDE                 */
   509 -         3 SHARE UBIN(2) UNAL,           /* Share mode                         */
   510 -         3 SHANY BIT(1) UNAL,            /* Share any.  Dirty retreval         */
   511 -         3 * BIT(1) UNAL,
   512 -         3 CANSSS BIT(1) UNAL,           /* Canned schema subset               */
   513 -         3 ATMP BIT(1) UNAL,             /* Temporary Area                     */
   514 -         3 FUN UBIN(2) UNAL,             /* Function mode                      */
   515 -         3 INDEXED BIT(1) UNAL,          /* Area is Indexed org                */
   516 -         3 KEYED BIT(1) UNAL,            /* Area has alternate keys            */
   517 -         3 LOAD BIT(1) UNAL,             /* Load mode.  No keys                */
   518 -         3 CAC BIT(1) UNAL,              /* Concurrent access control          */
   519 -         3 UNPROTECT BIT(1) UNAL,
   520 -       2 * UBIN(17) UNAL,
   521 -       2 CURR SBIN (36) UNAL,            /* Dbk - current of realm             */
   522 -       2 NCHA UBIN(36) UNAL,             /* # of calc headers area             */
   523 -       2 HIGHDBK UBIN(36) UNAL,          /* High DBK in area                   */
   524 -       2 HIGHPG UBIN(36) UNAL,           /* High page in area                  */
   525 -       2 FIPN SBIN(36) UNAL,             /* First inventory page               */
   526 -       2 DDCB UBIN(18) UNAL,             /* DCB# for data area                 */
   527 -       2 KDCB UBIN(18) UNAL,             /*  "    "  alternate key area        */
   528 -       2 ENCRP BIT(36) UNAL,             /* Encription seed                    */
   529 -       2 * BIT(36) UNAL,
   530 - %IF ~AWSTYPE;
   531 -       2 * BIT(36) UNAL;
   532 - %ELSE;
   533 -       2 * BIT(36) UNAL,
   534 -       2 KEY,                            /* Only present if indexed area*/
   535 -         3 LENGTH UBIN(9) UNAL,          /* Length of primary key              */
   536 -         3 VALUE CHAR(AWS.KEY.LENGTH);   /* Key value                          */
   537 - %ENDIF;
   538 - %MEND;
   539 - %MACRO RWS;
   540 - DCL RWS$ PTR;
   541 - DCL 1 RWS BASED(RWS$) ALIGNED,
   542 -       2 PRIV,
   543 -         3 * UBIN(9) UNAL,
   544 -         3 PG BIT(1) UNAL,               /* get                                */
   545 -         3 PM BIT(1) UNAL,               /* modify                             */
   546 -         3 PS BIT(1) UNAL,               /* store                              */
   547 -         3 PE BIT(1) UNAL,               /* erase                              */
   548 -         3 PC BIT(1) UNAL,               /* connect                            */
   549 -         3 PD BIT(1) UNAL,               /* disconnect                         */
   550 -         3 PF BIT(1) UNAL,               /* find                               */
   551 -         3 * UBIN(2) UNAL,
   552 -       2 * UBIN(18) UNAL,
   553 -       2 CURR SBIN(36) UNAL,             /* DBKEY current of type              */
   554 -       2 KEY,                            /* Only present if indexed area       */
   555 -         3 LENGTH UBIN(9) UNAL,          /* Length of primary key              */
   556 -         3 VALUE CHAR(RWS.KEY.LENGTH);   /* Key value                          */
   557 - %SUB RWS#PRIV#ALL    ='000774'O/*RWS#PRIV#ALL*/;
   558 - %MEND;
   559 - %MACRO SETWS;
   560 - DCL SETWS$ PTR;
   561 - DCL 1 SETWS BASED(SETWS$) ALIGNED,
   562 -       2 PRIV,
   563 -         3 CSE BIT(1) UNAL,              /* current was erased                 */
   564 -         3 * UBIN(8) UNAL,
   565 -         3 PSO BIT(1) UNAL,              /* order                              */
   566 -         3 * UBIN(3) UNAL,
   567 -         3 PSC BIT(1) UNAL,              /* connect                            */
   568 -         3 PSD BIT(1) UNAL,              /* disconnect                         */
   569 -         3 PSF BIT(1) UNAL,              /* find                               */
   570 -         3 * UBIN(2) UNAL,
   571 -       2 AC UBIN(18) UNAL,               /* AC offset if for key currency      */
   572 -       2 CURR SBIN (36) UNAL,            /* Dbk - current of set               */
   573 -       2 KEY,
   574 -         3 LENGTH UBIN (9) UNAL,         /* Length of secondary key*/
   575 -         3 VALUE CHAR(SETWS.KEY.LENGTH); /* IXID Key value and local DBKEY     */
   576 - %SUB SETWS#PRIV#ALL    ='000434'O/*SETWS#PRIV#ALL*/;
   577 - %MEND;
   578 - %MACRO SETWSPA;
   579 - DCL 1 SETWSPA BASED(SETWS$) ALIGNED,    /* SET WORK SPACE FOR PA SETS         */
   580 -       2 PRIV,
   581 -         3 CSE BIT(1) UNAL,              /* Current was erased                 */
   582 -         3 * UBIN(8) UNAL,
   583 -         3 PSO BIT(1) UNAL,              /* order                              */
   584 -         3 * UBIN(3) UNAL,
   585 -         3 PSC BIT(1) UNAL,              /* Connect                            */
   586 -         3 PSD BIT(1) UNAL,              /* Disconnect                         */
   587 -         3 PSF BIT(1) UNAL,              /* Find                               */
   588 -         3 * BIT(2) UNAL,
   589 -       2 * UBIN(18) UNAL,
   590 -       2 CURR SBIN(36) UNAL,             /* Dbk - Current of set               */
   591 -       2 PA,
   592 -         3 CBENO UBIN(18) UNAL,          /* PA entry offset                    */
   593 -         3 CBCPO UBIN(18) UNAL,          /* PA pointer offset                  */
   594 -         3 CBCPA UBIN(36) UNAL;          /* DBK OF CURRENT PA REC              */
   595 - %MEND;
   596 - %MACRO FWS;
   597 - DCL FWS$ PTR;
   598 - DCL 1 FWS BASED(FWS$) ALIGNED,
   599 -       2 PRIV,
   600 -         3 * UBIN(9) UNAL,
   601 -         3 PFG BIT(1) UNAL,              /* get                                */
   602 -         3 PFM BIT(1) UNAL,              /* modify                             */
   603 -         3 PFS BIT(1) UNAL,              /* store                              */
   604 -         3 * UBIN(6) UNAL,
   605 -       2 * UBIN(18) UNAL;
   606 - %SUB FWS#PRIV#ALL    ='000700'O/*FWS#PRIV#ALL*/;
   607 - %MEND;
   608 - %MACRO RECVAR(STCLASS=BASED);
   609 - DCL 1 RECVAR STCLASS ALIGNED, /* record variable witin primitve call block*/
   610 -       2 DBKEY SBIN (36) UNAL,           /* data base key of record            */
   611 -       2 BADDR,                          /* address of buffer contining record */
   612 -         3 OFFSET UBIN (18) UNAL,        /* word offset of record              */
   613 -         3 * UBIN (18) UNAL,
   614 -       2 BADDR$ REDEF BADDR PTR,
   615 -       2 RADDR,                          /* address of the record              */
   616 -         3 OFFSET UBIN (18) UNAL,        /* word offset of record              */
   617 -         3 * UBIN(2) UNAL,               /* byte number within word            */
   618 -         3 * UBIN (16) UNAL,
   619 -       2 RADDR$ REDEF RADDR PTR,
   620 -       2 RECSZ UBIN (18) UNAL,
   621 -       2 RTP,
   622 -         3 CONTROL,
   623 -           4 * BIT(2) UNAL,
   624 -           4 LD BIT(1) UNAL,             /* True = logicaly deleted record     */
   625 -           4 * BIT(3) UNAL,
   626 -         3 TYPE UBIN (12) UNAL,
   627 -       2 SCPTR UBIN (18) UNAL,
   628 -       2 RSPTR UBIN (18) UNAL,
   629 -       2 ACPTR UBIN (18) UNAL,
   630 -       2 RAPTR UBIN (18) UNAL,
   631 -       2 RCPTR UBIN (18) UNAL,
   632 -       2 * UBIN (18) UNAL,
   633 -       2 BLKD SBIN (36) UNAL,            /* lock indicator (0=unlocked)        */
   634 -       2 *(0:2) SBIN(36) UNAL,           /* Unused in CP-6                     */
   635 -       2 AWS$ PTR UNAL,                  /* Area work space pointer            */
   636 -       2 PA UNAL,
   637 -         3 CBRCE UBIN(18) UNAL,          /* PA ENTRY OFFSET FOR C-O-S          */
   638 -         3 CBRCP UBIN(18) UNAL,          /* PA PTR OFFSET FOR C-O-S            */
   639 -         3 CBRPA UBIN(36) UNAL,          /* DBK FOR PA REC GIVING ENTRY OF REC */
   640 -       2 * (0:1) SBIN (36) UNAL;         /*  reserved                          */
   641 - %MEND;
