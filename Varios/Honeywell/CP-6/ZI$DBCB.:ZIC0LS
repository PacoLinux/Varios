%MACRO SETWSPA;
DCL 1 SETWSPA BASED(SETWS$) ALIGNED,    /* SET WORK SPACE FOR PA SETS         */
      2 PRIV,
        3 CSE BIT(1) UNAL,              /* Current was erased                 */
        3 * UBIN(8) UNAL,
        3 PSO BIT(1) UNAL,              /* order                              */
        3 * UBIN(3) UNAL,
        3 PSC BIT(1) UNAL,              /* Connect                            */
        3 PSD BIT(1) UNAL,              /* Disconnect                         */
        3 PSF BIT(1) UNAL,              /* Find                               */
        3 * BIT(2) UNAL,
      2 * UBIN(18) UNAL,
      2 CURR SBIN(36) UNAL,             /* Dbk - Current of set               */
      2 PA,
        3 CBENO UBIN(18) UNAL,          /* PA entry offset                    */
        3 CBCPO UBIN(18) UNAL,          /* PA pointer offset                  */
        3 CBCPA UBIN(36) UNAL;          /* DBK OF CURRENT PA REC              */
%MEND;

