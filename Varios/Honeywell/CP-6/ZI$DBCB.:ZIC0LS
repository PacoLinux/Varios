%MACRO DBSTK;
DCL 1 DBCBSTK BASED ALIGNED,            /* Find/erase stack entry structure.  */
      2 OFFSET UBIN (18) UNAL,
      2 * UBIN (18) UNAL,
      2 DBK SBIN (36) UNAL,
      2 ALT REDEF DBK,
        3 OFFSET UBIN (18) UNAL,
        3 * UBIN (18) UNAL;
%MEND;

FILE=ZI$DBCB                       DBCBSTK
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
                                 0!OFFSET                             !*                                  !W
                                  I-----------------I-----------------I-----------------I-----------------I
                                 1!DBK                                                                    !
                                  I-----------------I-----------------I-----------------I-----------------I
ALT                              1!OFFSET                             !*                                  !B
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO CRUNI;
DCL 1 CRUNI BASED ALIGNED,              /* Current of run for indexed         */
      2 LENGTH UBIN(9) UNAL,
      2 VALUE CHAR(CRUNI.LENGTH);       /* Primary key value                  */
%MEND;

FILE=ZI$DBCB                       CRUNI
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
                                 0!LENGTH           !VALUE            !# # # # # # # # # # # # # # # # # #!W
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO AWS(AWSTYPE(INDEXED='1'B,INTEGRATED='0'B)='0'B);
DCL AWS$ PTR;                      /* pointer to area work space in the dbcb  */
DCL 1 AWS BASED(AWS$) ALIGNED,     /* An area work space entry of the dbcb    */
      2 ATRIB,
        3 CAE BIT(1) UNAL,              /* CURRENT OF AREA ERASED             */
        3 CURONLY BIT(1) UNAL,          /* ENQ ON CURRENCY ONLY               */
        3 CJRNL BIT(1) UNAL,            /* Journaling required                */
        3 TMPLT BIT(1) UNAL,            /* Area is Template                   */
        3 TJRNL BIT(1) UNAL,            /* Common Journaling required         */
        3 CHKSUM BIT(1) UNAL,           /* CHECKSUM REQUIREDE                 */
        3 SHARE UBIN(2) UNAL,           /* Share mode                         */
        3 SHANY BIT(1) UNAL,            /* Share any.  Dirty retreval         */
        3 * BIT(1) UNAL,
        3 CANSSS BIT(1) UNAL,           /* Canned schema subset               */
        3 ATMP BIT(1) UNAL,             /* Temporary Area                     */
        3 FUN UBIN(2) UNAL,             /* Function mode                      */
        3 INDEXED BIT(1) UNAL,          /* Area is Indexed org                */
        3 KEYED BIT(1) UNAL,            /* Area has alternate keys            */
        3 LOAD BIT(1) UNAL,             /* Load mode.  No keys                */
        3 CAC BIT(1) UNAL,              /* Concurrent access control          */
        3 UNPROTECT BIT(1) UNAL,
      2 * UBIN(17) UNAL,
      2 CURR SBIN (36) UNAL,            /* Dbk - current of realm             */
      2 NCHA UBIN(36) UNAL,             /* # of calc headers area             */
      2 HIGHDBK UBIN(36) UNAL,          /* High DBK in area                   */
      2 HIGHPG UBIN(36) UNAL,           /* High page in area                  */
      2 FIPN SBIN(36) UNAL,             /* First inventory page               */
      2 DDCB UBIN(18) UNAL,             /* DCB# for data area                 */
      2 KDCB UBIN(18) UNAL,             /*  "    "  alternate key area        */
      2 ENCRP BIT(36) UNAL,             /* Encription seed                    */
      2 * BIT(36) UNAL,
%IF ~AWSTYPE;
      2 * BIT(36) UNAL;
%ELSE;
      2 * BIT(36) UNAL,
      2 KEY,                            /* Only present if indexed area*/
        3 LENGTH UBIN(9) UNAL,          /* Length of primary key              */
        3 VALUE CHAR(AWS.KEY.LENGTH);   /* Key value                          */
%ENDIF;
%MEND;

