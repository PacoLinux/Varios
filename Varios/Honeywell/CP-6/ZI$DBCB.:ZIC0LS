%MACRO RWS;
DCL RWS$ PTR;
DCL 1 RWS BASED(RWS$) ALIGNED,
      2 PRIV,
        3 * UBIN(9) UNAL,
        3 PG BIT(1) UNAL,               /* get                                */
        3 PM BIT(1) UNAL,               /* modify                             */
        3 PS BIT(1) UNAL,               /* store                              */
        3 PE BIT(1) UNAL,               /* erase                              */
        3 PC BIT(1) UNAL,               /* connect                            */
        3 PD BIT(1) UNAL,               /* disconnect                         */
        3 PF BIT(1) UNAL,               /* find                               */
        3 * UBIN(2) UNAL,
      2 * UBIN(18) UNAL,
      2 CURR SBIN(36) UNAL,             /* DBKEY current of type              */
      2 KEY,                            /* Only present if indexed area       */
        3 LENGTH UBIN(9) UNAL,          /* Length of primary key              */
        3 VALUE CHAR(RWS.KEY.LENGTH);   /* Key value                          */
%SUB RWS#PRIV#ALL    ='000774'O/*RWS#PRIV#ALL*/;
%MEND;

