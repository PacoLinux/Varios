%MACRO SETWS;
DCL SETWS$ PTR;
DCL 1 SETWS BASED(SETWS$) ALIGNED,
      2 PRIV,
        3 CSE BIT(1) UNAL,              /* current was erased                 */
        3 * UBIN(8) UNAL,
        3 PSO BIT(1) UNAL,              /* order                              */
        3 * UBIN(3) UNAL,
        3 PSC BIT(1) UNAL,              /* connect                            */
        3 PSD BIT(1) UNAL,              /* disconnect                         */
        3 PSF BIT(1) UNAL,              /* find                               */
        3 * UBIN(2) UNAL,
      2 AC UBIN(18) UNAL,               /* AC offset if for key currency      */
      2 CURR SBIN (36) UNAL,            /* Dbk - current of set               */
      2 KEY,
        3 LENGTH UBIN (9) UNAL,         /* Length of secondary key*/
        3 VALUE CHAR(SETWS.KEY.LENGTH); /* IXID Key value and local DBKEY     */
%SUB SETWS#PRIV#ALL    ='000434'O/*SETWS#PRIV#ALL*/;
%MEND;

