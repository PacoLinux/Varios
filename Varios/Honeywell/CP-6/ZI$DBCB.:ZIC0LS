FRI. SEP 05 '97 11:17:17.96  FILE=ZI$DBCB

%MACRO   DBCB;
DCL DBCB$ PTR;
DCL 1 DBCB BASED(DBCB$) ALIGNED,        /* Storage daligned at run time       */
/*                                                                  */
/*             Control section equivalences                         */
/*                                                                  */
      2 CSECT,
        3 SSACCT CHAR(8) UNAL,          /* Subschema account                  */
        3 * SBIN(36) UNAL,
        3 ATBL,                         /*Addr of open area table             */
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (18) UNAL,
        3 TBUF$ PTR,                    /* Pointer to temp rec buf            */
        3 * UBIN (18) UNAL,
        3 * BIT(18) UNAL,
        3 * SBIN (36) UNAL,
        3 DBCS CHAR(4) UNAL,
        3 KEYPA$ PTR,
        3 IXBUF$ PTR,                   /* Pointer to indexed buffer          */
        3 *(0:1) SBIN WORD UNAL,
        3 CBFSZ UBIN (18) UNAL,         /*Size of dbcb + fact                 */
        3 CIWSOF UBIN(18) UNAL,
        3 CFSA SBIN (36) UNAL,          /*Options     <ehw>                   */
        3 PABUF SBIN (36) UNAL,         /*Address of pa buffer                */
        3 * UBIN WORD,
/*                                                                  */
/*             Subroutine work space                                */
/*                                                                  */
      2 SWS,
        3 ARGS UBIN (18) UNAL,          /* Address of args from call - all    */
        3 FMSBF UBIN (18) UNAL,         /* Addr of fms buf for tss            */
        3 TSI SBIN (36) UNAL,           /* Time-sharing indicator - all       */
        3 RVTAL SBIN (36) UNAL,         /* Reference vector tally - ready     */
        3 * BIT(36) UNAL,
        3 SCLST SBIN (36) UNAL,         /* Sc list control word - stor&find   */
        3 * SBIN(36) UNAL,
        3 CLNG UBIN (18) UNAL,          /* Length of calc key - calc          */
        3 EEIND UBIN (18) UNAL,    /* Erase-establish currency IND.-UPDT curr */
        3 HASH SBIN (36) UNAL,          /* Hash - calc                        */
        3 CDES SBIN (36) UNAL,          /* Alpha descr for a calc key - calc  */
        3 * UBIN (27) UNAL,
        3 CCHAR CHAR (1),               /* Curr char of calc key - calc       */
        3 DDBK SBIN (36) UNAL,          /* Direct data base key - store       */
        3 SSZTP,                        /*Size & type - store                 */
          4 * UBIN (6) UNAL,
          4 TYPE UBIN (12) UNAL,
          4 SIZE UBIN (18) UNAL,
        3 BSPG SBIN (36) UNAL,     /* Base page (dbk of base loc) - store     */
        3 CURRS,                        /*Current rs - store                  */
          4 CURRENT UBIN (18) UNAL,
          4 BACKOUT UBIN (18) UNAL,
        3 DELRS UBIN (18) UNAL,         /*Current rs - erase                  */
        3 UCRS UBIN (18) UNAL,          /* Current rs ptr - update currencies */
        3 CURSS UBIN (18) UNAL,         /*Current ss - find ass.              */
        3 RSREQ UBIN (18) UNAL,         /*Rs requested - find ass.            */
        3 CDBK SBIN (36) UNAL,          /* Current dbk - find ass.            */
        3 ROCUR SBIN (36) UNAL,         /*Rec occurrence no. - find a/r       */
        3 MODFR,                        /*Fr list ptr - modify                */
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (18) UNAL,
        3 MODSC,                        /*Sc list ptr - modify                */
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (18) UNAL,
        3 MODRS UBIN (18) UNAL,         /*Current rs - modify                 */
        3 MODIN UBIN (18) UNAL,         /*Including set indicator - modify    */
        3 MODCD UBIN (18) UNAL,         /*Current cd ptr - modify             */
        3 CONCD UBIN (18) UNAL,         /*Current cd - connect                */
        3 FLDTL,                   /*Tally & ind word for fr list- get fields */
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (18) UNAL,
        3 FCSS UBIN (18) UNAL,          /* Ss ptr for calc keys - find calc   */
        3 MRBKI UBIN (18) UNAL, /*New entry has been inserted indic - modify bo */
        3 CACD UBIN (18) UNAL,          /* Current cd ptr - calc              */
        3 CAAC UBIN (18) UNAL,          /* Current ac or zero - calc          */
        3 FCNT UBIN (18) UNAL,     /* No. Entries left in list - mv/cmp fld   */
        3 * UBIN (18) UNAL,
        3 CMPL UBIN (18) UNAL,          /* Current entry in list - mv/cmp fld */
        3 * UBIN(18) UNAL,
        3 DPSLW SBIN (36) UNAL,         /*No. Lines to slew - error routine   */
        3 FKIND SBIN (36) UNAL,    /*Find w/i current indicator - find ass.   */
        3 STMASK BIT (36) UNAL,         /*Size & type mask - all              */
        3 TMASK BIT (36) UNAL,          /*Type mask - all                     */
        3 CTMASK BIT (36) UNAL,         /*Compliment of type mask - all       */
        3 ITRS UBIN (18) UNAL,          /* Current rs ptr - if tests          */
        3 FTIND UBIN (18) UNAL,         /*First time indicator (.DBHIA)       */
        3 ITPTR SBIN (36) UNAL,         /*Nxt ptr from curr rec - if tests    */
        3 SUST,                         /* Subroutine status - all            */
          4 CURRENT UBIN (18) UNAL,
          4 BACKOUT UBIN (18) UNAL,
        3 * (0:5) SBIN UNAL,            /* No longer used                     */
        3 CSUB,                         /* Ptr to curr subr. Cntrs - all      */
          4 CTRPTR UBIN (18) UNAL,
          4 ELPTR UBIN (18) UNAL,
        3 RNGO SBIN (36) UNAL,          /* Length of range of owner - store   */
        3 RNGD SBIN (36) UNAL,          /* Displ. Of owner w/i range - store  */
        3 ODBK SBIN (36) UNAL,          /* Dbk to relink to - modify          */
        3 RDYCW BIT (36) UNAL,          /*Ready control word - ready          */
        3 READY REDEF RDYCW,
          4 * UBIN (9) UNAL,
          4 ER BIT(1) UNAL,             /* exclusive retrieval                */
          4 EU BIT(1) UNAL,             /* exclusive update                   */
          4 PR BIT(1) UNAL,             /* protected retrieval                */
          4 PU BIT(1) UNAL,             /* protected update                   */
          4 CR BIT(1) UNAL,             /* concurrent retrieval               */
          4 CU BIT(1) UNAL,             /* concurrent update                  */
          4 BM BIT(1) UNAL,             /* load mode                          */
          4 * UBIN (2) UNAL,
          4 NOAC UBIN (18) UNAL,        /* number of ac's                     */
        3 RDCNT SBIN (36) UNAL,         /*Neg. Of no. Of ac's - ready         */
        3 RDAC UBIN (18) UNAL,          /* Curr ac or ind. Ptr - ready        */
        3 ARGS2 UBIN (18) UNAL, /*Address of args from CALL(.DBEUP-FORTRAN)   */
        3 PKAW,               /* Rc ubin word, fr for area-id - find area     */
          4 RCPTR UBIN (18) UNAL,
          4 FRPTR UBIN (18) UNAL,
        3 FRAC UBIN (18) UNAL,     /* Curr ac for printing stats - finish     */
        3 OPADP UBIN (18) UNAL,         /*Old pa key duplicate indic.         */
        3 FICNT SBIN (36) UNAL,    /*Neg. Of no. Of ac's in list - finish     */
        3 FIAC UBIN (18) UNAL,     /* Curr ac or ptr to curr rv E.- finish    */
        3 UPPAI UBIN (18) UNAL,         /*Update pa ofsts indic. (.DBUPC)     */
        3 STBIN SBIN (36) UNAL,         /*Binary no. For stats - finish       */
        3 STDEC (0:1) SBIN (36) UNAL,   /*Decimal no. For stats - finish      */
        3 SNIND SBIN (36) UNAL,         /*Area-id name indicator - find area  */
        3 CDP UBIN (18) UNAL,           /* Current dp ptr - proc. Inter       */
        3 RSPAM UBIN (18) UNAL,         /*Curr rs ptr for pa member           */
        3 CAG,                          /* Curr ag - call db proc             */
          4 CURRENT UBIN (18) UNAL,
          4 LASTSTS UBIN (18) UNAL,
        3 CRA UBIN (18) UNAL,           /* Curr ra - find chr                 */
        3 * UBIN (18) UNAL,
        3 TEMP,                         /* Temporary storage                  */
          4 PTR1$ PTR ALIGNED,     /* for access to temp wd1 as a pointer     */
          4 WORD1 REDEF PTR1$ SBIN WORD, /* for access to temp wd1 as a word  */
          4 INAFLAG REDEF PTR1$ SBIN WORD, /* used during rdy & ina functions */
          4 PTR2$ PTR ALIGNED,     /* for access to temp wd2 as a pointer     */
          4 WORD2 REDEF PTR2$ SBIN WORD, /* for access to temp wd2 as a word  */
        3 CSTS,                         /* Curr stats ofst - finish           */
          4 OFFSET UBIN (18) UNAL,
          4 NAME CHAR (2),
/*                                                                  */
/*             Subroutine call counters                             */
/*                                                                  */
        3 STAT,
          4 CALLS(0:25),
            5 CALLED SBIN(36) UNAL,
            5 READ SBIN(36) UNAL,
            5 WROTE SBIN(36) UNAL,
        3 DBKSI SBIN (36) UNAL,         /*Dbk of curr si rec                  */
        3 REMLT,                   /*Remaining lock bits in lt & bind. Value  */
          4 TYPE UBIN (9) UNAL,
          4 LOCK UBIN (9) UNAL,
          4 BINDVAL UBIN (18) UNAL,
        3 CURLT UBIN (18) UNAL,         /*Current lock bits & bind. Value     */
        3 * UBIN (18) UNAL,
        3 CLI UBIN (18) UNAL,           /* Curr lock ind.                     */
        3 CWSOF UBIN (18) UNAL,         /*Curr work space ofst                */
        3 CURLN (0:2) SBIN (36) UNAL,   /*Current privacy key                 */
        3 VFDSC SBIN (36) UNAL,         /*Eis desc for varying length field   */
        3 FLDDSC SBIN (36) UNAL, /*Eis desc defining desc for varying length field */
        3 * UBIN(18) UNAL,
        3 * UBIN (18) UNAL,
        3 GACDK SBIN (36) UNAL,         /*Dbk to find ac for                  */
        3 PCEXI SBIN (36) UNAL,         /*Partial compare indicator           */
        3 FNIND SBIN (36) UNAL,         /*First/next in key indicator         */
        3 RETFR UBIN (18) UNAL,         /*Fr ptr for special regs             */
        3 * UBIN (18) UNAL,
        3 OWNPTR SBIN (36) UNAL,        /*Dbk of owner of pa set (backout)    */
        3 * (0:12) SBIN (36) UNAL,
/*                                                                  */
/*             Stack for erase                                      */
/*                                                                  */
      2 DBEST,
        3 CONTROL,
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (12) UNAL,
          4 DELTA UBIN (6) UNAL,
        3 REFRESH,
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (12) UNAL,
          4 DELTA UBIN (6) UNAL,
        3 STACK (0:61) SBIN (36) UNAL,
/*                                                                  */
/*             Stack for find                                       */
/*                                                                  */
      2 DBSTK,
        3 CONTROL,
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (12) UNAL,
          4 DELTA UBIN (6) UNAL,
        3 REFRESH,
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (12) UNAL,
          4 DELTA UBIN (6) UNAL,
        3 STACK (0:61) SBIN (36) UNAL,
/*                                                                  */
/*             Ic storage stack                                     */
/*                                                                  */
      2 DBIC,
        3 CONTROL,
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (12) UNAL,
          4 DELTA UBIN (6) UNAL,
        3 STACK (0:14) SBIN (36) UNAL,
/*                                                                  */
/*             Primitive call block                                 */
/*                                                                  */
      2 PCB,
        3 RVI UBIN (18) UNAL,           /* Rec var input ptr                  */
        3 * UBIN (18) UNAL,
        3 RVO UBIN (18) UNAL,           /* Rec var output ptr                 */
        3 CRUN UBIN (18) UNAL,          /* Current of run indicator.          */
        3 RVT UBIN (18) UNAL,           /* Rec var temp ptr                   */
        3 * UBIN (18) UNAL,
        3 RVPA UBIN (18) UNAL,          /* Rec var pointer array ptr          */
        3 * UBIN (18) UNAL,
        3 * (0:1) SBIN (36) UNAL,
        3 PRSS UBIN (18) UNAL,     /* Ss request - move/compare match keys    */
        3 CIND UBIN (18) UNAL,          /* Indic. Reg after compare           */
        3 PRRC UBIN(18) UNAL,           /* Record class request               */
        3 PRRS UBIN(18) UNAL,           /*RS parameter                        */
        3 PRSC UBIN (18) UNAL,          /* Set class request                  */
        3 PRCSE BIT (1) UNAL,           /*Bit 18: 1 = curr is prior           */
        3 * UBIN (17) UNAL,
        3 PRFR UBIN (18) UNAL,          /* Field request - move compare field */
        3 PRFR2 UBIN (18) UNAL,         /* Fr PTR(2ND OPER.) - comp. Fields   */
        3 PREXC SBIN (36) UNAL,    /*Exception code on normal return from     */
/*                               Ufas logical i/o index routines    */
        3 KCARG0 SBIN (36) UNAL,        /*AR0 save area for key comp rtn      */
        3 KCARG3 SBIN (36) UNAL,        /*AR3 save for key comp rtn           */
        3 KCARG4 SBIN (36) UNAL,        /*AR4 save for key comprtn            */
        3 PROPA SBIN (36) UNAL,         /*Dbk of owner of pa set              */
        3 * SBIN (36) UNAL,
        3 RV1 (0:15) SBIN (36) UNAL,    /* Record variable no. 1              */
        3 RV2 (0:15) SBIN (36) UNAL,    /* Record variable no. 2              */
        3 RV3 (0:15) SBIN (36) UNAL,    /* Record variable no. 3              */
        3 RV4 (0:15) SBIN (36) UNAL,    /* Record variable no. 4              */
/*                                                                  */
/*             Primitive work space                                 */
/*                                                                  */
      2 PWS,
        3 NPIND SBIN (36) UNAL,         /* Next/prior indicator               */
        3 NPOPTR SBIN (36) UNAL,        /* Next/prior/owner pointer   <trm>*/
        3 RSZTP,                        /* Record size/type                   */
          4 * BIT (2) UNAL,
          4 LGCDLT BIT (1) UNAL,
          4 TYPE UBIN (15) UNAL,
          4 SIZE UBIN (18) UNAL,
        3 BEF SBIN (36) UNAL,           /* Dbk of before rec - insert         */
        3 NEW SBIN (36) UNAL,           /* Dbk of new rec - insert            */
        3 AFT SBIN (36) UNAL,           /* Dbk of after rec - insert          */
        3 * SBIN(36) UNAL,              /* UNUSED IN CP-6                     */
        3 * SBIN (36),                  /* unused in CP-6, was dbkrq for gcos */
        3 TFLD SBIN (36) UNAL,          /* Temp field descr.                  */
        3 SKTYP UBIN (18) UNAL,         /* Rc ptr for sorted set              */
        3 PGSPLT UBIN (18) UNAL,        /*Ptr group split indicator           */
/*                               (nonzero = ptr group split occurred)*/
        3 PCD UBIN (18) UNAL,           /* Current cd ptr - move/comp. Keys   */
        3 PCD2 UBIN (18) UNAL,          /* Cd PTR(2ND OPER.) - comp. Fields   */
        3 SKIND SBIN (36) UNAL,         /* Sort key indicator                 */
        3 PRSTS SBIN (36) UNAL,         /*I/o status - find space             */
        3 SKFLG,                        /* Last compare flag                  */
          4 FIRST BIT (1) UNAL,    /* first compare was ... 0=next, 1=prior   */
          4 * UBIN (34) UNAL,
          4 LAST BIT (1) UNAL,     /* last compare was ... 0=next, 1=prior    */
        3 CHDBK SBIN (36) UNAL,         /*Dbk of calc hdr rec                 */
        3 IPC SBIN (36) UNAL,           /* Insert point in calc set           */
        3 IPCF SBIN (36) UNAL,          /* Insert point found indicator       */
        3 DBKSR SBIN (36) UNAL,         /*Dbk of starting record              */
        3 BN1 (0:1) SBIN (36) UNAL,     /* No. From temp buf - compare fields */
        3 BN2 (0:1) SBIN (36) UNAL,     /* No. From page buf - compare fields */
        3 VFLD SBIN (36) UNAL,          /* Eis desc for varying length field  */
        3 TFLD2 SBIN (36) UNAL,         /* Temp eis desc                      */
        3 RNGTAL,                       /*Range table tally - find space      */
          4 CURRENT BIT (36) UNAL,
          4 REFRESH BIT (36) UNAL,
        3 RNG1,                         /* Range 1 lo & hi - find space       */
          4 LOWDBK SBIN (36) UNAL,
          4 HIGHDBK SBIN (36) UNAL,
        3 RNG2,                         /* Range 2 lo & hi - find space       */
          4 LOWDBK SBIN (36) UNAL,
          4 HIGHDBK SBIN (36) UNAL,
        3 KEYDS SBIN (36) UNAL,         /*Desc for key in key process area    */
        3 UCADR SBIN (36) UNAL,         /*User compare address                */
        3 CFLPT1 (0:1) SBIN (36) UNAL, /*2 words for 1ST arg in float. Pt. Compare */
        3 CFLPT2 (0:1) SBIN (36) UNAL, /*2 words for 2ND arg in float. Pt. Compare */
        3 * UBIN (18) UNAL,
        3 USTAT UBIN (18) UNAL,    /*Ufas status code on alternate return     */
        3 PCMIN SBIN (36) UNAL,         /*Partial compare indicator           */
        3 TMPPA UBIN (18) UNAL,         /*Temp storage for pa routines        */
        3 XPRS UBIN (18) UNAL,          /* Rs ptr for index update primitives */
        3 DBICREFRESH, /* REFRESH for DBIC--in this section because there was room here */
          4 OFFSET UBIN (18) UNAL,
          4 COUNT UBIN (12) UNAL,
          4 DELTA UBIN (6) UNAL,
        3 KCREG (0:7) SBIN (36) UNAL,   /*Register save area for key comp rtn */
        3 ENTNO UBIN (18) UNAL,         /*Entry no. Of a pa member rec        */
        3 PTRNO UBIN (18) UNAL,         /*Ptr no. Of a pa member in a ptr grp */
        3 NOPTRS UBIN (18) UNAL,        /*No. Of ptrs in curr ptr grp         */
        3 ENTTYP UBIN (18) UNAL,        /*Type code from curr pa entry        */
        3 ENTPTR SBIN (36) UNAL,        /*Ptr from curr pa entry              */
        3 NXPRI UBIN (18) UNAL, /*Next/prior indic. For pa (neg=prior,pos=next) */
        3 EFIND UBIN (18) UNAL,    /*Entry found indic (0 = entry not found)  */
        3 PARCW SBIN (36) UNAL,         /*Pa record control word              */
        3 ULIM UBIN (18) UNAL,          /* Upper limit-used in binary search  */
        3 LOLIM UBIN (18) UNAL,         /*Lower limit-used in binary search   */
        3 CENTRY UBIN (18) UNAL,        /* Current pa entry                   */
        3 ENIND UBIN (18) UNAL,         /*Entry type indicator                */
        3 ENTSZ SBIN (36) UNAL,         /* Pa entry size                      */
        3 PTESZ SBIN (36) UNAL,         /*Size of entry in pa ptr grp         */
        3 UPACW SBIN (36) UNAL,         /*Pa control word-update prims        */
        3 CENTRN UBIN (18) UNAL,        /*Current pa entry no.                */
        3 CENTRO UBIN (18) UNAL,        /*Current pa entry offset             */
        3 CPTRN UBIN (18) UNAL,         /* Current pa ptr no.                 */
        3 CPTRO UBIN (18) UNAL,         /*Current pa ptr offset               */
        3 PTRESZ UBIN (18) UNAL,        /*Size of entry in pa ptr grp-update  */
        3 PASPLT UBIN (18) UNAL,   /*Pa rec split INDICATOR(1 = pa split)     */
        3 ESZ SBIN (36) UNAL,           /* Size of pa entry                   */
        3 NUMENT UBIN (18) UNAL,        /*No. Of entries in pa rec            */
        3 PARVPA UBIN (18) UNAL, /*Pa in pa rec var INDIC.(1 = not in pa rv)  */
        3 NUMPTR UBIN (18) UNAL,        /*No. Of ptrs in pa ptr grp           */
        3 MEMTYP UBIN (18) UNAL,        /*Member type and ptr indicator       */
        3 MEMPTR SBIN (36) UNAL,        /*Curr pa mem ptr or ofst to ptr grp  */
        3 PAOPTR SBIN (36) UNAL,        /* Owner of pa set                    */
        3 DEPCW SBIN (36) UNAL,         /*Pa control word-delete pa rtns      */
        3 PADBK SBIN (36) UNAL,         /* Pa dbk                             */
        3 PAEOF UBIN (18) UNAL,         /* Pa entry offset                    */
        3 PAPOF UBIN (18) UNAL,         /* Pa ptr offset                      */
        3 SNPAE UBIN (18) UNAL,         /*Space needed for new pa entry       */
        3 DUSEPA UBIN (18) UNAL,        /*Dont use pa set indicator           */
        3 RCW1 SBIN (36) UNAL,          /* Record control word for pa rec     */
        3 RCW2 SBIN (36) UNAL,          /* Pa control word for pa rec         */
        3 FETM UBIN (18) UNAL,     /* 1ST entry no. To be moved(split pa)     */
        3 PADUP SBIN (18) UNAL,         /*Pa duplicate indicator (0 = nondup, */
/*                            Neg = dup of prior, pos = dup of next) */
        3 OPARCW SBIN (36) UNAL,        /*Pa control word of old pa(split pa) */
        3 NPARCW SBIN (36) UNAL,        /*Pa control word of new pa(split pa) */
        3 SOE SBIN (36) UNAL,           /* Size of entry(split pa)            */
        3 SPCAVN UBIN (18) UNAL,        /*Space avail in new pa (split pa)    */
        3 SPCAVO UBIN (18) UNAL,        /*Space avail in old pa (split pa)    */
        3 PAEN UBIN (18) UNAL,          /* Pa entry NO.-DELETE pa rtns        */
        3 PAEO UBIN (18) UNAL,          /* Pa entry ofst-delete pa rtns       */
        3 PGSIZE SBIN (36) UNAL,        /*Size of ptr group to be deleted     */
        3 PAESZ SBIN (36) UNAL, /*Size of pa entry (used in delete ptr grp)   */
      2 IOWS,                           /* I/o workspace                      */
        3 BTX0 UBIN (18) UNAL,          /* X0 for rplx search of buffers      */
        3 LUPD UBIN (18) UNAL,     /* Bwa of buf last updated in act. Chain   */
        3 BEMT UBIN (18) UNAL,          /* Addr of bwa for empty buffer       */
        3 * UBIN (18) UNAL,
        3 ENDSR SBIN (36) UNAL,         /*Encode descriptor for uwa           */
        3 CBIB UBIN (18) UNAL,          /* Addr of bwa being set up           */
        3 * UBIN (18) UNAL,
        3 DBK (0:1) SBIN (36) UNAL,     /* Current data base key              */
        3 * UBIN (18) UNAL,             /* was acd (current ac def) in gcos   */
        3 IAAC UBIN (18) UNAL,          /* Ac def for area to init            */
        3 DCDSR SBIN (36) UNAL,         /*Decode descr. For fld in rec        */
        3 *(0:1) UBIN(36) UNAL,
        3 LNNO SBIN (36) UNAL,          /* Line no. Requested                 */
        3 * UBIN(36) UNAL,
        3 CREC SBIN (36) UNAL,          /* Current record offset              */
        3 SPND UBIN (18) UNAL,          /* Space needed        - find space   */
        3 SZREQ REDEF SPND UBIN (18) UNAL, /* Size requested - find space     */
        3 SPLF UBIN (18) UNAL,          /* Space left          find space     */
        3 FSOF SBIN (36) UNAL,          /* Current record offset - find space */
        3 FSLN SBIN (36) UNAL,          /* Current line no.    - find space   */
        3 CURPG SBIN (36) UNAL,         /*Current page        - init area     */
        3 NXTC SBIN (36) UNAL,          /* Next calc hdr. Rec  - init area    */
        3 CHRO UBIN (18) UNAL,          /* Next avail. Ofst    - init area    */
        3 * UBIN (18) UNAL,
        3 CHR BIT (36) UNAL,            /* Calc header record   - init area   */
        3 CTPAI SBIN (36) UNAL,    /*2 part area indicator- open/close area   */
        3 HIDB SBIN (36) UNAL,          /* Hi dbk for rec      - find space   */
        3 DBKR (0:1) SBIN (36),         /* Current of area - next/prior of ar */
        3 DBKF REDEF DBKR,
          4 * SBIN (36),
          4 RQ SBIN (36),               /* Dbk requested - find space         */
          4 AC REDEF RQ SBIN (36),      /*Dbk to find ac for - find ac        */
        3 CPGR,                    /* Curr page & line no. - next/prior of ar */
          4 PAGE SBIN (36) UNAL,
          4 LINE SBIN (36) UNAL,
        3 IOINC SBIN (36) UNAL,    /*Increment from current- next/prior of ar */
        3 ACREC SBIN (36) UNAL,    /*Rec ofst in page    - next/prior of ar   */
        3 CRD SBIN (36) UNAL,           /* Current read counter               */
        3 CWRT SBIN (36) UNAL,          /* Current write counter              */
        3 B32N SBIN (36) UNAL,     /* 32 bit binary no.   - encode/decode     */
        3 B36N SBIN (36) UNAL,     /* 36 bit binary no.   - encode/decode     */
        3 IALC SBIN (36) UNAL,          /* Line no. Of last chr - init area   */
        3 IACLN,                        /*Ofst & no. Of av. Ln - init area    */
          4 OFFSET UBIN (18) UNAL,
          4 AVAIL UBIN (18) UNAL,
        3 IANLN,                        /*Ofst & no. Nxt av ln - init area    */
          4 OFFSET UBIN (18) UNAL,
          4 AVAIL UBIN (18) UNAL,
        3 UBRD2 SBIN (36) UNAL,    /*Burid for find space,delete rec rtns     */
        3 LAS UBIN (18) UNAL,           /* Line array size in chars           */
        3 * UBIN (18) UNAL,
        3 * UBIN(36) UNAL,
        3 ACOPN UBIN(18) UNAL,          /* Ac of area to open/close           */
        3 * UBIN(18) UNAL,
        3 ACTP UBIN (18) UNAL,     /* Nxt av. Area tbl entry- open/close area */
        3 LNCNT UBIN (18) UNAL,         /*Line cnt-update ofsts in line array */
        3 FBUF BIT (36) UNAL,           /* ND4 for 4-BIT buf                  */
        3 FBUFU BIT (36) UNAL,          /*ND4 for 4-BIT buf(less sign)        */
        3 FBUFM BIT (36) UNAL,          /*ND4 for 4-BIT buf(msd & sign)       */
        3 FBUMD BIT (36) UNAL,          /*AD4 for 4-BIT buf(msd only)         */
        3 FBUF6 BIT (36) UNAL,          /*AD6 for 6-BIT buf                   */
        3 FBU61 BIT (36) UNAL,          /*AD6 for 6-BIT BUF(1ST char only)    */
        3 FBUF1 BIT (36) UNAL,          /*ND4 for 4-BIT buf(sign + 11 digits) */
        3 FBUF4 BIT (36) UNAL,          /*AD4 for 4-BIT buf                   */
        3 CBID UBIN (18) UNAL,          /* Current burid - inventory routines */
        3 ELO UBIN (18) UNAL, /* Even line offset-update array after page compact */
        3 TSFN (0:1) SBIN (36) UNAL,    /* File name for tss i/o              */
        3 CBRID UBIN (18) UNAL,    /*Current burid-used in retrieval rtns     */
        3 CAL UBIN (18) UNAL,           /* Curr avail. Line no. - find space  */
        3 PLO SBIN (36) UNAL,           /* Prior line ptr ofst - find space   */
        3 LSPC UBIN (18) UNAL,          /* Line array spc needed- find space  */
        3 UPACD UBIN (18) UNAL,    /*Ac ptr for find space,delete rec rtns    */
        3 LOL SBIN (36) UNAL,           /* Lo line no.         - find space   */
        3 HIL SBIN (36) UNAL,           /* Hi line no.         - find space   */
        3 NLN SBIN (36) UNAL,      /* Number of line nos allocated on page    */
        3 PACD UBIN (18) UNAL,          /* Ac ptr for I/0 routines            */
        3 CINVSZ UBIN (18) UNAL, /*    No. Invent. Units    - update inventory */
        3 CIPG SBIN (36) UNAL,     /* Curr invent. Page NO.- update inventory */
        3 CIAC UBIN (18) UNAL,          /* Ac ptr for current buffer          */
        3 INVOF UBIN (18) UNAL,    /*Byte ofst in inv page- update inventory  */
        3 CDPSI SBIN (36) UNAL,         /*Curr data page no.                  */
        3 LDPSI SBIN (36) UNAL,         /*Last data page no.                  */
        3 SNSI SBIN (36) UNAL,          /* No. Chars needed, iu's found       */
        3 POFSI SBIN (36) UNAL,         /*Ofst in curr invent. Page           */
        3 CIPSI SBIN (36) UNAL,         /*Curr invent. Page no.               */
        3 CIBSI SBIN (36) UNAL,         /*Addr of curr inv. Buf               */
        3 IOSTS SBIN (36) UNAL,    /*Except. Code from error in i/o routin.   */
        3 CFCT UBIN (18) UNAL,          /* Current fact addr                  */
        3 DCPR REDEF CFCT UBIN(18) UNAL, /* disc read stats maint             */
        3 NOBF UBIN (18) UNAL,          /* No. Of buf left to flush           */
        3 DCPW REDEF NOBF UBIN(18) UNAL, /* disc write stats maint.           */
        3 PNEOO SBIN (36) UNAL,         /*Pg no. Of i/o error                 */
        3 FBF72 BIT (36) UNAL,     /*AN4 eis descriptor - sign + 22 digits    */
        3 B72N (0:1) SBIN (36) UNAL,    /* 72-BIT binary number               */
        3 BUFRQ SBIN (36) UNAL,         /*Number of buffers requested         */
        3 TCTRS SBIN (36) UNAL,    /*Result from tct instr on encode/decode   */
        3 SCDES SBIN (36) UNAL,         /*Source descriptor for modify ci     */
        3 DSDES SBIN (36) UNAL,    /*Destination descriptor for modify ci     */
        3 PGHD (0:2) SBIN (36) UNAL,    /* Temp for 3-WORD page hdr           */
        3 WLOA SBIN (36) UNAL,          /* Word in line offset array          */
        3 VFDSR SBIN (36) UNAL,         /*Eis desc for varyiable length field */
        3 LINPT SBIN (36) UNAL,         /*Length of input                     */
        3 NEWSIZ UBIN (18) UNAL,   /*    New size of varying length record    */
        3 OLDSIZ UBIN (18) UNAL,   /*    Old size of varying length record    */
        3 SIZCHG SBIN (36) UNAL,   /*    Size change of varying length record */
        3 VFLNG UBIN (18) UNAL,         /*Varying field length                */
        3 VRSIZ UBIN (18) UNAL,         /*Current size of variable record     */
        3 RCHD UBIN (18) UNAL,          /* Record header                <ehw> */
        3 * UBIN (18) UNAL,
        3 * (0:1) SBIN (36) UNAL,
      2 FIB1 (0:23) SBIN (36) UNAL,     /* Gcos fib for data base files.      */
      2 FIBP (0:23) SBIN (36) UNAL,     /* Gcos fib for print files.          */
      2 BUF4 (0:7) SBIN (36) UNAL,      /* Gcos 4-bit buffer.                 */
      2 BUF6 (0:10) SBIN (36) UNAL,     /* Gcos 6-bit buffer.                 */
      2 BMPB (0:3) SBIN (36) UNAL, /* Gcos buffer management parameter block. */
      2 MODFL (0:11) SBIN (36) UNAL,    /* Gcos modify ci list.               */
      2 CRUN,                           /* Current of run.                    */
        3 DBK SBIN (36) UNAL,           /* data base key                      */
        3 RC UBIN (18) UNAL,            /* Rc offset in schema sub-set        */
        3 * UBIN(18) UNAL,
      2 SFWS,                           /* Schema file area work-space.       */
        3 ATRIB,
          4 CAC BIT(1) UNAL,            /* Concurrent access control          */
          4 JRNL BIT(1) UNAL,           /* Journaling required or requested   */
          4 * BIT(16) UNAL,
        3 * UBIN(18) UNAL,
        3 CURR SBIN (36) UNAL,          /* Dbk - current of realm             */
        3 GRC SBIN (36) UNAL,           /* gross read counter                 */
        3 GWC SBIN (36) UNAL,           /* gross write counter                */
        3 IGRC SBIN (36) UNAL,          /* gross read counter - key file      */
        3 IGWC SBIN (36) UNAL,          /* gross write counter - key file     */
        3 DDCB UBIN(18) UNAL,           /* DCB# for schema file               */
        3 KDCB UBIN(18) UNAL,           /* Unused here                        */
        3 ENCRP UBIN(36) UNAL;          /* Unused here                        */
%MEND;

