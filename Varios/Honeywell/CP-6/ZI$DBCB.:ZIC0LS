11:17 SEP 05 '97 ZI$DBCB.:ZIC0TOU
     1 - /*M* ZI$DBCB - 'IDS-II' data base control block for 'PL-6' access. */
     2 - /*T***********************************************************/
     3 - /*T*                                                         */
     4 - /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
     5 - /*T*                                                         */
     6 - /*T***********************************************************/
     7 - %MACRO   DBCB;
     8 - DCL DBCB$ PTR;
     9 - DCL 1 DBCB BASED(DBCB$) ALIGNED,        /* Storage daligned at run time       */
    10 - /*                                                                  */
    11 - /*             Control section equivalences                         */
    12 - /*                                                                  */
    13 -       2 CSECT,
    14 -         3 SSACCT CHAR(8) UNAL,          /* Subschema account                  */
    15 -         3 * SBIN(36) UNAL,
    16 -         3 ATBL,                         /*Addr of open area table             */
    17 -           4 OFFSET UBIN (18) UNAL,
    18 -           4 COUNT UBIN (18) UNAL,
    19 -         3 TBUF$ PTR,                    /* Pointer to temp rec buf            */
    20 -         3 * UBIN (18) UNAL,
    21 -         3 * BIT(18) UNAL,
    22 -         3 * SBIN (36) UNAL,
    23 -         3 DBCS CHAR(4) UNAL,
    24 -         3 KEYPA$ PTR,
    25 -         3 IXBUF$ PTR,                   /* Pointer to indexed buffer          */
    26 -         3 *(0:1) SBIN WORD UNAL,
    27 -         3 CBFSZ UBIN (18) UNAL,         /*Size of dbcb + fact                 */
    28 -         3 CIWSOF UBIN(18) UNAL,
    29 -         3 CFSA SBIN (36) UNAL,          /*Options     <ehw>                   */
    30 -         3 PABUF SBIN (36) UNAL,         /*Address of pa buffer                */
    31 -         3 * UBIN WORD,
    32 - /*                                                                  */
    33 - /*             Subroutine work space                                */
    34 - /*                                                                  */
    35 -       2 SWS,
    36 -         3 ARGS UBIN (18) UNAL,          /* Address of args from call - all    */
    37 -         3 FMSBF UBIN (18) UNAL,         /* Addr of fms buf for tss            */
    38 -         3 TSI SBIN (36) UNAL,           /* Time-sharing indicator - all       */
    39 -         3 RVTAL SBIN (36) UNAL,         /* Reference vector tally - ready     */
    40 -         3 * BIT(36) UNAL,
    41 -         3 SCLST SBIN (36) UNAL,         /* Sc list control word - stor&find   */
    42 -         3 * SBIN(36) UNAL,
    43 -         3 CLNG UBIN (18) UNAL,          /* Length of calc key - calc          */
    44 -         3 EEIND UBIN (18) UNAL,    /* Erase-establish currency IND.-UPDT curr */
    45 -         3 HASH SBIN (36) UNAL,          /* Hash - calc                        */
    46 -         3 CDES SBIN (36) UNAL,          /* Alpha descr for a calc key - calc  */
    47 -         3 * UBIN (27) UNAL,
    48 -         3 CCHAR CHAR (1),               /* Curr char of calc key - calc       */
    49 -         3 DDBK SBIN (36) UNAL,          /* Direct data base key - store       */
    50 -         3 SSZTP,                        /*Size & type - store                 */
    51 -           4 * UBIN (6) UNAL,
    52 -           4 TYPE UBIN (12) UNAL,
    53 -           4 SIZE UBIN (18) UNAL,
    54 -         3 BSPG SBIN (36) UNAL,     /* Base page (dbk of base loc) - store     */
    55 -         3 CURRS,                        /*Current rs - store                  */
    56 -           4 CURRENT UBIN (18) UNAL,
    57 -           4 BACKOUT UBIN (18) UNAL,
    58 -         3 DELRS UBIN (18) UNAL,         /*Current rs - erase                  */
    59 -         3 UCRS UBIN (18) UNAL,          /* Current rs ptr - update currencies */
    60 -         3 CURSS UBIN (18) UNAL,         /*Current ss - find ass.              */
    61 -         3 RSREQ UBIN (18) UNAL,         /*Rs requested - find ass.            */
    62 -         3 CDBK SBIN (36) UNAL,          /* Current dbk - find ass.            */
    63 -         3 ROCUR SBIN (36) UNAL,         /*Rec occurrence no. - find a/r       */
    64 -         3 MODFR,                        /*Fr list ptr - modify                */
    65 -           4 OFFSET UBIN (18) UNAL,
    66 -           4 COUNT UBIN (18) UNAL,
    67 -         3 MODSC,                        /*Sc list ptr - modify                */
    68 -           4 OFFSET UBIN (18) UNAL,
    69 -           4 COUNT UBIN (18) UNAL,
    70 -         3 MODRS UBIN (18) UNAL,         /*Current rs - modify                 */
    71 -         3 MODIN UBIN (18) UNAL,         /*Including set indicator - modify    */
    72 -         3 MODCD UBIN (18) UNAL,         /*Current cd ptr - modify             */
    73 -         3 CONCD UBIN (18) UNAL,         /*Current cd - connect                */
    74 -         3 FLDTL,                   /*Tally & ind word for fr list- get fields */
    75 -           4 OFFSET UBIN (18) UNAL,
    76 -           4 COUNT UBIN (18) UNAL,
    77 -         3 FCSS UBIN (18) UNAL,          /* Ss ptr for calc keys - find calc   */
    78 -         3 MRBKI UBIN (18) UNAL, /*New entry has been inserted indic - modify bo */
    79 -         3 CACD UBIN (18) UNAL,          /* Current cd ptr - calc              */
    80 -         3 CAAC UBIN (18) UNAL,          /* Current ac or zero - calc          */
    81 -         3 FCNT UBIN (18) UNAL,     /* No. Entries left in list - mv/cmp fld   */
    82 -         3 * UBIN (18) UNAL,
    83 -         3 CMPL UBIN (18) UNAL,          /* Current entry in list - mv/cmp fld */
    84 -         3 * UBIN(18) UNAL,
    85 -         3 DPSLW SBIN (36) UNAL,         /*No. Lines to slew - error routine   */
    86 -         3 FKIND SBIN (36) UNAL,    /*Find w/i current indicator - find ass.   */
    87 -         3 STMASK BIT (36) UNAL,         /*Size & type mask - all              */
    88 -         3 TMASK BIT (36) UNAL,          /*Type mask - all                     */
    89 -         3 CTMASK BIT (36) UNAL,         /*Compliment of type mask - all       */
    90 -         3 ITRS UBIN (18) UNAL,          /* Current rs ptr - if tests          */
    91 -         3 FTIND UBIN (18) UNAL,         /*First time indicator (.DBHIA)       */
    92 -         3 ITPTR SBIN (36) UNAL,         /*Nxt ptr from curr rec - if tests    */
    93 -         3 SUST,                         /* Subroutine status - all            */
    94 -           4 CURRENT UBIN (18) UNAL,
    95 -           4 BACKOUT UBIN (18) UNAL,
    96 -         3 * (0:5) SBIN UNAL,            /* No longer used                     */
    97 -         3 CSUB,                         /* Ptr to curr subr. Cntrs - all      */
    98 -           4 CTRPTR UBIN (18) UNAL,
    99 -           4 ELPTR UBIN (18) UNAL,
   100 -         3 RNGO SBIN (36) UNAL,          /* Length of range of owner - store   */
   101 -         3 RNGD SBIN (36) UNAL,          /* Displ. Of owner w/i range - store  */
   102 -         3 ODBK SBIN (36) UNAL,          /* Dbk to relink to - modify          */
   103 -         3 RDYCW BIT (36) UNAL,          /*Ready control word - ready          */
   104 -         3 READY REDEF RDYCW,
   105 -           4 * UBIN (9) UNAL,
   106 -           4 ER BIT(1) UNAL,             /* exclusive retrieval                */
   107 -           4 EU BIT(1) UNAL,             /* exclusive update                   */
   108 -           4 PR BIT(1) UNAL,             /* protected retrieval                */
   109 -           4 PU BIT(1) UNAL,             /* protected update                   */
   110 -           4 CR BIT(1) UNAL,             /* concurrent retrieval               */
   111 -           4 CU BIT(1) UNAL,             /* concurrent update                  */
   112 -           4 BM BIT(1) UNAL,             /* load mode                          */
   113 -           4 * UBIN (2) UNAL,
   114 -           4 NOAC UBIN (18) UNAL,        /* number of ac's                     */
   115 -         3 RDCNT SBIN (36) UNAL,         /*Neg. Of no. Of ac's - ready         */
   116 -         3 RDAC UBIN (18) UNAL,          /* Curr ac or ind. Ptr - ready        */
   117 -         3 ARGS2 UBIN (18) UNAL, /*Address of args from CALL(.DBEUP-FORTRAN)   */
   118 -         3 PKAW,               /* Rc ubin word, fr for area-id - find area     */
   119 -           4 RCPTR UBIN (18) UNAL,
   120 -           4 FRPTR UBIN (18) UNAL,
   121 -         3 FRAC UBIN (18) UNAL,     /* Curr ac for printing stats - finish     */
   122 -         3 OPADP UBIN (18) UNAL,         /*Old pa key duplicate indic.         */
   123 -         3 FICNT SBIN (36) UNAL,    /*Neg. Of no. Of ac's in list - finish     */
   124 -         3 FIAC UBIN (18) UNAL,     /* Curr ac or ptr to curr rv E.- finish    */
   125 -         3 UPPAI UBIN (18) UNAL,         /*Update pa ofsts indic. (.DBUPC)     */
   126 -         3 STBIN SBIN (36) UNAL,         /*Binary no. For stats - finish       */
   127 -         3 STDEC (0:1) SBIN (36) UNAL,   /*Decimal no. For stats - finish      */
   128 -         3 SNIND SBIN (36) UNAL,         /*Area-id name indicator - find area  */
   129 -         3 CDP UBIN (18) UNAL,           /* Current dp ptr - proc. Inter       */
   130 -         3 RSPAM UBIN (18) UNAL,         /*Curr rs ptr for pa member           */
   131 -         3 CAG,                          /* Curr ag - call db proc             */
   132 -           4 CURRENT UBIN (18) UNAL,
   133 -           4 LASTSTS UBIN (18) UNAL,
   134 -         3 CRA UBIN (18) UNAL,           /* Curr ra - find chr                 */
   135 -         3 * UBIN (18) UNAL,
   136 -         3 TEMP,                         /* Temporary storage                  */
   137 -           4 PTR1$ PTR ALIGNED,     /* for access to temp wd1 as a pointer     */
   138 -           4 WORD1 REDEF PTR1$ SBIN WORD, /* for access to temp wd1 as a word  */
   139 -           4 INAFLAG REDEF PTR1$ SBIN WORD, /* used during rdy & ina functions */
   140 -           4 PTR2$ PTR ALIGNED,     /* for access to temp wd2 as a pointer     */
   141 -           4 WORD2 REDEF PTR2$ SBIN WORD, /* for access to temp wd2 as a word  */
   142 -         3 CSTS,                         /* Curr stats ofst - finish           */
   143 -           4 OFFSET UBIN (18) UNAL,
   144 -           4 NAME CHAR (2),
   145 - /*                                                                  */
   146 - /*             Subroutine call counters                             */
   147 - /*                                                                  */
   148 -         3 STAT,
   149 -           4 CALLS(0:25),
   150 -             5 CALLED SBIN(36) UNAL,
   151 -             5 READ SBIN(36) UNAL,
   152 -             5 WROTE SBIN(36) UNAL,
   153 -         3 DBKSI SBIN (36) UNAL,         /*Dbk of curr si rec                  */
   154 -         3 REMLT,                   /*Remaining lock bits in lt & bind. Value  */
   155 -           4 TYPE UBIN (9) UNAL,
   156 -           4 LOCK UBIN (9) UNAL,
   157 -           4 BINDVAL UBIN (18) UNAL,
   158 -         3 CURLT UBIN (18) UNAL,         /*Current lock bits & bind. Value     */
   159 -         3 * UBIN (18) UNAL,
   160 -         3 CLI UBIN (18) UNAL,           /* Curr lock ind.                     */
   161 -         3 CWSOF UBIN (18) UNAL,         /*Curr work space ofst                */
   162 -         3 CURLN (0:2) SBIN (36) UNAL,   /*Current privacy key                 */
   163 -         3 VFDSC SBIN (36) UNAL,         /*Eis desc for varying length field   */
   164 -         3 FLDDSC SBIN (36) UNAL, /*Eis desc defining desc for varying length field */
   165 -         3 * UBIN(18) UNAL,
   166 -         3 * UBIN (18) UNAL,
   167 -         3 GACDK SBIN (36) UNAL,         /*Dbk to find ac for                  */
   168 -         3 PCEXI SBIN (36) UNAL,         /*Partial compare indicator           */
   169 -         3 FNIND SBIN (36) UNAL,         /*First/next in key indicator         */
   170 -         3 RETFR UBIN (18) UNAL,         /*Fr ptr for special regs             */
   171 -         3 * UBIN (18) UNAL,
   172 -         3 OWNPTR SBIN (36) UNAL,        /*Dbk of owner of pa set (backout)    */
   173 -         3 * (0:12) SBIN (36) UNAL,
   174 - /*                                                                  */
   175 - /*             Stack for erase                                      */
   176 - /*                                                                  */
   177 -       2 DBEST,
   178 -         3 CONTROL,
