%MACRO FWS;
DCL FWS$ PTR;
DCL 1 FWS BASED(FWS$) ALIGNED,
      2 PRIV,
        3 * UBIN(9) UNAL,
        3 PFG BIT(1) UNAL,              /* get                                */
        3 PFM BIT(1) UNAL,              /* modify                             */
        3 PFS BIT(1) UNAL,              /* store                              */
        3 * UBIN(6) UNAL,
      2 * UBIN(18) UNAL;
%SUB FWS#PRIV#ALL    ='000700'O/*FWS#PRIV#ALL*/;
%MEND;

