!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=CURSOR_CRU,WSN=LOCAL
!RESOURCE TIME=5,MEM=255
!ROUTE Peter Rumsey, LADC
!PL6 CURSOR_SI61.SIACCT OVER *CURSOR_OU6,LP(NSYS,OU,LS,SR(.LBACCT),NWARN,SCHEMA)
!LEMUR
COPY *CURSOR_OU6 OVER CURSOR.GNACCT(SEVERITY=7)
END
!MOD CURSOR.GNACCT TO (ACC((?,Z?),EXEC),ACS((LINK?,LEMUR?),READ))
