      139    10379    3             DO DEFI = 0 TO 24;
      140    10380    3                  IF S_D.UD.TEXTSZ( DEFI ) ~= 0
      141    10381    3                  THEN NONE = '0'B;
      142    10382    3                  S_D.UD.TEXTSZ(DEFI) = 0;
      143    10383    3                END;
