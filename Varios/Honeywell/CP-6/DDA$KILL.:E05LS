        8        8        %SET LISTSUB='1'B;
        9        9        /*D*  NAME:  DDA$KILL
       10       10              CALL:  CALL DDA$KILL
       11       11              INPUT:  S_D.DO$ POINTS TO THE KILL FRAME
       12       12              DESCRIPTION:  DDA$KILL IS CALLED BY DDA$DO.  STORED
       13       13                     THE KILL 'TYPE' OR ID RANGE DICTATES WHAT IS TO BE
       14       14                     DONE. */
       15       15                                                /**/
       16       16        DDA$KILL: PROC;
       17       17                                                /**/
       18       18    1   DCL TYPE SBIN;
       19       19    1   DCL F_FROM SBIN;
       20       20    1   DCL F_TO SBIN;
       21       21    1   DCL CMD SBIN;
       22       22    1   DCL DEFI SBIN;
