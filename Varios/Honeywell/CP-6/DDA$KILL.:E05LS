      356    10596                                                /**/
      357    10597    1   KILL_FIX: PROC;
      358    10598                                                /**/
      359    10599    3        DO I = F_FROM TO F_TO;
      360    10600    3             CCMD = S_D.FIXCOM.CMD( I );
      361    10601    3             IF CCMD ~= 0
      362    10602    3             THEN IF CMD = CCMD OR CMD = 0
      363    10603    4                  THEN DO;
      364    10604    4                       NONE = '0'B;
      365    10605    4                       S_D.KILL1 = I;
      366    10606    4                       IF NOT S_D.FIXCOM.FPRG(I)
      367    10607    5                       THEN DO;
      368    10608    5                            IF CCMD = 30 /* C_OA */
