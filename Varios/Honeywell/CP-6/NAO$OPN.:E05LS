       10       10                CALL: CALL NAO$OPN
       11       11                INPUT:  B$JIT.DCB$ points to current DCB.
       12       12                PURPOSE:  Opens a DCB to an array processor.
       13       13        */
       14       14
       15       15        %INCLUDE F$DCB;
       16       64        %INCLUDE F_FMTCODE_C;
       17       98        %INCLUDE FR$RAT;
       18      151        %FR$RAT (NAME=FR$RAT,STCLASS="BASED(RAT$)");
       19      191        %INCLUDE FR_SUBS;
       20      259        %INCLUDE B$JIT;
       21      862        %INCLUDE B_ERRORS_C;
       22     1989        %INCLUDE CP_6_SUBS;
       23     2529        %INCLUDE SS_SCHED_C;
       24     2762
       25     2763    1   DCL NAI$INIT ENTRY;
       26     2764    1   DCL SSR$REG ENTRY(3) ALTRET;
       27     2765    1   DCL FRZ$GETRES ENTRY(5) ALTRET;
       28     2766
       29     2767    1   DCL B$JIT$ PTR SYMREF;
       30     2768    1   DCL FR$HD$(0:0) PTR SYMREF;
       31     2769
       32     2770    1   DCL JDCB$ PTR;
       33     2771    1   DCL RAT$ PTR;
       34     2772    1   DCL BUF$ PTR;
       35     2773    1   DCL SYSID UBIN;
       36     2774    1   DCL MODE UBIN;
       37     2775
PL6.E3A0      #001=NAO$OPN File=NAO$OPN.:E05TSI                                  WED 07/30/97 03:26 Page:2    
       38     2776        %SUB F$DCB=JDCB$->F$DCB;
       39     2777
       40     2778    1           JDCB$=B$JIT.DCB$;
       41     2779
       42     2780    2           IF F$DCB.RESNT.TYP ~= 'AP' THEN DO;
       43     2781    2              CALL ERRSET(%E$INVASN);
       44     2782        /*E*       ERROR:  NAO-E$INVASN-E
       45     2783                   MESSAGE: Invalid resource type for FPAP ASN.
       46     2784                   MESSAGE1: The resource type must be set to 'AP' to open a DCB
       47     2785                             to a floating point processor.
       48     2786                   DESCRIPTION:  ASN=FPAP and RES~='AP'.
       49     2787        */
       50     2788    2              RETURN;
       51     2789    2              END;
       52     2790    1           F$DCB.RESNT.NUM='01';
       53     2791
       54     2792    1           RAT$=FR$HD$(FR_NPOOL);
       55     2793    2           DO WHILE(RAT$~=ADDR(NIL));
       56     2794    2              IF FR$RAT.TYP = 'AP'
       57     2795    2                AND FR$RAT.FLG.DOWN='0'B
       58     2796    2                AND (B$JIT.APATTR & ~FR$RAT.M1)='0'B THEN
       59     2797    2                 EXIT;
       60     2798    2              RAT$=FR$RAT.LNK$;
       61     2799    2              END;
       62     2800    2           IF RAT$=ADDR(NIL) THEN DO;
       63     2801    2              CALL ERRSET(%E$NORSS);
       64     2802        /*E*       ERROR:  NAO-E$NORSS-E
       65     2803                   MESSAGE:  No array processors with the requested attributes exist on
       66     2804                             this system.
       67     2805
       68     2806        */
       69     2807    2              RETURN;
       70     2808    2              END;
       71     2809
       72     2810    1           SYSID=B$JIT.SYSID;
       73     2811    1           MODE=B$JIT.MODE;
       74     2812    1           RAT$=ADDR(NIL);
PL6.E3A0      #001=NAO$OPN File=NAO$OPN.:E05TSI                                  WED 07/30/97 03:26 Page:3    
       75     2813    2           DO WHILE(RAT$=ADDR(NIL));
       76     2814    3              CALL FRZ$GETRES(F$DCB.RESNT.TYP,B$JIT.APATTR,SYSID,MODE,RAT$)
              2814                       WHENALTRETURN DO;
       77     2815    4                 IF F$DCB.SHARE = %NONE# OR B$JIT.ERR.CODE ~= %E$NORSS THEN DO;
       78     2816    4                    RETURN;
       79     2817    4                    END;
       80     2818    4                 CALL SSR$REG(%SS_NAP) WHENALTRETURN DO;
       81     2819    4                    RETURN;
       82     2820    4                    END;
       83     2821    3                 B$JIT.ERR='0'B;
       84     2822    3                 END;
       85     2823    2              END;
       86     2824
       87     2825    1           F$DCB.SETX=FR$RAT.DCTX;
       88     2826    1           F$DCB.ASN=%FPAP#;
       89     2827    1           F$DCB.IASN=%FPAP#;
       90     2828    1           F$DCB.IFMT=FMTARRAYPROCESSOR#;
       91     2829    1           F$DCB.FFLG='0'B;
       92     2830    1           F$DCB.FFLG.WNEW='1'B;
       93     2831    1           F$DCB.FCD='1'B;
       94     2832    1           F$DCB.FCI='1'B;
       95     2833    1           CALL NAI$INIT;
       96     2834    1           RETURN;
       97     2835
       98     2836    1   ERRSET: PROC(ERRCODE);
       99     2837
      100     2838    2   DCL ERRCODE UBIN;
      101     2839
      102     2840    2           B$JIT.ERR='160117400002'O;
      103     2841    2           B$JIT.ERR.CODE=ERRCODE;
      104     2842    2           RETURN;
      105     2843
      106     2844    2   END ERRSET;
