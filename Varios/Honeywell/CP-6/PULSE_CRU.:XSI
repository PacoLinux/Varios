!ROUTE Greg Smith, Bucknell
!PL6 PULSE_SI61.SIACCT OVER *PULSE_OU61.GNACCT(NLS,NSYS,SR(.SIACCT,.LBACCT))
!PL6 PULSE_SI62.SIACCT OVER *PULSE_OU62.GNACCT(NLS,NSYS,SR(.SIACCT,.LBACCT))
!BMAP.XACCT PULSE_SIG1.SIACCT OVER *PULSE_OUG1.GNACCT(NLS)
!PARTRGE PULSE_SIN1.SIACCT OVER *PULSE_OUN1.GNACCT(NLS,NSYS,SR(.SIACCT,.LBACCT))
!LINK *PULSE_OU61.GNACCT,;
      *PULSE_OU62.GNACCT,;
      *PULSE_OUN1.GNACCT,;
      *PULSE_OUG1.GNACCT ;
      OVER PULSE.GNACCT(STDINVOC,IP=250,DP=50)
!DEL *DA?
!EXTRACT.XACCT
 DA *DA
 EX PULSE_SI63.SIACCT
 B INTO PULSE.GNACCT,PULSE_C61.SIACCT PRO
 END
