      225    11964
      226    11965    4           END;
PL6.E3A0      #001=Z8E3$ERROR_CORR_DD File=Z8E3.:Z8A6TSI                         SAT 08/09/97 11:28 Page:8    
      227    11966    3         ELSE
      228    11967    4           DO;
      229    11968                  %SAVE_TOKEN_ON_THE_QUEUE;
      230    11990
      231    11991                  %LOAD_SAVED_TOKEN;
      232    11996    4           END;
      233    11997    3       END;
      234    11998    2     ELSE
      235    11999    3       DO;
      236    12000    3         IF (SUBSTR (LEX_SYMBOL.SYMBOL, 0, LEX_SYMBOL.SIZE) = 'SQL' AND
      237    12001    3            O1COM.LX_STK.DNT$ (O1COM.LX_TOP) -> DNT.TEXT.NAME = 'EXEC') THEN
      238    12002    4           DO;
      239    12003                    /* next few lines might be revived in the future  */
      240    12004                    /* and needs to be insert into previous block too */
      241    12005                    /*   CALL Z9EL$SQL_PREP;                          */
      242    12006
      243    12007                    /* if returned, SQL preprocessor is not available */
      244    12008                    /*   COMCOM.SQL_NOT_AVAILABLE = TRUE;             */
      245    12009
      246    12010
      247    12011    4             IF CHECK_FIRST_SQL_PREFIX THEN
      248    12012    5               DO;
      249    12013    5                 SQL.EXEC_LINE = O1COM.LX_STK.LINE(O1COM.LX_TOP);
      250    12014    5                 SQL.EXEC_COLUMN = O1COM.LX_STK.COLUMN(O1COM.LX_TOP);
      251    12015    5                 SQL.SQL_LINE = LEX_SYMBOL.LINE;
      252    12016    5                 SQL.SQL_COLUMN = LEX_SYMBOL.COLUMN;
      253    12017    5                 O1COM.LX_STK.B.ERROR (O1COM.LX_TOP) = TRUE;
      254    12018    5                 O1COM.LA_STK.B.ERROR (O1COM.LA_GET) = TRUE;
      255    12019    5               END;
      256    12020
