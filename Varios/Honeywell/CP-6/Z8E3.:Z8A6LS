      158    11735
      159    11736    2   DCL Z8AE$ERROR  ENTRY (12);
      160    11737    2   DCL Z8EL$HANDLE_LEVEL_CHANGE  ENTRY (1);
      161    11738        /*  DCL Z9EL$SQL_PREP          ENTRY;  */
      162    11739
      163    11740        %MACRO SKIP_SQL_TOKENS;
      164    11741          DO;
      165    11742                  /* Skips all symbols until the symbol "END-EXEC" or
      166    11743                     end-of-file is hit, also issues an error message to
      167    11744                     tell the user to use SQL option to process SQL statement
      168    11745                  */
      169    11746
      170    11747                  CALL Z8AE$ERROR (%E$USE_SQL_OPTION,
      171    11748                                   LEX_SYMBOL.LINE,
      172    11749                                   LEX_SYMBOL.COLUMN);
      173    11750
      174    11751                  DO WHILE (SUBSTR(LEX_SYMBOL.SYMBOL,0,LEX_SYMBOL.SIZE) ~=
      175    11752                            'END-EXEC'    AND
      176    11753                            LEX_SYMBOL.TYPE ~= %END_OF_FILE );
      177    11754
      178    11755                    %GET_A_TOKEN;
      179    11756
      180    11757                  END; /* DO WHILE */
      181    11758
      182    11759                  IF SUBSTR(LEX_SYMBOL.SYMBOL,0,LEX_SYMBOL.SIZE) = 'END-EXEC' THEN
      183    11760                    DO;
      184    11761
      185    11762                      CHECK_FIRST_SQL_PREFIX = FALSE;
      186    11763
      187    11764                      %GET_A_TOKEN;
      188    11765
      189    11766                      IF SUBSTR(LEX_SYMBOL.SYMBOL,0,LEX_SYMBOL.SIZE) = 'EXEC' THEN
PL6.E3A0      #001=Z8E3$ERROR_CORR_DD File=Z8E3.:Z8A6TSI                         SAT 08/09/97 11:28 Page:7    
      190    11767                        GOTO START_SKIP;
      191    11768                      ELSE
      192    11769                        DO;
      193    11770                          %SAVE_TOKEN_ON_THE_QUEUE;
      194    11771                        END;
      195    11772                    END;
      196    11773
      197    11774
      198    11775           END;
      199    11776        %MEND;
      200    11777
      201    11778
      202    11779    2     CHECK_FIRST_SQL_PREFIX = TRUE;
      203    11780
      204    11781    2     START_SKIP:
      205    11782
      206    11783    2     IF SUBSTR (LEX_SYMBOL.SYMBOL, 0, LEX_SYMBOL.SIZE) = 'EXEC' THEN
      207    11784    3       DO;
      208    11785              %SAVE_CURRENT_TOKEN;
      209    11790
      210    11791              %GET_A_TOKEN;
      211    11829
      212    11830    3         IF SUBSTR(LEX_SYMBOL.SYMBOL,0,LEX_SYMBOL.SIZE) = 'SQL' THEN
      213    11831    4           DO;
      214    11832
      215    11833    4            IF CHECK_FIRST_SQL_PREFIX THEN
      216    11834    5              DO;
      217    11835    5                 SQL.EXEC_LINE = TEMP_LEX_SYMBOL.LINE;
      218    11836    5                 SQL.EXEC_COLUMN = TEMP_LEX_SYMBOL.COLUMN;
