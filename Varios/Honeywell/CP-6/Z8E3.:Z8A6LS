       44     7873        */
       45     7874
       46     7875
       47     7876
       48     7877
       49     7878        %MACRO TERMINATE;
       50     7879          /* Insert code for abnormal termination of parser */
       51     7880          CALL Z9AT$TERMINATOR;
       52     7881        %MEND;
       53     7882
       54     7883
       55     7884        %EQU DI_CHAR_LEN_MAX = 100;   /* Maximum length of an error plug-in */
       56     7885
       57     7886
       58     7887        %MACRO ERRORPROC;
       59     7888        ERROR : PROC(ERROR_TYPE, PLUG1$, PLUG2$);
       60     7889
       61     7890        DCL ERROR_TYPE SBIN WORD;
       62     7891        DCL PLUG1$ PTR;  /* TO DNT */
       63     7892        DCL PLUG2$ PTR;  /* TO DNT */
       64     7893
       65     7894        DCL NEWPLUG1$ PTR;
       66     7895        DCL NEWPLUG2$ PTR;
       67     7896        DCL Z8AE$ERROR ENTRY(12);
       68     7897
       69     7898        DCL 1 PARSEDNT BASED ALIGNED,
       70     7899              2 TEXT,
       71     7900                3 LENGTH SBIN WORD,
       72     7901                3 NAME CHAR(PARSEDNT.TEXT.LENGTH);
       73     7902
       74     7903        /* ERROR LOCATION IS SET WITH THE RESET_ERROR_LOC MACRO BEFORE
       75     7904           THE ERROR PROCEDURE IS CALLED */
       76     7905
       77     7906        IF ADDR(PLUG1$) = ADDR(NIL) THEN
       78     7907          /* Only error type is specified */
PL6.E3A0      #001=Z8E3$ERROR_CORR_DD File=Z8E3.:Z8A6TSI                         SAT 08/09/97 11:28 Page:4    
       79     7908          CALL Z8AE$ERROR(ERROR_TYPE, O1COM.ERRLINE, O1COM.ERRCOL);
       80     7909        ELSE
       81     7910          IF ADDR(PLUG2$) = ADDR(NIL) THEN
       82     7911            /* Only 1 plug is specified */
       83     7912            DO;
       84     7913              %NEW_DNT(@POINTER = NEWPLUG1$,
       85     7914                          @SIZE = "PLUG1$ -> PARSEDNT.TEXT.LENGTH",
       86     7915                          @NAME = "PLUG1$ -> PARSEDNT.TEXT.NAME");
       87     7916
       88     7917              %IF_NOT_SQL_PREFIX_THEN;
       89     7918              CALL Z8AE$ERROR(ERROR_TYPE, O1COM.ERRLINE, O1COM.ERRCOL, NEWPLUG1$);
       90     7919            END;
       91     7920          ELSE
       92     7921            /* 2 PLUGS WERE SPECIFIED */
       93     7922            DO;
       94     7923              %NEW_DNT(@POINTER = NEWPLUG1$,
       95     7924                            @SIZE = "PLUG1$ -> PARSEDNT.TEXT.LENGTH",
       96     7925                            @NAME = "PLUG1$ -> PARSEDNT.TEXT.NAME");
       97     7926              %NEW_DNT(@POINTER = NEWPLUG2$,
       98     7927                          @SIZE = "PLUG2$ -> PARSEDNT.TEXT.LENGTH",
       99     7928                          @NAME = "PLUG2$ -> PARSEDNT.TEXT.NAME");
      100     7929
      101     7930              %IF_NOT_SQL_PREFIX_THEN;
      102     7931            CALL Z8AE$ERROR (ERROR_TYPE, O1COM.ERRLINE, O1COM.ERRCOL, NEWPLUG1$, NEWPLUG2$);
      103     7932            END;
      104     7933
      105     7934        END ERROR;
      106     7935        %MEND;
      107     7936
      108     7937
      109     7938
      110     7939        %MACRO RESET_ERROR_LOC;
      111     7940
      112     7941          /* RESET THE LINE & COLUMN TO THE POINT OF ERROR */
      113     7942          DO;
      114     7943             O1COM.ERRLINE = O1COM.LA_STK.LINE(O1COM.LA_GET);
      115     7944             O1COM.ERRCOL  = O1COM.LA_STK.COLUMN(O1COM.LA_GET);
PL6.E3A0      #001=Z8E3$ERROR_CORR_DD File=Z8E3.:Z8A6TSI                         SAT 08/09/97 11:28 Page:5    
      116     7945          END;
      117     7946        %MEND;
      118     7947
      119     7948
      120     7949
      121     7950
      122     7951
      123     7952
      124     7953
      125     7954        /* Set up the trial parse length to try a sample parse on
      126     7955           any proposed correction during error recovery.  The specified
      127     7956           number designates how many tokens to parse past the proposed
      128     7957           correction.  If the trial parse fails, the cost to insert these
      129     7958           symbols will be doubled.  This will encourage deletion of
      130     7959           symbols when multiple corrections need to be made in adjacent
      131     7960           contexts.  Setting trial  parse length to 0 will disable
      132     7961           the trial parse.
      133     7962        */
      134     7963        %EQU TRAIL_PARSE_LENGTH = 2;
      135     7964
      136     7965        %MACRO CALL_SCANNER;
      137     7966
      138     7967          /* Call the scanner to obtain the next token.  The token should be
      139     7968             placed in LA_STK(LA_PUT).
      140     7969          */
      141     7970          CALL Z8EL$LEX;
      142     7971
      143     7972        %MEND;
