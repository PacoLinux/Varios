       29     7858    1         2 EXEC_COLUMN    SBIN WORD  INIT (0),
       30     7859    1         2 SQL_LINE       SBIN WORD  INIT (0),
       31     7860    1         2 SQL_COLUMN     SBIN WORD  INIT (0);
       32     7861
       33     7862        %MACRO IF_NOT_SQL_PREFIX_THEN;
       34     7863           IF NOT ( ( O1COM.ERRLINE = SQL.EXEC_LINE   AND
       35     7864                      O1COM.ERRCOL = SQL.EXEC_COLUMN  )  OR
       36     7865                    ( O1COM.ERRLINE = SQL.SQL_LINE   AND
       37     7866                      O1COM.ERRCOL = SQL.SQL_COLUMN ) ) THEN
