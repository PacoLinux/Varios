    " R_DISPLAY N;TYPES;TN;RECS;FLDS;FLD;DBNAMES;F
[1]  $COM*T* Copyright (c) Bull HN Information Systems Inc., 1989
[2]   N_LOOKUP N $DMD DBNAMES_0 FIXTEXT $QDBNAMES
[3]   R_0 0$R ' '
[4]   TN_(TYPES_$QDBTYPES)[;''$R $QIO]
[5]   $GO (~&/TN[N]$E 2 3 5)/ER0
[6]   RECS_(FLDS_TN[N]=2)/N $DMD FLDS_-99999,(~FLDS)/N
[7]  L0:$GO (0=$R RECS)/L0X
[8]   FLDS_(($--1$TAKRECS),((3=TN)&(-1$TAKRECS)=,TYPES[;3+$QIO])/$I $R TN),FLDS
[9]   RECS_-1$DRPRECS $DMD $GO L0
[10] L0X:F_1$TAK$R R_FIELDVALUES FLD_(FLDS$GE0)/FLDS
[11]  R_((F,9)$R ' '),DBNAMES[FLD;],((F,2)$R ' '),R
[12]  R_R CAT1((($R RECS),9)$R ' RECORD: '),DBNAMES[RECS_|((FLDS<0)&FLDS>-99999)/FLDS;]
[13]  R_R CAT1(-99999$NE-1$TAKFLDS)/' FIELDS:'
[14]  R_R[$GU$GUFLDS<0;]
[15]  $GO 0
[16] ER0:('CAN''T DISPLAY:',(,(~&/TN[N]$E 2 3 5)$RD1' ',DBNAMES[N;]))$QERS 833
[17]  $GO 0
[18] $COM*I*    NAME:       DISPLAY
[19] $COM       PURPOSE:    To display values within the UWA.
[20] $COM       DESCRIPTION:
[21] $COM                   The right argument is the items to be
[22] $COM                   displayed.  It may be either a vector of
[23] $COM                   item and record names, a namelist containing
[24] $COM                   left justified one item or record name per row,
[25] $COM                   or a vector of item or record name indices.
[26] $COM                   The result contains the item names, and item
[27] $COM                   values requested.  If a record name is specified
[28] $COM                   the values of all items in that record is
[29] $COM                   returned.
    "
 
$COM  $QDBNAMES      2
$COM  $QDBTYPES      4
$COM  $QERS         16
$COM  $QIO           4     8
$COM  CAT1          12    13
$COM  DBNAMES        0     2_   11]   12]   16]
$COM  ER0            5    16:
$COM  F              0    10_   11    11
$COM  FIELDVALUES   10
$COM  FIXTEXT        2
$COM  FLD            0    10_   11
$COM  FLDS           0     6_    6_    6     8_    8    10    10    12    12
$COM                12    13    14
$COM  LOOKUP         2
$COM  L0             7:    9
$COM  L0X            7    10:
$COM  N              0     2_    2     5     6     6     6    16    16
$COM  R              0_    3_   10_   11_   11    12_   12    13_   13    14_
$COM                14]
$COM  RECS           0     6_    7     8     8     9_    9    12    12_
$COM  TN             0     4_    5]    6]    8     8    16]
$COM  TYPES          0     4_    8]
0;'        FUNCTION: LOOKUP        '
 
 
    " R_LOOKUP N;NAMES;NN;RN
[1]  $COM*T* Copyright (c) Bull HN Information Systems Inc., 1989
[2]   (0 2$R 0)$QSM 'LOOKUP' $DMD NN_1$TAKRN_$R NAMES_$QDBNAMES $DMD RN_1$DRPRN
[3]   $GO (' '=1$TAK0$R N)/L0
[4]   'NOT DBNAME INDEX' $QERS 830#$I ~(&/(,N)$E $I NN)&0 1$OR.=$R $R N
[5]   R_,N $DMD $GO 0
[6]  L0:$GO (0 1 2=$R $R N)/L1,L1,L2
[7]   'NOT LEGAL NAMELIST' $QERS 831
[8]  L1:$GO (3073$GE16$DCD('0123456789ABCDE'$I 3$TAK$QVERSION)$-$QIO)/LOLD
[9]   (1 2$R LS,4)$QSM 'LOOKUP'
[10]  R_$QDBANLZ 1 FIXTEXT N $DMD $GO 0
[11] LS:(0 2$R 0)$QSM 'LOOKUP'
[12] LOLD:N_BUILDNL N
[13] L2:$GO (RN>-1$TAK$R N)/L3 $DMD $GO (RN=-1$TAK$R N)/L4
[14]  NAMES_(NN,RN_-1$TAK$R N)$TAKNAMES $DMD $GO L4
[15] L3:N_((1$TAK$R N),RN)$TAKN
[16] L4:N_1 FIXTEXT N
[17]  $GO (0$E +/R_N&.=$TPSNAMES)/ERNF
[18]  R_$QIO++/&\~R
[19]  $GO 0
[20] ERNF:R_(R$NE' ')/R_,1$DRP,',',0 FIXTEXT(1$NE+/R)$RD1N
[21]  ('NOT FOUND: ',R)$QERS 832
[22]  $GO 0
[23] $COM*I*    NAME:       LOOKUP
[24] $COM       PURPOSE:    To determine if the argument contains IDS names
[25] $COM       DESCRIPTION:
[26] $COM                   The right argument may be a text vector
[27] $COM                   with IDS names separated by blanks, commas
[28] $COM                   or carriage returns,  a text matrix with
[29] $COM                   IDS names left justified one name per row,
[30] $COM                   or the right argument may contain IDS name
[31] $COM                   indices.  The result is a vector of the IDS
[32] $COM                   name indices.
    "
 
$COM  $QDBANLZ     10
$COM  $QDBNAMES     2
$COM  $QERS         4     7    21
$COM  $QIO          8    18
$COM  $QVERSION     8
$COM  BUILDNL      12
$COM  ERNF         17    20:
$COM  FIXTEXT      10    16    20
$COM  LOLD          8    12:
$COM  LS            9    11:
$COM  L0            3     6:
$COM  L1            6     6     8:
$COM  L2            6    13:
$COM  L3           13    15:
$COM  L4           13    14    16:
$COM  N             0     3     4     4     5     6    10    12_   12    13
$COM               13    14    15_   15    15    16_   16    17    20
$COM  NAMES         0     2_   14_   14    17
$COM  NN            0     2_    4    14
$COM  R             0_    5_   10_   17_   18_   18    20_   20    20_   20
$COM               21
$COM  RN            0     2_    2_    2    13    13    14_   15
0;'        FUNCTION: FIXTEXT       '
 
 
    " R_T FIXTEXT N;AV
[1]  $COM*T* Copyright (c) Bull HN Information Systems Inc., 1989
[2]   $GO (2$NE$TBRR_N)/0
[3]   AV_$QAV $DMD AV[($I 32)]_' '
[4]   $GO (0$NE$QNC 'T')/L $DMD T_1
[5]  L:AV[$QAV$I '$_$-_-$DLT']_'_-$_$-$DLT$DLT'[$QIO+5$R0 1+2#(2#T=2)+(T<2)#(T=1)$OR$QTT=1]
[6]   R_AV $QXL N
[7]   $GO 0
[8]  $COM
[9]  $COM*I*    NAME:       FIXTEXT
[10] $COM       PURPOSE:    To provide special mappings for some characters
[11] $COM       DESCRIPTION:
[12] $COM                   THE IDS INTERFACE PERMITS NAMES TO CONTAIN
[13] $COM                   UNDERSCORES, AND APL ON TERMINALS OTHER THAN
[14] $COM                   TERMINAL 1 USES THIS CHARACTER FOR ASSIGNMENT,
[15] $COM                   AND WITHIN THE WORKSPACE, DELTA IS USED TO
[16] $COM                   REPRESENT THE NAME WHEN IT IS SHARED WITH IDS.
[17] $COM                   THE LEFT ARGUMENT CONTROLS THE RESULT TYPE TO
[18] $COM                   RETURN:  0 REQUESTS THAT UNDERSCORES PRINT AS
[19] $COM                   AN UNDERSCORE AND NOT THE MNEMONIC, 1 REQUESTS
[20] $COM                   THAT UNDERSCORES ARE TO BE TRUE UNDERSCORES
[21] $COM                   AND 2 REQUESTS THAT THE UNDERSCORES BE REPLACED
[22] $COM                   BY DELTAS.
    "
 
$COM  $QAV   3     5     6
$COM  $QIO   5
$COM  $QNC   4
$COM  $QTT   5
$COM  AV     0     3_    3]_   5]_   6]
$COM  L      4     5:
$COM  N      0     2     6
$COM  R      0_    2_    6_
$COM  T      0     4_    5     5     5
0;'        FUNCTION: FIELDVALUES   '
 
 
    " R_Q FIELDVALUES N;AV;F;X;CR
[1]  $COM*T* Copyright (c) Bull HN Information Systems Inc., 1989
[2]   AV_$QAV $DMD AV[((13+$QIO)$NE$I 31)/$I 31]_' '
[3]   AV[255+$I 256]_' ' $DMD CR_$QAV[13+$QIO]
[4]   R_0 FIXTEXT CR BUILDNL AV[$QAV$I $QDBFROM(F_$QDBTYPES[N;5]=0)/N]
[5]   $GO (0$NE$QNC 'Q')/QQ $DMD Q_''''
[6]  QQ:R_Q,(((1$TAK$R R),1$MAX$MAX/(R$NE' ')$MAX.#$I -1$TAK$R R)$TAKR),Q
[7]   $GO (2$NE$QNC 'FMTS')/FLDS
[8]   $GO (0=$R X_(1=F_(F+F)+(~F)#FMTS[N;$QIO]>0)/N)/FLDS
[9]   R_R CAT1 BUILDNL(,0,FMTS[X;,1+$QIO])$FMT$QDBFROM X
[10] FLDS:R_R CAT1 BUILDNL$FMT$QDBFROM(0=F)/N
[11]  R_R[$GU$GDF;]
[12]  $GO 0
[13] $COM*I*    NAME:       FIELDVALUES
[14] $COM       PURPOSE:    To return the current values of the items.
[15] $COM       DESCRIPTION:
[16] $COM                   The right argument is a vector of item indices,
[17] $COM                   the result is a matrix, containing one item
[18] $COM                   value per row, text item values delimited by
[19] $COM                   quotes.
    "
 
$COM  $QAV        2     3]    4
