   23      %EQU N_COMMAND#=1;                     /* Command node */
   24
   25      %EQU N_NODENAME#=2;                    /* Node name */
   26
   27      %EQU N_PATHNAME#=3;                    /* Path name */
   28
   29      %EQU N_DIRECTORY#=4;                   /* Path for external directory */
   30
   31      %EQU N_IAM#=10;                        /* IAM command */
   32      %EQU N_NODE#=11;                       /* NODE command */
   33      %EQU N_SYNON#=12;                      /* SYNON command */
   34      %EQU N_END#=16;                        /* END command */
   35
   36      %EQU N_DIALUP#=20;                     /* MODE=DIALUP */
   37      %EQU N_HARDWIRE#=21;                   /* MODE=HARDWIRE */
