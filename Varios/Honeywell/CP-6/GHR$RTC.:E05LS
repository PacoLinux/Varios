PL6.E3A0      #001=GHR$RTC_MON File=GHR$RTC.:E05TSI                              TUE 07/29/97 21:36 Page:1    
        1        1        /*M*  Real Time Clock Interrupt Service Routines                    */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8             NAME: GHR$RTC
        9        9             PURPOSE:  Contains PL6 procedures for the Real Time Clock.
       10       10                                                                            */
       11       11        /*D* NAME: GHR$RTC
       12       12             PURPOSE:  To service the Real Time Clock Interrupt level.
       13       13             CALL: CALL GHR$RTC;
       14       14             DESCRIPTION:  This procedure is called from a L6ASM routine that
       15       15             received control when the RTC Interrupt was triggered.  It is
       16       16             contained in the Monitor IS.  If the monitor was interrupted,
       17       17             it returns.  If the user was interrupted, it calls GHS$MONTIMERWU
       18       18             to handle clock runout requests.  Then it calls GHQ$QEND to check
       19       19             if the interrupted user should be quantum ended.  If so, the
       20       20             ALTRET return is taken.  If the user is to continue running, the
       21       21             normal return is taken.                                        */
       22       22
       23       23        GHR$RTC_MON: PROC ALTRET;
       24       24
       25       25        /*x*                               */
       26       26        %INCLUDE GH_SCHD_E;
       27      122        %INCLUDE G_HJIT_M;
       28      267        %INCLUDE GH_HARDWARE_M;
       29      365 S      %INCLUDE G_LCP6_E;
       30      366 S      %INCLUDE GH_LCP6_M;
       31      367
       32      368        %G$MHJIT (STCLASS="BASED(G$MHJIT$)");
       33     7242        %G$UHJIT (STCLASS="BASED(G$UHJIT$)");
       34    10216        %G$LOW_MEM (STCLASS="BASED(G$LOW_MEM$)");
       35    10265
       36    10266    1   DCL GHQ$QEND ENTRY ALTRET;
       37    10267    1   DCL GHR$MON ENTRY;
PL6.E3A0      #001=GHR$RTC_MON File=GHR$RTC.:E05TSI                              TUE 07/29/97 21:36 Page:2    
       38    10268    1   DCL GHR$USR ENTRY;
       39    10269    1   DCL GHR$IDL ENTRY;
       40    10270    1   DCL GHS$MONTIMRWU ENTRY;
       41    10271
       42    10272    1   DCL G$UHJIT$ PTR SYMREF;
       43    10273    1   DCL G$MHJIT$ PTR SYMREF;
       44    10274    1   DCL G$LOW_MEM$ PTR SYMREF;
       45    10275    1   DCL G_CPUSTATE(0:3) UBIN SYMREF;
       46    10276
       47    10277    1   DCL I UBIN;
       48    10278
       49    10279        %EJECT;
PL6.E3A0      #001=GHR$RTC_MON File=GHR$RTC.:E05TSI                              TUE 07/29/97 21:36 Page:3    
       50    10280
       51    10281    1        IF G$MHJIT.INTRVL_TYPE = %GH_USRXEQ THEN
       52    10282    1           RETURN;
       53    10283    2        IF G_CPUSTATE(G$MHJIT.PNO) = %G_IDLE# THEN DO;
       54    10284    2           CALL GHR$IDL;
       55    10285    2           CALL GHS$MONTIMRWU;
       56    10286    2           END;
       57    10287    1   QEND: ALTRETURN;
       58    10288
       59    10289
       60    10290        /*      Check for quantum end              */
       61    10291    1   GHR$RTC_USR: ENTRY ALTRET;
       62    10292
       63    10293        /*      Set Monitor running               */
       64    10294    1        CALL GHR$MON;
       65    10295    1        CALL GHS$MONTIMRWU;
       66    10296    1        CALL GHQ$QEND ALTRET(QEND);
       67    10297    1        CALL GHR$USR;
       68    10298    1        RETURN;
       69    10299
       70    10300
       71    10301    1   GHR$INITENT: ENTRY ALTRET;
       72    10302    2        DO WHILE ('1'B);
       73    10303    2           I = 0;
       74    10304    2           END;
       75    10305    1        END GHR$RTC_MON;
       76    10306        %EOD;

