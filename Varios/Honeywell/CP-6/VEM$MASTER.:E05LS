      282      851    1   EXEC_CP_ALT:
      283      852    1        CMD$ = CMD_BASE$;
      284      853    2        IF (FLAGS.RLFLAG) THEN DO;
      285      854    2           FLAGS.TXFLAG = TRUE;
      286      855    2           FLAGS.RLFLAG = FALSE;
      287      856    2           END;
PL6.E3A0      #001=VEM$MASTER File=VEM$MASTER.:E05TSI                            WED 07/30/97 07:08 Page:10   
      288      857    1        RETURN;
      289      858        /**/
      290      859    1   IF_FAIL:
      291      860    1        CMD$ = PINCRW(CMD$,CMD$ -> CMD_CNTRLWD.OFFSET);
      292      861    1        IF_LEVEL = 1;
      293      862        /**/
      294      863    2        DO WHILE (CMD$ -> CMD_CNTRLWD.FLAGS.ENDCT =  FALSE);
      295      864    2           CMD_CODE = CMD$ -> CMD_CNTRLWD.CMD_CODE;
      296      865    2           IF (CMD_CODE = IFCMD) THEN IF_LEVEL = IF_LEVEL + 1; /* NESTED      */
      297      866    3           ELSE IF (CMD_CODE = EICMD) THEN DO; /* END IF                      */
      298      867    3                 IF_LEVEL = IF_LEVEL - 1;
      299      868    3                 IF (IF_LEVEL = 0) THEN GOTO NEXT_CMD;
      300      869    3                 END;
      301      870    3              ELSE IF (CMD_CODE = ELCMD)AND(IF_LEVEL = 1) THEN DO; /* PROCESS ELSE */
      302      871    3                    CMD_CODE = IFCMD;
      303      872    3                    GOTO IF_TEST;
      304      873    3                    END;
      305      874    2           CMD$ = PINCRW(CMD$,CMD$ -> CMD_CNTRLWD.OFFSET);
      306      875    2           END;
      307      876        /**/
      308      877    2        IF (FLAGS.RLFLAG) THEN DO;
      309      878    2           FLAGS.TXFLAG = TRUE;
      310      879    2           FLAGS.RLFLAG = FALSE;
      311      880    2           END;
