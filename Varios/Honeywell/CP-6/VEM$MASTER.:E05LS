PL6.E3A0      #001=VEM$MASTER File=VEM$MASTER.:E05TSI                            WED 07/30/97 07:08 Page:11   
      316      885        /**/
      317      886    1   IF_SSE_CHECK: PROC ALTRET;
      318      887        /**/
      319      888    2   DCL 1 STR_FLGS,
      320      889    2          2 CLMNFLAG BIT(1) UNAL,
      321      890    2          2 NOTFLAG BIT(1) UNAL,
      322      891    2          2 ANDFLAG BIT(1) UNAL,
      323      892    2          2 ORFLAG BIT(1) UNAL,
      324      893    2          2 EORFLAG BIT(1) UNAL,
      325      894    2          2 * BIT(13) UNAL;
      326      895    2   DCL IF_EVAL BIT(1) AUTO;
      327      896        /**/
      328      897    2        IF (CMD$ -> IF_CMD_TBL.SSE.PTR$(0) = ADDR(NIL)) THEN RETURN;
      329      898        /**/
      330      899    2        IF_LEVEL = 0;
      331      900    2        IF_EVAL = FALSE;
      332      901        /**/
      333      902    2   IF_SSE_1:
      334      903    2        STR_FLGS = CMD$ -> IF_CMD_TBL.SSE.FLAGS(IF_LEVEL);
      335      904    2        IF ((IF_EVAL) AND (STR_FLGS.ORFLAG)) THEN GOTO IF_SSE_NEXT;
      336      905    2        IF ((~IF_EVAL) AND (STR_FLGS.ANDFLAG)) THEN GOTO IF_SSE_NEXT;
      337      906        /**/
      338      907    2        CALL VEX$FIND_MATCH(ADDR(CMD$ -> IF_CMD_TBL.SSE(IF_LEVEL)),
      339      908    2           CMD$ -> IF_CMD_TBL.LO_CLMN,CMD$ -> IF_CMD_TBL.HI_CLMN,
      340      909    2           MATCH_CLMN) ALTRET(IF_SSE_2);
      341      910                                                /* MATCHING STRING IS PRESENT         */
      342      911    2        IF ((~IF_EVAL) AND
      343      912    2           (((STR_FLGS.ORFLAG) OR (STR_FLGS.EORFLAG)) AND (~STR_FLGS.NOTFLAG)))
      344      913    2        THEN IF_EVAL = TRUE;
      345      914        /**/
      346      915    2        ELSE IF ((IF_EVAL) AND
      347      916    2              ((STR_FLGS.ANDFLAG) AND (STR_FLGS.NOTFLAG)) OR
      348      917    2              ((STR_FLGS.EORFLAG) AND (~STR_FLGS.NOTFLAG)))
      349      918    2           THEN IF_EVAL = FALSE;
      350      919        /**/
      351      920    2        GOTO IF_SSE_NEXT;
      352      921        /**/                                    /* MATCHING STRING NOT FOUND          */
PL6.E3A0      #001=VEM$MASTER File=VEM$MASTER.:E05TSI                            WED 07/30/97 07:08 Page:12   
      353      922    2   IF_SSE_2:
      354      923    2        IF ((~IF_EVAL) AND
      355      924    2           ((STR_FLGS.NOTFLAG)  AND ((STR_FLGS.ORFLAG) OR (STR_FLGS.EORFLAG))))
      356      925    2        THEN IF_EVAL = TRUE;
      357      926        /**/
      358      927    2        ELSE IF ((IF_EVAL) AND
      359      928    2              (((STR_FLGS.ANDFLAG) AND (~STR_FLGS.NOTFLAG)) OR
      360      929    2              ((STR_FLGS.EORFLAG) AND (STR_FLGS.NOTFLAG))))
      361      930    2           THEN IF_EVAL = FALSE;
      362      931        /**/
      363      932    2   IF_SSE_NEXT:
      364      933    2        IF_LEVEL = IF_LEVEL + 1;
      365      934    3        IF (CMD$ -> IF_CMD_TBL.SSE.PTR$(IF_LEVEL) = ADDR(NIL)) THEN DO;
      366      935    3           CALL SEARCHR(REC_SIZE, I, B$SR_NON_BLANK, REC_BUF, REC_SIZE) ;
      367      936    3           REC_SIZE = REC_SIZE + 1 ;
      368      937    3           IF (IF_EVAL) THEN RETURN;
      369      938    3           ELSE ALTRETURN;
      370      939    3           END;
      371      940        /**/
      372      941    2        GOTO IF_SSE_1;
      373      942        /**/
      374      943    2   END;
      375      944    1   END;                                    /* END VEM$MASTER                     */

