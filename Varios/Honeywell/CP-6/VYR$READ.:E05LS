      101    11853    1         SIZE = DCB$  -> F$DCB.ARS#;
      102    11854    2         IF SIZE = 0 THEN DO;
      103    11855                  %ERROR_IT;
      104    11858    2             END;
      105    11859
      106    11860    2         IF M$SI$->F$DCB.DDEV.XEQ# OR M$SI$->F$DCB.DDEV.IC# THEN DO;
