   000000  X 000000 4776 00            231         LDP7    I$MONIS$
   000001    7 00000 7213 10           232         LXL1    0,0,7           FETCH SCREECH CODE
   000002    000007 3612 03            233         ANX1    =O7,DU
   000003    000007 1012 03            234         CMPX1   =O7,DU          CHECK TYPE
   000004  X 000000 6002 00            235         TZE     REBOOT          SCREECH; GET NEW AARDVARK
                                       236  *
                                       237  *  CONTINUE HERE IF SUA OR AFTER AARDVARK IS REBOOTED           *
                                       238  *
   000005                              239  IRR$OPRCVR NULL
   000005  X 000000 4776 00            240          LDP7    I$MONIS$
   000006 (2)002564 4502 00            241         STZ     MSKDFEP
   000007 (2)002553 7402 00            242         STX0    RVB.X0          REMEMBER ADDR OF SCREECH CODE
                                       243  *
   000010    004210 6342 07            244         LDI     =O4210,DL       IR   OVMSK, MM, FLOX
   000011    000000 2332 00            245         RMCM                    INTERRUPT MASK TO AQ
   000012 (2)002554 7572 00            246         STAQ    RVB.IMASK       REMEMBER IT
   000013    000017 3752 07            247         ANA     =O17,DL         ZERO ALL CHANNELS
   000014    000017 3762 07            248         ANQ     =O17,DL
   000015    000000 5532 00            249         SMCM    0               DISABLE ALL INTERRUPTS
   000016 (2)002354 7042 00            250         TSX4    IRR$BMAP_CLOCK  SYSTIME
   000017 (2)002550 1172 00            251         CMPAQ   IR_MS_COOKIE
   000020 (2)000031 6022 00            252         TNC     PRE_COOKIE
   000021 (2)002550 1772 00            253         SBAQ    IR_MS_COOKIE
