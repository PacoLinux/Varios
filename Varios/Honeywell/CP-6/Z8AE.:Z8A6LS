      188     4184    1            ERR_TAIL = ERR$ ;
      189     4185
      190     4186
      191     4187    1          IF ERROR_NUMBER >= %FIRST_FATAL THEN
      192     4188    1            IF COMCOM.ERRLIM = 0 THEN
      193     4189    2              DO;
      194     4190                     /* The error limit has been exceeded.  Issue an
