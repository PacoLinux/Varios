      240    20989    2         2  INFO   REDEF VECTOR_ UNAL,
      241    20990    2           3 BOUND_       UBIN (18)  UNAL,
      242    20991    2           3 *            BIT  (54)  UNAL;
      243    20992    2   DCL SEC_COUNT          SBIN;
      244    20993
      245    20994        %FPT_FDS (FPTN = FPT_FDS, STCLASS = AUTO);
      246    21009        %EJECT;
PL6.E3A0      #001=Z9IO$OUTPUT_OU File=Z9IO.:Z8A6TSI                             SAT 08/09/97 14:02 Page:9    
      247    21010        /*+++++++++++++*/
      248    21011    2     SECTION_COUNT :  PROC  ( COUNT );                 /*#30694*/
      249    21012        /*+++++++++++++*/
      250    21013
      251    21014    3   DCL COUNT      SBIN;
      252    21015    3   DCL SECTION$   PTR;
      253    21016
      254    21017    3   COUNT = COMCOM.SEGMENTC;
      255    21018    3   SECTION$ = COMCOM.BLOCK$;
      256    21019
      257    21020    4   DO WHILE SECTION$ ~= ADDR(NIL);
      258    21021
      259    21022    4     COUNT = COUNT + 1;
      260    21023    4     SECTION$ = SECTION$ -> BLOCK.NEXT;
      261    21024
      262    21025    4   END;
      263    21026
      264    21027    3   END SECTION_COUNT;
      265    21028        %EJECT;
PL6.E3A0      #001=Z9IO$OUTPUT_OU File=Z9IO.:Z8A6TSI                             SAT 08/09/97 14:02 Page:10   
      266    21029    2   IF COMCOM.OB.VIRTUAL THEN
      267    21030    3     DO;    /* For saving a few pages real memory to back up the    */
      268    21031                 /* virtual memory, Z9AM$GET_VIRTUAL_SPACE reserves only */
      269    21032                 /* 12K memory for the auto storage, this 12k will be    */
      270    21033                 /* used up before phase 5, and XUO$OUINIT needs 10K real*/
      271    21034                 /* space, so, some space is freed from the auto segment.*/
      272    21035
      273    21036    3       FPT_FDS.V_ = VECTOR(FPT_FDS.V);
      274    21037    3       FPT_FDS.RESULTS_ = VECTOR (VECT.VECTOR_);
      275    21038    3       FPT_FDS.V.AUTOSEG# = '1'B;
      276    21039    3       FPT_FDS.V.SEGSIZE# = 0;
      277    21040
      278    21041    3       CALL M$FDS (FPT_FDS);
      279    21042
      280    21043    3       IF VECT.INFO.BOUND_ > 3071 THEN
