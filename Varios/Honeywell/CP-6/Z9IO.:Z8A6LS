      332    21095    3                   CALL Z8AE$ERROR(%E$ANNOTATOR_ABORT);
      333    21096    3                   ALTRETURN; /* Abandom attempt to generate OU */ /*#35094*/
      334    21097    3                 END;
      335    21098
      336    21099        /*
      337    21100           If there is a main program in the source file, then
      338    21101           the OU we create will be a main prog.  To find this
      339    21102           we walk the symdef chain for a main_program bit set.
PL6.E3A0      #001=Z9IO$OUTPUT_OU File=Z9IO.:Z8A6TSI                             SAT 08/09/97 14:02 Page:12   
      340    21103        */
      341    21104
      342    21105    2   SYMDEF$ = COMCOM.SYMDEF$;
      343    21106
      344    21107    2   P_OPERND_TYPE = 1; /* Section */
      345    21108
      346    21109    3   DO WHILE (SYMDEF$ ~= ADDR(NIL));
      347    21110    3     IF SYMDEF$ -> SYMDEF_.B.MAIN_PROGRAM THEN
      348    21111    3       EXIT;
      349    21112
      350    21113    3     SYMDEF$ = SYMDEF$ -> SYMDEF_.NEXT;
      351    21114    3   END; /* While */
      352    21115
      353    21116    2   IF SYMDEF$ = ADDR(NIL) THEN
      354    21117    2     P_OPERND_TYPE = 0; /* Not a main program */
      355    21118    2     ELSE START_OFFSET = SYMDEF$ -> SYMDEF_.SA; /* Main program entry address */
      356    21119
      357    21120    2   CALL XUO$HEADSTART (P_OPERND_TYPE,
      358    21121    2                       0,   /* P_OPERAND = 0 */
      359    21122    2                       START_OFFSET);  /* P_OFFSET = 0 or main program start address */
      360    21123
      361    21124    2   IF COMCOM.OU_EXTERNAL_NAME$ ~= ADDR(NIL) THEN
      362    21125    2       CALL XUO$HEADNAME (COMCOM.OU_EXTERNAL_NAME$ -> DNT.TEXT);
      363    21126    2   ELSE
      364    21127    3     DO;
      365    21128    3       P_NAME_HEAD.SIZE = LENGTHC(%DEFAULT_PROG_NAME);
      366    21129    3       P_NAME_HEAD.TEXT = %DEFAULT_PROG_NAME;
      367    21130    3       CALL XUO$HEADNAME (P_NAME_HEAD);
      368    21131    3     END;
      369    21132
      370    21133    2   SEVERITY_INDEX = COMCOM.MAX_ERROR / 100;
      371    21134    2   IF SEVERITY_OF_ERROR(SEVERITY_INDEX) < 4 THEN
      372    21135    2     CALL XUO$HEAD_SEVERITY(0);
      373    21136    2   ELSE
      374    21137    2     CALL XUO$HEAD_SEVERITY(11);
      375    21138
      376    21139    2   CURRENT_SCOPE$ = COMCOM.FIRST_PROC$;
