      114    20849
      115    20850        /*I*
      116    20851               NAME: SPECIFY_OU_SECTION
      117    20852        DESCRIPTION:
      118    20853                  Accept a pointer to a block ,segment, or symref symbol
      119    20854                  table item and enter that data into the appropriate
      120    20855                  OU section or external reference table.
      121    20856        */
      122    20857
      123    20858        %B$SECTIONSUBS;
      124    20873
      125    20874    2   DCL SECTION$ PTR;      /* Pointer to ST item */
      126    20875    2   DCL EREF_INDEX SBIN WORD;     /* External reference table index*/
      127    20876
      128    20877    2   DCL 01 P_SECTION_UTS_ZERO CONSTANT,
      129    20878    2          02  ZERO BIT(1) UNAL INIT(TRUE),
      130    20879    2          02  UTS  BIT(1) UNAL INIT(FALSE),
      131    20880    2          02  *    BIT(6) UNAL INIT(FALSE);
      132    20881
      133    20882    2   DCL SECT_NAME_INDEX SBIN WORD;
      134    20883         %EJECT;
PL6.E3A0      #001=Z9IO$OUTPUT_OU File=Z9IO.:Z8A6TSI                             SAT 08/09/97 14:02 Page:5    
      135    20884        /*+++++++++++++++++++*/
      136    20885    2     SETUP_BLOCK_SEGMENT: PROC (TYPE);
      137    20886        /*+++++++++++++++++++*/
      138    20887
      139    20888    3     DCL TYPE SBIN;  /* Section type code */
      140    20889
      141    20890
      142    20891    3       CALL XUO$DNAME (SECTION$->ST.DNT$->DNT.TEXT,
      143    20892    3                       SECT_NAME_INDEX);
      144    20893
      145    20894    4       DO SELECT(TYPE);
      146    20895
      147    20896    4         SELECT(%DATASECTION, %ROSECTION);
      148    20897
      149    20898    4           CALL XUO$SECTBUILD (TYPE,
      150    20899    4                               SECT_NAME_INDEX,
      151    20900    4                               SECTIONS_ALLOCATED,
      152    20901    4                               P_SECTION_UTS_ZERO,   /* Initialize with zeroes */
      153    20902    4                               ,   /* Even word starting location */
      154    20903    4                              )   /* NSAB reserved for future use */
      155    20904    4               WHENALTRETURN
      156    20905    5                     DO;
      157    20906    5                       CALL Z8AE$ERROR(%E$ANNOTATOR_ABORT);
      158    20907    5                     END;
      159    20908
      160    20909    4         SELECT(ELSE);
      161    20910
      162    20911    4           CALL XUO$SECTBUILD (TYPE,
      163    20912    4                               SECT_NAME_INDEX,
      164    20913    4                               SECTIONS_ALLOCATED,
      165    20914    4                               ,   /* Initialize with zeroes */
      166    20915    4                               ,   /* Even word starting location */
      167    20916    4                              )   /* NSAB reserved for future use */
      168    20917    4               WHENALTRETURN
      169    20918    5                     DO;
      170    20919    5                       CALL Z8AE$ERROR(%E$ANNOTATOR_ABORT);
      171    20920    5                     END;
PL6.E3A0      #001=Z9IO$OUTPUT_OU File=Z9IO.:Z8A6TSI                             SAT 08/09/97 14:02 Page:6    
      172    20921    4       END;  /* Do select */
      173    20922
      174    20923    3       CALL XUO$SECTSIZE (SECTIONS_ALLOCATED,
      175    20924    3                              BINBIT(SECTION$ -> BLOCK.SIZE, 36))
      176    20925    3           WHENALTRETURN
      177    20926    4                 DO;
      178    20927    4                   CALL Z8AE$ERROR(%E$ANNOTATOR_ABORT);
      179    20928    4                 END;
      180    20929            /* There is no need to CALL XUO$SECTNAME, since the section name is
      181    20930               passed to XUO$SECTBUILD. */
      182    20931
      183    20932    3       SECTION$ -> ST.OU_INDEX = SECTIONS_ALLOCATED;
      184    20933
      185    20934    3   END SETUP_BLOCK_SEGMENT;
      186    20935        %EJECT;
PL6.E3A0      #001=Z9IO$OUTPUT_OU File=Z9IO.:Z8A6TSI                             SAT 08/09/97 14:02 Page:7    
      187    20936    3   DO CASE (SECTION$ -> ST.ID);
      188    20937
      189    20938    3     CASE (%BLOCK);
      190    20939    3       IF SECTION$ = COMCOM.BLOCK$ THEN
      191    20940    3         CALL SETUP_BLOCK_SEGMENT(%DATASECTION);
      192    20941    3       ELSE
      193    20942    3         CALL SETUP_BLOCK_SEGMENT(%LCOMSECTION);
