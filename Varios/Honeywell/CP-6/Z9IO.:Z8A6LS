      417    21180        /*------------------*/
      418    21181    1     ENTER_SYMDEF_NAMES : PROC;
      419    21182        /*------------------*/
      420    21183
      421    21184             %SUB_CST;   /* Subs for CALL_TYPE found in B$OBJECT_C */
      422    21200
      423    21201    2        DCL SYMDEF$       PTR;
      424    21202    2        DCL PRIMARY_NAME  BIT (1);
      425    21203    2        DCL CALL_TYPE     UBIN;
      426    21204    2        DCL NUMBER_ARGUMENTS UBIN;
      427    21205    2        DCL SECTION_OFFSET   UBIN;
      428    21206
      429    21207
      430    21208    2        SYMDEF$ = COMCOM.SYMDEF$;
      431    21209
      432    21210    3        DO WHILE (SYMDEF$ ~= ADDR(NIL));
      433    21211
      434    21212    3          CALL XUO$DNAME (SYMDEF$ -> SYMDEF_.DNT$ -> DNT.TEXT,
      435    21213    3                          PROG_NAME_POINTER);
      436    21214
      437    21215    3          IF COMCOM.OU_EXTERNAL_NAME$ = SYMDEF$ -> SYMDEF_.DNT$ THEN
