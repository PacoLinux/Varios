      283    21046
      284    21047    4           CALL M$FDS (FPT_FDS);
      285    21048    4         END;
      286    21049    3     END;
      287    21050
      288    21051    2    CALL SECTION_COUNT( SEC_COUNT );                   /*#30694*/
      289    21052    2   SECTIONS_ALLOCATED = 0;
      290    21053    2   OBJ_SECT_NUMBER = 0;
      291    21054    2   OBJ_SECT_TYPE_R = 1;
      292    21055    2   START_OFFSET = 0;
      293    21056    2   COMCOM.CURRENT.LINE = 0;
      294    21057    2   COMCOM.CURRENT.COLUMN = 0;
      295    21058
