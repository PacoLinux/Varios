      197    20946
      198    20947
      199    20948    3     CASE (%SYMREF_);
      200    20949
      201    20950    3       CALL XUO$RNAME (SECTION$ -> ST.DNT$ -> DNT.TEXT,
      202    20951    3                      SECT_NAME_INDEX);
      203    20952
      204    20953    3       CALL XUO$EREF (SECT_NAME_INDEX,
      205    20954    3                      '0'B,        /* PRIMARY REFERENCE */
      206    20955    3                      '0'B,          /* No altret */
      207    20956    3                      '0'B,         /* NO ARG CHECK */
      208    20957    3                      0,            /* STD CP6 callable procedure */
      209    20958    3                      0,             /* no args */
      210    20959    3                      EREF_INDEX);    /* ENTREF table index */
      211    20960
      212    20961    3       SECTION$ -> ST.OU_INDEX = EREF_INDEX;
      213    20962
      214    20963    3     CASE (%DCB_);
      215    20964
      216    20965    3       CALL SETUP_BLOCK_SEGMENT(%DCBSECTION);
      217    20966
      218    20967
      219    20968    3   END;  /* case on ST.ID */
      220    20969
      221    20970    2   END SPECIFY_OU_SECTION;
