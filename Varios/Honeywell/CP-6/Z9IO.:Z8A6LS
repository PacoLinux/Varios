      632    21410
      633    21411
      634    21412    2       P$ = PROC$;
      635    21413
      636    21414    3       DO WHILE (P$ -> PROC_.FATHER ~= ADDR(NIL)) AND
      637    21415    3                (P$ -> PROC_.BROTHER ~= NEXT_SCOPE$);
      638    21416    3         CALL CLOSE_LOGICAL_BLOCK(P$);
      639    21417
      640    21418    3         P$ = P$ -> PROC_.FATHER;
      641    21419    3       END; /* Do while */
