      382    21145        /*-----------------*/
      383    21146    1     INIT_ALL_SECTIONS : PROC;
      384    21147        /*-----------------*/
      385    21148
      386    21149        /*I*
      387    21150               NAME: INIT_ALL_SECTIONS
      388    21151        DESCRIPTION: Initialize XUO tables by entering all COBOL
      389    21152                       sections and data blocks into tables.  SPECIFY_OU_SECTION
      390    21153                       is called.
      391    21154        */
      392    21155
      393    21156        /* Add all procedure sections to OU */
      394    21157
      395    21158    3   DO COUNT = 0 TO COMCOM.SEGMENTC - 1;
      396    21159    3     CALL SPECIFY_OU_SECTION (COMCOM.SEGMENT$(COUNT));
      397    21160    3   END; /* do */
      398    21161
      399    21162        /* Add all data blocks to OU */
      400    21163
      401    21164    2   SECTION$ = COMCOM.BLOCK$;
      402    21165    3   DO WHILE (SECTION$ ~= ADDR(NIL));
      403    21166    3     CALL SPECIFY_OU_SECTION (SECTION$);
      404    21167    3     SECTION$ = SECTION$ -> BLOCK.NEXT;
      405    21168    3   END;  /* do while */
      406    21169        /* Add all SYMREF's to OU */
      407    21170
      408    21171    2   SECTION$ = COMCOM.SYMREF$;
      409    21172
      410    21173    3   DO WHILE (SECTION$ ~= ADDR(NIL));
      411    21174    3     CALL SPECIFY_OU_SECTION (SECTION$);
      412    21175    3     SECTION$ = SECTION$ -> SYMREF_.NEXT;
      413    21176    3   END; /* While */
      414    21177
      415    21178    2   END INIT_ALL_SECTIONS;
      416    21179        %EJECT;
