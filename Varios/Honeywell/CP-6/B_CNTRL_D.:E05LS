       52+    6444         %LSET LISTDIR='0'B;
       53+    6445         %LSET LISTEXP='1'B;
       54+    6446         %LSET LISTCOM='0'B;
       55+    6447         %LSET LISTSUB='1'B;
       56+    6448         %LSET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
       57+    6449         %IF (SUBSTR(STR,0,6)='STATIC') OR
       58+    6450         (SUBSTR(STR,0,8)='CONSTANT') OR
       59+    6451         (SUBSTR(STR,0,3)='EXT');
       60+    6452         %LSET INIT=CHARTEXT('INIT');
       61+    6453         %ELSE;
       62+    6454         %LSET INIT=CHARTEXT('/*');
       63+    6455         %ENDIF;
       64+    6456
       65+    6457        DCL 1 FPTN STCLASS ALIGNED,
       66+    6458            %IF FIT=1;
       67+    6459              2 COUNT UBIN %INIT(SIZEW(FPTN))%CHARTEXT('/**/'),
PL6.E3A0      #001=B_CNTRL_D File=B_CNTRL_D.:E05TSI                              TUE 07/29/97 11:07 Page:13   
