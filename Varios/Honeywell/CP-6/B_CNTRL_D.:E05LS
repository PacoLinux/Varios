      712+    7104                                /* CPRIO    : max prio that this console
      713+    7105                                              can put jobs up to                    */
      714+    7106
      715+    7107              %(LVL+1) WSN CHAR(8) UNAL %INIT(' ')%CHARTEXT('/**/'),
      716+    7108                                /* WSN      : WSN of issuing console; used
      717+    7109                                              ensure that the console is allowed
      718+    7110                                              to muck with the selected job.        */
      719+    7111
PL6.E3A0      #001=B_CNTRL_D File=B_CNTRL_D.:E05TSI                              TUE 07/29/97 11:07 Page:83   
      720+    7112            %IF TYP=1;    /*PRIO*/
      721+    7113              %(LVL+1) PRIO UBIN %INIT(0)%CHARTEXT('/**/') LAST
      722+    7114                                /* PRIO     : desired new prio for SYSID.           */
      723+    7115
      724+    7116            %ELSEIF TYP=2;/*RERUN*/
      725+    7117              %(LVL+1) SELECTION SBIN %INIT(0)%CHARTEXT('/**/') LAST
      726+    7118                                /* SELECTION: = 0 ---> !RERUN SYSID
      727+    7119                                              < 0 ---> !RERUN ALL
      728+    7120                                              > 0 ---> !ZAP                         */
      729+    7121
      730+    7122            %ELSEIF TYP=3;/*'ALL' E.G. DELETEIALL, PRIOIALL */
      731+    7123              %(LVL+1) ACCT CHAR(8) %INIT(' ')%CHARTEXT('/**/'),
      732+    7124                                /* ACCT     : if nonblank, restricts the
      733+    7125                                              operation to this account.            */
      734+    7126
      735+    7127              %(LVL+1) NEWPRIO SBIN %INIT(0)%CHARTEXT('/**/'),
      736+    7128                                /* NEWPRIO  : for PRIOALL, the desired prio.        */
      737+    7129
      738+    7130              %(LVL+1) OLDPRIO SBIN %INIT(0)%CHARTEXT('/**/') LAST
      739+    7131                                /* OLDPRIO  : for PRIOALL, -1 means operate
      740+    7132                                              on all jobs, else operate only
      741+    7133                                              on those at this prio.                */
      742+    7134
      743+    7135            %ENDIF;
      744+    7136        %MEND;
