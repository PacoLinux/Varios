#define   FALSE        0
#define   TRUE         1
#define   FOREVER      1
#define   NEVER        0
 
/*  RETURN values for errors, etc  */
#define   OK           0
#define   ILLEGAL      1
#define   GOTO_BEGIN   2
#define   FATAL        9
 
/*  COMMAND OPTIONS  */
#define   CMD_HELP        0
#define   CMD_IPI_EXT_ST  1
#define   CMD_RESP_PACK   2
#define   CMD_PARAM_ID    3
#define   CMD_REV_INFO    4
 
/*  HELP constants  */
#define   H_MENU       0        /* HELP called from main menu area */
#define   H_ONE        1
#define   H_TWO        2
#define   H_THREE      3
#define   H_FOUR       4
#define   H_FIVE       5
 
/*  BOUNDS  */
#define   MAXBYTES   200       /* maximun IPI Extended Status Bytes */
 
/*  DEFINES FOR READABILITY  */
#define   BYTE_0     '0'
#define   BYTE_1     '1'
#define   BYTE_2     '2'
#define   BYTE_3     '3'
#define   BYTE_4     '4'
#define   BYTE_5     '5'
#define   BYTE_6     '6'
#define   BYTE_7     '7'
#define   BYTE_8     '8'
#define   BYTE_9     '9'
#define   BYTE_10    'A'
 
#define   PARAM_ID_X2      '2'
#define   PARAM_ID_X3      '3'
#define   PARAM_ID_X4      '4'
#define   PARAM_ID_X5      '5'
#define   PARAM_ID_X6      '6'
#define   PARAM_ID_X7      '7'
#define   PARAM_ID_X8      '8'
#define   PARAM_ID_X9      '9'
#define   PARAM_ID_Xa      'a'
#define   PARAM_ID_XA      'A'
 
#define  STATUS_TYPE_1     '1'
#define  STATUS_TYPE_3     '3'
#define  STATUS_TYPE_4     '4'
#define  STATUS_TYPE_5     '5'
 
#define  OCTET_1     '1'
#define  OCTET_2     '2'
#define  OCTET_3     '3'
#define  OCTET_4     '4'
#define  OCTET_5     '5'
#define  OCTET_6     '6'
#define  OCTET_7     '7'
#define  OCTET_8     '8'
#define  OCTET_9     '9'
#define  OCTET_A     'A'
#define  OCTET_B     'B'
#define  OCTET_C     'C'
#define  OCTET_D     'D'
#define  OCTET_E     'E'
#define  OCTET_F     'F'
#define  OCTET_G     'G'
#define  OCTET_H     'H'
 
#define  BIT_0      0x80
#define  BIT_1      0x40
#define  BIT_2      0x20
#define  BIT_3      0x10
#define  BIT_4      0x08
#define  BIT_5      0x04
#define  BIT_6      0x02
#define  BIT_7      0x01
 
#define  HEX_03     0x03
#define  HEX_05     0x05
#define  HEX_0B     0x0b
#define  HEX_0C     0x0c
#define  HEX_0F     0x0f
#define  HEX_21     0x21
#define  HEX_22     0x22
#define  HEX_2F     0x2f
#define  HEX_30     0x30
#define  HEX_3F     0x3f
#define  HEX_67     0x67
#define  HEX_7C     0x7c
#define  HEX_7D     0x7d
#define  HEX_7F     0x7f
#define  HEX_D0     0xd0
#define  HEX_E6     0xe6
#define  HEX_EF     0xef
#define  HEX_F0     0xf0
#define  HEX_F9     0xf9
#define  HEX_FF     0xff
