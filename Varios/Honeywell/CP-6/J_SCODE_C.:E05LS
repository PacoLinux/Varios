    7      %EQU S$SSFRAME=1/*S$SSFRAME*/;          /*INVALID FRAME ON SAFE-STORE         */
    8      %EQU S$MAGIC=2/*S$MAGIC*/;              /*THE USERS ARGUEMENT STACK IS FULL   */
    9      %EQU S$CPTCB=3/*S$CPTCB*/;              /*CANT GET TCB FOR COMMAND PROCESSOR  */
   10      %EQU S$RUNDSPC=4/*S$RUNDSPC*/;          /*RUNDSPC CALLED WITH BAD WSR*/
   11      %EQU S$CPEXIT=5/*S$CPEXIT*/;            /* CANT ANLZ M$CPEXIT-JSP$PMME        */
   12      %EQU S$GHOST=6/*S$GHOST*/;              /* SYSTEM GHOST ABORTED               */
   13      %EQU S$FMRUND=7/*S$FMRUND*/;            /* FMSEG NOT EMPTY AT RUNDOWN*/
   14      %EQU S$MING=8/*S$MING*/;                /*SYSTEM GHOST ABORTED                */
   15
   16      /*     Load and Link Screech Codes (200-209) */
   17      %EQU S$BADNF=200/*S$BADNF*/;            /*BAD *N FILE                         */
   18      %EQU S$SYSIDNF=201/*S$SYSIDNF*/;        /*SYSID MISMATCH IN *N                */
   19      %EQU S$INFONF=202/*S$INFONF*/;          /*INCONSISTANT INFO IN *N             */
   20      %EQU S$IOERRNF=203/*S$IOERRNF*/;        /*IO ERROR ON *N                      */
   21      %EQU S$LDLNK=204/*S$LDLNK*/;            /*ERROR IN LDLNK CLEANUP*/
   22      %EQU S$SSPDS=205;               /* Unable to initialize processor data segs */
   23      %EQU S$VARRAY=206;            /* Cant open/close virtual array */
   24
   25      %EQU S$DELSET=207;            /* Error deleting records from *N */
   26
   27      /*     AUTOSAVE Screech Codes    */
   28      %EQU S$ASAVE=208/*S$ASAVE*/;            /* Unexpected MM err in autosave      */
   29
   30      /*     SEGLD Screech Codes    */
   31      %EQU S$SEGLD=209/*S$SEGLD*/;            /* ERROR REPLACING OVERLAY            */
   32
   33      /*     More Job Step Codes    */
   34      %EQU S$WSTAR=210;             /* Error writing to *S file */
   35      %EQU S$DELUSR=211;              /* The REG to delete user returned. */
   36      %EQU S$CPSUA=1080/*S$CPSUA*/;                /*COMMAND PROC ABORTED                */

