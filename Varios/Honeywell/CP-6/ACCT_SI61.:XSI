CONTUTS: PROC (UTS, TIME, DATE);
DCL UTS UBIN;
DCL TIME CHAR(11);
DCL DATE CHAR(8);
%INCLUDE CP_6;
%FPT_TIME (FPTN=FPT_TIME, STCLASS=STATIC,
     SOURCE=UTS,
           DEST=LOCAL);
                   /**/
     FPT_TIME.TSTAMP_ = VECTOR(UTS);
     FPT_TIME.DATE_ = VECTOR(DATE);
     FPT_TIME.TIME_ = VECTOR(TIME);
     CALL M$TIME(FPT_TIME);
     RETURN;
END;
