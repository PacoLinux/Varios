    9      %EQU S$STRSERR=50/*STRSERR*/;           /* ERROR DOING ACCOUNTING */
   10      %EQU S$BDREM=51/*BDREM*/;               /*REMOVE OF OPEN OR UNMOUNTED RES*/
   11      %EQU S$BCLRES=52/*BCLRES*/;             /*RESOURCE CLOSE CALLED ON NON-RES*/
   12      %EQU S$RSOWNR=53/*RSOWNR*/;             /*CLOSER OF RES NOT OWNER*/
   13      %EQU S$RSNOPN=54/*RSNOPN*/;             /*CLOSE OF UNOPENED RESOURCE*/
   14      %EQU S$NRNDWN=55/*NRNDWN*/;             /*RES STILL OPEN AT DELETE USER TIME*/
   15      %EQU S$AVRDCT=56/*AVRDCT*/;             /*BAD DCTX FROM AVR*/
   16      %EQU S$AVRTYP=57/*AVRTYP*/;             /*BAD RES TYPE FROM AVR*/
   17      %EQU S$R2MANY=58/*R2MANY*/;             /*MORE RES RELEASED THAN EXIST*/
   18      %EQU S$R2FEW=59;                        /*call to deluser with resources allocated*/

