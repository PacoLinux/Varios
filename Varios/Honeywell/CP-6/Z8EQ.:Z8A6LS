PL6.E3A0      #008=Z8EQ$OPTIMIZED_INITIAL File=Z8EQ.:Z8A6TSI                     SAT 08/09/97 12:45 Page:246  
     1341    16321    1     SON_LTS$ = INIT_LTS$ -> INIT_LTS.SON$;
     1342    16322    1     IF SON_LTS$ = ADDR (NIL) THEN
     1343    16323    2       DO;
     1344    16324    2         LOP$ = INIT_LTS$ -> INIT_LTS.BASE$;
     1345    16325    2         IF (NOT INIT_LTS$ -> INIT_LTS.GROUP OR
     1346    16326    2             NOT INIT_LTS$ -> INIT_LTS.MFILLABLE) AND
     1347    16327    2            NOT INIT_LTS$ -> INIT_LTS.BCD AND
     1348    16328    2            LOP$ -> ETT.ID = %DNA /* Elementary numeric item */
     1349    16329    2           THEN LOP$ = LOP$ -> ETT.LOP; /* Remove DNA */
     1350    16330    2         IF INIT_LTS$ -> INIT_LTS.SUBSCR$ = ADDR (NIL) THEN
     1351    16331    3           DO;
     1352    16332    3             IF INIT_LTS$ -> INIT_LTS.OFF ~= 0 THEN
     1353    16333    3               CALL Z8AF$NEW_OFF (LOP$,
     1354    16334    3                                  INIT_LTS$ -> INIT_LTS.OFF,
     1355    16335    3                                  LOP$);
     1356    16336    3           END;
     1357    16337    2         ELSE
     1358    16338    3           DO; /* Z8AF$NEW_OFF_AND_SSOP_NO_SBCHK removes the SIZ triad if any */
     1359    16339    3             CALL Z8AF$NEW_OFF_AND_SSOP_NO_SBCHK (LOP$,
     1360    16340    3                                                  INIT_LTS$ -> INIT_LTS.OFF,
     1361    16341    3                                                  INIT_LTS$ -> INIT_LTS.SUBSCR$,
     1362    16342    3                                                  LOP$);
     1363    16343    3             IF INIT_LTS$ -> INIT_LTS.BASE$ -> ETT.ID = %SIZ THEN
     1364    16344    3               CALL Z8AF$FORME (%SIZ, LOP$, INIT_LTS$ -> INIT_LTS.BASE$ -> ETT.ROP,
             16344                        LOP$);
     1365    16345    3           END;
     1366    16346              %FISH (TRIAD = INIT_LTS$ -> INIT_LTS.BASE$, RTN = ST$$);
     1367    16354    2         IF INIT_LTS$ -> INIT_LTS.GROUP
     1368    16355    2             AND INIT_LTS$ -> INIT_LTS.MFILLABLE THEN
     1369    16356    3           DO;
     1370    16357    3             IF INIT_LTS$ -> INIT_LTS.BCD AND ST$$ -> VARIABLE.TYPE = %TYA THEN
     1371    16358    4               DO;
     1372    16359                      /* Group item composed only of GBCD elementary items */
     1373    16360    4                 MKCN.CON = INIT_LTS$ -> INIT_LTS.LENGTH * 3 / 2;
     1374    16361    4                 CALL Z8AC$MAKE_CONST (ETT$$);
     1375    16362    4                 CALL Z8AF$FORME (%SIZ, LOP$, ETT$$, LOP$);
     1376    16363    4                 CALL Z8AF$FORME (%D96, LOP$, 0, LOP$);
PL6.E3A0      #008=Z8EQ$OPTIMIZED_INITIAL File=Z8EQ.:Z8A6TSI                     SAT 08/09/97 12:45 Page:247  
     1377    16364    4               END;
     1378    16365                  %NEW_ILL (@ID   = %MFILL,
     1379    16366                            @LOP  = LOP$,
     1380    16367                            @FILL = INIT_LTS$ -> INIT_LTS.FILL);
     1381    16746    3           END;
     1382    16747    2         ELSE
     1383    16748    3           DO; /* Elementary item */
     1384    16749    3             IF INIT_LTS$ -> INIT_LTS.BASE$ -> ETT.ID = %DNA AND
     1385    16750    3                 INIT_LTS$ -> INIT_LTS.BCD THEN
     1386    16751    3               CALL Z8AF$FORME (%DAN, LOP$, 0, LOP$);
     1387    16752                  %INIT_LX_ETT_ITEM (@POINTER = "ADDR (DATA_ITEM)",
     1388    16753                                     @ETT$    = LOP$,
     1389    16754                                     @TYPE    = ST$$ -> VARIABLE.TYPE);
     1390    16776    3             IF INIT_LTS$ -> INIT_LTS.POINTER THEN
     1391    16777    3               CALL Z8E7$POINTER_MOVE (ADDR (DATA_ITEM), ADDR (NIL));
     1392    16778    3             ELSE
     1393    16779    4               DO;
     1394    16780    4                 IF ST$$ -> VARIABLE.B.SUB_CLASS.TYPE = %CLASS_NUMERIC
     1395    16781    4                   THEN INIT_VAL.SYMBOL = %RSV_WRD_ZEROES;
     1396    16782    4                   ELSE INIT_VAL.SYMBOL = %RSV_WRD_SPACES;
     1397    16783    4                 INIT_VAL.LINE = ST$$ -> VARIABLE.LINE_NO;
