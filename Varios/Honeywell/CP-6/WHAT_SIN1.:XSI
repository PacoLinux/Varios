%LIST;
%INCLUDE WHAT_C1;
%EJECT;
        WHAT_NODES(%ROOT##,D) = ( .FID ['('] [<',' , WHAT_CMDS >] [')'] .END )
 
          WHAT_CMDS = [.B] ( AALL         | ;
                             ACORE        | ;
                             ALL          | ;
                             ANSPROT      | ;
                             BOOTFLG      | ;
                             GRANLOW      | ;
                             GRANSCHED    | ;
                             HEADER       | ;
                             HOLDEXPIRE   | ;
                             INIT_UTS     | ;
                             MINI_ID      | ;
                             MONINFO      | ;
                             MON_UTS      | ;
                             MUAIS        | ;
                             NUM_NODES    | ;
                             NODE_NAME    | ;
                             NODE         | ;
                             PATCHWEEK    | ;
                             PCORE        | ;
                             SALUTATION   | ;
                             SCOUNT       | ;
                             SITE_ID      | ;
                             SITE_NAME    | ;
                             SITEINFO     | ;
                             STREAMFILEID | ;
                             SUA_UTS      | ;
                             SYMBINFO     | ;
                             VERSION      | ;
                             WATCH        )
          AALL(%AALL##)                = 'A/LMOST' 'A/LL'
          ACORE(%ACORE##)              = 'AC/ORE'
          ALL(%ALL##)                  = 'AL/L'
          ANSPROT(%ANSPROT##)          = 'AN/SPROT'
          BOOTFLG(%BOOTFLG##)          = 'B/OOTFLG'
          GRANLOW(%GRANLOW##)          = 'GRANL/OW'
          GRANSCHED(%GRANSCHED##)      = 'GRANS/CHED'
          HEADER(%HEADER##)            = 'HE/ADER' ['I/NG']
          HOLDEXPIRE(%HOLDEXPIRE##)    = 'H/OLD' ['E/XPIRE']
          INIT_UTS(%INIT_UTS##)        = 'I/NIT_UTS'
          MINI_ID(%MINI_ID##)          = 'MI/NI_ID'
          MONINFO(%MONINFO##)          = 'MONI/NFO'
          MON_UTS(%MON_UTS##)          = 'MON_/UTS'
          MUAIS(%MUAIS##)              = 'M/UAIS'
          NODE(%NODE##)                = 'N/ODE#'
          NODE_NAME(%NODE_NAME##)      = 'N/ODE' ['_'] 'N/AME'
          NUM_NODES(%NUM_NODES##)      = 'NU/M' ['_'] 'N/ODES'
          PATCHWEEK(%PATCHWEEK##)      = 'PA/TCH' ['WEEK']
          PCORE(%PCORE##)              = 'P/CORE'
          SALUTATION(%SALUTATION##)    = 'SA/LUTATION'
          SCOUNT(%SCOUNT##)            = 'SC/OUNT'
          SITE_ID(%SITE_ID##)          = 'SITE_I/D'
          SITE_NAME(%SITE_NAME##)      = 'SITE_N/AME'
          SITEINFO(%SITEINFO##)        = 'SITEI/NFO'
          STREAMFILEID(%STREAMFILEID##)= 'ST/REAMFILEID'
          SUA_UTS(%SUA_UTS##)          = 'SU/A_UTS'
          SYMBINFO(%SYMBINFO##)        = 'SY/MBINFO'
          VERSION(%VERSION##)          = 'V/ERSION'
          WATCH(%WATCH##)              = 'W/ATCH'
