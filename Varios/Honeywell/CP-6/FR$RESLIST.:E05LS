    7      /**/
    8      /**/
    9      %MACRO FR$RESLIST (FPTN=FR$RESLIST,STCLASS=BASED);
   10      /**/
   11      DCL 1 FPTN STCLASS DALIGNED,
   12            2 NN SBIN WORD,
   13            2 FAILX UBIN WORD,
   14            2 MEMORY UBIN HALF UNAL,
   15            2 DISK UBIN HALF UNAL,
   16            2 TAPES UBIN HALF UNAL,
   17            2 OTHER UBIN HALF UNAL,
   18            2 SULST (0:7) UBIN(9) UNAL;
   19      /**/
   20      %MEND;
   21      /**/
   22      /**/

