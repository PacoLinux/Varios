     1514    37442    2           %XSP_FNC_PPERIND#);     /* XSP_PEV_PPERIND */
     1515    37443
     1516    37444    2       IF EVENT < %XSP_PEV_SACDREQ THEN
     1517    37445    2           GOTO SEND_PPDU;
     1518    37446    3       IF EVENT >= %XSP_PEV_SACDREQ AND EVENT <= %XSP_PEV_SCONCFN THEN DO;
     1519    37447                /* this is a Session event */
     1520    37448    3           IF BINBIT( EVENT, 1) THEN
     1521    37449                    /* these can only be destined for the other endpoint */
     1522    37450    3               CALL XSS$SCREECH( XSP_F$EVNTERR);
     1523    37451    3           VBLK.EVENT = EVENT;
