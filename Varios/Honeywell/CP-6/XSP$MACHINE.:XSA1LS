     1527    37455    4               ALTRETURN;
     1528    37456    4               END;
     1529    37457    3           RETURN;
     1530    37458    3           END;
     1531    37459            /* determine whether event is to be reported to this user or other end */
     1532    37460    3       IF BINBIT( EVENT, 1) THEN DO;
     1533    37461                /* odd event number means report it (as FPT) to this user */
     1534    37462    3           XSP$GENERIC.FNC = PEV_TO_FNC( EVENT / 2 - 32);
     1535    37463    3           IF XSS_TRACE > 0 THEN
     1536    37464    3               CALL XSP$DUMPFPT( XSP$GENERIC);
     1537    37465    4           IF PCTX.ENTRY$ ~= ENTADDR( NIL) THEN DO;
     1538    37466    4               CALL PCTX.ENTRY$( XSP$GENERIC)
     1539    37467    5               WHENALTRETURN DO;
     1540    37468    5                   PCTX.ABORTING = %TRUE;
     1541    37469    5                   END;
     1542    37470    4               END;
     1543    37471    3           ELSE
     1544    37472                    /* no Presentation-user handler routine defined.  Oops! */
PL6.E3A0      #001=XSP$MACHINE File=XSP$MACHINE.:XSA1TSI                         WED 10/01/97 12:04 Page:46   
     1545    37473    3               CALL XSS$SCREECH( XSP_F$NOPRES);
     1546    37474    3           END;
     1547    37475    3       ELSE DO;
     1548    37476                /* even event number means send it (as PPDU) to the other end */
     1549    37477    3   SEND_PPDU:
     1550    37478    3           VBLK.EVENT = EVENT;
     1551    37479    3           CALL XSP$ENCODE( VBLK)
     1552    37480    4           WHENALTRETURN DO;
     1553    37481    4               PCTX.ABORTING = %TRUE;
     1554    37482    4               END;
     1555    37483    3           END;
     1556    37484    2       IF PCTX.ABORTING THEN
     1557    37485    2           ALTRETURN;
     1558    37486    2       RETURN;
     1559    37487
     1560    37488    2       END EVNT;
     1561    37489        %EJECT;
