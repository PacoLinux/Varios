     1450    37378    3               CALL EVNT( %XSP_PEV_ARP);  /* ignore possible ALTRETURN */
     1451    37379    3               CALL EVNT( %XSP_PEV_PPABIND);  /* ignore possible ALTRETURN */
     1452    37380    3               PCTX.STATE = %XSP_PST_I0;
     1453    37381    3               END;
     1454    37382    2           END;
     1455    37383    2       IF PCTX.STATE = %XSP_PST_I0 THEN DO;
     1456    37384                /* State is 'Idle, no connection' so release context */
     1457    37385    2           CALL XSP$RELCTX( VBLK.CTX$);
     1458    37386    2           IF PCTX.ABORTING THEN
     1459    37387                    /* during this trip thru the state machine, connection aborted */
     1460    37388                    /* have E$STATE or other error reported to user */
     1461    37389    2               ALTRETURN;
     1462    37390    2           END;
