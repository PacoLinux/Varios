     1562    37490        /*****************************************************************************
     1563    37491        *                                                                            *
     1564    37492        *       OOPS Routine                                                         *
     1565    37493        *                                                                            *
     1566    37494        *****************************************************************************/
     1567    37495    1   OOPS: PROC;
     1568    37496
     1569    37497            /* An illegal state/event combination was detected */
     1570    37498    2       IF VBLK.EVENT < %XSP_PEV_PACDREQ THEN
     1571    37499                /* Illegal incoming PPDU or Session event, so abort connection */
     1572    37500    2           PCTX.ABORTING = %TRUE;
     1573    37501    2       ELSE
     1574    37502                /* Illegal event was a user request, so just crab about it */
     1575    37503    2           PCTX.STATE = PCTX.STATE + 128;
     1576    37504    2       RETURN;
     1577    37505
     1578    37506    2       END OOPS;
