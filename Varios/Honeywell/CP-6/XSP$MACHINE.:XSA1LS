        7        7        /*X* DDA */
        8        8        /*P*
        9        9                NAME:   XSP$MACHINE
       10       10                PURPOSE:
       11       11                    This module is the state machine for the ISO Presentation
       12       12                    layer.
       13       13        */
       14       14        /*F*
       15       15                NAME:   XSP$MACHINE
       16       16                PURPOSE:
       17       17                    This module is the state machine for the ISO Presentation
       18       18                    layer.
       19       19                DESCRIPTION:
       20       20                    On entry, the parameter VBLK contains or points to all the
       21       21                    information of interest.  The event being reported is combined
       22       22                    with the current state of the connection and looked up in the
       23       23                    table of permitted actions.  Depending on the particular action
       24       24                    to be performed, other routines are called.  In general, internal
       25       25                    Presentation routines are called, various flags are updated in
       26       26                    the Presentation connection context, the state of the connection
       27       27                    is possibly updated, and the module RETURNs.
       28       28
       29       29                    This module assumes that VBLK.CTX$ points to the Presentation
       30       30                    connection context for this event, and that VBLK.FPT$ points to
       31       31                    a Presentation primitive FPT corresponding to this event.  In
       32       32                    the case of an outgoing event, VBLK.FPT$ points to the user-
       33       33                    supplied Request or Response FPT that caused the event.  In the
       34       34                    case of an incoming event, VBLK.FPT$ points to the library-
       35       35                    supplied Indication or Confirm FPT that will be reported to the
       36       36                    application's Presentation handler.
       37       37
PL6.E3A0      #001=XSP$MACHINE File=XSP$MACHINE.:XSA1TSI                         WED 10/01/97 12:04 Page:2    
       38       38                    In the code below, the notation TABLE xx refers to a specific
       39       39                    protocol state machine table in the ISO Presentation protocol
       40       40                    spec.
       41       41        */
       42       42        XSP$MACHINE: PROC( VBLK) ALTRET;
       43       43        %INCLUDE XSS_SESSION_E;
       44      608        %INCLUDE XSP_PRESENTATION_E;
       45      707        %INCLUDE XSP_PRESENTATION_M;
       46     2193            %XSP$PRIMITIVES( STCLASS="BASED( PFPT$)");
       47     3892            %XSP$CTXTLIST( NAME=CLST, STCLASS="BASED( CLST$)");
       48     3919            %XSP$CTXTLIST( NAME=XSP_ABSLIST, STCLASS=SYMREF);
       49     3946            %XSP$CTXTLIST( NAME=XSP_TFRLIST, STCLASS=SYMREF);
       50     3973            %XSP$CTXTNAME( NAME=NAME, STCLASS=BASED);
       51     4015        %INCLUDE XSP_INTERNAL_E;
       52     4289        %INCLUDE XSP_INTERNAL_M;
       53     4950            %XSP$PCTX( NAME=PCTX, STCLASS="BASED( PCTX$)");
       54     5106            %XSP$VBLK( NAME=VBLK, STCLASS="");
       55     5252            %XSP$CTXTMAP( NAME=CMAP, STCLASS="BASED( CMAP$)");
       56     5293        %INCLUDE XSP_MACHINE_M;
       57     5508        %INCLUDE UM_CP6;
       58     6360        %MACRO FATALDEF( NAME=XSP_FATAL, STCLASS="CONSTANT SYMDEF", LVL=1, LAST=";",
       59     6361                         ERR#=0, SEV=7);
       60     6362            %VLP_ERRCODE( FPTN=NAME, STCLASS=STCLASS, FCG='XS', MID='P',
       61     6363                          MON='0'B, ERR#=ERR#, SEV=SEV);
       62     6364        %MEND;
       63     6365        %INCLUDE XSC_PERR_C;
       64     6442            %FATALDEF( NAME=XSP_F$EVNTERR, ERR#=%F$EVNTERR);
       65     6490            /*E*  ERROR:  XSP-F$EVNTERR-7
       66     6491                MESSAGE:  State machine requested wrong event.
       67     6492            */
       68     6493            %FATALDEF( NAME=XSP_F$NOPRES, ERR#=%F$NOPRES);
       69     6541            /*E*  ERROR:  XSP-F$NOPRES-7
       70     6542                MESSAGE:  No handler for this Presentation primitive.
       71     6543            */
       72     6544        %EQU MAXBITS = 8;  /* biggest context id expected from user */
       73     6545
       74     6546    1       DCL XSP$DUMPFPT ENTRY(1);
PL6.E3A0      #001=XSP$MACHINE File=XSP$MACHINE.:XSA1TSI                         WED 10/01/97 12:04 Page:3    
       75     6547    1       DCL XSP$ENCODE ENTRY(1) ALTRET;
       76     6548    1       DCL XSP$RELCTX ENTRY(1) ALTRET;
       77     6549    1       DCL XSP$SSEND ENTRY(1) ALTRET;
       78     6550    1       DCL XSS$BUFPTR ENTRY(2) ALTRET;
       79     6551    1       DCL XSS$SCREECH ENTRY(1);
       80     6552
       81     6553    1       DCL XSS_TRACE SBIN SYMREF;
       82     6554
       83     6555    1       DCL CLST$ PTR;
       84     6556    1       DCL CMAP$ PTR;
       85     6557    1       DCL I SBIN;
       86     6558    1       DCL J SBIN;
       87     6559    1       DCL PCTX$ PTR;
       88     6560    1       DCL PFPT$ PTR;
       89     6561            /* PRED, PINV, and CONTINUING used by the {IF/ELSE/END}PREDDO macros */
       90     6562    1       DCL PRED(0:35) BIT(1) UNAL;
       91     6563    1       DCL PINV(0:35) BIT(1) UNAL;
       92     6564    1       DCL CONTINUING BIT(1);
       93     6565        %EJECT;
PL6.E3A0      #001=XSP$MACHINE File=XSP$MACHINE.:XSA1TSI                         WED 10/01/97 12:04 Page:4    
       94     6566        /*****************************************************************************
       95     6567        *                                                                            *
       96     6568        *       Various internal MACROs                                              *
       97     6569        *                                                                            *
       98     6570        *****************************************************************************/
       99     6571        %MACRO EVENT( EV=0); /* generate an output event of the specified type */
      100     6572            CALL EVNT( EV) ALTRET( EVNTERR);
      101     6573        %MEND;
      102     6574        %MACRO INVALID; /* an illegal state/event combination has occurred */
      103     6575            CALL OOPS;
      104     6576        %MEND;
      105     6577        %EJECT;
PL6.E3A0      #001=XSP$MACHINE File=XSP$MACHINE.:XSA1TSI                         WED 10/01/97 12:04 Page:5    
      106     6578        /*****************************************************************************
      107     6579        *                                                                            *
      108     6580        *       Set up to enter the Presentation state machine                       *
      109     6581        *                                                                            *
      110     6582        *****************************************************************************/
      111     6583    1       PCTX$ = VBLK.CTX$;
      112     6584    1       PFPT$ = VBLK.FPT$;
      113     6585    1       CALL XSS$BUFPTR( PCTX.CTXTMAPX, CMAP$)
      114     6586    2       WHENALTRETURN DO;
      115     6587    2           CMAP$ = ADDR( NIL);
      116     6588    2           END;
      117     6589            /* update state/event history */
      118     6590    1       I = 1 + PCTX.HISTX;
      119     6591    1       IF I > 7 THEN
      120     6592    1           I = 0;
      121     6593    1       PCTX.HISTX = I;
      122     6594    1       PCTX.HIST.EVENT( I) = VBLK.EVENT;
      123     6595    1       PCTX.HIST.NXTST( I) = %XSP_PST_00;  /* filled in before RETURN */
      124     6596        %SET INPRED = '0'B;  /* set up {IF|ELSE|END}PREDDO macro invocation check */
      125     6597            /* set predicate flags up */
      126     6598    1       PRED = '000000000000'O;
      127     6599    1       PINV = '777777777777'O;
      128     6600            /* fan out on the event and state */
      129     6601    2       DO CASE( VBLK.EVENT);
      130     6602        %EJECT;
