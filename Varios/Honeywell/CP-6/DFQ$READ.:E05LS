       39     5995    1        FPT_READFED.V_ = VECTOR(FPT_READFED.V);
       40     5996    1        FPT_READFED.V.DCB# = CONTEXT$ -> FPRG$CONTEXT.DCBNO;
       41     5997    1        IF NODAT
       42     5998    1        THEN FPT_READFED.V.DVBYTE.NODAT# = '1'B;
