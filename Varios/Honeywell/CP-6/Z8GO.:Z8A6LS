      753    13273
      754    13274    7               CASE (ELSE) ;
      755    13275
      756    13276                      %INCR_COUNT (OP = LOP, BIT = ARITH) ;
      757    13341
      758    13342    7             END ; /* Case of ID */
      759    13343
      760    13344    6           CASE (%ROP_EXPRESSION_NO_REDEF) ; /* Not Used */
      761    13345                  %INCR_COUNT (OP = ROP, BIT = ARITH) ;
      762    13417
      763    13418    6           CASE (%TWO_EXPRESSIONS_NO_REDEF) ;
      764    13419    6             IF ILL_ETT$ -> ILL.LOP -> ETT.ID <= %LAST_ETT THEN
      765    13420    7               DO;
      766    13421                      %INCR_COUNT (OP = LOP, BIT = ARITH) ;
      767    13486    7               END;
      768    13487                  %INCR_COUNT (OP = ROP, BIT = ARITH) ;
      769    13559
      770    13560                  /* Update op. seg. count if exponentiation */
      771    13561
      772    13562    6             IF ILL_ETT$ -> ETT.ID = %EXDD
      773    13563    6             OR ILL_ETT$ -> ETT.ID = %EXRR
      774    13564    6             OR ILL_ETT$ -> ETT.ID = %EXQQ     THEN
