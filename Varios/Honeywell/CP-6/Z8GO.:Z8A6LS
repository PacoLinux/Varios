      852    13777    6           HEAD_CCOSU = O3COM.EVALP$ ;
      853    13778    6           IF TAIL_CCOSU = ADDR(NIL)    THEN
      854    13779    6             TAIL_CCOSU = O3COM.EVALP$ ;
      855    13780
      856    13781    6           IF O3COM.CURRENT_SEQ_NO = COMCOM.FIRST_ENTRY_SEQ_NUM THEN
      857    13782    7             DO;
      858    13783                    %PICK_ESC_LIST ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:32   
      859    13787    7             END ;
      860    13788
      861    13789    6         END /* If Then Do */ ;
      862    13790
      863    13791    6         ELSE DO ; /* CNTE = 1 or 0 --- Delete EVALP */
      864    13792
      865    13793    6           CALL DELETE_EVALP ;
      866    13794
      867    13795    6         END /* Else Do */ ;
      868    13796
      869    13797    5         O3COM.EVALP$ = ILL$ ; /* Update Loop Index */
      870    13798
      871    13799    5       END /* Do While ID = EVALP */ ;
      872    13800
      873    13801            /* Insert INITV's after CCOSU's */
      874    13802
      875    13803    5       IF FIRST_INITV$ ~= ADDR(NIL) THEN DO ;
