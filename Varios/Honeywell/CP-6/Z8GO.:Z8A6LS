      317    11632        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:14   
      318    11633    4     CASE (%DO1, %DO2, %DO3, %DO4, %DO5 );
      319    11634
      320    11635    4       IF  O3COM.VARYING$ ~= ADDR(NIL)
      321    11636    4           AND   ILL_ETT$ -> ILL.ID = %DO5    THEN
      322    11637    5         DO;
      323    11638
      324    11639              /*V* O3COM.VARYING$ points to a list of GSA
      325    11640                   items as candidates for INITV and INCRV
      326    11641                   creation.  Find the four with the highest
      327    11642                    counts.
      328    11643              */
      329    11644
      330    11645    5           IF O3COM.GSA_COUNT > %#INDEX_REGS    THEN
      331    11646    5             CALL SORT_GSAS ;          /* Sets GSA_COUNT = #XR's */
      332    11647    5           GST$ = O3COM.GSA$ ;
      333    11648
      334    11649    6           DO WHILE (O3COM.GSA_COUNT > 0) ;
      335    11650
      336    11651    6             CALL Z8GX$INIT_SUBS (GST$ -> GST.ROP_OF_SSOP, INIT$) ;
      337    11652    6             CALL Z8GX$SUBS_INCR (GST$ -> GST.ROP_OF_SSOP, INCR$) ;
      338    11653
      339    11654                  /* Shift INCRV expression to upper for ADLXn */
      340    11655
      341    11656    6             IF INCR$ -> ETT.ID ~= %TRIAD    THEN
      342    11657    6               CALL Z8AF$FORME (%MPI2, INCR$, 18, INCR$) ;
      343    11658
      344    11659                  %RESERVE_ILL (ITEM = %INITV, POINTER = ILL$) ;
      345    11669    6             ILL$ -> ILL.LOP = INIT$ ;
      346    11670    6             ILL$ -> ILL.ROP = GST$ -> GST.ROP_OF_SSOP ;
      347    11671
      348    11672                  /* Link the INITV at the LINOI previous to the DO5 */
      349    11673
      350    11674    6             OPN$ = ILS$ -> ILS.NEXT ;
      351    11675    6             ILL$ -> ILL.ILL$ = OPN$ -> ILS.ILL$ ;
      352    11676    6             OPN$ -> ILS.ILL$ = ILL$ ;
      353    11677
      354    11678                  %RESERVE_ILL (ITEM = %INCRV, POINTER = INCRV$) ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:15   
      355    11688    6             INCRV$ -> ILL.LOP = INCR$ ;
      356    11689    6             INCRV$ -> ILL.ROP = GST$ -> GST.ROP_OF_SSOP ;
      357    11690
      358    11691                  /* Link the INCRV at a LINOI before the DOEND */
      359    11692
      360    11693    6             OPN$ = O3COM.INCRV_ILS$ ;
      361    11694    6             INCRV$ -> ILL.ILL$ = OPN$ -> ILS.ILL$ ;
