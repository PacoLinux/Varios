      366    11699                  %INCR_COUNT (OP = LOP, BIT = ARITH, ITEM$ = INCRV$,
      367    11700                               COMPUTE_SEQ_ALREADY_COMPUTED = '0'B) ;
      368    11765
      369    11766    6             GST$ = GST$ -> GST.NEXT ;
      370    11767    6             O3COM.GSA_COUNT = O3COM.GSA_COUNT - 1 ;
      371    11768
      372    11769    6           END /* Do While GSA_COUNT > 0 */ ;
