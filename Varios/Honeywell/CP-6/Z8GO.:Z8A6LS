      889    13817                    %PUT_ESC_LIST ;
      890    13821    6             END;
      891    13822    5         END ;
      892    13823
      893    13824
      894    13825    4       ILL_ETT$ = O3COM.EVALP$ ; /* ILL item after last EVALP */
      895    13826    4       O3COM.EVALP$ = ADDR(NIL) ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:33   
      896    13827    4       GOTO UPDATE_COUNT ;   /* Optimize original ILL item */
      897    13828
      898    13829    4   END /* CASE (P3VEC) */ ;
      899    13830        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:34   
      900    13831            /* Update op. seg. count for ILL library calls */
