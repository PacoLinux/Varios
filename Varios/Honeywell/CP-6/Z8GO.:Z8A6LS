      831    13756    5             OR O3COM.CURRENT_SEQ_NO = COMCOM.H_ENTRY$ -> ILS.SEQNO + 1 )
      832    13757    5          AND ( O3COM.EVALP$ -> CCOSU.CNTE = 1
      833    13758    5            AND ETT_ID ~= %OFF ) )
      834    13759    6         THEN DO ;
      835    13760
      836    13761    6           O3COM.EVALP$ -> ILL.ID = %CCOSU ;
      837    13762    6           IF  O3COM.CURRENT_SEQ_NO = COMCOM.H_ENTRY$ -> ILS.SEQNO + 1
      838    13763    6           AND ETT_ID ~= %ESC
