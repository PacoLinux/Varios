     1118    14061    2          CALL X8CO$CONV_TO_OCTAL (TRIAD$, SUBSTR (OBUF, 39, 12)) ;
     1119    14062    2          CALL Z9AL$DUMP_LINE ;
     1120    14063
     1121    14064    2   END OPTIMIZER_ALERT ;
     1122    14065
     1123    14066        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:43   
     1124    14067        /*I*   NAME: INCR_ROP_OF_SSOP_OR_SIZ
     1125    14068
     1126    14069               CALL: CALL INCR_ROP_OF_SSOP_OR_SIZ (SSOP$ or SIZ$, id) ;
     1127    14070
     1128    14071              INPUT: OP$ -- pointer to SSOP or SIZ triad.
     1129    14072                     PARENT_ID -- ID of the guy requesting increment.
     1130    14073
     1131    14074             OUTPUT: OP$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE
     1132    14075                     is incremented by an amount appropriate to
     1133    14076                     the way Phase 4 references instruction tags.
     1134    14077
     1135    14078        DESCRIPTION:
     1136    14079                      Increment the count of the ROP if it's
     1137    14080                      a SIZ or a non-INITV-INCRV SSOP.
     1138    14081        */
     1139    14082
     1140    14083    1   INCR_ROP_OF_SSOP_OR_SIZ: PROC (OP$, PARENT_ID) ;
     1141    14084
     1142    14085    2   DCL  OP$        PTR ;
     1143    14086    2   DCL  PARENT_ID  UBIN WORD ;
     1144    14087    2   DCL  SSOP$      PTR ;
     1145    14088    2   DCL  SIZ$       PTR ;
     1146    14089    2   DCL  BASE_ST$$  PTR ;
     1147    14090
     1148    14091    2        SSOP$ = ADDR(NIL) ;
     1149    14092    2        SIZ$  = ADDR(NIL) ;
     1150    14093
     1151    14094    3        DO CASE (OP$ -> ETT.ID) ;
     1152    14095
     1153    14096    3          CASE (%SIZ, %REFMOD) ;  /*#28637*/
     1154    14097
     1155    14098    3            INCREMENT_ROP_COUNT = TRUE ;
     1156    14099    3            SIZ$ = OP$ -> ETT.ROP ;
     1157    14100    3            IF OP$ -> ETT.LOP -> ETT.ID = %SSOP   THEN
     1158    14101    3              SSOP$ = OP$ -> ETT.LOP -> ETT.ROP ;
     1159    14102
     1160    14103    3          CASE (%SSOP) ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:44   
     1161    14104
     1162    14105    3            GST$ = O3COM.GSA$ ;
     1163    14106    3            INCREMENT_ROP_COUNT = TRUE;
     1164    14107
     1165    14108    4            DO WHILE (GST$ ~= ADDR(NIL)) ;
     1166    14109
     1167    14110    4              IF GST$ -> GST.ROP_OF_SSOP = OP$ -> ETT.ROP   THEN
     1168    14111    5                DO;
     1169    14112    5                  INCREMENT_ROP_COUNT = FALSE ;
     1170    14113    5                  GST$ = ADDR(NIL);
     1171    14114    5                END;
     1172    14115    4              ELSE
     1173    14116    4                GST$ = GST$ -> GST.NEXT ;
     1174    14117
     1175    14118    4            END;       /* Do While */
     1176    14119
     1177    14120    3            IF INCREMENT_ROP_COUNT = TRUE   THEN
     1178    14121    4              DO;
     1179    14122    4                SSOP$ = OP$ -> ETT.ROP ;
     1180    14123    4                IF OP$ -> ETT.LOP -> ETT.ID = %SIZ          /*#28637*/
     1181    14124    4                     OR OP$ -> ETT.LOP -> ETT.ID = %REFMOD    THEN
     1182    14125    4                  SIZ$ = OP$ -> ETT.LOP -> ETT.ROP ;
     1183    14126    4              END;
     1184    14127
     1185    14128    3          CASE (ELSE) ;
     1186    14129
     1187    14130    3            INCREMENT_ROP_COUNT = FALSE ;
     1188    14131
     1189    14132    3        END /* Case of ID */ ;
     1190    14133
     1191    14134
     1192    14135    3        IF INCREMENT_ROP_COUNT THEN DO ;
     1193    14136
     1194    14137               %FISH (TRIAD = OP$,
     1195    14138                      RTN   = BASE_ST$$);
     1196    14146
     1197    14147    3          IF BASE_ST$$ -> ST.ID = %PARA THEN
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:45   
     1198    14148    4            DO;
     1199    14149                   %INCR_COUNT (ITEM$ = OP$, OP = ROP, BIT = ARITH);
     1200    14221    4            END;
     1201    14222    3          ELSE
     1202    14223    4            DO;
     1203    14224                   %INCR_COUNT (ITEM$ = OP$, OP = ROP, BIT = SUBS) ;
     1204    14296    4            END;
     1205    14297
     1206    14298    4          IF PARENT_ID = %EVAL THEN DO ;
     1207    14299    4            OP$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE =
     1208    14300    4             OP$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE - 1 ;
     1209    14301    4            IF SSOP$ ~= ADDR(NIL)    THEN
     1210    14302    5              DO;
     1211    14303                        /* Create an EVALP for the expr if one */
     1212    14304                        /* doesn't already exist.              */
     1213    14305
     1214    14306    5                IF SSOP$ -> ETT.EVALP$ = ADDR(NIL)
     1215    14307    5                    OR SSOP$ -> ETT.EVALP$ -> ILS.ID ~= %EVALP
     1216    14308    5                    OR SSOP$ -> ETT.EVALP$ -> CCOSU.EVEX ~= SSOP$  THEN
     1217    14309    6                  DO;
     1218    14310    6                    CALL Z8GI$INSERT_EVALP (SSOP$) ;
     1219    14311    6                    SSOP$ -> ETT.EVALP$ -> CCOSU.CNTE = 0 ;
     1220    14312    6                  END ;
     1221    14313    5                SSOP$ -> ETT.EVALP$ -> CCOSU.CNTE = SSOP$ -> ETT.EVALP$
     1222    14314    5                        -> CCOSU.CNTE + O3COM.EVALP$ -> CCOSU.CNTE -1;
     1223    14315
     1224    14316    5              INCREMENT_ROP_COUNT = FALSE ;
     1225    14317    5            END ;
     1226    14318    5            IF SIZ$ ~= ADDR(NIL) THEN DO ;
     1227    14319
     1228    14320                   /* Create an EVALP for the expr if one */
     1229    14321                   /* doesn't already exist.              */
     1230    14322
     1231    14323    5              IF SIZ$ -> ETT.EVALP$ = ADDR(NIL)
     1232    14324    5              OR SIZ$ -> ETT.EVALP$ -> ILS.ID ~= %EVALP
     1233    14325    5              OR SIZ$ -> ETT.EVALP$ -> CCOSU.EVEX ~= SIZ$
     1234    14326    6              THEN DO ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:46   
     1235    14327    6                CALL Z8GI$INSERT_EVALP (SIZ$) ;
     1236    14328    6                SIZ$ -> ETT.EVALP$ -> CCOSU.CNTE = 0 ;
     1237    14329    6              END ;
     1238    14330    5              SIZ$ ->ETT.EVALP$ -> CCOSU.CNTE =
     1239    14331    5               SIZ$ ->ETT.EVALP$ -> CCOSU.CNTE +
     1240    14332    5               O3COM.EVALP$ -> CCOSU.CNTE - 1 ;
     1241    14333    5              INCREMENT_ROP_COUNT = FALSE ;
     1242    14334    5            END ; /* SIZ$ <> Nil */
     1243    14335    4          END ; /* Parent ID = %EVAL */
     1244    14336    3        END ; /* Increment ROP Count */
     1245    14337
     1246    14338    3        IF O3COM.EVALP$ -> CCOSU.CNTE > 1 THEN DO ;
     1247    14339
     1248    14340    4          IF INCREMENT_ROP_COUNT THEN DO ;
     1249    14341
     1250    14342    4            OP$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE =
     1251    14343    4              OP$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE +
     1252    14344    4              O3COM.EVALP$ -> CCOSU.CNTE - 1 ;
     1253    14345
     1254    14346    4          END /* IF ID = SSOP or SIZ */ ;
     1255    14347
     1256    14348               /* Increment the count of the ESC triad */
     1257    14349    3          ETT$ = OP$ ;
     1258    14350    4          DO WHILE (ETT$ -> ETT.ID ~= %BASE
     1259    14351    4                AND ETT$ -> ETT.ID ~= %TRIAD) ;
     1260    14352    4            ETT$ = ETT$ -> ETT.LOP ;
     1261    14353    4          END ;
     1262    14354
     1263    14355    4          IF ETT$ -> ETT.ID = %BASE THEN DO ;
     1264    14356
     1265    14357                 /* Create an EVALP for the ESC if one */
     1266    14358                 /* doesn't already exist.             */
     1267    14359
     1268    14360    4            IF ETT$ -> ETT.ROP -> ETT.EVALP$ = ADDR(NIL)
     1269    14361    4            OR ETT$ -> ETT.ROP -> ETT.EVALP$ -> ILS.ID ~= %EVALP
     1270    14362    5            THEN DO ;
     1271    14363    5              CALL Z8GI$INSERT_EVALP (ETT$ -> ETT.ROP) ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:47   
     1272    14364    5              ETT$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE = 0 ;
     1273    14365    5            END ;
     1274    14366
     1275    14367    4            ETT$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE =
     1276    14368    4              ETT$ -> ETT.ROP -> ETT.EVALP$ -> CCOSU.CNTE +
     1277    14369    4              O3COM.EVALP$ -> CCOSU.CNTE - 1 ;
     1278    14370
     1279    14371    4          END ; /* ID = %BASE */
     1280    14372
     1281    14373    3        END /* IF count > 1 */ ;
     1282    14374
     1283    14375    2   END INCR_ROP_OF_SSOP_OR_SIZ ;
     1284    14376
     1285    14377    1   END Z8GO$GLOBAL_OPTIMIZER ;
