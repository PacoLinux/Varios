      377    11774
      378    11775    5         END /* If Then Do */ ;
      379    11776
      380    11777              /* DO# = Seq# of next DOn (DO Begin) item */
      381    11778
      382    11779    4         O3COM.DO_ILS$ = ILL_ETT$ -> DO_N.ITO ;
      383    11780    4         IF O3COM.DO_ILS$ = ADDR(NIL)    THEN
      384    11781    4           O3COM.DO# = 0 ;        /*   No Outer DO   */
      385    11782    4         ELSE
      386    11783    5           DO;                    /*  Update DO# and DOEND#  */
      387    11784    5             O3COM.DO# = O3COM.DO_ILS$ -> ILS.SEQNO ;
      388    11785    5             O3COM.DOEND# = O3COM.DO_ILS$ -> ILS.ILL$ -> DO_N.ITE -> ILS.SEQNO;
      389    11786    5           END ;
      390    11787    4         O3COM.LOOP_LEVEL = O3COM.LOOP_LEVEL - 1 ;
      391    11788        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:16   
      392    11789          /*V* VAL_and_VAL_END -- these items are created by the
      393    11790               front-end for multiple receiving moves, adds, etc.
      394    11791               The reason for the special treatment is due to
      395    11792               a desire to obey the rules of Cobol, yet maintain a
      396    11793               reasonable level of efficiency.  As an example,
      397    11794               consider the following:
      398    11795
      399    11796          MOVE A (I) TO B (I), I, A (I).
      400    11797
      401    11798               Cobol says that this is equivalent to:
      402    11799
      403    11800          MOVE A (I) TO temp.
      404    11801          MOVE temp TO B (I).
      405    11802          MOVE temp TO I.
      406    11803          MOVE temp TO A (I).
      407    11804
      408    11805               99% of the time, a multiple receiving move can be done
      409    11806               without the use of a temp; but we still have handle the
      410    11807               1% of the time when the sending field is redefined by
      411    11808               one of the receiving fields, or the sending field's
      412    11809               subscripts or size-expressions are redefined by one
      413    11810               of the receiving fields.
      414    11811
      415    11812               When Phase 3 finds a VAL_END on the ILS/ILL, it will
      416    11813               check the redefinitions of VAL.ETT$.  If there exists
      417    11814               a redefinition between the VAL and the VAL_END, then
      418    11815               Phase 3 will put out a special CCOSU (B.VAL_FREEZE = '1'B).
      419    11816          */
      420    11817
      421    11818    4     CASE (%VAL_END) ;          /*    Multiple receiving fields     */
      422    11819
      423    11820    4       VAL_ILS$ = ILL_ETT$ -> VAL_END.VAL$ ;
      424    11821    4       VAL_END_SEQ# = O3COM.CURRENT_SEQ_NO ;
      425    11822    4       VAL_ILL$ = VAL_ILS$ -> ILS.ILL$ ;
      426    11823                                                         /*#28761*/
      427    11824    5       DO WHILE (VAL_ILL$ -> ILL.ID = %EVALP) ;
      428    11825    5         VAL_ILL$ = VAL_ILL$ -> CCOSU.ILL$ ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:17   
      429    11826    5       END ;
      430    11827
      431    11828    4     VAL_ETT$ = VAL_ILL$ -> VAL.ETT$ ;
      432    11829
      433    11830          /* VAL_SEQ# = Seq# of innermost VAL item. */
      434    11831
      435    11832    4     VAL_END_ILL$ = ILL_ETT$ ;
      436    11833    4     VAL_END_ILS$ = ILS$ ;
      437    11834
      438    11835    5     DO WHILE (VAL_END_ILL$ -> ILL.ID = %VAL_END) ;
      439    11836    5       VAL_ILS$ = VAL_END_ILL$ -> VAL_END.VAL$ ;
      440    11837    5       VAL_END_ILS$ = VAL_END_ILS$ -> ILS.NEXT ;
      441    11838    5       VAL_END_ILL$ = VAL_END_ILS$ -> ILS.ILL$ ;
      442    11839    5     END ; /* Do While */
      443    11840
      444    11841    4     VAL_SEQ# = VAL_ILS$ -> ILS.SEQNO ;
      445    11842
      446    11843          /* Evaluate next redefinition if not already done. */
      447    11844
      448    11845    4     IF VAL_ETT$ -> ETT.EVALP$ = ADDR(NIL)
      449    11846    4           OR VAL_ETT$ -> ETT.EVALP$ -> ILL.ID ~= %EVALP
      450    11847    4           OR VAL_ETT$ -> ETT.CMP_SC >= O3COM.CURRENT_SEQ_NO     THEN
      451    11848    4       CALL Z8GS$SCAN_MAX (VAL_ETT$) ;
      452    11849
      453    11850    4     IF VAL_ETT$ -> ETT.CMP_SC < VAL_SEQ#      THEN
      454    11851    5       DO;
      455    11852                 /* No redefinitions between VAL and VAL_END. */
      456    11853                 /* Link out VAL_END item from ILS.           */
      457    11854
      458    11855    5         VAL_SEQ# = %MAX_SEQ# ;
      459    11856    5         ILS$ = ILS$ -> ILS.NEXT ;
      460    11857    5         GOTO DO_WHILE_ILS$_NON_NIL ;
      461    11858
      462    11859    5       END ; /* If Then Do */
      463    11860    4     ELSE
      464    11861
      465    11862    5       DO;   /*   There is a redefinition between VAL & VAL_END   */
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:18   
      466    11863
      467    11864    5         VAL_ETT$ -> ETT.CMP_SC = VAL_SEQ# ;
      468    11865    5         CALL Z8GI$INSERT_EVALP (VAL_ETT$) ;
      469    11866    5         VAL_ETT$ -> ETT.EVALP$ -> CCOSU.B.VAL_FREEZE = '1'B ;
      470    11867    5         VAL_ETT$ -> ETT.EVALP$ -> CCOSU.CNTE = 0 ;
      471    11868    5         ILS$ = ILS$ -> ILS.NEXT ;
      472    11869    5         GOTO DO_WHILE_ILS$_NON_NIL ;
      473    11870
      474    11871    5       END ; /* Else Do */
      475    11872        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:19   
      476    11873    4     CASE (%VAL) ;              /* De-link VAL from ILL/ILS         */
      477    11874
      478    11875    4       VAL_SEQ# = %MAX_SEQ# ;
      479    11876    4       ILS$ = ILS$ -> ILS.NEXT ;
