      257    11572        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:12   
      258    11573    4     CASE (%DOEND) ;            /*    DO# = Seq# of DO Begin item   */
      259    11574
      260    11575    4       O3COM.DO_ILS$ = ILL_ETT$ -> DO_N.ITB ;
