      721    13177    4       PREV$ = ADDR(NIL) ;
      722    13178
      723    13179    5       DO WHILE (O3COM.EVALP$ -> ILL.ID = %EVALP) ;
      724    13180
      725    13181              /*V* Update the counts of LOP & ROP */
      726    13182
      727    13183    5         ILL_ETT$ = O3COM.EVALP$ -> CCOSU.EVEX ;
      728    13184
      729    13185    6         DO CASE (P3VEC(ILL_ETT$ -> ETT.ID)) ;
      730    13186
