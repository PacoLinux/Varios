        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7
        8        8
        9        9        /*P*   NAME: Z8GO$GLOBAL_OPTIMIZER
       10       10
       11       11            PURPOSE: Link in CCOSU, INITV, and INCRV ILL items
       12       12                     into the ILS/ILL tables to improve the
       13       13                     overall efficiency of the generated code.
       14       14             AUTHOR: John Heath.
       15       15        */
       16       16
       17       17        /*D*   NAME: Z8GO$GLOBAL_OPTIMIZER
       18       18
       19       19               CALL: CALL Z8GO$GLOBAL_OPTIMIZER (called from EXEC).
       20       20
       21       21          INTERFACE: Z8GI -- Insert an EVALP.
       22       22                     Z8GS -- Scan triad tree, set CMP_SC.
       23       23
       24       24              INPUT: COMCOM.FIRST_ILS -- Head link of ILS table.
       25       25
       26       26             OUTPUT: COMCOM.FIRST_ILS -- Head link of ILS table for Phase 4.
       27       27
       28       28        DESCRIPTION: Link through the entire program (via the ILS table),
       29       29                     and provide counts of the number of references for
       30       30                     every expression or reference triad via CCOSU items.
       31       31                     Each CCOSU is good for a redefinition range of the
       32       32                     triad.  See Z8GD for more details.
       33       33        */
       34       34
       35       35        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:53   
       36       36        Z8GO$GLOBAL_OPTIMIZER: PROC ;

     36 25 000000   000000 700200 xent  Z8GO$GLOBAL* TSX0  ! X66_AUTO_0
        25 000001   000066 000000                    ZERO    54,0

       37       37
       38       38        %MACRO INCR_COUNT (OP = LOP, BIT = ARITH, ITEM$ = ILL_ETT$,
       39       39                           COMPUTE_SEQ_ALREADY_COMPUTED = '1'B) ;
       40       40
       41       41               DO;
       42       42
       43       43                 /* Check for VALUE replacement optimization */
       44       44
       45       45                 %IF TEXTCHAR(OP) = 'LOP' ; /* Don't optimize DNA's LOP */
       46       46                   IF  ITEM$ -> ETT.ID ~= %DNA
       47       47                   AND ITEM$ -> ETT.ID ~= %EVAL
       48       48                   AND ITEM$ -> ETT.ID ~= %OFF
       49       49                   AND ITEM$ -> ETT.ID ~= %SHIFT_L
       50       50                   AND ITEM$ -> ETT.ID ~= %SHIFT_R
       51       51                   AND ITEM$ -> ETT.ID ~= %CMPAO
       52       52                   AND ITEM$ -> ETT.ID ~= %CMPAS
       53       53                   AND ITEM$ -> ETT.ID ~= %ADDR
       54       54                   AND COMCOM.SEGMENTC < 2 THEN
       55       55                     CALL Z8GO$CHECK_OP (ITEM$ -> ILL.LOP) ;
       56       56                 %ELSEIF TEXTCHAR(OP) ~= 'LOP->ARGLIST.ARG$(I)'
       57       57                     AND TEXTCHAR(OP) ~= 'ROP->ARGLIST.ARG$(I)';
       58       58                   IF  ITEM$ -> ETT.ID ~= %CMPAO /* Don't optz CMPAO's ROP */
       59       59                   AND ITEM$ -> ETT.ID ~= %MVANO
       60       60                   AND COMCOM.SEGMENTC < 2 THEN
       61       61                     CALL Z8GO$CHECK_OP (ITEM$ -> ILL.OP) ;
       62       62                 %ENDIF ;
       63       63
       64       64                 /* MVII of CVDI => MVDI optimization moved from Z8E7.     */
       65       65                 /* Likewise, MVDD of CVID4/9 ==> MVID optimization.       */
       66       66                 /* These optimizations have to be done after the VALUE optz. */
       67       67                 %IF TEXTCHAR (OP) = 'ROP' ;
       68       68                   IF  ITEM$ -> ILL.ID = %MVII
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:54   
       69       69                   AND ITEM$ -> ILL.ROP -> ETT.ID = %CVDI THEN DO;
       70       70                     ITEM$ -> ILL.ID  = %MVDI ;
       71       71                     ITEM$ -> ILL.ROP = ITEM$ -> ILL.ROP -> ETT.LOP ;
       72       72                   END ; /* ILL = MVII and ROP = CVDI */
       73       73                   IF  ITEM$ -> ILL.ID = %MVDD
       74       74                   AND (ITEM$ -> ILL.ROP -> ETT.ID = %CVID4 OR
       75       75                        ITEM$ -> ILL.ROP -> ETT.ID = %CVID9) THEN DO;
       76       76                     LOP$$$ = ITEM$ -> ILL.LOP ;
       77       77                     %FISH (TRIAD = LOP$$$, RTN = ST$$$) ;
       78       78                     IF ST$$$ -> VARIABLE.NDSC.SF = 0 THEN DO ;
       79       79                       ITEM$ -> ILL.ID  = %MVID;
       80       80                       ITEM$ -> ILL.ROP = ITEM$ -> ILL.ROP -> ILL.LOP;
       81       81                     END; /* Scale factor = zero */
       82       82                   END;
       83       83                 %ENDIF ; /* OP is ROP */
       84       84
       85       85                 IF ITEM$ -> ILL.OP->ETT.EVALP$ = ADDR (NIL)
       86       86                 OR ITEM$ -> ILL.OP->ETT.EVALP$ -> ILL.ID ~= %EVALP
       87       87                 OR ITEM$ -> ILL.OP->ETT.EVALP$ -> CCOSU.EVEX ~= ITEM$ -> ILL.OP
       88       88                 OR ITEM$ -> ILL.OP->ETT.CMP_SC >= O3COM.CURRENT_SEQ_NO THEN DO ;
       89       89
       90       90                   %IF COMPUTE_SEQ_ALREADY_COMPUTED = '0'B ;
       91       91                      CALL Z8GS$SCAN_MAX (ITEM$ -> ILL.OP) ;
       92       92                   %ENDIF ;
       93       93
       94       94                   CALL Z8GI$INSERT_EVALP (ITEM$ -> ILL.OP) ;
       95       95
       96       96                   IF ITEM$ -> ILL.OP->ETT.EVALP$ = ADDR(NIL)
       97       97                   OR ITEM$ -> ILL.OP->ETT.EVALP$ -> ILL.ID ~= %EVALP THEN DO ;
       98       98                     IF COMCOM.OB.O3DUMP
       99       99                     THEN CALL OPTIMIZER_ALERT (ITEM$ -> ILL.OP) ;
      100      100                   END ;
      101      101                 END ; /* EVALP is non-existent or obsolete */
      102      102                 ELSE DO ;
      103      103                   ILL$ = ITEM$ -> ILL.OP -> ETT.EVALP$ ;
      104      104                   ILL$ -> CCOSU.CNTE = ILL$ -> CCOSU.CNTE + 1 ;
      105      105                   ILL$ -> CCOSU.LSEQ = O3COM.CURRENT_SEQ_NO ;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:55   
      106      106                   ILL$ -> CCOSU.LEVEL = MAXIMUM (ILL$ -> CCOSU.LEVEL,
      107      107                                                  O3COM.LOOP_LEVEL) ;
      108      108                 END ;
      109      109
      110      110                 ITEM$ -> ILL.OP->ETT.EVALP$ -> CCOSU.B.BIT = '1'B ;
      111      111               END ;
      112      112
      113      113        %MEND ;
      114      114
      115      115        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:56   
      116      116        %EQU PHASE = 3 ;
      117      117        %EQU Z8GO = 8715 ; /* 8 || G=7 || O=15 */
      118      118        %EQU MODULE = Z8GO ;
      119      119        %EQU #INDEX_REGS = 4 ;
      120      120        %EQU DO5_DEFLB_DIFF = 4 ; /* SEQ# Difference between DO5 and DEFLB */
      121      121        %EQU DOEND_AD2I_DIFF = 5 ; /* SEQ# Difference twix DOEND and AD2I */
      122      122        %EQU DOEND_LINOI_DIFF= 4 ; /* SEQ# Difference twix DOEND and INCRV */
      123      123        %INCLUDE Z8A_DEFC ;
      124      191        %INCLUDE Z8A_SUPP_MAC ;
      125     2591        %INCLUDE Z8A_COMCOM ;
      126     3241        %INCLUDE Z8A_ERR_NUMS ;
      127     4005        %INCLUDE Z8A_ILS ;
