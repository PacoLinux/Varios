      782    13572    6             IF ILL_ETT$ -> ETT.ID = %BASE    THEN
      783    13573    7               DO;
      784    13574
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:30   
      785    13575    7                 IF ILL_ETT$ -> ILL.ROP -> ETT.ID <= %LAST_ETT   THEN
      786    13576    8                   DO;
      787    13577                          %INCR_COUNT (OP = ROP, BIT = ARITH) ;
      788    13649    8                   END;
      789    13650    7               END ;
      790    13651
      791    13652    6             ELSE      /* ID = SSOP, SIZ, OFF, or FISS */
      792    13653
      793    13654    7               DO;
      794    13655    7                 IF ILL_ETT$ -> ETT.LOP -> ETT.ID ~= %PARA   THEN
      795    13656    8                   DO;
      796    13657                          %INCR_COUNT (OP = LOP, BIT = ARITH) ;
      797    13722    8                   END ;
      798    13723    7               END ; /* ID = SSOP, SIZE, OFF, or FISS */
      799    13724
      800    13725                  /* Increment the count of the ROP if it's
      801    13726                     a SIZ or a non-INITV-INCRV SSOP.
      802    13727                  */
