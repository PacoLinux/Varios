     1639     6807    2   IF I_CONSTANT < 100 AND I_CONSTANT > -100 THEN LENGTH = 3;
     1640     6808    2   ELSE
     1641     6809    2   IF I_CONSTANT < 1000 AND I_CONSTANT > -1000 THEN LENGTH = 4;
