      733    13189
      734    13190    6           CASE (%ONE_EXPRESSION_NO_REDEF) ;
      735    13191
      736    13192    7             DO CASE (ILL_ETT$ -> ETT.ID) ;
      737    13193
      738    13194    7               CASE (%EVAL) ;
      739    13195
      740    13196                      /* Increment the count of the ROP of the */
      741    13197                      /* LOP if the LOP is a SSOP or a SIZ.    */
