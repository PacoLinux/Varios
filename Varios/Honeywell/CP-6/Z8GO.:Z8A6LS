     1674     6842
     1675     6843    2   QLT$$ = ST$$ -> VARIABLE.QLT$;
     1676     6844    2   IF QLT$$ = ADDR(NIL) THEN
     1677     6845    2     ALTRETURN;
     1678     6846    2   ST2$ = ST$$;
     1679     6847    3   DO FOREVER;
     1680     6848
     1681     6849    3     IF ST2$ -> SCALAR.B.REDEF_SUB
     1682     6850    3     OR ST2$ -> SCALAR.B.REDEF_OBJ
     1683     6851    3     OR ST2$ -> SCALAR.B.RENAMED
     1684     6852    3     OR QLT$$ -> QLT.B.RENAMES
     1685     6853    3     OR ST2$ -> SCALAR.B.EXTERNAL
     1686     6854    3     OR ST2$ -> SCALAR.SEQ#.FD ~= 0
     1687     6855    3     OR ST2$ -> SCALAR.SEQ#.BD ~= %MAX_SEQ# THEN
     1688     6856    3       ALTRETURN;
     1689     6857
