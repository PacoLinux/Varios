      777    13567          /* Changed to '+ 1'        + O3COM.EVALP$ -> CCOSU.CNTE ;*/
      778    13568    6                                   + 1;
      779    13569
      780    13570    6           CASE (%SSOP_SIZ_BASE) ; /* & FISS & OFF */
