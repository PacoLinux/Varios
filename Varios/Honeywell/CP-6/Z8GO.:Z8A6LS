      192    11498    1      COMCOM.FIRST_ENTRY_SEQ_NUM = COMCOM.FIRST_ENTRY_SEQ_NUM + 1 ;
      193    11499    1      ILS$ = COMCOM.FIRST_ILS ; /* The last ILS item */
      194    11500    1      O3COM.CURRENT_SEQ_NO = ILS$ -> ILS.SEQNO ;
