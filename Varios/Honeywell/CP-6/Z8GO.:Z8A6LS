      804    13729    6             CALL INCR_ROP_OF_SSOP_OR_SIZ (ILL_ETT$,
      805    13730    6                                           ILL_ETT$ -> ETT.ID) ;
      806    13731
      807    13732    6         END /* Case of EVALP -> triad ID */ ;
      808    13733
      809    13734              /*V* Change the EVALP ID to CCOSU if one of the
      810    13735                   following conditions is met:
      811    13736                   (1) the count is > 1 (more than one ref),
      812    13737                   (2) the expression can be removed from a loop,
      813    13738                or (3) the expression is "good forever".
      814    13739              */
      815    13740
      816    13741    5         ETT_ID = O3COM.EVALP$ -> CCOSU.EVEX -> ETT.ID ;
      817    13742    5         ETT$$ = O3COM.EVALP$ -> CCOSU.EVEX ;
      818    13743
      819    13744    5         IF  ETT_ID = %ESC
      820    13745    5             AND ETT$$ -> ETT.LOP -> ST.ID = %BLOCK
      821    13746    5             AND ETT$$ -> ETT.LOP -> ST.COUNT = 1
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:31   
      822    13747    5           OR ETT_ID = %TRIAD
      823    13748    5           OR ETT_ID = %CMPAS THEN
