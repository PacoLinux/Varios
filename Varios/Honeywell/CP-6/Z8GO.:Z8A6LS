      233    11548    1       DUMMY_DPI$ -> DPI.NEXT = ADDR(NIL);
      234    11549        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:11   
      235    11550               /* Link thru the entire ILS back to front */
      236    11551
      237    11552    2      DO WHILE (ILS$ ~= ADDR(NIL)) ;
      238    11553
      239    11554    2        IF ILS$ -> ILS.SEQNO ~= 0
      240    11555    2        OR O3COM.CURRENT_SEQ_NO = 1 THEN
      241    11556    2          O3COM.CURRENT_SEQ_NO = ILS$ -> ILS.SEQNO ;
