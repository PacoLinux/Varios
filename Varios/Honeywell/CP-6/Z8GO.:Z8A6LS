      902    13833    4          DO CASE (ILL_ETT$ -> ILL.ID) ;
      903    13834
      904    13835    4            CASE (%STOP, %STOPL, %ACCEP, %DISPL, %OS_CALL) ;
      905    13836
      906    13837    4              IF O3COM.OP$ ~= ADDR(NIL)   THEN
      907    13838    4                O3COM.OP$ -> CCOSU.CNTE = O3COM.OP$ -> CCOSU.CNTE + 1 ;
      908    13839
      909    13840    4          END /* Case of ILL ID */ ;
      910    13841
      911    13842    3        END /* IF ILL_ETT$ ~= ADDR(NIL) */ ;
      912    13843
      913    13844    2        NEXT_ILS$ = ILS$ -> ILS.NEXT ;
      914    13845    2        ILS$ -> ILS.NEXT = PREV_ILS$ ;
