     1691     6859    3     IF QLT$$ = ADDR(NIL) THEN EXIT;
     1692     6860    3     ST2$ = QLT$$ -> QLT.ST$;
     1693     6861
PL6.E3A0      #002=Z8GO$CHECK_OP File=Z8GO.:Z8A6TSI                              TUE 08/26/97 07:55 Page:270  
     1694     6862    3   END; /* DO FOREVER (WHILE QLT$$ not NIL) */
     1695     6863
     1696     6864    2   END CHECK_FOR_REDEFS;
     1697     6865
     1698     6866        %EJECT;
