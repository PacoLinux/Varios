      996    13939        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:38   
      997    13940        /*D*       NAME: PUT_ESC_LIST
      998    13941
      999    13942                   CALL: CALL PUT_ESC_LIST ;
     1000    13943
     1001    13944            DESCRIPTION: This routine is called when the current sequence
     1002    13945                         number is COMCOM.FIRST_ENTRY_SEQ_NUM and the ESC CCOSU's
     1003    13946                         have been processed.  TOP5.COUNT is an array
     1004    13947                         containing the counts of the most referenced
     1005    13948                         ESC triads. This routine will add these top 5
     1006    13949                         CCOSU items to COMCOM.ESC_LIST for use by Phase 4.
     1007    13950
     1008    13951        */
     1009    13952
     1010    13953    1   PUT_ESC_LIST: PROC ;
     1011    13954
     1012    13955
     1013    13956    2          I = 0;
     1014    13957
     1015    13958    2        LOOP1:
     1016    13959    3          DO WHILE (TOP5.PTR$(I) ~= ADDR(NIL));
     1017    13960
     1018    13961    3            COMCOM.ESC_LIST.ESC$(I) = TOP5.PTR$(I);
     1019    13962    3            TOP5.PTR$(I) -> ETT.EVALP$ -> CCOSU.B.IGNORE = '1'B;
     1020    13963
     1021    13964    3            IF TOP5.PTR$(I) -> ETT.ID = %ESC THEN
     1022    13965    3              COMCOM.ESC_LIST.OPTION(I) = %PR_BLOCK;
     1023    13966    3            ELSE
     1024    13967    3              COMCOM.ESC_LIST.OPTION(I) = %PR_ARGP;
     1025    13968
     1026    13969    3            COMCOM.ESC_LIST.PR#(I) = %PR7 - I;
     1027    13970
     1028    13971                 /* Currently, only PR7 is available for global assignment */
     1029    13972                 /* so, exit the do  loop here. In the future we might  be */
     1030    13973                 /* able to use the PR6 and PR5 too, then, these code will */
     1031    13974                 /* be added:                                              */
     1032    13975                 /*   IF I = 2 THEN EXIT LOOP1; ELSE I = I + 1;            */
     1033    13976
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:39   
     1034    13977    3            EXIT LOOP1;
     1035    13978
     1036    13979    3          END;
     1037    13980    2   END PUT_ESC_LIST ;
