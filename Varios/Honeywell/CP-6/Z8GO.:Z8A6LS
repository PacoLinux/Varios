PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:35   
      932    13863    1    DELETE_EVALP: PROC ;
      933    13864
      934    13865    2          ILL$ = O3COM.EVALP$ -> CCOSU.ILL$ ;
      935    13866
      936    13867    2          IF PREV$ = ADDR(NIL)     THEN
      937    13868    2            ILS$ -> ILS.ILL$ = ILL$ ;
      938    13869    2          ELSE
      939    13870    2            PREV$ -> CCOSU.ILL$ = ILL$;
      940    13871
      941    13872               /* Link Discarded EVALP items into
      942    13873                  EVALP pool.                          */
      943    13874               %INSERT_INTO_SL_LIST(PTR=O3COM.EVALP$,
      944    13875                                    ITEM=CCOSU,
      945    13876                                    HEAD=O3COM.HEAD_EVALP$,
      946    13877                                    NEXT=NEXT$ );
      947    13890    2    END DELETE_EVALP ;
      948    13891
      949    13892        %EJECT;
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:36   
      950    13893        /*D*       NAME: PICK_ESC_LIST
      951    13894
      952    13895                   CALL: CALL PICK_ESC_LIST ;
      953    13896
      954    13897            DESCRIPTION: This routine is called when the current sequence
      955    13898                         number is COMCOM.FIRST_ENTRY_SEQ_NUM and a EVALP has been
      956    13899                         turned into a CCOSU.  If the ID of the CCOSU's
      957    13900                         triad is ESC and the count is high enough, then
      958    13901                         this CCOSU's count will be added to the list of
      959    13902                         counts that represent the top 5 referenced ESCs
      960    13903                         in the program.  The routine PUT_ESC_LIST will use
      961    13904                         this information.
      962    13905
      963    13906        */
      964    13907
      965    13908    1   PICK_ESC_LIST: PROC ;
      966    13909
      967    13910    2          IF ( ( O3COM.EVALP$ -> CCOSU.EVEX->ETT.ID = %ESC
      968    13911    2             AND O3COM.EVALP$ -> CCOSU.EVEX->ETT.LOP -> ST.ID = %BLOCK
      969    13912    2             AND O3COM.EVALP$ -> CCOSU.EVEX ~= COMCOM.ESC )
      970    13913    2            OR ( O3COM.EVALP$ -> CCOSU.EVEX->ETT.ID = %ARGP
      971    13914    2             AND COMCOM.SYMDEFC = 1 ) )
      972    13915    3          AND    O3COM.EVALP$ -> CCOSU.CNTE > TOP5.COUNT (%(#PTR_REGS - 1)) THEN DO ;
      973    13916
      974    13917    4            DO I = 0 TO %(#PTR_REGS - 1) ;
      975    13918
      976    13919    5              IF O3COM.EVALP$ -> CCOSU.CNTE > TOP5.COUNT(I) THEN DO ;
      977    13920
      978    13921    6                DO J = %(#PTR_REGS - 2) DOWNTO I ;
      979    13922    6                  TOP5.COUNT (J+1) = TOP5.COUNT (J) ;
      980    13923    6                  TOP5.PTR$ (J + 1) = TOP5.PTR$ (J);
      981    13924    6                END ;
      982    13925
      983    13926    5                TOP5.COUNT(I) = O3COM.EVALP$ -> CCOSU.CNTE ;
      984    13927    5                TOP5.PTR$ (I) = O3COM.EVALP$ -> CCOSU.EVEX;
      985    13928    5                GOTO LOOP_EXIT ;
      986    13929
PL6.E3A0      #001=Z8GO$GLOBAL_OPTIMIZER File=Z8GO.:Z8A6TSI                      TUE 08/26/97 07:51 Page:37   
      987    13930    5              END /* If CNTE > COUNT (I) */ ;
      988    13931
      989    13932    4            END /* DO I */ ;
      990    13933    3   LOOP_EXIT: ;
      991    13934
      992    13935    3          END /* If Then Do */ ;
      993    13936
      994    13937    2   END PICK_ESC_LIST ;
