      630    10554
      631    10555    4                    CALL Z8EU$CREATE_IDENTIFIER_FROM_ST (INDEX_GUY$,
      632    10556    4                      L$,
      633    10557    4                      SAVE_LINE,
      634    10558    4                      SAVE_COLUMN);
      635    10559
