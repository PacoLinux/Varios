      640    10564
      641    10565                      %FISH (TRIAD = L$ -> LX_ETT_ITEM.ETT$, RTN = ST$);
      642    10573    3                 IF ST$ -> ST.ID ~= %PARA   AND
      643    10574    3                   ST$ -> ST.ID ~= %SECTION   THEN
      644    10575    3                    CALL ARG_ERROR;        /*  This is for the                   */
      645    10576                                                /*  remember call to                  */
      646    10577                                                /*  to PL6.                           */
