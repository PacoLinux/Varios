      132    14742             %INIT_LX_ETT_ITEM (     @POINTER = "ADDR(TEMP_ITEM)");
      133    14764    2        ADDR(TEMP_ITEM) -> LX_ETT_ITEM.TYPE = %TYD;
      134    14765
      135    14766    2        CALL Z8AT$CREATE_TEMPORARY (%SCALAR, TEMP_ST$);
      136    14767    2        TEMP_ST$ -> VARIABLE.TYPE = %TYD;
