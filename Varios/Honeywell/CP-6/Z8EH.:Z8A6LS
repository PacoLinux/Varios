      137    14768    2        TEMP_ST$ -> VARIABLE.NDSC.TN = %NDSC9;
      138    14769
      139    14770    3        DO CASE (ACCEPT_ARG$ -> LX_ETT_ITEM.TYPE);
      140    14771
      141    14772    3         CASE (%TYH, %TYI, %TYJ, %TYK);
      142    14773
      143    14774    3           TEMP_ST$ -> VARIABLE.NDSC.N = %CVID9_LENGTH;
