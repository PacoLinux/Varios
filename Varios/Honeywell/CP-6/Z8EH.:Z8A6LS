PL6.E3A0      #007=Z8EH$CHECK_LABEL File=Z8EH.:Z8A6TSI                           SAT 08/09/97 12:00 Page:133  
      659        1        /*T***********************************************************/
      660        2        /*T*                                                         */
      661        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      662        4        /*T*                                                         */
      663        5        /*T***********************************************************/
      664        6        /******************/
      665        7          Z8EH$CHECK_LABEL : PROC ( LABEL_TYPE,
      666        8        /******************/        LABEL_CHECK,
      667        9                                    LABEL_ERROR,
      668       10                                    LABEL$ );
      669       11
      670       12        /*I*
      671       13                 NAME: CHECK_LABEL
      672       14
      673       15          DESCRIPTION: A label (section or paragraph) is checked to ensure
      674       16                       that it has not been used as a variable.  An error
      675       17                       message is issued if the multiple use is detected.
      676       18        */
      677       19
      678       20
      679       21    1    DCL LABEL_TYPE SBIN WORD;               /* %PARA or %SECTION                  */
      680       22    1    DCL LABEL_CHECK SBIN WORD;              /* %PARA or %SECTION                  */
      681       23    1    DCL LABEL_ERROR SBIN WORD;              /* error number                       */
      682       24    1    DCL LABEL$      PTR;                    /* to LX_TERMINAL                     */
      683       25
      684       26    1    DCL LABEL_ST$ PTR;
      685       27
      686       28        /* Check a section or paragraph for illegal use as a data name.
      687       29        */
      688       30         %EQU PHASE = 1;
      689       31         %INCLUDE Z8A_DEFC;
      690       99         %INCLUDE Z8A_SUPP_MAC;
      691     2499         %INCLUDE Z8A_ST;
      692     6158         %INCLUDE Z8A_ERR_NUMS;
      693     6922         %INCLUDE Z8E_STACKS;
      694     9188         %INCLUDE Z8E_O1COM;
      695     9949         %INCLUDE Z8A_DNT;
PL6.E3A0      #007=Z8EH$CHECK_LABEL File=Z8EH.:Z8A6TSI                           SAT 08/09/97 12:00 Page:134  
      696    10170         %INCLUDE Z8E_MACROS;
      697    10845
      698    10846    1    DCL Z8AE$ERROR                     ENTRY(12);
      699    10847
      700    10848    1    DCL LABEL_CHECK_FOUND BOOLEAN;
      701    10849    1    DCL DATA_NAME_FOUND BOOLEAN;
      702    10850
      703    10851    1           LABEL_ST$ = LABEL$ -> LX_TERMINAL.DNT$ -> DNT.ST$;
      704    10852
      705    10853    1           DATA_NAME_FOUND = FALSE;
      706    10854    1           LABEL_CHECK_FOUND = FALSE;
      707    10855
      708    10856    2           DO WHILE LABEL_ST$ ~= ADDR(NIL);
      709    10857
      710    10858
      711    10859    2              IF LABEL_ST$ -> VARIABLE.ID = LABEL_CHECK
      712    10860    2                AND LABEL_ST$ -> LABEL.PROC$ = O1COM.CURRENT_PROC$ THEN
      713    10861    2                 LABEL_CHECK_FOUND = TRUE;
      714    10862
      715    10863    2              IF LABEL_ST$ -> VARIABLE.PROC$ = O1COM.CURRENT_PROC$ THEN
      716    10864    3                DO CASE (LABEL_ST$ -> ST.ID);
      717    10865    3                  CASE (%ALPHABET, %ARRAY, %CLASS, %CONDITION, %DML_KEY, %DML_SET,
      718    10866    3                        %FD, %MNEMONIC, %RD, %REALM, %SCALAR, %SD, %SWITCH);
      719    10867    3                    DATA_NAME_FOUND = TRUE;
      720    10868    3                END;
      721    10869
      722    10870    2              LABEL_ST$ = LABEL_ST$ -> VARIABLE.NEXT;
      723    10871
      724    10872    2              END;
      725    10873
      726    10874    1           IF LABEL_CHECK_FOUND THEN
      727    10875                   %CONDITIONAL_ERROR (@LX_ITEM$ = LABEL$,
      728    10876                     @ERROR# = %E$SECTION_PARA_USAGE,
      729    10877                     @LINE = LABEL$ -> LX_TERMINAL.LINE,
      730    10878                     @COLUMN = LABEL$ -> LX_TERMINAL.COLUMN,
      731    10879                     @PLUG1 = LABEL$ -> LX_TERMINAL.DNT$);
      732    10909
PL6.E3A0      #007=Z8EH$CHECK_LABEL File=Z8EH.:Z8A6TSI                           SAT 08/09/97 12:00 Page:135  
      733    10910    1           IF DATA_NAME_FOUND THEN
      734    10911                   %CONDITIONAL_ERROR (@LX_ITEM$ = LABEL$,
      735    10912                     @ERROR# = LABEL_ERROR,
      736    10913                     @LINE = LABEL$ -> LX_TERMINAL.LINE,
      737    10914                     @COLUMN = LABEL$ -> LX_TERMINAL.COLUMN,
      738    10915                     @PLUG1 = LABEL$ -> LX_TERMINAL.DNT$);
      739    10945
      740    10946    1   END Z8EH$CHECK_LABEL;
      741    10947        %EOD;

