      603    10520    4                 DO CASE (ST$ -> ST.ID);
      604    10521    4                   CASE (%SCALAR, %ARRAY, %POINTER, %CONST);
      605    10522    4      /* Okay */ ;
      606    10523    4                   CASE (ELSE);
      607    10524    4                     CALL ARG_ERROR;
      608    10525    4                 END;
      609    10526
      610    10527    3               CASE(%TYY, %TYX);
      611    10528              /* IBM index data type.  This type is meaningless for
      612    10529                 calling other procs, and for moving in the case of
      613    10530                 CALL ... BY CONTENT.  For this reason, we will use
      614    10531                 the TYA guy that redefines the index guy.          */
      615    10532
      616    10533                      %FISH (TRIAD = L$ -> LX_ETT_ITEM.ETT$,
      617    10534                      RTN = INDEX_GUY$);
      618    10542
      619    10543    3                 IF INDEX_GUY$ -> SCALAR.TYPE = %TYX
      620    10544    3                   AND INDEX_GUY$ -> SCALAR.LTS$ ~= ADDR(NIL) THEN
      621    10545    3                    CALL ARG_ERROR;
      622    10546    3                 ELSE
      623    10547    4                 DO;
      624    10548
      625    10549    4                    INDEX_GUY$ = INDEX_GUY$ -> SCALAR.QLT$ -> QLT.BROTHER -> QLT.ST$;
      626    10550
      627    10551    4                    SAVE_LINE = L$ -> LX_ETT_ITEM.LINE;
      628    10552    4                    SAVE_COLUMN = L$ -> LX_ETT_ITEM.COLUMN;
