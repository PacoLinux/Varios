      119    14729    4                TYPE_DESC.SIZ$ = ADDR(NIL);
      120    14730
      121    14731    4                CALL Z8EU$NEW_TEMP_FROM_DESC (TYPE_DESC,
      122    14732    4                                              ADDR(TEMP_ITEM));
      123    14733    4              END;
      124    14734    3            ELSE
      125    14735    3              CALL Z8EU$NEW_TEMP_FROM_ETT (ACCEPT_ARG$ -> LX_ETT_ITEM.ETT$,
