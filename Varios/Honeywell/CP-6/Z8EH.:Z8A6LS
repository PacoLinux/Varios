PL6.E3A0      #001=Z8EH$ACCEPT_STATEMENT File=Z8EH.:Z8A6TSI                      SAT 08/09/97 11:51 Page:2    
       26    14502        /*I*
       27    14503                 NAME: ACCEPT_STATEMENT
       28    14504
       29    14505          DESCRIPTION: Generate an %ACCEP ILL item with references to a descriptor
       30    14506                       list and an argument list.  If the variable being accepted
       31    14507                       has a format that cannot be directly input , a temporary
       32    14508                       variable is created, the ACCEPT is done to the temporary
       33    14509                       variable, and then this value is moved into the actual variable.
       34    14510        */
       35    14511
       36    14512
       37    14513    1    DCL ACCEPT_ARG$               PTR;
       38    14514    1    DCL ACCEPT_CODE               SBIN WORD;
       39    14515
       40    14516         %DCL_AUTO_LX_ITEM (@NAME=TEMP_ITEM);
       41    14520    1    DCL TEMP_REQUIRED BOOLEAN;
       42    14521    1    DCL TEMP_ST$ PTR;
       43    14522    1    DCL ST$$     PTR;
       44    14523    1    DCL RT_DESC_LIST$ PTR;
       45    14524    1    DCL RT_ARG_LIST$ PTR;
       46    14525    1    DCL NEL$ PTR;
       47    14526    1    DCL 1 TYPE_DESC,
       48    14527              %TYPE_DESC;
       49    14561
       50    14562    1    DCL Z8AT$CREATE_TEMPORARY          ENTRY(2);
       51    14563    1    DCL Z8AF$NEW_BASE                  ENTRY(3);
       52    14564    1    DCL Z8AE$ERROR                     ENTRY(12);
       53    14565    1    DCL Z9EY$BUILD_RUNTIME_DESCRIPTOR  ENTRY(3);
       54    14566    1    DCL Z8EY$BUILD_RUNTIME_ARGLIST     ENTRY(3);
       55    14567    1    DCL Z8E7$MOVE                      ENTRY(2);
       56    14568    1    DCL Z8EU$NEW_TEMP_FROM_DESC        ENTRY(2);
       57    14569    1    DCL Z8EU$NEW_TEMP_FROM_ETT         ENTRY(2);
       58    14570
       59    14571    1    TEMP_REQUIRED = FALSE;
       60    14572
       61    14573    1   IF COMCOM.OB.FIPS.LEVEL >= %INT_LEV THEN
       62    14574    1     IF ACCEPT_CODE ~= %ACCEPT_SYSIN OR ACCEPT_ARG$ -> LX_ETT_ITEM.B.HIGH_LEV THEN
PL6.E3A0      #001=Z8EH$ACCEPT_STATEMENT File=Z8EH.:Z8A6TSI                      SAT 08/09/97 11:51 Page:3    
       63    14575    1       CALL Z8AE$ERROR (%E$LEVEL_CHECK,
       64    14576    1                        O1COM.LINE_NO,
       65    14577    1                        O1COM.COLUMN_NO);
       66    14578
       67    14579        /* Check type of identifiers */
       68    14580
       69    14581    2    DO CASE( ACCEPT_ARG$ -> LX_ETT_ITEM.TYPE );
       70    14582
       71    14583    2      CASE( %TYD, %TYO );
       72    14584
       73    14585             /*  OKAY   */
       74    14586
       75    14587    2      CASE( %TYA, %TYN );
       76    14588
       77    14589    2        IF NOT ACCEPT_ARG$ -> LX_ETT_ITEM.B.REFMOD   THEN
       78    14590    3          DO;
       79    14591
       80    14592                 %FISH(TRIAD = ACCEPT_ARG$ -> LX_ETT_ITEM.ETT$,
       81    14593                       RTN = TEMP_ST$);
       82    14601
       83    14602    3            IF TEMP_ST$ -> VARIABLE.B.CLASS = %CLASS_NUMERIC_EDITED THEN
       84    14603    4              DO;
       85    14604    4                TEMP_REQUIRED = TRUE;
       86    14605                     %GET_LTS (@ID = %NEL,
       87    14606                              @ST$ = TEMP_ST$,
       88    14607                              @RETURN$ = NEL$);
       89    14650
       90    14651    4                TYPE_DESC.DESC = NEL$ -> NEL.DESC;
       91    14652    4                TYPE_DESC.TYPE = %TYD;
       92    14653    4                TYPE_DESC.SIZ$ = ADDR(NIL);
       93    14654
       94    14655    4                CALL Z8EU$NEW_TEMP_FROM_DESC (TYPE_DESC,
       95    14656    4                                              ADDR(TEMP_ITEM));
       96    14657    4              END;
       97    14658    3    END;
       98    14659
       99    14660    2      CASE( %TYB, %TYE );
PL6.E3A0      #001=Z8EH$ACCEPT_STATEMENT File=Z8EH.:Z8A6TSI                      SAT 08/09/97 11:51 Page:4    
      100    14661
      101    14662    2        TEMP_REQUIRED = TRUE;
      102    14663    2        IF ACCEPT_ARG$ -> LX_ETT_ITEM.B.REFMOD THEN
      103    14664    2          CALL Z8EU$NEW_TEMP_FROM_ETT (ACCEPT_ARG$ -> LX_ETT_ITEM.ETT$,
      104    14665    2                                       ADDR(TEMP_ITEM));
      105    14666    2        ELSE
      106    14667    3          DO;
      107    14668
      108    14669                 %FISH(TRIAD = ACCEPT_ARG$ -> LX_ETT_ITEM.ETT$,
      109    14670                       RTN   = TEMP_ST$);
      110    14678
      111    14679    3            IF TEMP_ST$ -> VARIABLE.B.CLASS = %CLASS_NUMERIC_EDITED THEN
