      593    10503    1           L$ = LIST$ -> LX_LIST.HEAD$;
      594    10504
      595    10505    2           DO WHILE L$ ~= ADDR(NIL);
      596    10506
      597    10507    3              DO CASE(L$ -> LX_ETT_ITEM.TYPE);
      598    10508
      599    10509    3               CASE(%TYD, %TYI, %TYJ, %TYO, %TYR, %TYQ, %TYE, %TYH, %TYA,
