!COPY NO OVER *UI.UIACCT
!IF $FID_EXIST('GIN_SIG1.UIACCT') THEN COPY GIN_SIG1.UIACCT OVER *UI.UIACCT
!BMAP GIN_SIG1.SIACCT,*UI.UIACCT OVER *GIN_OUG1 (LS,OU)
!IF $SITE ~= 'LADC L66A' .OR. 'UIACCT'~='*NO*' THEN GOTO NOT_B
!COPY DAS$SYMBOL.COUACCT OVER GIN_OU62.SIACCT
!COPY DA_SYMBOLS_D.COUACCT OVER GIN_OU63.SIACCT
