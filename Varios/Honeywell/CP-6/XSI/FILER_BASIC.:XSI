!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY,XACCT=:XSI
!JOB NAME=FILER_BASIC
!RESOURCE TIME=60,MEM=256
!" The following JCL munges the CP_6_C file.
!C CP_6_C.LBACCT OVER *CP6C(LN)
!C B$JIT_C.LBACCT INTO *CP6C(LN(40000))
!E *CP6C
SE 40000-99999;IF/%/OR////,1,1;IF/(B$/;/ACRO B$JIT0X/E/END;/;/0/E/(B$JIT=B$JIT,STCLASS=BASED);/;EL;DE
E
!DRAW2_RU1.XACCT *CP6C OVER *CP6C1
!ST.X
!E *CP6C
SE;IF/%LSET LIST/;DE
END
!E *CP6C1
"SE;IF/L%SIZ/OR/LEN%LNG/;TY;/L%SIZ/S/L/;/LEN%LNG/S/LEN/;TY
SE90000-99999
IFNOT/VL/AND NOT/FP/AND NOT/B$/AND NOT/F$/,6,7;TY;DE;
EL/B$FIXED/OR/VLP_ACCESS/OR/VLP_ATTR/OR/VLP_ACSVEH/OR/VLP_EOMTABLE/;TY;DE;
EL/B$ECCBI/OR/B$EVNT/OR/B$SVLDT/OR/B$TDIO/OR/B$WSR_DESC/OR/FPT_ATTACH/;TY;DE;
EL/T$W$F$DCB/AND/STCLASS=STATIC/;/STATIC/S/BASED/
IP.999
%INCLUDE *CP6C;
END
!ST.X
!DIR .SIACCT
!SET M$LO _CP6C2,CTG,FORM=OLDWID
!PREPPY.XACCT *CP6C1 (LS,NSYS)
!R M$LO
!ST.X
!C _CP6C2 OVER _CP6C2(SVFC,LN)
!E _CP6C2
SE;IF/Preppy/OR/Include file/OR/in procedure/OR/in file/OR/is referenced/OR/%SET LISTDIR/;DE;
EL;1l26;0/T$W$/D;0f/&&/;IF/ &&/OR/%EQ/,1,3;DE;EL;/&&/D;0///**///D;IF/    /,1,4;5L4
end
!ST.X
!"
!" The CP_6_C file has now been munged, into file _CP6C2.  Now create the source
!" files.
!"
!C NO OVER FILER_SI9(XTNSIZE=20,IXTNSIZE=20,TY=DK,NREL)
!BASIC
LOAD FILER_SIB1.SIACCT
LIST
FASTER
_CP6C2
!ST.X
!BASIC
LOAD FILER_SIB2.SIACCT
LIST
FASTER
!ST.X
!BASIC
LOAD FILER_SIB3.SIACCT
LIST
FASTER
!ST.X
