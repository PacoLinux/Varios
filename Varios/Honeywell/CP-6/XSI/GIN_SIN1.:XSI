%INCLUDE GIN_C61;
 
GI_CMDS_WITH(D,%GI_CMDS_WITH) =  .FID .FID '(' COMMANDS [')'] .END
GI_CMDS_WITHOUT(D,%GI_CMDS_WITHOUT) =  COMMANDS  .END
 
COMMANDS = (DI_CMDS | UPDATE_LIST)
 
DI_CMDS(%DI_CMDS) = 'DI/SPLAY' < ',' , ( ;
    DI_FLAGS|DI_DCBS|DI_PRIVS|DI_ALT|DI_SHA| ;
    DI_FLAVOR|DI_TCB|DI_LOCKS| ;
    DI_PATCHS|DI_DPATCH|DI_IPATCH| ;
    DI_ALL) >
DI_FLAGS(%DI_FLAGS) = 'FLAG/S'
DI_DCBS(%DI_DCBS) = ('DCB/S'|'#')
DI_PRIVS(%DI_PRIVS) = 'PRI/VILEGES'
DI_ALT(%DI_ALT) = 'ALT/SHARELIB'
DI_SHA(%DI_SHA) = 'SHA/RELIB'
DI_FLAVOR(%DI_FLAVOR) = 'FLAV/OR'
DI_LOCKS(%DI_LOCKS) = 'LOC/KS'
DI_TCB(%DI_TCB) = 'TCB'
DI_PATCHS(%DI_PATCHS) = 'PA/TCHS'
DI_DPATCH(%DI_DPATCH) = 'DP/ATCH'
DI_IPATCH(%DI_IPATCH) = 'IP/ATCH'
DI_ALL(%DI_ALL) = 'ALL'
 
UPDATE_LIST(%UPDATE_LIST)= < ',' , OPTIONS >
OPTIONS = ;
    ( PSEUDOFLAVOR_CMDS | ;
      DEF_CMDS | ;
      DCB_CMDS | ;
      HEAD_CMDS | ;
      ESP_CMDS )
 
PSEUDOFLAVOR_CMDS(%PSEUDOFLAVOR_CMDS) = ;
    'PSE/UDOFLAVOR' '=' (HOST | FEP)
DCB_CMDS(%DCB_CMDS) = ;
    ( DCBSLOTS | ;
      DCB# | ;
      DCBN | ;
      NDCB# | ;
      NDCBN )
DCBSLOTS(%DCBSLOTS) =  'DC/BSLOTS' '=' .DEC
DCB#(%DCB#) = ;
    ( '#'-  | 'DCB'- )  ( ONE | TWO | THREE | FOUR ) '=' DCBNAME
DCBN(%DCBN) = ;
    (SIDCB | UIDCB | OUDCB | LODCB) '=' DCBNAME
NDCB#(%NDCB#) = ;
    ( 'N#'- | 'NDCB'- ) (ONE|TWO|THREE|FOUR)
NDCBN(%NDCBN) = ;
    'N'- (SIDCB|UIDCB|OUDCB|LODCB)
ONE(1)='1'
TWO(2)='2'
THREE(3)='3'
FOUR(4)='4'
SIDCB(1)='SI/DCB'
UIDCB(2)='UI/DCB'
OUDCB(3)='OU/DCB'
LODCB(4)='LO/DCB'
 
HEAD_CMDS(%HEAD_CMDS) = ;
    ( SHARELIB | ;
      NOSHARELIB | ;
      ALTSHARELIB | ;
      NOALTSHARELIB | ;
      FLAVOR | ;
      TCB | ;
      WLOCK | ;
      SLOCK | ;
      PRIVS | ;
      HEADBITS )
 
SHARELIB(%SHARELIB) = 'SHARE/LIB' '=' LIBNAME
NOSHARELIB(%NOSHARELIB)='NOSH/ARELIB'
ALTSHARELIB(%ALTSHARELIB) = 'ALT/SHARELIB' '=' LIBNAME
NOALTSHARELIB(%NOALTSHARELIB)='NOALT/SHARELIB'
FLAVOR(%FLAVOR) = 'FL/AVOR' '=' (HOST | FEP | .DEC)
TCB(%TCB) = 'TCB' '=' .DEC
WLOCK(%WLOCK) = 'WLOCK' LOCK_LIST ['=' (YES|ON|NO|OFF) ]
SLOCK(%SLOCK) = 'SLOCK' LOCK_LIST ['=' (YES|ON|NO|OFF) ]
LOCK_LIST(%LOCK_LIST) = '(' < ',' , .DEC > ')'
 
HEADBITS(%HEADBITS) = ;
    (SHAREABLE|NSHAREABLE|STDINVOC|PROCACC|NOCHGPMEM|SURCHARGE|LAUTO) ;
    [ '=' (YES|ON|NO|OFF) ]
SHAREABLE(%SHAREABLE) = 'SHAREA/BLE'
NSHAREABLE(%NSHAREABLE) = 'NSHAREA/BLE'
STDINVOC(%STDINVOC) = 'STD/INVOC'
PROCACC(%PROCACC) = 'PR/OCACC'
NOCHGPMEM(%NOCHGPMEM) = 'NOC/HGPMEM'
SURCHARGE(%SURCHARGE) = 'SUR/CHARGE'
LAUTO(%LAUTO) = 'LA/UTO'
 
PRIVS(%PRIVS) = ;
    'PRI/VILEGES' '(' PRIV_LIST ')' [ '=' (YES|ON|NO|OFF) ]
PRIV_LIST(%PRIV_LIST) = < ',' , ( 'BIT'- BITNN | {;
FMEFT|FMBLK|FMSEC|FMDIAG|FMREAD|;
SNAP|SCREECH|DISPJOB|SYSCON|ASAVE|;
GPP|SYSLOG|MFEP|CFEP|CQ|INTCON|IOQW|IOQ|;
EXPM|PM|TND|JIT|MSYS|MAXMEM|EXMM|SPCLMM } ) >
 
FMEFT(0)='FME/FT'
FMBLK(1)='FMB/LK'
FMSEC(2)='FMS/EC'
FMDIAG(3)='FMD/IAG'
FMREAD(4)='FMR/EAD'
SNAP(12)='SNA/P'
SCREECH(13)='SCR/EECH'
DISPJOB(14)='D/ISPJOB'
SYSCON(16)='SYSC/ON'
ASAVE(17)='AS/AVE'
GPP(18)='GP/P'
SYSLOG(20)='SYSL/OG'
MFEP(21)='MF/EP'
CFEP(22)='CF/EP'
CQ(23)='CQ'
INTCON(24)='INT/CON'
IOQW(25)='IOQW'
IOQ(26)='IOQ'
EXPM(27)='EXP/M'
PM(28)='P/M'
TND(29)='T/ND'
JIT(30)='J/IT'
MSYS(32)='MS/YS'
MAXMEM(33)='MA/XMEM'
EXMM(34)='EXM/M'
SPCLMM(35)='SP/CLMM'
BITNN(36) = .DEC2
 
ESP_CMDS(%ESP_CMDS) = ;
    ( PATCHS | DPATCH | IPATCH )
PATCHS(%PATCHS)='PA/TCHS'
IPATCH(%IPATCH)='IP/ATCH'
DPATCH(%DPATCH)='DP/ATCH'
 
DEF_CMDS(%DEF_CMDS) = ;
    ( ;
    ( REMOVE | KEEP | REPLACE ) .B ( ENTDEF | SYMDEF | SEGDEF ) ;
        [ DEF_LIST ] | ;
    ( PRIMARY | SECONDARY ) .B ENTDEF DEF_LIST ;
    )
REMOVE(%REMOVE) = 'REM/OVE'
KEEP(%KEEP) = 'KEE/P'
REPLACE(%REPLACE) = 'REP/LACE'
PRIMARY(%PRIMARY) = 'PRIM/ARY'
SECONDARY(%SECONDARY) = 'SEC/ONDARY'
 
ENTDEF(%ENTDEF) = 'ENT/DEFS'
SYMDEF(%SYMDEF) = 'SYM/DEFS'
SEGDEF(%SEGDEF) = ( 'SEG/DEFS' | 'ARE/ADEFS' )
DEF_LIST(%DEF_LIST) = '(' < ',' , DEFNAME > ')'
 
DCBNAME = (DCBNUMBER | DCBTEXT)
DCBNUMBER(%DCBNUMBER) = .DECIMAL29
DCBTEXT(%DCBTEXT) = .SYMBOL31
LIBNAME = (' ' |.ALPHA31+'0123456789$_-:' )
DEFNAME = .ALPHA+'0123456789$_-:.#@?'
HOST(%HOST) = 'H/OST'
FEP(%FEP) =  'F/EP'
YES(%ON)='Y/ES'
ON(%ON)='ON'
NO(%OFF)='N/O'
OFF(%OFF)='OFF'
