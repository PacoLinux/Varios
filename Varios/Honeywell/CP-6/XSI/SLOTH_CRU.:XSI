!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY,XACCT=X,COUACCT=:E04OU
!JOB NAME=SLOTH_CRU,WSN=LOCAL
!RESOURCE TIME=5,MEM=256
!ROUTE Dave Wagner, LADC
!PL6 SLOTH_SI61.SIACCT OVER *SLOTH_OU61.GNACCT,LP (LS,OU,WA,SC,SR(.SIACCT,.LBACCT,.COUACCT))
!PARTRGE SLOTH_SIN1.SIACCT OVER *SLOTH_OUN1.GNACCT,LP (LS,OU,SR(.SIACCT,.LBACCT))
!LINK *SLOTH_OU61.GNACCT ,;
      *SLOTH_OUN1.GNACCT ;
         OVER SLOTH.GNACCT (IP=100,DP=10)
!EXTRACT.XACCT
DATA_BASE *SLOTH_BASE
EXTRACT SLOTH_C61.SIACCT
BUILD INTO SLOTH.GNACCT, SLOTH_C61.SIACCT PROCESSOR
END
