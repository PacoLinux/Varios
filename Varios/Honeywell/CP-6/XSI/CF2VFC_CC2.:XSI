/******************************************************************************/
/*                                                                            */
/* cp6vfc.h - CP-6 VFC characters                                             */
/*                                                                            */
/*            The codes may be interpreted as follows:                        */
/*                P      Print                                                */
/*                Unn    Upspace nn lines                                     */
/*                F      Fortran                                              */
/*                B      Advance to Bottom of Page                            */
/*                T      Advance to Top of Page                               */
/*                Cnn    Advance to Channel nn                                */
/*                N      Newline                                              */
/*                R      Return to Column 1                                   */
/*                K      Keep Current Position                                */
/*                                                                            */
/******************************************************************************/

#define VFC_PU1 'A'   
#define VFC_PU2 'B'   
#define VFC_PU3 'C'   
#define VFC_PU4 'D'   
#define VFC_PU5 'E'   
#define VFC_PU6 'F'   
#define VFC_PU7 'G'   
#define VFC_PU8 'H'   
#define VFC_PU9 'I'   
#define VFC_PU10 'J'  
#define VFC_PU11 'K'  
#define VFC_PU12 'L'  
#define VFC_PU13 'M'  
#define VFC_PU14 'N'  
#define VFC_PU15 'O'  
#define VFC_PU0 '`'    
#define VFC_U1P 'a'   
#define VFC_U2P 'b'   
#define VFC_U3P 'c'   
#define VFC_U4P 'd'   
#define VFC_U5P 'e'   
#define VFC_U6P 'f'   
#define VFC_U7P 'g'   
#define VFC_U8P 'h'   
#define VFC_U9P 'i'   
#define VFC_U10P 'j'  
#define VFC_U11P 'k'  
#define VFC_U12P 'l'  
#define VFC_U13P 'm'  
#define VFC_U14P 'n'  
#define VFC_U15P 'o'  
#define VFC_PB 'P'    
#define VFC_PC1 'Q'   
#define VFC_PC2 'R'   
#define VFC_PC3 'S'   
#define VFC_PC4 'T'   
#define VFC_PC5 'U'   
#define VFC_PC6 'V'   
#define VFC_PC7 'W'   
#define VFC_PC8 'X'   
#define VFC_PC9 'Y'   
#define VFC_PC10 'Z'  
#define VFC_PC11 '['  
#define VFC_PC12 '\\' 
#define VFC_PC13 ']'  
#define VFC_PC14 '^'  
#define VFC_PC15 '_'  
#define VFC_FTP '0'   
#define VFC_C1P '1'   
#define VFC_C2P '2'   
#define VFC_C3P '3'   
#define VFC_C4P '4'   
#define VFC_C5P '5'   
#define VFC_C6P '6'   
#define VFC_C7P '7'   
#define VFC_C8P '8'   
#define VFC_C9P '9'   
#define VFC_C10P ':'  
#define VFC_C11P ';'  
#define VFC_C12P '<'  
#define VFC_C13P '='  
#define VFC_C14P '>'  
#define VFC_C15P '?'  
#define VFC_PK '@'     
#define VFC_TPU0 'p'    
#define VFC_TPU1 'q'  
#define VFC_TPU2 'r'  
#define VFC_TPU3 's'  
#define VFC_TPU4 't'  
#define VFC_TPU5 'u'  
#define VFC_TPU6 'v'  
#define VFC_TPU7 'w'  
#define VFC_TPU8 'x'  
#define VFC_TPU9 'y'  
#define VFC_TPU10 'z' 
#define VFC_TPU11 '{' 
#define VFC_TPU12 '|' 
#define VFC_TPU13 '}' 
#define VFC_TPU14 '~' 
#define VFC_TPU15 '\x7f' 
#define VFC_CONDITIONAL_B '\x1a'
#define VFC_NPU '\x1e'          
#define VFC_NPU1 '\x1f'         
