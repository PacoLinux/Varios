!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=LNCOUNT_CRU,WSN=LOCAL
!RESOURCE TIME=25,MEM=256
!ROUTE Bill Huckeba, LADC
!PARTRGE LNCOUNT_SIN1.SIACCT OVER *LNC:POU.GNACCT,*LNC_LSP (NSYS,SR(.SIACCT,.LBACCT))
!PL6 LNCOUNT_SI61.SIACCT OVER *LNC:BO1.GNACCT,*LNC_LS1 (NSYS,BC,SR(.SIACCT,.LBACCT),LS)
!PL6 LNCOUNT_SI62.SIACCT OVER *LNC:BO2.GNACCT,*LNC_LS2 (NSYS,BC,SR(.SIACCT,.LBACCT),LS)
!COPY *LNC_LS1,*LNC_LS2,*LNC_LSP ON *LNCOUNT_LIST
!LINK B_COPYRIGHT.LBACCT, *LNC:BO1.GNACCT,*LNC:BO2.GNACCT,*LNC:POU.GNACCT,;
       B_POINTER_D.LBACCT;
     OVER LNCOUNT.GNACCT (IP=100,DP=10)
!EXTRACT.XACCT
DA *TEMP
EX LNCOUNT_C2.SIACCT
BU OVER LNCOUNT_ERM.GNACCT,LNCOUNT_C2.SIACCT PRO
END
!LDEV LP01,FORM='PERF'
!EDGEMARK.XACCT *LNCOUNT_LIST ON LP (LEFT='LNCOUNT',RIGHT='GWJ')
