/*  Field Table MACRO  */
 
%MACRO FIELD (XCORD=0,YCORD=0,CVAL=' ',CLEN=0,HILATT(NONE=0,BLINK=1,
              RVRVD=2,UNDLN=3,RVRBLN=4)=0,LAST=",",NAME=FIELD,
              HILITE(YES='1'B,NO='0'B)='0'B);
        2 NAME ALIGNED,
           3 HILTE BIT(1) INIT(HILITE),
           3 HIATT UBIN(35) UNAL INIT(HILATT),
           3 XC UBIN(18) HALIGNED INIT(XCORD),
           3 YC UBIN(18) HALIGNED INIT(YCORD),
           3 * CHAR(16) INIT(' '),
           3 CURVAL,
              4 LEN UBIN(9) UNAL INIT(SIZEC(CVAL)),
              4 VAL CHAR(CLEN) INIT(CVAL) LAST
%MEND;
 
/*  Screen Table MACROs  */
 
%MACRO SCREEN (NAME=SCREEN,LINE1=' ',LINE2=' ',LINE3=' ',LINE4=' ',
               LINE5=' ',LINE6=' ',LINE7=' ',LINE8=' ',LINE9=' ',
               LINE10=' ',LINE11=' ',LINE12=' ',LINE13=' ',LINE14=' ',
               LINE15=' ',LINE16=' ',LINE17=' ',LINE18=' ',LINE19=' ',
               LINE20=' ',LINE21=' ',LINE22=' ',LINE23=' ',LINE24=' ');
  DCL 1 NAME STATIC,
        2 LIN1 CHAR(80) INIT(LINE1),
        2 LIN2 CHAR(80) INIT(LINE2),
        2 LIN3 CHAR(80) INIT(LINE3),
        2 LIN4 CHAR(80) INIT(LINE4),
        2 LIN5 CHAR(80) INIT(LINE5),
        2 LIN6 CHAR(80) INIT(LINE6),
        2 LIN7 CHAR(80) INIT(LINE7),
        2 LIN8 CHAR(80) INIT(LINE8),
        2 LIN9 CHAR(80) INIT(LINE9),
        2 LIN10 CHAR(80) INIT(LINE10),
        2 LIN11 CHAR(80) INIT(LINE11),
        2 LIN12 CHAR(80) INIT(LINE12),
        2 LIN13 CHAR(80) INIT(LINE13),
        2 LIN14 CHAR(80) INIT(LINE14),
        2 LIN15 CHAR(80) INIT(LINE15),
        2 LIN16 CHAR(80) INIT(LINE16),
        2 LIN17 CHAR(80) INIT(LINE17),
        2 LIN18 CHAR(80) INIT(LINE18),
        2 LIN19 CHAR(80) INIT(LINE19),
        2 LIN20 CHAR(80) INIT(LINE20),
        2 LIN21 CHAR(80) INIT(LINE21),
        2 LIN22 CHAR(80) INIT(LINE22),
        2 LIN23 CHAR(80) INIT(LINE23),
        2 LIN24 CHAR(80) INIT(LINE24);
 
%MEND;
 
%MACRO SPC_TITLES (NAME=SPC_TITLES,XCORD=0,YCORD=0,TITLE=' ',
                   TLEN=0,HILATT(NONE=0,BLINK=1,RVRVD=2,UNDLN=3,
                   RVRBLN=4)=0,LAST=",");
        2 NAME,
           3 NEXT$ PTR INIT(ADDR(NIL)),
           3 XC UBIN(18) HALIGNED INIT(XCORD),
           3 YC UBIN(18) HALIGNED INIT(YCORD),
           3 HIATT UBIN(9) UNAL INIT(HILATT),
           3 TTL,
              4 LEN UBIN(9) UNAL INIT(SIZEC(TITLE)),
              4 VAL CHAR(TLEN) INIT(TITLE) LAST
%MEND;
 
/*
        These macros are for generating the ptr array that has
        the start pointers for each routine, screen, and
        special-title table.  You MUST specify names of tables in
        ascending order, from (for example) ROUTINE1 to ROUTINE3.
        ROUTINEn specifies the name of a ROUTINE table, SCREENn
        specifies the name of a SCREEN table, and SPCn specifies
        the name of a Special-titles table.  All SPCn are optional,
        but each of ROUTINEn, SCREENn, and SPCn must correspond to
        a single screen.  Logically, MOM looks at them as a single
        table, and references each set of entries with a single index.
*/
%MACRO FLDS (ROUTINE1=NIL,SCREEN1=NIL,SPC1=NIL,
        ROUTINE2=NIL,SCREEN2=NIL,SPC2=NIL,
        ROUTINE3=NIL,SCREEN3=NIL,SPC3=NIL,
        ROUTINE4=NIL,SCREEN4=NIL,SPC4=NIL,
        ROUTINE5=NIL,SCREEN5=NIL,SPC5=NIL,
        ROUTINE6=NIL,SCREEN6=NIL,SPC6=NIL,
        ROUTINE7=NIL,SCREEN7=NIL,SPC7=NIL,
        ROUTINE8=NIL,SCREEN8=NIL,SPC8=NIL,
        ROUTINE9=NIL,SCREEN9=NIL,SPC9=NIL,
        ROUTINE10=NIL,SCREEN10=NIL,SPC10=NIL,NUM=0);
%LSET NUM = NUM + 1;
%IF TEXTCHAR(ROUTINE1) = TEXTCHAR(NIL);
%IF TEXTCHAR(ROUTINE2) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE3) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE4) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE5) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE6) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE7) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE8) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE9) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(ROUTINE10) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC1) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC2) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC3) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC4) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC5) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC6) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC7) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC8) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC9) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SPC10) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN1) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN2) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN3) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN4) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN5) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN6) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN7) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN8) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN9) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(SCREEN10) = TEXTCHAR(NIL);
%ENDIF;
%IF TEXTCHAR(NUM) = TEXTCHAR(NIL);
%ENDIF;
%ELSE;
  %IF TEXTCHAR(ROUTINE2) = TEXTCHAR(NIL);
        %RESUB LAST = ";";
  %ENDIF;
        2 %CHARTEXT(CONCAT('RTN',BINCHAR(NUM))),
           3 LOC$ PTR INIT(ADDR(ROUTINE1)),
           3 SIZ# UBIN INIT(SIZEW(ROUTINE1)),
        2 %CHARTEXT(CONCAT('SCN',BINCHAR(NUM))) PTR INIT(ADDR(SCREEN1)),
        2 %CHARTEXT(CONCAT('SPC',BINCHAR(NUM))),
           3 LOC$ PTR INIT(ADDR(SPC1)),
           3 SIZ# UBIN INIT(SIZEC(SPC1)) LAST
%FLDS (ROUTINE1=ROUTINE2,SCREEN1=SCREEN2,SPC1=SPC2,
        ROUTINE2=ROUTINE3,SCREEN2=SCREEN3,SPC2=SPC3,
        ROUTINE3=ROUTINE4,SCREEN3=SCREEN4,SPC3=SPC4,
        ROUTINE4=ROUTINE5,SCREEN4=SCREEN5,SPC4=SPC5,
        ROUTINE5=ROUTINE6,SCREEN5=SCREEN6,SPC5=SPC6,
        ROUTINE6=ROUTINE7,SCREEN6=SCREEN7,SPC6=SPC7,
        ROUTINE7=ROUTINE8,SCREEN7=SCREEN8,SPC7=SPC8,
        ROUTINE8=ROUTINE9,SCREEN8=SCREEN9,SPC8=SPC9,
        ROUTINE9=ROUTINE10,SCREEN9=SCREEN10,SPC9=SPC10,
        ROUTINE10=NIL,SCREEN10=NIL,SPC10=NIL,NUM=NUM);
%ENDIF;
%MEND;
%MACRO STARTS (SCREENS=0,ROUTINE1=NIL,SCREEN1=NIL,SPC1=NIL,
               ROUTINE2=NIL,SCREEN2=NIL,SPC2=NIL,
               ROUTINE3=NIL,SCREEN3=NIL,SPC3=NIL,
               ROUTINE4=NIL,SCREEN4=NIL,SPC4=NIL,
               ROUTINE5=NIL,SCREEN5=NIL,SPC5=NIL,
               ROUTINE6=NIL,SCREEN6=NIL,SPC6=NIL,
               ROUTINE7=NIL,SCREEN7=NIL,SPC7=NIL,
               ROUTINE8=NIL,SCREEN8=NIL,SPC8=NIL,
               ROUTINE9=NIL,SCREEN9=NIL,SPC9=NIL,
               ROUTINE10=NIL,SCREEN10=NIL,SPC10=NIL);
%EQU NUMSCRNS = SCREENS - 1;
%LSET NUM = 0;
%RESUB LAST = ",";
  DCL 1 STARTS STATIC,
%FLDS (ROUTINE1=ROUTINE1,SCREEN1=SCREEN1,SPC1=SPC1,
        ROUTINE2=ROUTINE2,SCREEN2=SCREEN2,SPC2=SPC2,
        ROUTINE3=ROUTINE3,SCREEN3=SCREEN3,SPC3=SPC3,
        ROUTINE4=ROUTINE4,SCREEN4=SCREEN4,SPC4=SPC4,
        ROUTINE5=ROUTINE5,SCREEN5=SCREEN5,SPC5=SPC5,
        ROUTINE6=ROUTINE6,SCREEN6=SCREEN6,SPC6=SPC6,
        ROUTINE7=ROUTINE7,SCREEN7=SCREEN7,SPC7=SPC7,
        ROUTINE8=ROUTINE8,SCREEN8=SCREEN8,SPC8=SPC8,
        ROUTINE9=ROUTINE9,SCREEN9=SCREEN9,SPC9=SPC9,
        ROUTINE10=ROUTINE10,SCREEN10=SCREEN10,SPC10=SPC10,NUM=NUM);
 
  DCL 1 START$ (0:%NUMSCRNS) REDEF STARTS,
        2 RTN,
           3 LOC$ PTR,
           3 SIZ# UBIN,
        2 SCN$ PTR,
        2 SPC,
           3 LOC$ PTR,
           3 SIZ# UBIN;
%MEND;
/*
                    EQUs for field attributes.
*/
%EQU NONE# = 0;
%EQU BLINK# = 1;
%EQU RVRVD# = 2;
%EQU UNDLN# = 3;
%EQU RVRBLN# = 4;
/*
                    %INCLUDE of B$JIT_C and invocations of JIT
                    macros.  MOM requires the B$JIT structure
                    for initialization.
*/
%INCLUDE B$JIT_C;
%B$JIT0(STCLASS=BASED);
%U$JIT1;
%M$JIT2;
%F$JIT3;
%S$JIT4;
%J$JIT5;
%A$JIT6;
/*
              This macro sets up the level-one structure DCL for
              the ROUTINE, FIELD and Special-titles tables.
*/
%MACRO LVL1DCL (NAME=LVL1,STATIC=STATIC);
  DCL 1 NAME STATIC,
%MEND;
