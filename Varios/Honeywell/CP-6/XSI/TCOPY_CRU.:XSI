!DEFAULT SIACCT=:XSI, GNACCT=:XSI, LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=TCOPY_CRU,WSN=LOCAL
!RES MEM=256, TIME=10
!ROUTE Andrew Birner, Zenith
!PL6 TCOPY_SI61.SIACCT OVER *TCOPY_OU61.GNACCT,LP(LS,SC,SR(.LBACCT, .SIACCT))
!PARTRGE TCOPY_SIN1.SIACCT OVER *TCOPY_OUN1.GNACCT,LP(LS,SR(.SIACCT))
!PL6 TCOPY_SI62.SIACCT OVER *TCOPY_OU62.GNACCT,LP(LS,SC,SR(.SIACCT,.LBACCT))
!LINK B_COPYRIGHT.LBACCT,;
       *TCOPY_OU61.GNACCT,;
       *TCOPY_OUN1.GNACCT,;
       *TCOPY_OU62.GNACCT ;
 OVER  TCOPY.GNACCT (SIDCB=M$EI, OUDCB=M$EO, PRIV(FMSEC))
!EXTRACT.XACCT
   DA *TCOPY
   EX TCOPY_SI61.SIACCT
   BU INTO TCOPY.GNACCT, TCOPY_C61.SIACCT PRO
   Q
