!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=UNGMAP_CRU,WSN=LOCAL
!RESOURCE TIME=5,MEM=128
!ROUTE Dave Platt, LADC
!" CRU just exists for $MAKE_ALL_JCL
!" Currently(8/90) the link for UNGMAP is located as :LNKUNGMAP.:ZWDVMSC
!" on L66A.  This is location is subject to change without notice
!" :LNKUNGMAP added to CRU (2/92).
!LINK ;
      ZPA$UNGMAP_DRIVER.:ZWE2OU, ;
      ZPC$DISPPROC_L6.:ZWE2OU, ;
      ZPC$UNGMAP.:ZWE2OU, ;
      ZPD$OPT_NODES.:ZWE2OU, ;
      ZPG$ERROR_WRITE.:ZWE2OU ;
         OVER UNGMAP.:XSI (IPATCH=200, DPATCH=20)
!MOD UNGMAP.GNACCT TO (NBA)
!MOD UNGMAP.GNACCT TO (BA)
