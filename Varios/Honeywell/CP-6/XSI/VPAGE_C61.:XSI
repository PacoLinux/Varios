DCL OLDPAGE CHAR(2000) STATIC SYMDEF;
DCL NEWPAGE CHAR(2000) STATIC SYMDEF;
DCL WX UBIN STATIC SYMDEF;
DCL WY UBIN STATIC SYMDEF;
DCL X UBIN STATIC SYMDEF;
DCL Y UBIN STATIC SYMDEF;
DCL MAXX UBIN STATIC SYMDEF;
DCL MAXY UBIN STATIC SYMDEF;
DCL INIT_CALLED BIT(1) STATIC SYMDEF INIT('0'B);
DCL ALL_CLEAR BIT(1) STATIC SYMDEF INIT('0'B);
DCL INVERS BIT(1) STATIC SYMDEF INIT('0'B);
DCL V_NORMAL UBIN STATIC SYMDEF INIT(14);
DCL V_INVERT UBIN STATIC SYMDEF INIT(26);
