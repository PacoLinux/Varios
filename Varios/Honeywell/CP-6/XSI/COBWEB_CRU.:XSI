!RESOURCE TIME=5,MEM=256
!ROUTE Jim Laguardia, LADC
!LET LISTING = 'DEST'
!LET MONNUM=$SUBSTR($DATE,2,2)
!LET MONTHS='JanFebMarAprMayJunJulAugSepOctNovDec'
!LET RIGHT=$SUBSTR(MONTHS,(MONNUM*3)-3,3) || ' ' || $SUBSTR($DATE,4,2)
!IF $SUBSTR(RIGHT,4,1) ~= '0' THEN GOTO DATE_OK
!LET RIGHT= $SUBSTR(RIGHT,0,4) || $SUBSTR(RIGHT,5,1)
!DATE_OK:
!PL6 COBWEB_SI61.SIACCT INTO *COBWEB_OU61.GNACCT,DEST (NSYS,OU,LS,SR(.SIACCT,.LBACCT))
!PARTRGE COBWEB_SIN1.SIACCT INTO *COBWEB_OUN1.GNACCT,DEST (NSYS,SR(.SIACCT,.LBACCT))
!IF LISTING = 'LP' THEN GOTO DONT_EDGEMARK
!EDGEMARK.XACCT DEST TO LP@LOCAL(F='PERF',L='COBWEB.X',R='%RIGHT')
!DONT_EDGEMARK:
!LINK B_COPYRIGHT.LBACCT, ;
      *COBWEB_OU61.GNACCT,;
      *COBWEB_OUN1.GNACCT ;
 OVER COBWEB.GNACCT (NSI,NUI,NOU,NLO,STDINVOC,IP=50,DP=10,PRI=FMR)
!EXTRACT.XACCT
DA *COBWEB_EXDB.GNACCT
EX COBWEB_SI61.SIACCT
BU OVER *COBWEB_ERRS.GNACCT,COBWEB_C1.SIACCT PRO
END
!C *COBWEB_ERRS.GNACCT INTO COBWEB.GNACCT
