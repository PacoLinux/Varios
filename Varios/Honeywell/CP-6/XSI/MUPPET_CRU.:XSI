!ROUTE Dave Wagner, LADC
!PL6 MUPPET_SI61.SIACCT OVER *MUPPET_OU61.GNACCT,LP  (SC,LS,OU,SR(.SIACCT,.LBACCT))
!PARTRGE MUPPET_SIN1.SIACCT OVER *MUPPET_OUN1.GNACCT,LP (LS,OU,SR(.SIACCT,.LBACCT))
!LINK B_COPYRIGHT.LBACCT,;
      *MUPPET_OU61.GNACCT,;
      *MUPPET_OUN1.GNACCT;
      OVER MUPPET.GNACCT (NDCB1, NDCB2, NDCB3, NDCB4,;
                          DCBSLOTS=300,;
                          IPATCH=100, DPATCH=10)
!EXTRACT.XACCT
DATA_BASE *MUPPET_BASE
EXTRACT MUPPET_C61.SIACCT
BUILD INTO MUPPET.GNACCT, MUPPET_C61.SIACCT PROCESSOR
END
