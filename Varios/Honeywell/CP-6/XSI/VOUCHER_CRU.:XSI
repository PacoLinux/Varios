! FORGE
LIB *:VOUCHER.SIACCT
READ VOUCHER_SIf1.SIACCT
! FORGE
LIB *:VOUCHER.SIACCT
GENERATE FORM1 OVER *VOUCHER_CF1.SIACCT (LANG=FORT)
GENERATE FORM2 OVER *VOUCHER_CF2.SIACCT (LANG=FORT)
GENERATE FORM3 OVER *VOUCHER_CF3.SIACCT (LANG=FORT)
END
! FORTRAN VOUCHER_SIF1.SIACCT OVER *VOUCHER_OU.GNACCT,LP (DBM,LS,SR(.SIACCT),NWA)
! COBOL85 VOUCHER_SIC1.SIACCT INTO *VOUCHER_OU.GNACCT,LP (LS)
! PL6 VOUCHER_SI61.SIACCT INTO *VOUCHER_OU.GNACCT,LP (LS)
! LINK B_COPYRIGHT.LBACCT,*VOUCHER_OU OVER VOUCHER.GNACCT;
    (UNSAT(:LIB_FORGE.:SYS,:LIB_ARES.:SYS,;
