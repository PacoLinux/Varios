%EQU CMD=1;                            /* PATCHCMD */
%EQU LST=2;                            /* Option list */
%EQU SRCE=11;                          /* Source fid */
%EQU HOW=12;                           /* TO/ON/OVER/INTO */
%EQU DEST=13;                          /* Destination fid */
%EQU HOWTO=1;                          /* TO */
%EQU HOWON=2;                          /* ON */
%EQU HOWINTO=3;                        /* INTO */
%EQU HOWOVER=4;                        /* OVER */
%EQU SNOPT=21;                         /* STAR # option */
%EQU PTOPT=22;                         /* PATCH TYPE option */
%EQU AIOPT=23;                         /* AUTHORS INITIALS option */
%EQU SVOPT=24;                         /* SYSTEM VERSION option */
%EQU RUOPT=25;                         /* RUN UNIT option */
%EQU OCOPT=26;                         /* OLD COMMENTS option */
%EQU PCOPT=27;                         /* PATCH COMMENTS optio */
%EQU NCOPT=28;                         /* NEW COMMENTS option */
%EQU UTOPT=29;                         /* UTS option */
%EQU TYOPT=30;                         /* TYPE option */
%EQU GLOPT=31;                         /* GLOM option */
%EQU DFOPT=32;                         /* DFWMC option */
%EQU DNOPT=33;                         /* DONE option */
%EQU OUOPT=34;                         /* OBJECT UNIT option */
%EQU LIBOPT=35;                        /* LIBRARY option */
%EQU GLOMA=1;                          /* ASYNC GLOM */
%EQU GLOMB=2;                          /* BSC GLOM */
%EQU GLOMD=3;                          /* DISKETTE GLOM */
%EQU GLOMF=4;                          /* FORMS GLOM */
%EQU GLOMH=5;                          /* HOST_LINK GLOM */
%EQU GLOMK=6;                          /* KERNEL GLOM */
%EQU GLOMM=7;                          /* MULE GLOM */
%EQU GLOMP=8;                          /* PIGLET GLOM */
%EQU GLOMR=9;                          /* RBT GLOM */
%EQU GLOMU=10;                         /* URP GLOM */
%EQU GLOMX=11;                         /* X.25 GLOM */
%EQU GLOM3=12;                         /* 3270 GLOM */
%EQU GLOM7=13;                         /* 7700 GLOM */
%EQU YESSIR=2;                         /* YES option */
%EQU NOSIR=1;                          /* NO option */
%EQU INITS=51;                         /* Initials */
%EQU VERSION=52;                       /* Version */
%EQU FID=53;                           /* RUM/OU/library name */
%EQU UTS=54;                           /* UTS */
%EQU STARLADC=1;                       /* LADC star */
%EQU STARDEC=2;                        /* Decimal star # */
%EQU PTYPEA=1;                         /* AARDVARK patch */
%EQU PTYPEX=2;                         /* XDELTA patch */
%EQU PTYPEM=3;                         /* Monitor patch */
%EQU PTYPEF=4;                         /* FEP patch */
%EQU PTYPER=5;                         /* RUM patch */
%EQU PTYPEP=6;                         /* PLOVER patch */
