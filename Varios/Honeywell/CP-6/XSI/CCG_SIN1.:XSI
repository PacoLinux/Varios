/*X* DMC,DFC */
%INCLUDE CCG_C1;
CCG@R(%CCG@ROOT) = [.B] CCGFIDX .B CCGFIDX [CCGASYMX] ['(' CCGOPTS [')']] .END
CCGFIDX(%CCG@FIDX) = .FID
CCGASYMX(%CCG@FIDX) = .ASYM+'('
CCGOPTS = < ',', {CCGMYSTA|CCGAU|CCGREP|CCGMSGID|CCGMSGTYP|CCGORIGIN|CCGDEST|CCGQSEL|CCGDELETE} >
CCGMYSTA = 'MY/STATION' '=' (CCGMYSTAQ|CCGMYSTAF)
CCGMYSTAQ(%CCG@MYSTA) = .Q
CCGMYSTAF(%CCG@MYSTA) = .FID
CCGAU = (CCGAUN|CCGAUY)
CCGAUN(%CCG@AUN) = 'A/U' '=' 'N/O'
CCGAUY(%CCG@AUY) = 'A/U' ['=' 'Y/ES']
CCGMSGTYP = 'MS/GTYP' '=' (CCGMSGTYPQ|CCGMSGTYPF)
CCGMSGTYPQ(%CCG@MSGTYP) = .Q
CCGMSGTYPF(%CCG@MSGTYP) = .FID
CCGORIGIN = 'O/RIGIN' '=' (CCGORIGINQ|CCGORIGINF)
CCGORIGINQ(%CCG@ORIGIN) = .Q
CCGORIGINF(%CCG@ORIGIN) = .FID
CCGDEST = 'D/EST' '=' (CCGDESTQ|CCGDESTF)
CCGDESTQ(%CCG@DEST) = .Q
CCGDESTF(%CCG@DEST) = .FID
CCGMSGID = 'MSGI/D' '=' CCGMSGIDD
CCGMSGIDD(%CCG@MSGID) = .DEC
CCGQSEL = 'Q/SELECT' '=' (CCGQSELANO|CCGQSELDIR|CCGQSELALL)
CCGQSELALL(%CCG@QSELALL) = 'A/LL'
CCGQSELDIR(%CCG@QSELDIR) = 'D/IRECT'
CCGQSELANO(%CCG@QSELANO) = 'AN/ONYMOUS'
CCGREP = (CCGREPN|CCGREPY)
CCGREPN(%CCG@REPN) = 'REP/ORT' '=' 'N/O'
CCGREPY(%CCG@REPY) = 'REP/ORT' ['=' 'Y/ES']
CCGDELETE = (CCGDELETEN|CCGDELETEY)
CCGDELETEN(%CCG@DELETEN) = 'DEL/ETE' '=' 'N/O'
CCGDELETEY(%CCG@DELETEY) = 'DEL/ETE' ['=' 'Y/ES']
