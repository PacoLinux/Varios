!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY,TLACCT=:TOOLE04, XACCT=:XSI
!JOB NAME=CRUNCHBIRD,WSN=LOCAL
!RESOURCE TIME=3,MEM=256
!ROUTE Bruce Frost, LADC
!PARTRGE CRUNCHBIRD_SIN1.SIACCT OVER *CRUNCHBIRD_OUN1.GNACCT,LP(NSYS,OU,LS,SRCH(.SIACCT))
!PL6 CRUNCHBIRD_SI61.SIACCT OVER *CRUNCHBIRD_OU61.GNACCT,LP(NSYS,LS,SR(.SIACCT,.TLACCT,.LBACCT))
!LINK B_COPYRIGHT.LBACCT, ;
      *CRUNCHBIRD_OU61.GNACCT, ;
      *CRUNCHBIRD_OUN1.GNACCT  ;
         OVER CRUNCHBIRD.GNACCT (IPATCH=100,DPATCH=20,;
                                SIDCB=M$IN,OUDCB=M$OUT, STDINVOC)
!MOD CRUNCHBIRD.GNACCT TO (ACC((ZZZCUST,?),EXEC))
