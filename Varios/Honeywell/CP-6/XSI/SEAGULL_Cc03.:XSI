/*P*    NAME:     SEAGULL_Cc03
        PURPOSE:  Contains #defines, structs, prototypes, etc. for the
                  string related functions put into SEAGULL.
        DESCRIPTION:
            This file is the Header file for SEAGULL_SIc03, the source
            file for string related functions.  This file will also be
            munged into the header file SEAGULL:h (in the X account).
*/
/********************************************************************/
 
#ifndef __SEAGULL_Cc03__
#define __SEAGULL_Cc03__
 
/*D*  NAME:    SG03 functional prototypes
      PURPOSE: location for string related functional prototypes
*/
 
#ifndef  __NO_SG03__
/* -- SEAGULL_SIc03 -- (string) FUNCTIONS */
char *sg3__StripNewline( char *);
char *sg3__StringToUpper( char *);
char *sg3__StringToLower( char *);
 
#ifndef  StripNewline
#define  StripNewline(a)  sg3__StripNewline(a)
#endif
#ifndef  StringToUpper
#define  StringToUpper(a) sg3__StringToUpper(a)
#endif
#ifndef  StringToLower
#define  StringToLower(a) sg3__StringToLower(a)
#endif
 
/*ifndef __NO_SG03__ */
#endif
 
/*ifndef __SEAGULL_Cc03__ (next #endif should be last line in file) */
#endif
