!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=APLANAR_ID_CRU,WSN=LOCAL
!RESOURCE TIME=5,MEM=256
!ROUTE Bruce Frost, LADC
!"  MUNGER function eats MULTIBAN_ID.XACCT and makes APLANAR_ID.:XSI
!APL APLANAR.SIACCT
'XG$FONT.:LIBRARY,REASSIGN,IN,ALL' $QFOPEN 1
'APLANAR$_ID.GNACCT,REASSIGN,CTG,CREATE,NEWFILE' $QFOPEN 2
 
MUNGE$_FONTS
 
$QFCLEAR
 
)END
!MOD APLANAR_ID.GNACCT TO (READ=(?,Z?))
