!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY,XACCT=:XSI
!JOB NAME=ROUTE_CRU,WSN=LOCAL
!RES MEM=255,TIME=2
!ROUTE John Joseph, LADC
!PL6 ROUTE_SI61.SIACCT OVER *ROUTE_OU6.GNACCT,LP (LS,OU,SC,SR(.SIACCT,.LBACCT))
!PARTRGE ROUTE_SIN1.SIACCT INTO *ROUTE_OU6.GNACCT,LP (LS,OU,SC,SR(.SIACCT,.LBACCT))
!LINK B_COPYRIGHT.LBACCT,;
          *ROUTE_OU6.GNACCT  ;
   OVER ROUTE.GNACCT (PRIV=(FMR),IP=50,DP=50)
!EXTRACT.XACCT
DA *ROUTE
EX ROUTE_SI61.SIACCT
BU OVER *ERRMSG.GNACCT PRO
RE
X
!C *ERRMSG.GNACCT INTO ROUTE.GNACCT
!MOD ROUTE.GNACCT TO (ACC((ZZZCUST,?),EXEC))
