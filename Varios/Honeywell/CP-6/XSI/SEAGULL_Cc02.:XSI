/*P*    NAME:     SEAGULL_Cc02
        PURPOSE:  Contains #defines, structs, etc. for time/date funcs.
        DESCRIPTION:
            This file is the Header file for SEAGULL_SIc02, the source
            file for time/date related functions.  This file will also
            be munged into the header file SEAGULL:h (in the X account).
*/
/********************************************************************/
 
#ifndef __SEAGULL_Cc02__
#define __SEAGULL_Cc02__
 
#ifndef  __NO_SG02__
/* -- SEAGULL_SIc03 -- (utility) FUNCTIONS */
char *sg2__GetTimeDate();
char *sg2__GetTime();
char *sg2__GetDate();
 
#ifndef  GetTimeAndDate
#define  GetTimeAndDate   sg2__GetTimeDate
#endif
#ifndef  GetTime
#define  GetTime          sg2__GetTime
#endif
#ifndef  GetDate
#define  GetDate          sg2__GetDate
#endif
 
/*ifndef __NO_SG02__ */
#endif
 
/*ifndef __SEAGULL_Cc02__ (next #endif should be last line in file) */
#endif
