%EQU DAYS_OPTION = 1;
%EQU CRUNCH_OPTION = 2;
 
%EQU DEFAULT_DAYS = -1;
 
/* STEPCCs used to indicate error conditions [I'm lazy] */
 
%EQU STEPCC_OK = 0;
%EQU STEPCC_XUG_ERROR = 10; /* XUG detected an error of some sort */
%EQU STEPCC_NO_PROFILE = 20; /* Profile record not the first in mailbox */
%EQU STEPCC_UNKNOWN_RECORD = 30; /* Encountered an unknown record type */
%EQU STEPCC_IO_ERROR = 40; /* I/O error of some sort */
