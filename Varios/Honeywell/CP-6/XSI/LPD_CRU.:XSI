!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY,DIRACCT=:XSI,COUACCT=:E04OU, XACCT=X
!DEFAULT DEST=*LS
!JOB NA=LPD,WSN=LOCAL
!RESOURCE MEM=511
!ROUTE Tim Cashin, LADC
!LET MSG='TO AC=' || $ACCT || ' Your LPD job (' || $SYSID || ') just started.'
!IF $MODE='BATCH' THEN %MSG
!DIR .DIRACCT
!LET LISTING= 'DEST'
!LET MONNUM=$SUBSTR($DATE,2,2)
!LET MONTHS='JanFebMarAprMayJunJulAugSepOctNovDec'
!LET RIGHT=$SUBSTR(MONTHS,(MONNUM*3)-3,3) || ' ' || $SUBSTR($DATE,4,2)
!IF $SUBSTR(RIGHT,4,1) ~= '0' THEN GOTO DATE_OK
!LET RIGHT= $SUBSTR(RIGHT,0,4) || $SUBSTR(RIGHT,5,1)
!DATE_OK:
!DEL *LPD_OUN1.GNACCT
!DEL *LPD_OU61.GNACCT
!DEL *LPD_OUc1.GNACCT
!L LPD_SIc1.SIACCT,LPD_SI61.SIACCT,LPD_SIN1.SIACCT,LPD_C61.SIACCT,LPD_C62.SIACCT,LPD_Cc3
!CC LPD_SIc1.SIACCT OVER *LPD_OUc1.GNACCT,DEST(OU,LS,SR(.SIACCT,.LBACCT,.XACCT,.COUACCT),SC)
!PL6 LPD_SI61.SIACCT INTO *LPD_OU61.GNACCT,DEST(OU,LS,SR(.SIACCT,.LBACCT,.XACCT,.COUACCT),SC)
!PARTRGE LPD_SIN1.SIACCT INTO *LPD_OUN1.GNACCT,DEST(LS,OU,SR(.SIACCT,.LBACCT))
!IF LISTING = 'LP' THEN GOTO DONT_EDGEMARK
!EDGEMARK.X DEST TO LP@(F='PERF',L='LPD.X',R='%RIGHT')
!DONT_EDGEMARK:
!LINK ;
       *LPD_OUc1.GNACCT,;
       *LPD_OU61.GNACCT,;
       *LPD_OUN1.GNACCT,;
       B_COPYRIGHT.LBACCT;
  OVER ;
       LPD.GNACCT (IPATCH=100,DPATCH=10,PRIV(FMSEC,JIT,MSYS))
!LET MSG='TO AC=' || $ACCT || ' Your LPD job (' || $SYSID || ') just finished.'
!IF $MODE='BATCH' THEN %MSG
