      593     7706        /**********************************************************************/
      594     7707        /*I*      NAME:CNVRT_FID
      595     7708                  PURPOSE:  BREAKS UP A FID INTO ITS VARIOUS COMPONENTS
      596     7709                  INPUT:    OUT$SYM
      597     7710                  OUTPUT:   FID into various vlp's
      598     7711                            errors output resulting from M$FID pmme execution
      599     7712        */
      600     7713        /**********************************************************************/
      601     7714    1   CNVRT_FID: PROC ALTRET;
      602     7715        /*
      603     7716          ****    BREAK UP THE FID
      604     7717        */
      605     7718    2      FPT_FID =CPSL_CFPT_FID;
      606     7719    2      FPT_FID.V_=VECTOR(FPT_FID.V);
      607     7720    2      FPT_FID.SN_ = VECTOR(FID_SN);
      608     7721    2      FPT_FID.WSN_ = VECTOR(FID_WSN);
      609     7722    2      FPT_FID.ASN_ = VECTOR(FID_ASN);
      610     7723    2      FPT_FID.RES_ = VECTOR(FID_RES);
      611     7724    2      FPT_FID.NAME_ = VECTOR(FID_NAME);
      612     7725    2      FPT_FID.ACCT_ = VECTOR(FID_ACCT);
      613     7726    2      FPT_FID.PASS_ = VECTOR(FID_PASS);
      614     7727    2      FPT_FID.RESULTS_ = VECTOR(FID_RESULTS);
      615     7728    2      FPT_FID.TEXTFID_ = VECTOR(OUT$SYM.TEXT);
      616     7729    2      CALL M$FID(FPT_FID) ALTRET(CNVRT_FID50);
      617     7730    2      RETURN;
      618     7731    2   CNVRT_FID50:
      619     7732    2      CALL POST_ERROR (OUT$SYM.CPOS,
      620     7733    2       B$TCB.ALT$ -> B$EXCFR.ERR);
      621     7734    2      ALTRETURN;
      622     7735    2   END CNVRT_FID;
      623     7736                 %EJECT;
PL6.E3A0      #001=CXX$CONTEXT2 File=CXX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:29 Page:21   
      624     7737        /**********************************************************************/
      625     7738        /*I*      NAME:   CHK_DCBNAME
      626     7739                  PURPOSE: VALIDATE A DCB NAME
      627     7740                  INPUT:   PTR$->P$S
      628     7741                           PTR1$->P$B
      629     7742        */
      630     7743        /**********************************************************************/
      631     7744    1   CHK_DCBNAME: PROC ;
      632     7745    2   DCL I SBIN;
      633     7746    2   DCL 1 FORT_DCBNAME BASED ALIGNED,/* CXB$CHKFORT moves the name back two bytes */
      634     7747    2             2 * BIT(18),
      635     7748    2             2 COUNT UBIN BYTE CALIGNED,
      636     7749    2             2 TEXT CHAR(FORT_DCBNAME.COUNT);
      637     7750
      638     7751
      639     7752        /*
      640     7753          ****    CHECK DCB LENGTH ... CAN'T EXCEED 29 CHARS, IF FORTRAN TYPE.
      641     7754                  OTHERWISE 31 CHARS. MAX.
      642     7755        */
      643     7756    2      I = OUT$SYM.COUNT;
      644     7757    2      IF OUT$SYM.NDTYPE = %DEC#
      645     7758    2        THEN
      646     7759    2         I = I + 2;  /* Need to add F$ for fortran unit numbers */
      647     7760    2      IF OUT$SYM.TEXTC$ ~= ADDR(OUT$SYM.COUNT)
      648     7761    2        THEN
      649     7762    2         I = ADDR(OUT$SYM.TEXTC$)->FORT_DCBNAME.COUNT;
      650     7763    2      IF (I > 31) THEN
      651     7764    2         CALL POST_ERROR(OUT$SYM.CPOS,E_XBIGDCB);
      652     7765    2      IF (OUT$SYM.TEXT = 'M$UC' OR
      653     7766    2       OUT$SYM.TEXT = 'M$LM') THEN
      654     7767    2         CALL POST_ERROR(OUT$SYM.CPOS,E_XILLDCB);
      655     7768    2      RETURN;
      656     7769    2   END CHK_DCBNAME;
      657     7770        %EJECT;
PL6.E3A0      #001=CXX$CONTEXT2 File=CXX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:29 Page:22   
      658     7771    1   POST_ERROR: PROC (CPOS, ERRCODE);
      659     7772
      660     7773    2   DCL CPOS UBIN WORD;
      661     7774    2   DCL ERRCODE BIT(36) ALIGNED;
      662     7775
      663     7776    2      CPSL_COMMAND_PARAMETERS.ERRCODE = ERRCODE;
      664     7777    2      CPSL_COMMAND_PARAMETERS.ERR_CPOS = CPOS;
      665     7778
      666     7779    2      IF NOT CPSL_COMMAND_PARAMETERS.FLAGS.INHIBIT_ERRMSG
      667     7780    3        THEN DO;
      668     7781
      669     7782    3         CPSL_COMMAND_PARAMETERS.FLAGS.REPORTED_TO_USER = %YES#;
      670     7783
      671     7784    3         IF CPSL_COMMAND_PARAMETERS.FLAGS.ECHO ~= %DONT_ECHO
      672     7785    4           THEN DO;
      673     7786
      674     7787    4            CPSL_COMMAND_PARAMETERS.FLAGS.ECHO = %DONT_ECHO;
      675     7788
      676     7789    4            CALL CXG$ECHO (XUG_GETCMD);
      677     7790
      678     7791    4            END;
      679     7792
      680     7793    3         XUG_GETCMD.ERR_CODE = ERRCODE;
      681     7794    3         XUG_GETCMD.EPOS# = CPOS;
      682     7795    3         XUG_GETCMD.USE_EPOS# = %YES#;
      683     7796
      684     7797    3         CALL CXG$ERRPTR (XUG_GETCMD);
      685     7798    3         CALL CXG$ERRMSG (XUG_GETCMD);
      686     7799
      687     7800    3         END;
      688     7801
      689     7802    2      RETURN;
      690     7803
      691     7804    2   END POST_ERROR;
      692     7805
      693     7806        %EJECT;
PL6.E3A0      #001=CXX$CONTEXT2 File=CXX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:29 Page:23   
      694     7807    1   SETUP_AND_DECODE: PROC ALTRET;
      695     7808
      696     7809    2      IF OUT$BLK.NDTYPE = %LITERAL# OR OUT$BLK.NDTYPE = %DSTRING#
      697     7810    2       OR OUT$BLK.NDTYPE >= %NULL# OR OUT$BLK.NSUBLKS = 0
      698     7811    2        THEN
      699     7812    2         PTR1$ = ADDR(OUT$BLK);
      700     7813    2        ELSE
      701     7814    2         PTR1$ = OUT$BLK.SUBLK$(0);
      702     7815
      703     7816    2      VALUE = -1;
      704     7817
      705     7818    2      IF OUT$BLK.CODE ~= DCBOPT AND OUT$BLK.CODE ~= FIDOPT
      706     7819    2        THEN
      707     7820    2         IF OUT$SYM.NDTYPE = %DEC#
      708     7821    2           THEN
      709     7822    2            CALL CXX$DECBIN(OUT$SYM.COUNT,VALUE) ALTRET(E_DECBIN);
      710     7823    2           ELSE
      711     7824    2            IF OUT$SYM.NDTYPE = %OCT#
      712     7825    2              THEN
      713     7826    2               CALL CXX$OCTBIN(OUT$SYM.COUNT,VALUE) ALTRET(E_DECBIN);
      714     7827
      715     7828    2      IF ADDR(VALUE_OUT) ~= ADDR(NIL)
      716     7829    2        THEN
      717     7830    2         VALUE_OUT = VALUE;
      718     7831
      719     7832    2      RETURN;
      720     7833
      721     7834    2   E_DECBIN: CALL POST_ERROR (OUT$SYM.CPOS, E_BDCVAL);
      722     7835
      723     7836    2      ALTRETURN;
      724     7837
      725     7838    2   END SETUP_AND_DECODE;
      726     7839
      727     7840    1   END CXX$CONTEXT2;
      728     7841        %EOD;

