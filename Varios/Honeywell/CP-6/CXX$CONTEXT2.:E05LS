      738     3245         %INCLUDE CX_ISUBS_C;
      739     3597
      740     3598        %PARSE$OUT (NAME=OUT$BLK, STCLASS=PARAM);
      741     3643    1   DCL ERRBUF UBIN PARAM;
      742     3644        %PARSE$OUT (NAME=P$B, STCLASS="BASED(BLK$)");
      743     3689    1   DCL BLK$ PTR;
      744     3690    1   DCL P$ PTR;
      745     3691    1   DCL NUM_OPTS SBIN;
      746     3692    1   DCL INDX SBIN;
      747     3693        %CPSL_COMMAND_PARAMETERS (NAME=CPSL_PARAM, STCLASS=AUTO);
      748     3711
      749     3712    1   DCL CXP$POSTERR ENTRY(5);
      750     3713    1   DCL CXX$SET ENTRY(4);
      751     3714    1   DCL CXX$RESET ENTRY(4);
      752     3715    1   DCL CXX$LDEV ENTRY(4);
      753     3716    1   DCL CXX$WINDOW ENTRY(4);
      754     3717    1   DCL CXX$TERMINAL ENTRY(4);
      755     3718    1   DCL CXX$UNHOLD ENTRY(4);
      756     3719        %EJECT;
PL6.E3A0      #002=CXX$VERIFY File=CXX$CONTEXT2.:E05TSI                          TUE 07/29/97 12:31 Page:129  
      757     3720    1      NUM_OPTS = 0;
      758     3721    1      INDX = 0;
      759     3722    2      DO WHILE NUM_OPTS < OUT$BLK.NSUBLKS;
      760     3723    2         BLK$ = OUT$BLK.SUBLK$(NUM_OPTS);
      761     3724    2         CPSL_PARAM = '0'B;
      762     3725    2         CPSL_PARAM.FLAGS.INHIBIT_ERRMSG = '1'B;
      763     3726    2         NUM_OPTS = NUM_OPTS + 1;
      764     3727    3   CXX_CASE: DO CASE (OUT$BLK.CODE);
      765     3728    3           CASE (ERASECMND, HOLDCMND, LDEVCMND, PRINTCMND);
      766     3729    3            IF P$B.CODE = LD_TABS AND INDX < BLK$->P$B.NSUBLKS
      767     3730    3              THEN GOTO CXX_SUBNODE;
      768     3731    3            CALL CXX$LDEV (P$B,,CPSL_PARAM);
      769     3732    3            GOTO CXX_WINDOW;
      770     3733
      771     3734    3           CASE (UNHOLDCMND);
      772     3735    3            P$ = BLK$;
      773     3736    3            IF P$B.CODE = UNHOLD_FID
      774     3737    4              THEN DO;
      775     3738    4               P$ = P$B.SUBLK$(1);
      776     3739    4               P$->P$B.SUBLK$(0)->P$B.CODE = P$->P$B.CODE;
      777     3740    4               END;
      778     3741    3            IF INDX < P$->P$B.NSUBLKS
      779     3742    4              THEN DO;
      780     3743    4               CALL CXX$UNHOLD (P$->P$B.SUBLK$(INDX)->P$B,,CPSL_PARAM);
      781     3744    4               NUM_OPTS = NUM_OPTS - 1;
      782     3745    4               INDX = INDX + 1;
      783     3746    4               END;
      784     3747    4              ELSE DO;
      785     3748    4               CALL CXX$UNHOLD (P$B,,CPSL_PARAM);
      786     3749    4               INDX = 0;
      787     3750    4               END;
      788     3751
      789     3752    3           CASE (RESETCMND);
      790     3753    3            CALL CXX$RESET (P$B,,CPSL_PARAM);
      791     3754    3            NUM_OPTS = OUT$BLK.NSUBLKS;
      792     3755
      793     3756    3           CASE (SETCMND, ADJUSTCMND);
PL6.E3A0      #002=CXX$VERIFY File=CXX$CONTEXT2.:E05TSI                          TUE 07/29/97 12:31 Page:130  
      794     3757    3            IF (P$B.CODE = SNOPT OR P$B.CODE = TABOPT)
      795     3758    3             AND INDX < P$B.NSUBLKS
      796     3759    3              THEN GOTO CXX_SUBNODE;
      797     3760    3            IF (P$B.CODE = ACCESSOPT OR P$B.CODE = ACSVEHOPT
      798     3761    3             OR P$B.CODE = READOPT OR P$B.CODE = WRITEOPT)
      799     3762    3             AND INDX < P$B.SUBLK$(0)->P$B.NSUBLKS
      800     3763    3              THEN
      801     3764    4            DO;
      802     3765    4               BLK$ = P$B.SUBLK$(0);
      803     3766    4   CXX_SUBNODE: BLK$ = P$B.SUBLK$(INDX);
      804     3767    4               CALL CXX$SET (P$B,,CPSL_PARAM);
      805     3768    4               INDX = INDX + 1;
      806     3769    4               NUM_OPTS = NUM_OPTS - 1;
      807     3770    4               EXIT CXX_CASE;
      808     3771    4               END;
      809     3772    3            CALL CXX$SET (P$B,,CPSL_PARAM);
      810     3773    3            GOTO CXX_WINDOW;
      811     3774
      812     3775    3           CASE (TERMINALCMND);
      813     3776    3            CALL CXX$TERMINAL (P$B,,CPSL_PARAM);
      814     3777    3   CXX_WINDOW: CALL CXX$WINDOW (BLK$->P$B,,CPSL_PARAM);
      815     3778    3            INDX = 0;
      816     3779    3           END;
      817     3780    2         IF CPSL_PARAM.ERRCODE
      818     3781    2           THEN
      819     3782    2            CALL CXP$POSTERR (ERRBUF,
      820     3783    2             CPSL_PARAM.ERR_CPOS, CPSL_PARAM.ERRCODE);
      821     3784    2         END;
      822     3785    1      RETURN;
      823     3786    1   END CXX$VERIFY;

