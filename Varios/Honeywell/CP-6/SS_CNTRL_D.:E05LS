   11      DCL S_PRIOB(0:4) SBIN STATIC SYMDEF INIT(1*0); /* BASE EXECUTION PRIORITY*/
   12      DCL SL_MF(0:4) SBIN STATIC SYMDEF INIT(10*0); /* MASTER FNC LIMIT, BY MODE*/
   13      DCL SL_MFU(0:4) SBIN STATIC SYMDEF INIT(4*0); /* MF UNBLOCK LIMIT*/
