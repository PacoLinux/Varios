      176    19512    3               CALL QCE$ERROR_INTERNAL ( %E$INTERNAL_COMPILER_ERROR );
      177    19513
      178    19514    3           END;
      179    19515
      180    19516    2           FPT@WAIT = QI_FPT_WAIT;
      181    19517    2           VBASE ( FPT@WAIT.V_ ) = ADDR ( FPT@WAIT.V );
      182    19518    2           FPT@WAIT.UNITS = VECTOR ( TIME_TO_WAIT_IN_SECONDS );
