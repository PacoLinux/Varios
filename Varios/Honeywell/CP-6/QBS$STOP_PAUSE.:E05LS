PL6.E3A0      #001=QBS$STOP_PAUSE File=QBS$STOP_PAUSE.:E05TSI                    WED 07/30/97 04:55 Page:3    
             19376    1           2 MESSAGE_LINE_GZ_HEADER$ PTR,
             19377                           /* Pointer to QI$GZ_HEADER for the
             19378                              MESSAGE LINE to use.  */
             19379    1           2 IS_MESSAGE_A_DAD BIT(1) ALIGNED,
             19380                           /* Set if MESSAGE$ points to a DAD,
             19381                              reset if MESSAGE$ points to text
             19382                              (in which case MESSAGE_LENGTH is valid).  */
             19383    1           2 MESSAGE_LENGTH UBIN,
             19384                           /* Length, in bytes, of the error message.  */
             19385    1           2 MESSAGE$ PTR,
             19386                           /* See IS_MESSAGE_A_DAD.  */
             19387    1           2 FRAME_INDEX UBIN,
             19388                           /* Index of the current frame.  */
             19389    1           2 FIELD_INDEX UBIN,
             19390                           /* Index of the field to re-get (if any).  */
             19391    1           2 STATUS UBIN,
             19392                           /* Returned status code:  0 -> Immediate return to level 0;
             19393                                                    >0 -> Status is ok.  */
             19394    1           2 FIELD_ERROR_STOP_INDEX UBIN,
             19395                           /* Passed index for QCG$GET_FIELD processing.  */
             19396    1           2 VISUAL_ATTRIBUTE UBIN;
             19397                           /* Matches %EQUs for QI#xxx_VISUAL_ATTR
             19398                              (currently, blinking or non-blinking message line).  */
             19405            %ENDIF;
