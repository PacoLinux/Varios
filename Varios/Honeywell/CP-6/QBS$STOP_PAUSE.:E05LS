        1        1        /*M* QBS$STOP_PAUSE:  "STOP dataname" and "STOP 'literal'".  */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QBS$STOP_PAUSE
       10       10              PURPOSE:      Write to the MESSAGE LINE, and STOP (for a while).
       11       11              DESCRIPTION:  Execute a STOP_PAUSE op-code, whose argument
       12       12                            is a FILE or WORKING-STORAGE SECTION DAD.
       13       13        */
       14       14
       15       15        QBS$STOP_PAUSE: PROC;
       16       16
       17       17        /*D*  NAME:         QBS$STOP_PAUSE
       18       18              CALL:         CALL QBS$STOP_PAUSE;
       19       19              PARAMETERS:   None.
       20       20              INTERFACE:    Called by QAT$TALLYHO when a STOP_PAUSE
       21       21                            op-code is encountered.  Calls QCM$MESS_whatever
       22       22                            to do the actual MESSAGE LINE I-O.
       23       23              ENVIRONMENT:  Nothing unusual.
       24       24              INPUT:        QI_1,
       25       25                            FPL rununit static data following the op-code.
       26       26              OUTPUT:       Writes to, reads from, and erases from the MESSAGE LINE.
       27       27              SCRATCH:      Not a whole lot.
       28       28              DESCRIPTION:  QCM$MESS is called to write the data to the default
       29       29                            MESSAGE LINE (see commentary in QCM$MESS).  QCM$MESS_PAUSE
       30       30                            is called to read 1 byte from the MESSAGE LINE (hence
       31       31                            the "pause"), and finally QCM$MESS_ERASE is called to,
       32       32                            yes, erase the data after the pause.
       33       33        */
       34       34
       35       35          %INCLUDE LCP_6;
       36      226          %INCLUDE NK_VFC_C;
       37      300          %FPT_DEVICE ( FPTN = FPT@DEVICE,
