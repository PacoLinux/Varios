PL6.E3A0      #001=QBS$STOP_PAUSE File=QBS$STOP_PAUSE.:E05TSI                    WED 07/30/97 04:55 Page:4    
       75    19411    1       INSTR$ = QI_1.INSTR$;
       76    19412    1       QI_1.INSTR$ = ADDR ( INSTR$ -> QI$OP_STOP_PAUSE.NEXT_CODE@P );
       77    19413
       78    19414    1       QI_1.A_REG = 0;
       79    19415
       80    19416    1       IF INSTR$ -> QI$OP_STOP_PAUSE.IS_EXPLICIT_POSTING
       81    19417    1         OR INSTR$ -> QI$OP_STOP_PAUSE.IS_IMPLICIT_POSTING
       82    19418    1       THEN
       83    19419    1   POST_THE_MESSAGE:
       84    19420    2         DO;
       85    19421    2           QI_1.SOURCE$ = ADDR ( INSTR$ -> QI$OP_STOP_PAUSE.POSTING_OPERAND@P );
       86    19422
       87    19423    2           CALL QCS$SETUP_SOURCE;
       88    19424
       89    19425    2           QCM@MESS.MESSAGE_LINE_GZ_HEADER$ = QI_1.MESSAGE_LINE_GZ.HEADER$;
