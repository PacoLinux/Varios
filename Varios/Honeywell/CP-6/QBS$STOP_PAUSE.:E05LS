       92    19428    2           QCM@MESS.VISUAL_ATTRIBUTE = %QI#OUTPUT_VISUAL_ATTRIBUTE;
       93    19429
       94    19430    2           CALL QCM$MESS ( QCM@MESS );
       95    19431
       96    19432    2           IF QCM@MESS.STATUS = %QI#USE_INVALID_RETURN
       97    19433    2           THEN
       98    19434    2             RETURN;
       99    19435    2         END POST_THE_MESSAGE;
      100    19436
      101    19437    1       IF INSTR$ -> QI$OP_STOP_PAUSE.IS_PAUSING
      102    19438    1       THEN
      103    19439    1   PAUSE_THE_TIME:
      104    19440    2         DO;
      105    19441                /*  First, update the screen.  */
      106    19442    2           IF QI_1.SCREEN_UPDATE_FLAGS
      107    19443    2           THEN
      108    19444    2             CALL QCU$UPDATE_SCREEN;
      109    19445                /*  Then, reposition the cursor to column 1.  */
      110    19446    2           FPT@WRITE = QI_FPT_WRITE;
      111    19447    2           VBASE ( FPT@WRITE.V_ ) = ADDR ( FPT@WRITE.V );
PL6.E3A0      #001=QBS$STOP_PAUSE File=QBS$STOP_PAUSE.:E05TSI                    WED 07/30/97 04:55 Page:5    
      112    19448    2           FPT@WRITE.V.DCB# = QI_1.MESSAGE_LINE_GZ.HEADER$ -> QI$GZ_HEADER.DCB#;
      113    19449    2           FPT@WRITE.V.DVBYTE.VFC = %YES;
      114    19450    2           FPT@WRITE.BUF_ = VECTOR ( VFC_OVERPRINT );
      115    19451
      116    19452    2           CALL QGB$WRITE ( FPT@WRITE, QGB_PARAM )
      117    19453    2           WHENALTRETURN
      118    19454    3             DO;
      119    19455    3               CALL QCE$ERROR_M$INTERNAL ( %E$UNEXPECTED_ALTRET );
      120    19456    3             END;
      121    19457
      122    19458                /*  Update the MESSAGE-LINE with any IMPLICIT/EXPLICIT messages
      123    19459                    from above, and with the repositioning information.  */
      124    19460    2           FPT@DEVICE = QI_FPT_DEVICE;
      125    19461    2           VBASE ( FPT@DEVICE.V_ ) = ADDR ( FPT@DEVICE.V );
      126    19462    2           FPT@DEVICE.V.DCB# = QI_1.MESSAGE_LINE_GZ.HEADER$ -> QI$GZ_HEADER.DCB#;
      127    19463
      128    19464    2           CALL QGB$DEVICE ( FPT@DEVICE, QGB_PARAM )
      129    19465    2           WHENALTRETURN
      130    19466    3             DO;
      131    19467    3               CALL QCE$ERROR_M$INTERNAL ( %E$UNEXPECTED_ALTRET );
      132    19468    3             END;
      133    19469
      134    19470                /*  Finally, wait for the PAUSING interval.  */
      135    19471    2           QI_1.SOURCE$ = ADDR ( INSTR$ -> QI$OP_STOP_PAUSE.PAUSING_OPERAND@P );
      136    19472
      137    19473    2           CALL QCS$SETUP_SOURCE;
      138    19474
      139    19475    2           QI_1.OP.SIZE_ERROR = %QI#DONT_CHECK_FOR_SIZE_ERROR;
      140    19476
      141    19477    3           DO CASE ( QI_1.TYPE.S );
      142    19478
      143    19479    3             CASE ( %QI#TYPE_BINARY );
      144    19480    3               QI_1.OP.CIP.T_A (0) = QI_31_DIGIT_ST_PACKED_A (0);
      145    19481    3               QI_1.OP.DEC.T = 0;
      146    19482    3               QI_1.OP.DATA.T$ = ADDR ( QI@OTTER_SPACE );
      147    19483    3               CALL QCC$CBD ( QI_1.OP )
      148    19484    3               WHENALTRETURN
PL6.E3A0      #001=QBS$STOP_PAUSE File=QBS$STOP_PAUSE.:E05TSI                    WED 07/30/97 04:55 Page:6    
      149    19485    4                 DO;
