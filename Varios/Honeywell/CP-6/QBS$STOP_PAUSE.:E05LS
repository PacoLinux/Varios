      151    19487    4                 END;
      152    19488
      153    19489    3               QI_1.OP.CIP.S_A (0) = QI_1.OP.CIP.T_A (0);
      154    19490    3               QI_1.OP.DEC.S = 0;
      155    19491    3               QI_1.OP.DATA.S$ = QI_1.OP.DATA.T$;
      156    19492
