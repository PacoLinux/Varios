%MACRO CD;
/* Cd - control field descriptor structure.                   */
DCL CD$ PTR;
DCL 1 CD BASED (CD$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 SYNC BIT (1) UNAL,            /* 1 = synonym cd                     */
        3 CONT BIT (1) UNAL,            /* 1 = descending                     */
        3 RELT UBIN (4) UNAL,           /* Relationship type                  */
        3 AV BIT (1) UNAL,              /* 1 = virtual cd                     */
        3 * UBIN (11) UNAL,
        3 MDNO UBIN (6) UNAL, /* member duplicate clause # (1st clause=0)     */
      2 RSCDN UBIN (18) UNAL,           /* RS.CD next                         */
      2 RSCDH UBIN (18) UNAL,           /* RS.CD head                         */
      2 CDSH UBIN (18) UNAL,            /* FR.CDS head                        */
      2 CDRH UBIN (18) UNAL,            /* FR.CDR head                        */
      2 CDSN UBIN (18) UNAL,
      2 CDRN UBIN (18) UNAL;
%MEND;

