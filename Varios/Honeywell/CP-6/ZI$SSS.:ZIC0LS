FRI. SEP 05 '97 11:19:04.84  FILE=ZI$SSS

DCL SSS$ PTR;
%MACRO AC;
/*************************************************************/
/* Ac - area class record structure (main).  */
DCL AC$ PTR;
DCL 1 AC BASED (AC$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 FORG UBIN (4) UNAL,           /* 0 = integrated                     */
                                        /* 1 = integrated with key file       */
                                        /* 2 = indexed                        */
                                        /* 3 = indexed with key file          */
        3 ORG REDEF FORG,
          4 * UBIN (2) UNAL,
          4 INDEXED BIT (1) UNAL,
          4 KEYED BIT (1) UNAL,
        3 NCIA BIT (1),
        3 PAIA BIT (1),
        3 ITHR UBIN (9) UNAL,
        3 PMOD BIT (2),
        3 ATMP BIT (1),
        3 ANL UBIN (6) UNAL,
      2 OAACN UBIN (18) UNAL,
      2 ACRAN UBIN (18) UNAL,
      2 ABAS SBIN (36),
      2 WSOF UBIN (18) UNAL,
      2 ACID UBIN (18) UNAL,
      2 PLSEP UBIN (18) UNAL,
      2 BLP UBIN (18) UNAL,
      2 SANAM,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 PGLIM DALIGNED,
        3 LOWPG UBIN(36),
        3 IHIGHPG UBIN(36),             /* Initial value                      */
      2 DBLIM DALIGNED,
        3 LOWDBK UBIN(36),
        3 IHIGHDBK UBIN(36),            /* Initial value                      */
      2 * UBIN (18) UNAL,
      2 LPP UBIN (18) UNAL,
      2 * UBIN (18) UNAL,
      2 INTVL UBIN (18) UNAL,
      2 INVT UBIN (18) UNAL,
      2 PGSZ UBIN (18) UNAL,
      2 BPLDBK UBIN(18) UNAL,
      2 FLAGS,
        3 CHKSUM BIT(1) UNAL,
        3 ENCRP BIT(1) UNAL,
        3 CJRNL BIT(1) UNAL,
        3 TMPLT BIT(1) UNAL,
        3 TJRNL BIT(1) UNAL,
        3 * BIT(13) UNAL,
      2 INCHA UBIN(36),                 /* Initial value                      */
      2 * CHAR (2),
      2 IUSZ UBIN (18) UNAL,
      2 * CHAR (2),
      2 DPPI UBIN (18) UNAL,
      2 IFIPN UBIN(36),                 /* Initial value                      */
      2 CLCN,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 CLCP,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 UFS SBIN (36),
      2 LAP,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 ANAM CHAR(AC.DEF.ANL);
DCL SANAME CHAR(32) BASED;

