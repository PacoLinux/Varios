%MACRO SS;
/* Ss - set selection structure (                             */
DCL SS$ PTR;
DCL 1 SS BASED (SS$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 * UBIN (3) UNAL,
        3 SSTP UBIN (3) UNAL,           /* Set selection type                 */
        3 * UBIN (18) UNAL,
      2 RSCDN UBIN (18) UNAL,           /* RS.CD next                         */
      2 RSSSN UBIN (18) UNAL,           /* RS.SS next                         */
      2 SSH UBIN (18) UNAL,             /* Rs search pointer                  */
      2 RSSSH UBIN (18) UNAL;           /* RS.SS head                         */
%MEND;

