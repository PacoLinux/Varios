%MACRO OA;
/*************************************************************/
/* Oa - object area control structure (main). */
DCL OA$ PTR;
DCL 1 OA BASED (OA$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 STI BIT (1),
        3 * BIT (2),
        3 SSL UBIN (3) UNAL,
      2 SZISS UBIN (18) UNAL,
      2 OAACN UBIN (18) UNAL,
      2 OASCN UBIN (18) UNAL,
      2 OARCN UBIN (18) UNAL,
      2 UWSSZ UBIN (18) UNAL,
      2 CLCSC UBIN (18) UNAL,
      2 CLCRC UBIN (18) UNAL,
      2 SCNAM,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 SSNAM,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 SSDT CHAR(8) UNAL,
      2 SSVDT CHAR(8) UNAL,
      2 NENC UBIN (18) UNAL,
      2 * UBIN (18) UNAL,
      2 ENTBL UBIN (18) UNAL,
      2 * UBIN (18) UNAL,
      2 NDCD UBIN (18) UNAL,
      2 ENV UBIN (4) UNAL,
      2 * UBIN (14) UNAL,
      2 DCTBL UBIN (18) UNAL,
      2 * UBIN (18) UNAL,
      2 AMAX UBIN (18) UNAL,
      2 ACNT UBIN (18) UNAL,
      2 CSZ UBIN (18) UNAL,
      2 LRSZ UBIN (18) UNAL,
      2 UWSZ UBIN (18) UNAL,
      2 LSSZ UBIN (18) UNAL,
      2 FEASZ UBIN(18) UNAL,
      2 FWSSZ UBIN(18) UNAL,
      2 KPASZ UBIN (18) UNAL,
      2 CIWSOF UBIN(18) UNAL, /* work space offset for current of run for index */
      2 LPASZ UBIN(18) UNAL,
      2 * UBIN(18) UNAL,
      2 SCFC CHAR (2),
      2 * UBIN (18) UNAL,
      2 SNAM CHAR (OA.SCNAM.EIS.COUNT);
DCL SSNAME CHAR(32) BASED ALIGNED;

