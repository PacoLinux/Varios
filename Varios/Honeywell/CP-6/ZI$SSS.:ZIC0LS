%MACRO OA1;
/* OA1 - object area control structure (secondary). */
DCL OA1$ PTR;
DCL 1 OA1 BASED (OA1$) ALIGNED,
      2 XXDPN UBIN (18) UNAL,
      2 * UBIN (18) UNAL;
%SUB OA1OFFSET=-1/*OA1OFFSET*/;
%MEND;

