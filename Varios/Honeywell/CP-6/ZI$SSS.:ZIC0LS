%MACRO AG1;
/* AG1 - argument structure (secondary). */
DCL AG1$ PTR;
DCL 1 AG1 BASED (AG1$) ALIGNED,
      2 * UBIN (18) UNAL,
      2 DPAGN UBIN (18) UNAL;           /* Dp - ag next.                      */
%SUB AG1OFFSET=-1/*AG1OFFSET*/;
%MEND;

FILE=ZI$SSS                        AG1
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
                                 0!*                                  !DPAGN                              !W
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO S$CONTROL;
DCL 1 CONTROL BASED ALIGNED,
      2 VALIDATION_STATUS SBIN (36),
/*           30 = successful validation.
             -1 = not successful validation.    */
      2 TRANSLATION_STATUS SBIN (36),
/*           0 = successful translation
             n = # translation errors       */
      2 FILL1 SBIN (36),
      2 TYPE30 SBIN (36),               /* 30                                 */
      2 UWA_SIZE SBIN (36),             /* in words                           */
      2 FILL2 (0:10) SBIN (36),
/*          0:3 used by COBOL-74.
            4:10 unused.                    */
      2 SCHEMA_NAME CHAR (32) UNAL,
      2 SUBSCHEMA_NAME CHAR (32) UNAL,
      2 TYPE100 SBIN (36),
      2 LANGUAGE SBIN (36),
      2 OPTIONS SBIN (36),
      2 COPY_PRIVACY_KEY CHAR (12) UNAL,
      2 DIAGNOSTIC_PRIVACY_KEY CHAR (12) UNAL,
      2 NEXT_UWA_LOC SBIN (36),
      2 SUBSCHEMA_GTIME BIT (72) ALIGNED,
      2 VALIDATION_GTIME BIT (72) ALIGNED,
      2 FILL3 (0:8) SBIN (36),
      2 INVOKE_PRIVACY_LOCK CHAR (12) UNAL,
      2 FILL4 (0:1) SBIN (36),
      2 NEXT_AVAILABLE_RECORD SBIN (36) ALIGNED,
      2 FIRST_CU_RECORD SBIN (36),
      2 FIRST_NF_RECORD SBIN (36),
      2 FIRST_SDS_RECORD SBIN (36),
      2 FIRST_NFE_RECORD SBIN (36),
      2 FIRST_ISS_BLOCK SBIN (36);      /* Word 63 of control segment         */
/*                 Block # of first block in schema subset segment  */
%MEND;

