%MACRO RA;
/* Ra - record in area structure.                               */
DCL RA$ PTR;
DCL 1 RA BASED (RA$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 * UBIN(4) UNAL,
        3 FRANGE BIT(1) UNAL,           /* Full range                         */
        3 * BIT(19) UNAL,
      2 RCRAN UBIN (18) UNAL,           /* Rc-ra next                         */
      2 ACRAN UBIN (18) UNAL,
      2 RCRAH UBIN (18) UNAL,           /* RC.RA head ptr                     */
      2 ACRAH UBIN (18) UNAL,           /* AC.RA head ptr                     */
      2 PGRNG,                          /*  Page range                        */
        3 LOWDBK UBIN(36),
        3 IHIGHDBK UBIN(36),            /* Initial value                      */
        3 RAWSOF REDEF IHIGHDBK UBIN(36), /* AWS offset if FRANGE             */
      2 INCHRS UBIN(36),                /* Initial value                      */
      2 CHRD SBIN (36) UNAL;            /* Displ. Of lo dbk from chr          */
%MEND;

