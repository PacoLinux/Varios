%MACRO DP;
/* Dp - database procedure structure (main).                         */
DCL DP$ PTR;
DCL 1 DP BASED (DP$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 * UBIN (6) UNAL,
        3 CTYP UBIN (3) UNAL,           /* Type of call                       */
        3 DBPT UBIN (15) UNAL,          /* Data base procedure type           */
      2 COFS UBIN (18) UNAL,            /* Ofst to db proc. Type              */
      2 NAGD UBIN (6) UNAL,             /* Number of ag descriptors.          */
      2 CNSZ UBIN (12) UNAL,            /* Size of constant section           */
      2 SECTION (0:0) UBIN;             /* constant section                   */
%MEND;

