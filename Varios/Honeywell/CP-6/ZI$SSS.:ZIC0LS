   325 -         3 * BIT(19) UNAL,
   326 -       2 RCRAN UBIN (18) UNAL,           /* Rc-ra next                         */
   327 -       2 ACRAN UBIN (18) UNAL,
   328 -       2 RCRAH UBIN (18) UNAL,           /* RC.RA head ptr                     */
   329 -       2 ACRAH UBIN (18) UNAL,           /* AC.RA head ptr                     */
   330 -       2 PGRNG,                          /*  Page range                        */
   331 -         3 LOWDBK UBIN(36),
   332 -         3 IHIGHDBK UBIN(36),            /* Initial value                      */
   333 -         3 RAWSOF REDEF IHIGHDBK UBIN(36), /* AWS offset if FRANGE             */
   334 -       2 INCHRS UBIN(36),                /* Initial value                      */
   335 -       2 CHRD SBIN (36) UNAL;            /* Displ. Of lo dbk from chr          */
   336 - %MEND;
   337 - %MACRO RS;
   338 - /* Rs structure - for set owner (so) & set member (sm) elements */
   339 - DCL RS$ PTR;
   340 - DCL 1 RS BASED (RS$) ALIGNED,
   341 -       2 DEF,
   342 -         3 TYPE BIT(12) UNAL,
   343 -                                    /* set owner element (so) = octal 0100     */
   344 -                                    /* set member element (sm) = octal 0040    */
   345 -         3 VIA BIT (1) UNAL,             /*  1 = this is loc mode set          */
   346 -         3 * UBIN (2) UNAL,
   347 -         3 MEMT UBIN (3) UNAL,           /*  Member type mask  ( sm only )     */
   348 -                                         /* 0 = member of chain                */
   349 -                                         /* 1 = pa member                      */
   350 -                                         /* 2 = pa record                      */
   351 -         3 DORD UBIN (2) UNAL,           /*  Duplicate order                   */
   352 -         3 OSM BIT (1) UNAL,             /*  1 = optional set member           */
   353 -         3 MSM BIT (1) UNAL,             /*  1 = manual set member             */
   354 -         3 SMOD BIT (1) UNAL,       /* 1 = this set membership cannot be mod.  */
   355 -         3 DUPP BIT (1) UNAL,            /* 1 = dup keys present               */
   356 -         3 RNGK BIT (1) UNAL,
   357 -         3 DUPC BIT (1) UNAL,            /* 1 = dups not allowed for calc      */
   358 -         3 IXNUM UBIN(10) UNAL,          /* Alternate index # within record    */
   359 -       2 RSCDN UBIN (18) UNAL,           /* RS.CD next                         */
   360 -       2 RSSSN UBIN (18) UNAL,           /* RS.SS next                         */
   361 -       2 SCRSN UBIN (18) UNAL,
   362 -       2 RCRSN UBIN (18) UNAL,           /* Rc-rs next                         */
   363 -       2 RCRSH UBIN (18) UNAL,           /* RC.RS set head ptr                 */
   364 -       2 SCRSH UBIN (18) UNAL,           /* SC.RS set head ptr                 */
   365 -       2 NPOS,                           /* Pos. Of next ptr in record         */
   366 -         3 EIS,                          /* Eis descriptor                     */
   367 -           4 REG UBIN (3) UNAL,
   368 -           4 OFFSET UNAL,
   369 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   370 -             5 CHR UBIN (3) UNAL,
   371 -           4 TA UBIN (2) UNAL,
   372 -           4 * BIT (1),
   373 -           4 COUNT UBIN (12) UNAL,
   374 -       2 CSPAH REDEF NPOS,               /* Pos of pa owner ptr in pa mem rec  */
   375 -         3 EIS,                          /* Eis descriptor                     */
   376 -           4 REG UBIN (3) UNAL,
   377 -           4 OFFSET UNAL,
   378 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   379 -             5 CHR UBIN (3) UNAL,
   380 -           4 TA UBIN (2) UNAL,
   381 -           4 * BIT (1),
   382 -           4 COUNT UBIN (12) UNAL,
   383 -       2 PPOS,                           /* Pos. Of prior ptr in record        */
   384 -         3 EIS,                          /* Eis descriptor                     */
   385 -           4 REG UBIN (3) UNAL,
   386 -           4 OFFSET UNAL,
   387 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   388 -             5 CHR UBIN (3) UNAL,
   389 -           4 TA UBIN (2) UNAL,
   390 -           4 * BIT (1),
   391 -           4 COUNT UBIN (12) UNAL,
   392 -       2 HPOS,                           /* Pos. Of head ptr in record         */
   393 -         3 EIS,                          /* Eis descriptor                     */
   394 -           4 REG UBIN (3) UNAL,
   395 -           4 OFFSET UNAL,
   396 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   397 -             5 CHR UBIN (3) UNAL,
   398 -           4 TA UBIN (2) UNAL,
   399 -           4 * BIT (1),
   400 -           4 COUNT UBIN (12) UNAL,
   401 -       2 CSMPP,                          /* Pos of member ptr in pa rec        */
   402 -         3 EIS,                          /* Eis descriptor                     */
   403 -           4 REG UBIN (3) UNAL,
   404 -           4 OFFSET UNAL,
   405 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   406 -             5 CHR UBIN (3) UNAL,
   407 -           4 TA UBIN (2) UNAL,
   408 -           4 * BIT (1),
   409 -           4 COUNT UBIN (12) UNAL;
   410 - %SUB RS_CLASS_MEMBER='0040'O /* RS_CLASS_MEMBER */;
   411 - %SUB RS_CLASS_OWNER='0100'O /* RS_CLASS_OWNER */;
   412 - %MEND;
   413 - %MACRO SM1;
   414 - /* SM1 - set member (rs) structure (secondary). */
   415 - DCL SM1$ PTR;
   416 - DCL 1 SM1 BASED (SM1$) ALIGNED,
   417 -       2 XXDPN UBIN (18) UNAL,
   418 -       2 * UBIN (18) UNAL;
   419 - %SUB SM1OFFSET=-1/*SM1OFFSET*/;
   420 - %MEND;
   421 - %MACRO SS;
   422 - /* Ss - set selection structure (                             */
   423 - DCL SS$ PTR;
   424 - DCL 1 SS BASED (SS$) ALIGNED,
   425 -       2 DEF,
   426 -         3 TYPE BIT(12) UNAL,
   427 -         3 * UBIN (3) UNAL,
   428 -         3 SSTP UBIN (3) UNAL,           /* Set selection type                 */
   429 -         3 * UBIN (18) UNAL,
   430 -       2 RSCDN UBIN (18) UNAL,           /* RS.CD next                         */
   431 -       2 RSSSN UBIN (18) UNAL,           /* RS.SS next                         */
   432 -       2 SSH UBIN (18) UNAL,             /* Rs search pointer                  */
   433 -       2 RSSSH UBIN (18) UNAL;           /* RS.SS head                         */
   434 - %MEND;
   435 - %MACRO CD;
   436 - /* Cd - control field descriptor structure.                   */
   437 - DCL CD$ PTR;
   438 - DCL 1 CD BASED (CD$) ALIGNED,
   439 -       2 DEF,
   440 -         3 TYPE BIT(12) UNAL,
   441 -         3 SYNC BIT (1) UNAL,            /* 1 = synonym cd                     */
   442 -         3 CONT BIT (1) UNAL,            /* 1 = descending                     */
   443 -         3 RELT UBIN (4) UNAL,           /* Relationship type                  */
   444 -         3 AV BIT (1) UNAL,              /* 1 = virtual cd                     */
   445 -         3 * UBIN (11) UNAL,
   446 -         3 MDNO UBIN (6) UNAL, /* member duplicate clause # (1st clause=0)     */
   447 -       2 RSCDN UBIN (18) UNAL,           /* RS.CD next                         */
   448 -       2 RSCDH UBIN (18) UNAL,           /* RS.CD head                         */
   449 -       2 CDSH UBIN (18) UNAL,            /* FR.CDS head                        */
   450 -       2 CDRH UBIN (18) UNAL,            /* FR.CDR head                        */
   451 -       2 CDSN UBIN (18) UNAL,
   452 -       2 CDRN UBIN (18) UNAL;
   453 - %MEND;
   454 - %MACRO DP;
   455 - /* Dp - database procedure structure (main).                         */
   456 - DCL DP$ PTR;
   457 - DCL 1 DP BASED (DP$) ALIGNED,
   458 -       2 DEF,
   459 -         3 TYPE BIT(12) UNAL,
   460 -         3 * UBIN (6) UNAL,
   461 -         3 CTYP UBIN (3) UNAL,           /* Type of call                       */
   462 -         3 DBPT UBIN (15) UNAL,          /* Data base procedure type           */
   463 -       2 COFS UBIN (18) UNAL,            /* Ofst to db proc. Type              */
   464 -       2 NAGD UBIN (6) UNAL,             /* Number of ag descriptors.          */
   465 -       2 CNSZ UBIN (12) UNAL,            /* Size of constant section           */
   466 -       2 SECTION (0:0) UBIN;             /* constant section                   */
   467 - %MEND;
   468 - %MACRO DP1;
   469 - /* DP1 - database procedure structure (secondary). */
   470 - DCL DP1$ PTR;
   471 - DCL 1 DP1 BASED (DP1$) ALIGNED,
   472 -       2 XXDPN UBIN (18) UNAL,
   473 -       2 DPAGN UBIN (18) UNAL;           /* Dp - ag next.                      */
   474 - %SUB DP1OFFSET=-1/*DP1OFFSET*/;
   475 - %MEND;
   476 - %MACRO AG;
   477 - /* Ag - argument structure (main).                                   */
   478 - DCL AG$ PTR;
   479 - DCL 1 AG BASED (AG$) ALIGNED,
   480 -       2 DEF,
   481 -         3 TYPE BIT(12) UNAL,
   482 -         3 ENDX UBIN (6) UNAL,           /*  Index into encode vector          */
   483 -         3 DPST BIT (1) UNAL,            /* 1 = this is status arg             */
   484 -         3 DPSA BIT (1) UNAL,            /* 1 = this is pseudo arg(nu by dbcs) */
   485 -         3 * UBIN (10) UNAL,
   486 -         3 DNDX UBIN (6) UNAL,           /*  Index into decode vector          */
   487 -       2 ROFST,                          /*  Linkage section eis descriptor    */
   488 -         3 EIS,                          /* Eis descriptor                     */
   489 -           4 REG UBIN (3) UNAL,
   490 -           4 OFFSET UNAL,
   491 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   492 -             5 CHR UBIN (3) UNAL,
   493 -           4 TA UBIN (2) UNAL,
   494 -           4 * BIT (1),
   495 -           4 COUNT UBIN (12) UNAL,
   496 -       2 UOFST,                          /*  Argument source eis descriptor    */
   497 -         3 EIS,                          /* Eis descriptor                     */
   498 -           4 REG UBIN (3) UNAL,
   499 -           4 OFFSET UNAL,
   500 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
   501 -             5 CHR UBIN (3) UNAL,
   502 -           4 TA UBIN (2) UNAL,
   503 -           4 * BIT (1),
   504 -           4 COUNT UBIN (12) UNAL;
   505 - %MEND;
   506 - %MACRO AG1;
   507 - /* AG1 - argument structure (secondary). */
   508 - DCL AG1$ PTR;
   509 - DCL 1 AG1 BASED (AG1$) ALIGNED,
   510 -       2 * UBIN (18) UNAL,
   511 -       2 DPAGN UBIN (18) UNAL;           /* Dp - ag next.                      */
   512 - %SUB AG1OFFSET=-1/*AG1OFFSET*/;
   513 - %MEND;
   514 - %MACRO S$CONTROL;
   515 - DCL 1 CONTROL BASED ALIGNED,
   516 -       2 VALIDATION_STATUS SBIN (36),
   517 - /*           30 = successful validation.
   518 -              -1 = not successful validation.    */
   519 -       2 TRANSLATION_STATUS SBIN (36),
   520 - /*           0 = successful translation
   521 -              n = # translation errors       */
   522 -       2 FILL1 SBIN (36),
   523 -       2 TYPE30 SBIN (36),               /* 30                                 */
   524 -       2 UWA_SIZE SBIN (36),             /* in words                           */
   525 -       2 FILL2 (0:10) SBIN (36),
   526 - /*          0:3 used by COBOL-74.
   527 -             4:10 unused.                    */
   528 -       2 SCHEMA_NAME CHAR (32) UNAL,
   529 -       2 SUBSCHEMA_NAME CHAR (32) UNAL,
   530 -       2 TYPE100 SBIN (36),
   531 -       2 LANGUAGE SBIN (36),
   532 -       2 OPTIONS SBIN (36),
   533 -       2 COPY_PRIVACY_KEY CHAR (12) UNAL,
   534 -       2 DIAGNOSTIC_PRIVACY_KEY CHAR (12) UNAL,
   535 -       2 NEXT_UWA_LOC SBIN (36),
   536 -       2 SUBSCHEMA_GTIME BIT (72) ALIGNED,
   537 -       2 VALIDATION_GTIME BIT (72) ALIGNED,
   538 -       2 FILL3 (0:8) SBIN (36),
   539 -       2 INVOKE_PRIVACY_LOCK CHAR (12) UNAL,
   540 -       2 FILL4 (0:1) SBIN (36),
   541 -       2 NEXT_AVAILABLE_RECORD SBIN (36) ALIGNED,
   542 -       2 FIRST_CU_RECORD SBIN (36),
   543 -       2 FIRST_NF_RECORD SBIN (36),
   544 -       2 FIRST_SDS_RECORD SBIN (36),
   545 -       2 FIRST_NFE_RECORD SBIN (36),
   546 -       2 FIRST_ISS_BLOCK SBIN (36);      /* Word 63 of control segment         */
   547 - /*                 Block # of first block in schema subset segment  */
   548 - %MEND;
