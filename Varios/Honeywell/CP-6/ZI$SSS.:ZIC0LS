     7 - DCL SSS$ PTR;
     8 - %MACRO AC;
     9 - /*************************************************************/
    10 - /* Ac - area class record structure (main).  */
    11 - DCL AC$ PTR;
    12 - DCL 1 AC BASED (AC$) ALIGNED,
    13 -       2 DEF,
    14 -         3 TYPE BIT(12) UNAL,
    15 -         3 FORG UBIN (4) UNAL,           /* 0 = integrated                     */
    16 -                                         /* 1 = integrated with key file       */
    17 -                                         /* 2 = indexed                        */
    18 -                                         /* 3 = indexed with key file          */
    19 -         3 ORG REDEF FORG,
    20 -           4 * UBIN (2) UNAL,
    21 -           4 INDEXED BIT (1) UNAL,
    22 -           4 KEYED BIT (1) UNAL,
    23 -         3 NCIA BIT (1),
    24 -         3 PAIA BIT (1),
    25 -         3 ITHR UBIN (9) UNAL,
    26 -         3 PMOD BIT (2),
    27 -         3 ATMP BIT (1),
    28 -         3 ANL UBIN (6) UNAL,
    29 -       2 OAACN UBIN (18) UNAL,
    30 -       2 ACRAN UBIN (18) UNAL,
    31 -       2 ABAS SBIN (36),
    32 -       2 WSOF UBIN (18) UNAL,
    33 -       2 ACID UBIN (18) UNAL,
    34 -       2 PLSEP UBIN (18) UNAL,
    35 -       2 BLP UBIN (18) UNAL,
    36 -       2 SANAM,
    37 -         3 EIS,                          /* Eis descriptor                     */
    38 -           4 REG UBIN (3) UNAL,
    39 -           4 OFFSET UNAL,
    40 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
    41 -             5 CHR UBIN (3) UNAL,
    42 -           4 TA UBIN (2) UNAL,
    43 -           4 * BIT (1),
    44 -           4 COUNT UBIN (12) UNAL,
    45 -       2 PGLIM DALIGNED,
    46 -         3 LOWPG UBIN(36),
    47 -         3 IHIGHPG UBIN(36),             /* Initial value                      */
    48 -       2 DBLIM DALIGNED,
    49 -         3 LOWDBK UBIN(36),
    50 -         3 IHIGHDBK UBIN(36),            /* Initial value                      */
    51 -       2 * UBIN (18) UNAL,
    52 -       2 LPP UBIN (18) UNAL,
    53 -       2 * UBIN (18) UNAL,
    54 -       2 INTVL UBIN (18) UNAL,
    55 -       2 INVT UBIN (18) UNAL,
    56 -       2 PGSZ UBIN (18) UNAL,
    57 -       2 BPLDBK UBIN(18) UNAL,
    58 -       2 FLAGS,
    59 -         3 CHKSUM BIT(1) UNAL,
    60 -         3 ENCRP BIT(1) UNAL,
    61 -         3 CJRNL BIT(1) UNAL,
    62 -         3 TMPLT BIT(1) UNAL,
    63 -         3 TJRNL BIT(1) UNAL,
    64 -         3 * BIT(13) UNAL,
    65 -       2 INCHA UBIN(36),                 /* Initial value                      */
    66 -       2 * CHAR (2),
    67 -       2 IUSZ UBIN (18) UNAL,
    68 -       2 * CHAR (2),
    69 -       2 DPPI UBIN (18) UNAL,
    70 -       2 IFIPN UBIN(36),                 /* Initial value                      */
    71 -       2 CLCN,
    72 -         3 EIS,                          /* Eis descriptor                     */
    73 -           4 REG UBIN (3) UNAL,
    74 -           4 OFFSET UNAL,
    75 -             5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
    76 -             5 CHR UBIN (3) UNAL,
    77 -           4 TA UBIN (2) UNAL,
    78 -           4 * BIT (1),
    79 -           4 COUNT UBIN (12) UNAL,
    80 -       2 CLCP,
    81 -         3 EIS,                          /* Eis descriptor                     */
