%MACRO RS;
/* Rs structure - for set owner (so) & set member (sm) elements */
DCL RS$ PTR;
DCL 1 RS BASED (RS$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
                                   /* set owner element (so) = octal 0100     */
                                   /* set member element (sm) = octal 0040    */
        3 VIA BIT (1) UNAL,             /*  1 = this is loc mode set          */
        3 * UBIN (2) UNAL,
        3 MEMT UBIN (3) UNAL,           /*  Member type mask  ( sm only )     */
                                        /* 0 = member of chain                */
                                        /* 1 = pa member                      */
                                        /* 2 = pa record                      */
        3 DORD UBIN (2) UNAL,           /*  Duplicate order                   */
        3 OSM BIT (1) UNAL,             /*  1 = optional set member           */
        3 MSM BIT (1) UNAL,             /*  1 = manual set member             */
        3 SMOD BIT (1) UNAL,       /* 1 = this set membership cannot be mod.  */
        3 DUPP BIT (1) UNAL,            /* 1 = dup keys present               */
        3 RNGK BIT (1) UNAL,
        3 DUPC BIT (1) UNAL,            /* 1 = dups not allowed for calc      */
        3 IXNUM UBIN(10) UNAL,          /* Alternate index # within record    */
      2 RSCDN UBIN (18) UNAL,           /* RS.CD next                         */
      2 RSSSN UBIN (18) UNAL,           /* RS.SS next                         */
      2 SCRSN UBIN (18) UNAL,
      2 RCRSN UBIN (18) UNAL,           /* Rc-rs next                         */
      2 RCRSH UBIN (18) UNAL,           /* RC.RS set head ptr                 */
      2 SCRSH UBIN (18) UNAL,           /* SC.RS set head ptr                 */
      2 NPOS,                           /* Pos. Of next ptr in record         */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 CSPAH REDEF NPOS,               /* Pos of pa owner ptr in pa mem rec  */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 PPOS,                           /* Pos. Of prior ptr in record        */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 HPOS,                           /* Pos. Of head ptr in record         */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 CSMPP,                          /* Pos of member ptr in pa rec        */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL;
%SUB RS_CLASS_MEMBER='0040'O /* RS_CLASS_MEMBER */;
%SUB RS_CLASS_OWNER='0100'O /* RS_CLASS_OWNER */;
%MEND;

