%MACRO FR1;
/* FR1 - field in record structure (secondary). */
DCL FR1$ PTR;
DCL 1 FR1 BASED (FR1$) ALIGNED,
      2 XXDPN UBIN (18) UNAL,
      2 NLIND UBIN (18) UNAL;
%SUB FR1OFFSET=-1/*FR1OFFSET*/;
%MEND;

