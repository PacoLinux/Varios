DCL SANAME CHAR(32) BASED;
%SUB AC_CLASS='2000'O /* AC_CLASS */;
%MEND;
%MACRO AC1;
/* AC1 - area class record structure (secondary). */
DCL AC1$ PTR;
DCL 1 AC1 BASED (AC1$) ALIGNED,
      2 XXDPN UBIN (18) UNAL,
      2 IXPLL UBIN (18) UNAL;
%SUB AC1OFFSET=-1/*AC1OFFSET*/;
%MEND;

FILE=ZI$SSS                        AC1
