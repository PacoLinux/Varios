%MACRO SC;
/*************************************************************/
/* Sc - set class structure (main).        */
DCL SC$ PTR;
DCL 1 SC BASED (SC$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 SETM UBIN (3) UNAL,
        3 * UBIN (3) UNAL,
        3 SETO UBIN (4) UNAL,
        3 SNSS BIT (1),
        3 * BIT (1),
        3 SRCR BIT (1),
        3 ALTO BIT (1),
        3 LG BIT (1),
        3 SETT UBIN (3) UNAL,
        3 SNL UBIN (6) UNAL,
      2 SCWSO UBIN (18) UNAL,
      2 OASCN UBIN (18) UNAL,
      2 SCRSN UBIN (18) UNAL,
      2 SCID UBIN (18) UNAL,
      2 SNAM CHAR(SC.DEF.SNL);
%SUB SC_CLASS='0400'O /* SC_CLASS */;
%SUB PRIMARY_KEY=2 /* PRIMARY_KEY */;
%SUB SECONDARY_KEY=3 /* SECONDARY_KEY */;
%SUB CALC_SET=1 /* CALC_SET */;
%MEND;

