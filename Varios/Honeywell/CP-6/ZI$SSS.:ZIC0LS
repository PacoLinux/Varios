    88 -           4 COUNT UBIN (12) UNAL,
    89 -       2 UFS SBIN (36),
    90 -       2 LAP,
    91 -         3 EIS,                          /* Eis descriptor                     */
    92 -           4 REG UBIN (3) UNAL,
    93 -           4 OFFSET UNAL,
