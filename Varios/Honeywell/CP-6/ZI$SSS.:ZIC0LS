%MACRO RC;
/*************************************************************/
/* Rc - record class structure (main). */
DCL RC$ PTR;
DCL 1 RC BASED (RC$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 * BIT (3),
        3 PSTO UBIN (3) UNAL,
        3 RERS BIT (1),
        3 RSTO BIT (1),
        3 SR BIT (1),
        3 FVR BIT (1),
        3 MAR BIT (1),
        3 RKEY BIT (1),
        3 RERA BIT (1),
        3 DIDP BIT (1),
        3 CSPAC BIT (1),
        3 * BIT (3),
        3 RNL UBIN (6) UNAL,
      2 RCRAN UBIN (18) UNAL,           /* Rc-ra next                         */
      2 RCID UBIN (18) UNAL,            /* Record id # for binding            */
      2 OARCN UBIN (18) UNAL,           /* Oa-rc next                         */
      2 RCRSN UBIN (18) UNAL,           /* Rc-rs next                         */
      2 RSIZ UBIN (18) UNAL,            /* Record size (bytes)                */
      2 RCON UBIN (6) UNAL,             /* Constant = 20                      */
      2 RTYP UBIN (12) UNAL,            /* Record type                        */
      2 RCWSO UBIN (18) UNAL,           /* Work space offset                  */
      2 RCFRN UBIN (18) UNAL,           /* Rc-fr next                         */
      2 RNAM CHAR (RC.DEF.RNL);
%SUB RC_CLASS='1000'O /* RC_CLASS */;
%MEND;

