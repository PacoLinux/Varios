%MACRO RC1;
/* RC1 - record class structure (secondary). */
DCL RC1$ PTR;
DCL 1 RC1 BASED (RC1$) ALIGNED,
      2 CSPSB UBIN (18) UNAL,
      2 CSPES UBIN (18) UNAL,
      2 XXDPN UBIN (18) UNAL,
      2 OTVL UBIN (18) UNAL;            /* Offset to variable length data     */
%SUB RC1OFFSET=-2/*RC1OFFSET*/;
%MEND;

