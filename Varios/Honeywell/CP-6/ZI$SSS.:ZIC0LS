%MACRO SC1;
/* SC1 - set class structure (secondary). */
DCL SC1$ PTR;
DCL 1 SC1 BASED (SC1$) ALIGNED,
      2 IXID UBIN (18) UNAL,
      2 KLNG UBIN (18) UNAL,
      2 XXDPN UBIN (18) UNAL,
      2 * UBIN (18) UNAL;
%SUB SC1OFFSET=-2/*SC1OFFSET*/;
%MEND;

