                                 0!XXDPN                              !IXPLL                              !W
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO FR;
/************************************************************/
/* Fr - field in record structure (main).         */
DCL FR$ PTR;
DCL 1 FR BASED (FR$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 ENDX UBIN (6) UNAL,
        3 EBCM BIT (1),
        3 EBCL BIT (1),
        3 COMC UBIN (3) UNAL,
        3 FVF BIT (1),
        3 SYN BIT (1),
        3 FUSE UBIN (3) UNAL,
        3 FMOD BIT (1),
        3 FSR BIT (1),
        3 DNDX UBIN (6) UNAL,
      2 ROFST,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 UOFST,
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 FRID REDEF OFFSET UBIN(18) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 CDSN UBIN (18) UNAL,
      2 CDRN UBIN (18) UNAL,
      2 * UBIN (12) UNAL,
      2 FNL UBIN (6) UNAL,
      2 RCFRN UBIN (18) UNAL,
      2 RCFRH UBIN (18) UNAL,
      2 FRWSO UBIN (18) UNAL,
      2 FNAM CHAR (FR.FNL);
%SUB FR_CLASS='0020'O /* FR_CLASS */;
%MEND;

