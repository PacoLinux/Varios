%MACRO AG;
/* Ag - argument structure (main).                                   */
DCL AG$ PTR;
DCL 1 AG BASED (AG$) ALIGNED,
      2 DEF,
        3 TYPE BIT(12) UNAL,
        3 ENDX UBIN (6) UNAL,           /*  Index into encode vector          */
        3 DPST BIT (1) UNAL,            /* 1 = this is status arg             */
        3 DPSA BIT (1) UNAL,            /* 1 = this is pseudo arg(nu by dbcs) */
        3 * UBIN (10) UNAL,
        3 DNDX UBIN (6) UNAL,           /*  Index into decode vector          */
      2 ROFST,                          /*  Linkage section eis descriptor    */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL,
      2 UOFST,                          /*  Argument source eis descriptor    */
        3 EIS,                          /* Eis descriptor                     */
          4 REG UBIN (3) UNAL,
          4 OFFSET UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
          4 TA UBIN (2) UNAL,
          4 * BIT (1),
          4 COUNT UBIN (12) UNAL;
%MEND;

