    8      struct field {  /* for field specifiers */
    9       struct field *next;
   10       arith fd_mask;
   11       int fd_shift;
   12       unsigned fd_width; /* unsigned because intg overflow in add_field */
   13       struct sdef *fd_sdef;   /* upward pointer */
   14      };
