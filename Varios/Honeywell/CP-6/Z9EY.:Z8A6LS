     1236    13253
     1237    13254    1   DCL Z8AT$CREATE_TEMPORARY       ENTRY(2);
     1238    13255
     1239    13256
     1240    13257    1      LIST$ = O1COM.BASE_LTS_LIST$;
     1241    13258    1      SIZE = 0;
     1242    13259    1      SEQ# = 0;
     1243    13260    1      ARG_LIST$ = O1COM.CURRENT_PROC$ -> PROC_.PROC_DEF$ -> SYMDEF_.ARGLIST$;
     1244    13261
     1245    13262               /* reverse the sequence of the BASE_LTS list */
     1246    13263
     1247    13264    1      NEXT$ = LIST$ -> BASE_LTS.NEXT;
     1248    13265    1      TEMP$ = ADDR(NIL);
     1249    13266
     1250    13267    2      DO WHILE(NEXT$ ~= ADDR(NIL));
     1251    13268    2        LIST$ -> BASE_LTS.NEXT = TEMP$;
     1252    13269    2        TEMP$ = LIST$;
     1253    13270    2        LIST$ = NEXT$;
     1254    13271    2        NEXT$ = NEXT$ -> BASE_LTS.NEXT;
     1255    13272    2      END;
     1256    13273
     1257    13274    1      LIST$ -> BASE_LTS.NEXT = TEMP$;
     1258    13275
     1259    13276             /* chain all the BASE_LTSs  which are not associated with  */
     1260    13277             /*  redefines subject items                                */
     1261    13278
     1262    13279    1      I = 0;
     1263    13280    1      LIST2$ = ADDR(NIL);
     1264    13281    1      LIST2_TAIL$ = ADDR(NIL);
     1265    13282
     1266    13283    1      IF ARG_LIST$ ~= ADDR(NIL) THEN
     1267    13284    2        DO;
     1268    13285    3          DO I = 0 TO ARG_LIST$ -> ARGLIST.N - 2;
     1269    13286    3            IF ARG_LIST$ -> ARGLIST.ARG$(I) -> SCALAR.B.LINKAGE THEN
     1270    13287    4              DO;
     1271    13288                     %GET_LTS (@ID         = %BASE_LTS,
     1272    13289                               @ST$        = "ARG_LIST$ -> ARGLIST.ARG$(I)",
PL6.E3A0      #003=Z9EY$CREATE_BASE_POINTERS File=Z9EY.:Z8A6TSI                  SAT 08/09/97 14:01 Page:207  
     1273    13290                               @RETURN$    = LTS$$);
     1274    13333
     1275    13334    4                IF LTS$$ = ADDR(NIL) THEN
     1276    13335    5                  DO;            /* this is not a level 01 item */
     1277    13336    5                    QLT$$ = ARG_LIST$ -> ARGLIST.ARG$(I) -> SCALAR.QLT$;
     1278    13337                                      /* get the level 01 item */
     1279    13338    6                    DO WHILE (QLT$$ -> QLT.FATHER ~= ADDR(NIL));
     1280    13339    6                      QLT$$ = QLT$$ -> QLT.FATHER;
     1281    13340    6                    END; /* DO WHILE */
     1282    13341
     1283    13342                         %GET_LTS (@ID     = %BASE_LTS,
     1284    13343                                   @ST$    = QLT$$ -> QLT.ST$,
     1285    13344                                   @RETURN$ = LTS$$);
     1286    13387    5                  END;
     1287    13388
     1288    13389                         /* if the argument is a redefines subject,  */
     1289    13390                         /* find the redefines object                */
     1290    13391
     1291    13392    5                DO WHILE (LTS$$ -> BASE_LTS.REDEFINE$ ~= ADDR(NIL));
     1292    13393
     1293    13394                       %GET_LTS (@ID = %BASE_LTS,
     1294    13395                                 @ST$ = LTS$$ -> BASE_LTS.REDEFINE$,
     1295    13396                                 @RETURN$ = LTS$$);
     1296    13439
     1297    13440    5                END;  /* DO WHILE */
     1298    13441
     1299    13442    4                IF LTS$$ -> BASE_LTS.SEQ# ~= 0 THEN
     1300    13443    4                  GOTO NEXT_ARGUMENT;    /* duplicated argument */
     1301    13444
     1302    13445                       /* assign the sequence number and chained to a list */
     1303    13446
     1304    13447    4                LTS$$ -> BASE_LTS.SEQ# = I + 1;
     1305    13448
     1306    13449    4                IF LIST2$ = ADDR(NIL) THEN
     1307    13450    5                  DO;
     1308    13451    5                    LIST2$ = LTS$$;
     1309    13452    5                    LIST2_TAIL$ = LTS$$;
PL6.E3A0      #003=Z9EY$CREATE_BASE_POINTERS File=Z9EY.:Z8A6TSI                  SAT 08/09/97 14:01 Page:208  
     1310    13453    5                  END;
     1311    13454    4                ELSE
     1312    13455    5                  DO;
     1313    13456    5                    LIST2_TAIL$ -> BASE_LTS.LINK = LTS$$;
     1314    13457    5                    LIST2_TAIL$ = LTS$$;
     1315    13458    5                  END;
     1316    13459
     1317    13460    4              END;   /* IF ..LINKAGE THEN DO */
     1318    13461
     1319    13462    3            NEXT_ARGUMENT:;
     1320    13463    3          END; /* DO I= .... */
     1321    13464
     1322    13465    2        END;  /* IF ARG_LIST$ ^= ADDR(NIL) THEN DO .. */
     1323    13466
     1324    13467             /* chain those BASE_LTSs that are not associated with arguments */
     1325    13468
     1326    13469    1      NEXT$ = LIST$;
     1327    13470
     1328    13471    2      DO WHILE (NEXT$ ~= ADDR(NIL));
     1329    13472
     1330    13473    2        IF NEXT$ -> BASE_LTS.SEQ# = 0 AND
     1331    13474    2          NEXT$ -> BASE_LTS.REDEFINE$ = ADDR(NIL) THEN
     1332    13475    3          DO;
     1333    13476    3            IF LIST2$ = ADDR(NIL) THEN
     1334    13477    4              DO;
     1335    13478    4                LIST2$ = NEXT$;
     1336    13479    4                LIST2_TAIL$ = NEXT$;
     1337    13480    4              END;
     1338    13481    3            ELSE
     1339    13482    4              DO;
     1340    13483    4                LIST2_TAIL$ -> BASE_LTS.LINK = NEXT$;
     1341    13484    4                LIST2_TAIL$ = NEXT$;
     1342    13485    4              END;
     1343    13486    3          END;
     1344    13487
     1345    13488    2        NEXT$ = NEXT$ -> BASE_LTS.NEXT;
     1346    13489
PL6.E3A0      #003=Z9EY$CREATE_BASE_POINTERS File=Z9EY.:Z8A6TSI                  SAT 08/09/97 14:01 Page:209  
     1347    13490    2      END; /* DO WHILE */
     1348    13491
     1349    13492             /* Create a one word pointer item for every based variable.*/
     1350    13493             /* For those redefines subject, copy the pointer from it's */
     1351    13494             /* redefines object. The base pointer is a level 2 item    */
     1352    13495             /* which ise subordinate to the pseudostack.               */
     1353    13496
     1354    13497    1      COMCOM_QLT$ = COMCOM.PREV_QLT$;  /* save away previous QLT$ */
     1355    13498    1      COMCOM.PREV_QLT$ = ADDR(NIL);    /* see Z8AT$CREATE_TEMPORARY    */
     1356    13499    1      STACK$ = O1COM.CURRENT_PROC$ -> PROC_.PSEUDO_STACK$ -> ETT.LOP;
     1357    13500
     1358    13501               /* At first, create a one word dummy for return address */
     1359    13502               /* which is needed in CP-6 environment                  */
     1360    13503
     1361    13504    1      CALL Z8AT$CREATE_TEMPORARY (%SCALAR, TEMP$);
     1362    13505
     1363    13506    1      COMCOM.PREV_QLT$ -> QLT.LEVEL_NO = 2;
     1364    13507    1      COMCOM.PREV_QLT$ -> QLT.FATHER = STACK$ -> SCALAR.QLT$;
     1365    13508    1      STACK$ -> SCALAR.QLT$ -> QLT.SON = COMCOM.PREV_QLT$;
     1366    13509    1      STACK$ -> SCALAR.B.ELEMENTARY = FALSE;
     1367    13510
     1368    13511    1      SIZE = 4;       /* initial pseudo stack size in bytes */
     1369    13512    1      SEQ# = 0;       /* reassign the sequence number start with 0 */
     1370    13513    1      NEXT$ = LIST2$;   /* prime list without redefines  */
     1371    13514
     1372    13515    2      DO WHILE (NEXT$ ~= ADDR(NIL));
     1373    13516
     1374    13517    2        SIZE = SIZE + 4;
     1375    13518    2        SEQ# = SEQ# + 1;
     1376    13519    2        NEXT$ -> BASE_LTS.SEQ# = SEQ#;
     1377    13520
     1378    13521    2        CALL Z8AT$CREATE_TEMPORARY (%SCALAR, NEXT$ -> BASE_LTS.POINTER$);
     1379    13522
     1380    13523    2        NEXT$ -> BASE_LTS.POINTER$ -> ST.TYPE = %TYP;
     1381    13524    2        NEXT$ -> BASE_LTS.POINTER$ -> SCALAR.B.CLASS = %CLASS_UNDEFINED;
     1382    13525    2        NEXT$ -> BASE_LTS.POINTER$ -> SCALAR.SA = SEQ#;
     1383    13526    2        NEXT$ -> BASE_LTS.POINTER$ -> SCALAR.LTS$ = NEXT$;
PL6.E3A0      #003=Z9EY$CREATE_BASE_POINTERS File=Z9EY.:Z8A6TSI                  SAT 08/09/97 14:01 Page:210  
     1384    13527    2        COMCOM.PREV_QLT$ -> QLT.LEVEL_NO = 2;
     1385    13528    2        COMCOM.PREV_QLT$ -> QLT.FATHER = STACK$ -> SCALAR.QLT$;
     1386    13529
     1387    13530    2        NEXT$ = NEXT$ -> BASE_LTS.LINK;
     1388    13531
     1389    13532    2      END;  /* DO WHILE  */
     1390    13533
     1391    13534    1      IF STACK$ -> SCALAR.ADSC.N < SIZE THEN
     1392    13535    1        STACK$ -> SCALAR.ADSC.N = SIZE;     /* the original stack size */
     1393    13536                                                 /* is defined in Z8EN      */
     1394    13537
     1395    13538             /* copy the base pointer and sequence number from redefines */
     1396    13539             /* objects to redefines subjects                            */
     1397    13540
     1398    13541    1      NEXT$ = LIST$;   /* original list */
     1399    13542
     1400    13543    2      DO WHILE (NEXT$ ~= ADDR(NIL));
     1401    13544
     1402    13545    2        IF NEXT$ -> BASE_LTS.REDEFINE$ ~= ADDR(NIL) THEN
     1403    13546    3          DO;
     1404    13547                 %GET_LTS (@ID = %BASE_LTS,
     1405    13548                           @ST$ = NEXT$ -> BASE_LTS.REDEFINE$,
     1406    13549                           @RETURN$ = LTS$$);
     1407    13592
     1408    13593    3            NEXT$ -> BASE_LTS.SEQ# = LTS$$ -> BASE_LTS.SEQ#;
     1409    13594    3            NEXT$ -> BASE_LTS.POINTER$ = LTS$$ -> BASE_LTS.POINTER$;
     1410    13595    3          END;
     1411    13596
     1412    13597    2        NEXT$ = NEXT$ -> BASE_LTS.NEXT;
     1413    13598
     1414    13599    2      END; /* DO WHILE */
     1415    13600
     1416    13601    1      COMCOM.PREV_QLT$ = COMCOM_QLT$;  /* restore previous QLT */
     1417    13602
     1418    13603    1    END Z9EY$CREATE_BASE_POINTERS;

