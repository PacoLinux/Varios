!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=LISTHELP,WSN=LOCAL
!RESOURCE MEM=256,TIME=10,NHPSN,ACCOUNT=(.SIACCT,.GNACCT,.LBACCT)
!ROUTE Lee Hallin, LADC
!"
!"  By default, the output created by this job is EDGEMARKed.
!"  However, if you do NOT want it EDGEMARKed, do a DEST=LP substitution on
!"  your BATCH or XEQ command such as:
!"    !BATCH LISTHELP_CRU.XACCT DEST=LP
!"
!DEFAULT DEST=*LS
!LET LISTING= 'DEST'
!LET MONNUM=$SUBSTR($DATE,2,2)
!LET MONTHS='JanFebMarAprMayJunJulAugSepOctNovDec'
!LET RIGHT=$SUBSTR(MONTHS,(MONNUM*3)-3,3) || ' ' || $SUBSTR($DATE,4,2)
!IF $SUBSTR(RIGHT,4,1) ~= '0' THEN GOTO DATE_OK
!LET RIGHT= $SUBSTR(RIGHT,0,4) || $SUBSTR(RIGHT,5,1)
!DATE_OK:
!PL6 LISTHELP_SI61.SIACCT OVER *LISTHELP_OU61.GNACCT,DEST(NSYS,OU,LS,SR(.SIACCT,.LBACCT),SC)
!PARTRGE LISTHELP_SIN1.SIACCT INTO *LISTHELP_OUN.GNACCT,DEST(LS,SR(.SIACCT,.LBACCT))
!IF LISTING = 'LP' THEN GOTO DONT_EDGEMARK
!EDGEMARK.XACCT DEST TO LP@(F='PERF',L='LISTHELP.XACCT',R='%RIGHT')
!DONT_EDGEMARK:
!LINK ;
      B_COPYRIGHT.LBACCT,;
      *LISTHELP_OU61.GNACCT,;
      *LISTHELP_OUN.GNACCT ;
 OVER LISTHELP.GNACCT(SIDCB=M$SI,LODCB=M$LO,STDINVOC)
