     3229    25736    5        CASE(%LEVEL_66);
     3230    25737    5          HIER_STK_PTR = 02;
     3231    25738
     3232    25739    5        CASE(%LEVEL_01);
     3233    25740    5          HIER_STK_PTR = %LEVEL_01;
     3234    25741
     3235    25742    5        CASE(ELSE);
     3236    25743    5          IF REDUCED_DATA.LEVEL.CURRENT <= REDUCED_DATA.LEVEL.PREVIOUS THEN
     3237    25744    5            CALL FIND_HIER_INDEX (REDUCED_DATA.LEVEL.CURRENT, HIER_STK_PTR);
     3238    25745    5          ELSE
     3239    25746    5            HIER_STK_PTR = HIER_STK_PTR + 1;
     3240    25747
     3241    25748    5      END; /* Case */
     3242    25749
     3243    25750    4      CALL CLEAR_HIER (HIER_STK_PTR, PREV_LEVEL);
     3244    25751
     3245    25752    4      CALL LINK_HIER_WITH_QLT (HIER_STK_PTR);
     3246    25753
