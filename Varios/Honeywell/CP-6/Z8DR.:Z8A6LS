     2309    23486    6                ALTRETURN;
     2310    23487    6              END;
     2311    23488
     2312    23489    5            VALUE$ = CDECL$ -> LX_REC_DECL.P.VALUE$;
     2313    23490
     2314    23491    5            RETURN;
     2315    23492
     2316    23493    5          CASE (ELSE);
     2317    23494    5            CALL Z8AE$ERROR (%E$UNEXPECTED_CASE);
     2318    23495    5            ALTRETURN;
     2319    23496
     2320    23497    5        END; /* Case */
     2321    23498
     2322    23499    4   END VALUE_CONTEXT_CHECK;
