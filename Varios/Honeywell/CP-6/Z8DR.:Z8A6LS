     2379    23647    4        IF COMCOM.OB.FIPS.LEVEL >= %HIGH_LEV THEN
     2380    23648    4          IF CDECL$ -> LX_REC_DECL.P.EXTERNAL$ ~= ADDR(NIL)  OR
