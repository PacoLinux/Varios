      382    18620        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:14   
      383    18621        /*===============*/
      384    18622    1     PROCESS_88_LIST: PROC (COND_LIST$);
      385    18623        /*===============*/
      386    18624
      387    18625    2   DCL COND_LIST$                PTR;
      388    18626
      389    18627    2   DCL CURRENT_88$               PTR;
      390    18628    2   DCL NEW_COND$                 PTR;
      391    18629    2   DCL 1 REDUCED_DATA     ALIGNED,
      392    18630             %REDUCED_DATA_INFO;
      393    18713
      394    18714    2      CURRENT_88$ = COND_LIST$ -> LX_LIST.HEAD$ -> LX_ITEM.LINK;
      395    18715
      396    18716    3      DO WHILE (CURRENT_88$ ~= ADDR(NIL));
      397    18717             %INITIALIZE_REDUCED_DATA;
      398    18731
      399    18732    3        CALL VERIFY_88 (CURRENT_88$ -> LX_LARGE.LARGE_ITEM$,
      400    18733    3                        REDUCED_DATA);
      401    18734
      402    18735    3        CALL PROCESS_88 (CURRENT_88$ -> LX_LARGE.LARGE_ITEM$,
      403    18736    3                         REDUCED_DATA,
      404    18737    3                         NEW_COND$);
      405    18738
      406    18739    3        CURRENT_88$ = CURRENT_88$ -> LX_LARGE.LINK;
      407    18740    3      END;
      408    18741
      409    18742    2      HIER_STK.B.CONDITION_OBJ (HIER_STK_PTR) = TRUE;
      410    18743
      411    18744    2   END PROCESS_88_LIST;
      412    18745        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:15   
      413    18746        /*======================*/
      414    18747    1     LINK_LEVEL_01_WITH_QLT: PROC;
      415    18748        /*======================*/
      416    18749
      417    18750    2   DCL BRO$               PTR;
      418    18751    2   DCL FATHER$            PTR;
      419    18752
      420    18753    3        DO CASE (O1COM.DATA_DECL_CONTEXT);
      421    18754
      422    18755    3          CASE (%WORKING_STORAGE, %COMMUNICATION_DESC);
      423    18756    3            BRO$ = HIER_STK.QLT$(1);
      424    18757
      425    18758    4            DO WHILE (BRO$ -> QLT.BROTHER ~= ADDR(NIL));
      426    18759    4              BRO$ = BRO$ -> QLT.BROTHER;
      427    18760    4            END;
      428    18761
      429    18762                 %UPDATE_PREV_QLT$ (@NEW_QLT$ = "HIER_STK.QLT$(1)");
      430    18769
      431    18770    3            IF BRO$ ~= HIER_STK.QLT$ (1) THEN
      432    18771    3              COMCOM.PREV_QLT$ = BRO$;
      433    18772
      434    18773    3          CASE (%LINKAGE_SECTION);
      435    18774    3            BRO$ = HIER_STK.QLT$(1);
      436    18775
      437    18776                 %NEW_BASE_LTS (@POINTER = LTS$,
      438    18777                                @NEXT    = O1COM.BASE_LTS_LIST$,
      439    18778                                @ST$     = "HIER_STK.QLT$(1)->QLT.ST$");
      440    18877
      441    18878    3            O1COM.BASE_LTS_LIST$ = LTS$;
      442    18879
      443    18880    3            CALL Z8DO$LINK_WITH_LTS (HIER_STK.QLT$(1)->QLT.ST$-> SCALAR.LTS$,
