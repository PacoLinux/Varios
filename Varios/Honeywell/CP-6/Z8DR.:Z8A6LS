     2662    24180        /*================*/
     2663    24181
     2664    24182    4   DCL DATA_NAME2_ST$            PTR;
     2665    24183    4   DCL TERM$                     PTR;
     2666    24184    4   DCL TERM3$                    PTR;
     2667    24185    4   DCL DATA_NAME3_ST$            PTR;
     2668    24186    4   DCL DN2$                      PTR;
     2669    24187    4   DCL DN3$                      PTR;
     2670    24188
     2671    24189        /*   66 DATANAME1 RENAMES DATANAME2 THRU DATANAME3. */
     2672    24190
     2673    24191
     2674    24192    4      DN2$   = CDECL$ -> LX_REC_DECL.P.RENAMES$ ->
     2675    24193    4               LX_LIST.HEAD$;
     2676    24194    4      DN3$   = CDECL$ -> LX_REC_DECL.P.RENAMES$ ->
     2677    24195    4               LX_LIST.TAIL$;
     2678    24196
     2679    24197    4      IF CDECL$ -> LX_REC_DECL.P.RENAMES$ -> LX_LIST.COUNT > 1 THEN
     2680    24198             /* TWO OBJECTS FOR THE RENAMES CLAUSE */
     2681    24199    4        CALL RENAMES_THRU (DN2$, DN3$)
     2682    24200    4          WHENALTRETURN
     2683    24201    5            DO;
     2684    24202    5              GOTO BAD_RENAMES;
     2685    24203    5            END;
     2686    24204    4      ELSE
     2687    24205             /* 66 A RENAMES B. -- One object */
     2688    24206    4        CALL RENAMES_NO_THRU (DN2$)
     2689    24207    4          WHENALTRETURN
