     3355    25874
     3356    25875    4        IF COMCOM.OB.FIPS.LEVEL >= %HIGH_LEV THEN
     3357    25876    4          IF TEMP_LENGTH > %ANSI_MAX_DIGITS THEN
     3358    25877    5            DO;
     3359    25878                   %CONDITIONAL_ERROR (@LX_ITEM$ = ITEM$,
     3360    25879                                       @ERROR#   = %E$ANSI_MAX_PIC_DIGITS,
     3361    25880                                       @LINE     = ITEM$ -> LX_ITEM.LINE,
     3362    25881                                       @COLUMN   = ITEM$ -> LX_ITEM.COLUMN);
     3363    25911    5              LENGTH = %ANSI_MAX_DIGITS;
     3364    25912    5            END;
     3365    25913
     3366    25914    4        IF NOT COMCOM.OB.FIPS.LEVEL >= %HIGH_LEV THEN
     3367    25915    4          IF TEMP_LENGTH > %BULL_MAX_PIC_DIGITS THEN
     3368    25916    5            DO;
     3369    25917                   %CONDITIONAL_ERROR (@LX_ITEM$ = ITEM$,
     3370    25918                                       @ERROR#   = %E$NUMERIC_ITEM_TOO_LONG,
