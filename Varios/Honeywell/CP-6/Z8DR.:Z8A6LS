     3597    26826
     3598    26827               %PACK_ADSC (@DESC    = REDUCED_DATA.DESC,
     3599    26828                           @TA      = TA,
     3600    26829                           @LENGTH  = 0);
     3601    26853    5        END;
     3602    26854    4      ELSE
     3603    26855             %PACK_ADSC (@DESC   = REDUCED_DATA.DESC,
     3604    26856                         @TA      = TA,
     3605    26857                         @LENGTH = PIC_OUT.WIDTH);
