PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:7    
      206    18337
      207    18338        /* Main Body */
      208    18339
      209    18340        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:8    
      210    18341
      211    18342        /*===================*/
      212    18343    1     CHECK_FOR_CONDITION: PROC  ALTRET;
      213    18344        /*===================*/
      214    18345
      215    18346        /*I*
      216    18347
      217    18348          NAME:  CHECK_FOR_CONDITION
      218    18349
      219    18350          PURPOSE:
      220    18351                 To check for 88's (condition names).  If they are here pool them, if
      221    18352                 not return.
      222    18353
      223    18354          CALL:
      224    18355                 CALL CHECK_FOR_CONDITION;  == Global CDECL$ and NDECL$
      225    18356
      226    18357
      227    18358          DESCRIPTION:
      228    18359                 This checks for 88's.   If the first item is an 88 it must be at the top
      229    18360                 of the program.  This is so because we check for the next on being 88's
      230    18361                 and we would have caught it earlier.  If the first item is 88 delete it
      231    18362                 and flag it, she's no good.  We also check for the next item being 88.
      232    18363                 This check is for when a level besides 88 is followed by an 88.  If this
      233    18364                 is the case we start the pooling of 88's as well as the preceding decl.
      234    18365                 We do this because we don't know if the preceding decl is elementary or
      235    18366                 non-elementary which is crucial to our processing.  The pooling is pointed
      236    18367                 to be O1COM.LEVEL_88_LIST$ (Non nil means something in it).  If there
      237    18368                 is already something in the 88 list then 2 possibilities exist. 1st is
      238    18369                 this item is also an 88, simply add it to the pool.  2nd possibility is
      239    18370                 this item is not an 88, simply process it with the regular(non 88) decl
      240    18371                 we saved away using the saved one as the current and this one as the next.
      241    18372                 Then process the 88 list.  That's all there is to it.  Basically 3 cases.
      242    18373
      243    18374                    Scenario 1:  88 first in program, flag and delete it.
      244    18375
      245    18376                    Scenario 2:  0n followed by 88.  Pool both and altreturn (exit Z8DR).
      246    18377
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:9    
      247    18378                    Scenario 3:  A.) Current 88, Next 88 - pool both
      248    18379                                 B.) Current 88, Next 0n - Process saved decl then all 88's.
      249    18380
      250    18381
      251    18382        */
      252    18383
      253    18384    2   DCL NEXT#                     SBIN WORD;
      254    18385    2   DCL CURRENT#                  SBIN WORD;
      255    18386    2   DCL LEVEL$                    PTR;
      256    18387    2   DCL LARGE$                    PTR;
      257    18388
      258    18389    2      IF NDECL$ = ADDR(NIL) THEN
      259    18390    2        NEXT# = %LEVEL_01;
      260    18391    3      ELSE DO;
      261    18392    3        CALL CHARBIN (NEXT#,
      262    18393    3                      NDECL$ -> LX_REC_DECL.P.LEVEL_NO$ ->
      263    18394    3                      LX_ITEM.DNT$ -> DNT.TEXT.NAME);
      264    18395    3        IF NEXT# = %LEVEL_88 AND COMCOM.OB.FIPS.LEVEL >= %INT_LEV THEN
      265    18396    3          CALL Z8AE$ERROR (%E$LEVEL_CHECK,
      266    18397    3                           NDECL$ -> LX_REC_DECL.P.LEVEL_NO$ -> LX_TERMINAL.LINE,
      267    18398    3                           NDECL$ -> LX_REC_DECL.P.LEVEL_NO$ -> LX_TERMINAL.COLUMN);
      268    18399    3        END;
      269    18400
      270    18401    2      CALL CHARBIN (CURRENT#,
      271    18402    2                    CDECL$ -> LX_REC_DECL.P.LEVEL_NO$ ->
      272    18403    2                    LX_ITEM.DNT$ -> DNT.TEXT.NAME);
      273    18404
      274    18405    2      IF O1COM.LEVEL_88_LIST$ ~= ADDR(NIL) THEN
      275    18406    2        IF CURRENT# = %LEVEL_88 THEN
      276    18407    3          DO;
      277    18408    3            CALL ADD_TO_88_LIST (O1COM.LEVEL_88_LIST$);
      278    18409
      279    18410    3            IF NDECL$ = ADDR(NIL) THEN
      280    18411    4              DO;
      281    18412    4                CALL DATA_DECLARATION (O1COM.LEVEL_88_LIST$ -> LX_LIST.HEAD$ ->
      282    18413    4                                            LX_LARGE.LARGE_ITEM$,
      283    18414    4                                            ADDR(NIL));
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:10   
      284    18415    4                IF HIER_STK_PTR = %LEVEL_01 THEN
      285    18416    4                  CALL LINK_LEVEL_01_WITH_QLT;
      286    18417
      287    18418    4                CALL PROCESS_88_LIST (O1COM.LEVEL_88_LIST$);
      288    18419    4                CALL Z8ED$DISPOSE_ABSSYN_TREE (O1COM.LEVEL_88_LIST$);
      289    18420    4              END;
      290    18421
      291    18422    3            ALTRETURN;
      292    18423    3          END;
      293    18424    2        ELSE
      294    18425    3          DO;
      295    18426    3            CALL DATA_DECLARATION (O1COM.LEVEL_88_LIST$ -> LX_LIST.HEAD$ ->
      296    18427    3                                        LX_LARGE.LARGE_ITEM$,
      297    18428    3                                   CDECL$);
      298    18429
      299    18430    3            IF HIER_STK_PTR = %LEVEL_01 THEN
      300    18431    3              CALL LINK_LEVEL_01_WITH_QLT;
      301    18432
      302    18433    3            CALL PROCESS_88_LIST (O1COM.LEVEL_88_LIST$);
      303    18434    3            CALL Z8ED$DISPOSE_ABSSYN_TREE (O1COM.LEVEL_88_LIST$);
      304    18435
      305    18436    3            IF NEXT# = %LEVEL_88 THEN
      306    18437    4              DO;
      307    18438    4                CALL START_88_LIST (O1COM.LEVEL_88_LIST$);
      308    18439    4                ALTRETURN;
      309    18440    4              END;
      310    18441    3            ELSE
      311    18442    3              RETURN;
      312    18443    3          END;
      313    18444    2      ELSE
      314    18445    2        IF CURRENT# = %LEVEL_88 THEN
      315    18446    3          DO;
      316    18447    3            LEVEL$ = CDECL$ -> LX_REC_DECL.P.LEVEL_NO$;
      317    18448                 %CONDITIONAL_ERROR (@LX_ITEM$ = LEVEL$,
      318    18449                                     @ERROR#   = %E$LEVEL_1_OR_77_AT_TOP,
      319    18450                                     @LINE     = LEVEL$ -> LX_ITEM.LINE,
      320    18451                                     @COLUMN   = LEVEL$ -> LX_ITEM.COLUMN);
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:11   
      321    18481    3            ALTRETURN;
      322    18482    3          END;
      323    18483    2        ELSE
      324    18484    2          IF NEXT# = %LEVEL_88 THEN
      325    18485    3            DO;
      326    18486    3              CALL START_88_LIST (O1COM.LEVEL_88_LIST$);
      327    18487    3              ALTRETURN;
      328    18488    3            END;
      329    18489
      330    18490    2      RETURN;
      331    18491
      332    18492    2   END CHECK_FOR_CONDITION;
      333    18493        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:12   
      334    18494        /*=============*/
      335    18495    1     START_88_LIST: PROC (LEVEL_88_LIST$);
      336    18496        /*=============*/
      337    18497
      338    18498    2   DCL LEVEL_88_LIST$            PTR;
      339    18499
      340    18500    2   DCL LARGE$                    PTR;
      341    18501
      342    18502             %NEW (@POINTER = LEVEL_88_LIST$,
      343    18503                   @TEMPLATE = LX_LIST);
      344    18513
      345    18514             %INIT_LX_LIST (@POINTER = LEVEL_88_LIST$);
      346    18524
      347    18525             %NEW (@POINTER   = LARGE$,
      348    18526                   @TEMPLATE  = LX_LARGE);
      349    18536
      350    18537             %INIT_LX_LARGE (@POINTER = LARGE$);
      351    18544
