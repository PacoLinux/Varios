      375    18598
      376    18599             %ADD_LX_ITEM_TO_LIST_TAIL (@LIST$  = O1COM.LEVEL_88_LIST$,
      377    18600                                        @FROM$  = LARGE$);
      378    18616
      379    18617    2        CDECL$ = ADDR(NIL);
      380    18618
