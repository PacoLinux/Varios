      175    18143        %PIC_OUT (STCLASS = EXT);
      176    18216        %GROUP_DECL;
      177    18226
      178    18227    1   DCL Z8DO$LINK_WITH_LTS        ENTRY (2);
      179    18228    1   DCL Z8DU$BUILD_INITIAL_ITEM   ENTRY (2);
      180    18229    1   DCL Z8EU$DELETE_LIST_ITEM     ENTRY (2);
      181    18230    1   DCL Z8DR$NEW_INDEX            ENTRY (5);
