     3582    26720
     3583    26721
     3584    26722    4      IF REDUCED_DATA.USAGE.TYPE = %RSV_WRD_GBCD
     3585    26723    4        THEN TA = %ADSC6;
     3586    26724    4        ELSE TA = %ADSC9;
