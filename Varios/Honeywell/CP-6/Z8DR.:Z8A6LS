      183    18232    1   DCL Z8EC$NEW_CONST_WITH_TYPE_DESC    ENTRY (3) ALTRET;
      184    18233    1   DCL Z8EC$CREATE_CONST                ENTRY (3) ALTRET;
      185    18234    1   DCL Z8AF$FORME                ENTRY (4);
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:6    
      186    18235    1   DCL Z8AC$MAKE_CONST           ENTRY (1);
      187    18236    1   DCL Z8AE$ERROR                ENTRY (12);
      188    18237
      189    18238    1   DCL Z8AU$TYPE_DESCRIPTOR             ENTRY (2);
      190    18239    1   DCL Z8AU$CREATE_NUM_DESC             ENTRY (3);
      191    18240    1   DCL Z8ED$DISPOSE_ABSSYN_TREE         ENTRY (1);
      192    18241    1   DCL Z8DP$VALIDATE_PIC                ENTRY (3) ALTRET;
      193    18242
      194    18243    1   DCL DNT$_FROM_RSWNO                  (0:0) PTR SYMREF READONLY;
      195    18244
      196    18245    1   DCL CDECL$                           PTR;
      197    18246    1   DCL NDECL$                           PTR;
      198    18247    1   DCL CURRENT_DECL$                    PTR;
      199    18248    1   DCL NEXT_DECL$                       PTR;
      200    18249    1   DCL 1 REDUCED_DATA            ALIGNED,
      201    18250             %REDUCED_DATA_INFO       /*(@LVL = 2)*/;
      202    18333    1   DCL BRO$                      PTR;
