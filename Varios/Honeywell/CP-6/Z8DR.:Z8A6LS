     1168    20581    3     VERIFY_ORDER: PROC (DN2_ST$, DN3_ST$, RENAMES$) ALTRET;
     1169    20582        /*============*/
     1170    20583
     1171    20584    4   DCL RENAMES$           PTR;
     1172    20585    4   DCL DN2_ST$            PTR;
     1173    20586    4   DCL DN3_ST$            PTR;
     1174    20587
     1175    20588    4   DCL SEARCH_QLT$        PTR;
     1176    20589    4   DCL START_FOUND        BOOLEAN;
     1177    20590
     1178    20591
     1179    20592    4      START_FOUND = FALSE;
     1180    20593    4      SEARCH_QLT$ = HIER_STK.QLT$(1);
     1181    20594
     1182    20595    5      DO WHILE (SEARCH_QLT$ ~= ADDR(NIL));
     1183    20596    5        IF SEARCH_QLT$ -> QLT.ST$ = DN3_ST$
     1184    20597    5        AND START_FOUND THEN
     1185    20598    5          EXIT;
     1186    20599
     1187    20600    5        IF SEARCH_QLT$ -> QLT.ST$ = DN2_ST$ THEN
     1188    20601    5          START_FOUND = TRUE;
     1189    20602    5        ELSE
     1190    20603    5          IF SEARCH_QLT$ -> QLT.ST$ = DN3_ST$ THEN
     1191    20604    6            DO;
     1192    20605                   %CONDITIONAL_ERROR (@LX_ITEM$ = RENAMES$,
     1193    20606                                       @ERROR#   = %E$END_RENAMES_FOUND_FIRST,
     1194    20607                                       @LINE     = RENAMES$ -> LX_ITEM.LINE,
     1195    20608                                       @COLUMN   = RENAMES$ -> LX_ITEM.COLUMN);
     1196    20638    6              ALTRETURN;
     1197    20639    6            END;
     1198    20640
     1199    20641             %STEP_QLT (POINTER = SEARCH_QLT$);
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:50   
     1200    20651    5      END; /* While -- search_qlt$ ~= addr(nil) */
     1201    20652
     1202    20653    4      IF SEARCH_QLT$ = ADDR(NIL) THEN
     1203    20654    5        DO;
     1204    20655               %CONDITIONAL_ERROR (@LX_ITEM$ = RENAMES$,
     1205    20656                                   @ERROR#   = %E$RENAME_MUST_FOLLOW_01,
     1206    20657                                   @LINE     = RENAMES$ -> LX_ITEM.LINE,
     1207    20658                                   @COLUMN   = RENAMES$ -> LX_ITEM.COLUMN);
     1208    20688    5          ALTRETURN;
     1209    20689    5        END;
     1210    20690
     1211    20691    4      SEARCH_QLT$ = SEARCH_QLT$ -> QLT.FATHER;
     1212    20692
     1213    20693    5      DO WHILE (SEARCH_QLT$ ~= ADDR(NIL));
     1214    20694    5        IF SEARCH_QLT$ -> QLT.ST$ -> ARRAY.ID = %ARRAY THEN
     1215    20695    6          DO;
     1216    20696                 %CONDITIONAL_ERROR (@LX_ITEM$ = RENAMES$,
     1217    20697                                     @ERROR#   = %E$OCCURS_IN_RENAMES,
     1218    20698                                     @LINE     = RENAMES$ -> LX_ITEM.LINE,
     1219    20699                                     @COLUMN   = RENAMES$ -> LX_ITEM.COLUMN);
     1220    20729    6            ALTRETURN;
     1221    20730    6          END;
     1222    20731    5        ELSE
     1223    20732    5          IF SEARCH_QLT$ -> QLT.ST$ = DN2_ST$ THEN
     1224    20733    6            DO;
     1225    20734                   %CONDITIONAL_ERROR (@LX_ITEM$ = RENAMES$,
     1226    20735                                       @ERROR#   = %E$RENAME_START_CONTAIN_END,
     1227    20736                                       @LINE     = RENAMES$ -> LX_ITEM.LINE,
     1228    20737                                       @COLUMN   = RENAMES$ -> LX_ITEM.COLUMN);
     1229    20767    6              ALTRETURN;
     1230    20768    6            END;
     1231    20769
     1232    20770    5        SEARCH_QLT$ = SEARCH_QLT$ -> QLT.FATHER;
     1233    20771    5      END; /* While -- search_qlt$ ~= addr(nil) */
     1234    20772
     1235    20773    4   END VERIFY_ORDER;
     1236    20774        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:51   
     1237    20775        /*==================*/
     1238    20776    3     CHECK_REDEF_OBJECT: PROC (TARGET_BRO$, OBJ_DNT$, OBJECT_ST$) ALTRET;
     1239    20777        /*==================*/
     1240    20778
     1241    20779    4   DCL TARGET_BRO$               PTR;
     1242    20780    4   DCL OBJ_DNT$                  PTR;
     1243    20781    4   DCL OBJECT_ST$                PTR;
     1244    20782
     1245    20783    4   DCL OBJ_BRO$                  PTR;
     1246    20784    4   DCL TARGET_FOUND              BOOLEAN;
     1247    20785
     1248    20786
     1249    20787    4      OBJECT_ST$ = OBJ_DNT$ -> DNT.ST$;
     1250    20788
     1251    20789    4   ST_SEARCH:
     1252    20790    5      DO WHILE (OBJECT_ST$ ~= ADDR(NIL));
     1253    20791    5        IF OBJECT_ST$ -> SCALAR.QLT$ -> QLT.LEVEL_NO
     1254    20792    5        ~= REDUCED_DATA.LEVEL.CURRENT THEN
     1255    20793    5          OBJECT_ST$ = OBJECT_ST$ -> SCALAR.NEXT;
     1256    20794    5        ELSE
     1257    20795    5          IF NOT OBJECT_ST$ -> SCALAR.B.REDEF_OBJ AND
     1258    20796    5             NOT ( COMCOM.OB.IBM  AND
     1259    20797    5                   OBJECT_ST$ -> SCALAR.B.REDEF_SUB ) THEN
     1260    20798    5            OBJECT_ST$ = OBJECT_ST$ -> SCALAR.NEXT;
     1261    20799    5          ELSE
     1262    20800    6            DO;
     1263    20801    6              OBJ_BRO$ = OBJECT_ST$ -> SCALAR.QLT$ -> QLT.BROTHER;
     1264    20802
     1265    20803    6   BRO_SEARCH:
     1266    20804    7              DO WHILE (OBJ_BRO$ ~= ADDR(NIL));
     1267    20805    7                IF OBJ_BRO$ = TARGET_BRO$
     1268    20806    7                  AND OBJ_BRO$ -> QLT.ST$ -> SCALAR.B.REDEF_SUB THEN
     1269    20807    7                  RETURN;
     1270    20808    7                ELSE
     1271    20809    7                  IF OBJ_BRO$ -> QLT.ST$ -> SCALAR.B.REDEF_SUB THEN
     1272    20810    7                    OBJ_BRO$ = OBJ_BRO$ -> QLT.BROTHER;
     1273    20811    7                  ELSE
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:52   
     1274    20812    7                    EXIT BRO_SEARCH;
     1275    20813    7              END BRO_SEARCH;
     1276    20814
     1277    20815    6              OBJECT_ST$ = OBJECT_ST$ -> SCALAR.NEXT;
     1278    20816    6            END;
     1279    20817    5      END ST_SEARCH;
     1280    20818
     1281    20819    4      ALTRETURN;
     1282    20820
     1283    20821    4   END CHECK_REDEF_OBJECT;
     1284    20822        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:53   
     1285    20823        /*======================*/
     1286    20824    3     VERIFY_REDEF_PLACEMENT: PROC (REDEF$) ALTRET;
     1287    20825        /*======================*/
     1288    20826
     1289    20827    4   DCL REDEF$                    PTR;
     1290    20828
     1291    20829    4   DCL I                         SBIN WORD;
     1292    20830    4   DCL LEVEL_FOUND               BOOLEAN;
     1293    20831    4   DCL PLUG1$                    PTR;
     1294    20832
     1295    20833    4      LEVEL_FOUND = FALSE;
     1296    20834
     1297    20835    4   LEVEL_SEARCH:
     1298    20836    5      DO I = 1 TO HIER_STK_PTR;
     1299    20837    5        IF HIER_STK.LEVEL# (I) = REDUCED_DATA.LEVEL.CURRENT THEN
     1300    20838    6          DO;
     1301    20839    6            LEVEL_FOUND = TRUE;
     1302    20840    6            EXIT LEVEL_SEARCH;
     1303    20841    6          END;
     1304    20842    5      END;
     1305    20843
     1306    20844    4      IF NOT LEVEL_FOUND THEN
     1307    20845    5        DO;
     1308    20846    5          PLUG1$ = CDECL$ -> LX_REC_DECL.P.DATA_NAME$ -> LX_ITEM.DNT$;
     1309    20847
     1310    20848               %CONDITIONAL_ERROR (@LX_ITEM$ = REDEF$,
     1311    20849                                   @ERROR#   = %E$NON_MATCH_REDEF_LEVEL,
     1312    20850                                   @LINE     = REDEF$ -> LX_ITEM.LINE,
     1313    20851                                   @COLUMN   = REDEF$ -> LX_ITEM.COLUMN,
     1314    20852                                   @PLUG1    = PLUG1$,
     1315    20853                                   @PLUG2    = REDEF$ -> LX_ITEM.DNT$);
     1316    20883    5          ALTRETURN;
     1317    20884    5        END;
     1318    20885    4      ELSE
     1319    20886    4        IF HIER_STK.QLT$ (I) -> QLT.ST$ -> SCALAR.DNT$ = REDEF$ -> LX_ITEM.DNT$ THEN
     1320    20887    4          REDUCED_DATA.REDEFINES.OBJECT$ = HIER_STK.QLT$ (I) -> QLT.ST$;
     1321    20888    4        ELSE
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:54   
     1322    20889    4          IF REDEF$ -> LX_ITEM.DNT$ -> DNT.ST$ -> ST.ID ~= %SCALAR AND
     1323    20890    4             REDEF$ -> LX_ITEM.DNT$ -> DNT.ST$ -> ST.ID ~= %ARRAY THEN
     1324    20891    5              DO;
     1325    20892    5                REDUCED_DATA.REDEFINES.OBJECT$ = ADDR(NIL);
     1326    20893
     1327    20894    5                CALL Z8AE$ERROR (%E$ILLEGAL_SYMBOL,
     1328    20895    5                                 REDEF$ -> LX_ITEM.LINE,
     1329    20896    5                                 REDEF$ -> LX_ITEM.COLUMN,
     1330    20897    5                                 REDEF$ -> LX_ITEM.DNT$);
     1331    20898    5                ALTRETURN;
     1332    20899    5              END;
     1333    20900    4            ELSE
     1334    20901
     1335    20902    4          CALL CHECK_REDEF_OBJECT (HIER_STK.QLT$ (I),
     1336    20903    4                                   REDEF$ -> LX_ITEM.DNT$,
     1337    20904    4                                   REDUCED_DATA.REDEFINES.OBJECT$)
     1338    20905    4            WHENALTRETURN
     1339    20906    5              DO;
     1340    20907    5                REDUCED_DATA.REDEFINES.OBJECT$ = ADDR(NIL);
     1341    20908
     1342    20909                     %CONDITIONAL_ERROR (@LX_ITEM$ = REDEF$,
     1343    20910                                         @ERROR#   = %E$NON_ADJACENT_REDEF,
     1344    20911                                         @LINE     = REDEF$ -> LX_ITEM.LINE,
     1345    20912                                         @COLUMN   = REDEF$ -> LX_ITEM.COLUMN);
     1346    20942    5                ALTRETURN;
     1347    20943    5              END;
     1348    20944
     1349    20945    4      IF REDUCED_DATA.REDEFINES.OBJECT$ -> ARRAY.ID = %ARRAY THEN
     1350    20946    5        DO;
     1351    20947    5          REDUCED_DATA.REDEFINES.OBJECT$ = ADDR(NIL);
     1352    20948               %CONDITIONAL_ERROR (@LX_ITEM$ = REDEF$,
     1353    20949                                   @ERROR#   = %E$OCCURS_IN_REDEF,
     1354    20950                                   @LINE     = REDEF$ -> LX_ITEM.LINE,
     1355    20951                                   @COLUMN   = REDEF$ -> LX_ITEM.COLUMN);
     1356    20981    5          ALTRETURN;
     1357    20982    5        END;
     1358    20983
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:55   
     1359    20984    4      IF REDUCED_DATA.REDEFINES.OBJECT$ -> SCALAR.B.REDEF_SUB THEN
     1360    20985    4        IF NOT COMCOM.OB.IBM THEN
     1361    20986    5          DO;
     1362    20987                 %CONDITIONAL_ERROR (@LX_ITEM$ = REDEF$,
     1363    20988                                     @ERROR#   = %E$REDEF_REDEF_ERROR,
     1364    20989                                     @LINE     = REDEF$ -> LX_TERMINAL.LINE,
     1365    20990                                     @COLUMN   = REDEF$ -> LX_TERMINAL.COLUMN);
     1366    21020    5            ALTRETURN;
     1367    21021    5          END;
     1368    21022
     1369    21023    4      RETURN;
     1370    21024
     1371    21025
     1372    21026    4   END VERIFY_REDEF_PLACEMENT;
     1373    21027        %EJECT;
