     4025    27769    5          DIM# = (HIER_STK.DIMS$(I) -> LTS.N - %MIN_DIM_SIZE)
     4026    27770    5               / %NO_WORDS_PER_DIM + 1;
     4027    27771
     4028    27772
     4029    27773               %FILL_DIMS (@DIMS$   = DIMS$,
     4030    27774                           @SS#     = DIM#,
     4031    27775                           @DIM     = REDUCED_DATA.OCCURS.INT2);
