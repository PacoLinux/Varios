      864    20156    3     EXTRACT_USAGE: PROC;
      865    20157        /*=============*/
      866    20158
      867    20159
      868    20160
      869    20161    4      IF CDECL$ -> LX_REC_DECL.P.USAGE$ = ADDR(NIL) THEN
      870    20162    5        DO;
      871    20163
      872    20164    5          IF REDUCED_DATA.LEVEL.CURRENT <= GROUP.USAGE.LEVEL THEN
      873    20165                 %UPDATE_GROUP_USAGE (@USAGE = %NO_USAGE,
      874    20166                                          @LEVEL = REDUCED_DATA.LEVEL.CURRENT);
      875    20172    5          ELSE
      876    20173    5            REDUCED_DATA.USAGE.TYPE = GROUP.USAGE.TYPE;
      877    20174    5        END;
      878    20175    4      ELSE
      879    20176    5        DO;
      880    20177    5          REDUCED_DATA.USAGE.TYPE = CDECL$ -> LX_REC_DECL.P.USAGE$ ->
      881    20178    5                                    LX_ITEM.SYMBOL;
      882    20179
      883    20180    5          IF REDUCED_DATA.USAGE.TYPE = %RSV_WRD_COMP THEN
      884    20181    5            REDUCED_DATA.USAGE.TYPE = O1COM.COMP_DEFAULT;
      885    20182
      886    20183    5          IF REDUCED_DATA.USAGE.TYPE = %RSV_WRD_DISPLAY THEN
      887    20184    5            REDUCED_DATA.USAGE.TYPE  = %NO_USAGE;
      888    20185
      889    20186    5          IF GROUP.USAGE.TYPE ~= %NO_USAGE THEN
      890    20187    6            DO;
      891    20188    6              IF REDUCED_DATA.LEVEL.CURRENT <= GROUP.USAGE.LEVEL THEN
      892    20189                     %UPDATE_GROUP_USAGE (@USAGE = REDUCED_DATA.USAGE.TYPE,
      893    20190                                              @LEVEL = REDUCED_DATA.LEVEL.CURRENT);
      894    20196    6            END;
      895    20197    5          ELSE
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:36   
      896    20198                 /* No Group usage specified so the current usage is
      897    20199                    predominant */
      898    20200                 %UPDATE_GROUP_USAGE (@USAGE = REDUCED_DATA.USAGE.TYPE,
      899    20201                                          @LEVEL = REDUCED_DATA.LEVEL.CURRENT);
      900    20207
      901    20208    5        END;
      902    20209
      903    20210    4        IF NOT REDUCED_DATA.B.ELEMENTARY THEN
      904    20211    4          REDUCED_DATA.USAGE.TYPE = %NO_USAGE;
      905    20212
      906    20213
      907    20214
      908    20215    4   END EXTRACT_USAGE;
      909    20216        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:37   
      910    20217        /*=============*/
      911    20218    3     EXTRACT_LEVEL: PROC;
      912    20219        /*=============*/
      913    20220
      914    20221    4      DCL I SBIN;
      915    20222
      916    20223    4      REDUCED_DATA.LEVEL.PREVIOUS = O1COM.LEVEL_NO;
      917    20224
      918    20225    4      CALL CHARBIN (O1COM.LEVEL_NO,
      919    20226    4                    CDECL$ -> LX_REC_DECL.P.LEVEL_NO$ ->
      920    20227    4                    LX_ITEM.DNT$ -> DNT.TEXT.NAME);
      921    20228
      922    20229    4      REDUCED_DATA.LEVEL.CURRENT = O1COM.LEVEL_NO;
      923    20230
      924    20231           %CHANGE_LEVEL_TO_CONSEC (@LEVEL = REDUCED_DATA.LEVEL.PREVIOUS);
      925    20253           %CHANGE_LEVEL_TO_CONSEC(@LEVEL = REDUCED_DATA.LEVEL.CURRENT);
      926    20275
      927    20276    4      IF NDECL$ = ADDR(NIL) THEN
      928    20277    4        REDUCED_DATA.LEVEL.NEXT = %LEVEL_01;
      929    20278    4      ELSE
      930    20279    5        DO;
      931    20280    5          CALL CHARBIN (REDUCED_DATA.LEVEL.NEXT,
      932    20281    5                        NDECL$ -> LX_REC_DECL.P.LEVEL_NO$ ->
      933    20282    5                        LX_ITEM.DNT$ -> DNT.TEXT.NAME);
      934    20283
      935    20284               %CHANGE_LEVEL_TO_CONSEC(@LEVEL = REDUCED_DATA.LEVEL.NEXT);
      936    20306    5        END;
      937    20307
      938    20308    4      IF REDUCED_DATA.LEVEL.CURRENT = %LEVEL_01 THEN
      939    20309    5        DO;
      940    20310    5          GROUP = '0'B;
      941    20311    5          GROUP.USAGE.LEVEL = %LEVEL_01;
      942    20312    5        END;
      943    20313    4      ELSE IF COMCOM.OB.FIPS.LEVEL > 0 AND REDUCED_DATA.LEVEL.CURRENT < REDUCED_DATA.
             20313               LEVEL.PREVIOUS
      944    20314    4          AND CDECL$ -> LX_REC_DECL.P.LEVEL_NO$ -> LX_ITEM.DNT$ ->
      945    20315    4          DNT.TEXT.NAME ~= '66' THEN
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:38   
      946    20316    5   SEQ_CHECK: DO;
      947    20317    6          DO I = HIER_STK_PTR DOWNTO 2;
      948    20318    6            IF HIER_STK.LEVEL#(I) = REDUCED_DATA.LEVEL.CURRENT THEN
      949    20319    6              EXIT SEQ_CHECK;
      950    20320    6          END;
      951    20321    5          CALL Z8AE$ERROR (%E$ANSI_LEVEL_SEQ_ERROR,
      952    20322    5                           CDECL$ -> LX_REC_DECL.P.LEVEL_NO$ -> LX_ITEM.LINE,
      953    20323    5                           CDECL$ -> LX_REC_DECL.P.LEVEL_NO$ -> LX_ITEM.COLUMN);
      954    20324    5        END; /* SEQ_CHECK */
      955    20325
      956    20326    4      REDUCED_DATA.B.ELEMENTARY = (REDUCED_DATA.LEVEL.NEXT <=
      957    20327    4                                   REDUCED_DATA.LEVEL.CURRENT);
      958    20328
      959    20329    4   END EXTRACT_LEVEL;
      960    20330        %EJECT;
PL6.E3A0      #001=Z8DR$RECORD_DECL File=Z8DR.:Z8A6TSI                           SAT 08/09/97 11:18 Page:39   
