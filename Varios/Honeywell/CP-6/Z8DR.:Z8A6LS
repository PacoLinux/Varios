     3997    27551
     3998    27552    4      IF REDUCED_DATA.LEVEL.CURRENT <= REDUCED_DATA.LEVEL.PREVIOUS THEN
     3999    27553    4        CALL FIND_HIER_INDEX (REDUCED_DATA.LEVEL.CURRENT, J);
     4000    27554    4      ELSE
     4001    27555    4        J = HIER_STK_PTR + 1;
     4002    27556
     4003    27557    5      DO I = J - 1 DOWNTO 1;
     4004    27558    5        IF HIER_STK.DIMS$(I) ~= ADDR(NIL) THEN
     4005    27559    5          EXIT;
     4006    27560    5      END;
     4007    27561
     4008    27562    4      IF I <= %SINGLE_DIMENSION THEN
     4009    27563    5        DO;
     4010    27564    5          DIM# = %SINGLE_DIMENSION;
     4011    27565
     4012    27566               %NEW_DIMS (@POINTER = DIMS$,
     4013    27567                          @SIZE    = "%MIN_DIM_SIZE + %NO_WORDS_PER_DIM");
     4014    27660
