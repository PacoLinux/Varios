    5      /*T*                                                         */
    6      /*T***********************************************************/
    7
    8      /*P*
    9                NAME: Z8I_ERR_TXT
   10             PURPOSE: Provide the text of the error messages for use by
   11                      the Annotator.
   12         DESCRIPTION:
   13
   14         The error message text in the tables below may contain
   15         plug in information which is included in the error message
   16         when the error is printed on the compilation listing.
   17         See the entry points to the routine Z8AE$ERROR in order to
   18         set up the plug in values when an error is generated.
   19         In each error message text, however, the point at which the
   20         textual substitution is made must be designated.  This is
   21         done with the character sequence !0 thru !9.  When
   22         the Annotator prints an error message, the text of the
   23         message is scanned for exclamation points followed by a digit.
   24         When found, the Annotator substitutes the corresponding plug in
   25         value before the message is actually printed.
   26      */
   27
   28      %INCLUDE Z8I_ERR_SEV;
   29
   30      /* THE MACROS ON THE FOLLOWING PAGES PRODUCE TABLES OF ERROR
   31         MESSAGES FOR USE IN THE ANNOTATOR.  TEN ARRAYS OF POINTERS
   32         TO ERROR TEXT EXIST:
   33
   34                 ERRMSG_TABLE_ADDRS_0
   35                 ERRMSG_TABLE_ADDRS_1
   36                 ERRMSG_TABLE_ADDRS_2
   37                 ERRMSG_TABLE_ADDRS_3
09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  2    
   38                 ERRMSG_TABLE_ADDRS_4
   39                 ERRMSG_TABLE_ADDRS_5
   40                 ERRMSG_TABLE_ADDRS_6
   41                 ERRMSG_TABLE_ADDRS_7
   42                 ERRMSG_TABLE_ADDRS_8
   43                 ERRMSG_TABLE_ADDRS_9
   44      */
   45      %EJECT;

09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  3    
   46      /* START A TABLE STRUCTURE */
   47
   48      %MACRO ERRMSG_TABLE_START (TABLENO = 0);
   49
   50             DCL 1 %(CHARTEXT (CONCAT('ERRMSG_TABLE_',
   51                                      BINCHAR(TABLENO,1))))
   52                      UNAL CONSTANT
   53
   54      % SET ERRMSG_COUNT = 0;
   55      % SET ERRMSG_TABLE_NO = TABLENO;
   56      % SET NEXT_ERR_NO = TABLENO * 100;
   57      %MEND;
   58
   59
   60      %EJECT;

09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  4    
   61      /* GENERATE AN ADDR TO STRUCTURE ENTRY, FOR USE IN MACRO
   62         ERRMSG_TABLE_END ONLY */
   63
   64      %MACRO ERRMSG_TXT_ADDR (ERRMSG_NUM = 0);
   65
   66      % IF (ERRMSG_COUNT > ERRMSG_NUM);
   67
   68             ADDR( %(CHARTEXT(CONCAT('ERRMSG_TABLE_',
   69                                     BINCHAR(ERRMSG_TABLE_NO,1))))
   70                               . %(CHARTEXT(CONCAT('ERR_TXT_',
   71                                     BINCHAR(ERRMSG_TABLE_NO*100 + ERRMSG_NUM, 3))))
   72                 ),
   73      % SET X = ERRMSG_NUM + 1;
   74      % ERRMSG_TXT_ADDR  (ERRMSG_NUM = X);
   75
   76      % ENDIF;
   77
   78      %MEND;
   79
   80
   81      %EJECT;

09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  5    
   82      /* END A TABLE STRUCTURE */
   83
   84      %MACRO ERRMSG_TABLE_END (ERRMSG_NO_LAST = 0);
   85
   86             , 2 %(CHARTEXT(CONCAT('ERR_TXT_LAST_',
   87                                    BINCHAR(ERRMSG_TABLE_NO,1))))
   88                 CHAR(1);
   89
   90      % IF (ERRMSG_NO_LAST ~= NEXT_ERR_NO - 1);
   91      % ERROR('**** SOME ERRORS HAVE BEEN SKIPPED IN THIS TABLE');
   92      % ENDIF;
   93
   94             DCL %(CHARTEXT(CONCAT('ERRMSG_TABLE_ADDRS_',
   95                             BINCHAR(ERRMSG_TABLE_NO,1))))
   96                (0: %(CHARTEXT(BINCHAR(ERRMSG_COUNT,3)))
   97                ) PTR CONSTANT INIT(
   98                                     % ERRMSG_TXT_ADDR;
   99                     ADDR( %(CHARTEXT(CONCAT('ERRMSG_TABLE_',
  100                                         BINCHAR(ERRMSG_TABLE_NO,1))))
  101                       . %(CHARTEXT(CONCAT('ERR_TXT_LAST_',
  102                              BINCHAR(ERRMSG_TABLE_NO,1))))
  103                          ));
  104
  105      %MEND;
  106
  107
  108      % EJECT;
  109      /* PRINT THE ERROR ON THE LSTIN LISTING */
  110
  111      %MACRO PRINT_ERR_NUM (N=0);
  112
  113      %LSET LISTDIR = '0'B;
  114      %LSET LISTMAC = '1'B;
  115      %LSET LISTSUB = '1'B;
  116      %LSET LISTEXP = '1'B;
  117                                                              %CHARTEXT(CONCAT('/*',BINCHAR(N,3),'*/'))
  118                                                              %CHARTEXT(' ')
09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  6    
  119      %MEND;
  120
  121      %EJECT;

09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  7    
  122      /* ADD AN ERROR MESSAGE TO THE TABLES */
  123
  124      %MACRO ERRMSG_TEXT (ERRNUM=-1, ERRTXT=' ');
  125
  126      % IF (ERRNUM = -1);
  127      % ERROR('**** THE ERROR NUMBER FOR THE TEXT MUST BE SPECIFIED');
  128      % ENDIF;
  129
  130      % IF (ERRTXT = ' ');
  131      % ERROR('****THE TEXT OF THE ERROR MESSAGE MUST BE GIVEN');
  132      % ENDIF;
  133
  134      % IF (ERRNUM ~= NEXT_ERR_NO);
  135      % ERROR ('**** AN ERROR MESSAGE NUMBER HAS BEEN SKIPPED IN THE TABLE');
  136      % ENDIF;
  137
  138             , 2 %(CHARTEXT(CONCAT('ERR_TXT_',
  139                                BINCHAR(ERRNUM,3))))
  140                 CHAR( %(CHARTEXT(BINCHAR(LENGTHC(ERRTXT), 3)))
  141      ) INIT(ERRTXT)
  142
  143      %LSET LISTDIR = '0'B;
  144      %LSET LISTMAC = '1'B;
  145      %PRINT_ERR_NUM(N=ERRNUM);
  146      /* %LSET LISTMAC = '0'B;  */
  147      % SET ERRMSG_COUNT = ERRMSG_COUNT + 1;
  148      % SET NEXT_ERR_NO = NEXT_ERR_NO + 1;
  149      %MEND;
  150
  151      %EJECT;

09:23 AUG 09 '97 Z8I_ERR_TXT.:Z8A6SI/Z8I_ERR_TXT.:Z8A6TUI                  8    
