PL6.E3A0      #001=Z8GX$IS_SSOP_INITV_ABLE File=Z8GX.:Z8A6TSI                    SAT 08/09/97 13:16 Page:1    
        1        1        /*M* Z8GX -- 3 Routines -- All Recursive */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7
        8        8
        9        9        /*P*   NAME: Z8GX$IS_SSOP_INITV_ABLE
       10       10
       11       11            PURPOSE: Determine if a subscript expression is a viable
       12       12                     candidate for reduction in strength optimization
       13       13                     via the INITV and INCRV ILL items.
       14       14
       15       15               NAME: Z8GX$INIT_SUBS
       16       16
       17       17            PURPOSE: Substitute in the loops FROM value for the
       18       18                     index variable, thus creating an INITV value.
       19       19
       20       20               NAME: Z8GX$SUBS_INCR
       21       21
       22       22            PURPOSE: Substitute in the loop's BY value for the index
       23       23                     variable, thus creating an INCRV value. Note that
       24       24                     zero must be substituted for other subscript
       25       25                     variables.
       26       26
       27       27             AUTHOR: John Heath.
       28       28
       29       29        */
       30       30
       31       31        %EJECT;
PL6.E3A0      #001=Z8GX$IS_SSOP_INITV_ABLE File=Z8GX.:Z8A6TSI                    SAT 08/09/97 13:16 Page:2    
       32       32        /*D*   NAME: Z8GX$IS_SSOP_INITV_ABLE
       33       33
       34       34        DESCRIPTION: Determine if a subscript expression is a viable
       35       35                     candidate for reduction in strength optimization
       36       36                     via the INITV and INCRV ILL items.
       37       37
       38       38                     To be considered viable, the expression must have
       39       39                     the following attributes:
       40       40
       41       41                       (1) It must be of the form V1*S1 + V2*S2 + ...
       42       42                           where Vi are subsript variables,
       43       43                             and Si are span constants.
       44       44
       45       45                       (2) One of the Vi must be the index variable of
       46       46                           a DO5 item.
       47       47
       48       48                       (3) None of the other Vi must be redefined within
       49       49                           the loop (DO5 ... DOEND).
       50       50
       51       51               CALL: CALL IS_SSOP_INITV_ABLE (EXPR$, NOPE, YEP) ;
       52       52
       53       53              INPUT: EXPR$ -- a ROP of SSOP expression to be analyzed.
       54       54
       55       55                     VARYING$ -- the index of the innermost DO5.
       56       56
       57       57             OUTPUT: NOPE -- '1'B means it's not INITVable.
       58       58
       59       59                     YEP -- '1'B means it's INITVable. (NOPE must be 0).
       60       60
       61       61        */
       62       62
       63       63        Z8GX$IS_SSOP_INITV_ABLE: PROC (EXPR$, NOPE, YEP) ;
       64       64
       65       65        %EQU PHASE = 3 ;
       66       66        %EQU Z8GX = 8724 ;
       67       67        %EQU MODULE = Z8GX ;
       68       68
PL6.E3A0      #001=Z8GX$IS_SSOP_INITV_ABLE File=Z8GX.:Z8A6TSI                    SAT 08/09/97 13:16 Page:3    
       69       69        %INCLUDE Z8A_DEFC ;
       70      137        %INCLUDE Z8A_SUPP_MAC ;
       71     2537        %INCLUDE Z8A_ETT ;
       72     2838        %INCLUDE Z8G_O3COM ;
       73     2878
       74     2879    1   DCL   EXPR$     PTR ;     /* Expression to analyze  */
       75     2880    1   DCL   LOP$      PTR ;     /* Fish Thru Conversions  */
       76     2881    1   DCL   NOPE      BIT (1) ALIGNED ; /* Not INITVable if on    */
       77     2882    1   DCL   YEP       BIT (1) ALIGNED ; /* INITVable if on and NOPE is off */
       78     2883    1   DCL   NO        BIT (1) ALIGNED ; /* Temporary bit to be or'd with NOPE */
       79     2884    1   DCL   YES       BIT (1) ALIGNED ; /* Temporary bit to be or'd with YEP  */
       80     2885
       81     2886        %EJECT;
PL6.E3A0      #001=Z8GX$IS_SSOP_INITV_ABLE File=Z8GX.:Z8A6TSI                    SAT 08/09/97 13:16 Page:4    
       82     2887    2          DO CASE (EXPR$->ETT.ID) ;
       83     2888
       84     2889    2            CASE (%ADI) ;
       85     2890
       86     2891    2              CALL Z8GX$IS_SSOP_INITV_ABLE (EXPR$->ETT.LOP, NOPE, YEP) ;
       87     2892
       88     2893    2              CALL Z8GX$IS_SSOP_INITV_ABLE (EXPR$->ETT.ROP, NO, YES) ;
       89     2894
       90     2895    2              NOPE = NOPE | NO ;
       91     2896    2              YEP  = YEP  | YES ;
       92     2897
       93     2898    2            CASE (%MPI, %MPI2) ;
       94     2899
       95     2900    2              LOP$ = EXPR$ -> ETT.LOP ;
       96     2901    2              IF LOP$->ETT.ID = %CVDI
       97     2902    2              THEN LOP$ = LOP$ -> ETT.LOP ;
       98     2903    2              IF LOP$->ETT.ID = %CVDSF
       99     2904    2              THEN LOP$ = LOP$ -> ETT.LOP ;
      100     2905
      101     2906    3              IF LOP$->ETT.ID = %BASE THEN DO ;
      102     2907    3                IF LOP$ = O3COM.VARYING$
      103     2908    3                THEN YES = '1'B ;
      104     2909    3              END ;
      105     2910    2              ELSE NOPE = '1'B ;
      106     2911
      107     2912    2              IF  EXPR$->ETT.ID = %MPI
      108     2913    2              AND EXPR$->ETT.ROP->ETT.ID ~= %TRIAD /* a constant */
      109     2914    2              THEN YES = '0'B ;
      110     2915
      111     2916    2              YEP = YEP | YES ;
      112     2917
      113     2918    2            CASE (%CVDI, %CVDSF) ;
      114     2919
      115     2920    2              CALL Z8GX$IS_SSOP_INITV_ABLE (EXPR$->ETT.LOP, NOPE, YEP) ;
      116     2921
      117     2922    2            CASE (%BASE) ;
      118     2923
PL6.E3A0      #001=Z8GX$IS_SSOP_INITV_ABLE File=Z8GX.:Z8A6TSI                    SAT 08/09/97 13:16 Page:5    
      119     2924    2              IF EXPR$ = O3COM.VARYING$
      120     2925    2              THEN NOPE = '1'B ;
      121     2926
      122     2927    2            CASE (ELSE) ;
      123     2928
      124     2929    2              NOPE = '1'B ;
      125     2930
      126     2931    2          END /* Case of EXPR$ ID */ ;
      127     2932
      128     2933    1          RETURN ;
      129     2934
      130     2935    1   END Z8GX$IS_SSOP_INITV_ABLE ;
      131     2936
      132     2937        %EOD;

