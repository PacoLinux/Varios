      217     7370    2              CALL Z8AF$FORME (FORME_ID, RESULT$, EXPR$->ETT.ROP, RESULT$) ;
      218     7371
      219     7372    2            CASE (%BASE) ;
      220     7373
      221     7374    2              IF EXPR$ = O3COM.VARYING$
      222     7375    2              THEN RESULT$ = O3COM.FROM$ ;
      223     7376    2              ELSE RESULT$ = EXPR$ ;
      224     7377
      225     7378    2            CASE (%TRIAD) ;
      226     7379
