PL6.E3A0      #002=Z8GX$INIT_SUBS File=Z8GX.:Z8A6TSI                             SAT 08/09/97 13:16 Page:25   
      158     7311        /*D*   NAME: Z8GX$INIT_SUBS
      159     7312
      160     7313               CALL: CALL Z8GX$INIT_SUBS (EXPR$, RESULT$) ;
      161     7314
      162     7315              INPUT: EXPR$ -- ROP of SSOP before substitution.
      163     7316                     GST$  -- Current GSA item.
      164     7317                     FROM$ -- Triad representing FROM value.
      165     7318                     VARYING$ -- BASE triad for VARYING identifier.
      166     7319
      167     7320             OUTPUT: RESULT$ -- Result of substituting FROM$ into
      168     7321                                the subscript expression for the
      169     7322                                loop index variable.
      170     7323
      171     7324        DESCRIPTION:: A typical ROP of SSOP will look like this::
      172     7325
      173     7326                                      ADI   (I*S1 + J*S2)
      174     7327                                     /   \
      175     7328                                  MPI     MPI
      176     7329                                 /   \   /   \
      177     7330                                I    S1 J    S2
      178     7331
      179     7332                       where I is the loop index,
      180     7333                             J is some variable,
      181     7334                         and S1 and S2 are spans.
      182     7335
      183     7336                       "I" and "J" will be BASE triads.  S1 and S2 will
      184     7337                       be TRIAD triads (pointing to constants).
      185     7338
      186     7339                       This routine will substitute the FROM identifier
      187     7340                       (or constant) from the loop for "I", and leave "J"
      188     7341                       as is.  The result (RESULT$) will be:
      189     7342
      190     7343                           FROM * S1  +  J * S2
      191     7344
      192     7345                       The INCR_SUBS routine substitutes the "BY"
      193     7346                       identifier (or constant) from the loop for
      194     7347                       "I", and substitutes zero for "J".  The
PL6.E3A0      #002=Z8GX$INIT_SUBS File=Z8GX.:Z8A6TSI                             SAT 08/09/97 13:16 Page:26   
      195     7348                       result (RESULT$) will be:
      196     7349
      197     7350                           BY * S1
      198     7351
      199     7352        */
      200     7353
      201     7354        %EJECT;
PL6.E3A0      #002=Z8GX$INIT_SUBS File=Z8GX.:Z8A6TSI                             SAT 08/09/97 13:16 Page:27   
      202     7355    2          DO CASE (EXPR$->ETT.ID) ;
      203     7356
      204     7357    2            CASE (%ADI, %MPI) ;
      205     7358
      206     7359    2              CALL Z8GX$INIT_SUBS (EXPR$->ETT.LOP, LOP$) ;
      207     7360    2              CALL Z8GX$INIT_SUBS (EXPR$->ETT.ROP, RESULT$) ;
      208     7361
      209     7362    2              FORME_ID = EXPR$->ETT.ID ;
      210     7363    2              CALL Z8AF$FORME (FORME_ID, LOP$, RESULT$, RESULT$) ;
      211     7364
      212     7365    2            CASE (%CVDI, %CVDSF, %MPI2) ;
