      152     7305    1   DCL   EXPR$     PTR ;    /* ROP of SSOP expression */
      153     7306    1   DCL   RESULT$   PTR ;    /* After substitution     */
      154     7307    1   DCL   LOP$      PTR ;    /* Temporary expression   */
      155     7308    1   DCL   FORME_ID   SBIN WORD ; /* Required to pass an ID to FORME */
