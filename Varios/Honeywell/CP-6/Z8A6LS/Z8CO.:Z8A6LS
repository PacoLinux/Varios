PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:3    
       64    13900        /*=============*/
       65    13901    1     GLOBAL_FATHER : PROC;
       66    13902        /*=============*/
       67    13903
       68    13904    2        P$ = O1COM.CURRENT_PROC$;
       69    13905
       70    13906    3        DO WHILE (P$ ~= ADDR(NIL));
       71    13907    3          IF P$ = CURRENT_ST$ -> SCALAR.PROC$ THEN
       72    13908    3            EXIT;
       73    13909
       74    13910    3          P$ = P$ -> PROC_.FATHER;
       75    13911    3        END; /* While */
       76    13912
       77    13913    2        IF P$ ~= ADDR(NIL) THEN
       78    13914    3          DO;
       79    13915    3            GLOBAL_FOUND = TRUE;
       80    13916    3            ST$$ = CURRENT_ST$;
       81    13917    3          END;
       82    13918
       83    13919    2   END GLOBAL_FATHER;
       84    13920        %EJECT;
PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:4    
       85    13921
       86    13922    1    NO_ERROR_FLAG = FALSE;
       87    13923    1    GOTO COMMON_CODE;
       88    13924    1   Z8CO$QUALIFY_VARIABLE_NO_ERROR: ENTRY (QUAL_ID$, ST$$) ALTRET;
       89    13925    1    NO_ERROR_FLAG = TRUE;
       90    13926    1   COMMON_CODE:
       91    13927    1    LOCAL_FOUND = FALSE;
       92    13928    1    GLOBAL_FOUND= FALSE;
       93    13929    1    LIST$       = QUAL_ID$ -> LX_LIST.HEAD$;
       94    13930    1    CURRENT_ST$ = LIST$ -> LX_TERMINAL.DNT$ -> DNT.ST$;
       95    13931    1    ST$$         = CURRENT_ST$;
       96    13932
       97    13933    1    IF CURRENT_ST$ = ADDR(NIL) THEN
       98    13934    2      DO;
       99    13935    2        IF NOT NO_ERROR_FLAG THEN
      100    13936    2          CALL Z8AE$ERROR (%E$UNDECLARED_DATA_NAME,
      101    13937    2                           LIST$ -> LX_TERMINAL.LINE,
      102    13938    2                           LIST$ -> LX_TERMINAL.COLUMN,
      103    13939    2                           LIST$ -> LX_TERMINAL.DNT$);
      104    13940
      105    13941    2        CALL RECOVERY;
      106    13942
      107    13943    2        ALTRETURN;
      108    13944    2      END;
      109    13945
      110    13946    2    DO UNTIL (CURRENT_ST$ = ADDR(NIL));
      111    13947
      112    13948    2   VALID_ST:
      113    13949    3      DO;
      114    13950
      115    13951    4        DO CASE(CURRENT_ST$ -> ST.ID);
      116    13952
      117    13953    4          CASE(%SCALAR, %ARRAY);
      118    13954
      119    13955    4            CURRENT_FATHER$ = CURRENT_ST$ -> SCALAR.QLT$ -> QLT.FATHER;
      120    13956
      121    13957    4          CASE(%SWITCH);
PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:5    
      122    13958    4            CURRENT_FATHER$ = CURRENT_ST$ -> SWITCH.QLT$ -> QLT.FATHER;
      123    13959
      124    13960    4          CASE(%CONDITION);
      125    13961
      126    13962    4            CURRENT_FATHER$ = CURRENT_ST$ -> CONDITION.QLT$ -> QLT.FATHER;
      127    13963
      128    13964    4          CASE(%BLOCK, %CONST, %DCB_, %PARA, %SECTION, %SEGMENT,
      129    13965    4               %SYMDEF_, %SYMREF_, %TEMP, %INTDEF);
      130    13966    4              EXIT VALID_ST;
      131    13967
      132    13968    4          CASE(ELSE);
      133    13969
      134    13970    4            CURRENT_FATHER$ = ADDR(NIL);
      135    13971    4        END;
      136    13972
      137    13973
      138    13974    4        DO WHILE (CURRENT_FATHER$ ~= ADDR(NIL) AND
      139    13975    4                  LIST$ -> LX_TERMINAL.LINK ~= ADDR(NIL));
      140    13976
      141    13977    4          IF CURRENT_FATHER$ -> QLT.ST$ -> SCALAR.DNT$ -> DNT.ST$
      142    13978    4             = LIST$ -> LX_TERMINAL.LINK -> LX_ITEM.DNT$ -> DNT.ST$ THEN
      143    13979    4            LIST$ = LIST$ -> LX_TERMINAL.LINK;
      144    13980
      145    13981    4          CURRENT_FATHER$ = CURRENT_FATHER$ -> QLT.FATHER;
      146    13982
      147    13983    4        END; /* While -- current_father$ ~= nil and list$->lx_item.next ~= nil */
      148    13984
      149    13985    3        IF LIST$ = QUAL_ID$ -> LX_LIST.TAIL$ THEN
      150    13986    4          DO;
      151    13987    4            IF LOCAL_FOUND THEN
      152    13988    4              IF CURRENT_ST$ -> VARIABLE.PROC$ = O1COM.CURRENT_PROC$ THEN
      153    13989    5                DO;
      154    13990    5                  IF NOT CURRENT_ST$ -> ST.DNT$ -> DNT.B.ERROR_VARIABLE THEN
      155    13991    5                    IF NOT NO_ERROR_FLAG THEN
      156    13992    5                      CALL Z8AE$ERROR (%E$AMBIGUOUS_REFERENCE,
      157    13993    5                                       QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.LINE,
      158    13994    5                                       QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.COLUMN,
PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:6    
      159    13995    5                                       QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.DNT$);
      160    13996    5                  ST$$ = QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.DNT$ -> DNT.ST$;
      161    13997    5                  ALTRETURN;
      162    13998    5                END;
      163    13999    4              ELSE
      164    14000                     /* Since a local guy was already found and we are already
      165    14001                        in a previous proc, then we can exit. */
      166    14002    4                RETURN;
      167    14003    4            ELSE          /* A local guy has not been found yet */
      168    14004    4              IF GLOBAL_FOUND THEN
      169    14005    4                IF CURRENT_ST$ -> VARIABLE.PROC$ = ST$$ -> VARIABLE.PROC$ THEN
      170    14006    5                  DO;
      171    14007                         /* Since a GLOBAL item was already found, and
      172    14008                            its PROC$ is the same as this one,
      173    14009                            then it is ambiguous                          */
      174    14010    5                    IF NOT CURRENT_ST$ -> ST.DNT$ -> DNT.B.ERROR_VARIABLE THEN
      175    14011    5                      IF NOT NO_ERROR_FLAG THEN
      176    14012    5                        CALL Z8AE$ERROR (%E$AMBIGUOUS_REFERENCE,
      177    14013    5                                         QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.LINE,
      178    14014    5                                         QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.
             14014                                                  COLUMN,
      179    14015    5                                         QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.DNT$)
             14015                                                  ;
      180    14016    5                    ST$$ = QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.DNT$ -> DNT.ST$;
      181    14017    5                    ALTRETURN;
      182    14018    5                  END;
      183    14019    4                ELSE
      184    14020                       /* Since we have already found a GLOBAL item,
      185    14021                          and the item we just found has a different
      186    14022                          PROC$, it must have been created before the
      187    14023                          one we have.  With these rules, we can therefore
      188    14024                          rule out any ambiguities                        */
      189    14025    4                  RETURN;
      190    14026    4              ELSE        /* A global item has not been found yet */
      191    14027    4                IF CURRENT_ST$ -> VARIABLE.PROC$ = O1COM.CURRENT_PROC$ THEN
      192    14028    5                  DO;
      193    14029    5                    LOCAL_FOUND = TRUE;
PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:7    
      194    14030    5                    ST$$ = CURRENT_ST$;
      195    14031    5                  END;
      196    14032    4                ELSE
      197    14033    5                  DO CASE (CURRENT_ST$ -> ST.ID);
      198    14034
      199    14035    5                    CASE (%SCALAR, %ARRAY,
      200    14036                               %SET_OF_FILE_DESCRIPTIONS;
      201    14041    5                          );
      202    14042    5                      IF CURRENT_ST$ -> VARIABLE.B.GLOBAL THEN
      203    14043    5                        CALL GLOBAL_FATHER ;
      204    14044
      205    14045    5                    CASE (%CLASS, %ALPHABET, %MNEMONIC, %SWITCH,
      206    14046    5                          %REALM, %DML_SET, %DML_KEY, %PRIVACY_KEY, %SYM_CHAR);
      207    14047    5                      CALL GLOBAL_FATHER;
      208    14048
      209    14049    5                    CASE (%CONDITION);
      210    14050    5                      IF CURRENT_ST$ -> CONDITION.B.GLOBAL THEN
      211    14051    5                        CALL GLOBAL_FATHER;
      212    14052
      213    14053    5                    CASE (ELSE);
      214    14054    5                      /* Do nothing */;
      215    14055
      216    14056    5                  END; /* CASE */
      217    14057    4          END;
      218    14058    3      END VALID_ST;
      219    14059
      220    14060    2        LIST$ = QUAL_ID$ -> LX_LIST.HEAD$;
      221    14061    2        CURRENT_ST$ = CURRENT_ST$ -> SCALAR.NEXT;
      222    14062    2    END; /* Until -- current_st$ = addr(nil) */
      223    14063
      224    14064    1    IF  (NOT LOCAL_FOUND)
      225    14065    1    AND (NOT GLOBAL_FOUND) THEN
      226    14066    2      DO;
      227    14067             %CONDITIONAL_ERROR (@LX_ITEM$ = QUAL_ID$ -> LX_LIST.HEAD$,
      228    14068                      @ERROR# = %E$DATA_NAME_QUAL_ERROR,
      229    14069                      @LINE   = QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.LINE,
      230    14070                      @COLUMN = QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.COLUMN,
PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:8    
      231    14071                      @PLUG1  = QUAL_ID$ -> LX_LIST.HEAD$ -> LX_TERMINAL.DNT$);
      232    14101
      233    14102    2        CALL RECOVERY;
      234    14103
      235    14104    2        ALTRETURN;
      236    14105    2      END;
      237    14106
      238    14107
      239    14108    1   END Z8CO$QUALIFY_VARIABLE;
      240    14109        %EOD;

