      617     9179        %INCLUDE Z8A_DNT ;
      618     9400        %INCLUDE Z8A_TYPES;
      619     9574        %INCLUDE Z8A_SRCH;
      620     9623        %INCLUDE Z8A_LIBRARY;
      621     9837        %INCLUDE Z8E_STACKS;
      622    12103        %INCLUDE Z8E_O1COM;
      623    12864
      624    12865    1   DCL Z8AS$SR_ETT        ENTRY (1);
PL6.E3A0      #005=Z8CO$CREATE_LIBRARY_BASE File=Z8CO.:Z8A6TSI                   SAT 08/09/97 11:05 Page:121  
      625    12866    1   DCL Z8AT$CREATE_TEMPORARY ENTRY (2);
      626    12867    1   DCL Z8AH$NEW_DNT ENTRY(3);
      627    12868
      628    12869    1   DCL LIBRARY_OFFSET_OPCODE     UBIN WORD;
      629    12870    1   DCL RETURN$                   PTR;
      630    12871
      631    12872    1   DCL SCALAR$                   PTR;
      632    12873    1   DCL SYMREF$                    PTR;
      633    12874    1   DCL NDNT$                       PTR;
      634    12875    1   DCL TABLE_NAME            CHAR (10);
      635    12876
      636    12877
      637    12878    1      IF LIBRARY_OFFSET_OPCODE = %OP_ASCII_TO_EBCDIC THEN
      638    12879    1        SUBSTR(TABLE_NAME, 0, 6) = 'U.ATOE';
      639    12880    1      ELSE
      640    12881    1      SUBSTR (TABLE_NAME, 0, 6) = 'U.ATOA';
      641    12882    1     CALL BINCHAR(SUBSTR(TABLE_NAME, 6, 4),
      642    12883    1                  LIBRARY_OFFSET_OPCODE);
      643    12884
      644    12885    1     CALL Z8AH$NEW_DNT (TABLE_NAME,
      645    12886    1                        10,
      646    12887    1                        NDNT$);
      647    12888
      648    12889    1     IF NDNT$ -> DNT.ST$ ~= ADDR(NIL) THEN
      649    12890    1       SCALAR$ = NDNT$ -> DNT.ST$;
      650    12891    1     ELSE
      651    12892    2       DO;
      652    12893    2          SYMREF$ = COMCOM.OPERATOR_SEGMENT$ -> ETT.LOP ;
      653    12894
      654    12895               %NEW_SCALAR (@ST$       = SCALAR$,
      655    12896                            @TYPE      = %TYA,
      656    12897                            @DNT$      = DNT$,
      657    12898                            @DESCL     = 512,
      658    12899                            @ELEMENTARY= TRUE,
      659    12900                            @COMPILER_CREATED = TRUE,
      660    12901                            @R         = %EXT_PROGRAM,
      661    12902                            @COUNT     = "SYMREF$ -> VARIABLE.COUNT",
PL6.E3A0      #005=Z8CO$CREATE_LIBRARY_BASE File=Z8CO.:Z8A6TSI                   SAT 08/09/97 11:05 Page:122  
      662    12903                            @SA        = LIBRARY_OFFSET_OPCODE) ;
      663    13103
      664    13104    2          NDNT$ -> DNT.ST$ = SCALAR$ ;
      665    13105    2       END;
      666    13106
      667    13107          %SRCH_ETT (@ID  = %BASE,
      668    13108                     @LOP = SCALAR$,
      669    13109                     @ROP = COMCOM.OPERATOR_SEGMENT$,
      670    13110                     @ETT$ = RETURN$) ;
      671    13125
      672    13126    1   END Z8CO$CREATE_LIBRARY_BASE;
      673    13127        %EOD;

