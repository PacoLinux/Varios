       37    13839
PL6.E3A0      #001=Z8CO$QUALIFY_VARIABLE File=Z8CO.:Z8A6TSI                      SAT 08/09/97 10:54 Page:2    
       38    13840    1   DCL P$                        PTR;
       39    13841    1   DCL LOCAL_FOUND               BIT(1) ALIGNED;
       40    13842    1   DCL GLOBAL_FOUND              BIT(1) ALIGNED;
       41    13843    1   DCL NO_ERROR_FLAG             BOOLEAN;
       42    13844    1   DCL 1 DESC                    ALIGNED,
       43    13845             %DESCRIPTOR              (LVL = 2, ENDING = YES);
