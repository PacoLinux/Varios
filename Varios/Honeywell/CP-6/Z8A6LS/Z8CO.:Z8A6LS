      324    11237
      325    11238    2        DO CASE(ID$ -> LX_ITEM.ID);
      326    11239    2          CASE(%LX_TERMINAL);
      327    11240    2            IF NOT (ID$ -> LX_TERMINAL.DNT$ -> DNT.B.ALPHA_LITERAL
      328    11241    2            OR ID$ -> LX_TERMINAL.DNT$ -> DNT.B.FIGURATIVE_CONSTANT) THEN
      329    11242    3              DO;
      330    11243                     %CONDITIONAL_ERROR (@LX_ITEM$ = ID$,
      331    11244                                         @ERROR# = %E$MUST_BE_DISPLAY,
      332    11245                                         @LINE = ID$ -> LX_ETT_ITEM.LINE,
      333    11246                                         @COLUMN = ID$ -> LX_ETT_ITEM.COLUMN);
      334    11276    3                ALTRETURN;
