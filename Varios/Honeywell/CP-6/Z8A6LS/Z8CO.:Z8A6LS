      346    11288                           /* OK */
      347    11289
      348    11290    4                    CASE (ELSE) ; /* %FD, ... */
      349    11291                           %CONDITIONAL_ERROR (@LX_ITEM$ = ID$,
      350    11292                                               @ERROR# = %E$MUST_BE_DISPLAY,
