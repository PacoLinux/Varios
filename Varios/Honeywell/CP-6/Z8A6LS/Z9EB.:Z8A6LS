       82    16966        /*----------------*/
       83    16967    1     BUILD_USEP_TABLE : PROC ( UCW_CONST);
       84    16968        /*----------------*/
       85    16969        /*I*
       86    16970             NAME: BUILD_USEP_TABLE
       87    16971             PURPOSE:
       88    16972                   Preoffs and ptr_sets items into the USEP table.
       89    16973             CALL:
       90    16974                   CALL BUILD_USEP_TABLE ( UCW_CONST );
       91    16975             PARAMETERS:
       92    16976                     UCW_CONST - Word constant describing the specific use
       93    16977             DATA:
       94    16978                     O1COM.USEP$  - Pointer to USEP table
       95    16979                     O1COM.USEP_NUMBER - Number of entries into table
       96    16980             DESCRIPTION:
       97    16981                    BUILD_USEP_TABLE  creates ILLs that preoff the constant
       98    16982                    and ptr_set the use section into the USEP table.  It
       99    16983                    also updates the table entry count.
      100    16984        */
      101    16985    2       DCL UCW_CONST   SBIN WORD;
      102    16986    2       DCL USEP_LENGTH UBIN WORD STATIC INIT(1);
      103    16987        %EJECT;
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:5    
      104    16988            %NEW_CONST ( @ST$ = ST$);
      105    17029    2       ST$ -> CONST.CONSTI.WORD1 = UCW_CONST;
      106    17030
      107    17031            %NEW_ILL( @ID = %PREOFF,
      108    17032                      @LOP  = O1COM.USEP$,
      109    17033                      @ROP  = ST$,
      110    17034                      @N    = 1,
      111    17035                      @OFFSET = USEP_LENGTH);
      112    17414
      113    17415    2       USEP_LENGTH = USEP_LENGTH + 1;
      114    17416            %NEW_ILL( @ID   = %P_A_PRESET,
      115    17417                      @OFFSET = USEP_LENGTH,
      116    17418                      @TO   = O1COM.USEP$,
      117    17419                      @ROPCON = 0,
      118    17420                      @LOP  = USE_SECT$);
      119    17799
      120    17800    2       O1COM.USEP_NUMBER = O1COM.USEP_NUMBER + 1;
      121    17801    2       USEP_LENGTH = USEP_LENGTH + 1;
      122    17802                 /*V*  USEP_LENGTH IS INCREMENTED BY 1  */
      123    17803                 /*V*  AFTER EACH ILL BECAUSE A LENGTH  */
      124    17804                 /*V*  OF 1 HAS A ZERO OFFSET.          */
      125    17805            %NEW_USE_LTS ( @POINTER = LTS$);
      126    17911    2       LTS$ -> USE.PROC_CHAIN$ = COMCOM.USE_PROC_CHAIN$;
      127    17912    2       LTS$ -> USE.PROC_LABEL$ = USE_SECT$;
      128    17913    2       COMCOM.USE_PROC_CHAIN$ = LTS$;
      129    17914
      130    17915    2   END BUILD_USEP_TABLE;
      131    17916        %EJECT;
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:6    
      132    17917        /*----------------*/
      133    17918    1     USE_DB_STATEMENT : PROC ;
      134    17919        /*----------------*/
      135    17920        /*I*
      136    17921             NAME: USE_DB_STATEMENT
      137    17922             PURPOSE:
      138    17923                   Sets up constant for DB-EXCEPTION and checks for errors
      139    17924             CALL:
      140    17925                   CALL USE_DB_STATE
      141    17926             DATA:
      142    17927                  None.
      143    17928             DESCRIPTION:
      144    17929                   USE_DB_STATEMENT builds the DB-EXCEPTION words for the
      145    17930                   USEP table.  There are three types of exceptions:  ALL,
      146    17931                   OTHER, and indicator.
      147    17932        */
      148    17933    2       DCL   1   DB_EXCEPTION     STATIC,
      149    17934    2               2   ALL        BOOLEAN,
      150    17935    2               2   OTHER      BOOLEAN,
      151    17936    2               2   INDICATOR  BOOLEAN;
      152    17937    2       DCL  FIRST_DB_EXCEPTION   BOOLEAN  STATIC  INIT(%TRUE);
      153    17938    2       DCL  IDENT$     PTR;
      154    17939    2       DCL  RETURN$    PTR;
      155    17940    2       DCL  UCW_WORD     SBIN  WORD;
      156    17941    2       DCL  1  UCW    REDEF  UCW_WORD,
      157    17942    2               2   TYPE    UNAL UBIN(9),
      158    17943    2               2   STATUS_CODE   UNAL UBIN(27);
      159    17944
      160    17945    2       DCL  ERROR_FLAG     BOOLEAN;
      161    17946    2       DCL  1  TYPE_DESC      ALIGNED,
      162    17947                    %TYPE_DESC;
      163    17981        %EJECT;
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:7    
      164    17982    2       UCW.TYPE = 3;
      165    17983    2       IF FIRST_DB_EXCEPTION = %TRUE     THEN      /*V*  Build USEP   */
      166    17984    3        DO;
      167    17985    3           FIRST_DB_EXCEPTION = %FALSE;
      168    17986    3           DB_EXCEPTION.ALL = %FALSE;
      169    17987    3           DB_EXCEPTION.OTHER = %FALSE;
      170    17988    3           DB_EXCEPTION.INDICATOR = %FALSE;
      171    17989    3         END;
      172    17990
      173    17991    3       DO CASE (STATEMENT_TYPE);
      174    17992
      175    17993    3          CASE (%DB_EXCEPTION_ALL);
      176    17994    3               IF (DB_EXCEPTION.ALL = %TRUE) OR (DB_EXCEPTION.OTHER = %TRUE) OR
      177    17995    3            (DB_EXCEPTION.INDICATOR = %TRUE)    THEN
      178    17996    3                   CALL Z8AE$ERROR( %E$DB_EXCEPTION_ALL_ERROR);
      179    17997    3               ELSE
      180    17998    4                 DO;
      181    17999    4                   DB_EXCEPTION.ALL = %TRUE;
      182    18000    4                   UCW.STATUS_CODE = 0;
      183    18001    4                   CALL BUILD_USEP_TABLE(UCW_WORD);
      184    18002    4                 END;
      185    18003
      186    18004    3           CASE (%DB_EXCEPTION_OTHER);
      187    18005
      188    18006    3               IF DB_EXCEPTION.OTHER = %TRUE   THEN
      189    18007    3                   CALL Z8AE$ERROR( %E$DB_EXCEPT_OTHER_ERROR);
      190    18008    3               ELSE
      191    18009    3                   IF DB_EXCEPTION.ALL = %TRUE   THEN
      192    18010    3                       CALL Z8AE$ERROR( %E$DB_EXCEPTION_ALL_ERROR);
      193    18011    3                   ELSE
      194    18012    4                    DO;
      195    18013
      196    18014    4                       DB_EXCEPTION.OTHER = %TRUE;
      197    18015    4                       UCW.STATUS_CODE = 0;
      198    18016    4                       CALL BUILD_USEP_TABLE(UCW_WORD);
      199    18017    4                     END;
      200    18018
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:8    
      201    18019    3           CASE (%DB_EXCEPTION_INDICATOR);
      202    18020    3               IF DB_EXCEPTION.ALL = %TRUE    THEN
      203    18021    3                   CALL Z8AE$ERROR(%E$DB_EXCEPTION_ALL_ERROR);
      204    18022    3               ELSE
      205    18023    4                 DO;
      206    18024    4                   IDENT$ = LIST$ -> LX_LIST.HEAD$;
      207    18025
      208    18026    5                   DO WHILE IDENT$ ~= ADDR(NIL);
      209    18027    5                       IF IDENT$ -> LX_TERMINAL.DNT$ -> DNT.B.DUPLICATE = %TRUE   THEN
      210    18028    5                         CALL Z8AE$ERROR( %E$DB_EXCEPT_DUPLICATE,
      211    18029    5                                           IDENT$ -> LX_TERMINAL.LINE,
      212    18030    5                                           IDENT$ -> LX_TERMINAL.COLUMN);
      213    18031    5                       ELSE
      214    18032    6                       DO;
      215    18033    6                         IDENT$ -> LX_TERMINAL.DNT$ -> DNT.B.DUPLICATE = %TRUE;
      216    18034    6                         IF (IDENT$ -> LX_TERMINAL.DNT$ -> DNT.TEXT.LENGTH ~= 9) OR
      217    18035    6                           (IDENT$->LX_TERMINAL.DNT$->DNT.TEXT.NAME='"0000000"') THEN
      218    18036    6                             CALL Z8AE$ERROR( %E$DB_EXCEPT_NOT_VALID,
      219    18037    6                                            IDENT$->LX_TERMINAL.LINE,
      220    18038    6                                         IDENT$->LX_TERMINAL.COLUMN);
      221    18039    6                         ELSE
      222    18040    7                           DO;
      223    18041
      224    18042    7                             DB_EXCEPTION.INDICATOR = %TRUE;
      225    18043    7                             TYPE_DESC.TYPE = %TYI;
      226    18044
      227    18045    7                             CALL Z8EC$NEW_CONST_WITH_TYPE_DESC( TYPE_DESC,
      228    18046    7                                                                 IDENT$,
      229    18047    7                                                                 RETURN$)
      230    18048    7                               WHENALTRETURN
      231    18049    8                               DO;
      232    18050    8                                 ERROR_FLAG = %TRUE;
      233    18051    8                               END;
      234    18052    7                             IF ERROR_FLAG = %TRUE   THEN
      235    18053    7                                 ERROR_FLAG = %FALSE;
      236    18054    7                             ELSE
      237    18055    8                               DO;                     /*V* No error in converstion  */
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:9    
      238    18056    8                                 UCW_WORD = RETURN$->ETT.LOP->CONST.CONSTI.WORD1;
      239    18057    8                                 UCW.TYPE = 3;
      240    18058    8                                 CALL BUILD_USEP_TABLE(UCW_WORD);
      241    18059    8                               END;
      242    18060    7                           END;
      243    18061    6                       END;
      244    18062    5                       IDENT$ = IDENT$ -> LX_TERMINAL.LINK;
      245    18063    5                   END;      /*V*  WHILE   */
      246    18064    4                 END;
      247    18065    3       END;        /*V*  CASE STATEMENT_TYPE    */
      248    18066    2   END USE_DB_STATEMENT;
      249    18067        %EJECT;
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:10   
      250    18068        /*-----------*/
      251    18069    1     USE_PRIVACY :   PROC ;
      252    18070        /*-----------*/
      253    18071        /*I*
      254    18072             NAME: USE_PRIVACY
      255    18073             PURPOSE:
      256    18074                   Handles the USE for PRIVACY statement
      257    18075             CALL:
      258    18076                   CALL USE_PRIVACY
      259    18077             DATA:
      260    18078                  None.
      261    18079             DESCRIPTION:
      262    18080                     USE_PRIVACY determines what type of privacy phrase the
      263    18081                     statement is.  It can be for field items, records, or sets.
      264    18082                     It makes sure the verb specfied is legal for the type of
      265    18083                     phrase.  Also duplicate conditions are checked for.  It calls
      266    18084                     routines to check for these conditions. The USEP table entry
      267    18085                     is built by this routine.
      268    18086        */
      269    18087    2       DCL WORD_LIST$  PTR;
      270    18088    2       DCL FOR_OPTION$ PTR;
      271    18089    2       DCL WORD$       PTR;
      272    18090    2       DCL PRIVACY_PHRASE$ PTR;
      273    18091    2       DCL HEAD$       PTR;
      274    18092    2       DCL IDENT_ST$   PTR;
      275    18093    2       DCL IDENT$      PTR;
      276    18094    2       DCL RVA_TYPE    UBIN(18);
      277    18095    2       DCL VERB_COPY    UBIN(7);
      278    18096
      279    18097    2       DCL DUPLICATE_FLAG  BOOLEAN;
      280    18098    2       DCL VERB        UBIN(7);
      281    18099
      282    18100    2       DCL UCW_CONST   SBIN;
      283    18101    2       DCL 1 UCW_PRIVACY   REDEF   UCW_CONST,
      284    18102    2             2 TYPE        UBIN(9) UNAL,
      285    18103    2             2 VERBS       UBIN(7) UNAL,
      286    18104    2             2 MBZ         UBIN(1) UNAL,
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:11   
      287    18105    2             2 SS          UBIN(1) UNAL,
      288    18106    2             2 R           UBIN(18) UNAL;
      289    18107
      290    18108    2       DCL 1 VERB_ ,
      291    18109    2             2  VERBS   UBIN(7) UNAL,
      292    18110    2             2  LIST    REDEF VERBS,
      293    18111    2               3 GET       BOOLEAN,
      294    18112    2               3 MODIFY    BOOLEAN,
      295    18113    2               3 STORE     BOOLEAN,
      296    18114    2               3 ERASE_    BOOLEAN,
      297    18115    2               3 CONNECT   BOOLEAN,
      298    18116    2               3 DISCONNECT BOOLEAN,
      299    18117    2               3 FIND      BOOLEAN,
      300    18118    2             2  *          UBIN(11);
