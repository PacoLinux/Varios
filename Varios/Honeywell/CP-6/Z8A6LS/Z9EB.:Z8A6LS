        7        7        /*P*
        8        8             NAME: Z8EB_SI
        9        9             PURPOSE:
       10       10                   Builds calls to the IDS routines for DML statements.
       11       11             DESCRIPTION:
       12       12                    Checks for statement errors, builds RVA table and USEP
       13       13                    table, builds parameter lists for calls to IDS routines.
       14       14        */
       15       15        /*F*
       16       16             NAME: Z9EB$DML_USE
       17       17             PURPOSE:
       18       18                    Process the DML USE statement.
       19       19             DESCRIPTION:
       20       20                   Builds the USEP table for USE for DB-EXCEPTION and USE for
       21       21                   PRIVACY.
       22       22        */
       23       23        /*D*
       24       24             NAME: Z9EB$DML_USE
       25       25             CALL:
       26       26                   CALL Z9EB$DML_USE ( LIST$, STATEMENT_TYPE, USE_SECT$, USE_TYPE);
       27       27             PARAMETERS:
       28       28                   LIST$  - Pointer to lx list of status codes or privacy on words
       29       29                   STATEMENT_TYPE   - Type of DB-EXCEPTION
       30       30                   USE_SECT$  - Pointer to use section
       31       31                   USE_TYPE  - Type of USE statement
       32       32             INTERFACE:
       33       33                   Z8AE$ERROR
       34       34                   Z8EC$NEW_CONST_WITH_TYPE_DESC
       35       35                   Z9EB$BUILD_RVA_TABLE
       36       36
       37       37             ENVIRONMENT:
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:2    
       38       38                  None.
       39       39             INPUT:
       40       40                   LIST$
       41       41                   STATEMENT_TYPE
       42       42                   USE_SECT$
       43       43                   USE_TYPE
       44       44
       45       45             OUTPUT:
       46       46                   None.
       47       47
       48       48             DESCRIPTION:
       49       49                    Z9EB$DML_USE tests for PRIVACY or DB-EXCEPTION and
       50       50                    goes to the appropiate internal routines.
       51       51        */
       52       52        /*===========*/
       53       53          Z9EB$DML_USE : PROC (LIST$,STATEMENT_TYPE,USE_SECT$,USE_TYPE);
       54       54        /*============*/
       55       55
       56       56    1       DCL  LIST$    PTR;
       57       57    1       DCL STATEMENT_TYPE      SBIN;
       58       58    1       DCL  USE_SECT$   PTR;
       59       59    1       DCL  USE_TYPE    SBIN;
       60       60
       61       61            %EQU PHASE = 1;
