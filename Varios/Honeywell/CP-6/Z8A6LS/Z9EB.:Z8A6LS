     1398    15683    1     MEMBERSHIP :  PROC(RECORD_ST$,SET_ST$,CODE)  ALTRET;
     1399    15684        /*==========*/
     1400    15685        /*I*
     1401    15686             NAME: MEMBERSHIP
     1402    15687             PURPOSE:
     1403    15688                   Determine record set membership conditions.
     1404    15689             CALL:
     1405    15690                   Call MEMBERSHIP( RECORD_ST$, SET_ST$)
     1406    15691             PARAMETERS:
     1407    15692                    RECORD_ST$  - Symbol table pointer to record
     1408    15693                    SET_ST$  - SYMBOL table pointer to set item
     1409    15694             DATA:
     1410    15695                  None.
     1411    15696             DESCRIPTION:
     1412    15697                    The LTS  of the record is looked at to determine if the
     1413    15698                    set has the correct membership qualification.
     1414    15699        */
     1415    15700    2   DCL RECORD_ST$  PTR;
     1416    15701    2   DCL SET_ST$     PTR;
     1417    15702    2   DCL CODE        CHAR(6);
     1418    15703
     1419    15704        %GET_LTS (@ID = %DML_REC, @ST$ = RECORD_ST$, @RETURN$ = LTS$);
     1420    15747
     1421    15748    3   DO WHILE LTS$ ~= ADDR(NIL);
     1422    15749
     1423    15750    3       IF LTS$ -> DML_REC.SET_RELATIONSHIP = %TRUE    THEN
     1424    15751    3           IF SET_ST$ -> DML_ITEM.INTERNAL_NAME = LTS$ -> DML_REC.SET_NAME   THEN
     1425    15752    3             IF LTS$  -> DML_REC.SET_INFO.MEMBERSHIP = %OWNER  THEN
     1426    15753    3               IF CODE = %DBCON    THEN
     1427    15754    3                 RETURN;
     1428    15755    3               ELSE IF CODE = %DBFRS THEN /* FIND, format 4 or 5 */
     1429    15756    3                 IF CODE_TYPE = %SET_CALL AND
     1430    15757    3                     OPTION$ -> LX_ITEM.SYMBOL = %RSV_WRD_CURRENT THEN
     1431    15758    3                   RETURN; /* Format 5, owner of set is legal */
     1432    15759    3                 ELSE
     1433    15760    3                   ALTRETURN;
PL6.E3A0      #005=Z9EB$DML_STATEMENT File=Z9EB.:Z8A6TSI                         SAT 08/09/97 13:50 Page:228  
     1434    15761    3               ELSE
     1435    15762    3                 ALTRETURN;
     1436    15763    3             ELSE
     1437    15764    3               RETURN;
     1438    15765
     1439    15766    3       LTS$ = LTS$ -> DML_REC.EXTEND$;
     1440    15767    3   END;
     1441    15768
     1442    15769    2   ALTRETURN;
     1443    15770    2   END MEMBERSHIP;
     1444    15771        %EJECT;
