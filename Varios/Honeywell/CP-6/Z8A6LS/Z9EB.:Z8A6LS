      335    18153        /*------------*/
      336    18154    2     RECORD_CHECK :     PROC   ;
      337    18155        /*------------*/
      338    18156
      339    18157    3       IF WORD_LIST$ -> LX_ITEM.ID = %LX_TERMINAL AND WORD_LIST$ -> LX_ITEM.SYMBOL = %
             18157                EMPTY         THEN
      340    18158    3         IF PRIVACY_RECORD_ALL = %FALSE  THEN
      341    18159    4           DO;
      342    18160    4             VERB = %ALL_RECORD_VERBS;
      343    18161    4             PRIVACY_RECORD_ALL = %TRUE;
      344    18162    4             RETURN;
      345    18163    4           END;
      346    18164
      347    18165    3       IF PRIVACY_RECORD_ALL = %TRUE         THEN
      348    18166    4         DO;
      349    18167    4           CALL Z8AE$ERROR(%E$DUPLICATE_RECORD_ALL,
      350    18168    4                           WORD_LIST$ -> LX_LIST.HEAD$ -> LX_ITEM.LINE,
      351    18169    4                           WORD_LIST$ -> LX_LIST.HEAD$ -> LX_ITEM.COLUMN);
      352    18170    4           RETURN;
      353    18171    4         END;
      354    18172
      355    18173    3       DUPLICATE_FLAG = %FALSE;
      356    18174    3       WORD$ = WORD_LIST$ -> LX_LIST.HEAD$;
      357    18175    4       DO WHILE WORD$ ~= ADDR(NIL);
      358    18176
      359    18177    5         DO CASE(WORD$ -> LX_ITEM.SYMBOL);
      360    18178
      361    18179    5           CASE(%RSV_WRD_MODIFY);
      362    18180    5               IF MODIFY_RECORD_ALL = %FALSE        THEN
      363    18181    6                 DO;
      364    18182    6                   MODIFY_RECORD_ALL = %TRUE;
      365    18183    6                   VERB = VERB + %MODIFY;
      366    18184    6                 END;
      367    18185    5               ELSE
      368    18186    5                   DUPLICATE_FLAG = %TRUE;
      369    18187
      370    18188    5           CASE(%RSV_WRD_FIND);
PL6.E3A0      #001=Z9EB$DML_USE File=Z9EB.:Z8A6TSI                               SAT 08/09/97 13:43 Page:14   
      371    18189    5               IF FIND_RECORD_ALL = %FALSE        THEN
      372    18190    6                 DO;
      373    18191    6                   FIND_RECORD_ALL = %TRUE;
      374    18192    6                   VERB = VERB + %FIND;
      375    18193    6                 END;
      376    18194    5               ELSE
      377    18195    5                   DUPLICATE_FLAG = %TRUE;
      378    18196
      379    18197    5           CASE(%RSV_WRD_GET);
      380    18198    5               IF GET_RECORD_ALL = %FALSE        THEN
      381    18199    6                 DO;
      382    18200    6                   GET_RECORD_ALL = %TRUE;
      383    18201    6                   VERB = VERB + %GET;
      384    18202    6                 END;
