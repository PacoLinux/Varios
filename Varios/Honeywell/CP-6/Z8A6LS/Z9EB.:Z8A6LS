     1153    14362    1       RVA_ENTRY.TYPE = RVA_TYPE;
     1154    14363    2       DO CASE( RVA_TYPE );
     1155    14364
     1156    14365    2           CASE(%RVA_NULL);
     1157    14366    2               RVA_ENTRY.NAME = 0;
     1158    14367    2               O1COM.NILL_RVA = %TRUE;
     1159    14368
     1160    14369    2           CASE(%RVA_SET, %RVA_REALM);
     1161    14370    2               RVA_ENTRY.NAME = ITEM$ -> DML_ITEM.INTERNAL_NAME;
     1162    14371    2               ITEM$ -> DML_ITEM.B.DML_RVA = %TRUE;
     1163    14372
     1164    14373    2           CASE(%RVA_RECORD, %RVA_FIELD );
     1165    14374    2               IF NOT ITEM$ -> SCALAR.B.DML_UWA  THEN       /*#33404*/
     1166    14375    2                 CALL Z8AE$ERROR(%E$ITEM_NOT_IN_SCHEMA,
     1167    14376    2                                 COMCOM.CURRENT.LINE);
     1168    14377    2               ELSE
     1169    14378    2                 ITEM$ -> SCALAR.B.DML_RVA = %TRUE;
     1170    14379
     1171    14380    2               RVA_ENTRY.NAME = ITEM$ -> SCALAR.SA * 8;   /*V*  WORD TO HALF BYTE   */
     1172    14381    3               DO CASE ( ITEM$ -> SCALAR.TYPE );
     1173    14382
     1174    14383    3                   CASE (%TYD);
     1175    14384    3                     IF ITEM$ -> SCALAR.NDSC.TN = '1'B  THEN
     1176    14385    3                       RVA_ENTRY.NAME = RVA_ENTRY.NAME + ITEM$ -> SCALAR.NDSC.CN;
     1177    14386    3                     ELSE
     1178    14387    3                       RVA_ENTRY.NAME = RVA_ENTRY.NAME + (ITEM$ -> SCALAR.ADSC.CN * 2);
     1179    14388
     1180    14389    3                   CASE (%TYA);
     1181    14390    3                       RVA_ENTRY.NAME = RVA_ENTRY.NAME + (ITEM$ -> SCALAR.ADSC.CN * 2);
     1182    14391
     1183    14392    3                   CASE (ELSE);
     1184    14393    3                       RVA_ENTRY.NAME = RVA_ENTRY.NAME + (ITEM$ -> SCALAR.ADSC.CN * 2);
     1185    14394
     1186    14395    3               END;     /*V*     CASE ITEM$ -> SCALAR.TYPE    */
     1187    14396
     1188    14397    2       END;             /*V*     CASE  TYPE                 */
     1189    14398
