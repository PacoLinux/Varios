  171      %MACRO PARSE_SYMREFS;
  172      DCL 1 PRODR(0:%PRODR_LEN) UNAL SYMREF READONLY,
  173            2 GOAL UBIN(%BITS_GOAL) UNAL,
  174            2 POPSTACK UBIN(%BITS_PSS) UNAL,
  175            2 SEMANTIC UBIN(%BITS_SEM) UNAL;
  176      DCL 1 ACTION_COL(0:%ACTION_COL_LEN) UNAL SYMREF READONLY,
  177            2 DEFAULT SBIN(%BITS_ASDEF) UNAL,
  178            2 BASE UBIN(%BITS_ASBAS) UNAL;
  179      DCL 1 ACTION_ROW(0:%ACTION_ROW_LEN) UNAL SYMREF READONLY,
  180            2 NEXT UBIN(%BITS_ANCNXT) UNAL,
  181            2 CHECK UBIN(%BITS_ANCCHK) UNAL;
  182      DCL 1 GOTO_COL(0:%GOTO_COL_LEN) UNAL SYMREF READONLY,
  183            2 DEFAULT UBIN(%BITS_GTDEF) UNAL,
  184            2 BASE UBIN(%BITS_GTBAS) UNAL;
  185      DCL 1 GOTO_ROW(0:%GOTO_ROW_LEN) UNAL SYMREF READONLY,
09:08 AUG 09 '97 LRXDPDA_SUBS.:Z8A6SI                                      6    
  186            2 NEXT UBIN(%BITS_GNCNXT) UNAL,
  187            2 CHECK UBIN(%BITS_GNCCHK) UNAL;
  188      %MEND;
  189      %MACRO TSTRING_SYMREFS;
  190      DCL 1 TERM_STRING(0:%TERM_STRING_LEN) UNAL SYMREF READONLY,
  191            2 INDX UBIN(%BITS_T_S_INDX) UNAL,
  192            2 LEN UBIN(%BITS_T_S_LEN) UNAL;
  193      DCL TSTRING_SPACE SYMREF READONLY CHAR(%TSTRING_SPACE_LEN_CHARS);
  194      %MEND;
