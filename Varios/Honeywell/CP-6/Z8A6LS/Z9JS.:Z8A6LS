PL6.E3A0      #002=Z9JS$SCAN_POINTER File=Z9JS.:Z8A6TSI                          SAT 08/09/97 14:05 Page:12   
       66       45        Z9JS$SCAN_POINTER:  PROC(POINTER$);
       67       46
       68       47            %EQU PHASE = 6;
       69       48            %INCLUDE B_SEGIDS_C;
       70      587            %INCLUDE Z8A_DEFC;
       71      655
       72      656
       73      657
       74      658            %EQU LS0   = 'LS0';
       75      659            %EQU LS4   = 'LS4';
       76      660            %EQU LS5   = 'LS5';
       77      661            %EQU LS6   = 'LS6';
       78      662            %EQU LS7   = 'LS7';
       79      663            %EQU LS8   = 'LS8';
       80      664            %EQU LS9   = 'LS9';
       81      665            %EQU LS10  = 'LS10';
       82      666            %EQU LS11  = 'LS11';
       83      667            %EQU BLANK = ' ';
       84      668
       85      669    1       DCL I          SBIN WORD;
       86      670    1       DCL SEGID      CHAR(4);
       87      671    1       DCL OBUF       CHAR(120) EXT;
       88      672    1       DCL INBUF      CHAR(80)  EXT;
       89      673    1       DCL TEMP_PTR   CHAR(6);
       90      674    1       DCL POINTER$   PTR;
       91      675    1       DCL 1 POINTER  REDEF POINTER$,
       92      676    1           2 WD_OFF   UBIN HALF UNAL,
       93      677    1           2 C        BIT(2)    UNAL,
       94      678    1           2 B        BIT(4)    UNAL,
       95      679    1           2 SEGID    BIT(12)   UNAL;
       96      680
       97      681    1       DCL START      SBIN WORD;
       98      682    1       DCL PTR_FOUND  BIT(1);
       99      683    1       DCL PTR_LEN    UBIN WORD;
      100      684
      101      685
      102      686        %EJECT;
PL6.E3A0      #002=Z9JS$SCAN_POINTER File=Z9JS.:Z8A6TSI                          SAT 08/09/97 14:05 Page:13   
      103      687            /*V*  VALIDATE THE WORD OFFSET PORTION OF THE POINTER. */
      104      688
      105      689    1       PTR_LEN = 1;
      106      690    1       PTR_FOUND = FALSE;
      107      691    1       POINTER$ = ADDR(NIL);
      108      692    2   LOOP:  DO I = 0 TO 79;
      109      693    3            DO SELECT (SUBSTR (INBUF,I,1) );
      110      694    3              SELECT ( '0', '1', '2', '3', '4', '5', '6', '7');
      111      695    3                IF NOT  PTR_FOUND THEN
      112      696    4                  DO;
      113      697    4                    PTR_FOUND = TRUE;
      114      698    4                    START = I;
      115      699    4                  END;
      116      700    3                ELSE
      117      701    3                  PTR_LEN = PTR_LEN + 1;
      118      702
      119      703    3              SELECT ('.');
      120      704                     /* Ignore this symbol */
      121      705
      122      706    3              SELECT (' ');
      123      707    3                IF PTR_FOUND THEN
      124      708    3                  EXIT LOOP;
      125      709    3              SELECT (',');
      126      710    3                IF PTR_FOUND THEN
      127      711    3                  EXIT LOOP;
      128      712    3                ELSE
      129      713    4                  DO;
      130      714    4                    POINTER$ = ADDR(NIL);
      131      715    4                    RETURN;
      132      716    4                  END;
      133      717
      134      718
      135      719            /* ALL THE REMAINING CHARACTERS ARE ILLEGAL, SO FLAG AN ERROR
      136      720               AND REPROMPT FOR POINTER */
      137      721
      138      722    3              SELECT ( ELSE );
      139      723    3                POINTER$ = ADDR(NIL);
PL6.E3A0      #002=Z9JS$SCAN_POINTER File=Z9JS.:Z8A6TSI                          SAT 08/09/97 14:05 Page:14   
      140      724    3                RETURN;
      141      725    3            END;  /* SELECT */
      142      726    2          END; /* INDEX */
      143      727
      144      728    1       IF PTR_LEN < 6 THEN
      145      729    2         DO;
      146      730    2           TEMP_PTR = '000000';
      147      731    2           SUBSTR( TEMP_PTR, 6 - PTR_LEN, PTR_LEN ) = SUBSTR( INBUF,START,PTR_LEN );
      148      732    2           CALL CONVERT_TO_PTR$( TEMP_PTR );
      149      733    2         END;
      150      734    1       ELSE
      151      735    1         CALL CONVERT_TO_PTR$( SUBSTR( INBUF,START,PTR_LEN ) );
      152      736        %EJECT;
PL6.E3A0      #002=Z9JS$SCAN_POINTER File=Z9JS.:Z8A6TSI                          SAT 08/09/97 14:05 Page:15   
      153      737            /*V*  VALIDATE THE SEGID PORTION OF THE REQUESTED POINTER,
      154      738                  IF BLANK -- SET THE DEFAULT SEGID
      155      739            */
      156      740    1       IF SUBSTR (INBUF, START + PTR_LEN + 1, 1)  = '$' THEN
      157      741    1         SEGID = SUBSTR(INBUF,START+PTR_LEN+2,4);
      158      742    1       ELSE
      159      743    1         IF SUBSTR(INBUF,START+PTR_LEN,79 -(START + PTR_LEN) ) = ' ' THEN
      160      744    1           SEGID = ' ';
      161      745    1         ELSE
      162      746    2           DO;
      163      747    2             POINTER$ = ADDR(NIL);
      164      748    2             RETURN;
      165      749    2           END;
      166      750
      167      751    2       DO SELECT (SEGID);
      168      752    2         SELECT(%LS0, %BLANK );    /* DEFAULT IS $LS0 */
      169      753    2           POINTER.SEGID = %ISSID;
      170      754    2         SELECT (%LS4);
      171      755    2           POINTER.SEGID = %DS1SID;
      172      756    2         SELECT  (%LS5);
      173      757    2           POINTER.SEGID = %DS2SID;
      174      758    2         SELECT (%LS6);
      175      759    2           POINTER.SEGID = %DS3SID;
      176      760    2         SELECT (%LS7);
      177      761    2           POINTER.SEGID = %DS4SID;
      178      762    2         SELECT (%LS8);
      179      763    2           POINTER.SEGID = %DS5SID;
      180      764    2         SELECT (%LS9);
      181      765    2           POINTER.SEGID = %DS6SID;
      182      766    2         SELECT (%LS10);
      183      767    2           POINTER.SEGID = %DS7SID;
      184      768    2         SELECT (%LS11);
      185      769    2           POINTER.SEGID = %DS8SID;
      186      770    2         SELECT ( ELSE );
      187      771    2           POINTER$ = ADDR(NIL);
      188      772    2           RETURN;
      189      773    2       END; /*SELECT */
PL6.E3A0      #002=Z9JS$SCAN_POINTER File=Z9JS.:Z8A6TSI                          SAT 08/09/97 14:05 Page:16   
      190      774        %EJECT;
PL6.E3A0      #002=Z9JS$SCAN_POINTER File=Z9JS.:Z8A6TSI                          SAT 08/09/97 14:05 Page:17   
      191      775    1     CONVERT_TO_PTR$: PROC( CHAR6 );
      192      776
      193      777    2       DCL CHAR6          CHAR(6);
      194      778    2       DCL BIN1           UBIN WORD;
      195      779    2       POINTER.WD_OFF = 0;        /*  CLEAR FIRST HALF OF POINTER */
      196      780
      197      781    3       DO I = 0 TO 4;             /*  Convert to pointer          */
      198      782    3         CALL CHARBIN (BIN1, SUBSTR (CHAR6, I, 1) );
      199      783    3         POINTER.WD_OFF = ( POINTER.WD_OFF + BIN1 ) * 8;
      200      784    3       END;
      201      785    2       CALL CHARBIN (BIN1, SUBSTR (CHAR6, 5, 1) );
      202      786    2       POINTER.WD_OFF = POINTER.WD_OFF + BIN1;
      203      787
      204      788    2     END CONVERT_TO_PTR$;    /* proc */
      205      789
      206      790    1     END Z9JS$SCAN_POINTER;  /* PROC */
      207      791        %EJECT;
