    8      %EQU #PTR_REGS = 4 ; /* Number of Global pointers (ESC's)    */
    9                           /* = 4 for CP-6 & GCOS, = 3 for Multics */
   10      %EQU PR7 = 27; /*  See Z8H_RAT for other information  */
   11      %EQU PR_ARGP = 0; /*  LDP7  arg1,,PR2     Args on the USING list    */
   12      %EQU PR_BLOCK = 1; /*  EPPR7 0,,PR5       Items with EXTERNAL attri.*/
   13                         /*  LAR7  <block>                                */
   14
   15      %MACRO PICK_ESC_LIST ;
