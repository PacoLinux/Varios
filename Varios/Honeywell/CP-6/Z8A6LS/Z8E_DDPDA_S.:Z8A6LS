  289      %MACRO START_TABLE_PRODR;
  290      DCL 1 PRODR(0:%PRODR_LEN) BIT(%BITS_PRODR) UNAL CONSTANT SYMDEF INIT('0'B
  291      %MEND;
  292      %MACRO PRODM(GOAL=0, PSS=0, SEM=0);
  293      ,%(CONCAT(BINBIT(GOAL,%BITS_GOAL),BINBIT(PSS,%BITS_PSS),BINBIT(SEM,%BITS_SEM)))
  294      %MEND;
  295      %MACRO END_TABLE_PRODR;
  296          );
