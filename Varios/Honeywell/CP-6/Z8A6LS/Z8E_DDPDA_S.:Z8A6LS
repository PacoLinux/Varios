  389      %MACRO PARSE_SYMREFS;
  390      DCL 1 PRODR(0:%PRODR_LEN) UNAL SYMREF READONLY,
  391            2 GOAL UBIN(%BITS_GOAL) UNAL,
  392            2 POPSTACK UBIN(%BITS_PSS) UNAL,
  393            2 SEMANTIC UBIN(%BITS_SEM) UNAL;
  394      DCL 1 ACTION_COL(0:%ACTION_COL_LEN) UNAL SYMREF READONLY,
  395            2 DEFAULT SBIN(%BITS_ASDEF) UNAL,
  396            2 BASE UBIN(%BITS_ASBAS) UNAL;
  397      DCL 1 ACTION_ROW(0:%ACTION_ROW_LEN) UNAL SYMREF READONLY,
  398            2 NEXT SBIN(%BITS_ANCNXT) UNAL,
  399            2 CHECK UBIN(%BITS_ANCCHK) UNAL;
  400      DCL 1 GOTO_COL(0:%GOTO_COL_LEN) UNAL SYMREF READONLY,
  401            2 DEFAULT UBIN(%BITS_GTDEF) UNAL,
  402            2 BASE UBIN(%BITS_GTBAS) UNAL;
  403      DCL 1 GOTO_ROW(0:%GOTO_ROW_LEN) UNAL SYMREF READONLY,
  404            2 NEXT UBIN(%BITS_GNCNXT) UNAL,
  405            2 CHECK UBIN(%BITS_GNCCHK) UNAL;
  406      %MEND;
  407      %MACRO TSTRING_SYMREFS;
09:14 AUG 09 '97 Z8E_DDPDA_S.:Z8A6SI/Z8E_DDPDA_S.:Z8A6TUI                  12   
