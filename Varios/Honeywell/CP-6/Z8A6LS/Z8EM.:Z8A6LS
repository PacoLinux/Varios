     2465    27698    2                       LX_REC_DECL.P.RENAMES$ ~= ADDR(NIL)  THEN
     2466    27699    3            DO;
     2467    27700    3              CALL CHARBIN(LEVEL#,LEVEL_NO$ -> LX_TERMINAL.DNT$ -> DNT.TEXT.NAME);
     2468    27701    3              IF LEVEL# ~= %LEVEL_66 THEN
     2469    27702    4                DO;
     2470    27703    4                  CALL Z8AE$ERROR (%E$ONLY_66_IN_RENAMES,
     2471    27704    4                                   LEVEL_NO$ -> LX_TERMINAL.LINE,
     2472    27705    4                                   LEVEL_NO$ -> LX_TERMINAL.COLUMN);
     2473    27706    4                  CALL Z8AH$NEW_DNT(%LEVEL_66_TEXT,
     2474    27707    4                                    %LEVEL_66_LENGTH,
     2475    27708    4                                    DNT$$);
     2476    27709    4                  LEVEL_NO$ -> LX_ITEM.DNT$ = DNT$$;
     2477    27710    4                END;
     2478    27711    3            END; /* THERE IS A RENAMES - IF */
     2479    27712    2        RETURN;
     2480    27713    2      END LEVEL_CHECK;
