      216    20150         %MACRO NEW_COPY_ID_TREE (@QUAL_ID$ = ,
      217    20151                                  @SUB$     = ,
      218    20152                                  @ACTION# =,
      219    20153                                  @REF$     = );
      220    20154           DO;
      221    20155             %INIT_DEFERED_ID_TREE (@POINTER = "ADDR(TEMP_LX_NODE)",
      222    20156                                    @ACTION_NO# = @ACTION#);
      223    20157             %NEW_COPY_LX_STK (@POINTER = "ADDR(TEMP_LX_NODE) -> DEFERED_ID_TREE.QUAL_ID$",
      224    20158                               @FROM    = @QUAL_ID$);
      225    20159             %IF (TEXTCHAR(@SUB$) ~= '');
      226    20160                %NEW_COPY_LX_STK (@POINTER = "ADDR(TEMP_LX_NODE) -> DEFERED_ID_TREE.SUB$",
      227    20161                                  @FROM    = @SUB$);
      228    20162             %ENDIF;
      229    20163             %IF (TEXTCHAR(@REF$) ~= '');
      230    20164                %NEW_COPY_LX_STK (@POINTER = "ADDR(TEMP_LX_NODE) -> DEFERED_ID_TREE.REF$",
      231    20165                                  @FROM    = @REF$);
      232    20166             %ENDIF;
      233    20167
      234    20168             %NEW_COPY_LX_ITEM (@FROM$ = "ADDR(TEMP_LX_NODE)",
      235    20169                                @TO$   = HEAP$);
      236    20170             %INIT_LX_LARGE (@POINTER = "ADDR(TEMP_LX_NODE)");
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:10   
      237    20171             ADDR(TEMP_LX_NODE) -> LX_LARGE.LARGE_ITEM$ = HEAP$;
      238    20172
      239    20173             %PROPAGATE_TEMP_LX_NODE;
      240    20174           END;
      241    20175         %MEND;
      242    20176         %EJECT;
