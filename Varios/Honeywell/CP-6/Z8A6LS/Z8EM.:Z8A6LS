     5105    36613
     5106    36614
     5107    36615    4            SELECT (ELSE);
     5108    36616                   /*N* SORT AND SORT MERGE STUFF */
     5109    36617
     5110    36618    4          END; /* Case id */
     5111    36619
     5112    36620    3   NEXT_FILE:
     5113    36621    3          LIST$ = LIST$ -> LX_ITEM.LINK;
     5114    36622    3        END; /* While */
