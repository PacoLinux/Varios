     2451    27684    1     LEVEL_CHECK: PROC (LEVEL_NO$, LARGE$);
     2452    27685        /*===========*/
     2453    27686
     2454    27687               %EQU LEVEL_66 = 66;
     2455    27688               %EQU LEVEL_66_TEXT = '66';
