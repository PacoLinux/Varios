      967    21408    2      CALL ALLOCATE_CD_BUFFER (O1COM.CURRENT_CD_ENTRY$ -> CD_LTS.BUFFER_LENGTH,
      968    21409    2                               O1COM.CURRENT_CD_ENTRY$ -> CD_LTS.BUFFER_ST$);
