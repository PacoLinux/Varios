      950    21391    2                  PROC_.PROC_SUPP$ -> PROC_SUPP.CD_LIST$;
      951    21392    2      O1COM.CURRENT_PROC$ -> PROC_.PROC_SUPP$ -> PROC_SUPP.CD_LIST$ =
      952    21393    2                  O1COM.CURRENT_CD_ENTRY$;
