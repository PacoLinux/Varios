      247    20181        %MACRO ARITH_EXPR_UNARY_OPERATION (@OP = %OP_MINUS);
      248    20182          DO;
      249    20183            %INIT_UNARY_OP (@POINTER = "ADDR(TEMP_LX_NODE)",
