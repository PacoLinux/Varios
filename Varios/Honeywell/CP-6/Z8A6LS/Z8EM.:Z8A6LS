      954    21395           /* set the QLTs father field and reset COMCOM.PREV_QLT */
      955    21396
      956    21397    2      BRO$ = O1COM.CURRENT_CD_ENTRY$ -> CD_LTS.WORK_ST$ -> SCALAR.QLT$;
      957    21398    2      FATHER$ = BRO$ -> QLT.FATHER;
      958    21399    2      COMCOM.PREV_QLT$ = FATHER$;
