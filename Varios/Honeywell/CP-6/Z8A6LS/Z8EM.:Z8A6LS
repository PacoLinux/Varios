     2573    28001    1     MULTIPLE_FILE_TAPE: PROC (FILE_LIST$);
     2574    28002        /*==================*/
     2575    28003
     2576    28004    2        DCL FILE_LIST$           PTR;
     2577    28005
     2578    28006    2        DCL LIST$                PTR;
     2579    28007    2        DCL FD$$                 PTR;
     2580    28008    2        DCL LAST$                PTR;
     2581    28009    2        DCL HEAD$                PTR;
