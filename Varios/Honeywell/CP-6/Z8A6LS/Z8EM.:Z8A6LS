     2296    27459
     2297    27460    4                ST$$ = ST$$ -> ST.NEXT;
     2298    27461
     2299    27462    4              CASE (ELSE);
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:86   
     2300    27463    4                ST$$ = ST$$ -> ST.NEXT;
     2301    27464    4            END; /* Case */
     2302    27465    3        END; /* While */
     2303    27466
     2304    27467    2        IF RET_ST$ ~= ADDR(NIL) THEN
     2305    27468    2          ST$$ = RET_ST$;
     2306    27469
     2307    27470    2        ALTRETURN;
     2308    27471
     2309    27472    2   END FIND_FILE;
