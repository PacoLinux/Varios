      285    20219        /*===========*/
      286    20220    1     ACCESS_MODE: PROC (ACCESS$, ACCESS_MODE);
      287    20221        /*===========*/
      288    20222
      289    20223             /* Functions performed:
      290    20224                  1. Check for duplicate access clauses.
      291    20225                  2. Update FILE_LTS item.
      292    20226             */
      293    20227
      294    20228    2        DCL ACCESS$               PTR;
      295    20229    2        DCL ACCESS_MODE           SBIN;
      296    20230
      297    20231
      298    20232    3        IF O1COM.CURRENT_FILE_LTS$ -> FILE_LTS.ACCESS_MODE = %NOT_SPECIFIED THEN DO;
      299    20233    3          O1COM.CURRENT_FILE_LTS$ -> FILE_LTS.ACCESS_MODE = ACCESS_MODE;
      300    20234    3          IF COMCOM.OB.FIPS.LEVEL >= %INT_LEV AND ACCESS_MODE = %DYNAMIC_ACCESS OR
      301    20235    3              COMCOM.OB.FIPS.LEVEL > %INT_LEV AND ACCESS_MODE = %RANDOM_ACCESS THEN
      302    20236    3            CALL Z8AE$ERROR (%E$LEVEL_CHECK,
      303    20237    3                             O1COM.LX_STK.LINE(O1COM.LX_TOP - 1),
      304    20238    3                             O1COM.LX_STK.COLUMN(O1COM.LX_TOP - 1));
      305    20239    3        END;
      306    20240    2        ELSE
      307    20241               %CONDITIONAL_ERROR (@LX_ITEM$ = ACCESS$,
      308    20242                                   @ERROR#   = %E$DUP_DD_CLAUSE,
      309    20243                                   @LINE     = ACCESS$ -> LX_TERMINAL.LINE,
      310    20244                                   @COLUMN   = ACCESS$ -> LX_TERMINAL.COLUMN,
      311    20245                                   @PLUG1    = "DNT$_FROM_RSWNO(%RSV_WRD_ACCESS)");
      312    20275
      313    20276    2        IF O1COM.CURRENT_FILE_LTS$ -> FILE_LTS.ORGANIZATION ~= %NOT_SPECIFIED THEN
      314    20277    2          CALL CHECK_DIFF_ORG_AND_ACCESS (ACCESS$,
      315    20278    2                                          ACCESS_MODE,
      316    20279    2                                          O1COM.CURRENT_FILE_LTS$ ->
      317    20280    2                                          FILE_LTS.ORGANIZATION);
      318    20281
      319    20282    2   END ACCESS_MODE;
      320    20283        %EJECT;
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:14   
      321    20284
