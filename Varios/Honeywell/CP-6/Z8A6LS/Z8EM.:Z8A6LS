     2639    28167    3            CALL CHARBIN (LTS$$ -> FILE_LTS.POSITION,
     2640    28168    3                          LIST$ -> LX_LIST.TAIL$ -> LX_ITEM.DNT$ -> DNT.TEXT.NAME);
     2641    28169
     2642    28170    3          LTS$$ -> FILE_LTS.B.MULTIPLE_FILE_TAPE = TRUE;
     2643    28171
     2644    28172    3   NEXT_FILE:
     2645    28173    3          LIST$ = LIST$ -> LX_LIST.LINK;
