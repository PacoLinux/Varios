      960    21401    3      DO WHILE (BRO$ -> QLT.BROTHER ~= ADDR(NIL));
      961    21402
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:37   
