     1905    26897    1     ENTER_VALUE: PROC (LIT$, ST$$, VALUE, NEXT_SEQ#) ALTRET;
     1906    26898        /*===========*/
     1907    26899
     1908    26900    2        DCL ST$$          PTR;
     1909    26901    2        DCL VALUE         SBIN;
     1910    26902    2        DCL LIT$          PTR;
     1911    26903    2        DCL NEXT_SEQ#     SBIN;
     1912    26904
     1913    26905
     1914    26906    3        DO CASE (ST$$ -> ST.ID);
     1915    26907    3          CASE (%CLASS);
     1916    26908    3            CALL ENTER_VALUE_IN_CLASS (ST$$, VALUE)
     1917    26909    3              WHENALTRETURN
     1918    26910    4                DO;
     1919    26911    4                  CALL Z8AE$ERROR (%E$ERROR_IN_ALPHABET_LITERAL,
     1920    26912    4                                   LIT$ -> LX_ITEM.LINE,
     1921    26913    4                                   LIT$ -> LX_ITEM.COLUMN);
     1922    26914    4                END;
     1923    26915
     1924    26916    3          CASE (%ALPHABET);
     1925    26917    3            CALL ENTER_VALUE_IN_TABLE (ST$$, VALUE, NEXT_SEQ#)
     1926    26918    3              WHENALTRETURN
     1927    26919    4                DO;
     1928    26920    4                  CALL Z8AE$ERROR (%E$ERROR_IN_ALPHABET_LITERAL,
     1929    26921    4                                   LIT$ -> LX_ITEM.LINE,
     1930    26922    4                                   LIT$ -> LX_ITEM.COLUMN);
     1931    26923    4                  ALTRETURN;
     1932    26924    4                END;
     1933    26925
     1934    26926    3          CASE (ELSE);
     1935    26927    3            CALL Z8AE$ERROR (%E$UNEXPECTED_CASE,
     1936    26928    3                             LIT$ -> LX_ITEM.LINE,
     1937    26929    3                             LIT$ -> LX_ITEM.COLUMN);
     1938    26930
     1939    26931    3        END; /* Case */
     1940    26932
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:72   
     1941    26933    2   END ENTER_VALUE;
     1942    26934        %EJECT;
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:73   
