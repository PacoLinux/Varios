PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:6    
      143    20077        %MACRO CHECK_FOR_GTR_1_CHAR (@TERMINAL$ = , @NEW_DNT$ = );
      144    20078
      145    20079           DO;
      146    20080             IF @TERMINAL$ -> LX_TERMINAL.SYMBOL = %ALPHA_LITERAL
      147    20081             OR @TERMINAL$ -> LX_TERMINAL.SYMBOL = %SINGLE_QUOTE THEN
      148    20082               DO;
      149    20083                 CALL Z8EC$INTERNAL_STRING_REP (@TERMINAL$, @NEW_DNT$);
      150    20084
      151    20085                 IF @NEW_DNT$ -> DNT.TEXT.LENGTH > 3 THEN
      152    20086                   CALL Z8AE$ERROR (%E$LITERAL_GTR_1_CHAR,
      153    20087                                    @TERMINAL$ -> LX_TERMINAL.LINE,
      154    20088                                    @TERMINAL$ -> LX_TERMINAL.COLUMN);
      155    20089               END;
      156    20090           END;
      157    20091
      158    20092        %MEND;
      159    20093        %EJECT;
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:7    
      160    20094        %MACRO GET_VALUE (@TERMINAL$ = ,
      161    20095                          @DNT$      = ,
      162    20096                          @VALUE     = );
      163    20097
      164    20098           DO SELECT (@TERMINAL$ -> LX_TERMINAL.SYMBOL);
      165    20099             SELECT (%ALPHA_LITERAL, %SINGLE_QUOTE);
      166    20100               CALL Z8EC$INTERNAL_STRING_REP (@TERMINAL$, @DNT$);
      167    20101
      168    20102               @VALUE = ASCBIN ( SUBSTR (@DNT$ -> DNT.TEXT.NAME,1,1) );
      169    20103
      170    20104             SELECT (%NUMBER);
      171    20105               CALL CHARBIN (@VALUE, @TERMINAL$ -> LX_TERMINAL.DNT$ -> DNT.TEXT.NAME);
      172    20106
      173    20107               @VALUE = @VALUE - 1;
      174    20108
      175    20109             SELECT (%RSV_WRD_HIGH_VALUES);
      176    20110               @VALUE = BITBIN('177'O);
      177    20111
      178    20112             SELECT (%RSV_WRD_LOW_VALUES);
      179    20113               @VALUE = 0;
      180    20114
      181    20115             SELECT (%RSV_WRD_QUOTES);
      182    20116               @VALUE = ASCBIN ('"');
      183    20117
      184    20118             SELECT (%RSV_WRD_SPACES);
      185    20119               @VALUE = ASCBIN (' ');
      186    20120
      187    20121             SELECT (%RSV_WRD_ZEROES);
      188    20122               @VALUE = 0;
      189    20123
      190    20124             SELECT (ELSE);
      191    20125               CALL Z8AE$ERROR (%E$ERROR_IN_ALPHABET_LITERAL,
      192    20126                                @TERMINAL$ -> LX_TERMINAL.LINE,
      193    20127                                @TERMINAL$ -> LX_TERMINAL.COLUMN);
      194    20128               @VALUE = 512;
      195    20129
      196    20130           END; /* Case */
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:8    
      197    20131
      198    20132        %MEND;
      199    20133        %EJECT;
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:9    
