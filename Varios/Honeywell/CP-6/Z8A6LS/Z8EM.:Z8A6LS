     2590    28018    3        DO WHILE (LIST$ ~= ADDR(NIL));
     2591    28019    3          CALL Z8DO$QUALIFY_TERMINAL (LIST$ -> LX_LIST.HEAD$, FD$$)
     2592    28020    3            WHENALTRETURN
     2593    28021    4              DO;
     2594    28022    4                GOTO NEXT_FILE;
     2595    28023    4              END;
     2596    28024
     2597    28025    4          IF FD$$ -> FD.ID ~= %FD THEN DO;
     2598    28026                 /* This can't be a multiple file tape. It isn't a file. */
     2599    28027                 %CONDITIONAL_ERROR (@LX_ITEM$ = LIST$ -> LX_LIST.HEAD$,
     2600    28028                                     @ERROR# = %E$FILE_TYPE_EXPECTED,
     2601    28029                                     @PLUG1 = "DNT$_FROM_RSWNO(%RSV_WRD_FD)");
     2602    28059    4            GOTO NEXT_FILE;
     2603    28060    4          END;
     2604    28061
     2605    28062               %GET_LTS (@ID       = %FILE_LTS,
     2606    28063                         @ST$      = FD$$,
     2607    28064                         @RETURN$  = LTS$$);
     2608    28107    3          IF LTS$$ -> FILE_LTS.B.MULTIPLE_FILE_TAPE THEN
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:98   
     2609    28108    4            DO;
     2610    28109                   %CONDITIONAL_ERROR (@LX_ITEM$ = LIST$ -> LX_LIST.HEAD$,
     2611    28110                     @ERROR# = %E$DUP_DD_CLAUSE,
     2612    28111                     @LINE   = LIST$ -> LX_LIST.HEAD$ -> LX_ITEM.LINE,
     2613    28112                     @COLUMN = LIST$ -> LX_LIST.HEAD$ -> LX_ITEM.COLUMN,
     2614    28113                     @PLUG1  = "DNT$_FROM_RSWNO(%RSV_WRD_TAPE)");
     2615    28143
     2616    28144    4              GOTO NEXT_FILE;
     2617    28145    4            END;
     2618    28146
     2619    28147    3          CALL Z9EM$TAPE_CHECK (LIST$ -> LX_LIST.HEAD$,
     2620    28148    3                                LTS$$)
     2621    28149    3            WHENALTRETURN
     2622    28150    4              DO;
     2623    28151    4                GOTO NEXT_FILE;
     2624    28152    4              END;
     2625    28153
     2626    28154    3          IF HEAD$ = ADDR(NIL) THEN
     2627    28155    4            DO;
     2628    28156    4              LAST$ = LTS$$;
     2629    28157    4              HEAD$ = LTS$$;
     2630    28158    4            END;
     2631    28159    3          ELSE
     2632    28160    4            DO;
     2633    28161    4              LTS$$ -> FILE_LTS.MULT_TAPE.HEAD$ = HEAD$;
     2634    28162    4              LAST$ -> FILE_LTS.MULT_TAPE.LINK = LTS$$;
     2635    28163    4              LAST$ = LTS$$;
