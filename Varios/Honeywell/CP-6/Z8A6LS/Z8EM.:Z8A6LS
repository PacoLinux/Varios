     2264    27423    1     FIND_FILE : PROC (FILE$, ST$$) ALTRET;
     2265    27424        /*=========*/
     2266    27425
     2267    27426    2        DCL FILE$         PTR; /* To lx_term for the file */
     2268    27427    2        DCL ST$$          PTR; /* Return to file found */
     2269    27428    2        DCL DNT$$         PTR;
     2270    27429    2        DCL RET_ST$       PTR;
     2271    27430
     2272    27431
     2273    27432    2        DNT$$ = FILE$ -> LX_TERMINAL.DNT$;
     2274    27433    2        ST$$  = DNT$$ -> DNT.ST$;
