     2517    27822    3        DO I = START_VALUE TO END_VALUE;
     2518    27823    3          CALL ENTER_VALUE (ITEM$, ITEM_ST$$, I, NEXT_SEQ#) ALTRET (EXIT_LOOP);
     2519    27824    3        END;
