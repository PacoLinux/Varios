     3248    31883
     3249    31884    3          CASE (
     3250    31885                     %SET_OF_RELATIVE_ORGANIZATION;
     3251    31889    3               );
     3252    31890                 /* Check for PADDING, DELIMITER, LINAGE, RECORD KEY
     3253    31891                    and ALTERNATE RECORD KEY's                       */
