      216    20150         %MACRO NEW_COPY_ID_TREE (@QUAL_ID$ = ,
      217    20151                                  @SUB$     = ,
      218    20152                                  @ACTION# =,
      219    20153                                  @REF$     = );
      220    20154           DO;
      221    20155             %INIT_DEFERED_ID_TREE (@POINTER = "ADDR(TEMP_LX_NODE)",
      222    20156                                    @ACTION_NO# = @ACTION#);
      223    20157             %NEW_COPY_LX_STK (@POINTER = "ADDR(TEMP_LX_NODE) -> DEFERED_ID_TREE.QUAL_ID$",
      224    20158                               @FROM    = @QUAL_ID$);
      225    20159             %IF (TEXTCHAR(@SUB$) ~= '');
      226    20160                %NEW_COPY_LX_STK (@POINTER = "ADDR(TEMP_LX_NODE) -> DEFERED_ID_TREE.SUB$",
      227    20161                                  @FROM    = @SUB$);
