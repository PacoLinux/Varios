      323    20286    1      ALLOCATE_CD_BUFFER: PROC (BUFFER_LENGTH, BUFFER_ST$);
      324    20287         /*==================*/
      325    20288
      326    20289
      327    20290        %EQU CD_BUFFER_TEXT = 'CD_BUFFER';
      328    20291        %EQU CD_BUFFER_TEXT_LENGTH = 9;
      329    20292
      330    20293    2   DCL BUFFER_LENGTH          SBIN WORD;
      331    20294    2   DCL BUFFER_ST$             PTR;
      332    20295
      333    20296
      334    20297    2      LEX_SYMBOL.SIZE = %CD_BUFFER_TEXT_LENGTH;
      335    20298    2      SUBSTR (LEX_SYMBOL.SYMBOL,
      336    20299    2              0,
      337    20300    2              LEX_SYMBOL.SIZE) = %CD_BUFFER_TEXT;
      338    20301
      339    20302    2      CALL Z8AH$SEARCH_;
      340    20303
      341    20304    2      CALL CREATE_CD_AREA (DNT$, BUFFER_LENGTH, BUFFER_ST$, %LEVEL_01);
      342    20305
      343    20306    2    END ALLOCATE_CD_BUFFER;
      344    20307         %EJECT;
