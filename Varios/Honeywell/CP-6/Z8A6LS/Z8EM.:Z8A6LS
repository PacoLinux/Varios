      201    20135         %MACRO SAVE_REPORT_TYPE (@REPORT_TYPE = %NO_REPORT_TYPE);
      202    20136           DO;
      203    20137             CALL CHECK_AND_SAVE_ITEM (ADDR(O1COM.LX_STK(O1COM.LX_TOP)),
      204    20138                                       O1COM.REPORT_DD$ -> LX_REPORT_REC.P.TYPE$,
      205    20139                                       %RSV_WRD_TYPE)
      206    20140               WHENRETURN
      207    20141                 DO;
      208    20142                   O1COM.REPORT_DD$ -> LX_REPORT_REC.D.REPORT_TYPE = @REPORT_TYPE;
      209    20143                   O1COM.REPORT_DD$ -> LX_REPORT_REC.CLAUSE_COUNT =
      210    20144                     O1COM.REPORT_DD$ -> LX_REPORT_REC.CLAUSE_COUNT + 1;
      211    20145                 END;
      212    20146
      213    20147           END;
      214    20148         %MEND;
