     2278    27437    3          IF ST$$ -> SCALAR.PROC$ ~= O1COM.CURRENT_PROC$ THEN
     2279    27438                 /* It is not in this proc so altreturn */
     2280    27439    3            IF RET_ST$ = ADDR(NIL) THEN
     2281    27440    3              ALTRETURN;
     2282    27441    3            ELSE
     2283    27442    4              DO;
     2284    27443    4                ST$$ = RET_ST$;
     2285    27444    4                RETURN;
     2286    27445    4              END;
     2287    27446    3          ELSE
     2288    27447    4            DO CASE (ST$$ -> ST.ID);
     2289    27448    4              CASE (
     2290    27449                          %SET_OF_FILE_DESCRIPTIONS;
     2291    27454    4                    );
     2292    27455    4                IF ST$$ -> FD.QLT$ -> QLT.SON -> QLT.BROTHER = ADDR(NIL) THEN
     2293    27456    4                  RETURN;
     2294    27457    4                ELSE
