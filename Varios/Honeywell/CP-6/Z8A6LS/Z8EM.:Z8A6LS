     2042    27034    1     EXTERNAL_BLOCK_CHECK : PROC (LTS$$);
     2043    27035        /*====================*/
     2044    27036
     2045    27037    2        DCL LTS$$       PTR;   /* To FILE_LTS */
     2046    27038    2        DCL BLOCK$$     PTR;   /* To Created BLOCK */
     2047    27039    2        DCL P$          PTR;
     2048    27040
     2049    27041
     2050    27042             /*
     2051    27043               Look for another FD with the same name, that is also external.
     2052    27044               If one is found, this refers to the same one, set the counts
     2053    27045               and get the hell out!
     2054    27046             */
     2055    27047
     2056    27048    2        P$ = LTS$$ -> FILE_LTS.ST$ -> FD.DNT$ -> DNT.ST$;
     2057    27049
     2058    27050    3        DO WHILE (P$ ~= ADDR(NIL));
     2059    27051    3          IF P$ -> ST.ID = %FD
     2060    27052    3          AND P$ ~= LTS$$ -> FILE_LTS.ST$ THEN
     2061    27053    3            IF P$ -> FD.B.EXTERNAL THEN
     2062    27054    3              EXIT;
     2063    27055
     2064    27056    3          P$ = P$ -> ST.NEXT;
     2065    27057    3        END;
     2066    27058
     2067    27059    2        IF P$ ~= ADDR(NIL) THEN
     2068    27060    3          DO;
     2069    27061                 /* A file like it was found */
     2070    27062    3            LTS$$ -> FILE_LTS.ST$ -> FD.COUNT = P$ -> FD.COUNT;
     2071    27063    3            LTS$$ -> FILE_LTS.ST$ -> FD.B.ALLOCATED = TRUE;
     2072    27064    3            RETURN;
     2073    27065    3          END;
     2074    27066
     2075    27067             %NEW_BLOCK (@ST$   = BLOCK$$,
     2076    27068                         @DNT$  = LTS$$ -> FILE_LTS.ST$ -> FD.DNT$);
     2077    27145
PL6.E3A0      #001=Z8EM$SEMANTICS_DATA_DIV File=Z8EM.:Z8A6TSI                    SAT 08/09/97 12:08 Page:77   
     2078    27146    2        LTS$$ -> FILE_LTS.ST$ -> FD.COUNT = BLOCK$$ -> BLOCK.COUNT;
     2079    27147
     2080    27148    2   END EXTERNAL_BLOCK_CHECK;
