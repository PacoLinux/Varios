     1314    14026        /*I*     NAME:         PRINT_AND_CLEAR
     1315    14027                               --------------------
     1316    14028               THIS ROUTINE DETERMINES IF THE ROOT OF THE TREE IS
     1317    14029               BEING PRINTED.  IF SO, THE LEADING FILL CHARARTERS ARE
     1318    14030               REMOVED AND THE CALL TO Z9AL$DUMP_LINE IS MADE TO PRINT
     1319    14031               OBUF.  IF OBUF DOES NOT CONTAIN THE ROOT IT IS PRINTED
     1320    14032               AS IS.  IN BOTH CASES,  THE ITEM'S NAME IS CLEARED FROM
     1321    14033               OBUF -- LEAVING VERTICAL TREE BRANCHES INTACT.
     1322    14034
     1323    14035        */
     1324    14036
     1325    14037
     1326    14038    1     PRINT_AND_CLEAR: PROC ( COLUMN, LENGTH, OP );
     1327    14039
     1328    14040    2       DCL COLUMN       UBIN WORD;
     1329    14041    2       DCL LENGTH       UBIN WORD;
     1330    14042    2       DCL OP           UBIN WORD;
     1331    14043
     1332    14044
     1333    14045
     1334    14046            /*V*  INTERACTIVE TREES? */
     1335    14047
     1336    14048    2       CALL Z9AL$DUMP_LINE;
     1337    14049
     1338    14050            /* CLEAR ITEM FROM OBUF */
     1339    14051
