      922    13634    4               SUBSTR (OBUF, PRINT_NDX, 8) = ':--'; /* SHORT FILL */
      923    13635    4               MAX_LENGTH = 7;      /* ALLOW FULL NAME LENGTH */
      924    13636    4             END;
      925    13637    3           ELSE
      926    13638    4             DO;
      927    13639    4               MAX_LENGTH = 4;  /* CUT LENGTH TO RETAIN DASHES */
      928    13640    4               SUBSTR (OBUF, PRINT_NDX, 8) = ':-------';  /*LONG FILLER */
      929    13641    4             END;
      930    13642
