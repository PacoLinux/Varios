      134    12846    1       DCL LOWER_TREES  BIT(1) STATIC INIT(FALSE);
PL6.E3A0      #001=Z8JW$WALK_TREE File=Z8JW.:Z8A6TSI                             SAT 08/09/97 13:33 Page:88   
      135    12847    1       DCL ADDRESS      UBIN WORD STATIC;
      136    12848    1       DCL ABSTRACT     BIT(1)    STATIC;
      137    12849
      138    12850        %EJECT;
PL6.E3A0      #001=Z8JW$WALK_TREE File=Z8JW.:Z8A6TSI                             SAT 08/09/97 13:33 Page:89   
      139    12851    1       PRINT_NDX = PRINT_ARG + 8;

