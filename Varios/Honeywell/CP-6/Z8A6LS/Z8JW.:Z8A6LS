      910    13622    2       IF ID$ ~= ADDR(NIL) THEN
      911    13623    3         DO;
      912    13624
      913    13625            /*V*                     HORIZONTAL TREE BRANCHES
      914    13626                                     ---------- ---- --------
      915    13627                    CHECK FOR TREE LEAF, IF LEAF PUT IN A SHORTENED
      916    13628                    FILL CHARACTER TO ELIMINATE TRAILING DASHES.
      917    13629            */
