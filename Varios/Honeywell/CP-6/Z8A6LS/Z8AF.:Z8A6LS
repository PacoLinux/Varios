VERSION A06

PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:1    
        1        1        /*M* Z8AF$FORME -- Form Expression. */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        Z8AF$FORME_ON_SIZE: PROC (ID, LOP$, ROP$, RESULT$, ON_SIZE_FLAG) ;
        8        8        /*
        9        9                     FORME optimizations performed:
       10       10
       11       11               A1: index('D', 'ABCDEFG')    ==> 3
       12       12               A2: index('ABCDEFG', 'D')    ==> 1
       13       13               B:  tyi(3) + tyi(20)         ==> 23
       14       14               C:  tyi(3) - tyi(20)         ==> -17
       15       15         @     D:  tyi(3) * tyi(20)         ==> 60
       16       16               E:  tyi(20)/ tyi(10)         ==> 2
       17       17               F:  - tyi(8)                 ==> -8
       18       18               G:  mini(12,20)              ==> 12
       19       19               H:  tyi(8) + tyi(-9)         ==> tyi(8) - (- tyi(-9))
       20       20               I:  tyi(8) + tyi(0)          ==> tyi(8)
       21       21               J:  tyi(X) + tyi(7) + tyi(2) ==> tyi(X) + tyi(9)
       22       22               K:  tyi(X) - tyi(Y) + tyi(Y) ==> tyi(X)
       23       23               L:  tyi(Y) - tyi(X) + tyi(Y) ==> tyi(X) -- Suppressed
       24       24               M:  tyi(X) - tyi(8) + tyi(1) ==> tyi(X) - (tyi(8) - tyi(1))
       25       25               N:  tyi(8) - tyi(X) + tyi(1) ==> (tyi(1) + tyi(8)) - tyi(X)
       26       26               O:  tyi(X) - tyi(X)          ==> tyi(0)
       27       27               P:  tyi(X) - tyi(-8)         ==> tyi(X) + (- tyi(-8))
       28       28               Q:  tyi(X) - tyi(0)          ==> tyi(X)
       29       29               R:  tyi(X) - tyi(Y) - tyi(X) ==> (- tyi(Y))
       30       30               S:  tyi(X) - tyi(3) - tyi(5) ==> tyi(X) - (tyi(3) + tyi(5))
       31       31               T:  tyi(3) - tyi(X) - tyi(6) ==> (tyi(3) - tyi(6)) - tyi(X)
       32       32               U:  tyi(X) + tyi(Y) - tyi(Y) ==> tyi(X)
       33       33               V:  tyi(Y) + tyi(X) - tyi(Y) ==> tyi(X)
       34       34               W:  tyi(X) + tyi(3) - tyi(5) ==> tyi(X) + (tyi(3) - tyi(5))
       35       35               X:  tyi(3) + tyi(X) - tyi(5) ==> tyi(X) + (tyi(3) - tyi(5))
       36       36               Note -- optimization X unused as tyi(3) + tyi(X) will already
       37       37                       have been converted to tyi(X) + tyi(3); use W.
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:2    
       38       38         @     Y:  tyi(X) * tyi(-1)         ==> (- tyi(X))
       39       39         @     Z:  tyi(X) * tyi(0)          ==> tyi(0)
       40       40         @    AA:  tyi(X) * tyi(1)          ==> tyi(X)
       41       41         @    BB:  tyi(X) * tyi(3) * tyi(4) ==> tyi(X) * (tyi(3) * tyi(4))
       42       42         @    CC:  tyi(X) shl 3 * tyi(3)    ==> tyi(X) * (tyi(8) * tyi(3))
       43       43         @    DD:  tyi(X) * 2               ==> tyi(X) shl n
       44       44         @    EE:  tyi(x) * tyi(2) + tyi(1) ==> tyi(x) shl n + tyi(1)
       45       45         @    FF:  tyi(x) * tyi(2) - tyi(1) ==> tyi(x) shl n - tyi(1)
       46       46              GG:  tyi(x) - (tyi(x) - tyi(y)) ==> tyi(y)
       47       47              HH:  tyi(5) - (tyi(x) - tyi(3)) ==> tyi(5) + tyi(3) - tyi(x)
       48       48              II:  - (tyi(x) - tyi(y))      ==> (tyi(y) - tyi(x))
       49       49              JJ:  - (- tyi(x))             ==> (tyi(x))
       50       50              KK:  mini(x, x)               ==> tyi(x)
       51       51         @    LL:  tyd(x) *^ 10 or tyd /^ 10==> tyd +- {scale factor}
       52       52              MM:  tyi(x) / tyi(x)          ==> tyi(1)
       53       53              NN:  tyr(x) / tyr(x)          ==> tyr(1)
       54       54              OO:  tyi(0) / tyi(x)          ==> tyi(0)
       55       55              PP:  tyi(x) / (tyi(x) * tyi(y)) ==> (tyi(1) / tyi(y))
       56       56              QQ:  tyi(x) / (tyi(y) * tyi(x)) ==> (tyi(1) / tyi(y))
       57       57              RR:  tyi(x) / (tyi(x) shl n)  ==> tyi(0)
       58       58         @    SS:  tyi(x) ** ^2             ==> tyi(x) * tyi(x) * tyi(x) ->->
       59       59         @    TT:  tyi(x) ** ^2 + 1         ==>  "     "  "     " -->
       60       60         @    UU:  tyi(x) ** ^2 + 2         ==>  "     "  "     " --> * *
       61       61         @    VV:  tyi(x) ** ^2 + 1 * ^2    ==>  "     "  "     " -->  * * * *
       62       62              WW:  CVID4 (tyi(8))           ==> tyd(8)
       63       63         @    XX:  CVDI (ADD (CVID (tyi), tyd)) ==> ADI (tyi, CVDI (tyd))
       64       64              YY:  tyi(5) - (tyi(3) - tyi(x)) ==> tyi(5) - tyi(3) + tyi(x)
       65       65              ZZ:  tyi(x) - (tyi(x) + tyi(y)) ==> (- tyi(y))
       66       66             AAA:   CVIR (tyi(4))               ==> TYR
       67       67             BBB:   CVIQ (tyi(4))               ==> TYQ
       68       68             CCC:   CVDRR (TYD)                 ==> TYR
       69       69             DDD:   CVDQR (TYD)                 ==> TYQ
       70       70             EEE:   tyi(x) - (tyi(y) + tyi(x))  ==> (- tyi(y))
       71       71             FFF:   tyi(5) - (tyi(x) + tyi(3))  ==> tyi(5) - tyi(3) - tyi(x)
       72       72             GGG:   CVDI (TYD)                  ==> TYI
       73       73             HHH:   tyi(x) / tyi(1)             ==> tyi(x)
       74       74             III:   tyi(x) shl 2 / tyi(28)      ==> tyi(x) / tyi(28 shr 2)
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:3    
       75       75             JJJ:   (tyd(x) * tyd(y)) / tyd(x)  ==> tyd(y)
       76       76             KKK:   (tyd(x) * tyd(y)) / tyd(y)  ==> tyd(x)
       77       77             LLL:   (tyi(x) shl m) shl n        ==> tyi(x) shl (n + m)
       78       78             MMM:   (tyi(x) shr m) shl n        ==> tyi(x) shl (n - m) if n > m
       79       79             NNN:   (tyi(x) shr m) shl n        ==> tyi(x)             if n = m
       80       80             OOO:   (tyi(x) shr m) shl n        ==> tyi(x) shr (m - n) if n < m
       81       81             PPP:   (tyi(x) shr m) shr n        ==> tyi(x) shr (n + m)
       82       82             QQQ:   (tyi(x) shl m) shr n        ==> tyi(x) shr (n - m) if n > m
       83       83             RRR:   (tyi(x) shl m) shr n        ==> tyi(x)             if n = m
       84       84             SSS:   (tyi(x) shl m) shr n        ==> tyi(x) shl (m - n) if n < m
       85       85             TTT:   CMPD(CVID(tyi),tyd)         ==> CMPI(tyi,CVDI(tyd))
       86       86             UUU:   CMPD(tyd,CVID(tyi))         ==> CMPI(CVDI(tyd),tyi)
       87       87
       88       88
       89       89         @ indicates that these optimizations are inhibited if this
       90       90           is part of an "ON SIZE ERROR" expression.
       91       91
       92       92        */
       93       93        %EJECT;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:4    
       94       94
       95       95        %EJECT;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:5    
       96       96
       97       97            %SET PHASE = 1;
       98       98             %INCLUDE Z8A_DEFC;
       99      166             %INCLUDE Z8A_SUPP_MAC;
      100     2566             %INCLUDE Z8A_COMCOM;
      101     3216             %INCLUDE Z8A_DESC;
      102     3262             %INCLUDE Z8A_ETT;
      103     3563             %INCLUDE Z8A_ST;
      104     7222             %INCLUDE Z8A_TYPES;
      105     7396             %INCLUDE Z8A_DNT;
      106     7617             %INCLUDE Z8A_LTS;
      107     9694             %INCLUDE Z8A_SRCH;
      108     9743             %INCLUDE Z8A_MKCN;
      109     9760             %INCLUDE Z8E_STACKS ;
      110    12026             %INCLUDE Z8E_WINDOW;
      111    14096
      112    14097    1        DCL ID SBIN WORD;
      113    14098    1        DCL LOP$ PTR;
      114    14099    1        DCL ROP$ PTR;
      115    14100    1        DCL RESULT$ PTR;
      116    14101    1        DCL ON_SIZE_FLAG BIT (1) ALIGNED ; /* For On Size Error */
      117    14102    1        DCL ON_SIZE BIT (1) ALIGNED ;      /* Normally '0'B     */
      118    14103    1        DCL SYMBOL  UBIN WORD ;
      119    14104    1        DCL LOP PTR AUTO;
      120    14105    1        DCL LOPLOP  PTR ;
      121    14106    1        DCL SIGNIFICANCE SBIN WORD;
      122    14107
      123    14108    1        DCL 1 ROPCON AUTO ALIGNED,
      124    14109                   %DESCRIPTOR(ENDING = YES);
      125    14137
      126    14138    1        DCL ROPCON_UBIN REDEF ROPCON UBIN;
      127    14139    1        DCL ROP REDEF ROPCON PTR;
      128    14140    1        DCL 1  ROP_FILL REDEF ROPCON,
      129    14141    1             2 CHAR_1   CHAR (1) UNAL,
      130    14142    1             2 UBIN_3   UBIN (27) UNAL;
      131    14143
      132    14144    1        DCL 1 TYPE_DESC ALIGNED,
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:6    
      133    14145                   %TYPE_DESC;
      134    14179    1        DCL CONST$ PTR;
      135    14180
      136    14181    1        DCL DNT$$ PTR;
      137    14182    1        DCL LX_TERMINAL$ PTR ;
      138    14183    1        DCL ZIP CHAR(20) CONSTANT INIT('00000000000000000000');
      139    14184    1        DCL   Z8AC$MAKE_CONST ENTRY (1); /* Make a COMP-6 constant */
      140    14185    1        DCL   Z8AC$MAKE_CONST_R ENTRY (1) ; /* Make COMP-11 const  */
      141    14186    1        DCL   Z8AC$MAKE_CONST_D ENTRY(1);
      142    14187    1        DCL   Z8AC$MAKE_CONST_O ENTRY(1);
      143    14188    1        DCL Z8AU$TYPE_DESCRIPTOR ENTRY(2);
      144    14189    1        DCL Z8AS$SR_ETT          ENTRY (1);
      145    14190    1        DCL Z8EC$CREATE_CONST ENTRY (3)  ALTRET;
      146    14191    1        DCL Z8AU$BYTE_TO_WORD_UNITS ENTRY (3);
      147    14192
      148    14193
      149    14194    1        DCL LOP_ST$ PTR;
      150    14195    1        DCL ROP_ST$ PTR;
      151    14196    1        DCL LOP_STRING$ PTR;
      152    14197    1        DCL ROP_STRING$ PTR;
      153    14198    1        DCL LOP_LEN SBIN WORD;
      154    14199    1        DCL ROP_LEN SBIN WORD;
      155    14200
      156    14201    1        DCL LOP_CONST SBIN WORD;
      157    14202    1        DCL ROP_CONST SBIN WORD;
      158    14203
      159    14204    1        DCL NEW_ID SBIN WORD;
      160    14205    1        DCL   INTEGER   SBIN WORD;
      161    14206    1        DCL   INTEGER2  SBIN WORD;
      162    14207    1        DCL   POWER_OF_2 SBIN WORD;
      163    14208    1        DCL   POWER_OF_2_ SBIN WORD;
      164    14209    1        DCL   SBIN_J       SBIN WORD;
      165    14210    1        DCL   SBIN_I       SBIN WORD;
      166    14211    1        DCL 1 ONE_FLOAT ALIGNED STATIC,         /* Bin exponent real one */
      167    14212    1            2 EXPONENT  SBIN (8) UNAL INIT (1),
      168    14213    1            2 MANTISSA  BIT (28) UNAL INIT ('0100000000000000000000000000'B);
      169    14214    1        DCL   FLOAT_ONE REDEF ONE_FLOAT SBIN WORD;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:7    
      170    14215    1        DCL 1 ONE_HEX_F ALIGNED STATIC,         /* Hex exponent real one */
      171    14216    1            2 EXPONENT  SBIN (8) UNAL INIT (1),
      172    14217    1            2 MANTISSA  BIT (28) UNAL INIT ('0000100000000000000000000000'B);
      173    14218    1        DCL   HEX_F_ONE REDEF ONE_HEX_F SBIN WORD;
      174    14219    1        DCL 1 PACKED,
      175    14220    1             2 OFFSET   SBIN HALF HALIGNED,
      176    14221    1             2 TYPE     UBIN HALF HALIGNED;
      177    14222
      178    14223             %DCL_AUTO_LX_ITEM (@NAME = LX_NUMBER) ;
      179    14227
      180    14228        %EJECT;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:8    
      181    14229    1        ON_SIZE = ON_SIZE_FLAG ;
      182    14230    1        GOTO FORME_START ;
      183    14231
      184    14232    1   Z8AF$FORME: ENTRY (ID, LOP$, ROP$, RESULT$) ;
      185    14233
      186    14234    1        ON_SIZE = '0'B ;
      187    14235
      188    14236    1   FORME_START: ; /* Some optimizations are inhibited if ON_SIZE is on */
      189    14237
      190    14238
      191    14239        /* If the user passes the same pointer for either the LOP or the
      192    14240           ROP as he does for the RESULT$, then we have to protect him.
      193    14241           We use RESULT$ as an intermediate result as well as the return
      194    14242           result.  So... if we happen to reference the LOP, or the ROP
      195    14243           (which ever = RESULT$).... after using RESULT$ for our own
      196    14244           purposes.... *BANG*!
      197    14245
      198    14246        */
      199    14247
      200    14248    1        LOP = LOP$;
      201    14249    1        ROP = ROP$;
      202    14250
      203    14251        %EJECT;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:9    
      204    14252        /*V* IDEAL_STATE
      205    14253
      206    14254             Assure that any constant appears on the ROP of a commutative
      207    14255           operator;  also assure that expressions are on the LOP.
      208    14256
      209    14257        */
      210    14258    2        DO CASE(ID);
      211    14259    2          CASE(%MPI, %MPD, %MPQ, %MPR, %MPO, %ADI, %ADD, %ADR, %ADQ, %ADO, %ORI, %ANDI)
             14259                   ;
      212    14260    2            IF (LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID ~= %TRIAD)
      213    14261    2            OR (LOP -> ETT.ID <= %LAST_REFERENCE_TRIAD
      214    14262    2                  AND ROP -> ETT.ID > %LAST_REFERENCE_TRIAD) THEN
      215    14263    3              DO;
      216    14264    3                CALL Z8AF$FORME(ID, ROP, LOP, RESULT$);
      217    14265    3                GOTO EXIT_WITH_TRANSFORMATION;
      218    14266    3              END;
      219    14267
      220    14268
      221    14269    2        END /* Case */;
      222    14270
      223    14271        %EJECT;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:10   
      224    14272
      225    14273    2        DO CASE(ID);
      226    14274    2          CASE(%INDEX);
      227    14275    2            IF LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID = %TRIAD THEN
      228    14276    3   A1:        DO;
      229    14277    3                LOP_ST$ = LOP -> ETT.LOP;
      230    14278    3                ROP_ST$ = ROP -> ETT.LOP;
      231    14279
      232    14280    3                LOP_LEN = LOP_ST$ -> CONST.ADSC.N;
      233    14281    3                ROP_LEN = ROP_ST$ -> CONST.ADSC.N;
      234    14282
      235    14283                     %GET_CONSTANT_ADDRESS ( @CONSTANT = LOP_ST$,
      236    14284                                             @STRING   = LOP_STRING$ );
      237    14292
      238    14293                     %GET_CONSTANT_ADDRESS ( @CONSTANT = ROP_ST$,
      239    14294                                             @STRING   = ROP_STRING$ );
      240    14302
      241    14303    3                CALL INDEX ( MKCN.CON,
      242    14304    3                             SUBSTR(LOP_STRING$ -> CHARS, 0, LOP_LEN),
      243    14305    3                             SUBSTR(ROP_STRING$ -> CHARS, 0, ROP_LEN));
      244    14306
      245    14307    3                CALL Z8AC$MAKE_CONST(RESULT$);
      246    14308
      247    14309    3                GOTO EXIT_WITH_TRANSFORMATION;
      248    14310    3              END;
      249    14311
      250    14312    2   A2:      /* if length(LOP) > length(ROP) then RESULT is length(ROP) */
      251    14313
      252    14314    2            CALL Z8AU$TYPE_DESCRIPTOR (LOP, TYPE_DESC) ;
      253    14315
      254    14316    3            IF TYPE_DESC.SIZ$ = ADDR(NIL) THEN DO ;
      255    14317
      256    14318                   %UNPACK_ADSC (@DESC    = TYPE_DESC.DESC,
      257    14319                                 @LENGTH  = LOP_LEN) ;
      258    14342    3              CALL Z8AU$TYPE_DESCRIPTOR (ROP, TYPE_DESC) ;
      259    14343
      260    14344    4              IF TYPE_DESC.SIZ$ = ADDR(NIL) THEN DO ;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:11   
      261    14345
      262    14346                     %UNPACK_ADSC (@DESC   = TYPE_DESC.DESC,
      263    14347                                   @LENGTH = ROP_LEN) ;
      264    14370
      265    14371    5                IF LOP_LEN > ROP_LEN THEN DO ;
      266    14372
      267    14373    5                  MKCN.CON = ROP_LEN ;
      268    14374    5                  CALL Z8AC$MAKE_CONST (RESULT$) ;
      269    14375    5                  GOTO EXIT_WITH_TRANSFORMATION ;
      270    14376
      271    14377    5                END ; /* Length(LOP) > Length(ROP) */
      272    14378
      273    14379    4              END ; /* Length(ROP) is constant */
      274    14380
      275    14381    3            END ; /* Length(LOP) is constant */
      276    14382
      277    14383    2          CASE(%ADI);
      278    14384    2            IF LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID = %TRIAD THEN
      279    14385    3   B:         DO;
      280    14386    3                MKCN.CON = LOP -> ETT.LOP -> CONST.CONSTI.WORD1 +
      281    14387    3                           ROP -> ETT.LOP -> CONST.CONSTI.WORD1;
      282    14388
      283    14389    3                CALL Z8AC$MAKE_CONST(RESULT$);
      284    14390
      285    14391    3                GOTO EXIT_WITH_TRANSFORMATION;
      286    14392    3              END;
      287    14393
      288    14394    2          CASE(%SBI);
      289    14395    2            IF LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID = %TRIAD THEN
      290    14396    3   C:         DO;
      291    14397    3                MKCN.CON = LOP -> ETT.LOP -> CONST.CONSTI.WORD1 -
      292    14398    3                           ROP -> ETT.LOP -> CONST.CONSTI.WORD1;
      293    14399
      294    14400    3                CALL Z8AC$MAKE_CONST(RESULT$);
      295    14401
      296    14402    3                GOTO EXIT_WITH_TRANSFORMATION;
      297    14403    3              END;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:12   
      298    14404
      299    14405    2          CASE(%MPI);
      300    14406    2            IF ON_SIZE = '0'B
      301    14407    2            AND LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID = %TRIAD THEN
      302    14408    3   D:         DO;
      303    14409    3                MKCN.CON = LOP -> ETT.LOP -> CONST.CONSTI.WORD1 *
      304    14410    3                           ROP -> ETT.LOP -> CONST.CONSTI.WORD1;
      305    14411
      306    14412    3                CALL Z8AC$MAKE_CONST(RESULT$);
      307    14413
      308    14414    3                GOTO EXIT_WITH_TRANSFORMATION;
      309    14415    3              END;
      310    14416
      311    14417    2          CASE(%DVI);
      312    14418    2            IF LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID = %TRIAD THEN
      313    14419    3   E:         DO;
      314    14420    3                MKCN.CON = LOP -> ETT.LOP -> CONST.CONSTI.WORD1 /
      315    14421    3                           ROP -> ETT.LOP -> CONST.CONSTI.WORD1;
      316    14422
      317    14423    3                CALL Z8AC$MAKE_CONST(RESULT$);
      318    14424
      319    14425    3                GOTO EXIT_WITH_TRANSFORMATION;
      320    14426    3              END;
      321    14427
      322    14428    2          CASE(%USI);
      323    14429    2            IF LOP -> ETT.ID = %TRIAD THEN
      324    14430    3   F:         DO;
      325    14431    3                MKCN.CON = - (LOP -> ETT.LOP -> CONST.CONSTI.WORD1);
      326    14432
      327    14433    3                CALL Z8AC$MAKE_CONST(RESULT$);
      328    14434
      329    14435    3                GOTO EXIT_WITH_TRANSFORMATION;
      330    14436    3              END;
      331    14437
      332    14438    2          CASE(%MINI);
      333    14439    2            IF LOP -> ETT.ID = %TRIAD AND ROP -> ETT.ID = %TRIAD THEN
      334    14440    3   G:         DO;
PL6.E3A0      #001=Z8AF$FORME_ON_SIZE File=Z8AF.:Z8A6TSI                         SAT 08/09/97 10:43 Page:13   
      335    14441    3                LOP_CONST = LOP -> ETT.LOP -> CONST.CONSTI.WORD1;
      336    14442    3                ROP_CONST = ROP -> ETT.LOP -> CONST.CONSTI.WORD1;
      337    14443
      338    14444    3                IF LOP_CONST <= ROP_CONST THEN
      339    14445    3                  RESULT$ = LOP;
      340    14446    3                ELSE
      341    14447    3                  RESULT$ = ROP;
      342    14448
      343    14449    3                GOTO EXIT_WITH_TRANSFORMATION;
      344    14450    3              END;
