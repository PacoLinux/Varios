       12       12
       13       13        OCNFG:  PROC;
       14       14
       15       15        /*D*
       16       16               NAME: OCNFG
       17       17            PURPOSE: ORDER THE CONFIGURATION SETS IN PREPARATION
       18       18                     FOR PRODUCING TABLES.
       19       19        */
       20       20
       21       21
       22       22        %INCLUDE LR_GRAM_EXT;
       23      561        %INCLUDE LR_CNFG_EXT;
       24      714        /* If TABLES are going to be written, then the configuration
       25      715           sets must be re-ordered to compatify the resulting tables.
       26      716
       27      717           Any particular configuration set will fall into 1 of 4
       28      718           catagories:
       29      719
       30      720           0) Special configuration that is the final state. always mapped to 0
       31      721           1) Only binary search table entries are required.
       32      722           2) Both binary search and bit table search entries required.
       33      723           3) Only bit table search entries required.
       34      724        */
       35      725
       36      726    1   DCL CONFIG_MAP(0:%NR_CONFIG_MAX) SBIN;
       37      727
PL6.E3A0      #001=OCNFG File=LR_OCNFG.:Z8A6TSI                                  SAT 08/09/97 09:36 Page:2    
       38      728        %EJECT;
PL6.E3A0      #001=OCNFG File=LR_OCNFG.:Z8A6TSI                                  SAT 08/09/97 09:36 Page:3    
       39      729    1   MAKE_MAP:PROC;
       40      730
       41      731    2   DCL 1 SPLIT(0:%NR_CONFIG_MAX),
       42      732    2         2 BITSRCH SBIN HALF UNAL,
       43      733    2         2 BOTH SBIN HALF UNAL;
       44      734
       45      735    2   DCL I                SBIN;
       46      736    2   DCL NR_BOTH          SBIN;
       47      737    2   DCL NR_BITSRCH       SBIN;
       48      738    2   DCL NR_BINSRCH       SBIN;
       49      739    2   DCL C$               PTR;
       50      740    2   DCL KEY0$            PTR;
       51      741    2   DCL KEYN$            PTR;
       52      742    2   DCL I#               SBIN;
       53      743    2   DCL KEYI$            PTR;
       54      744    2   DCL REDUCT_COUNT     SBIN;
       55      745    2   DCL NR_MARGINAL      SBIN;
       56      746    2   DCL NR_BIT_MUST      SBIN;
       57      747    2   DCL MORE_REDUCTIONS  BOOLEAN;
       58      748
       59      749
       60      750    2           NR_BOTH=0;
       61      751    2           NR_BINSRCH=2;
       62      752    2           NR_BITSRCH=0;
       63      753
       64      754    3           DO I#=0 TO NR_CONFIG-1;
       65      755
       66      756    3              C$=C#_TO_$(I#);
       67      757
       68      758        /* Examine all reductions */
       69      759
       70      760    3              REDUCT_COUNT=0;
       71      761
       72      762    3              MORE_REDUCTIONS=TRUE;
       73      763
       74      764    4              DO WHILE(MORE_REDUCTIONS);
       75      765
PL6.E3A0      #001=OCNFG File=LR_OCNFG.:Z8A6TSI                                  SAT 08/09/97 09:36 Page:4    
       76      766    4                 IF REDUCT_COUNT>=C$->C_SET_B.NR_ELEM THEN
       77      767    4                    MORE_REDUCTIONS=FALSE;
       78      768    4                 ELSE
       79      769    5                 DO;
       80      770    5                    KEYI$=C$->C_SET_B.ELEM.KEY$(REDUCT_COUNT);
       81      771    5                    IF KEYI$->KEY_B.W.SYM_AFTER~=0 THEN
       82      772    5                       MORE_REDUCTIONS=FALSE;
       83      773    5                    ELSE
       84      774    6                    DO;     /* found another reduction     */
       85      775    6                       REDUCT_COUNT=REDUCT_COUNT+1;
       86      776
       87      777    6                       KEYI$->KEY_B.FOLLOW$->INCL_B.KEY_TYPE=KEY_TYPE_BIT;
       88      778    6                       KEYI$->KEY_B.FOLLOW$->INCL_B.BIT_COUNT=0;
       89      779
       90      780    6                       END;
       91      781    5                    END;
       92      782    4                 END;
       93      783    3              C$ -> C_SET_B.REDUCE_COUNT = REDUCT_COUNT;
       94      784    3              KEY0$=C$->C_SET_B.ELEM.KEY$(0);
       95      785    3              KEYN$=C$->C_SET_B.ELEM.KEY$(C$->C_SET_B.NR_ELEM-1);
       96      786
       97      787    3              IF C$ -> C_SET_B.KEY$ -> KEY_B.W.SYM_AFTER = BOF_SYMBOL_PTR->SYMBOL.
               787                       SYM_INDX THEN
       98      788    4                DO;
       99      789    4                  CONFIG_MAP(1) = I#;
      100      790    4                  C$ -> C_SET_B.KEY$ -> KEY_B.W.KEY_TYPE = KEY_TYPE_BIN;
      101      791    4                END;
      102      792    3              ELSE
      103      793    3              IF KEY0$->KEY_B.W.SYM_AFTER=0 AND
      104      794    3                KEYN$->KEY_B.W.SYM_AFTER~=0 THEN
      105      795    4   ON_BINSRCH_REDUC:DO;  /* Reductions and shifts in set */
      106      796    4                 SPLIT.BOTH(NR_BOTH)=I#;
      107      797    4                 NR_BOTH=NR_BOTH+1;
      108      798    4                 C$->C_SET_B.KEY$->KEY_B.W.KEY_TYPE=KEY_TYPE_BOTH;
      109      799    4                 END ON_BINSRCH_REDUC;
      110      800    3              ELSE
      111      801    3                 IF KEY0$->KEY_B.W.SYM_AFTER~=0 THEN
PL6.E3A0      #001=OCNFG File=LR_OCNFG.:Z8A6TSI                                  SAT 08/09/97 09:36 Page:5    
      112      802    4   ON_BINSRCH_ONLY_LIST:DO;   /* Only shifts in set */
      113      803    4                    CONFIG_MAP(NR_BINSRCH)=I#;
      114      804    4                    NR_BINSRCH=NR_BINSRCH+1;
      115      805    4                    C$->C_SET_B.KEY$->KEY_B.W.KEY_TYPE=KEY_TYPE_BIN;
      116      806    4                    END ON_BINSRCH_ONLY_LIST;
      117      807    3                 ELSE
      118      808    3                    IF KEY0$->KEY_B.ELEM.PROD(0)~=0 THEN
      119      809    4   ON_BITSRCH_ONLY_LIST:DO;    /* Only reductions in set */
      120      810    4                       SPLIT.BITSRCH(NR_BITSRCH)=I#;
      121      811    4                       NR_BITSRCH=NR_BITSRCH+1;
      122      812    4                       C$->C_SET_B.KEY$->KEY_B.W.KEY_TYPE=KEY_TYPE_BIT;
      123      813    4                       END ON_BITSRCH_ONLY_LIST;
      124      814    3                    ELSE
      125      815    4                    DO; /* THIS IS THE FINAL PRODUCTION    */
      126      816    4                       CONFIG_MAP(0)=I#;
      127      817    4                       END;
      128      818
      129      819    3              END;
      130      820
      131      821    2           LAST_BINSRCH_ONLY=NR_BINSRCH-1;
      132      822
      133      823    3           DO I=0 TO NR_BOTH-1;
      134      824    3              CONFIG_MAP(I+LAST_BINSRCH_ONLY+1)=SPLIT.BOTH(I);
      135      825    3              END;
      136      826
      137      827    2           LAST_BOTH_BITBIN=LAST_BINSRCH_ONLY+NR_BOTH;
      138      828
      139      829    3           DO I=0 TO NR_BITSRCH-1;
      140      830    3              CONFIG_MAP(LAST_BOTH_BITBIN+I+1)=SPLIT.BITSRCH(I);
      141      831    3              END;
      142      832
      143      833    2   END MAKE_MAP;
      144      834
      145      835        %EJECT;
PL6.E3A0      #001=OCNFG File=LR_OCNFG.:Z8A6TSI                                  SAT 08/09/97 09:36 Page:6    
      146      836    1   DO_MAPPING:PROC;
      147      837
      148      838    2   DCL NEW_C#_TO_$(0:%NR_CONFIG_MAX) PTR;
      149      839    2   DCL I SBIN;
      150      840    2   DCL J SBIN;
      151      841
      152      842    3           DO I=0 TO NR_CONFIG-1;
      153      843
      154      844    3              NEW_C#_TO_$(I)=C#_TO_$(CONFIG_MAP(I));
      155      845
      156      846    3              END;
      157      847
      158      848    3           DO I=0 TO NR_CONFIG-1;
      159      849
      160      850    3              NEW_C#_TO_$(I)->C_SET_B.C_SET#=I;
      161      851    3              C#_TO_$(I)=NEW_C#_TO_$(I);
      162      852
      163      853    3              END;
      164      854
      165      855    2   END DO_MAPPING;
      166      856        %EJECT;
PL6.E3A0      #001=OCNFG File=LR_OCNFG.:Z8A6TSI                                  SAT 08/09/97 09:36 Page:7    
      167      857                   /* Main body of OCNFG */
      168      858
      169      859
      170      860    1           CALL MAKE_MAP;
      171      861    1           CALL DO_MAPPING;
      172      862    1           INIT_CONFIG=CONFIG_MAP(0);
      173      863
      174      864    1   END OCNFG;
      175      865        %EOD;

