      211      726    2   DCL SUCC_STATE_P   PTR;
      212      727    2   DCL E2             SBIN WORD;
      213      728    2   DCL I2             SBIN WORD;
      214      729    2   DCL NEW_DOT_POS    UBIN HALF;
      215      730    2   DCL PROD           UBIN HALF;
      216      731    2   DCL SUCC_FOUND     BOOLEAN;
      217      732
      218      733
      219      734    2   SUCC_STATE_P   = C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E)->KEY_B.KEY_VAL$;
      220      735    2   NEW_DOT_POS    = C#_TO_$(STATE)->C_SET_B.ELEM.KEY$(E) ->KEY_B.ELEM.DOT_POS(I) + 1;
      221      736    2   PROD           = C#_TO_$(STATE)->C_SET_B.ELEM.KEY$(E)->KEY_B.ELEM.PROD(I);
      222      737    2   SUCC_FOUND     = FALSE;
      223      738    2   E2             = 0;
      224      739
      225      740    3   DO UNTIL SUCC_FOUND;
      226      741
      227      742    3     I2 = 0;
      228      743
      229      744    4     DO WHILE (NOT SUCC_FOUND) AND
      230      745    4              (I2 < SUCC_STATE_P -> C_SET_B.ELEM.KEY$(E2) -> KEY_B.W.ELEM_COUNT);
      231      746
      232      747    4       SUCC_FOUND = (PROD = SUCC_STATE_P -> C_SET_B.ELEM.KEY$(E2) ->
      233      748    4                                        KEY_B.ELEM.PROD(I2))
      234      749    4            & (NEW_DOT_POS = SUCC_STATE_P -> C_SET_B.ELEM.KEY$(E2) ->
      235      750    4                                        KEY_B.ELEM.DOT_POS(I2));
      236      751    4       IF NOT SUCC_FOUND THEN
      237      752    4         I2 = I2 + 1;
      238      753
      239      754    4     END;
      240      755
      241      756    3     IF NOT SUCC_FOUND THEN
      242      757    3       E2 = E2 + 1;
PL6.E3A0      #002=CALC_ITEM_NUMBERS File=LR_OCNFG.:Z8A6TSI                      SAT 08/09/97 09:36 Page:39   
      243      758
      244      759    3   END;
      245      760
      246      761    2   C#_TO_$(STATE) ->C_SET_B.ELEM.ITEM$(E)->ITEM_B.ELEM.SUCC_ITEM#(I) =
      247      762    2          SUCC_STATE_P -> C_SET_B.ELEM.ITEM$(E2) -> ITEM_B.ELEM.ITEM#(I2);
      248      763
      249      764    2   END FINDSUCC;
      250      765
      251      766
      252      767        %EJECT;
PL6.E3A0      #002=CALC_ITEM_NUMBERS File=LR_OCNFG.:Z8A6TSI                      SAT 08/09/97 09:36 Page:40   
      253      768        /* Main body of CAL_ITEM_NUMBERS */
      254      769
      255      770    2   DO STATE = 0 TO NR_CONFIG - 1;
      256      771
      257      772    2     ITEM_CNT = 0;
      258      773
      259      774          /* Count all basis items */
      260      775
      261      776    3     DO E = 0 TO C#_TO_$(STATE) -> C_SET_B.NR_ELEM - 1;
      262      777
      263      778            /* ALLOCATE THE ITEM SETS */
      264      779    3       CALL GETNEW (C#_TO_$(STATE)->C_SET_B.ELEM.ITEM$(E),
      265      780    3                    BINBIT((C#_TO_$(STATE)->C_SET_B.ELEM.KEY$(E)->
      266      781    3                        KEY_B.W.ELEM_COUNT)*SIZEW(ITEM_B.ELEM(0)),36));
      267      782
      268      783    4       DO I = 0 TO C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E)->KEY_B.W.ELEM_COUNT - 1;
      269      784
      270      785    4         IF C#_TO_$(STATE)->C_SET_B.ELEM.KEY$(E) -> KEY_B.ELEM.DOT_POS(I) > 0 THEN
      271      786    5           DO;
      272      787    5             C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(E)->ITEM_B.ELEM.ITEM#(I) = ITEM_CNT;
      273      788    5             ITEM_CNT = ITEM_CNT + 1;
      274      789    5           END;
      275      790
      276      791    4       END;
      277      792    3     END;
      278      793
      279      794    2     C#_TO_$(STATE) -> C_SET_B.BASIS_COUNT = ITEM_CNT;
      280      795    2   MAX_BASIS_ITEMS = MAXIMUM(MAX_BASIS_ITEMS, ITEM_CNT);
      281      796
      282      797          /* Get all closure items */
      283      798
      284      799    3     DO E = 0 TO C#_TO_$(STATE) -> C_SET_B.NR_ELEM - 1;
      285      800
      286      801    4       DO I = 0 TO C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E)->KEY_B.W.ELEM_COUNT - 1;
      287      802
      288      803    4         IF C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.ELEM.DOT_POS(I) = 0 THEN
      289      804    5           DO;
PL6.E3A0      #002=CALC_ITEM_NUMBERS File=LR_OCNFG.:Z8A6TSI                      SAT 08/09/97 09:36 Page:41   
      290      805    5             C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(E) -> ITEM_B.ELEM.ITEM#(I) = ITEM_CNT
               805                      ;
      291      806    5             ITEM_CNT = ITEM_CNT + 1;
      292      807    5           END;
      293      808
      294      809    4       END;
      295      810    3     END;
      296      811
      297      812    2     C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT = ITEM_CNT;
      298      813    2   MAX_TOTAL_ITEMS = MAXIMUM(MAX_TOTAL_ITEMS, ITEM_CNT);
      299      814
      300      815    2   END;  /* DO ON STATE */
      301      816
      302      817        /* Calculate successor item #S */
      303      818
      304      819    2   DO STATE = 0 TO NR_CONFIG - 1;
      305      820
      306      821    3     DO E = 0 TO C#_TO_$(STATE) -> C_SET_B.NR_ELEM - 1;
      307      822
      308      823    3       IF C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.W.SYM_AFTER > 0 THEN
