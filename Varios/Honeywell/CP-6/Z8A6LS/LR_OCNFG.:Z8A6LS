      200      715
      201      716
      202      717
      203      718
      204      719    1   FINDSUCC : PROC;  /* OF CALC_ITEM_NUMBERS */
      205      720        %EJECT;
PL6.E3A0      #002=CALC_ITEM_NUMBERS File=LR_OCNFG.:Z8A6TSI                      SAT 08/09/97 09:36 Page:38   
      206      721
      207      722        /* Find the successor item # in the successor state for the
      208      723           current item in the current state.
      209      724        */
