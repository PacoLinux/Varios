   Procedure PRINT_PREFIXES_FOR_TERMINAL requires 126 words for executable code.
   Procedure PRINT_PREFIXES_FOR_TERMINAL requires 16 words of local(AUTO) storage.

PL6.E3A0      #007=EXPAND_PREFIX File=LR_ERRREC.:Z8A6TSI                         SAT 08/09/97 09:30 Page:193  
      631        1        /*T***********************************************************/
      632        2        /*T*                                                         */
      633        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      634        4        /*T*                                                         */
      635        5        /*T***********************************************************/
      636        6        EXPAND_PREFIX : PROC (NT, T);
      637        7
      638        8        /*D*
      639        9               NAME: EXPAND_PREFIX
      640       10            PURPOSE:
      641       11
      642       12           Expand the current prefix entry (NT) into
      643       13           references to least_cost_strings.  Link this string into
      644       14           the LEAST_COST_PREFIX lists.
      645       15           Format of the prefix string is:
      646       16
      647       17               half word
      648       18               ----------
      649       19                   0    - string length (N)
      650       20                   1    - terminal index
      651       21                   2-3  - link to next_entry (terminal) for this non_terminal
      652       22                   4-N  - Indicies to least cost strings forming prefix
      653       23
      654       24        */
      655       25
      656       26    1   DCL NT SBIN WORD;
      657       27    1   DCL T SBIN WORD;
      658       28
      659       29        %INCLUDE LR_GRAM_EXT;
      660      568        %INCLUDE LR_CNFG_EXT;
      661      721        %INCLUDE LR_ERR_EXT;
      662      864
      663      865    1   DCL GET_STRING_MEM ENTRY(2);
      664      866
      665      867    1   DCL RP     SBIN WORD;
      666      868    1   DCL STRLEN SBIN WORD;
      667      869    1   DCL SYM    SBIN WORD;
PL6.E3A0      #007=EXPAND_PREFIX File=LR_ERRREC.:Z8A6TSI                         SAT 08/09/97 09:30 Page:194  
      668      870    1   DCL SP     PTR;
      669      871
      670      872
      671      873    1   SYM = RIGHT_PARTS(PROD.RIGHT_START(CURRE.PROD(NT)) + CURRE.INDX(NT));
      672      874
      673      875    1   IF NOT CURRE.EXPANDED(SYM) THEN
      674      876    1     CALL EXPAND_PREFIX (SYM, T);
      675      877
      676      878    1   STRLEN = LEAST_COST_PREFIX(SYM) -> PREFIX.LEN_IN_H + CURRE.INDX(NT);
      677      879
      678      880    1   CALL GET_STRING_MEM (SP, STRLEN);
      679      881
      680      882    1   SP -> PREFIX.LINK     = LEAST_COST_PREFIX(NT);
      681      883    1   LEAST_COST_PREFIX(NT) = SP;
      682      884    1   SP -> PREFIX.TERM     = T;
      683      885
      684      886    2   DO RP = 0 TO CURRE.INDX(NT) - 1;
      685      887
      686      888    2     SP -> PREFIX.TOK(RP) = RIGHT_PARTS(RP + PROD.RIGHT_START(CURRE.PROD(NT)));
      687      889
      688      890    2   END;
      689      891
      690      892    1   SUBSTR( SP -> PREFIXCHR.TOKCHR,
      691      893    1             CURRE.INDX(NT) * 2,
      692      894    1           (LEAST_COST_PREFIX(SYM) -> PREFIX.LEN_IN_H - 3) * 2)
      693      895    1        = SUBSTR (LEAST_COST_PREFIX(SYM) -> PREFIXCHR.TOKCHR,
      694      896    1                  0,
      695      897    1                  (LEAST_COST_PREFIX(SYM) -> PREFIX.LEN_IN_H -3) * 2);
      696      898
      697      899    1   CURRE.EXPANDED(NT) = TRUE;
      698      900
      699      901    1   END EXPAND_PREFIX;
      700      902        %EOD;

PL6.E3A0      #007=EXPAND_PREFIX File=LR_ERRREC.:Z8A6TSI                         SAT 08/09/97 09:30 Page:195  
