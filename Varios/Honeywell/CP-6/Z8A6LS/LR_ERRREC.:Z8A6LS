PL6.E3A0      #006=PRINT_PREFIXES_FOR_TERMINAL File=LR_ERRREC.:Z8A6TSI           SAT 08/09/97 09:30 Page:162  
      551        1        /*T***********************************************************/
      552        2        /*T*                                                         */
      553        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      554        4        /*T*                                                         */
      555        5        /*T***********************************************************/
      556        6        PRINT_PREFIXES_FOR_TERMINAL : PROC (T);
      557        7
      558        8        /*D*
      559        9               NAME: PRINT_PREFIXES_FOR_TERMINAL
      560       10            PURPOSE:
      561       11
      562       12           Print all least cost prefixes for a particular
      563       13           terminal
      564       14        */
      565       15
      566       16    1   DCL T SBIN WORD;
      567       17
      568       18        %INCLUDE LR_GRAM_EXT;
      569      557        %INCLUDE LR_CNFG_EXT;
      570      710        %INCLUDE LR_ERR_EXT;
      571      853
      572      854    1   DCL OUT_TOKEN ENTRY(2);
      573      855    1   DCL OUT_SYMBOL ENTRY(1);
      574      856    1   DCL OUT_SYMBOL_TOKEN ENTRY(1);
      575      857    1   DCL NT SBIN WORD;
      576      858
      577      859
      578      860        %EJECT;
PL6.E3A0      #006=PRINT_PREFIXES_FOR_TERMINAL File=LR_ERRREC.:Z8A6TSI           SAT 08/09/97 09:30 Page:163  
      579      861    1   PRINT_PREFIX_STRING : PROC (SP);  /* OF PRINT_PREFIXES_FOR_TERMINAL */
      580      862
      581      863    2   DCL SP PTR;
      582      864
      583      865    2   DCL I    SBIN WORD;
      584      866    2   DCL J    SBIN WORD;
      585      867    2   DCL LCSI SBIN WORD;
      586      868
      587      869
      588      870    3   DO I = 0 TO SP->PREFIX.LEN_IN_H - 4;
      589      871
      590      872    3     LCSI = SP->PREFIX.TOK(I);
      591      873
      592      874    4     DO J = 0 TO LEAST_COST_STRING(LCSI)-> STRING.LEN_IN_H - 1;
      593      875
      594      876    4       CALL OUT_SYMBOL_TOKEN(SYM_PTRS(LEAST_COST_STRING(LCSI) ->STRING.TOK(J)));
      595      877
      596      878    4     END;
      597      879    3   END;
      598      880
      599      881    2   CALL FLUSH;
      600      882
      601      883    2   END PRINT_PREFIX_STRING;
      602      884
      603      885        %EJECT;
PL6.E3A0      #006=PRINT_PREFIXES_FOR_TERMINAL File=LR_ERRREC.:Z8A6TSI           SAT 08/09/97 09:30 Page:164  
      604      886         /* Start of Print_prefixes_for_terminal */
      605      887
      606      888        %SELECT_LO;
      607      891
      608      892        %OUTLINE(S="' '");
      609      898
      610      899        %OUT_STR(S="'Least cost prefixes for terminal '");
      611      902    1   CALL OUT_SYMBOL (SYM_PTRS(T));
      612      903    1   CALL FLUSH;
      613      904
      614      905        %OUTLINE(S="' '");
      615      911
      616      912    2   DO NT = NR_TERM + 1 TO LAST_NULL;
      617      913
      618      914    2     IF LEAST_COST_PREFIX(NT) ~= ADDR(NIL) THEN
      619      915    3       DO;
      620      916    3         CALL OUT_SYMBOL_TOKEN(SYM_PTRS(NT));
      621      917    3         IF LINELEN < 15 THEN
      622      918    3           CALL OUT_STR('               ',15-LINELEN);
      623      919              %OUT_STR(S="'-->'");
      624      922    3         CALL PRINT_PREFIX_STRING(LEAST_COST_PREFIX(NT));
      625      923    3       END;
      626      924
      627      925    2   END;
      628      926
      629      927    1   END PRINT_PREFIXES_FOR_TERMINAL;
      630      928        %EOD;

