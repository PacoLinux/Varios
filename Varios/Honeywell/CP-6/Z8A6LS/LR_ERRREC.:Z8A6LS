      121        6        CALC_LEAST_COST_STRINGS : PROC;

      6 72 000000   000000 700200 xent  CALC_LEAST_* TSX0  ! X66_AUTO_0
        72 000001   000016 000000                    ZERO    14,0

      122        7
      123        8        /*D*
      124        9               NAME: CALC_LEAST_COST_STRINGS
      125       10            PURPOSE:
      126       11
      127       12           Calculate all least cost strings S(A), where A is a
      128       13           non_terminal.  The algorithm used is:
      129       14
      130       15             For all non_terminals, A
      131       16               Do C(A) := Infinite;
      132       17
      133       18             Repeat
      134       19               Nochange := true;
      135       20               For all productions P = (A --> x1, ...xn) Do
      136       21                 If C(x1...xn) < C(A) then
      137       22                   Begin
      138       23                     C(A) := C(x1...xn);
      139       24                     S(A) := S(x1...xn);
      140       25                     Nochange := False;
      141       26                   End
      142       27             Until nochange;
      143       28
      144       29        */
      145       30
      146       31        %INCLUDE LR_GRAM_EXT;
      147      570        %INCLUDE LR_CNFG_EXT;
PL6.E3A0      #003=CALC_LEAST_COST_STRINGS File=LR_ERRREC.:Z8A6TSI               SAT 08/09/97 09:28 Page:55   
      148      723        %INCLUDE LR_ERR_EXT;
