   Procedure CREATE_LEAST_COST_STRING requires 12 words of local(AUTO) storage.

PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:112  
      320        1        /*T***********************************************************/
      321        2        /*T*                                                         */
      322        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      323        4        /*T*                                                         */
      324        5        /*T***********************************************************/
      325        6        CALC_LEAST_COST_PREFIX : PROC;
      326        7
      327        8        /*D*
      328        9               NAME: CALC_LEAST_COST_PREFIX
      329       10            PURPOSE:
      330       11
      331       12           Calculate all least cost prefixes E(A,b),
      332       13             where A is a terminal or a non_terminal, and
      333       14              b is a terminal.  Refer to:
      334       15
      335       16                 A LOCALLY LEAST COST LR ERROR CORRECTOR
      336       17                    by  FISCHER
      337       18                        COMPUTER SCIENCES DEPT REPORT #363
      338       19                        UNIV OF WISC
      339       20        */
      340       21
      341       22
      342       23        %INCLUDE LR_GRAM_EXT;
      343      562        %INCLUDE LR_CNFG_EXT;
      344      715        %INCLUDE LR_ERR_EXT;
      345      858
      346      859    1   DCL GET_STRING_MEM              ENTRY(2);
      347      860    1   DCL EXPAND_PREFIX               ENTRY(2);
      348      861    1   DCL PRINT_PREFIXES_FOR_TERMINAL ENTRY(1);
      349      862    1   DCL COLLECT_GARBAGE             ENTRY(1);
      350      863    1   DCL OUTPUT_LCP                  ENTRY(1);
      351      864
      352      865    1   DCL T        SBIN WORD;   /* TERMINAL */
      353      866    1   DCL NT       SBIN WORD;  /* NON_TERMINAL */
      354      867    1   DCL PN       SBIN WORD;
      355      868    1   DCL TP        PTR;
      356      869    1   DCL TEMPCOST SBIN WORD;
PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:113  
      357      870    1   DCL MINC     SBIN WORD;
      358      871    1   DCL I        SBIN WORD;
      359      872    1   DCL R        SBIN WORD;
      360      873    1   DCL S        SBIN WORD;
      361      874
      362      875        /* The character string NT tells which non_terminal prefixes have
      363      876           changed.  The character indexed by the NT is 'T' if changes
      364      877           or blank otherwise. */
      365      878
      366      879    1   DCL NT_CHANGED CHAR(%(NR_SYMS_MAX+1));
      367      880
      368      881    1   DCL NT_REF(0:NR_SYMS_MAX) PTR;
      369      882
      370      883    1   DCL 1 PRODREF ALIGNED BASED,
      371      884    1         2 LEN_IN_H UBIN HALF HALIGNED,
      372      885    1         2 PROD     SBIN HALF HALIGNED,
      373      886    1         2 POS      SBIN HALF HALIGNED,
      374      887    1         2 *        SBIN HALF HALIGNED,
      375      888    1         2 LINK     PTR ALIGNED;
      376      889
      377      890
      378      891
      379      892
      380      893
      381      894    1   IF OPTIONS(ERROR_LIST_E_OPT) THEN
      382      895    2     DO;
      383      896            %SELECT_LO;
      384      899            %OUTLINE(S="' '");
      385      905            %OUTLINE(S="' '");
      386      911            %OUTLINE(S="'==================='");
      387      917            %OUTLINE(S="'LEAST COST PREFIXES'");
      388      923            %OUTLINE(S="'==================='");
      389      929    2     END;
      390      930
      391      931        /* Initialize all terminal least cost prefixes */
      392      932    2   DO T = 1 TO NR_TERM;
      393      933
PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:114  
      394      934    2     CURRE.COST(T)     = INFINITE_COST;
      395      935    2     CURRE.PROD(T)     = -1;
      396      936    2     CURRE.INDX(T)     = -1;
      397      937    2     CURRE.EXPANDED(T) = FALSE;
      398      938
      399      939    2     LEAST_COST_PREFIX(T) = ADDR(NIL);
      400      940    2     NT_REF(T) = ADDR(NIL);
      401      941
      402      942
      403      943    2   END;
      404      944
      405      945    2   DO NT = NR_TERM + 1 TO LAST_NULL;
      406      946
      407      947    2     LEAST_COST_PREFIX(NT) = ADDR(NIL);
      408      948    2     NT_REF(NT) = ADDR(NIL);
      409      949
      410      950    2   END;
      411      951
      412      952        /* Build NT reference lists */
      413      953
      414      954    2   DO I = 0 TO NR_PROD - 1;
      415      955
      416      956    3     DO R = PROD.RIGHT_START(I) TO PROD.RIGHT_START(I) + PROD.RIGHT_CNT(I) - 1;
      417      957
      418      958    3       S = RIGHT_PARTS(R);
      419      959    3       TP = NT_REF(S);
      420      960    3       CALL GET_STRING_MEM(NT_REF(S), LENGTHW(PRODREF) * 2 - 1);
      421      961    3       NT_REF(S) -> PRODREF.LINK = TP;
      422      962    3       NT_REF(S) -> PRODREF.PROD = I;
      423      963    3       NT_REF(S) -> PRODREF.POS = R - PROD.RIGHT_START(I);
      424      964
      425      965    3     END;
      426      966    2   END;
      427      967
      428      968
      429      969        /* START THE ALGORITHM PROPER */
      430      970
PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:115  
      431      971    2   DO T = 1 TO NR_TERM;
      432      972
      433      973    2     CURRE.COST(T)     = 0;
      434      974    2     CURRE.PROD(T)     = -1;
      435      975    2     CURRE.INDX(T)     = -1;
      436      976    2     CURRE.EXPANDED(T) = TRUE;
      437      977
      438      978    2     CALL GET_STRING_MEM(LEAST_COST_PREFIX(T), 3);
      439      979    2     LEAST_COST_PREFIX(T) -> PREFIX.TERM = T;
      440      980    2     LEAST_COST_PREFIX(T) -> PREFIX.LINK = ADDR(NIL);
      441      981
      442      982    2     NT_CHANGED = ' ';
      443      983
      444      984          /* Set all non_terminal costs to Infinite for this terminal */
      445      985
      446      986    3     DO NT = NR_TERM + 1 TO LAST_NULL;
      447      987
      448      988    3       CURRE.COST(NT) = INFINITE_COST;
      449      989    3       CURRE.PROD(NT)    = -1;
      450      990    3       CURRE.INDX(NT)    = -1;
      451      991    3       CURRE.EXPANDED(NT)= FALSE;
      452      992
      453      993    3     END;
      454      994
      455      995
      456      996          /* Initialize stack with terminal */
      457      997
      458      998    2     NT = T;
      459      999
      460     1000    3     DO WHILE NT < LENGTHC(NT_CHANGED);
      461     1001
      462     1002    3       SUBSTR(NT_CHANGED, NT, 1) = ' ';
      463     1003
      464     1004    3       TP = NT_REF(NT);
      465     1005
      466     1006    4       DO WHILE TP  ~= ADDR(NIL);
      467     1007
PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:116  
      468     1008    4         PN = TP -> PRODREF.PROD;
      469     1009    4         MINC = CURRE.COST(PROD.GOAL_SYM_INDX(PN));
      470     1010    4         TEMPCOST = 0;
      471     1011
      472     1012    5         DO R = PROD.RIGHT_START(PN) TO PROD.RIGHT_START(PN) + TP->PRODREF.POS - 1;
      473     1013
      474     1014    5           TEMPCOST = TEMPCOST + SYM_PTRS(RIGHT_PARTS(R)) -> SYMBOL.COST.INS;
      475     1015
      476     1016    5         END;
      477     1017
      478     1018    4         TEMPCOST = TEMPCOST + CURRE.COST(RIGHT_PARTS(PROD.RIGHT_START(PN) + TP->
              1018                  PRODREF.POS));
      479     1019    4         IF TEMPCOST < MINC THEN
      480     1020    5           DO;
      481     1021    5             CURRE.COST(PROD.GOAL_SYM_INDX(PN)) = TEMPCOST;
      482     1022    5             CURRE.PROD(PROD.GOAL_SYM_INDX(PN)) = PN;
      483     1023    5             CURRE.INDX(PROD.GOAL_SYM_INDX(PN)) = TP->PRODREF.POS;
      484     1024    5             SUBSTR(NT_CHANGED, PROD.GOAL_SYM_INDX(PN), 1) = 'T';
      485     1025    5           END;
      486     1026
      487     1027    4         TP = TP ->PRODREF.LINK;
      488     1028
      489     1029    4       END;
      490     1030
      491     1031            /* Find next non_terminal whose value has changed */
      492     1032
      493     1033    3       CALL INDEX(NT, 'T', NT_CHANGED);
      494     1034
      495     1035    3     END;  /* WHILE */
      496     1036
      497     1037
      498     1038          /* Transfer the current least cost prefix values to the permanent
      499     1039             table of a list of strings of least cost prefixes.
      500     1040          */
      501     1041
      502     1042    3     DO NT = NR_TERM + 1 TO LAST_NULL;
      503     1043
PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:117  
      504     1044    3       IF NOT CURRE.EXPANDED (NT) THEN
      505     1045    3         IF CURRE.COST(NT) < INFINITE_COST THEN
      506     1046    3           CALL EXPAND_PREFIX (NT,T);
      507     1047
      508     1048    3     END;
      509     1049
      510     1050          /* Print out results if request */
      511     1051    2     IF OPTIONS(ERROR_LIST_E_OPT) THEN
      512     1052    2       CALL PRINT_PREFIXES_FOR_TERMINAL(T);
      513     1053
      514     1054          /* Output table slice */
      515     1055    2     CALL OUTPUT_LCP(T);
      516     1056
      517     1057          /* Reclaim string space */
      518     1058    2     CALL COLLECT_GARBAGE(LEAST_COST_PREFIX(T));
      519     1059    2     LEAST_COST_PREFIX(T) = ADDR(NIL);
      520     1060
      521     1061    2     CURRE.COST(T)     = INFINITE_COST;
      522     1062    2     CURRE.PROD(T)     = -1;
      523     1063    2     CURRE.INDX(T)     = -1;
      524     1064    2     CURRE.EXPANDED(T) = FALSE;
      525     1065
      526     1066    3     DO NT = NR_TERM + 1 TO LAST_NULL;
      527     1067
      528     1068    3       IF LEAST_COST_PREFIX(NT) ~= ADDR(NIL) THEN
      529     1069    4         DO;
      530     1070    4           CALL COLLECT_GARBAGE(LEAST_COST_PREFIX(NT));
      531     1071    4           LEAST_COST_PREFIX(NT) = ADDR(NIL);
      532     1072    4         END;
      533     1073
      534     1074    3     END;
      535     1075
      536     1076    2   END;  /* DO ON TERMINALS */
      537     1077
      538     1078        /* COLLECT ALL  REFERENCE LIST SPACE */
      539     1079
      540     1080    2   DO I = 1 TO LAST_NULL;
PL6.E3A0      #005=CALC_LEAST_COST_PREFIX File=LR_ERRREC.:Z8A6TSI                SAT 08/09/97 09:29 Page:118  
      541     1081    3     DO WHILE NT_REF(I) ~= ADDR(NIL);
      542     1082    3       TP = NT_REF(I) -> PRODREF.LINK;
      543     1083    3       CALL COLLECT_GARBAGE(NT_REF(I));
      544     1084    3       NT_REF(I) = TP;
      545     1085    3     END;
      546     1086    2   END;
      547     1087
      548     1088
      549     1089    1   END CALC_LEAST_COST_PREFIX;
      550     1090        %EOD;

