PL6.E3A0      #003=CALC_LEAST_COST_STRINGS File=LR_ERRREC.:Z8A6TSI               SAT 08/09/97 09:28 Page:45   
      116        1        /*T***********************************************************/
      117        2        /*T*                                                         */
      118        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      119        4        /*T*                                                         */
      120        5        /*T***********************************************************/
      121        6        CALC_LEAST_COST_STRINGS : PROC;
      122        7
      123        8        /*D*
      124        9               NAME: CALC_LEAST_COST_STRINGS
      125       10            PURPOSE:
      126       11
      127       12           Calculate all least cost strings S(A), where A is a
      128       13           non_terminal.  The algorithm used is:
      129       14
      130       15             For all non_terminals, A
      131       16               Do C(A) := Infinite;
      132       17
      133       18             Repeat
      134       19               Nochange := true;
      135       20               For all productions P = (A --> x1, ...xn) Do
      136       21                 If C(x1...xn) < C(A) then
      137       22                   Begin
      138       23                     C(A) := C(x1...xn);
      139       24                     S(A) := S(x1...xn);
      140       25                     Nochange := False;
      141       26                   End
      142       27             Until nochange;
      143       28
      144       29        */
      145       30
      146       31        %INCLUDE LR_GRAM_EXT;
      147      570        %INCLUDE LR_CNFG_EXT;
      148      723        %INCLUDE LR_ERR_EXT;
      149      866
      150      867    1   DCL GET_STRING_MEM             ENTRY(2);
      151      868    1   DCL COLLECT_GARBAGE            ENTRY(1);
      152      869    1   DCL OUT_TOKEN                  ENTRY(2);
PL6.E3A0      #003=CALC_LEAST_COST_STRINGS File=LR_ERRREC.:Z8A6TSI               SAT 08/09/97 09:28 Page:46   
      153      870    1   DCL CREATE_LEAST_COST_STRING   ENTRY(1);
      154      871    1   DCL OUT_SYMBOL ENTRY(1);
      155      872    1   DCL OUT_SYMBOL_TOKEN ENTRY(1);
      156      873
      157      874    1   DCL NT SBIN WORD;   /* Non terminal number */
      158      875    1   DCL NP SBIN WORD;  /* Prod Num */
      159      876    1   DCL CP SBIN WORD;  /* Cost of Production */
      160      877    1   DCL RP SBIN WORD;   /* Right part in produc */
      161      878    1   DCL I  SBIN WORD;
      162      879
      163      880    1   DCL NOCHANGE BOOLEAN;
      164      881
      165      882        /* Initialize least cost strings:
      166      883              terminal -> terminal
      167      884             Non term  -> null string
      168      885        */
      169      886
      170      887    2   DO I = 1 TO NR_TERM;
      171      888
      172      889    2     CALL GET_STRING_MEM(LEAST_COST_STRING(I), 1);
      173      890    2     LEAST_COST_STRING(I) -> STRING.TOK(0) = I;
      174      891
      175      892    2   END;
      176      893
      177      894    2   DO NT = NR_TERM + 1 TO LAST_NULL;
      178      895
      179      896    2     LEAST_COST_STRING(NT) = ADDR(NIL);
      180      897
      181      898        /* Set all non_terminal costs to infinite */
      182      899
      183      900    2     SYM_PTRS(NT) -> SYMBOL.COST.NT = INFINITE_COST;
      184      901    2     SYM_PTRS(NT) -> SYMBOL.COST.PROD = -1; /* NO PRODUCTION ASSOCIATED */
      185      902
      186      903    2   END;
      187      904
      188      905        /* Iterate over all productions */
      189      906
PL6.E3A0      #003=CALC_LEAST_COST_STRINGS File=LR_ERRREC.:Z8A6TSI               SAT 08/09/97 09:28 Page:47   
      190      907    1   NOCHANGE = FALSE;
      191      908
      192      909    2   DO WHILE (NOT NOCHANGE);
      193      910
      194      911    2     NOCHANGE = TRUE;
      195      912
      196      913    3     DO NP = 0 TO NR_PROD - 1;
      197      914
      198      915            /* Calculate production cost */
      199      916    3       CP = 0;
      200      917
      201      918    4       DO RP = PROD.RIGHT_START(NP) TO PROD.RIGHT_START(NP) + PROD.RIGHT_CNT(NP) - 1;
      202      919
      203      920    4         CP = CP + SYM_PTRS(RIGHT_PARTS(RP)) -> SYMBOL.COST.INS;
      204      921
      205      922    4       END;
      206      923
      207      924    3       IF CP < INFINITE_COST THEN
      208      925    3         IF CP < SYM_PTRS(PROD.GOAL_SYM_INDX(NP)) -> SYMBOL.COST.INS THEN
      209      926    4           DO;
      210      927
      211      928    4             SYM_PTRS(PROD.GOAL_SYM_INDX(NP)) -> SYMBOL.COST.INS = CP;
      212      929
      213      930    4             SYM_PTRS(PROD.GOAL_SYM_INDX(NP)) -> SYMBOL.COST.PROD = NP;
      214      931    4             NOCHANGE = FALSE;
      215      932
      216      933    4           END;
      217      934    3     END;
      218      935    2   END;
      219      936
      220      937        /* ACTUALLY CREATE THE STRINGS FOR EACH NON_TERMINAL */
      221      938
      222      939    2   DO NT = NR_TERM + 1 TO LAST_NULL;
      223      940    2     IF LEAST_COST_STRING(NT) = ADDR(NIL) THEN
      224      941    2       CALL CREATE_LEAST_COST_STRING(NT);
      225      942    2   END;
      226      943
PL6.E3A0      #003=CALC_LEAST_COST_STRINGS File=LR_ERRREC.:Z8A6TSI               SAT 08/09/97 09:28 Page:48   
      227      944
      228      945    1   IF OPTIONS(ERROR_LIST_S_OPT) THEN
      229      946    2     DO;
      230      947            %SELECT_LO;
      231      950
      232      951          /* Print out all least cost strings */
      233      952
      234      953          %OUTLINE(S="' '");
      235      959          %OUTLINE(S="' '");
      236      965          %OUTLINE(S="'=================='");
      237      971          %OUTLINE(S="'LEAST COST STRINGS'");
      238      977          %OUTLINE(S="'=================='");
      239      983          %OUTLINE(S="' '");
      240      989
      241      990    3     DO NT = NR_TERM + 1 TO LAST_NULL;
      242      991
      243      992    3       CALL OUT_SYMBOL_TOKEN(SYM_PTRS(NT));
      244      993    3       IF LINELEN < 15 THEN
      245      994    3         CALL OUT_STR('               ',15-LINELEN);
      246      995            %OUT_TOKEN(S="'-->'");
      247      998    4       DO I = 1 TO LEAST_COST_STRING(NT)->STRING.LEN_IN_H;
      248      999    4         CALL OUT_SYMBOL_TOKEN(SYM_PTRS(LEAST_COST_STRING(NT)->STRING.TOK(I-1)));
      249     1000    4       END;
      250     1001    3       CALL FLUSH;
      251     1002
      252     1003    3     END;
      253     1004    2     END;
      254     1005
      255     1006    1   END CALC_LEAST_COST_STRINGS;
      256     1007        %EOD;

PL6.E3A0      #003=CALC_LEAST_COST_STRINGS File=LR_ERRREC.:Z8A6TSI               SAT 08/09/97 09:28 Page:49   
