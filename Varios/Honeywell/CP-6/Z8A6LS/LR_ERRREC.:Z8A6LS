PL6.E3A0      #002=RECLAIM_CONFIG_MEM File=LR_ERRREC.:Z8A6TSI                    SAT 08/09/97 09:28 Page:18   
       47        1        /*T***********************************************************/
       48        2        /*T*                                                         */
       49        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
       50        4        /*T*                                                         */
       51        5        /*T***********************************************************/
       52        6        RECLAIM_CONFIG_MEM : PROC;
       53        7
       54        8        /*D*
       55        9               NAME: RECLAIM_CONFIG_MEM
       56       10            PURPOSE:
       57       11
       58       12           RECLAIM ALL MEMORY USED BUILDING CONFIGURATION SETS THAT
       59       13           WE CAN REUSE FOR THE ERROR CORRECTION CALCULATIONS.
       60       14        */
       61       15
       62       16        %INCLUDE LR_GRAM_EXT;
       63      555        %INCLUDE LR_CNFG_EXT;
       64      708        %INCLUDE LR_ERR_EXT;
       65      851
       66      852    1   DCL COLLECT_GARBAGE ENTRY (1);
       67      853
       68      854    1   DCL I        SBIN WORD;
       69      855    1   DCL K        SBIN WORD;
       70      856    1   DCL C$       PTR;
       71      857    1   DCL KEYN$    PTR;
       72      858    1   DCL INCL_LEN SBIN WORD;
       73      859    1   DCL SET_LEN  SBIN WORD;
       74      860
       75      861
       76      862
       77      863    1   INCL_LEN = (LENGTHW(INCL_B) + SET_LENGTHW - 1) * 2 - 1;
       78      864    1   SET_LEN  = SET_LENGTHW * 2 - 1;
       79      865
       80      866        /* Reclaim state INCL space */
       81      867
       82      868    2   DO I = 0 TO NR_CONFIG - 1;
       83      869
PL6.E3A0      #002=RECLAIM_CONFIG_MEM File=LR_ERRREC.:Z8A6TSI                    SAT 08/09/97 09:28 Page:19   
       84      870    2     C$ = C#_TO_$(I);
       85      871
       86      872    3     DO K = 0 TO C$ -> C_SET_B.NR_ELEM - 1;
       87      873
       88      874    3       IF C$ -> C_SET_B.ELEM.INCL$(K) ~= ADDR(NIL) THEN
       89      875    4         DO;
       90      876    4           C$ -> C_SET_B.ELEM.INCL$(K) -> STRING.LEN_IN_H = INCL_LEN;
       91      877    4           CALL COLLECT_GARBAGE (C$ -> C_SET_B.ELEM.INCL$(K));
       92      878    4           C$ -> C_SET_B.ELEM.INCL$(K) = ADDR(NIL);
       93      879    4         END;
       94      880
       95      881    3       KEYN$ = C$ -> C_SET_B.ELEM.KEY$(K);
       96      882
       97      883    3       IF KEYN$ -> KEY_B.SET$ ~= ADDR(NIL) THEN
       98      884    4         DO;
       99      885    4           KEYN$ -> KEY_B.SET$ -> STRING.LEN_IN_H = SET_LEN;
      100      886    4           CALL COLLECT_GARBAGE(KEYN$ -> KEY_B.SET$);
      101      887    4           KEYN$ -> KEY_B.SET$ = ADDR(NIL);
      102      888    4         END;
      103      889
      104      890    3       IF KEYN$ -> KEY_B.W.SYM_AFTER = 0 THEN
      105      891    3         IF KEYN$ -> KEY_B.FOLLOW$ ~= ADDR(NIL) THEN
      106      892    4           DO;
      107      893    4             KEYN$ -> KEY_B.FOLLOW$ -> STRING.LEN_IN_H = INCL_LEN;
      108      894    4             CALL COLLECT_GARBAGE (KEYN$ -> KEY_B.FOLLOW$);
      109      895    4             KEYN$ -> KEY_B.FOLLOW$ = ADDR(NIL);
      110      896    4           END;
      111      897    3     END;
      112      898    2   END;
      113      899
      114      900    1   END RECLAIM_CONFIG_MEM;
      115      901        %EOD;

