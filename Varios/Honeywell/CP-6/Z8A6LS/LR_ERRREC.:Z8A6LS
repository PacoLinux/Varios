PL6.E3A0      #004=CREATE_LEAST_COST_STRING File=LR_ERRREC.:Z8A6TSI              SAT 08/09/97 09:29 Page:82   
      257        1        /*T***********************************************************/
      258        2        /*T*                                                         */
      259        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      260        4        /*T*                                                         */
      261        5        /*T***********************************************************/
      262        6        CREATE_LEAST_COST_STRING : PROC(NT);
      263        7
      264        8        /*D*
      265        9               NAME: CREATE_LEAST_COST_STRING
      266       10            PURPOSE:
      267       11
      268       12           CREATE THE LEAST COST STRING FROM THE PRODUCTION WHICH
      269       13           WAS FOUND TO BE LEAST COST BY CALC_LEAST_COST_STRING.
      270       14        */
      271       15
      272       16    1   DCL NT SBIN WORD;
      273       17
      274       18    1   DCL STRLEN SBIN WORD;
      275       19    1   DCL RP     SBIN WORD;
      276       20
      277       21        %INCLUDE LR_GRAM_EXT;
      278      560        %INCLUDE LR_CNFG_EXT;
      279      713        %INCLUDE LR_ERR_EXT;
      280      856
      281      857    1   DCL GET_STRING_MEM ENTRY(2);
      282      858
      283      859
      284      860    1   STRLEN = 0;
      285      861
      286      862    1   IF SYM_PTRS(NT) -> SYMBOL.COST.PROD >= 0 THEN
      287      863
      288      864    2     DO;
      289      865    2       DO RP = PROD.RIGHT_START(SYM_PTRS(NT) -> SYMBOL.COST.PROD)
      290      866    3                 TO PROD.RIGHT_START(SYM_PTRS(NT) -> SYMBOL.COST.PROD)
      291      867    3                     + PROD.RIGHT_CNT(SYM_PTRS(NT) -> SYMBOL.COST.PROD)
      292      868    3                     - 1;
      293      869
PL6.E3A0      #004=CREATE_LEAST_COST_STRING File=LR_ERRREC.:Z8A6TSI              SAT 08/09/97 09:29 Page:83   
      294      870    3         IF LEAST_COST_STRING(RIGHT_PARTS(RP)) = ADDR(NIL) THEN
      295      871    3           CALL CREATE_LEAST_COST_STRING(BINBIT(RIGHT_PARTS(RP), 36));
      296      872
      297      873    3         STRLEN = STRLEN + LEAST_COST_STRING(RIGHT_PARTS(RP)) -> STRING.LEN_IN_H;
      298      874
      299      875    3       END;
      300      876
      301      877    2       CALL GET_STRING_MEM(LEAST_COST_STRING(NT), STRLEN);
      302      878
      303      879    2       LEAST_COST_STRING(NT) -> STRING.LEN_IN_H = 0;
      304      880
      305      881    2       DO RP = PROD.RIGHT_START(SYM_PTRS(NT) -> SYMBOL.COST.PROD)
      306      882    3                 TO PROD.RIGHT_START(SYM_PTRS(NT) -> SYMBOL.COST.PROD)
      307      883    3                     + PROD.RIGHT_CNT(SYM_PTRS(NT) -> SYMBOL.COST.PROD)
      308      884    3                     - 1;
      309      885
      310      886              %APPEND_STRING (S1="LEAST_COST_STRING(RIGHT_PARTS(RP))",
      311      887                              S2="LEAST_COST_STRING(NT)");
      312      901
      313      902    3       END;
      314      903    2     END;
      315      904    1   ELSE
      316      905    1     CALL GET_STRING_MEM(LEAST_COST_STRING(NT), 0); /* NULL STRING */
      317      906
      318      907    1   END CREATE_LEAST_COST_STRING;
      319      908        %EOD;

