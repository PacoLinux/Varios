      467    18586    2      IF REPORT_DATA.LEVEL.CURRENT = %LEVEL_01 THEN
      468    18587    3        DO;
      469    18588               %CONDITIONAL_ERROR (@LX_ITEM$ = PIC$,
      470    18589                                   @ERROR#   = %E$ERR_CLAUSE_IN_REP_01_ENTRY,
