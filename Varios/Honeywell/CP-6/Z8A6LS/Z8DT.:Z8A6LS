      556    18857    3          RETURN;
      557    18858    3        END;
      558    18859
      559    18860    2      IF NOT REPORT_DATA.B.ELEMENTARY THEN
      560    18861    3        DO;
      561    18862               %CONDITIONAL_ERROR (@LX_ITEM$ = VALUE$,
