      160    18117
      161    18118         %MACRO CHECK_NEXT_PAGE_W_TYPE (@GROUP_TYPE = );
      162    18119           DO;
      163    18120             IF REPORT_DATA.LINE_TYPE = %REPORT_LINE_NEXT_PAGE THEN
      164    18121               DO;
      165    18122                 DO CASE (@GROUP_TYPE);
      166    18123                   CASE (%REPORT_HEADING, %PAGE_HEADING, %PAGE_FOOTING);
      167    18124
      168    18125                        CALL Z8AE$ERROR (%E$NEXT_PAGE_IN_WRONG_GRP,
      169    18126                                         LINE$ -> LX_ITEM.LINE,
      170    18127                                         LINE$ -> LX_ITEM.COLUMN);
      171    18128                        REPORT_DATA.LINE_TYPE = %REPORT_LINE_INT;
      172    18129
      173    18130                   CASE (ELSE);
      174    18131                        /* OK */
      175    18132                 END;
      176    18133               END;
      177    18134           END;
