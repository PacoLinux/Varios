      569    18899
      570    18900    3      DO CASE(VALUE$ -> LX_ITEM.SYMBOL);
      571    18901
      572    18902    3        CASE(%NUMERIC_LITERAL);
      573    18903    4            DO;
      574    18904    4              CALL Z8AU$CREATE_NUM_DESC (VALUE$ -> LX_ITEM.DNT$ -> DNT.TEXT.NAME,
      575    18905    4                          BINBIT(VALUE$ -> LX_ITEM.DNT$ -> DNT.TEXT.LENGTH,36),
      576    18906    4                          NUM_DESC);
