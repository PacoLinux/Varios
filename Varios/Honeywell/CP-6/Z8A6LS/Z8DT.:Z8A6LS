      475    18623    3          RETURN;
      476    18624    3        END;
      477    18625
      478    18626    2      IF NOT REPORT_DATA.B.ELEMENTARY THEN
      479    18627    3        DO;
      480    18628               %CONDITIONAL_ERROR (@LX_ITEM$ = PIC$,
      481    18629                                   @ERROR#   = %E$ERR_CLAUSE_IN_GROUP_ENTRY,
      482    18630                                   @LINE     = PIC$ -> LX_ITEM.LINE,
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:17   
      483    18631                                   @COLUMN   = PIC$ -> LX_ITEM.COLUMN,
      484    18632                                   @PLUG1    = "DNT$_FROM_RSWNO(%RSV_WRD_PICTURE)");
      485    18662    3          CALL Z8ED$DISPOSE_ABSSYN_TREE(CDECL$ -> LX_REPORT_REC.P.PICTURE$);
      486    18663    3          RETURN;
      487    18664    3        END;
      488    18665
      489    18666    2      IF PIC_OUT.ERROR ~= %NO_ERROR THEN
      490    18667    3        DO;
      491    18668    3          COLUMN# = PIC_OUT.ERRCOL + PIC$ -> LX_ITEM.COLUMN;
      492    18669
      493    18670               %CONDITIONAL_ERROR (@LX_ITEM$ = PIC$,
      494    18671                                   @ERROR#   = PIC_OUT.ERROR,
      495    18672                                   @LINE     = PIC$ -> LX_ITEM.LINE,
      496    18673                                   @COLUMN   = COLUMN#);
