      515    18750    2      IF PIC_OUT.TYPE = %PIC_TY_K
      516    18751    2      AND (~ COMCOM.OB.KANJI) THEN
      517    18752    3        DO;
      518    18753               %CONDITIONAL_ERROR (@LX_ITEM$  = PIC$,
      519    18754                                   @ERROR#    = %E$PIC_N_ILLEGAL,
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:18   
      520    18755                                   @LINE      = PIC$ -> LX_ITEM.LINE,
      521    18756                                   @COLUMN    = PIC$ -> LX_ITEM.COLUMN);
      522    18786    3          RETURN;
      523    18787    3        END;
      524    18788
      525    18789    2   END VALIDATE_PIC;
      526    18790        %EJECT;
