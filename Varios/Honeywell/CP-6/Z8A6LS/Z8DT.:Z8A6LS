      587    18946                                         @LX_ITEM$ = VALUE$,
      588    18947                                         @LINE     = VALUE$ -> LX_ITEM.LINE,
      589    18948                                         @COLUMN   = VALUE$ -> LX_ITEM.COLUMN);
      590    18978    4            END;
      591    18979
      592    18980    3        CASE(%NUMBER, %NUMBER_OR_PARAGRAPH);
      593    18981    4            DO;
      594    18982    4              IF PIC_OUT.TYPE ~= %PIC_TY_N THEN
      595    18983                     %CONDITIONAL_ERROR (@ERROR#   = %E$NUM_CONST_W_ALPHA_ITEM,
      596    18984                                         @LX_ITEM$ = VALUE$,
      597    18985                                         @LINE     = VALUE$ -> LX_ITEM.LINE,
      598    18986                                         @COLUMN   = VALUE$ -> LX_ITEM.COLUMN);
      599    19016    4            END;
      600    19017
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:21   
      601    19018    3        CASE(%RSV_WRD_ZEROES);
      602    19019               /* Peachy */
      603    19020    3      ;
      604    19021    3        CASE(%ALPHA_LITERAL,     %RSV_WRD_HIGH_VALUES,
      605    19022    3             %RSV_WRD_SPACES,    %RSV_WRD_LOW_VALUES,
      606    19023    3             %RSV_WRD_QUOTES,    %SINGLE_QUOTE);
