      616    19062    3        CASE(ELSE);
      617    19063    3          CALL Z8AE$ERROR (%E$UNEXPECTED_CASE);
      618    19064    3          CALL Z8ED$DISPOSE_ABSSYN_TREE (CDECL$ -> LX_REPORT_REC.P.VALUE$);
      619    19065
      620    19066    3      END; /* Case */
