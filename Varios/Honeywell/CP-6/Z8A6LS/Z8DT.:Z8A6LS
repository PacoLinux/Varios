      869    19518    4              ELSE
      870    19519    4                CURRENT_LINE = REPORT_DATA.LINE_NUMBER;
      871    19520    4            END;
      872    19521
      873    19522    3          IF CURRENT_LINE <= PREV_LINE THEN
