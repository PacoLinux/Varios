      182    18139        /*============*/
      183    18140    1     PIC_RECOVERY:  PROC ;
      184    18141        /*============*/
      185    18142
      186    18143         /*
      187    18144              NAME: PIC_RECOVERY
      188    18145
      189    18146           DESCRIPTION: When a PICTURE clause is missing from a elementary
      190    18147                        entry, this routine is called to create a PIC 9999
      191    18148                        LX_ITEM for it.
      192    18149          */
      193    18150
      194    18151          %EQU PIC_TEXT = '9999';
      195    18152          %EQU PIC_TEXT_LEN = 4;
      196    18153
      197    18154    2     LEX_SYMBOL.SIZE = %PIC_TEXT_LEN;
      198    18155    2     SUBSTR (LEX_SYMBOL.SYMBOL, 0, %PIC_TEXT_LEN) = %PIC_TEXT;
      199    18156
      200    18157    2     CALL Z8AH$SEARCH_;
      201    18158    2     CALL Z8ED$NEW_HEAP(P$, %LENGTH_LX_ITEM);
      202    18159          %INIT_LX_TERMINAL(@POINTER = P$, @DNT$ = DNT$,
      203    18160              @LINE = CDECL$ -> LX_REPORT_REC.P.DATA_NAME$ -> LX_ITEM.LINE,
      204    18161              @COLUMN = CDECL$ -> LX_REPORT_REC.P.DATA_NAME$ -> LX_ITEM.COLUMN);
      205    18179    2     CDECL$ -> LX_REPORT_REC.P.PICTURE$ = P$;
      206    18180
      207    18181    2     END PIC_RECOVERY;
      208    18182          %EJECT;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:8    
      209    18183
      210    18184        /*=============*/
      211    18185    1     EXTRACT_LEVEL:    PROC;
      212    18186        /*=============*/
      213    18187
      214    18188         /*
      215    18189              NAME: EXTRACT_LEVEL
      216    18190
      217    18191           DESCRIPTION: Three data entries level value--previous, current
      218    18192                        and next--will be extracted and saved for later
      219    18193                        use. The elementary entry will be recognized and
      220    18194                        the missing PIC clause will be flagged and recovered.
      221    18195          */
      222    18196
      223    18197
      224    18198
      225    18199    2      REPORT_DATA.LEVEL.PREVIOUS = O1COM.LEVEL_NO;
      226    18200
      227    18201    2      O1COM.LEVEL_NO = CDECL$ -> LX_REPORT_REC.LEVEL_NUMBER;
      228    18202
      229    18203    2      REPORT_DATA.LEVEL.CURRENT = O1COM.LEVEL_NO;
      230    18204
      231    18205    2      IF NDECL$ = ADDR(NIL) THEN
      232    18206    2        REPORT_DATA.LEVEL.NEXT = %LEVEL_01;
      233    18207    2      ELSE
      234    18208    2        REPORT_DATA.LEVEL.NEXT = NDECL$ -> LX_REPORT_REC.LEVEL_NUMBER;
      235    18209
      236    18210    2      IF REPORT_DATA.LEVEL.CURRENT = %LEVEL_01 THEN
      237    18211    3        DO;
      238    18212    3          GROUP = '0'B;
      239    18213    3          GROUP.USAGE.LEVEL = %LEVEL_01;
      240    18214    3          GROUP.VALUE.LEVEL = %LEVEL_01;
      241    18215    3        END;
      242    18216    2      ELSE IF COMCOM.OB.FIPS.LEVEL > 0 AND
      243    18217    2          REPORT_DATA.LEVEL.CURRENT < REPORT_DATA.LEVEL.PREVIOUS THEN
      244    18218    3   SEQ_CHECK: DO;
      245    18219    4          DO I = REP_STK_PTR DOWNTO 2;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:9    
      246    18220    4            IF REP_STK.LEVEL#(I) = REPORT_DATA.LEVEL.CURRENT THEN
      247    18221    4              EXIT SEQ_CHECK;
      248    18222    4          END;
      249    18223    3          CALL Z8AE$ERROR (%E$ANSI_LEVEL_SEQ_ERROR,
      250    18224    3                           CDECL$ -> LX_REPORT_REC.P.LEVEL_NO$ -> LX_ITEM.LINE,
      251    18225    3                           CDECL$ -> LX_REPORT_REC.P.LEVEL_NO$ -> LX_ITEM.COLUMN);
      252    18226    3        END; /* SEQ_CHECK */
      253    18227
      254    18228    2      REPORT_DATA.B.ELEMENTARY = (REPORT_DATA.LEVEL.NEXT <=
      255    18229    2                                  REPORT_DATA.LEVEL.CURRENT);
      256    18230
      257    18231    2      IF REPORT_DATA.LEVEL.CURRENT <= GROUP.VALUE.LEVEL THEN
      258    18232    2        GROUP.VALUE.LEVEL = REPORT_DATA.LEVEL.CURRENT;
      259    18233
      260    18234    2      IF REPORT_DATA.B.ELEMENTARY  THEN
      261    18235    2        IF CDECL$ -> LX_REPORT_REC.P.PICTURE$ = ADDR(NIL) THEN
      262    18236    2          IF REPORT_DATA.LEVEL.CURRENT > %LEVEL_01 AND
      263    18237    2              REPORT_DATA.LEVEL.PREVIOUS ~= 0 THEN
      264    18238    3            DO;
      265    18239    3              CALL Z8AE$ERROR(%E$MISSING_DATA_PIC_CLAUSE,
      266    18240    3                    CDECL$ -> LX_REPORT_REC.P.DATA_NAME$ -> LX_ITEM.LINE,
      267    18241    3                    CDECL$ -> LX_REPORT_REC.P.DATA_NAME$ -> LX_ITEM.COLUMN,
      268    18242    3                    CDECL$ -> LX_REPORT_REC.P.DATA_NAME$ -> LX_ITEM.DNT$);
      269    18243    3              CALL PIC_RECOVERY;
      270    18244    3            END;
      271    18245
      272    18246    2   END EXTRACT_LEVEL;
      273    18247        %EJECT;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:10   
      274    18248
      275    18249        /*===========*/
      276    18250    1     EXTRACT_PIC: PROC;
      277    18251        /*===========*/
      278    18252
      279    18253         /*
      280    18254              NAME: EXTRACT_PIC
      281    18255
      282    18256           DESCRIPTION: The PICTURE string will be processed here,
      283    18257                        Z8DP$VALIDATE_PIC is called to produce the
      284    18258                        micro-operation strings and some useful information
      285    18259                        like the picture type and length.
      286    18260          */
      287    18261
      288    18262    2   DCL PIC$                      PTR;
      289    18263    2   DCL SIGN_TYPE                 SBIN WORD;
      290    18264
      291    18265
      292    18266    2      PIC$ = CDECL$ -> LX_REPORT_REC.P.PICTURE$;
      293    18267    2      PIC_STR = PIC$ -> LX_ITEM.DNT$ -> DNT.TEXT.NAME;
      294    18268    2      PIC_IN.PIC_L = PIC$ -> LX_ITEM.DNT$ -> DNT.TEXT.LENGTH;
      295    18269    2      PIC_IN.MOP_L  = %MAX_MOP_LENGTH;
      296    18270    2      PIC_OUT.ERROR = %NO_ERROR;
      297    18271
      298    18272    2      IF PIC_IN.PIC_L > %MAX_PIC_STR_LENGTH THEN
      299    18273    3        DO;
      300    18274               %CONDITIONAL_ERROR (@LX_ITEM$ = PIC$,
      301    18275                                   @ERROR#   = %E$PIC_STR_TOO_LONG,
      302    18276                                   @LINE     = PIC$ -> LX_ITEM.LINE,
      303    18277                                   @COLUMN   = PIC$ -> LX_ITEM.COLUMN);
      304    18307    3          RETURN;
      305    18308    3        END;
      306    18309
      307    18310    2      CALL EXTRACT_BLANK_WHEN_ZERO (PIC_IN.B.BLANK_WHEN_ZERO);
      308    18311
      309    18312           /* If a blank when zero was specified and an asterisk was in
      310    18313              the pic string it is illegal                              */
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:11   
      311    18314
      312    18315    2      IF CDECL$ -> LX_REPORT_REC.P.BLANK$ ~= ADDR(NIL)
      313    18316    2      AND NOT PIC_IN.B.BLANK_WHEN_ZERO THEN
      314    18317    2        REPORT_DATA.B.ASTERISK = TRUE;
      315    18318
      316    18319    2      CALL Z8DP$VALIDATE_PIC (PIC_STR, PIC_IN, PIC_OUT)
      317    18320    2        WHENALTRETURN
      318    18321    3          DO;
      319    18322                 /* Nothing - error flagged later */
      320    18323    3          END;
      321    18324
      322    18325
      323    18326    3      DO CASE(PIC_OUT.EIS_TYPE);
      324    18327    3        CASE (%PIC_EIS_LS, %PIC_EIS_TS, %PIC_EIS_LSTS, %PIC_EIS_FLT);
      325    18328    3            REPORT_DATA.B.SIGN_FOUND = TRUE;
      326    18329    3            REPORT_DATA.SIGN.TYPE = O1COM.DISPLAY_SIGN_TYPE;
      327    18330    3        CASE (ELSE);
      328    18331                 /* No sign found */
      329    18332    3      END;
      330    18333
      331    18334    2   END EXTRACT_PIC;
      332    18335        %EJECT;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:12   
      333    18336
      334    18337        /*=======================*/
      335    18338    1     EXTRACT_BLANK_WHEN_ZERO: PROC (BLANK_WHEN_ZERO);
      336    18339        /*=======================*/
      337    18340
      338    18341         /*
      339    18342              NAME: EXTRACT_BLANK_WHEN_ZERO
      340    18343
      341    18344           DESCRIPTION: BLANK WHEN ZERO clause is checked against the
      342    18345                        picture string and the bit BLANK_WHEN_ZERO is
      343    18346                        set.
      344    18347          */
      345    18348
      346    18349    2   DCL BLANK_WHEN_ZERO           BIT(1) ALIGNED;
      347    18350    2   DCL INDEX#                    SBIN WORD;
      348    18351
      349    18352
      350    18353
      351    18354    2      IF CDECL$ -> LX_REPORT_REC.P.BLANK$ ~= ADDR(NIL) THEN
      352    18355    3        DO;
      353    18356    3          CALL INDEX (INDEX#,
      354    18357    3                      %ASTERISK,
      355    18358    3                      CDECL$ -> LX_REPORT_REC.P.PICTURE$ ->
      356    18359    3                      LX_ITEM.DNT$ -> DNT.TEXT.NAME);
      357    18360
      358    18361    3          IF INDEX# ~= CDECL$ -> LX_REPORT_REC.P.PICTURE$ ->
      359    18362    3                       LX_ITEM.DNT$ -> DNT.TEXT.LENGTH THEN
      360    18363    3            BLANK_WHEN_ZERO = FALSE;
      361    18364    3          ELSE
      362    18365    3            BLANK_WHEN_ZERO = TRUE;
      363    18366    3        END;
      364    18367    2      ELSE
      365    18368    2        BLANK_WHEN_ZERO     = FALSE;
      366    18369
      367    18370    2   END EXTRACT_BLANK_WHEN_ZERO;
      368    18371        %EJECT;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:13   
      369    18372
      370    18373        /*==============*/
      371    18374    1     VALIDATE_LEVEL: PROC;
      372    18375        /*==============*/
      373    18376
      374    18377         /*
      375    18378              NAME: VALIDATE_LEVEL
      376    18379
      377    18380           DESCRIPTION: Entry level is validated here and the missing TYPE
      378    18381                        clause will be flagged and recovered too.
      379    18382
      380    18383          */
      381    18384
      382    18385
      383    18386    2   DCL LEVEL$                    PTR;
      384    18387
      385    18388
      386    18389
      387    18390    2      IF REPORT_DATA.LEVEL.PREVIOUS = 0  THEN
      388    18391    2        IF REPORT_DATA.LEVEL.CURRENT ~= %LEVEL_01  THEN
      389    18392    3        DO;
      390    18393    3          LEVEL$ = CDECL$ -> LX_REPORT_REC.P.LEVEL_NO$;
      391    18394
      392    18395               %CONDITIONAL_ERROR (@LX_ITEM$ = LEVEL$,
      393    18396                                   @ERROR#   = %E$LEVEL_1_OR_77_AT_TOP,
      394    18397                                   @LINE     = LEVEL$ -> LX_ITEM.LINE,
      395    18398                                   @COLUMN   = LEVEL$ -> LX_ITEM.COLUMN);
      396    18428
      397    18429    3          O1COM.LEVEL_NO = %LEVEL_01;
      398    18430    3          REPORT_DATA.LEVEL.CURRENT = %LEVEL_01;
      399    18431    3          IF REPORT_DATA.LEVEL.NEXT = %LEVEL_01 THEN
      400    18432    3            REPORT_DATA.B.ELEMENTARY = TRUE;
      401    18433    3        END;
      402    18434
      403    18435    2      IF REPORT_DATA.LEVEL.CURRENT < %LEVEL_01  THEN
      404    18436    3        DO;
      405    18437    3          LEVEL$ = CDECL$ -> LX_REPORT_REC.P.LEVEL_NO$;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:14   
      406    18438
      407    18439               %CONDITIONAL_ERROR (@LX_ITEM$ = LEVEL$,
      408    18440                                   @ERROR#   = %E$ERR_LEVEL_AND_01_ASSUMED,
      409    18441                                   @LINE     = LEVEL$ -> LX_ITEM.LINE,
      410    18442                                   @COLUMN   = LEVEL$ -> LX_ITEM.COLUMN);
      411    18472
      412    18473    3          O1COM.LEVEL_NO = %LEVEL_01;
      413    18474    3          REPORT_DATA.LEVEL.CURRENT = %LEVEL_01;
      414    18475    3          IF REPORT_DATA.LEVEL.NEXT = %LEVEL_01 THEN
      415    18476    3            REPORT_DATA.B.ELEMENTARY = TRUE;
      416    18477    3        END;
      417    18478
      418    18479    2      IF REPORT_DATA.LEVEL.CURRENT > %LEVEL_49  THEN
      419    18480    3        DO;
      420    18481    3          LEVEL$ = CDECL$ -> LX_REPORT_REC.P.LEVEL_NO$;
      421    18482
      422    18483               %CONDITIONAL_ERROR (@LX_ITEM$ = LEVEL$,
      423    18484                                   @ERROR#   = %E$ERR_LEVEL_AND_49_ASSUMED,
      424    18485                                   @LINE     = LEVEL$ -> LX_ITEM.LINE,
      425    18486                                   @COLUMN   = LEVEL$ -> LX_ITEM.COLUMN);
      426    18516
      427    18517    3          O1COM.LEVEL_NO = %LEVEL_49;
      428    18518    3          REPORT_DATA.LEVEL.CURRENT = %LEVEL_49;
      429    18519    3        END;
      430    18520
      431    18521    2      IF REPORT_DATA.LEVEL.CURRENT = %LEVEL_01  THEN
      432    18522    2        IF CDECL$ -> LX_REPORT_REC.P.TYPE$ = ADDR(NIL) THEN
      433    18523    3        DO;
      434    18524    3          LEVEL$ = CDECL$ -> LX_REPORT_REC.P.LEVEL_NO$;
      435    18525
      436    18526               %CONDITIONAL_ERROR (@LX_ITEM$ = LEVEL$,
      437    18527                                   @ERROR#   = %E$TYPE_CLAUSE_MISSING,
      438    18528                                   @LINE     = LEVEL$ -> LX_ITEM.LINE,
      439    18529                                   @COLUMN   = LEVEL$ -> LX_ITEM.COLUMN);
      440    18559
      441    18560    3          REPORT_DATA.GROUP_TYPE = %TYPE_DETAIL;
      442    18561    3        END;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:15   
      443    18562
      444    18563    2    END VALIDATE_LEVEL;
      445    18564         %EJECT;
PL6.E3A0      #001=Z8DT$REPORT_REC File=Z8DT.:Z8A6TSI                            SAT 08/09/97 11:22 Page:16   
      446    18565
      447    18566        /*============*/
      448    18567    1     VALIDATE_PIC: PROC;
      449    18568        /*============*/
      450    18569
      451    18570         /*
      452    18571              NAME: VALIDATE_PIC
      453    18572
      454    18573           DESCRIPTION: The PICTURE clause is validated and the BLANK
      455    18574                        WHEN ZERO clause is also checked against the PIC
      456    18575                        clause here.
      457    18576          */
      458    18577
      459    18578
      460    18579    2   DCL PIC$                      PTR;
      461    18580    2   DCL COLUMN#                   SBIN WORD;
