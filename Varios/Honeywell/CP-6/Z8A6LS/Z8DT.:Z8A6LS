      499    18705    3          PIC_OUT.WIDTH = %DEFAULT_NUMERIC_WIDTH;
      500    18706    3          PIC_OUT.SCALE = %DEFAULT_NUMERIC_SCALE;
      501    18707    3          PIC_OUT.EIS_TYPE = %PIC_EIS_NS;
      502    18708
      503    18709    3          RETURN;
      504    18710    3        END;
      505    18711
      506    18712    2      IF REPORT_DATA.B.ASTERISK THEN
      507    18713    3        DO;
      508    18714               %CONDITIONAL_ERROR (@LX_ITEM$ = PIC$,
      509    18715                                   @ERROR#   = %E$BWZ_AND_ASTERISK,
