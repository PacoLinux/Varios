     2570    23799    4            FIRST_LTS$ = NEXT_LTS$;
     2571    23800    4            NEXT_LTS$ = ADDR(NIL);
     2572    23801    4            FIRST_LINE_NO# = FIRST_LTS$ -> REPORT_GRP_LTS.LINE_NO#;
     2573    23802    4            LAST_LINE_NO# = FIRST_LINE_NO#;
     2574    23803    4            FIRST_LINE_TYPE = FIRST_LTS$ -> REPORT_GRP_LTS.LINE_TYPE;
     2575    23804    4          END;
     2576    23805    3        ELSE
     2577    23806    3          NEXT_LTS$ = NEXT_LTS$ -> REPORT_GRP_LTS.NEXT;
     2578    23807
     2579    23808    3      END; /* while */
     2580    23809
     2581    23810    2      IF FIRST_LTS$ ~= ADDR(NIL) THEN
