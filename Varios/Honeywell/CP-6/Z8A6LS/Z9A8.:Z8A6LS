PL6.E3A0      #001=Z9A8$DCBS File=Z9A8.:Z8A6TSI                                  SAT 08/09/97 13:36 Page:1    
        1        1        /*M* Z9A8$DCBS - Definition and initialization of DCBs */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X*   MCL=5,MOC,CLM=40,CRM=80,CCC,
        8        8               PLM=0,IND=3,SDI=2,CTI=6,AND,DCI=6,CSU=3,ECU=3,THI=3,DTI=6,STI=3       */
        9        9        /*P*   NAME:           Z9A8$DCBS
       10       10               PURPOSE:        Definition of DCBs and DCB pointers used
       11       11                               by the COBOL-85 compiler.
       12       12               DESCRIPTION:    Invokes the %M$DCB macro to define a DCB
       13       13                               for each file used by the compiler.
       14       14                               Initializes the DCB pointer for each file.
       15       15                                                                                     */
       16       16        /*D*   NAME:           Z9A8$DCBS
       17       17               CALL:
       18       18                               CALL Z9A8$DCBS;
       19       19                                     (called only by Z9A1$INITIALIZE).
       20       20               INTERFACE:      None.
       21       21               INPUT:          None.
       22       22               OUTPUT:
       23       23                               Sets Z9A8$xxxx# to n;
       24       24                               Sets Z9A8$xxxx$ to ADDR(xxxx);
       25       25                               Sets ZV1$DCB.ADDR$(n) to ADDR(xxxx);
       26       26                                         where xxxx is DCB name
       27       27                                         and n is DCB number.
       28       28               DESCRIPTION:    Invokes %INIT$DCB macro to initialize.
       29       29                                                                                     */
       30       30        %EJECT;
PL6.E3A0      #001=Z9A8$DCBS File=Z9A8.:Z8A6TSI                                  SAT 08/09/97 13:36 Page:2    
       31       31        Z9A8$DCBS:PROC;
       32       32                                                /**/
       33       33        %SET LISTSUB='1'B;
       34       34        %SET LISTEXP='1'B;
       35       35        %SET LISTMAC='1'B;
       36       36        %INCLUDE Z8A_SYS_FILE;
       37     6311        %INCLUDE Z9A_SUPP_MAC;
       38     6600
       39     6601        %MACRO INIT$DCB(DCBN=,FCG=);
       40     6602        %CHARTEXT(CONCAT(TEXTCHAR(%FCG),'$',TEXTCHAR(%DCBN),'#'))=DCBNUM(DCBN);
       41     6603        %CHARTEXT(CONCAT(TEXTCHAR(%FCG),'$',TEXTCHAR(%DCBN),'$'))=DCBADDR(DCBNUM(DCBN));
       42     6604        %MEND;
       43     6605        %PAGE(ARG=M$SI);
              6606        %LSET LISTDIR='0'B;

