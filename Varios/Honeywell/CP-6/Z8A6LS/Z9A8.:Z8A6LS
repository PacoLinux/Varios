        8        8               PLM=0,IND=3,SDI=2,CTI=6,AND,DCI=6,CSU=3,ECU=3,THI=3,DTI=6,STI=3       */
        9        9        /*P*   NAME:           Z9A8$DCBS
       10       10               PURPOSE:        Definition of DCBs and DCB pointers used
       11       11                               by the COBOL-85 compiler.
       12       12               DESCRIPTION:    Invokes the %M$DCB macro to define a DCB
       13       13                               for each file used by the compiler.
       14       14                               Initializes the DCB pointer for each file.
       15       15                                                                                     */
       16       16        /*D*   NAME:           Z9A8$DCBS
       17       17               CALL:
       18       18                               CALL Z9A8$DCBS;
       19       19                                     (called only by Z9A1$INITIALIZE).
       20       20               INTERFACE:      None.
       21       21               INPUT:          None.
       22       22               OUTPUT:
       23       23                               Sets Z9A8$xxxx# to n;
       24       24                               Sets Z9A8$xxxx$ to ADDR(xxxx);
       25       25                               Sets ZV1$DCB.ADDR$(n) to ADDR(xxxx);
       26       26                                         where xxxx is DCB name
       27       27                                         and n is DCB number.
       28       28               DESCRIPTION:    Invokes %INIT$DCB macro to initialize.
       29       29                                                                                     */
       30       30        %EJECT;
PL6.E3A0      #001=Z9A8$DCBS File=Z9A8.:Z8A6TSI                                  SAT 08/09/97 13:36 Page:40   
