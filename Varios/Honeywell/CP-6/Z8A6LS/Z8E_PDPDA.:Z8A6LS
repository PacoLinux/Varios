548       |

549  <INPUT_OUTPUT_EXTEND_opt> ::= <INPUT_OUTPUT_EXTEND>
550       |

551  <EXCEPTION_ERROR> ::= EXCEPTION
552       | ERROR

553  <USE_debugging_list_opt> ::= <USE_debugging_list>
554       |

555  <USE_debugging_list> ::= <USE_debugging_type>
556       | <USE_debugging_list> <USE_debugging_type>

557  <USE_debugging_type> ::= ALL PROCEDURES
558       | <all_references_opt> <identifier> <with_conversion_opt>

559  <all_references_opt> ::= ALL <REFERENCES_opt> <OF_opt>
560       |

561  <with_conversion_opt> ::= WITH CONVERSION
562       | CONVERSION
563       |

564  <imperative_WRITE> ::= '=start-of-statement=' WRITE <sep_id_no_refmod_no_sub> <from_ident_phrase_opt>
                    <before_after_advancing_opt> {ACT_SAVE_WRITE_ADV}
565       | '=start-of-statement=' WRITE <sep_id_no_refmod_no_sub> <from_ident_phrase_opt> AFTER POSITIONING
                    <WRITE_pos_num_id> <LINE_LINES_opt> {ACT_SAVE_WRITE_POS}

566  <delimited_WRITE> ::= <conditional_WRITE> END-WRITE

567  <conditional_WRITE> ::= <imperative_WRITE> <EOP_phrase> {ACT_COND_WRITE_EOP}
568       | <imperative_WRITE> <INVALID_clause> {ACT_COND_WRITE_INVALID}

569  <EOP_phrase> ::= <NOT_AT_opt> END-OF-PAGE <imperative_stmts> <NOT_EOP_phrase> {ACT_EOP}

570  <NOT_EOP_phrase> ::= NOT <AT_opt> END-OF-PAGE <imperative_stmts> {ACT_NOT_EOP}
571       |

572  <WRITE_pos_num_id> ::= number
573       | <ident_no_refmod>

