636  <DML_data_name_list> ::= <identifier_list>

637  <DML_store> ::= '=start-of-statement=' STORE <DML_data_name> {ACT_STORE}
638       | '=start-of-statement=' STORE <DML_data_name> <retaining_clause> {ACT_STORE_RETAINING}

639  <retaining_clause> ::= RETAINING <CURRENCY_opt> <FOR_opt> <retaining_type_list> {ACT_PROP_LX_TOP}

640  <retaining_type_list> ::= <retaining_type> {ACT_CREATE_LIST_OF_ONE_LX_STK}
641       | <retaining_type_list> <retaining_type> {ACT_ADD_LX_STK_TO_LIST_TAIL}

642  <retaining_type> ::= REALM
643       | SETS
644       | <DML_data_name>
645       | RECORD

646  <DML_use> ::= USE <FOR_opt> DB-EXCEPTION {ACT_DB_EXCEPT_ALL}
647       | USE <FOR_opt> DB-EXCEPTION <ON_opt> OTHER {ACT_DB_EXCEPT_OTHER}
648       | USE <FOR_opt> DB-EXCEPTION <ON_opt> <literal_list> {ACT_DB_EXCEPT_INDICATOR}
649       | USE <FOR_opt> PRIVACY <privacy_phrase_list> {ACT_PRIVACY}

650  <privacy_phrase_list> ::= <privacy_phrase> {ACT_CREATE_LIST_OF_ONE_LX_STK}
651       | <privacy_phrase_list> <privacy_phrase> {ACT_ADD_LX_STK_TO_LIST_TAIL}

652  <privacy_phrase> ::= <privacy_on_phrase> <FOR_opt> RECORDS {ACT_PRIVACY_PHRASE}
653       | <privacy_on_phrase> <FOR_opt> <sep_id_no_refmod_no_sub_list> {ACT_PRIVACY_PHRASE}
654       | <privacy_on_phrase> <FOR_opt> SETS {ACT_PRIVACY_PHRASE}

655  <privacy_on_phrase> ::= ON <privacy_word_list> {ACT_PROP_LX_TOP}
656       | <privacy_word_list>
657       |

658  <privacy_word_list> ::= '=start-of-statement=' <privacy_word> {ACT_CREATE_LIST_OF_ONE_LX_STK}
659       | <privacy_word_list> '=start-of-statement=' <privacy_word> {ACT_ADD_PRIVACY_TO_LIST_TAIL}

660  <privacy_word> ::= CONNECT
661       | DISCONNECT
662       | STORE
663       | ERASE
664       | GET
665       | MODIFY
666       | FIND

667  <arith_expr> ::= <arith_expr> + <term> {ACT_PLUS_OP}
668       | <arith_expr> - <term> {ACT_MINUS_OP}
669       | <term>

670  <term> ::= <term> * <pwr> {ACT_MULT_OP}
671       | <term> / <pwr> {ACT_DIV_OP}
672       | <pwr>

673  <pwr> ::= <pwr> '**' <unary> {ACT_EXP_OP}
674       | <pwr> ^ <unary> {ACT_EXP_OP}
675       | <unary>

676  <unary> ::= <factor>
677       | + <factor> {ACT_PLUS_FACTOR}
678       | - <factor> {ACT_MINUS_FACTOR}

679  <factor> ::= ( <arith_expr> ) {ACT_PAREN_EXPR}
680       | <num-lit>
681       | <identifier>
682       | <length_register>

683  <condition> ::= {ACT_START_COND} <rec_condition> {ACT_END_CONDITION}

684  <rec_condition> ::= <rec_condition> OR <AND_condition> {ACT_COND_OR_COND}
685       | <AND_condition>

686  <AND_condition> ::= <AND_condition> AND <NOT_condition> {ACT_COND_AND_COND}
687       | <NOT_condition>

688  <NOT_condition> ::= NOT <relation_condition> {ACT_NOT_RELATE_COND}
689       | <relation_condition>
690       | NOT <sign_class_condition> {ACT_NOT_SIGN_COND}
691       | <sign_class_condition>
692       | IS <DML_class_sign> {ACT_DML_CLASS_SIGN}
693       | IS NOT <DML_class_sign> {ACT_DML_NOT_CLASS_SIGN}
694       | NOT <DML_class_sign> {ACT_DML_NOT_CLASS_SIGN}
695       | <DML_class_sign> {ACT_DML_CLASS_SIGN}
696       | ( <rec_condition> ) {ACT_COND_PAREN_EXPR}
697       | NOT ( <rec_condition> ) {ACT_NOT_PAREN_COND}
698       | <arith_expr> {ACT_ARITH_EXPR_COND}
699       | NOT <arith_expr> {ACT_NOT_ARITH_EXPR_COND}
700       | <alpha_constant_no_zero> {ACT_ALPHA_CONST_COND}
701       | NOT <alpha_constant_no_zero> {ACT_NOT_ALPHA_CONST_COND}
702       | <relational_operator> <alpha_or_arith> {ACT_RELATE_ALPHA_ARITH}

703  <relation_condition> ::= <arith_expr> <relational_operator> <alpha_or_arith> {ACT_RELATION_CONDITION}
704       | <alpha_constant_no_zero> <relational_operator> <alpha_or_arith> {ACT_RELATION_CONDITION}
705       | <address_register> <relational_operator> <alpha_or_arith> {ACT_RELATION_CONDITION}
706       | <arith_expr> <relational_operator> ( <arith_abbrev> ) {ACT_ABRV}
707       | <alpha_constant_no_zero> <relational_operator> ( <arith_abbrev> ) {ACT_ABRV}
708       | <address_register> <relational_operator> ( <arith_abbrev> ) {ACT_ABRV}

709  <arith_abbrev> ::= ( <arith_abbrev> ) {ACT_PROP_LX_TOP_1}
710       | <arith_abbrev_1> <or_and> <arith_abbrev_2> {ACT_COND_OR_AND_COND}

711  <arith_abbrev_1> ::= <arith_abbrev_1> <or_and> <arith_abbrev_2> {ACT_COND_OR_AND_COND}
712       | <arith_abbrev_2>

713  <arith_abbrev_2> ::= <alpha_or_arith>
714       | NOT <alpha_or_arith> {ACT_NOT_ABBREV}
715       | ( <arith_abbrev_1> <or_and> <arith_abbrev_2> ) {ACT_COND_OR_AND_COND_1}

716  <or_and> ::= OR
717       | AND

718  <alpha_or_arith> ::= <arith_expr>
719       | <alpha_constant_no_zero>
720       | <address_register>

721  <sign_class_condition> ::= <arith_expr> IS NOT <class_sign> {ACT_NOT_CLASS_SIGN}
722       | <arith_expr> IS <class_sign> {ACT_CLASS_SIGN}
723       | <arith_expr> NOT <class_sign> {ACT_NOT_CLASS_SIGN}
724       | <arith_expr> <class_sign> {ACT_CLASS_SIGN}

725  <class_sign> ::= NUMERIC
726       | ALPHABETIC
727       | ALPHABETIC-LOWER
728       | ALPHABETIC-UPPER
729       | POSITIVE
730       | NEGATIVE
731       | ZERO
732       | EMPTY
733       | OWNER
734       | MEMBER
735       | TENANT
736       | <sep_id_no_refmod_no_sub>

737  <DML_class_sign> ::= OWNER
738       | MEMBER
739       | TENANT

740  <relational_operator> ::= <basic_rel_op>
741       | <less_than_rel_op>
742       | <new_rel_op>
743       | IS <basic_rel_op> {ACT_IS_REL_OP}
744       | IS <less_than_rel_op> {ACT_IS_REL_OP}
745       | IS <new_rel_op> {ACT_IS_REL_OP}
746       | NOT <basic_rel_op> {ACT_NOT_REL_OP}
747       | NOT <less_than_rel_op> {ACT_NOT_REL_OP}
748       | IS NOT <basic_rel_op> {ACT_NOT_REL_OP}
749       | IS NOT <less_than_rel_op> {ACT_NOT_REL_OP}
750       | EXCEEDS {ACT_EXCEEDS}
751       | IS UNEQUAL <TO_opt> {ACT_UNEQUAL}
752       | UNEQUAL <TO_opt> {ACT_UNEQUAL}

753  <less_than_rel_op> ::= LESS <THAN_opt> {ACT_LESS_THAN}
754       | < {ACT_LESS_THAN}

755  <basic_rel_op> ::= EQUAL <TO_opt> {ACT_EQUAL_TO}
756       | = <TO_opt> {ACT_EQUAL_TO}
757       | GREATER <THAN_opt> {ACT_GREATER_THAN}
758       | > {ACT_GREATER_THAN}

759  <new_gtr_rel_op> ::= GREATER <THAN_opt> OR EQUAL <TO_opt> {ACT_GREATER_THAN_OR_EQUAL}
760       | '>=' {ACT_GREATER_THAN_OR_EQUAL}

761  <new_rel_op> ::= <new_gtr_rel_op>
762       | LESS <THAN_opt> OR EQUAL <TO_opt> {ACT_LESS_THAN_OR_EQUAL}
763       | '<=' {ACT_LESS_THAN_OR_EQUAL}

764  <num_ident_lit> ::= <ident_no_refmod>
765       | <num-lit>
766       | <length_register>

767  <num-lit> ::= <integer>
768       | ZERO
769       | numeric_literal

770  <num_ident_lit_list> ::= <num_ident_lit> {ACT_CREATE_LIST_OF_ONE_LX_STK}
771       | <num_ident_lit_list> <num_ident_lit> {ACT_ADD_LX_STK_TO_LIST_TAIL}

772  <ident_ident-round> ::= <ident_no_refmod>
