625       |

626  <INCLUDING_ONLY> ::= INCLUDING
627       | ONLY

628  <DML_precalculate> ::= '=start-of-statement=' PRECALCULATE <DML_data_name> {ACT_DML_PRECALCULATE}

629  <DML_ready> ::= '=start-of-statement=' READY <data_name_list_opt> {ACT_DML_READY}
630       | '=start-of-statement=' READY <data_name_list_opt> USAGE-MODE <IS_opt> <RETRIEVAL_UPDATE_LOAD>
                    {ACT_DML_READY_USAGE}

