
<Cobol85_procedure_division> assumed to be the goal symbol.


===============
GRAMMAR LISTING
===============


1    <SYSTEM_GS> ::= =BEGINNING_OF_FILE= <Cobol85_procedure_division> =END_OF_FILE=

2    <Cobol85_procedure_division> ::= <program_procedure_division> {ACT_EOF}

3    <program_procedure_division> ::= <proc_div>
4         | <proc_div> <program>
5         | <proc_div> <end_program_list>
6         | <proc_div> END COBOL .
7         | <proc_div> <end_program_list> <program>

8    <program> ::= {GET_NEXT_OVERLAY} <control_div_opt> IDENTIFICATION DIVISION .

9    <control_div_opt> ::= CONTROL DIVISION .
10        |

11   <end_program_list> ::= <end_program>
12        | <end_program_list> <end_program>

13   <end_program> ::= END PROGRAM <data_name> . {ACT_END_PROGRAM}

14   <proc_div> ::= <procedure_div_using> . DECLARATIVES . <declaratives_sections> END DECLARATIVES .
                    {ACT_END_DECLARATIVES} <proc_section> {ACT_END_PROC_DIV_DECL}
15        | <procedure_div_using> . {ACT_NO_DECLARATIVES} <proc_section> {ACT_END_PROC_DIV}
16        | <procedure_div_using> . {ACT_NO_DECLARATIVES} <paragraphs> {ACT_END_PROC_DIV}

17   <procedure_div_using> ::= PROCEDURE DIVISION {ACT_PROCEDURE_DIV}
18        | PROCEDURE DIVISION USING <sep_id_no_refmod_no_sub_list> {ACT_PROC_DIV_USING}
19        | PROCEDURE DIVISION USING <sep_id_no_refmod_no_sub_list> ENTRY <section_name_list>
                    {ACT_PROC_DIV_USING_ENTRY}
20        | PROCEDURE DIVISION ENTRY <section_name_list> {ACT_PROC_DIV_ENTRY}

21   <proc_section> ::= <section_header>
22        | <proc_section> <paragraph> {ACT_APPEND_STMT_LIST}
23        | <proc_section> <section_header> {ACT_APPEND_STMT_LIST}

24   <declaratives_sections> ::= <section_header> <USE_statement> . {ACT_DECLARATIVES_SECTION}
25        | <declaratives_sections> <paragraph> {ACT_DECLARATIVES_PARA}
26        | <declaratives_sections> <section_header> <USE_statement> . {ACT_DECLARATIVES_NEW_SECTION}

27   <section_header> ::= <paragraph_def> SECTION . {ACT_SECTION}
28        | <paragraph_def> SECTION <integer> . {ACT_SECTION_NUMBERED}

29   <paragraphs> ::= <paragraph>
30        | <paragraphs> <paragraph> {ACT_APPEND_STMT_LIST}

31   <paragraph> ::= <paragraph_header> .
32        | <paragraph_header> . <sentences> {ACT_PARA_WITH_SENT}
33        | <paragraph_header> . '=start-of-statement=' EXIT . {ACT_EXIT}

34   <paragraph_header> ::= <paragraph_def> {ACT_PARAGRAPH}

35   <paragraph_def> ::= number_or_paragraph
36        | paragraph_def
37        | name_or_paragraph_def

38   <sentences> ::= <imp_cond_stmts> .
39        | <sentences> <imp_cond_stmts> . {ACT_ADD_SENTENCES}

40   <imp_cond_stmts> ::= <imperative_stmts>
41        | <imperative_stmts> <conditional_stmt> {ACT_ADD_TO_COND_LIST}
42        | <conditional_stmt> {ACT_CREATE_COND_LIST}

43   <imperative_stmts> ::= <imperative_stmt> {ACT_CREATE_STMT_LIST}
44        | <imperative_stmts> <imperative_stmt> {ACT_ADD_TO_STMT_LIST}

45   <imp_non_IF_cond> ::= <imperative_stmts>
46        | <imperative_stmts> <non_IF_cond_stmt> {ACT_ADD_TO_STMT_LIST}
47        | <non_IF_cond_stmt> {ACT_CREATE_STMT_LIST}

48   <conditional_stmt> ::= <conditional_IF>
49        | <non_IF_cond_stmt>

50   <non_IF_cond_stmt> ::= <conditional_ADD>
51        | <conditional_CALL>
52        | <conditional_COMPUTE>
53        | <conditional_DELETE>
54        | <conditional_DIVIDE>
55        | <conditional_EVALUATE>
56        | <conditional_MULTIPLY>
57        | <conditional_READ>
58        | <conditional_RECEIVE>
59        | <conditional_RETURN>
60        | <conditional_REWRITE>
61        | <conditional_SEARCH>
62        | <conditional_START>
63        | <conditional_STRING>
64        | <conditional_SUBTRACT>
65        | <conditional_UNSTRING>
66        | <conditional_WRITE>

67   <imperative_stmt> ::= <delimited_stmt>
68        | <imperative_ACCEPT>
69        | <imperative_ADD> {ACT_IMPERATIVE_ADD}
70        | <imperative_ADD> END-ADD {ACT_IMPERATIVE_ADD}
71        | <imperative_ALTER>
72        | <imperative_CALL> {ACT_IMPERATIVE_CALL}
73        | <imperative_CALL> END-CALL {ACT_IMPERATIVE_CALL}
74        | <imperative_CANCEL>
75        | <imperative_CLOSE>
76        | <imperative_COMPUTE> {ACT_IMPERATIVE_COMPUTE}
77        | <imperative_COMPUTE> END-COMPUTE {ACT_IMPERATIVE_COMPUTE}
78        | <imperative_CONTINUE>
79        | <imperative_DELETE> {ACT_IMPERATIVE_DELETE}
80        | <imperative_DELETE> END-DELETE {ACT_IMPERATIVE_DELETE}
81        | <imperative_DISABLE>
82        | <imperative_DISPLAY>
83        | <imperative_DIVIDE> {ACT_IMPERATIVE_DIVIDE}
84        | <imperative_DIVIDE> END-DIVIDE {ACT_IMPERATIVE_DIVIDE}
85        | <imperative_ENABLE>
86        | <imperative_ENTRY>
87        | <imperative_EXAMINE>
88        | <imperative_EXIT_PROGRAM>
89        | <imperative_GO_TO>
90        | <imperative_GENERATE>
91        | <imperative_INITIATE>
92        | <imperative_INITIALIZE>
93        | <imperative_INSPECT>
94        | <imperative_MERGE>
95        | <imperative_MOVE>
96        | <imperative_MULTIPLY> {ACT_IMPERATIVE_MULTIPLY}
97        | <imperative_MULTIPLY> END-MULTIPLY {ACT_IMPERATIVE_MULTIPLY}
98        | <imperative_OPEN>
99        | <imperative_PERFORM>
100       | <imperative_PURGE>
101       | <imperative_READ> {ACT_IMPERATIVE_READ}
102       | <imperative_READ> END-READ {ACT_IMPERATIVE_READ}
103       | <imperative_RECEIVE> {ACT_IMPERATIVE_RECEIVE}
104       | <imperative_RECEIVE> END-RECEIVE {ACT_IMPERATIVE_RECEIVE}
105       | <imperative_RELEASE>
106       | <imperative_REWRITE> {ACT_IMPERATIVE_REWRITE}
107       | <imperative_REWRITE> END-REWRITE {ACT_IMPERATIVE_REWRITE}
108       | <imperative_SEND>
109       | <imperative_SET>
110       | <imperative_SORT>
111       | <imperative_START> {ACT_IMPERATIVE_START}
112       | <imperative_START> END-START {ACT_IMPERATIVE_START}
113       | <imperative_STRING> {ACT_IMPERATIVE_STRING}
114       | <imperative_STRING> END-STRING {ACT_IMPERATIVE_STRING}
115       | <imperative_STOP>
116       | <imperative_SUBTRACT> {ACT_IMPERATIVE_SUBTRACT}
117       | <imperative_SUBTRACT> END-SUBTRACT {ACT_IMPERATIVE_SUBTRACT}
118       | <imperative_SUPPRESS>
119       | <imperative_TERMINATE>
120       | <imperative_UNSTRING> {ACT_IMPERATIVE_UNSTRING}
121       | <imperative_UNSTRING> END-UNSTRING {ACT_IMPERATIVE_UNSTRING}
122       | <imperative_WRITE> {ACT_IMPERATIVE_WRITE}
123       | <imperative_WRITE> END-WRITE {ACT_IMPERATIVE_WRITE}
124       | <DML_statement>

125  <delimited_stmt> ::= <delimited_ADD>
126       | <delimited_CALL>
127       | <delimited_COMPUTE>
128       | <delimited_DELETE>
129       | <delimited_DIVIDE>
130       | <delimited_EVALUATE>
131       | <delimited_IF>
132       | <delimited_MULTIPLY>
133       | <delimited_PERFORM>
134       | <delimited_READ>
135       | <delimited_RECEIVE>
136       | <delimited_RETURN>
137       | <delimited_REWRITE>
138       | <delimited_SEARCH>
139       | <delimited_START>
140       | <delimited_STRING>
141       | <delimited_SUBTRACT>
142       | <delimited_UNSTRING>
143       | <delimited_WRITE>

144  <DML_statement> ::= <DML_accept>
145       | <DML_connect>
146       | <DML_disconnect>
147       | <DML_erase>
148       | <DML_find>
149       | <DML_finish>
150       | <DML_get>
151       | <DML_modify>
152       | <DML_precalculate>
153       | <DML_ready>
154       | <DML_store>

155  <imperative_ACCEPT> ::= '=start-of-statement=' ACCEPT <identifier> {ACT_ACCEPT_DEFAULT}
156       | '=start-of-statement=' ACCEPT <identifier> FROM <mnemonic_name> {ACT_ACCEPT_MNEMONIC}
157       | '=start-of-statement=' ACCEPT <identifier> FROM DATE {ACT_ACCEPT_DATE}
158       | '=start-of-statement=' ACCEPT <identifier> FROM DAY {ACT_ACCEPT_DAY}
159       | '=start-of-statement=' ACCEPT <identifier> FROM TIME {ACT_ACCEPT_TIME}
160       | '=start-of-statement=' ACCEPT <identifier> FROM DAY-OF-WEEK {ACT_ACCEPT_DOW}
161       | '=start-of-statement=' ACCEPT <identifier> <MESSAGE_opt> COUNT {ACT_NOT_IMPLEMENTED}
                    {ACT_ACCEPT_COUNT}

162  <imperative_ADD> ::= '=start-of-statement=' ADD <ADD_body> {ACT_SAVE_ADD_INFO}

163  <delimited_ADD> ::= <conditional_ADD> END-ADD

164  <conditional_ADD> ::= <imperative_ADD> <size_error_phrase> {ACT_CONDITIONAL_ADD}

165  <ADD_body> ::= <num_ident_lit_list> TO <ident_ident-round_list> {ACT_ADD_TO}
166       | <ADD_to_no_round> GIVING <ident_ident-round_list> {ACT_ADD_GIVING}
167       | CORRESPONDING <sep_id_no_refmod_opt_sub> TO <sep_id_no_refmod_opt_sub_round>
                    {ACT_ADD_CORRESPONDING}

168  <ADD_to_no_round> ::= <num_ident_lit_list> TO <num_ident_lit> {ACT_ADD_TO_ADD_OPERAND_LIST}
169       | <num_ident_lit_list>

170  <size_error_phrase> ::= <NOT_ON_opt> SIZE ERROR <imperative_stmts> <NOT_phrase_opt> {ACT_SIZE_ERROR}

171  <NOT_phrase_opt> ::= NOT <ON_opt> SIZE ERROR <imperative_stmts> {ACT_NOT_SIZE_ERROR}
172       |

173  <imperative_ALTER> ::= '=start-of-statement=' ALTER <ALTER_list> {ACT_ALTER}

174  <ALTER_list> ::= <ALTER_item> {ACT_CREATE_LIST_OF_ONE_LX_STK}
175       | <ALTER_list> <ALTER_item> {ACT_ADD_LX_STK_TO_LIST_TAIL}

176  <ALTER_item> ::= <procedure_name> TO <PROCEED_TO_opt> <procedure_name> {ACT_ALTER_NAME}

177  <PROCEED_TO_opt> ::= PROCEED TO
178       |

179  <imperative_CALL> ::= '=start-of-statement=' CALL <ident_alpha_lit_no_ALL> {ACT_SAVE_CALL_NO_ARG}
180       | '=start-of-statement=' CALL <ident_alpha_lit_no_ALL> USING <CALL_using_list>
                    {ACT_SAVE_CALL_WITH_ARG}

181  <delimited_CALL> ::= <conditional_CALL> END-CALL

182  <conditional_CALL> ::= <imperative_CALL> <overflow_phrase> {ACT_COND_CALL}
183       | <imperative_CALL> <exception_phrase> {ACT_COND_CALL}

184  <CALL_using_list> ::= <arg_list> {ACT_CALL_REF_ARGS}
185       | <arg_list> <CALL_typed_list> {ACT_CALL_MERGE_ARGS}
186       | <CALL_typed_list>

187  <CALL_typed_list> ::= <CALL_typed>
188       | <CALL_typed_list> <CALL_typed> {ACT_CALL_MERGE_TYPED_ARGS}

189  <CALL_typed> ::= <BY_opt> REFERENCE <BY_REFERENCE_arg_list> {ACT_CALL_REF_ARGS}
190       | <BY_opt> CONTENT <BY_CONTENT_arg_list> {ACT_CALL_CONTENT_ARGS}

191  <overflow_phrase> ::= <ON_opt> OVERFLOW <imperative_stmts> {ACT_PROP_LX_TOP}

192  <exception_phrase> ::= <ON_opt> EXCEPTION <imperative_stmts> <NOT_EXCEPT_opt> {ACT_EXCP_OVER_ERROR}
193       | NOT_ON_EXCEPTION <imperative_stmts> <NOT_EXCEPT_opt> {ACT_EXCP_OVER_ERROR}

194  <NOT_EXCEPT_opt> ::= NOT_ON_EXCEPTION <imperative_stmts> {ACT_NOT_EXCP_OVER_ERROR}
195       |

196  <imperative_CANCEL> ::= '=start-of-statement=' CANCEL <ident_alpha_lit_no_ALL_list> {ACT_CANCEL}

197  <imperative_CLOSE> ::= '=start-of-statement=' CLOSE <CLOSE_file_list> {ACT_IMPERATIVE_CLOSE}

198  <CLOSE_file_list> ::= <CLOSE_file_options> {ACT_CREATE_LIST_OF_ONE_LX_STK}
199       | <CLOSE_file_list> <CLOSE_file_options> {ACT_ADD_LX_STK_TO_LIST_TAIL}

200  <CLOSE_file_options> ::= <file_name> <WITH_opt> LOCK {ACT_CLOSE_LOCK}
201       | <file_name> <WITH_opt_NO> REWIND {ACT_CLOSE_NO_REWIND}
202       | <file_name> REEL {ACT_CLOSE_REEL}
203       | <file_name> REEL <FOR_opt> REMOVAL {ACT_CLOSE_REEL_REMOVAL}
204       | <file_name> UNIT {ACT_CLOSE_UNIT}
205       | <file_name> UNIT <FOR_opt> REMOVAL {ACT_CLOSE_UNIT_REMOVAL}
206       | <file_name> REEL <WITH_opt_NO> REWIND {ACT_CLOSE_REEL_NO_REWIND}
207       | <file_name> UNIT <WITH_opt_NO> REWIND {ACT_CLOSE_UNIT_NO_REWIND}
208       | <file_name> {ACT_CLOSE_DEFAULT}

209  <imperative_COMPUTE> ::= '=start-of-statement=' COMPUTE <ident_ident-round_list> = <arith_expr>
                    {ACT_SAVE_COMPUTE_INFO}
210       | '=start-of-statement=' COMPUTE <ident_ident-round_list> FROM <arith_expr> {ACT_SAVE_COMPUTE_INFO}
211       | '=start-of-statement=' COMPUTE <ident_ident-round_list> EQUAL <arith_expr>
                    {ACT_SAVE_COMPUTE_INFO}

212  <delimited_COMPUTE> ::= <conditional_COMPUTE> END-COMPUTE

213  <conditional_COMPUTE> ::= <imperative_COMPUTE> <size_error_phrase> {ACT_CONDITIONAL_COMPUTE}

214  <imperative_CONTINUE> ::= '=start-of-statement=' CONTINUE {ACT_CONTINUE}

215  <imperative_DELETE> ::= '=start-of-statement=' DELETE <file_name> <RECORD_opt> {ACT_SAVE_DELETE}

216  <delimited_DELETE> ::= <conditional_DELETE> END-DELETE

217  <conditional_DELETE> ::= <imperative_DELETE> <INVALID_clause> {ACT_COND_DELETE}

218  <INVALID_clause> ::= <NOT_opt> INVALID <KEY_opt> <imperative_stmts> <NOT_INVALID_opt>
                    {ACT_INVALID_PHRASE}

219  <NOT_INVALID_opt> ::= NOT INVALID <KEY_opt> <imperative_stmts> {ACT_NOT_INVALID_PHRASE}
220       |

221  <imperative_DISABLE> ::= '=start-of-statement=' DISABLE {ACT_NOT_IMPLEMENTED} <DISABLE_type> <cd_name>
                    {ACT_DISABLE}

222  <DISABLE_type> ::= INPUT <TERMINAL_opt>
223       | I-O TERMINAL
224       | OUTPUT

225  <imperative_DISPLAY> ::= DISPLAY <ident_lit_list> <DISPLAY_options> {ACT_DISPLAY_STATEMENT}
226       | DISPLAY <ident_lit_list> UPON <mnemonic_name> <DISPLAY_options> {ACT_DISPLAY_UPON_STATEMENT}

227  <DISPLAY_options> ::= <WITH_opt_NO> ADVANCING {ACT_DISP_NO_ADVANCE}
228       |

229  <imperative_DIVIDE> ::= '=start-of-statement=' DIVIDE <DIVIDE_body> {ACT_SAVE_DIVIDE_INFO}

230  <delimited_DIVIDE> ::= <conditional_DIVIDE> END-DIVIDE

231  <conditional_DIVIDE> ::= <imperative_DIVIDE> <size_error_phrase> {ACT_CONDITIONAL_DIVIDE}

232  <DIVIDE_body> ::= <num_ident_lit> INTO <ident_ident-round_list> {ACT_DIVIDE_INTO_ID_LIST}
233       | <DIVIDE_no_round> GIVING <ident_ident-round_list> {ACT_DIVIDE_GIVING}
234       | <DIVIDE_no_round> GIVING <ident_ident-round> REMAINDER <identifier> {ACT_DIVIDE_WITH_REMAINDER}

235  <DIVIDE_no_round> ::= <num_ident_lit> INTO <num_ident_lit> {ACT_DIVIDE_INTO}
236       | <num_ident_lit> BY <num_ident_lit> {ACT_DIVIDE_BY}

237  <imperative_ENABLE> ::= '=start-of-statement=' ENABLE {ACT_NOT_IMPLEMENTED} <DISABLE_type> <cd_name>
                    {ACT_ENABLE}

238  <imperative_ENTRY> ::= ENTRY <alpha_constant_no_ALL> {ACT_IBM_ENTRY}
239       | ENTRY <alpha_constant_no_ALL> USING <sep_id_no_refmod_no_sub_list> {ACT_IBM_ENTRY_USING}

240  <delimited_EVALUATE> ::= <conditional_EVALUATE> END-EVALUATE

241  <conditional_EVALUATE> ::= '=start-of-statement=' EVALUATE <EVAL_expression_list> <EVAL_when_list>
                    {ACT_END_EVALUATE_NO_OTHER}
242       | '=start-of-statement=' EVALUATE <EVAL_expression_list> <EVAL_when_list> WHEN OTHER
                    <imperative_stmts> {ACT_END_EVALUATE}

243  <EVAL_expression_list> ::= <EVAL_expression> {ACT_CREATE_LIST_OF_ONE_LX_STK}
244       | <EVAL_expression_list> ALSO <EVAL_expression> {ACT_EVAL_ADD_ALSO_LIST}

245  <EVAL_expression> ::= {ACT_START_EVAL_COND} <rec_condition> {ACT_END_EVAL_COND}
246       | TRUE
247       | FALSE

248  <EVAL_when_list> ::= <EVAL_when_item> {ACT_CREATE_LIST_OF_ONE_LX_STK}
249       | <EVAL_when_list> <EVAL_when_item> {ACT_ADD_LX_STK_TO_LIST_TAIL}

250  <EVAL_when_item> ::= <WHEN_clause_list> <imperative_stmts> {ACT_EVAL_WHEN_CLAUSE}

251  <WHEN_clause_list> ::= WHEN <WHEN_selection_list> {ACT_CREATE_LIST_OF_ONE_LX_STK}
252       | <WHEN_clause_list> WHEN <WHEN_selection_list> {ACT_EVAL_ADD_ALSO_LIST}

253  <WHEN_selection_list> ::= <WHEN_selection_item> {ACT_CREATE_LIST_OF_ONE_LX_STK}
254       | <WHEN_selection_list> ALSO <WHEN_selection_item> {ACT_EVAL_ADD_ALSO_LIST}

255  <WHEN_selection_item> ::= ANY
256       | <EVAL_expression>
257       | {ACT_START_EVAL_COND} <arith_expr> THRU <alpha_or_arith> {ACT_EVAL_AE_THRU_AE}
258       | {ACT_START_EVAL_COND} <alpha_constant_no_zero> THRU <alpha_or_arith> {ACT_EVAL_AE_THRU_AE}
259       | {ACT_START_EVAL_COND} NOT <arith_expr> THRU <alpha_or_arith> {ACT_EVAL_NOT_AE_THRU_AE}
260       | {ACT_START_EVAL_COND} NOT <alpha_constant_no_zero> THRU <alpha_or_arith>
                    {ACT_EVAL_NOT_AE_THRU_AE}

261  <imperative_EXAMINE> ::= '=start-of-statement=' EXAMINE <identifier> TALLYING <EXAMINE_TALLYING_list>
                    {ACT_EXAMINE_TALLYING}
262       | '=start-of-statement=' EXAMINE <identifier> TALLYING <EXAMINE_TALLYING_list> <REPL_BY>
                    <EXAMINE_lit_opt> <EXAMINE_REPL_list_opt> {ACT_EXAMINE_TALLYING_REP}
263       | '=start-of-statement=' EXAMINE <identifier> REPLACING <EXAMINE_REPL_list> {ACT_EXAMINE_REPLACING}

264  <EXAMINE_TALLYING_list> ::= <EXAMINE_TALLYING_item> {ACT_CREATE_LIST_OF_ONE_LX_STK}
265       | <EXAMINE_TALLYING_list> <EXAMINE_TALLYING_item> {ACT_ADD_LX_STK_TO_LIST_TAIL}

266  <EXAMINE_TALLYING_item> ::= <EXAMINE_TALLYING_type> <EXAMINE_lit> {ACT_CREATE_LIST_OF_TWO_LX_STK}

267  <EXAMINE_lit_opt> ::= <EXAMINE_lit>
268       |

269  <EXAMINE_REPL_list_opt> ::= <EXAMINE_REPL_list>
270       |

271  <EXAMINE_REPL_list> ::= <EXAMINE_REPL_item>
272       | <EXAMINE_REPL_list> <EXAMINE_REPL_item> {ACT_EXAMINE_REPL_LIST}

273  <EXAMINE_REPL_item> ::= <EXAMINE_type> <EXAMINE_lit> BY <EXAMINE_lit> {ACT_EXAMINE_REPL_item}

274  <REPL_BY> ::= REPLACING
275       | REPLACING BY {ACT_PROP_LX_TOP}

276  <EXAMINE_TALLYING_type> ::= <ALL_or_LEADING>
277       | UNTIL FIRST

278  <EXAMINE_lit> ::= <integer>
279       | <ident_alpha_lit_no_ALL>

280  <EXAMINE_type> ::= <EXAMINE_TALLYING_type>
281       | FIRST

282  <imperative_EXIT_PROGRAM> ::= '=start-of-statement=' EXIT PROGRAM {ACT_EXIT_PROGRAM}

283  <imperative_GENERATE> ::= '=start-of-statement=' GENERATE <qualified_id> {ACT_GENERATE}

284  <imperative_GO_TO> ::= '=start-of-statement=' GO <TO_opt> <procedure_name_or_blank> {ACT_GO}
285       | '=start-of-statement=' GO <TO_opt> <procedure_name_list> DEPENDING <ON_opt> <ident_no_refmod>
                    {ACT_DEPENDING_GOTO}

286  <procedure_name_or_blank> ::= <procedure_name>
287       |

288  <procedure_name_list> ::= <procedure_name> {ACT_CREATE_LIST_OF_ONE_LX_STK}
289       | <procedure_name_list> <procedure_name> {ACT_ADD_LX_STK_TO_LIST_TAIL}

290  <conditional_IF> ::= '=start-of-statement=' IF <condition> <THEN_opt> <imp_cond_stmts>
                    {ACT_COND_IF_IMP_STMTS}
291       | '=start-of-statement=' IF <condition> <THEN_opt> <imp_cond_stmts> ELSE <imp_cond_stmts>
                    {ACT_COND_IF_ELSE_IMP_STMTS}
292       | '=start-of-statement=' IF <condition> <THEN_opt> <NEXT_SENTENCE> ELSE <imp_cond_stmts>
                    {ACT_COND_IF_NEXT_SENT_ELSE}
293       | '=start-of-statement=' IF <condition> <THEN_opt> <NEXT_SENTENCE> {ACT_USELESS_IF}
294       | '=start-of-statement=' IF <condition> <THEN_opt> <imp_cond_stmts> ELSE <NEXT_SENTENCE>
                    {ACT_COND_IF_ELSE_NEXT_SENTENCE}
295       | '=start-of-statement=' IF <condition> <THEN_opt> <NEXT_SENTENCE> ELSE <NEXT_SENTENCE>
                    {ACT_USELESS_IF_ELSE}

296  <delimited_IF> ::= '=start-of-statement=' IF <condition> <THEN_opt> <imp_non_IF_cond> END-IF
                    {ACT_DELIM_IF_IMP_STMTS}
297       | '=start-of-statement=' IF <condition> <THEN_opt> <imp_cond_stmts> ELSE <imp_non_IF_cond> END-IF
                    {ACT_DELIM_IF_ELSE_IMP_STMTS}
298       | '=start-of-statement=' IF <condition> <THEN_opt> <NEXT_SENTENCE> ELSE <imp_non_IF_cond> END-IF
                    {ACT_DELIM_IF_NEXT_SENT_ELSE}

299  <imperative_INITIALIZE> ::= '=start-of-statement=' INITIALIZE <INITIALIZE_id_list> {ACT_INITIALIZE}
300       | '=start-of-statement=' INITIALIZE <INITIALIZE_id_list> REPLACING <replacing_list>
                    {ACT_INITIALIZE_REPLACING}

301  <INITIALIZE_id_list> ::= <INITIALIZE_id> {ACT_CREATE_LIST_OF_ONE_LX_STK}
302       | <INITIALIZE_id_list> <INITIALIZE_id> {ACT_ADD_LX_STK_TO_LIST_TAIL}

303  <replacing_list> ::= <replacing_item> {ACT_CREATE_LIST_OF_ONE_LX_STK}
304       | <replacing_list> <replacing_item> {ACT_ADD_LX_STK_TO_LIST_TAIL}

