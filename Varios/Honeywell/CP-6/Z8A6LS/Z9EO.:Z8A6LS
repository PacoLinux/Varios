       98    15290                           = @LENGTH;
       99    15291                 END;
      100    15292               ELSE
      101    15293               %ENDIF;
      102    15294                 @DESC.DES_W.ADSC.ARG_SIZE  = @LENGTH;
      103    15295             %ELSE;
      104    15296               @DESC.DES_W.NDSC.N = @LENGTH;
      105    15297               @DESC.DES_W.NDSC.SF = @SCALE;
      106    15298             %ENDIF;
      107    15299           END;
      108    15300
      109    15301        %MEND;
