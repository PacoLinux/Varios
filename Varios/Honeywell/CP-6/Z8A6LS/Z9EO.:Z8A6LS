       85    15277                                  @TOT_ARG_COUNT = 0,
       86    15278                                  @DESC$ = "ADDR(NIL)");
       87    15279
       88    15280           DO;
       89    15281             @DESC.DES_W.TYPE  = @TYPE;
       90    15282
