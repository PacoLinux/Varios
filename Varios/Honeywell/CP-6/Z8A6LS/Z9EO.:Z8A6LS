      111    15303        %MACRO EBCDIC_GBCD_TO_RUNTIME_DESC (@NDSC = TYPE_DESC.DESC.NDSC,
      112    15304                                            @TYPE = TYPE_DESC.TYPE,
      113    15305                                            @RUN_DESC = );
      114    15306         DO;
      115    15307
      116    15308               @RUN_DESC.DES_W.NDSC.N = @NDSC.N;
      117    15309               @RUN_DESC.DES_W.NDSC.SF = @NDSC.SF;
      118    15310               DO CASE (BITBIN(@NDSC.S));
      119    15311
      120    15312                 CASE(%BITBIN(LEADING_SIGN));
      121    15313                   IF @NDSC.TN = %NDSC9 /* SEPARATE */ THEN
      122    15314                     @RUN_DESC.DES_W.TYPE = %RUNTIME_EBCDIC_LEAD_SIGN;
      123    15315                   ELSE
      124    15316                     @RUN_DESC.DES_W.TYPE = %RUNTIME_EBCDIC_LEAD_OVER;
      125    15317
      126    15318                 CASE(%BITBIN(TRAILING_SIGN));
      127    15319                   IF @NDSC.TN = %NDSC9 /* SEPARATE */ THEN
      128    15320                     @RUN_DESC.DES_W.TYPE = %RUNTIME_EBCDIC_TRAIL_SIGN;
      129    15321                   ELSE
      130    15322                     @RUN_DESC.DES_W.TYPE = %RUNTIME_EBCDIC_TRAIL_OVER;
      131    15323
      132    15324                 CASE(%BITBIN(NO_SIGN));
      133    15325                   @RUN_DESC.DES_W.TYPE = %RUNTIME_EBCDIC_NO_SIGN;
      134    15326               END; /* CASE */
      135    15327               IF @TYPE = %TYC THEN
      136    15328         @RUN_DESC.DES_W.TYPE = @RUN_DESC.DES_W.TYPE + %(RUNTIME_GBCD_NO_SIGN - RUNTIME_EBCD
             15328        IC_NO_SIGN);
      137    15329         END;
      138    15330        %MEND;
      139    15331        %EJECT;
