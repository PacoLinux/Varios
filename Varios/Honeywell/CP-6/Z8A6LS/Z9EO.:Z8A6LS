       40    15232        %MACRO NUMERIC_DESC_TO_RUNTIME_DESC(@NDSC = TYPE_DESC.DESC.NDSC,
       41    15233                                            @RUN_DESC = );
       42    15234         DO;
       43    15235
       44    15236               @RUN_DESC.DES_W.NDSC.N = @NDSC.N ;
       45    15237               @RUN_DESC.DES_W.NDSC.SF = @NDSC.SF ;
       46    15238               DO CASE (BITBIN(@NDSC.S));
       47    15239
       48    15240                 CASE(%(BITBIN(LEADING_SIGN)));
       49    15241                   IF @NDSC.TN = %NDSC9 THEN
       50    15242                     @RUN_DESC.DES_W.TYPE = %RUNTIME_UNPACKED_LEAD_SIGN;
       51    15243                   ELSE IF ST$ -> VARIABLE.B.P THEN
       52    15244                     @RUN_DESC.DES_W.TYPE = %RUNTIME_PACKED_LEAD_SIGN;
       53    15245                   ELSE
       54    15246                     @RUN_DESC.DES_W.TYPE = %RUNTIME_PACKED_LEAD_SIGN_EBC;
       55    15247
       56    15248                 CASE(%(BITBIN(TRAILING_SIGN)));
       57    15249                   IF @NDSC.TN = %NDSC9 THEN
       58    15250                     @RUN_DESC.DES_W.TYPE = %RUNTIME_UNPACKED_TRAIL_SIGN;
       59    15251                   ELSE IF ST$ -> VARIABLE.B.P THEN
       60    15252                     @RUN_DESC.DES_W.TYPE = %RUNTIME_PACKED_TRAIL_SIGN;
       61    15253                   ELSE
       62    15254                     @RUN_DESC.DES_W.TYPE = %RUNTIME_PACKED_TRAIL_SIGN_EBC;
       63    15255
       64    15256                 CASE(%(BITBIN(NO_SIGN)));
       65    15257                   IF @NDSC.TN = %NDSC9 THEN
       66    15258                     @RUN_DESC.DES_W.TYPE = %RUNTIME_UNPACKED_NO_SIGN;
       67    15259                   ELSE
       68    15260                     @RUN_DESC.DES_W.TYPE = %RUNTIME_PACKED_NO_SIGN;
       69    15261
       70    15262                 CASE(%(BITBIN(FLOATING_POINT_LEADING_SIGN))) ;
       71    15263                   IF @NDSC.TN = %NDSC9 THEN
       72    15264                     @RUN_DESC.DES_W.TYPE = %RUNTIME_UNPACKED_LEAD_FLOAT;
       73    15265                   ELSE
