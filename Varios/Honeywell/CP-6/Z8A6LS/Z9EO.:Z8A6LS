       79    15271        %MACRO FILL_RUNTIME_DESC (@DESC = ,
       80    15272                                  @TYPE = ,
       81    15273                                  @SCALE = ,
       82    15274                                  @LENGTH = ,
       83    15275                                  @LENGTH_VARIABLE = FALSE,
