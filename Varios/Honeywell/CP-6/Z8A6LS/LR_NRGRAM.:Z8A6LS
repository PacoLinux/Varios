      471       15
      472       16        SCANNER: PROC(TOKEN);
      473       17
      474       18
      475       19
      476       20        /*D*
      477       21                   NAME: SCANNER
      478       22        DESCRIPTION: The scanner is also a LALR(1) parser.  This fuctions
      479       23                     as the driver.  Whenever a token in recognized, the
      480       24                     lexical semantics will UNWIND back to the routine.
      481       25
      482       26                    Unfortunately this could not be accomplished using
      483       27                     coroutines because PL6 does not support them.
      484       28        */
      485       29
      486       30    1   DCL TOKEN PTR;
      487       31
      488       32        %INCLUDE LR_GRAM_EXT;
      489      571        %INCLUDE LRX_SUBS;
      490      619
      491      620    1   DCL LEX_REMEMBER REMEMBER EXT;
      492      621    1   DCL P_TOKEN PTR EXT;
      493      622
PL6.E3A0      #003=SCANNER File=LR_NRGRAM.:Z8A6TSI                               SAT 08/09/97 09:39 Page:194  
      494      623         %INCLUDE LR_PARSER_SI;
      495    20499         %INCLUDE PARSER_SUBS;
      496    20546
      497    20547    1   P_TOKEN = TOKEN;
      498    20548    1   REMEMBER LEX_FINISHED IN LEX_REMEMBER;
      499    20549    1   CALL PARSER;
      500    20550    1   LEX_FINISHED: ;
      501    20551    1   END SCANNER;
      502    20552        %EOD;

