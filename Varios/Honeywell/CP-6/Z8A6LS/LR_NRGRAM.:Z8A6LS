       10       10             NAME: RGRAM
       11       11          PURPOSE: Provide a driving routine for reading in the grammar.
       12       12        */
       13       13
       14       14
       15       15
       16       16
       17       17        RGRAM: PROC ;
       18       18
       19       19
       20       20        /*D*
       21       21               NAME: RGRAM
       22       22        DESCRIPTION: Initialize variables that will be set by calling routines.
       23       23                     Open the input file and setup the output file to M$LO.
       24       24                     Read in the options from the command line.  Pass them and
       25       25                     the system goal production to be parsed.  After parsing
       26       26                     them, the parser will get further input from the input
       27       27                     file.
       28       28
       29       29                     This module and the ones it calls are a functional
       30       30                     replacement for the original RGRAM.
       31       31
       32       32        */
       33       33        %INCLUDE LR_GRAM_EXT;
       34      572        %INCLUDE PARSER_SUBS;
       35      619
       36      620    1   DCL OUT_STR ENTRY(2);
       37      621    1   DCL FLUSH ENTRY;
PL6.E3A0      #001=RGRAM File=LR_NRGRAM.:Z8A6TSI                                 SAT 08/09/97 09:36 Page:2    
       38      622    1   DCL COMMAND_LINE_OPTIONS ENTRY;
       39      623    1   DCL OPEN_INPUT ENTRY;
       40      624    1   DCL HASH_HEADS(0:HASH_MOD) PTR EXT;
       41      625    1   DCL SYMSTART SBIN WORD EXT;
       42      626    1   DCL I SBIN;
       43      627        %EJECT;
PL6.E3A0      #001=RGRAM File=LR_NRGRAM.:Z8A6TSI                                 SAT 08/09/97 09:36 Page:3    
       44      628        %INCLUDE LR_PARSER_SI;
       45    21647        %EJECT;
PL6.E3A0      #001=RGRAM File=LR_NRGRAM.:Z8A6TSI                                 SAT 08/09/97 09:36 Page:4    
       46    21648    2   DO I=0 TO 25;
       47    21649    2      OPTIONS(I) = FALSE;
       48    21650    2   END;
       49    21651    1   KEY_REC = '           ';
       50    21652    1   INSTR_LISTED = TRUE;
       51    21653    1   LINELEN = 0; /* FOR OUT_STR */
       52    21654        %SELECT_LO;
       53    21657    1   EOF_FLAG = FALSE;
       54    21658    1   NR_SYMS = 0;
       55    21659    1   NR_TERM = 0;
       56    21660    1   NR_NT = 0;
       57    21661    1   NR_ONRIGHT = 0;
       58    21662    1   ERR_SEV = 0;
       59    21663    1   NR_PROD = 0;
       60    21664    2   DO I = 0 TO NR_SYMS_MAX;
       61    21665    2      SYM_PTRS(I) = ADDR(NIL);
       62    21666    2   END;
       63    21667    2   DO I = 0 TO HASH_MOD -1;
       64    21668    2      HASH_HEADS(I) = ADDR(NIL);
       65    21669    2   END;
       66    21670    1   SYMSTART = 0;
       67    21671    1   CALL OPEN_INPUT;
       68    21672    1   CALL COMMAND_LINE_OPTIONS;
       69    21673    1   INSTR_INDX = 0;
       70    21674    1   CALL INSERT(INSTRCHR,0,140 ,
       71    21675    1     '<SYSTEM_GS> ::= =BEGINNING_OF_FILE= <SYSTEM_GS> =END_OF_FILE=');
       72    21676        /* THE FIRST LINE HAS BEEN ST UP WITH A FAKE PRODUCTION */
       73    21677    1   INSTR_LEN = 140;
       74    21678    1   IF OPTIONS(LS_OPT)
       75    21679    2   THEN DO;
       76    21680             %OUTLINE(S="'============='");
       77    21686             %OUTLINE(S="'INPUT LISTING'");
       78    21692             %OUTLINE(S="'============='");
       79    21698             %OUTLINE(S="' '");
       80    21704    2   END;
       81    21705    1      CALL PARSER;
       82    21706    1   END RGRAM;
PL6.E3A0      #001=RGRAM File=LR_NRGRAM.:Z8A6TSI                                 SAT 08/09/97 09:36 Page:5    
       83    21707        %EOD;

