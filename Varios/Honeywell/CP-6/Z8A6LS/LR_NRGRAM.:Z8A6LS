      935       15        /*D*
      936       16               NAME: FORCE_LIST
      937       17            PURPOSE:
      938       18           REPRODUCE THE SOURCE LINE, EITHER BECAUSE
      939       19           THE LS OPTION IS SET, OR A DIAGNOSTIC IS TO BE
      940       20           PRINTED */
      941       21
      942       22        %INCLUDE LR_GRAM_EXT;
      943      561
      944      562    1   DCL OLINE CHAR(523);
      945      563    1   DCL I SBIN;
      946      564
      947      565
      948      566    1           IF INSTR_LISTED THEN
      949      567    1              RETURN;
      950      568    1           INSTR_LISTED=TRUE;
      951      569    1           CALL INSERT(OLINE,,,KEY_REC,SUBSTR(INSTRCHR,0,INSTR_LEN));
      952      570    1          I = LENGTHC(KEY_REC) + INSTR_LEN;
      953      571    1          CALL OUTPUT(OLINE,I);
      954      572    1           RETURN;
      955      573
      956      574    1   END FORCE_LIST;
      957      575        %EOD;
