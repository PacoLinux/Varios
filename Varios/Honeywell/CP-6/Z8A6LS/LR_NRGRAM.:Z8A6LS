      614     1034         %MACRO MAKE_LX_ITEM(TYPE=0);
      615     1035             P_TOKEN -> LX_ITEM.SYMPTR = ADDR(NIL);
      616     1036             P_TOKEN -> LX_ITEM.SYMBOL = TYPE;
      617     1037             P_TOKEN -> LX_ITEM.LINE =
      618     1038                  LEX_LX_STK.LINE(LEX_LX_TOP);
      619     1039             P_TOKEN -> LX_ITEM.COLUMN =
      620     1040                  LEX_LX_STK.COLUMN(LEX_LX_TOP);
      621     1041             SYMBOL_START = SYMSTART;
      622     1042             SYMLEN = LASTCOLUMN - SYMBOL_START + 1;
