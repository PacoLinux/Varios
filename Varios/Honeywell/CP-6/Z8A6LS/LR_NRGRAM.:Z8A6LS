      964        7
      965        8        /*D*
      966        9               NAME: LIMCHK
      967       10            PURPOSE: CHECK THE LIMITS OF A CONDITION AND
      968       11                     ISSUE DIAGNOSTIC IF APPROPRIATE.
      969       12        */
      970       13
      971       14    1   DCL SEV_P SBIN;
      972       15    1   DCL USED SBIN;
      973       16    1   DCL MAX_VAL SBIN;
      974       17    1   DCL MSG CHAR(20);
      975       18
      976       19        %INCLUDE LR_GRAM_EXT;
      977      558    1   DCL SEV SBIN;
      978      559    1   DCL ERRM CHAR(0) STATIC INIT('ERROR => %%');
      979      560    1   DCL 1 NTEXT STATIC,
      980      561    1         2 NUM1 CHAR(4),
      981      562    1         2 *    CHAR(0) INIT(' of '),
      982      563    1         2 NUM2 CHAR(4),
      983      564    1         2 *    CHAR(0) INIT('%%');
      984      565
      985      566    1   DCL DIAG ENTRY(6);
      986      567
      987      568    1           CALL BINCHAR(NTEXT.NUM1,USED);
      988      569    1           CALL BINCHAR(NTEXT.NUM2,MAX_VAL);
      989      570
      990      571    1           SEV=0;
      991      572    1           IF USED>MAX_VAL THEN
      992      573    2           DO;
      993      574    2              IF SEV_P=0 THEN
      994      575    2                 SEV=7;
PL6.E3A0      #008=LIMCHK File=LR_NRGRAM.:Z8A6TSI                                SAT 08/09/97 09:43 Page:377  
      995      576    2              ELSE
      996      577    2                 SEV=SEV_P;
      997      578    2              END;
      998      579
      999      580    1           IF SEV>0 THEN
     1000      581    1              CALL DIAG(SEV,ERRM,MSG,NTEXT);
     1001      582
     1002      583    1   END;
     1003      584        %EOD;

