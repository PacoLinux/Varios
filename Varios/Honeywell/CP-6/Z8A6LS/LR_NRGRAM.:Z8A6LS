      239     1178    3                     SUBSTR(P$->SYMBOL.TEXTCHR,0,P$->SYMBOL.SYMLEN) =
      240     1179    3                         SUBSTR(TEXTCHAR,0,SYMLEN)
      241     1180    4                 THEN DO; /* FOUND A PREDEFINED SYMBOL */
      242     1181    4                      FOUND = TRUE;
      243     1182    4                      LX_STK.SYMPTR(LX_INDEX) = P$;
      244     1183    4                      TARGET = P$->SYMBOL.SYM_INDX;
