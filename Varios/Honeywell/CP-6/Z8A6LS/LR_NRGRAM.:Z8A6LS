      208     1147    2    DCL TEXTCHAR CHAR(140); /* TEMP TO HOLD STRING */
      209     1148    2    DCL TEXTWORD(0:34) REDEF TEXTCHAR SBIN WORD;
      210     1149
      211     1150    2    DCL HASH_HEADS(0:HASH_MOD) PTR EXT;
      212     1151
      213     1152    2    SYMLEN = LX_STK.DNTPTR(LX_INDEX)->DNT.TEXT.LENGTH - 2; /* DON'T COUNT %% */
      214     1153    2    CALL INSERT(TEXTCHAR,,,LX_STK.DNTPTR(LX_INDEX)->DNT.TEXT.NAME);
      215     1154    2    IF SYMLEN > 1 AND
      216     1155    2       SUBSTR(TEXTCHAR,0,1) = ''''
      217     1156    3    THEN DO;
      218     1157    3         QUOTED_REP = TRUE;
      219     1158    3         CALL FIX_QUOTED_STRING(SYMLEN,TEXTCHAR);
      220     1159    3    END;
      221     1160    2    ELSE QUOTED_REP = FALSE;
      222     1161    2    HASHVAL = SYMLEN;
