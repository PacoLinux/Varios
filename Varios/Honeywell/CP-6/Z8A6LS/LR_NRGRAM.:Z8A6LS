      909      764    3           THEN CALL FORCE_LIST;
      910      765    3      END;
      911      766    2      CALL INDEX(INSTR_INDX,'/*/',
      912      767    2                 SUBSTR(INSTRCHR,0,INSTR_LEN),INSTR_INDX);
      913      768    2      IF INSTR_INDX < INSTR_LEN
      914      769    3      THEN DO;
      915      770    3           SYMSTART = INSTR_INDX;
      916      771    3           RETURN;
      917      772    3      END;
      918      773    2   END;
      919      774    1   END SCAN;
      920      775        %EOD;

