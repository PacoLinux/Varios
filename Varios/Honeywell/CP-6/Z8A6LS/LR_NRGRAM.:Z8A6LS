      164     1103    1   FIX_QUOTED_STRING: PROC(LENGTH,TEXT);
      165     1104    2   DCL LENGTH SBIN WORD;
      166     1105    2   DCL TEXT CHAR(140);
      167     1106
      168     1107    2   DCL START SBIN WORD;
      169     1108    2   DCL STOP SBIN WORD;
      170     1109    2   DCL NEWSTART SBIN WORD;
      171     1110    2   DCL NEWTEXT CHAR(140);
      172     1111
      173     1112    2   START = 1 ; /* SKIP INITIAL QUOTE */
      174     1113    2   STOP = LENGTH - 2; /* SKIP FINAL QUOTE */
      175     1114    2   NEWSTART = 0;
      176     1115
      177     1116    3   DO WHILE START <= STOP;
      178     1117    3      IF SUBSTR(TEXT,START,1) = ''''
      179     1118    3      THEN START = START + 1; /* SKIP A QUOTE */
      180     1119    3      SUBSTR(NEWTEXT,NEWSTART,1) = SUBSTR(TEXT,START,1);
      181     1120    3      NEWSTART = NEWSTART + 1;
      182     1121    3      START = START + 1;
      183     1122    3   END;
      184     1123
      185     1124    2   LENGTH = NEWSTART;
      186     1125    2   CALL INSERT(TEXT,,,SUBSTR(NEWTEXT,0,LENGTH),'%%  ');
      187     1126
      188     1127    2   END FIX_QUOTED_STRING;
