      135     6742    1   DCL LENGTH             SBIN WORD;
      136     6743
      137     6744        %DCL_NUM_DESC (@STRN = NUM_DESC);
      138     6753
      139     6754        /* Output data structure is:
      140     6755
      141     6756               DCL 1 NUM_DESC,
      142     6757                     2 DIGITS_LEFT             SBIN,
      143     6758                     2 DIGITS_RIGHT            SBIN,
      144     6759                     2 DIGITS_EXPONENT         SBIN,
      145     6760                     2 MANTISSA_SIGN_VALUE     SBIN,
