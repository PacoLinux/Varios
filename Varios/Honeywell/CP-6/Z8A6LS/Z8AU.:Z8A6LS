PL6.E3A0      #004=Z8AU$UNITS File=Z8AU.:Z8A6TSI                                 SAT 08/09/97 11:02 Page:169  
      845     8071
      846     8072
      847     8073    1      CALL Z8AU$TYPE_DESCRIPTOR (IN_ETT$, TYPE_DESC);
      848     8074
      849     8075    2      DO CASE (TYPE_DESC.TYPE);
      850     8076
      851     8077    2        CASE (%TYA, %TYE, %TYH, %TYJ, %TYK, %TYN, %TYY);
      852     8078    2          IF TYPE_DESC.DESC.ADSC.TA = %NDSC9 THEN
      853     8079    2            OUT_UNITS = %ALPHA9;
      854     8080    2          ELSE
      855     8081    2            OUT_UNITS = %ALPHA4;
      856     8082
      857     8083    2        CASE (%TYI, %TYR, %TYQ, %TYP, %TYX);
      858     8084    2          OUT_UNITS = %WORD_;
      859     8085
      860     8086    2        CASE (%TYB, %TYC);
      861     8087    2          OUT_UNITS = %ALPHA6;
      862     8088
      863     8089    2        CASE (%TYO, %TYF);
      864     8090    2          OUT_UNITS = %NUMERIC9;
      865     8091
      866     8092    2        CASE (%TYD);
      867     8093    2          IF TYPE_DESC.DESC.NDSC.TN = %NDSC9 THEN
      868     8094    2            OUT_UNITS = %NUMERIC9;
      869     8095    2          ELSE
      870     8096    2            OUT_UNITS = %NUMERIC4;
      871     8097
      872     8098    2        CASE (ELSE);
      873     8099    2          OUT_UNITS = %WORD_;
      874     8100    2          CALL Z8AE$ERROR (%E$UNEXPECTED_CASE);
      875     8101
      876     8102    2      END;/* Case */
      877     8103
      878     8104    1   END Z8AU$UNITS;/* Proc */
      879     8105        %EOD;

