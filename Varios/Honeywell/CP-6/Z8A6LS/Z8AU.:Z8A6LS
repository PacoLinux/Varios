        6        6        Z8AU$LENGTH_ETT: PROC (IN$, OUT$);
        7        7
        8        8        %EQU PHASE = 0; /* Exec module - phase */
        9        9
       10       10        %INCLUDE Z8A_DEFC;
       11       78        %INCLUDE Z8A_SUPP_MAC;
       12     2478        %INCLUDE Z8E_STACKS;
       13     4744        %INCLUDE Z8A_COMCOM;
       14     5394        %INCLUDE Z8A_MKCN;
       15     5411        %INCLUDE Z8A_TYPES;
       16     5585        %INCLUDE Z8A_ST;
       17     9244        %INCLUDE Z8A_ETT;
       18     9545        %INCLUDE Z8A_LTS;
       19    11622
       20    11623    1   DCL Z8AC$MAKE_CONST    ENTRY(1);
       21    11624    1   DCL Z8AF$FORME         ENTRY(4);
       22    11625    1   DCL Z8AU$TYPE_DESCRIPTOR ENTRY(2);
       23    11626
       24    11627    1   DCL OUT$               PTR;
       25    11628    1   DCL IN$                PTR;
       26    11629
       27    11630    1   DCL ST$$               PTR;
       28    11631    1   DCL LEN                UBIN WORD;
       29    11632    1   DCL TWO_ETT$           PTR;
       30    11633    1   DCL NEW_LEN$           PTR;
       31    11634
       32    11635    1   DCL 1 TYPE_DESC        ALIGNED,
       33    11636             %TYPE_DESC;
       34    11670
       35    11671           /* Fish to ST item or FISS or SIZ */
       36    11672
       37    11673    1      OUT$ = IN$;
PL6.E3A0      #001=Z8AU$LENGTH_ETT File=Z8AU.:Z8A6TSI                            SAT 08/09/97 10:52 Page:2    
       38    11674
       39    11675    1      CALL Z8AU$TYPE_DESCRIPTOR (IN$, TYPE_DESC);
       40    11676
       41    11677    1      IF TYPE_DESC.SIZ$ = ADDR(NIL) THEN
       42    11678    2        DO;
       43    11679    2          IF TYPE_DESC.TYPE = %TYA OR TYPE_DESC.TYPE = %TYN THEN
       44    11680    2            MKCN.CON = TYPE_DESC.DESC.ADSC.N;
       45    11681    2          ELSE
       46    11682    2            MKCN.CON = TYPE_DESC.DESC.NDSC.N;
       47    11683
       48    11684    2          CALL Z8AC$MAKE_CONST (OUT$);
       49    11685    2        END;
       50    11686    1      ELSE
       51    11687    1        OUT$ = TYPE_DESC.SIZ$;
       52    11688
       53    11689    1      RETURN;
       54    11690
       55    11691        %EJECT;
PL6.E3A0      #001=Z8AU$LENGTH_ETT File=Z8AU.:Z8A6TSI                            SAT 08/09/97 10:52 Page:3    
       56    11692    1   Z8AU$LENGTH:ENTRY(IN$, OUT$);
       57    11693
       58    11694             %INIT_LX_ETT_ITEM( @POINTER   = OUT$ ,
       59    11695                                @TYPE      = %TYI );
       60    11717
       61    11718    1        CALL Z8AU$LENGTH_ETT(IN$, OUT$ -> LX_ETT_ITEM.ETT$);
       62    11719
       63    11720    1        RETURN;
       64    11721
       65    11722
       66    11723    1   Z8AU$GET_CHAR_LENGTH: ENTRY(IN$, OUT$);
       67    11724
       68    11725
       69    11726    1        CALL Z8AU$LENGTH_ETT(IN$ -> LX_ETT_ITEM.ETT$,
       70    11727    1                             OUT$);
       71    11728
       72    11729    1        IF IN$ -> LX_ETT_ITEM.TYPE ~= %TYD THEN
       73    11730    1          RETURN;    /* Length is already in bytes */
       74    11731
       75    11732             %FISH (TRIAD = IN$ -> LX_ETT_ITEM.ETT$,
       76    11733                    RTN   = ST$$);
       77    11741
       78    11742    1        IF NOT ST$$ -> ST.NDSC.TN THEN
       79    11743    1          RETURN;    /* Length is already in bytes */
       80    11744
       81    11745               /* it is the length of a packed decimal, unit in half bytes */
       82    11746
       83    11747    1        IF OUT$ -> ETT.ID = %TRIAD THEN
       84    11748    2          DO;
       85    11749    2            LEN = OUT$ -> ETT.LOP -> CONST.CONSTI.WORD1;
       86    11750    2            MKCN.CON = (LEN + 1)/2;  /* makes half byte length to */
       87    11751                                          /* byte length               */
       88    11752    2            CALL Z8AC$MAKE_CONST(OUT$);
       89    11753
       90    11754    2            RETURN;
       91    11755    2          END;
       92    11756
PL6.E3A0      #001=Z8AU$LENGTH_ETT File=Z8AU.:Z8A6TSI                            SAT 08/09/97 10:52 Page:4    
       93    11757               /* Length is not a constant, create a new triad with    */
       94    11758               /* ADI and DVI on top of it, result triad would be      */
       95    11759               /* like                  DVI
       96    11760                                       /   \
       97    11761                                    ADI     2
       98    11762                                   /   \
       99    11763                                SIZ     1
      100    11764                            or REFMOD
      101    11765               */
      102    11766
      103    11767    1        CALL Z8AF$FORME(%ADI, OUT$, COMCOM.VALUE_1, NEW_LEN$);
      104    11768
      105    11769    1        MKCN.CON = 2;
      106    11770    1        CALL Z8AC$MAKE_CONST (TWO_ETT$);
      107    11771
      108    11772    1        CALL Z8AF$FORME (%DVI, NEW_LEN$, TWO_ETT$, OUT$);
      109    11773
      110    11774    1      RETURN;
      111    11775    1    END Z8AU$LENGTH_ETT;
      112    11776        %EOD;

