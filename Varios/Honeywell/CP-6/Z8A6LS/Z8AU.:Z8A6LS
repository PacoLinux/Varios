      147     6762                     2 COMMA_PRESENT           BIT(1) ALIGNED;
      148     6763        */
      149     6764
PL6.E3A0      #002=Z8AU$CREATE_NUM_DESC File=Z8AU.:Z8A6TSI                       SAT 08/09/97 10:55 Page:26   
      150     6765
      151     6766    1   DCL COL      SBIN;
      152     6767    1   DCL CLASS    SBIN;
      153     6768    1   DCL LAST_COL SBIN;
      154     6769
      155     6770        %MACRO SCAN_SEARCH(TABLE=);
      156     6771          CALL SEARCH(COL,
      157     6772                      CLASS,
      158     6773                      TABLE,
      159     6774                      SUBSTR(SYMBOL, 0, LENGTH),
      160     6775                      LAST_COL);
      161     6776        %MEND;
      162     6777
      163     6778
      164     6779        %MACRO CHECK_EOT;
      165     6780          DO;
      166     6781            IF COL < LENGTH THEN
      167     6782              LAST_COL = COL;
      168     6783            ELSE
      169     6784              RETURN;
      170     6785          END;
      171     6786        %MEND;
      172     6787
      173     6788
      174     6789        %MACRO ADV_COL;
      175     6790          DO;
      176     6791            COL = COL + 1;
      177     6792            %CHECK_EOT;
      178     6793            %SCAN_SEARCH(TABLE=ASCII_SEARCH_TABLE);
      179     6794          END;
      180     6795        %MEND;
      181     6796
      182     6797
      183     6798        /* Start of procedure body */
      184     6799    1   NUM_DESC.DIGITS_LEFT          = 0;
      185     6800    1   NUM_DESC.DIGITS_RIGHT         = 0;
      186     6801    1   NUM_DESC.DIGITS_EXPONENT      = 0;
PL6.E3A0      #002=Z8AU$CREATE_NUM_DESC File=Z8AU.:Z8A6TSI                       SAT 08/09/97 10:55 Page:27   
      187     6802    1   NUM_DESC.MANTISSA_SIGN_VALUE  = %NO_SIGN_VALUE;
      188     6803    1   NUM_DESC.EXPONENT_SIGN_VALUE  = %NO_SIGN_VALUE;
      189     6804    1   NUM_DESC.COMMA_PRESENT = FALSE;
      190     6805
      191     6806    1   LAST_COL = 0;
      192     6807        %SCAN_SEARCH(TABLE=ASCII_SEARCH_TABLE);
      193     6814
      194     6815        /* Get sign */
      195     6816    2   DO SELECT(CLASS);
      196     6817
      197     6818    2     SELECT(%PLUS);
      198     6819    2       NUM_DESC.MANTISSA_SIGN_VALUE = %PLUS_SIGN_VALUE;
      199     6820            %ADV_COL;
      200     6840
      201     6841    2     SELECT(%MINUS);
      202     6842    2       NUM_DESC.MANTISSA_SIGN_VALUE = %MINUS_SIGN_VALUE;
      203     6843            %ADV_COL;
      204     6863
      205     6864    2   END;
      206     6865
      207     6866        /* Get leading digits */
      208     6867    1   IF CLASS = %DIGIT THEN
      209     6868    2     DO;
      210     6869            %SCAN_SEARCH(TABLE=DIGIT_SCAN_TABLE);
      211     6876    2       NUM_DESC.DIGITS_LEFT = COL - LAST_COL;
      212     6877            %CHECK_EOT;
      213     6885    2     END;
      214     6886
      215     6887        /* Check decimal point */
      216     6888    1   IF CLASS = %PERIOD OR CLASS = %COMMA THEN
      217     6889    2     DO;
      218     6890
      219     6891    2       IF CLASS = %COMMA THEN
      220     6892    2         NUM_DESC.COMMA_PRESENT = TRUE;
      221     6893
      222     6894            %ADV_COL;
      223     6914
PL6.E3A0      #002=Z8AU$CREATE_NUM_DESC File=Z8AU.:Z8A6TSI                       SAT 08/09/97 10:55 Page:28   
      224     6915            /* Fractional digits */
      225     6916    2       IF CLASS = %DIGIT THEN
      226     6917    3         DO;
      227     6918                %SCAN_SEARCH(TABLE=DIGIT_SCAN_TABLE);
      228     6925    3           NUM_DESC.DIGITS_RIGHT = COL - LAST_COL;
      229     6926                %CHECK_EOT;
      230     6934    3         END;
      231     6935
      232     6936            /* Exponent */
      233     6937    2       IF CLASS = %LETTER_E THEN
      234     6938    3         DO;
      235     6939                %ADV_COL;
      236     6959
      237     6960                /* Sign on exponent */
      238     6961    4           DO SELECT (CLASS);
      239     6962
      240     6963    4             SELECT(%PLUS);
      241     6964    4               NUM_DESC.EXPONENT_SIGN_VALUE = %PLUS_SIGN_VALUE;
      242     6965                    %ADV_COL;
      243     6985
      244     6986    4             SELECT(%MINUS);
      245     6987    4               NUM_DESC.EXPONENT_SIGN_VALUE = %MINUS_SIGN_VALUE;
      246     6988                    %ADV_COL;
      247     7008
      248     7009    4           END;
      249     7010
      250     7011                /* Exponent digits */
      251     7012    3           IF CLASS = %DIGIT THEN
      252     7013    4             DO;
      253     7014                    %SCAN_SEARCH(TABLE=DIGIT_SCAN_TABLE);
      254     7021    4               NUM_DESC.DIGITS_EXPONENT = COL - LAST_COL;
      255     7022                    %CHECK_EOT;
      256     7030    4             END;
      257     7031
      258     7032    3         END;
      259     7033    2     END;
      260     7034
PL6.E3A0      #002=Z8AU$CREATE_NUM_DESC File=Z8AU.:Z8A6TSI                       SAT 08/09/97 10:55 Page:29   
      261     7035        /* If we got here, there was an error in the NUMERIC_LITERAL */
      262     7036    1     CALL Z8AE$ERROR (%E$INVALID_COMPILER_LITERAL,
