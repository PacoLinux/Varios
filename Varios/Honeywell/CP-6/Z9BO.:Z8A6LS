PL6.E3A0      #001=Z9BO$OPTIONS_RECOGNITION File=Z9BO.:Z8A6TSI                   SAT 08/09/97 13:41 Page:1    
        1        1        /*M* Z9BO$OPTIONS_RECOGNITION -- Parse Options Line */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7
        8        8        /*P*
        9        9             NAME: Z9BO$OPTIONS_RECOGNITION
       10       10             PURPOSE:
       11       11                  Parse the options line (in the compiler invokation line) and
       12       12                  set the option bits.
       13       13             DESCRIPTION:
       14       14                  Call the common options parser. It will call
       15       15                  Z9BO$NON_STANDARD_OPTIONS to set non-standard options.
       16       16                  The CP-6 standard options are set in this module after
       17       17                  return from the common options parser.
       18       18        */
       19       19
       20       20        /*F*
       21       21             NAME: Z9BO$OPTIONS_RECOGNITION
       22       22             PURPOSE:
       23       23                  Parse the option line and set option bits.
       24       24             DESCRIPTION:
       25       25                  Call XUX$COMMON_OPTIONS to parse the option line.
       26       26                  XUX$COMMON_OPTIONS will call Z9BO$NON_STANDARD_OPTIONS
       27       27                  to handle non_standard options.  XUX$COMMON_OPTIONS
       28       28                  uses parse nodes from Z9BG$NSTD_NODES to parse non-standard
       29       29                  options.  When XUX$COMMON_OPTIONS returns the standard
       30       30                  options are set from the XUX$STD_OPTIONS structure.
       31       31             REFERENCE:
       32       32                  Arch. file 12.2, DOC# 0751A-0
       33       33        */
       34       34        /*D*
       35       35             NAME: Z9BO$OPTIONS_RECOGNITION
       36       36             ENTRY:
       37       37                  Z9BO$OPTIONS_RECOGNITION
PL6.E3A0      #001=Z9BO$OPTIONS_RECOGNITION File=Z9BO.:Z8A6TSI                   SAT 08/09/97 13:41 Page:2    
       38       38             CALL:
       39       39                  CALL Z9BO$OPTIONS_RECOGNITION
       40       40             INTERFACE:
       41       41                  COMCOM.OB (the option bits)
       42       42             INPUT:
       43       43                  The option line from the invokation line.
       44       44             OUTPUT:
       45       45                  None.
       46       46             DESCRIPTION:
       47       47                  Call XUX$COMMON_OPTIONS to parse the option line.
       48       48                  XUX$COMMON_OPTIONS will call Z9BO$NON_STANDARD_OPTIONS
       49       49                  to handle non_standard options.  XUX$COMMON_OPTIONS
       50       50                  uses parse nodes from Z9BG$NSTD_NODES to parse non-standard
       51       51                  options.  When XUX$COMMON_OPTIONS returns the standard
       52       52                  options are set from the XUX$STD_OPTIONS structure.
       53       53        */
       54       54
       55       55        %EJECT;
PL6.E3A0      #001=Z9BO$OPTIONS_RECOGNITION File=Z9BO.:Z8A6TSI                   SAT 08/09/97 13:41 Page:3    
       56       56        Z9BO$OPTIONS_RECOGNITION:PROC;
       57       57
       58       58        %INCLUDE Z8A_SYS_FILE;
       59     6333        %INCLUDE XUX$INTERFACE_M;
       60     7110        %INCLUDE B$JIT_C.:LIBRARY;
       61     7424        %B$JIT0;
       62     7513        %U$JIT1;
       63     7587        %A$JIT6X;
       64     7590    1   DCL B$JIT$ PTR SYMREF;
       65     7591
       66     7592    1   DCL M$DO DCB;
       67     7593    1   DCL M$LO DCB;
       68     7594    1   DCL M$ME DCB;
       69     7595        %FPT_CORRES (FPTN=CORS, DCB1=M$DO, DCB2=M$LO);
       70     7609
       71     7610        %EQU PHASE = 0;
       72     7611        %INCLUDE Z8A_DEFC;
       73     7679        %INCLUDE Z8A_COMCOM;
       74     8329        %INCLUDE Z8A_ERR_NUMS;
       75     9093
       76     9094    1   DCL XUX$COMMON_OPTIONS ENTRY(1) ALTRET;
       77     9095    1   DCL Z8AE$ERROR ENTRY(12);
       78     9096    1   DCL Z9BO$NON_STANDARD_OPTION ENTRY(2);
       79     9097    1   DCL Z9AT$TERMINATOR ENTRY;
       80     9098
       81     9099    1   DCL Z9BG$NSTD_NODES UBIN WORD SYMREF;
       82     9100
       83     9101        %XUX$PARAM (USER_ROUTINE = Z9BO$NON_STANDARD_OPTION,
       84     9102                    USER_NODES   = Z9BG$NSTD_NODES,
       85     9103                    BC_UNIT      = NIL,
       86     9104                    DCB1         = M$DO,
       87     9105                    DCB2         = M$LO );
       88     9246
       89     9247        %XUX$LEGAL_OPTIONS (NSTD = YES,
       90     9248                            UREF = NO,
       91     9249                            BC   = NO);
       92     9376
PL6.E3A0      #001=Z9BO$OPTIONS_RECOGNITION File=Z9BO.:Z8A6TSI                   SAT 08/09/97 13:41 Page:4    
       93     9377        %XUX$SRCH_LIST(STCLASS=EXT);
       94     9472
       95     9473        %XUX$STD_OPTIONS;
       96     9637
       97     9638    1       COMCOM.LINE1 = B$JIT.CCBUF;
       98     9639
       99     9640
      100     9641            /* Set defaults for non-standard options */
      101     9642    1       COMCOM.OB.HEX    = %TRUE;               /*  Hex exponent  */
      102     9643
      103     9644    1       CALL XUX$COMMON_OPTIONS (XUX$PARAM)
      104     9645    2         WHENALTRETURN DO;
      105     9646    2            CALL Z9AT$TERMINATOR;
      106     9647    2         END;
      107     9648
      108     9649            /* Set standard options from results of XUX$COMMON_OPTIONS */
      109     9650    1       COMCOM.OB.DM = XUX$STD_OPTIONS.DMAP;
      110     9651    1       COMCOM.OB.MDM = XUX$STD_OPTIONS.MDMAP;
      111     9652    1       COMCOM.OB.LO = XUX$STD_OPTIONS.LO;
      112     9653    1       COMCOM.OB.LISTIN = XUX$STD_OPTIONS.LS;
      113     9654    1       COMCOM.OB.DECK = XUX$STD_OPTIONS.OU;
      114     9655    1       COMCOM.OB.PM  = XUX$STD_OPTIONS.PMAP;
      115     9656    1       COMCOM.OB.DSS  = (XUX$STD_OPTIONS.SCHEMA ~= %XUX$NONE);
      116     9657    1       COMCOM.OB.COMDK  = XUX$STD_OPTIONS.SO;
      117     9658    1       COMCOM.OB.UI = XUX$STD_OPTIONS.UI;
      118     9659    1       COMCOM.OB.OPTUI = XUX$STD_OPTIONS.OPTUI;       /*#30236*/
      119     9660    1       COMCOM.OB.NWARN = ~XUX$STD_OPTIONS.WARN;
      120     9661    1       COMCOM.OB.XREF = (XUX$STD_OPTIONS.XREF ~= %XUX$NONE);
      121     9662    1       COMCOM.OB.LNRSM = (XUX$STD_OPTIONS.UREF ~= %XUX$NONE);
      122     9663
      123     9664    2       IF COMCOM.OB.IBM AND COMCOM.OB.INITIAL THEN DO;
      124     9665    2         CALL Z8AE$ERROR (%E$IBM_INITIAL_ILLEGAL);
      125     9666    2         COMCOM.OB.INITIAL = FALSE;
      126     9667    2       END;
      127     9668
      128     9669    1       IF COMCOM.OB.PM AND NOT
      129     9670    1          (COMCOM.OB.LO.DATA OR COMCOM.OB.LISTOUT.DATA) THEN
PL6.E3A0      #001=Z9BO$OPTIONS_RECOGNITION File=Z9BO.:Z8A6TSI                   SAT 08/09/97 13:41 Page:5    
      130     9671    1         COMCOM.OB.LO.CODE = TRUE;
      131     9672
      132     9673    1       IF (COMCOM.OB.DM OR COMCOM.OB.MDM)
      133     9674    1          AND NOT (COMCOM.OB.LO.DATA OR COMCOM.OB.LISTOUT.DATA) THEN
      134     9675    1         COMCOM.OB.LO.DATA = TRUE;
      135     9676
      136     9677    1       IF COMCOM.OB.LO  OR  COMCOM.OB.LISTOUT  OR  COMCOM.OB.DM
      137     9678    1       OR COMCOM.OB.MDM OR COMCOM.OB.PM THEN
      138     9679    1         COMCOM.OB.DECK = TRUE;
      139     9680
      140     9681            /* Skip DO file if same as LO & LISTIN is specified */
      141     9682    1       CALL M$CORRES (CORS)
      142     9683    2        WHENRETURN DO;
      143     9684    2           IF B$JIT.PRFLAGS.COMMENT THEN
      144     9685    2             COMCOM.DO_REQ = '1'B;
      145     9686    2        END;
      146     9687
      147     9688    2        WHENALTRETURN DO;
      148     9689    2           IF B$JIT.PRFLAGS.COMMENT AND NOT COMCOM.OB.LISTIN THEN
      149     9690    2             COMCOM.DO_REQ = '1'B;
      150     9691    2           ELSE
      151     9692    2             COMCOM.DO_REQ = '0'B;
      152     9693    2        END;
      153     9694
      154     9695    1   END Z9BO$OPTIONS_RECOGNITION;
      155     9696        %EOD;

