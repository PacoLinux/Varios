!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI, UIACCT=*NO*
!JOB NAME=WEASEL_CRU,WSN=LOCAL
!RESOURCE TIME=15,MEM=256
!ROUTE Peter Rumsey, LADC
!PL6 WEASEL_SI61.SIACCT,WEASEL_SI61.UIACCT OVER *WEASEL_OU61.GNACCT,LP(OPTUI,SR(.GNACCT,.SIACCT,.LBACCT),MXREF)
!PL6 WEASEL_SI62.SIACCT,WEASEL_SI62.UIACCT OVER *WEASEL_OU62.GNACCT,LP(OPTUI,SR(.GNACCT,.SIACCT,.LBACCT),MXREF)
!PL6 WEASEL_SI63.SIACCT,WEASEL_SI63.UIACCT OVER *WEASEL_OU63.GNACCT,LP(OPTUI,SR(.GNACCT,.SIACCT,.LBACCT),MXREF)
!PARTRGE WEASEL_SIN1.SIACCT,WEASEL_SIN1.UIACCT OVER *WEASEL_OUN1.GNACCT,LP(OPTUI,SR(.GNACCT,.SIACCT,.LBACCT),LS,XREF)
!LINK B_COPYRIGHT.LBACCT, ;
      *WEASEL_OU61.GNACCT,;
      *WEASEL_OU62.GNACCT,;
      *WEASEL_OU63.GNACCT,;
      *WEASEL_OUN1.GNACCT ;
      OVER WEASEL.GNACCT(IP=100,DP=10)
!EXTRACT.XACCT .SIACCT,.UIACCT
DA *EXTRACT
DE
EX WEASEL_SI61
RE TY MP
DE TY MP
BU OVER *EXTRACT_H.GNACCT,WEASEL_C62.GNACCT PRO,NO,*AP2
END
!COPY *AP2 INTO ME
!COPY *EXTRACT_H.GNACCT INTO ME
!COPY *EXTRACT_H.GNACCT INTO WEASEL.GNACCT
!C WEASEL_SIH1.SIACCT TO ME(K)
