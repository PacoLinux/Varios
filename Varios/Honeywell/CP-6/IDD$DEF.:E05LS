      308     7405    4                 MHJIT=FID_BLK;
      309     7406    4               CASE(%GHOST1_CMD);
      310     7407    4                 CALL IDE$CHK_CMD(GHOST1) ALTRET(NEXT_COMMAND);
      311     7408    4                 CALL PARSE_FID(OUT$BLK.SUBLK$(0)->OUT$BLK) ALTRET(NEXT_COMMAND);
      312     7409    4                 GHOST1=FID_BLK;
