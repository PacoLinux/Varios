      329     7426    6                       END;
      330     7427    5                    END;
      331     7428    5                 IF NOT XDLTLS.FLAGS.CMD THEN DO;
      332     7429    5                    XDLTLS.ACCN=XDLT.ACCN;
      333     7430    5                    XDLTLS.RESULT.ACCN=XDLT.RESULT.ACCN;
      334     7431    5                    END;
      335     7432    4               CASE(%XDLTLS_CMD);
