      810     8010    1   ERRMSG: PROC(C#,EDCB,CPOS,FLAGLEV);
      811     8011    2   DCL C# BIT(36) UNAL;
      812     8012    2   DCL 1 CODE,
      813     8013    2         2 FCG BIT(18),
      814     8014    2         2 N UBIN(18) UNAL;
      815     8015    2   DCL EDCB SBIN;
      816     8016    2   DCL D# SBIN;
      817     8017    2   DCL CPOS SBIN;
      818     8018    2   DCL FLAGLEV SBIN;
      819     8019    2   DCL 1 ECODE STATIC,
      820     8020    2         2 FCG BIT(18) INIT('110404'O),
      821     8021    2         2 MON BIT(1) INIT('0'B),
      822     8022    2         2 CODE UBIN(14) UNAL,
      823     8023    2         2 SEV UBIN(3) UNAL INIT(0);
      824     8024        /**/
      825     8025        /**/
      826     8026        /**/
      827     8027    2           D#=DCBNUM(NIL);
      828     8028    3           IF ADDR(C#)=ADDR(NIL) THEN DO;
      829     8029    3              CODE=B$TCB$->B$TCB.ALT$->B$ALT.ERR;
      830     8030    3              D#=B$TCB$->B$TCB.ALT$->B$ALT.DCB#;
      831     8031    3              END;
      832     8032    3           ELSE DO;
      833     8033    3              CODE=C#;
      834     8034    3              END;
      835     8035    2           IF ADDR(EDCB)~=ADDR(NIL) THEN D#=EDCB;
      836     8036    3           IF ADDR(CPOS)~=ADDR(NIL) THEN DO;
      837     8037    3              CALL XUR$ERRPTR(CPOS,DCBNUM(M$DO));
      838     8038    3              END;
      839     8039    3           IF CODE.FCG THEN DO;
      840     8040    3              CALL XUR$ERRMSG(CODE,D#,,,,,FLAGLEV);
      841     8041    3              END;
      842     8042    3           ELSE DO;
      843     8043    3              ECODE.CODE=CODE.N;
      844     8044    3              CALL XUR$ERRMSG(ECODE,D#,,,,,FLAGLEV);
      845     8045    3              END;
      846     8046    2           FPT_EXIT.V.STEPCC# = 4;
PL6.E3A0      #001=IDD$DEF File=IDD$DEF.:E05TSI                                  TUE 07/29/97 22:29 Page:29   
      847     8047    2           RETURN;
      848     8048    2   END ERRMSG;
      849     8049    1   END IDD$DEF;
