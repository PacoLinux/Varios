      714     7914    5                    TYPE2=M$INPUT$->F$DCB.TYPE;
      715     7915    5                    IF TYPE1A(0)='?' THEN TYPE2A(0)='?';
      716     7916    5                    IF TYPE1A(1)='?' THEN TYPE2A(1)='?';
      717     7917    5                    IF TYPE1=TYPE2 THEN
      718     7918    5                       GOTO CHK_IGNORE;
      719     7919    5                    END /* DO I */;
      720     7920    4                 GOTO NEXT_FILE;
      721     7921    4   CHK_IGNORE:
      722     7922    4                 IGNR$=IGNRHEAD$;
      723     7923    5                 DO WHILE (IGNR$~=ADDR(NIL));
      724     7924    6                    IF M$INPUT$->F$DCB.NAME=IGNR$->INCLSPEC.FNAME THEN DO;
      725     7925    6                       IF M$INPUT$->F$DCB.ACCT=IGNR$->INCLSPEC.ACCN OR ~IGNR$->INCLSPEC
              7925                                .RESULT.ACCN THEN
      726     7926    6                          GOTO NEXT_FILE;
      727     7927    6                       END /* DO IF NAME */;
      728     7928    5                    IGNR$=IGNR$->INCLSPEC.FL$;
PL6.E3A0      #001=IDD$DEF File=IDD$DEF.:E05TSI                                  TUE 07/29/97 22:29 Page:24   
      729     7929    5                    END /* DO WHILE IGNR$ */;
      730     7930    4                 CALL IDC$COPY(DCBNUM(M$INPUT),VECTOR(FPARAM));
      731     7931    4                 IF NOT ID_VERIFLG THEN CALL IDE$FILE(DCBNUM(M$INPUT));
      732     7932    4   NEXT_FILE:
      733     7933    4                 END /* DO FOREVER */;
      734     7934    3   NXT_ACCT:
      735     7935    3              ACCT$=ACCT$->ACCNSPEC.FL$;
      736     7936    3              CALL IDE$FLUSH;
      737     7937    3              END /* DO WHILE ACCT$ */;
      738     7938    2   END PROCESS_ACCOUNT_SPECS;
