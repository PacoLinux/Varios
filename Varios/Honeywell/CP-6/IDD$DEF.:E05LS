      314     7411    5                    G1HJIT.ACCN=GHOST1.ACCN;
      315     7412    5                    G1HJIT.RESULT.ACCN=GHOST1.RESULT.ACCN;
      316     7413    5                    END;
      317     7414    4               CASE(%G1HJIT_CMD);
      318     7415    4                 CALL IDE$CHK_CMD(G1HJIT) ALTRET(NEXT_COMMAND);
      319     7416    4                 CALL PARSE_FID(OUT$BLK.SUBLK$(0)->OUT$BLK) ALTRET(NEXT_COMMAND);
      320     7417    4                 G1HJIT=FID_BLK;
