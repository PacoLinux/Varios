      760     7960    5                    FID_BLK.SNX.CNT=1;
      761     7961    5                    FID_BLK.SNX.TXT(0)=SRCH$->SRCHSPEC.SN;
      762     7962    5                    FID_BLK.RESULT.SNX='1'B;
      763     7963    5                    FID_BLK.ASNX=SRCH$->SRCHSPEC.ASN;
      764     7964    5                    CALL IDO$OPENFILE(FID_BLK,DCBNUM(M$INPUT),ERRCODE,VECTOR(FPARAM))
              7964                             ALTRET(NEXT_SRCH_ACCT);
      765     7965    5                    GOTO FILE_OPEN;
      766     7966    5   NEXT_SRCH_ACCT:
      767     7967    5                    IF ERRCODE.ERR ~= %E$NOFILE THEN
      768     7968    5                       CALL ERRMSG (ERRCODE, DCBNUM(M$INPUT));
      769     7969    5                    SRCH$=SRCH$->SRCHSPEC.FL$;
      770     7970    5                    END /* DO WHILE SRCH$ */;
      771     7971    4                 GOTO NEXT_INCL_FILE;
      772     7972    4                 END /* ELSE DO */;
      773     7973    3   FILE_OPEN:
PL6.E3A0      #001=IDD$DEF File=IDD$DEF.:E05TSI                                  TUE 07/29/97 22:29 Page:26   
      774     7974    3              CALL IDC$COPY(DCBNUM(M$INPUT),VECTOR(FPARAM),INCL$->INCLSPEC.FNAME);
      775     7975    3              IF NOT ID_VERIFLG THEN CALL IDE$FILEACCT(DCBNUM(M$INPUT));
      776     7976    3              INCL$->INCLSPEC.RESULT.FOUND='1'B;
      777     7977    3   NEXT_INCL_FILE:
      778     7978    3              INCL$=INCL$->INCLSPEC.FL$;
      779     7979    3              END /* DO WHILE INCL$ */;
      780     7980    2           CALL IDE$FLUSH;
      781     7981    2   END PROCESS_INCLUDE_SPECS;
