      262     7359    1   READ_DEF_COMMANDS: PROC;
      263     7360    2           CALL IDD$GETSEG(CMDSEG,1024);
      264     7361    2           CMDSEG$=CMDSEG.PTR$;
      265     7362    2           CMDSEGSZ=(CMDSEG.BOUND+1)/4;
      266     7363    2           END_FLG='0'B;
      267     7364    2           NOFILES_FLG='0'B;
      268     7365    2           LABEL_FLG='0'B;
      269     7366    2           BOOT_FLG='1'B;
      270     7367    2           ID_VERIFLG='0'B;
      271     7368    2           ID_LISTFLG='1'B;
      272     7369    3           DO WHILE(~END_FLG);
      273     7370    3              CALL XUR$GETCMD(ID_NODES,OUT$,VECTOR(PRMPT)) ALTRET(PARSE_ERR);
      274     7371    4              DO CASE(OUT$BLK.CODE);
      275     7372    4               CASE(%NULL_CMD);
      276     7373    4               CASE(%PO_CMD);
      277     7374    4                 IF POCMD_FLG THEN
      278     7375    4                    CALL ERRMSG(%E$DUPERR,DCBNUM(NIL));
      279     7376    5                 ELSE DO I=0 TO OUT$BLK.NSUBLKS-1;
      280     7377    5                    IF OUT$BLK.SUBLK$(I)->OUT$BLK.CODE=%NO_FILES_OPT THEN
      281     7378    5                       NOFILES_FLG='1'B;
      282     7379    6                    ELSE DO;
      283     7380    6                       ID_SITEINFO.VERSION#=OUT$BLK.SUBLK$(I)->SYM$BLK.TEXT;
      284     7381    6                       POCMD_FLG='1'B;
      285     7382    6                       END;
      286     7383    5                    END;
      287     7384    4               CASE(%BOOT_CMD);
      288     7385    4                 CALL IDE$CHK_CMD(BOOTSTRAP) ALTRET(NEXT_COMMAND);
      289     7386    4                 CALL PARSE_FID(OUT$BLK.SUBLK$(0)->OUT$BLK) ALTRET(NEXT_COMMAND);
      290     7387    4                 BOOTSTRAP=FID_BLK;
      291     7388    4               CASE(%FIRM_CMD);
      292     7389    4                 CALL IDE$CHK_CMD(FIRMWARE) ALTRET(NEXT_COMMAND);
      293     7390    4                 CALL PARSE_FID(OUT$BLK.SUBLK$(0)->OUT$BLK) ALTRET(NEXT_COMMAND);
      294     7391    4                 FIRMWARE=FID_BLK;
      295     7392    4               CASE(%ASCHEMA_CMD);
      296     7393    4                 CALL IDE$CHK_CMD(ASCHEMA) ALTRET(NEXT_COMMAND);
      297     7394    4                 CALL PARSE_FID(OUT$BLK.SUBLK$(0)->OUT$BLK) ALTRET(NEXT_COMMAND);
      298     7395    4                 ASCHEMA=FID_BLK;
PL6.E3A0      #001=IDD$DEF File=IDD$DEF.:E05TSI                                  TUE 07/29/97 22:29 Page:10   
      299     7396    4                 ASCHEMA_FLG='1'B;
      300     7397    4                 ASCHEMA_TYP=OUT$BLK.SUBLK$(1)->OUT$BLK.CODE;
      301     7398    4               CASE(%MON_CMD);
      302     7399    4                 CALL IDE$CHK_CMD(MONSTER) ALTRET(NEXT_COMMAND);
      303     7400    4                 CALL PARSE_FID(OUT$BLK.SUBLK$(0)->OUT$BLK) ALTRET(NEXT_COMMAND);
      304     7401    4                 MONSTER=FID_BLK;
