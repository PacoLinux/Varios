PL6.E3A0      #001=IDD$DEF File=IDD$DEF.:E05TSI                                  TUE 07/29/97 22:29 Page:27   
      783     7983    1   WRITE_MISSING_STUFF: PROC;
      784     7984    2   DCL INCL_NOT_FOUND CHAR(0) STATIC INIT('%2/ INCLUDE FILES NOT FOUND:%/');
      785     7985    2   DCL REQ_NOT_FOUND CHAR(0) STATIC INIT('%2/ REQUIRED FILES NOT FOUND:%/');
      786     7986    2           CALL X$WRITE(F_FDS,VECTOR(INCL_NOT_FOUND));
      787     7987        /**/
      788     7988    2           INCL$=INCLHEAD$;
      789     7989    3           DO WHILE(INCL$~=ADDR(NIL));
      790     7990    4              IF NOT INCL$->INCLSPEC.RESULT.FOUND THEN DO;
      791     7991    4                 I=INCL$->INCLSPEC.FNAME.CNT;
      792     7992    4                 CALL IDE$WRTCOLUMN(INCL$->INCLSPEC.FNAME.TXT,I,16);
      793     7993    4                 END;
      794     7994    3              INCL$=INCL$->INCLSPEC.FL$;
      795     7995    3              END /* DO WHILE INCL$ */;
      796     7996    2           CALL IDE$FLUSH;
      797     7997    3           IF BOOT_FLG THEN DO;
      798     7998    3              CALL X$WRITE(F_FDS,VECTOR(REQ_NOT_FOUND));
      799     7999    4              DO I=1 TO REQUIRED.NUMBER;
      800     8000    5                 IF NOT REQUIRED.PROCESS.FOUND(I) AND REQUIRED.PROCESS.NAME(I)~=' '
              8000                          THEN DO;
      801     8001    5                    CALL INDEX(J,' ',REQUIRED.PROCESS.NAME(I));
      802     8002    5                    CALL IDE$WRTCOLUMN(REQUIRED.PROCESS.NAME(I),J,16);
      803     8003    5                    END;
      804     8004    4                 END;
      805     8005    3              CALL IDE$FLUSH;
      806     8006    3              END;
      807     8007    2           RETURN;
      808     8008    2   END WRITE_MISSING_STUFF;
