      342     7439    5                    IF ID_PTCHHEAD$=ADDR(NIL) THEN ID_PTCHHEAD$=CMDSEG$;
      343     7440    5                    IF ID_PTCHTAIL$~=ADDR(NIL) THEN
      344     7441    5                       ID_PTCHTAIL$->INCLSPEC.FL$=CMDSEG$;
      345     7442    5                    ID_PTCHTAIL$=CMDSEG$;
      346     7443    5                    CMDSEG$=PINCRW(CMDSEG$,SIZEW(INCLSPEC));
