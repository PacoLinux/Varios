      705     7905    3              IF NOT ID_VERIFLG THEN
      706     7906    3                 CALL X$WRITE(F_FDS,VECTOR(ACCTHDR),VECTOR(ACCT$->ACCNSPEC.ACCN));
      707     7907    3              CALL IDO$SETFIRST(DCBNUM(M$INPUT),ACCT$->ACCNSPEC);
