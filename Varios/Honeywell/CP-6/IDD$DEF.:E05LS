              7948                          (FPARAM))
      749     7949    4                 WHENALTRETURN DO;
      750     7950    4                    IF ERRCODE.ERR ~= %E$NOFILE THEN CALL ERRMSG (ERRCODE, DCBNUM(
              7950                             M$INPUT));
      751     7951    4                    GOTO NEXT_INCL_FILE;
      752     7952    4                    END;
      753     7953    4              ELSE DO;
      754     7954    4                 SRCH$=SRCHHEAD$;
      755     7955    4                 MOVESIZE=SIZEC(FID_BLK);
      756     7956    4                 ADDR(FID_BLK)->MOVEBASE=ADDR(INCL$->INCLSPEC.FNAME)->MOVEBASE;
