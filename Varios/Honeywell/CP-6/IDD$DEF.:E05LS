      693     7893    1   PROCESS_ACCOUNT_SPECS: PROC;
      694     7894        /**/
      695     7895    2   DCL ACCT$ PTR;
      696     7896    2   DCL IGNR$ PTR;
      697     7897    2   DCL TYPE1 CHAR(2);
      698     7898    2   DCL TYPE1A(0:1) REDEF TYPE1 CHAR(1);
      699     7899    2   DCL TYPE2 CHAR(2);
      700     7900    2   DCL TYPE2A(0:1) REDEF TYPE2 CHAR(1);
