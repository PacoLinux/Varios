      120     5741    5                            TEMP2B.B1 = TEMP1B.B1;
      121     5742    5                            TEMP2B.B2 = TEMP1B.B2;
      122     5743    5                            TEMP2B.B3 = TEMP1B.B3;
      123     5744    5                            TEMP2B.B4 = TEMP1B.B4;
      124     5745    5                            S_D.JDE = TEMP2;
      125     5746    5                          END;
