      131     5752    3                  THEN UNWIND TO S_D.DO_MESSAGE;
      132     5753    3                  REMEMBER NEVER IN S_D.SCREECH_IC;
      133     5754    3                  CALL M$SCREECH (SNAP_MESSAGE) ALTRET(NEVER);
      134     5755    3   NEVER:
      135     5756    3                  S_D.ERROR = E@BADMESSAGE; /* E@BADMESSAGE */
