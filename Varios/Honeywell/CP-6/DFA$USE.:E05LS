      101     5722    1        CALL DFQ$READ_FD(S_D.CURRENT_FPRG_CONTEXT$, '0'B) ALTRET(ERR);
      102     5723    1        IF GD$MSG.FNC ~= %GD#RSPACK
      103     5724    2        THEN DO;
      104     5725    3             DO CASE(GD$MSG.FNC);
      105     5726    3               CASE(%GD#RSPBADADR);
      106     5727    3                  S_D.ERROR = E@BADADDR;   /* E@BADADDR */
