       96     1919    1           STATION = B$CGAURD.STATION;
       97     1920    1           CALL QQC$FINDSTA(STATION, S$, FOUND_FLAG);
       98     1921
       99     1922    1           IF FOUND_FLAG THEN
      100     1923    2             DO;
      101     1924    2               IF QQ_GLOBAL.HELP_S$ = S$ THEN
      102     1925    2                 QQ_GLOBAL.HELP_S$ = ADDR(NIL);
      103     1926    2               IF TP$STATION.FIRST_CMD$ ~= ADDR(NIL) THEN
      104     1927    3                 DO;
      105     1928    3                   CALL QQC$RLS_BLK(QQ_GLOBAL.POOL$, TP$STATION.FIRST_CMD$);
      106     1929    3                 END;
      107     1930
      108     1931
      109     1932    2               IF TP$STATION.FPLDTRC$ ~= ADDR(NIL) THEN
      110     1933    3                 DO;
      111     1934    3                   CALL QQC$RLS_BLK(QQ_GLOBAL.POOL$, TP$STATION.FPLDTRC$);
      112     1935    3                 END;
      113     1936
      114     1937    2               IF TP$STATION.QT$FORM$ ~= ADDR(NIL) THEN
      115     1938    3                 DO;
      116     1939    3                   CALL QQC$RLS_BLK(QQ_GLOBAL.POOL$, TP$STATION.QT$FORM$);
      117     1940    3                 END;
      118     1941
      119     1942    2               IF NOT TP$STATION.FLAGS.DISCONNECT THEN
      120     1943    3                 DO;
      121     1944                                           /* Don't send a msg to me if I'm an MCT    */
      122     1945    3                   CALL M$DEACTIVATE(FPT_DEACTIVATE) ALTRET(IGNOR1);
      123     1946    3   IGNOR1:         ;
      124     1947    3                   TP$STATION.FLAGS.MCT = '0'B;
      125     1948    3                   CALL QQC$BLDMSG_ERRBLK(MSGBLK);
      126     1949    3                   MSGBLK.ERRINFO.ERR(0) = CODE_UDOS;
PL6.E3A0      #001=QQA$STA_DIS File=QQA$STA_DIS.:E05TSI                          WED 07/30/97 05:27 Page:8    
      127     1950    3                   TEXTC_STATION.NAME# = TP$STATION.ID;
      128     1951    3                   TEXTC_STATION.L# = 8;
      129     1952    3                   MSGBLK.ERRINFO.FIELD1(0) = VECTOR(TEXTC_STATION);
      130     1953    3                   TEXTC_LINE.NAME# = TP$STATION.LINE_INFO;
      131     1954    3                   CALL INDEX (CHAR_STRING_SIZE, ' ', TP$STATION.LINE_INFO);
      132     1955    3                   TEXTC_LINE.L# = CHAR_STRING_SIZE;
      133     1956    3                   MSGBLK.ERRINFO.FIELD2(0) = VECTOR(TEXTC_LINE);
      134     1957    3                   CALL QQC$BLDMSG(MSGBLK);
      135     1958    3                 END;
      136     1959
      137     1960    2               P$ = PINCRW(QQ_GLOBAL.STATION$, QQ_GLOBAL.STATION_CNT * SIZEW(TP$STATION
              1960                        ));
      138     1961    2               TEMP_PTR$ = PINCRW(S$, SIZEW(TP$STATION));
      139     1962    2               CHAR_STRING_SIZE = POFFC(P$, TEMP_PTR$);
      140     1963    2               S$ -> CHAR$STRING = TEMP_PTR$ -> CHAR$STRING;
      141     1964    2               QQ_GLOBAL.STATION_CNT = QQ_GLOBAL.STATION_CNT - 1;
      142     1965    2               QQ_GLOBAL.STATIONSLOTS = QQ_GLOBAL.STATIONSLOTS + 1;
      143     1966
      144     1967    2               SLOTS_PER_PAGE = 1024 / SIZEW(TP$STATION);
      145     1968    2               IF QQ_GLOBAL.STATIONSLOTS - SLOTS_PER_PAGE > 5 THEN
      146     1969    3                 DO;
      147     1970    3                   VLP_VECTOR.PTR$ = QQ_GLOBAL.STATION$;
      148     1971    3                   CALL M$FDS(FPT_FDS) ALTRET(MON_ERR);
      149     1972    3                   QQ_GLOBAL.STATIONSLOTS = QQ_GLOBAL.STATIONSLOTS - SLOTS_PER_PAGE;
      150     1973    3                 END;
      151     1974    2             END;
      152     1975
      153     1976    1           RETURN;
      154     1977
      155     1978    1   MON_ERR:
      156     1979    1           QQ_GLOBAL.ERR = B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR;
      157     1980    1           CALL QQC$SCREECH;
      158     1981    1   END QQA$STA_DIS;

