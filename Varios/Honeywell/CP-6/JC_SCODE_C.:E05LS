    9      %EQU S$NOSLUG=250/*NOSLUG*/;            /*M$LDTRC FROM GHOST1 TO SLUG FAILED*/
   10      %EQU S$BDCVM=251/*BDCVM*/;    /*CANT M$CVM ONTO STOLEN PAGES FOR INITIAL USER*/
   11      %EQU S$NOMAKU=252/*NOMAKU*/;            /*ALTRET FROM M$MAKUSER PMME*/
   12      %EQU S$NGUSR=253/*NGUSR*/;         /*CANT GET USER SLOTS FOR SYSTEM GHOSTS*/
   13      %EQU S$NGPP=254/*NGPP*/;      /*CANT GET PHYSICAL PAGES TO START SYSTEM GHOSTS*/
   14      %EQU S$NOPHLP=255/*NOPHLP*/;            /*CANT OPEN THE HLPCG COMGROUP*/
   15      /* CODE 256 IS CURRENTLY UNUSED */
   16      %EQU S$NRDHLP=257/*NRDHLP*/;            /*CANT READ HLPCG COMGROUP*/
   17      /* CODE 258 IS CURRENTLY UNUSED */
   18      %EQU S$NOPCS=259/*NOPCS*/;    /*CANT OPEN TO CENTRAL SYSTEM COMGROUP (CSCG)*/
   19      %EQU S$NOPACCT=260/*NOPACCT*/;          /* CANT OPEN :ACCTLG                  */
