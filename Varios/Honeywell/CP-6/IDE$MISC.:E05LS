        5        5        /*T***********************************************************/
        6        6        /* IDE$MISC - MISCELLANEOUS ROUTINES FOR DEF */
        7        7        IDE$WRTCOLUMN: PROC(TEXT,TSIZ,CLMN);
        8        8        /**/
        9        9    1   DCL TEXT CHAR(TSIZ);
       10       10    1   DCL TSIZ SBIN;
       11       11    1   DCL CLMN SBIN;
       12       12        /**/
       13       13    1   DCL M$LO DCB;
       14       14    1   DCL M$LO$ PTR;
       15       15        %INCLUDE CP_6;
       16       96        %INCLUDE CP_6_SUBS;
       17      636        %FPT_WRITE (DCB=M$LO,VFC=YES,WAIT=YES);
       18      667    1   DCL CARRIER SBIN STATIC INIT(1);
       19      668    1   DCL BUF(0:132) CHAR(1) STATIC;
       20      669    1   DCL BUFC REDEF BUF CHAR(133);
       21      670    1   DCL MOVEBASE CHAR(TMP) BASED;
       22      671    1   DCL TMP SBIN;
       23      672    1   DCL WIDTH SBIN;
       24      673        %INCLUDE F$DCB;
       25      722        /**/
       26      723    1           TMP=TSIZ+CLMN-MOD(TSIZ,CLMN);
       27      724    1           M$LO$=DCBADDR(DCBNUM(M$LO));
       28      725    1           IF M$LO$->F$DCB.WIDTH=0 THEN WIDTH=72;
       29      726    1           ELSE WIDTH=M$LO$->F$DCB.WIDTH;
       30      727    1           IF WIDTH>132 THEN WIDTH=132;
       31      728    1           IF CARRIER+TSIZ>WIDTH THEN CALL IDE$FLUSH;
       32      729    1           ADDR(BUF(CARRIER))->MOVEBASE=TEXT;
       33      730    1           CARRIER=CARRIER+TMP;
       34      731    1           IF CARRIER>WIDTH THEN CARRIER=WIDTH;
       35      732    1           RETURN;
       36      733        /**/
       37      734    1   IDE$FLUSH: ENTRY;
PL6.E3A0      #001=IDE$WRTCOLUMN File=IDE$MISC.:E05TSI                           TUE 07/29/97 22:29 Page:2    
       38      735    1           IF CARRIER=1 THEN RETURN;
       39      736    1           BUF(0)='A';
       40      737    1           FPT_WRITE.BUF_=VECTOR(BUF);
       41      738    1           FPT_WRITE.BUF_.BOUND=CARRIER-1;
       42      739    1           CALL M$WRITE(FPT_WRITE);
       43      740    1           CARRIER=1;
       44      741    1           RETURN;
       45      742    1   END IDE$WRTCOLUMN;
       46      743        %EOD;

