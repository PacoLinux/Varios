      117        6        IDE$FMSECON: PROC NOAUTO;
      118        7        /**/
      119        8    1   DCL B$JIT$ PTR SYMREF;
      120        9        %INCLUDE B$JIT;
      121      299        %INCLUDE CP_6;
      122      380        %FPT_EXIT (STCLASS=EXT);
      123      395        %FPT_PRIV (PRIV=%PR_FMSEC#);
      124      410        %VLR_SITEINFO;
      125      429        %FPT_MONINFO(SITEINFO=VLR_SITEINFO);
      126      444        %INCLUDE ID_PERR_C;
      127      486    1   DCL 1 NOFMSEC ALIGNED CONSTANT,
      128      487    1         2 FCG BIT(18) UNAL INIT('110405'O),
      129      488    1         2 CODE UBIN(15) UNAL INIT(%E$NOFMSEC),
      130      489    1         2 SEV UBIN(3) UNAL INIT(0);
      131      490    1   DCL XUR$ERRMSG ENTRY(7) ALTRET;
      132      491        %EJECT;
PL6.E3A0      #004=IDE$FMSECON File=IDE$MISC.:E05TSI                             TUE 07/29/97 22:30 Page:42   
      133      492    1           IF B$JIT.PRIV.ACTIVE & %PR_FMSEC# THEN RETURN;
      134      493    1           CALL M$SPRIV(FPT_PRIV) ALTRET(FMSEC_ERR);
      135      494    1           RETURN;
      136      495    1   FMSEC_ERR:
      137      496    1         FPT_EXIT.V.STEPCC# = 4;
      138      497    1           CALL XUR$ERRMSG(NOFMSEC);
      139      498        /*E*    ERROR:   IDE-E$NOFMSEC
      140      499                MESSAGE: DEF cannot get FMSEC privilege.  DEF aborting.
      141      500                DESCRIPTION: DEF must either be run from an account with FMSEC
      142      501                             privilege or must reside in :SYS and be linked with
      143      502                             FMSEC.                                        */
      144      503    1   IDE$FMSECOFF: ENTRY;
      145      504    1           IF B$JIT.PRIV.JOB & %PR_FMSEC# THEN RETURN;
      146      505    1           IF NOT B$JIT.PRIV.ACTIVE & %PR_FMSEC# THEN RETURN;
      147      506        /* The following per JJ -  to allow ?HOST accts to DEF from :SYS; */
      148      507        /* the M$MONINFO pmme is done here to localize the code.          */
      149      508    1         CALL M$MONINFO (FPT_MONINFO);
      150      509    1         IF SUBSTR(VLR_SITEINFO.SITE_NAME#.C,0,9)='LADC L66B' AND
      151      510    1            SUBSTR(B$JIT.ACCN,3,4)='HOST' THEN RETURN;
      152      511        /* End of special code */
      153      512    1           CALL M$RPRIV(FPT_PRIV);
      154      513    1           RETURN;
      155      514    1   END IDE$FMSECON;

