       52        6        IDE$FILE: PROC(DCB#);
       53        7        /**/
       54        8    1   DCL DCB# SBIN;
       55        9        /**/
       56       10    1   DCL BUF(0:50) CHAR(1);
       57       11    1   DCL BUFC REDEF BUF CHAR(51);
       58       12    1   DCL TMP SBIN;
       59       13    1   DCL ACCTBASE CHAR(8) BASED;
       60       14    1   DCL IDE$WRTCOLUMN ENTRY(3);
       61       15        %INCLUDE F$DCB;
       62       64        /**/
       63       65    1           TMP=DCBADDR(DCB#)->F$DCB.NAME.L;
       64       66    1           BUFC=DCBADDR(DCB#)->F$DCB.NAME.C;
       65       67    1           CALL IDE$WRTCOLUMN(BUF,TMP,12);
       66       68    1           RETURN;
       67       69        /**/
       68       70    1   IDE$FILEACCT: ENTRY(DCB#);
       69       71    1           BUFC=DCBADDR(DCB#)->F$DCB.NAME.C;
       70       72    1           TMP=DCBADDR(DCB#)->F$DCB.NAME.L;
       71       73    1           BUF(TMP)='.';
       72       74    1           ADDR(BUF(TMP+1))->ACCTBASE=DCBADDR(DCB#)->F$DCB.ACCT;
       73       75    1           CALL INDEX(TMP,' ',BUFC);
       74       76    1           CALL IDE$WRTCOLUMN(BUF,TMP,20);
       75       77    1           RETURN;
       76       78    1   END IDE$FILE;
       77       79        %EOD;

