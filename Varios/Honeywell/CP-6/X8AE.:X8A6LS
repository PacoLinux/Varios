        7        7        X8AE$LONG_EDITED_MOVE:  PROC (SEND_FLD, REC_FLD, PIC_STR, DSC_BLK);
        8        8        /*P*
        9        9             NAME: X8AE$LONG_EDITED_MOVE
       10       10             PURPOSE:
       11       11                   Perform long edited moves of length > 63 charaters.
       12       12             DESCRIPTION:
       13       13                  None.
       14       14        */
       15       15        %EJECT;
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:2    
       16       16        /*D*
       17       17             NAME: X8AE$LONG_EDITED_MOVE
       18       18             CALL:
       19       19                  unknown
       20       20             PARAMETERS:
       21       21                    SEND_FLD    --  Sending field.
       22       22                    REC_FLD     --  Receiving field.
       23       23                    PIC_STR     --  Picture string.
       24       24                    DSC_BLK     --  Descriptor block containing the lengths of the
       25       25                                    sending field, picture string and the receiving
       26       26                                    field.
       27       27             INTERFACE:
       28       28                    LARGE_MOVE  --  Performs multiple character moves.
       29       29                    X9OS        --  Operator segment (calling routine).
       30       30             ENVIRONMENT:
       31       31                  None.
       32       32             INPUT:
       33       33                  None.
       34       34             OUTPUT:
       35       35                    REC_FLD  --  Receiving field.
       36       36             SCRATCH:
       37       37                  None.
       38       38             DESCRIPTION:
       39       39                    This routine performs long edited moves which exceed the
       40       40                  hardware limit of 63.  The routine is driven by the picture
       41       41                  string.  Each character of the picture string is peeled off and
       42       42                  the appropriate action is performed for that character.  Slashes (/)
       43       43                  and blanks (B) are placed directly into the receiving field at the
       44       44                  current index (REC_NDX) position.  A's, X's, and 9's cause one
       45       45                  character from the sending field to be moved into the receiving
       46       46                  field.  ('s are the start of a multiple character move.  The
       47       47                  number enclosed in the parathesis is removed and used as the
       48       48                  length of the move.  If the receiving field is larger than the
       49       49                  sending field, then the receiving field is filled with blanks.
       50       50                  If the reverse is true, then the sending field is truncated
       51       51                  on the right.
       52       52        */
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:3    
       53       53
       54       54            %INCLUDE X8A_LIBCOM;
       55      274
       56      275
       57      276    1       DCL 1 DSC_BLK           ALIGNED,
       58      277    1             2 SEND_LEN        UBIN WORD UNAL,
       59      278    1             2 REC_LEN         UBIN WORD UNAL,
       60      279    1             2 PIC_LEN         UBIN WORD UNAL;
       61      280    1       DCL SEND_FLD            CHAR( DSC_BLK.SEND_LEN );
       62      281    1       DCL REC_FLD             CHAR( DSC_BLK.REC_LEN  );
       63      282    1       DCL PIC_STR             CHAR( DSC_BLK.PIC_LEN  );
       64      283    1       DCL DIGIT               CHAR(1);
       65      284    1       DCL SEND_NDX            SBIN WORD;
       66      285    1       DCL REC_NDX             SBIN WORD;
       67      286    1       DCL PIC_NDX             SBIN WORD;
       68      287    1       DCL BLANK_FILL          BIT(1);
       69      288
       70      289        %EJECT;
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:4    
       71      290    1       BLANK_FILL = %FALSE;
       72      291    1       SEND_NDX = 0;
       73      292    1       REC_NDX  = 0;
       74      293    1       PIC_NDX  = 0;
       75      294
       76      295    2       DO WHILE (PIC_NDX <= DSC_BLK.PIC_LEN );
       77      296    2         DIGIT = SUBSTR (PIC_STR, PIC_NDX, 1);
       78      297    2         PIC_NDX = PIC_NDX + 1;
       79      298
       80      299    3         DO CASE ( ASCBIN( DIGIT ));
       81      300    3           CASE (%BIN_SLASH);
       82      301    3             SUBSTR( REC_FLD, REC_NDX, 1) = '/';
       83      302    3             REC_NDX = REC_NDX + 1;
       84      303
       85      304    3           CASE ( %B );
       86      305    3             SUBSTR ( REC_FLD, REC_NDX, 1) = ' ';
       87      306    3             REC_NDX = REC_NDX + 1;
       88      307
       89      308    3           CASE ( %BIN_0 );
       90      309    3             SUBSTR ( REC_FLD, REC_NDX, 1) = '0';
       91      310    3             REC_NDX = REC_NDX + 1;
       92      311
       93      312    3           CASE (%A, %X, %BIN_9);
       94      313    3             IF BLANK_FILL THEN
       95      314    3               SUBSTR(REC_FLD, REC_NDX, 1) = ' ';
       96      315    3             ELSE
       97      316    4               DO;
       98      317    4                 SUBSTR( REC_FLD, REC_NDX, 1) = SUBSTR( SEND_FLD, SEND_NDX, 1);
       99      318    4                 SEND_NDX = SEND_NDX + 1;
      100      319    4                 IF SEND_NDX > DSC_BLK.SEND_LEN THEN
      101      320    4                   BLANK_FILL = %TRUE;
      102      321    4               END;
      103      322    3             REC_NDX = REC_NDX + 1;
      104      323
      105      324    3           CASE(%BIN_L_PARA);     /*  LEFT PARANTHESIS */
      106      325    3             CALL LARGE_MOVE;
      107      326    3         END; /* CASE */
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:5    
      108      327    2       END; /* WHILE */
      109      328        %EJECT;
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:6    
      110      329    1     LARGE_MOVE:  PROC;
      111      330        /*I*
      112      331             NAME: LONG_MOVE
      113      332             PURPOSE:
      114      333
      115      334                    Performs multiple character moves.
      116      335             CALL:
      117      336                    CALL LONG_MOVE;
      118      337             PARAMETERS:
      119      338                  None
      120      339             DATA:
      121      340                    PIC_STR  -- Picture string.
      122      341                    PIC_NDX  -- Picture string index.
      123      342                    SEND_FLD -- Sending field.
      124      343                    SEND_NDX -- Sending field index.
      125      344                    REC_NDX  -- Receiving field index.
      126      345                    SEND_LEN -- Sending field length.
      127      346             DESCRIPTION:
      128      347                    First, the repeated character (REP_TYPE) and the current
      129      348                  picture string index (PIC_NDX => START) are saved.  Then the
      130      349                  repeat factor (MULT_LEN) is removed from the picture string.
      131      350                  Then, depending on the repaet character, a move action is performed.
      132      351        */
      133      352
      134      353    2       DCL I            SBIN WORD;
      135      354    2       DCL START        SBIN WORD;
      136      355    2       DCL MULT_LEN     SBIN WORD;
      137      356    2       DCL REP_TYPE     CHAR(1);
      138      357    2       DCL REMAIN_LEN   SBIN WORD;
      139      358
      140      359        %EJECT;
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:7    
      141      360    2       REP_TYPE = SUBSTR (PIC_STR, PIC_NDX-2, 1);
      142      361    2       START = PIC_NDX;
      143      362
      144      363    3       DO UNTIL ( SUBSTR( PIC_STR, PIC_NDX, 1) = ')' );
      145      364    3         PIC_NDX = PIC_NDX + 1;
      146      365    3       END; /* UNTIL */
      147      366
      148      367    2       CALL CHARBIN (MULT_LEN, SUBSTR( PIC_STR, START, PIC_NDX-START));
      149      368    2       MULT_LEN = MULT_LEN - 1;
      150      369
      151      370    3       DO CASE ( ASCBIN( REP_TYPE ));
      152      371    3         CASE(%B);
      153      372    4           DO I = REC_NDX TO REC_NDX +MULT_LEN;
      154      373    4             SUBSTR( REC_FLD, I, 1) = ' ';
      155      374    4           END; /* INDEX */
      156      375    3           REC_NDX = REC_NDX + MULT_LEN;
      157      376
      158      377    3         CASE(%BIN_0);
      159      378    4           DO I = REC_NDX TO REC_NDX +MULT_LEN;
      160      379    4             SUBSTR( REC_FLD, I, 1) = '0';
      161      380    4           END; /* INDEX */
      162      381    3           REC_NDX = REC_NDX + MULT_LEN;
      163      382
      164      383    3         CASE (%BIN_SLASH);
      165      384    4           DO I = REC_NDX TO REC_NDX +MULT_LEN;
      166      385    4             SUBSTR( REC_FLD, I, 1) = '/';
      167      386    4           END; /* INDEX */
      168      387    3           REC_NDX = REC_NDX + MULT_LEN;
      169      388
      170      389    3         CASE (%A, %X, %BIN_9);
      171      390    3           IF BLANK_FILL THEN
      172      391    3             SUBSTR( REC_FLD, REC_NDX, MULT_LEN ) = ' ';
      173      392    3          ELSE
      174      393    3            IF SEND_NDX + MULT_LEN <= DSC_BLK.SEND_LEN THEN
      175      394    4              DO;
      176      395    4                SUBSTR( REC_FLD,REC_NDX,MULT_LEN) =
      177      396    4                  SUBSTR( SEND_FLD,SEND_NDX,MULT_LEN);
PL6.E3A0      #001=X8AE$LONG_EDITED_MOVE File=X8AE.:X8A6TSI                      SAT 08/09/97 10:43 Page:8    
      178      397    4                SEND_NDX = SEND_NDX + MULT_LEN;
      179      398    4              END;
      180      399    3            ELSE
      181      400    4              DO;
      182      401    4                REMAIN_LEN = DSC_BLK.SEND_LEN - SEND_NDX;
      183      402    4                SUBSTR( REC_FLD, REC_NDX, REMAIN_LEN) =
      184      403    4                   SUBSTR( SEND_FLD, SEND_NDX, REMAIN_LEN);
      185      404    4                SUBSTR( REC_FLD, REC_NDX+REMAIN_LEN, MULT_LEN-REMAIN_LEN ) = ' ';
      186      405    4                BLANK_FILL = %TRUE;
      187      406    4              END;
      188      407
      189      408    3         REC_NDX = REC_NDX + MULT_LEN;
      190      409
      191      410    3       END; /* CASE */
      192      411
      193      412    2     END LARGE_MOVE;  /* PROC */
      194      413    1   END X8AE$LONG_EDITED_MOVE;  /* PROC */
