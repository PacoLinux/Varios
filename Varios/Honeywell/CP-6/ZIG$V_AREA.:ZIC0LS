PL6.E3A0      #001=ZIG$V_AREA File=ZIG$V_AREA.:ZIC0TSI                           FRI 09/05/97 12:16 Page:5    
       77     4662    1       CALL FIND_LENGTH(TYPE_200.AREA_NAME,L_SCHEMA_AC);
       78     4663    1       IF TYPE_200.SUB_AREA_NAME = ' '
       79     4664    1       THEN TYPE_200.SUB_AREA_NAME = TYPE_200.AREA_NAME;
       80     4665    1       CALL FIND_LENGTH(TYPE_200.SUB_AREA_NAME,L_SUBSCH_AC);
       81     4666    1       IF MOD(L_SCHEMA_AC,4) ~= 0
       82     4667    1       THEN L_SCHEMA_AC = L_SCHEMA_AC + 4 - MOD(L_SCHEMA_AC,4);
       83     4668    1       IF MOD(L_SUBSCH_AC,4) ~= 0
       84     4669    1       THEN L_SUBSCH_AC = L_SUBSCH_AC + 4 - MOD(L_SUBSCH_AC,4);
       85     4670    1       ALLOCATE_END = 0; /* Allocate a start of mem */
       86     4671    1       CALL ZIG$G_DYN_MEM_6(1,AC1$);
       87     4672    1       I = POFFW(AC1$,SSS$);
       88     4673    1       IF MOD(I,2) = 0
       89     4674    2       THEN DO;
       90     4675        /*
       91     4676              AC records must start on a odd word and at the moment it is on
       92     4677            a even word so skip a word.
       93     4678        */
       94     4679    2           CALL ZIG$G_DYN_MEM_6(1,AC1$);
       95     4680    2           END;
