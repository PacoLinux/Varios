      112     4697    2           CALL ERROR(%ZIG_V_AREA_NAME);
      113     4698        /*E*     ERROR:    ZIG-ZIG_V_AREA_NAME-C
PL6.E3A0      #001=ZIG$V_AREA File=ZIG$V_AREA.:ZIC0TSI                           FRI 09/05/97 12:16 Page:6    
      114     4699        MESSAGE: The area "%U1" is not present in the schema file.
      115     4700        */
      116     4701    2           RETURN;
      117     4702    2           END;
      118     4703    2       ELSE DO;
      119     4704    2           CURRENT_OFF = POFFW(AC$,SSS$);
      120     4705    2           STATUS = #IN_ALIAS;
      121     4706    2           ALLOCATE_END = 1;
      122     4707    2           HASH_SCHEMA_NAME = '1'B;
