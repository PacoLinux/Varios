        8        8        %SET LISTSUB='1'B;
        9        9        /*F* NAME: DFA$UNFID
       10       10             PURPOSE:
       11       11                   This module converts the data descriptor for the UNFID
       12       12                   command into L6 format
       13       13                   and writes a message to FD containing the data descriptor.
       14       14                   The data descriptor is evaluated in FD and get back
       15       15                   DCB#, DCBNAME, ORG and FUN.
       16       16             DESCRIPTION:
       17       17                   Input is data descriptor @ S.D.DESC$ */
       18       18
       19       19        DFA$UNFID: PROC ALTRET;
       20       20        /* Local Auto */
       21       21    1   DCL DCB# UBIN;
       22       22    1   DCL TEMP UBIN;
       23       23    1   DCL I UBIN;
       24       24    1   DCL HIGX UBIN;
       25       25    1   DCL LOWX UBIN;
       26       26    1   DCL DUM1 BIT(1) ALIGNED;
       27       27    1   DCL BB$ PTR;
       28       28    1   DCL REM UBIN;
       29       29    1   DCL CHAR120 CHAR(120) BASED;
       30       30    1   DCL CHAR1 CHAR(1) BASED;
       31       31    1   DCL CHAR2 CHAR(2) BASED;
       32       32    1   DCL CHAR8 CHAR(8) BASED;
       33       33    1   DCL CHAR32 CHAR(32) BASED;
       34       34    1   DCL NAME (0:30) UBIN BYTE CALIGNED;
       35       35    1   DCL NAMEC REDEF NAME CHAR(31) CALIGNED;
       36       36    1   DCL ACCT (0:7) UBIN BYTE CALIGNED;
       37       37    1   DCL ACCTC REDEF ACCT CHAR(8) CALIGNED;
PL6.E3A0      #001=DFA$UNFID File=DFA$UNFID.:E05TSI                              TUE 07/29/97 14:38 Page:2    
       38       38    1   DCL RES (0:3) UBIN BYTE CALIGNED;
       39       39    1   DCL RESC REDEF RES CHAR(4) CALIGNED;
       40       40    1   DCL COMMA BIT(1) ALIGNED;
       41       41    1   DCL TEMP$ PTR;
       42       42    1   DCL TEMP1 REDEF TEMP$ UBIN(36) ALIGNED;
       43       43    1   DCL 1 TEMP1B REDEF TEMP$ ALIGNED,
       44       44    1         2 * UBIN(4) UNAL,
       45       45    1         2 B1 UBIN(8) UNAL,
       46       46    1         2 B2 UBIN(8) UNAL,
       47       47    1         2 B3 UBIN(8) UNAL,
       48       48    1         2 B4 UBIN(8) UNAL;
       49       49    1   DCL TEMP2 UBIN(36) ALIGNED;
       50       50    1   DCL 1 TEMP2B REDEF TEMP2 ALIGNED,
       51       51    1         2 * BIT(1) UNAL,
       52       52    1         2 B1 UBIN(8) UNAL,
       53       53    1         2 * BIT(1) UNAL,
       54       54    1         2 B2 UBIN(8) UNAL,
       55       55    1         2 * BIT(1) UNAL,
       56       56    1         2 B3 UBIN(8) UNAL,
