      229     6384    3                  CALL DDR$MOVE('UR)', LENGTHC('UR)'), BB$, REM);
      230     6385    3               CASE(%G_ORG_CQ#);
      231     6386    3                  CALL DDR$MOVE('CQ)', LENGTHC('CQ)'), BB$, REM);
