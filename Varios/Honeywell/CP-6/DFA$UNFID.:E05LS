       75     6132        %FPT_WAIT (UNITS=1, STCLASS=CONSTANT);
       76     6146        %INCLUDE DU_SCODE_C;
       77     6159        %FPT_SCREECH(FPTN=SNAP_MESSAGE,STCLASS=CONSTANT,SCODE=SC_MESSAGE);
       78     6173        %VLP_SCODE(FPTN=SC_MESSAGE, STCLASS=CONSTANT, FCG=DU, MID=X, MON=NO,
       79     6174                   CODE=%S$DUMESSAGE, SEV=SEV_SNAP, CUSR=YES, ROSEG=YES,
       80     6175                   ISEG=YES, DDSEG=YES, DBSEG=YES, ASLSEG=YES);
       81     6236        /* Entry DCLs */
       82     6237    1   DCL DDE$ERROR ENTRY;
       83     6238    1   DCL DDR$MOVE ENTRY(4) ALTRET;
       84     6239    1   DCL DDQ$WRITE ENTRY(2);
       85     6240    1   DCL DFD$CONVERT_DD ENTRY(4) ALTRET;
       86     6241    1   DCL DFQ$READ_FD ENTRY(2) ALTRET;
       87     6242    1   DCL DFQ$WRITE_FD ENTRY(1) ALTRET;
       88     6243    1   DCL DUE$ERRMSG ENTRY;
       89     6244    1   DCL DUE$GERRMSG ENTRY;
       90     6245        /**/
       91     6246    1        IF S_D.RUM OR S_D.ANLZ
       92     6247    2        THEN DO;
       93     6248    2             S_D.ERROR = 34;         /* E@NOTYET */
       94     6249    2             GOTO ARET;
       95     6250    2           END;
       96     6251    1        LOWX = S_D.DO$->UNFID$CMD.ID_FROM;
       97     6252    1        HIGX = S_D.DO$->UNFID$CMD.ID_TO;
       98     6253    1        GD$MSG = '0'B;
       99     6254    1        GD$MSG.UNFID.K13 = S_D.DO$->UNFID$CMD.K13;
      100     6255    1        GD$MSG.UNFID.DCBNAME.L = S_D.DO$->UNFID$CMD.DCBN.L;
      101     6256    2        DO I = 0 TO 30;
      102     6257    2             GD$MSG.UNFID.DCBNAME.NAME(I) = ASCBIN(SUBSTR(S_D.DO$->UNFID$CMD.DCBN.C,I,1
              6257                      ));
      103     6258    2           END;
      104     6259    2        DO I = LOWX TO HIGX;
      105     6260    2             CALL M$WAIT (FPT_WAIT);
      106     6261    2             GD$MSG.UNFID.DCB# = I;
      107     6262    2             GD$MSG.FNC = %GD#RQSUNFID;
      108     6263                                                /* Write message to FD */
      109     6264    2             CALL DFQ$WRITE_FD (S_D.CURRENT_FPRG_CONTEXT$) ALTRET(ERR);
      110     6265                                                /* Read response from FD */
PL6.E3A0      #001=DFA$UNFID File=DFA$UNFID.:E05TSI                              TUE 07/29/97 14:38 Page:4    
      111     6266    2             CALL DFQ$READ_FD (S_D.CURRENT_FPRG_CONTEXT$, '0'B) ALTRET(ERR);
      112     6267    2             IF GD$MSG.FNC ~= %GD#RSPUNFID
      113     6268    3             THEN DO;
      114     6269    4                  DO CASE(GD$MSG.FNC);
      115     6270    4                    CASE(%GD#RSPNOSUCHDCB);
      116     6271    4                       IF GD$MSG.UNFID.K13 AND S_D.DO$->UNFID$CMD.ID_TO ~= I THEN GOTO
              6271                                NEXT;
      117     6272    4                       S_D.ERROR = 183; /* E@NODCB */
      118     6273    4                       CALL DDE$ERROR;
      119     6274    4                    CASE(%GD#RSPDCBEMPTY);
      120     6275    4                       IF GD$MSG.UNFID.K13 AND S_D.DO$->UNFID$CMD.ID_TO ~= I THEN GOTO
              6275                                NEXT;
      121     6276    4                       S_D.ERROR = 184; /* E@DCBNIL */
      122     6277    4                       CALL DDE$ERROR;
      123     6278    4                    CASE(ELSE);
      124     6279    4                       IF S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.XDELTA AND
      125     6280    4                         GD$MSG.FNC = %GD#RSPBADMSG
      126     6281    4                       THEN S_D.ERROR = 330; /* E@BADMSG */
      127     6282    5                       ELSE DO;
      128     6283    5                            IF GD$MSG.FNC = %GD#RSPNAK
      129     6284    6                            THEN DO;
      130     6285    6                                 IF S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.XDELTA
      131     6286    7                                 THEN DO;
      132     6287    7                                      TEMP1 = BITBIN(GD$MSG.ERROR_D.ERRCODE);
      133     6288    7                                      TEMP2B.B1 = TEMP1B.B1;
      134     6289    7                                      TEMP2B.B2 = TEMP1B.B2;
      135     6290    7                                      TEMP2B.B3 = TEMP1B.B3;
      136     6291    7                                      TEMP2B.B4 = TEMP1B.B4;
      137     6292    7                                      S_D.JDE = TEMP2;
      138     6293    7                                    END;
      139     6294    6                                 ELSE S_D.JDE = BITBIN(HOST_GD$MSG.ERROR_D.ERRCODE);
      140     6295    6                                 CALL DUE$GERRMSG;
      141     6296    6                                 RETURN;
      142     6297    6                               END;
      143     6298    5                            REMEMBER NEVER IN S_D.SCREECH_IC;
      144     6299    5                            CALL M$SCREECH (SNAP_MESSAGE) ALTRET(NEVER);
      145     6300    5   NEVER:
PL6.E3A0      #001=DFA$UNFID File=DFA$UNFID.:E05TSI                              TUE 07/29/97 14:38 Page:5    
      146     6301    5                            S_D.ERROR = 524; /* E@BADMESSAGE */
      147     6302    5                            CALL DDE$ERROR;
      148     6303    5                            S_D.ERROR = 502; /* E@DELBUG */
      149     6304    5                            CALL DDE$ERROR;
      150     6305    5                            RETURN;
      151     6306    5                          END;
      152     6307    4                    END;
      153     6308    3                  GOTO NEXT;
      154     6309    3                END;
      155     6310    3             IF S_D.DO$->UNFID$CMD.K13 THEN DO;
      156     6311    3                  IF (NOT S_D.DO$->UNFID$CMD.OPEN) AND (GD$MSG.UNFID.OPEN) THEN GOTO
              6311                           NEXT;
      157     6312    3                  IF (NOT S_D.DO$->UNFID$CMD.CLOSED) AND (NOT GD$MSG.UNFID.OPEN) THEN
              6312                           GOTO NEXT;
      158     6313    3                END;
      159     6314    2             S_D.OUTPUT = 'DCB #';
      160     6315    2             BB$ = PINCRC(ADDR(S_D.OUTPUT), 5);
      161     6316    2             DCB# = GD$MSG.UNFID.DCB#;
      162     6317    2             IF DCB# < 10
      163     6318    3             THEN DO;
      164     6319    3                  CALL BINCHAR(BB$->CHAR1, DCB#);
      165     6320    3                  BB$ = PINCRC(BB$, 2);
      166     6321    3                END;
      167     6322    3             ELSE DO;
      168     6323    3                  CALL BINCHAR(BB$->CHAR2, DCB#);
      169     6324    3                  BB$ = PINCRC(BB$, 3);
      170     6325    3                END;
      171     6326    2             NAME = HOST_GD$MSG.UNFID.DCBNAME.NAME;
      172     6327    2             BB$ -> CHAR32 = NAMEC;
      173     6328    2             BB$ = PINCRC(BB$, GD$MSG.UNFID.DCBNAME.L);
      174     6329    2             IF GD$MSG.UNFID.DCBNAME.L > 0
      175     6330    3             THEN DO;
      176     6331    3                  BB$->CHAR1 = ' ';
      177     6332    3                  BB$ = PINCRC(BB$, 1);
      178     6333    3                END;
      179     6334    2             COMMA = '0'B;
      180     6335    2             IF GD$MSG.UNFID.NAME.L ~= 0
PL6.E3A0      #001=DFA$UNFID File=DFA$UNFID.:E05TSI                              TUE 07/29/97 14:38 Page:6    
      181     6336    3             THEN DO;
      182     6337    3                  NAME = HOST_GD$MSG.UNFID.NAME.C;
      183     6338    3                  BB$ -> CHAR32 = NAMEC;
      184     6339    3                  BB$ = PINCRC(BB$,GD$MSG.UNFID.NAME.L);
      185     6340    3                  BB$ -> CHAR1 = '.';
      186     6341    3                  BB$ = PINCRC(BB$, 1);
      187     6342    3                  ACCT = HOST_GD$MSG.UNFID.ACCT;
      188     6343    3                  BB$ -> CHAR8 = ACCTC;
      189     6344    3                  BB$ = PINCRC(BB$, 8);
      190     6345    3                  COMMA = '1'B;
      191     6346    3                END;
      192     6347    2             CALL INDEX(TEMP, '  ', S_D.OUTPUT);
      193     6348    2             BB$ = PINCRC(ADDR(S_D.OUTPUT), TEMP+1);
      194     6349    2             REM = 120;
      195     6350    2             IF COMMA
      196     6351    2             THEN CALL DDR$MOVE(',RES=', LENGTHC(',RES='), BB$, REM);
      197     6352    2             ELSE CALL DDR$MOVE(' RES=', LENGTHC(' RES='), BB$, REM);
      198     6353    2             RES = HOST_GD$MSG.UNFID.RES;
      199     6354    2             CALL DDR$MOVE (RESC, 4, BB$, REM);
      200     6355    2             CALL DDR$MOVE (' ', 1, BB$, REM);
      201     6356    2             IF GD$MSG.UNFID.OPEN
      202     6357    3             THEN DO;
      203     6358    3                  CALL DDR$MOVE('(OPEN, ', 7, BB$, REM);
      204     6359    3                END;
      205     6360    3             ELSE DO;
      206     6361    3                  CALL DDR$MOVE('(CLOSED, ', 9, BB$, REM);
      207     6362    3                  IF GD$MSG.UNFID.WAS_OPEN
      208     6363    3                  THEN CALL DDR$MOVE('WAS OPEN, ', SIZEC('WAS OPEN, '), BB$, REM);
      209     6364    3                  ELSE CALL DDR$MOVE('NEVER OPEN, ', SIZEC('NEVER OPEN, '), BB$, REM);
      210     6365    3                END;
      211     6366    3             DO CASE(GD$MSG.UNFID.FUN#);
      212     6367    3               CASE(%UPDATE#);
      213     6368    3                  CALL DDR$MOVE('UPDATE, ', 8, BB$, REM);
      214     6369    3               CASE(%IN#);
      215     6370    3                  CALL DDR$MOVE('IN, ', 4, BB$, REM);
      216     6371    3               CASE(%CREATE#);
      217     6372    3                  CALL DDR$MOVE('CREATE, ', 8, BB$, REM);
PL6.E3A0      #001=DFA$UNFID File=DFA$UNFID.:E05TSI                              TUE 07/29/97 14:38 Page:7    
      218     6373    3               CASE(ELSE);
      219     6374    3                  CALL DDR$MOVE('?, ', 3, BB$, REM);
      220     6375    3               END;
      221     6376    3             DO CASE(GD$MSG.UNFID.ORG#);
      222     6377    3               CASE(%G_ORG_CONSEC#);
      223     6378    3                  CALL DDR$MOVE('CONSEC)', LENGTHC('CONSEC)'), BB$, REM);
      224     6379    3               CASE(%G_ORG_FORM#);
      225     6380    3                  CALL DDR$MOVE('FORM)', LENGTHC('FORM)'), BB$, REM);
