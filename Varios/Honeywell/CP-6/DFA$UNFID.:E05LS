      236     6391    3               CASE(ELSE);
      237     6392    3                  CALL DDR$MOVE('?)', LENGTHC('?)'), BB$, REM);
      238     6393    3               END;
      239     6394    2             CALL DDQ$WRITE(ADDR(S_D.OUTPUT), POFFC(BB$, ADDR(S_D.OUTPUT)));
      240     6395    2   NEXT:
      241     6396    2             IF NOT S_D.DO$->UNFID$CMD.K13 THEN GOTO RET;
      242     6397    2           END;                            /* DO DCB# */
