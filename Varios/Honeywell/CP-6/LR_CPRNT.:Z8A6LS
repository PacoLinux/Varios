       11       11        /*D*
       12       12               NAME: CPRNT
       13       13            PURPOSE: PRINT THE CONFIGURATION SETS(STATES) FOR THE
       14       14                     GRAMMAR BEING ANALYZED.
       15       15        */
       16       16
       17       17
       18       18        CPRNT:  PROC(CONFIG_NUM,PROD_NO,DOT_POS);
       19       19
       20       20        /* IF CONFIG_NUM = NIL, ALL CONFIGURATIONS SETS
       21       21           WILL BE PRINTED, OTHERWISE, ONLY THE ONE DESIGNATED.
       22       22        */
       23       23
       24       24        %INCLUDE LR_GRAM_EXT;
       25      563        %INCLUDE LR_CNFG_EXT;
       26      716
       27      717    1   DCL ENTER_LINE ENTRY(2);
       28      718    1   DCL PRINT_PROD ENTRY(3);
       29      719    1   DCL OUT_STR ENTRY(2);
       30      720    1   DCL OUT_TOKEN ENTRY(2);
       31      721    1   DCL OUT_SYMBOL ENTRY(1);
       32      722    1   DCL OUT_SYMBOL_TOKEN ENTRY(1);
       33      723    1   DCL FLUSH ENTRY;
       34      724    1   DCL PRINT_ITEM ENTRY(3);
       35      725
       36      726
       37      727    1   DCL CSLOW      SBIN;
PL6.E3A0      #001=CPRNT File=LR_CPRNT.:Z8A6TSI                                  SAT 08/09/97 09:27 Page:2    
       38      728    1   DCL CSHI       SBIN;
       39      729    1   DCL CONFIG_NUM SBIN;
       40      730    1   DCL PROD_NO    SBIN;
       41      731    1   DCL DOT_POS    SBIN;
       42      732    1   DCL C          SBIN;
       43      733    1   DCL P          SBIN;   /* CURRENT PRODUCTION BEING PRINTED */
       44      734    1   DCL R          SBIN;   /* INDEX INTO RIGHT HAND ELEMENTS OF PRODUCTION P */
       45      735    1   DCL S          SBIN;   /* INDEX INTO SYM_PTRS (IE., A SYMBOL) */
       46      736    1   DCL LAS$       PTR;
       47      737    1   DCL KEY$       PTR;
       48      738    1   DCL KEY_POS    SBIN;
       49      739    1   DCL CONFIG_TEXT_NO CHAR(4);
       50      740
       51      741    1   DCL 1 FALIGN ALIGNED BASED(FSET$),
       52      742    1         2 FSET(0:35) BIT(1) UNAL;
       53      743    1   DCL FSET$ PTR;
       54      744
       55      745    1   DCL E  SBIN;
       56      746    1   DCL DP SBIN;
       57      747
       58      748        %EJECT;
PL6.E3A0      #001=CPRNT File=LR_CPRNT.:Z8A6TSI                                  SAT 08/09/97 09:27 Page:3    
       59      749                 /* MAIN BODY OF CPRINT */
       60      750
       61      751        /* DETERMINE WHETHER ALL SETS WILL BE PRINTED OR JUST ONE */
       62      752
       63      753                %SELECT_LO;
       64      756
       65      757    1           IF ADDR(CONFIG_NUM)=ADDR(NIL) THEN
       66      758    2           DO;
       67      759                   %OUTLINE(S="' '");
       68      765                   %OUTLINE(S="' '");
       69      771                   %OUTLINE(S="'======'");
       70      777                   %OUTLINE(S="'STATES'");
       71      783                   %OUTLINE(S="'======'");
       72      789    2              CSLOW=0;
       73      790    2              CSHI=NR_CONFIG-1;
       74      791    2              END;
       75      792    1           ELSE
       76      793    2           DO;
       77      794    2              CSLOW=CONFIG_NUM;
       78      795    2              CSHI=CONFIG_NUM;
       79      796    2              END;
       80      797
       81      798    2           DO C=CSLOW TO CSHI;
       82      799                   %OUTLINE(S="' '");
       83      805
       84      806    2              CALL BINCHAR(CONFIG_TEXT_NO,C);
       85      807
       86      808    2              S=C#_TO_$(C)->C_SET_B.KEY$->KEY_B.W.SYM_AFTER;
       87      809
       88      810                   %OUT_STR(S="'State #'");
       89      813    2              CALL OUT_STR(CONFIG_TEXT_NO, LENGTHC(CONFIG_TEXT_NO));
       90      814                   %OUT_STR(S="' which is accessed by '");
       91      817    2              CALL OUT_SYMBOL(SYM_PTRS(S));
       92      818                   %OUTLINE(S="' is composed of the following items:'");
       93      824
       94      825
       95      826                   %OUTLINE(S="' '");
PL6.E3A0      #001=CPRNT File=LR_CPRNT.:Z8A6TSI                                  SAT 08/09/97 09:27 Page:4    
       96      832
       97      833    3              DO E=0 TO C#_TO_$(C)->C_SET_B.NR_ELEM-1;
       98      834
       99      835    3                 KEY$=C#_TO_$(C)->C_SET_B.ELEM.KEY$(E);
      100      836
      101      837    4                 DO KEY_POS=0 TO KEY$->KEY_B.W.ELEM_COUNT-1;
      102      838
      103      839    4                    CALL PRINT_ITEM(C, E, KEY_POS);
      104      840
      105      841    4                    IF KEY$->KEY_B.W.SYM_AFTER=0 AND
      106      842    4                      KEY$->KEY_B.FOLLOW$->INCL_B.SET_STATE=
      107      843    4                      SET_STATE_CALC THEN
      108      844
      109      845    5                    DO;
      110      846
      111      847    5                       FSET$=ADDR(KEY$->KEY_B.FOLLOW$->INCL_B.SET);
      112      848
      113      849
      114      850                            %OUT_STR(S="' Lookahead Symbols:'");
      115      853    6                       DO R=1 TO NR_TERM;
      116      854    6                          IF FALIGN.FSET(R) THEN
      117      855    6                             CALL OUT_SYMBOL_TOKEN(SYM_PTRS(R));
      118      856    6                          END;
      119      857
      120      858    5                       CALL FLUSH;
      121      859
      122      860    5                       END;
      123      861
      124      862    4                    END /* KEY */;
      125      863    3                 END /* PRODUCTION */;
      126      864
      127      865    2              END /*CONFIG */;
      128      866
      129      867    1           RETURN;
      130      868
      131      869    1   END CPRNT;
      132      870        %EOD;
