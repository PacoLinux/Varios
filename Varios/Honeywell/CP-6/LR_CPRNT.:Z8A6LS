      140        8            PURPOSE: PRINT THE PRODUCTION SPECIFIED ON THE CURRENT
      141        9                     OUTPUT FILE.
      142       10        */
      143       11
      144       12        PRINT_PROD:PROC (P, DP, PDP);
      145       13
      146       14    1   DCL P    SBIN;
      147       15    1   DCL DP   SBIN;
      148       16    1   DCL PDP  BIT(1) ALIGNED;
      149       17
      150       18    1   DCL S   SBIN;
      151       19    1   DCL R   SBIN;
      152       20
      153       21        %INCLUDE LR_GRAM_EXT;
      154      560
      155      561    1   DCL OUT_TOKEN ENTRY(2);
      156      562    1   DCL OUT_STR   ENTRY(2);
      157      563    1   DCL FLUSH     ENTRY;
      158      564    1   DCL OUT_SYMBOL ENTRY(1);
      159      565    1   DCL OUT_SYMBOL_TOKEN ENTRY(1);
      160      566
      161      567    1           S=PROD.GOAL_SYM_INDX(P);
      162      568
      163      569    1           CALL OUT_SYMBOL(SYM_PTRS(S));
      164      570
      165      571                %OUT_TOKEN(S="'::='");
      166      574
      167      575    1           IF PROD.RIGHT_CNT(P) = 0 THEN
      168      576                  %OUT_TOKEN (S='=empty=');
      169      579
PL6.E3A0      #002=PRINT_PROD File=LR_CPRNT.:Z8A6TSI                             SAT 08/09/97 09:27 Page:33   
      170      580    2           DO R=1 TO DP;
      171      581    2              S=RIGHT_PARTS(PROD.RIGHT_START(P)+R-1);
      172      582    2              CALL OUT_SYMBOL_TOKEN(SYM_PTRS(S));
      173      583    2              END;
      174      584
      175      585    1           IF PDP THEN
      176      586                  %OUT_TOKEN(S='"."');
      177      589
      178      590    2           DO R=DP+1 TO PROD.RIGHT_CNT(P);
      179      591    2              S=RIGHT_PARTS(PROD.RIGHT_START(P)+R-1);
      180      592    2              CALL OUT_SYMBOL_TOKEN(SYM_PTRS(S));
      181      593    2            END;
      182      594
      183      595    1            IF PROD.TRAILING_SEM_ACT(P) THEN
      184      596    1              CALL OUT_SYMBOL_TOKEN(SYM_PTRS(RIGHT_PARTS(PROD.RIGHT_START(P) +
      185      597    1                                    PROD.RIGHT_CNT(P))));
      186      598
