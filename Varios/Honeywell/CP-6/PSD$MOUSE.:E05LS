PL6.E3A0      #001=PSD$MOUSE File=PSD$MOUSE.:E05TSI                              WED 07/30/97 04:31 Page:6    
      116     6377    1   THURB: PROC (VALUE, WHERE);
      117     6378
      118     6379    2   DCL VALUE UBIN;
      119     6380    2   DCL VALUE_SBIN REDEF VALUE SBIN;
      120     6381
      121     6382    2   DCL 1 WHERE,
      122     6383    2         2 MSEC UBIN WORD,
      123     6384    2         2 USEC UBIN WORD;
      124     6385
      125     6386    2      WHERE.MSEC = VALUE / 1000;
      126     6387    2      WHERE.USEC = MOD(VALUE_SBIN, 1000);
      127     6388
      128     6389    2      RETURN;
      129     6390
      130     6391    2   END THURB;
      131     6392
      132     6393    1   FROBB: PROC (I, J, K);
      133     6394
      134     6395    2   DCL I UBIN;
      135     6396    2   DCL J UBIN;
      136     6397    2   DCL J_SBIN REDEF J SBIN;
      137     6398    2   DCL K UBIN;
      138     6399
      139     6400    2   DCL T UBIN;
      140     6401    2   DCL FACTOR UBIN;
      141     6402    2   DCL FACTOR_SBIN REDEF FACTOR SBIN;
      142     6403
      143     6404    2   DCL 1 BIGGEST SBIN WORD STATIC INIT ( BITBIN('377777777777'O) ) ;
      144     6405        %EQU FUDGE_FACTOR = 100000;
      145     6406
      146     6407    2      FACTOR = %FUDGE_FACTOR;
      147     6408
      148     6409    3      DO WHILE ( FACTOR > 1  AND  I > BIGGEST / FACTOR_SBIN ) ;
      149     6410
      150     6411    3         FACTOR = FACTOR / 2;
      151     6412
      152     6413    3         END;
PL6.E3A0      #001=PSD$MOUSE File=PSD$MOUSE.:E05TSI                              WED 07/30/97 04:31 Page:7    
      153     6414
      154     6415    2      IF ( J_SBIN / ( %FUDGE_FACTOR / FACTOR_SBIN ) ) = 0
      155     6416    2        THEN
      156     6417    2         T = 0;
      157     6418    2        ELSE
      158     6419    2         T = ( I * FACTOR_SBIN ) / ( J_SBIN / ( %FUDGE_FACTOR / FACTOR_SBIN ) ) ;
      159     6420
      160     6421    2      CALL THURB (T, K);
      161     6422
      162     6423    2      RETURN;
      163     6424
      164     6425    2   END FROBB;
      165     6426
      166     6427
      167     6428    1   MARK_LINE: PROC (CODER, ARRAY);
      168     6429
      169     6430    2   DCL 1 ARRAY,
      170     6431    2         2 COUNT UBIN WORD,
      171     6432    2         2 COUNT_SBIN REDEF COUNT SBIN WORD,
      172     6433    2         2 CPU UBIN WORD,
      173     6434    2         2 CPU_SBIN REDEF CPU SBIN WORD,
      174     6435    2         2 I_O UBIN WORD,
      175     6436    2         2 I_O_SBIN REDEF I_O SBIN WORD;
      176     6437
      177     6438    2   DCL CODER SBIN WORD;
      178     6439
      179     6440    2      IF CODER = %ALL_SERVICES
      180     6441    2        THEN
      181     6442    2         OUTPUT.PMME = '(all services)';
      182     6443    2        ELSE
      183     6444    2         OUTPUT.PMME = R$ -> TEXTC.TEXT;
      184     6445
      185     6446    3      DO CASE (CODER);
      186     6447
      187     6448    3       CASE (%NULL_ASN, %ALL_SERVICES);
      188     6449
      189     6450    3         OUTPUT.BRKDN = ' ';
PL6.E3A0      #001=PSD$MOUSE File=PSD$MOUSE.:E05TSI                              WED 07/30/97 04:31 Page:8    
      190     6451
      191     6452    3       CASE (%ALL_ASNS);
      192     6453
      193     6454    3         OUTPUT.BRKDN = '(all ASNs)';
      194     6455
      195     6456    3       CASE (%THIS_ASN);
      196     6457
      197     6458    3         OUTPUT.BRKDN = PS_ASN_NAMES(ASN);
      198     6459
      199     6460    3       END;
      200     6461
      201     6462    3      IF ARRAY.COUNT > 0 THEN DO;
      202     6463
      203     6464    3         ITEM_COUNT = ITEM_COUNT + 1;
      204     6465
      205     6466    3         OUTPUT.COUNT = ARRAY.COUNT;
      206     6467    3         CALL FROBB (ARRAY.COUNT, TOTAL_COUNT, OUTPUT.PCT_COUNT);
      207     6468    3         OUTPUT.CPU = ARRAY.CPU / 1000;
      208     6469    3         CALL FROBB (ARRAY.CPU, TOTAL_CPU, OUTPUT.PCT_CPU);
      209     6470    3         CALL THURB ( ARRAY.CPU_SBIN / ARRAY.COUNT_SBIN, OUTPUT.AVG_CPU );
      210     6471    3         OUTPUT.I_O = ARRAY.I_O;
      211     6472    3         CALL FROBB (ARRAY.I_O, TOTAL_IOS, OUTPUT.PCT_IOS);
      212     6473    3         CALL THURB ( ARRAY.I_O_SBIN * 1000 / ARRAY.COUNT_SBIN, OUTPUT.AVG_I_O );
      213     6474
      214     6475    3         CALL X$WRITE (PS_FDS, VECTOR(OUTPUT_FORMAT),
      215     6476    3          VECTOR(OUTPUT));
      216     6477
      217     6478    3         END;
      218     6479
      219     6480    2      RETURN;
      220     6481    2   END MARK_LINE;
      221     6482        %EJECT;
PL6.E3A0      #001=PSD$MOUSE File=PSD$MOUSE.:E05TSI                              WED 07/30/97 04:31 Page:9    
      222     6483    1      CALL PSD$EJECT;
      223     6484
      224     6485    1      CALL X$WRITE (PS_FDS, VECTOR(MOUSE_HEAD_FMT));
      225     6486
