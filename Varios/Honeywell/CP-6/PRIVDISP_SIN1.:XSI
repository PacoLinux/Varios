PRIVDISPCMD(99)=<',',PDOPT> .END
PDOPT=( FMEFT|FMBLK|FMSEC|FMDIAG|FMREAD|;
        DISPJOB|   SYSCON|ASAVE|;
        GPP|   SYSLOG|MFEP|CFEP|   IOQW|IOQ|;
        EXPM|PM|TND|JIT|   MSYS|MAXMEM|EXMM|SPCLMM|;
        CNTRLD|CNTRLC|EFT|EL|LABEL|PIGD|PIGC|SPIDERD|SPIDERC|;
        /*SUPER|*/NETCON|SUPERAUTH|SUPERWSN|SUPERFORM|PADMIN|SUPERD|;
                 VOLINIT|REPLAY|;
        RATES|;
        PASSWORD |;
        SUPER )                         /*SUPER must follow SUPERxx's.        */
FMEFT(0)='FMEFT'
FMBLK(1)='FMBLK'
FMSEC(2)='FMSEC'
FMDIAG(3)='FMDIAG'
FMREAD(4)='FMREAD'
DISPJOB(14)='DISPJOB'
SYSCON(16)='SYSCON'
ASAVE(17)='ASAVE'
GPP(18)='GPP'
SYSLOG(20)='SYSLOG'
MFEP(21)='MFEP'
CFEP(22)='CFEP'
IOQW(25)='IOQW'
IOQ(26)='IOQ'
EXPM(27)='EXPM'
PM(28)='PM'
TND(29)='TND'
JIT(30)='JIT'
MSYS(32)='MSYS'
MAXMEM(33)='MAXMEM'
EXMM(34)='EXMM'
SPCLMM(35)='SPCLMM'
/**/
CNTRLD(36)='CNTRLD'
