     3021    19666    1   IMMEDIATE:PROC;
     3022    19667        /*I*
     3023    19668             NAME: IMMEDIATE
     3024    19669             PURPOSE:
     3025    19670                  Check to see if a constant can be referenced in an
     3026    19671                  instruction using a DU or DL tag.
     3027    19672             DESCRIPTION:
     3028    19673                  'IMMEDIACY' depends on the instruction, the length of the
     3029    19674                  constant, and the type of the constant.  Check all this
     3030    19675                  stuff and if the constant qualifies fill in the Y field of
     3031    19676                  INST_WORD and put DU or DL into tag field.
     3032    19677        */
     3033    19678
     3034    19679           %EQU UPPER = 0;
     3035    19680           %EQU LOWER = 1;
     3036    19681
     3037    19682
     3038    19683    2      IF (NOT DEFER  AND  NOT INST_WORD.LOW.EIS  AND  INST_WORD.LOW.OP_CODE ~= %ARG_OP)
