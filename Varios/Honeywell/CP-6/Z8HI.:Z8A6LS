     3046    19691    4                  CASE(%LDA_OP);
     3047    19692    4                    INST_WORD.LOW.OP_CODE = %LCA_OP;
     3048    19693    4                    INST_WORD.SA = - (ST$$ -> CONST.CONSTS.LOWER);
     3049    19694    4                    INST_WORD.LOW.MF.TAG = %DL;
