     2908    19461    5                  END;
     2909    19462    4                ELSE
     2910    19463    4                  INST_WORD.SA = REF$ -> REF.SA;
     2911    19464                     %NEW_WORD (@OU_WORD = INST_WORD);
     2912    19511    4                INST_WORD.SA = 0;
     2913    19512    4                INST_WORD.Y.AR# = POINTER_REGISTER - %PR0;
     2914    19513    4                IF REF$ -> REF.NDSC.TN
     2915    19514    4                  THEN INST_WORD.LOW.OP_CODE = %A4BD_OP;
     2916    19515    4                  ELSE INST_WORD.LOW.OP_CODE = %A9BD_OP;
     2917    19516    4                INST_WORD.LOW.MF.AR = TRUE;
     2918    19517    4                INST_WORD.LOW.MF.TAG = REF$ -> REF.SSOP_TAG$ -> ETT.STATE;
     2919    19518                     %NEW_WORD (@OU_WORD = INST_WORD);
     2920    19565    4                INST_WORD = SAVED_INST_WORD;
     2921    19566    4                IF DEFER_BUFFER_IDX = 1 THEN
     2922    19567    5                  DO;
     2923    19568    5                    DEFER_BUFFER.MW.MF1.TAG (0) = 0;
     2924    19569    5                    DEFER_BUFFER.MW.MF1.AR (0) = TRUE;
     2925    19570    5                  END;
     2926    19571    4                ELSE /* DEFER_BUFFER_IDX = 2 */
     2927    19572    5                  DO;
     2928    19573    5                    IF REF$ -> REF.SSOP_TAG$ -> ETT.STATE =
     2929    19574    5                        DEFER_BUFFER.MW.MF1.TAG (0) THEN
     2930    19575    6                      DO;
     2931    19576                             /* Uh oh ... compiler expects two different quantities
     2932    19577                                to be in the same register at the same time ...
     2933    19578                                find where the other got moved to. */
     2934    19579    6                        P$ = PILL -> ILL.ROP -> ETT.REF$ -> REF.SSOP_TAG$;
     2935    19580    6                        IF P$ = ADDR (NIL) /* Not obvious how to find it */
     2936    19581    6                          THEN CALL Z8AE$ERROR (%E$UNEXPECTED_CASE);
     2937    19582    6                          ELSE DEFER_BUFFER.MW.MF1.TAG (0) = P$ -> ETT.STATE;
     2938    19583    6                      END;
     2939    19584    5                    DEFER_BUFFER.MW.MF2.TAG (0) = 0;
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:97   
     2940    19585    5                    DEFER_BUFFER.MW.MF2.AR (0) = TRUE;
     2941    19586    5                  END;
     2942    19587    4                IF NOT PR THEN
     2943    19588    4                  INST_WORD.SA = 0;
     2944    19589    4                INST_WORD.Y.AR# = POINTER_REGISTER - %PR0;
     2945    19590    4              END;
     2946    19591    3            CALL Z8HU$TAG(REF$ -> REF.SIZ_TAG$, ST$$ -> VARIABLE.B.GREATER_256K = TRUE)
             19591                     ;
     2947    19592    3            INST_WORD.ADSC.N = REF$ -> REF.SIZ_TAG$ -> ETT.STATE;
     2948    19593    3            IF DEFER_BUFFER_IDX = 1
     2949    19594    3                AND REF$ -> REF.SIZ_TAG$ -> ETT.STATE < %XR0 /* A or Q */
     2950    19595    3                AND PILL -> ETT.ID ~= %CFILL
     2951    19596    3                AND SSOP
     2952    19597    3                AND ST$$ -> VARIABLE.B.GREATER_256K THEN
     2953    19598    4              DO;
     2954    19599    4                OP_SIZ_TAG$ = REF$ -> REF.SIZ_TAG$;
     2955    19600    4                CALL Z8HU$INCCNT (OP_SIZ_TAG$, 1);
     2956    19601                     /* In this case, the compiler needs 4 arithmetic registers to
     2957    19602                        hold lengths and offsets for both descriptors. There are
     2958    19603                        only 2 available. Therefore, pointer registers need to be
     2959    19604                        assigned to combine the base PR if any and the offset, so
     2960    19605                        that arithmetic registers will be needed only for the sizes.
     2961    19606                        The values for the ROP must NOT tamper with the register
     2962    19607                        used for the length of the LOP. Temporarily increasing its
     2963    19608                        usage count should guarantee that it will be safe. */
     2964    19609    4                OP_SSOP_TAG$ = NODE$;
     2965    19610    5                DO WHILE OP_SSOP_TAG$ -> ETT.ID ~= %SSOP;
     2966    19611    5                  OP_SSOP_TAG$ = OP_SSOP_TAG$ -> ETT.LOP;
     2967    19612    5                END;
     2968    19613    4                CALL Z8HG$USE_REGISTER (OP_SSOP_TAG$, POINTER_REGISTER);
     2969    19614    4                BIG_OP_SSOP_FLAG = TRUE;
     2970    19615    4              END;
     2971    19616    3          END;
     2972    19617
     2973    19618    2          IF DEFER_BUFFER_IDX = 2 AND BIG_OP_SSOP_FLAG THEN
     2974    19619    3            DO;
     2975    19620    3              CALL Z8HU$DEC_COUNT (OP_SIZ_TAG$, 1);
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:98   
     2976    19621                   /* Unlock size tag from its register */
     2977    19622    3              CALL Z8HU$DEC_COUNT (OP_SSOP_TAG$, 1);
     2978    19623                   /* Likewise, ssop tag */
     2979    19624    3              BIG_OP_SSOP_FLAG = FALSE;
     2980    19625    3            END;
     2981    19626
     2982    19627
     2983    19628
     2984    19629    2   END HANDLE_REF_OPERAND;
     2985    19630        %EJECT;
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:99   
     2986    19631    1   HANDLE_CONSTANT:PROC;
     2987    19632
     2988    19633        /*I*
     2989    19634             NAME: HANDLE_CONSTANT
     2990    19635             PURPOSE:
     2991    19636                  Handle a reference to a constant in an instruction.
     2992    19637             DESCRIPTION:
     2993    19638                  Set up an instruction to reference a constant.  If
     2994    19639                  the constant can be put into the Y field of the instruction
     2995    19640                  using a DU or DL tag then do so (this is done in the
     2996    19641                  procedure IMMEDIATE).  If the constant can't be 'immediate'
     2997    19642                  then it must be materialized, i.e. it must be allocated
     2998    19643                  space in the program and referenced as if storage contained.
     2999    19644        */
     3000    19645    2        CALL IMMEDIATE;
     3001    19646
     3002    19647    3        DO CASE(INST_WORD.LOW.MF.TAG);
     3003    19648    3          CASE(%DU, %DL);
     3004    19649    3          CASE(ELSE);
     3005    19650    3            MATERIALIZED_CONSTANT = TRUE;
     3006    19651    3            INST_WORD.SA = REF$ -> REF.SA + INST_WORD.SA;
     3007    19652
     3008    19653    3            IF NOT ST$$ -> CONST.B.ALLOCATED THEN
     3009    19654    4              DO;
     3010    19655    4                ST$$ -> CONST.B.ALLOCATED = TRUE;
     3011    19656    4                ST$$ -> CONST.ALLOC_LINK = CONST$;
     3012    19657    4                CONST$ = ST$$;
     3013    19658    4              END;
     3014    19659
     3015    19660    3            IF DEFER THEN
     3016    19661    3              INST_WORD.ADSC = REF$ -> REF.ADSC;
     3017    19662    3        END /* Case */;
     3018    19663
     3019    19664    2   END HANDLE_CONSTANT;
