      455    14224        %EJECT;
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:16   
      456    14225    4   CASE(%AD);
      457    14226        /*V* ADD -- See MOVE for PSEUDO-CODE_UTILITIES documentation  */
