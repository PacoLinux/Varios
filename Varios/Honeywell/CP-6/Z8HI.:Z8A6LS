PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:10   
      317    14073        /*V*
      318    14074
      319    14075             INITIALIZE AUTOMATIC VARIABLES AND SET STARTING IC POINT.
      320    14076
      321    14077        */
      322    14078
      323    14079    1        DEFER = FALSE;
      324    14080    1        BIG_OP_SSOP_FLAG = FALSE;
      325    14081    1        CASE_PASS = 0;
      326    14082    1        ALLOTD_PASS = 0;
      327    14083    1        DEFER_BUFFER_IDX = 0;
      328    14084    1        MAPT_WORD = '0'O;
      329    14085    1        AORQ = FALSE;
      330    14086    1        PCOM.GABS = 0;
      331    14087    1        MAPT_VALID_COUNT = 0;
      332    14088
      333    14089
      334    14090
      335    14091        /* Compute the pseudo Instruction Counter from the PEPD descriptor
      336    14092           passed.
      337    14093        */
      338    14094
      339    14095    1        IC = PINCRW(PSEUDO_CODE_MODULE(DESCRIPTOR.MODULE),DESCRIPTOR.OFFSET);
      340    14096
      341    14097        /*
      342    14098
      343    14099                                   LOOP
      344    14100
      345    14101           THIS BEGINS THE INTERPRETATION LOOP.  THE ONLY WAY OUT
      346    14102           OF THE LOOP IS VIA A QUIT DIRECTIVE (ACTION %QUIT).
      347    14103
      348    14104        */
      349    14105    1   LOOP:
      350    14106    2        DO FOREVER;
      351    14107    2          PCM.FIRST_WORD = IC -> BASE.FIRST_WORD;
      352    14108    2          PCM.SECOND_WORD = IC -> BASE.SECOND_WORD;
      353    14109
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:11   
      354    14110        %EJECT;
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:12   
      355    14111        /*V*
      356    14112
      357    14113                                 SUPPORT_ACTIONS
      358    14114
      359    14115
      360    14116               The following actions in one way or another, alter the
      361    14117             sequence of pseudo_code interpretation.  These actions are
      362    14118             the only ones that can change the IC.
      363    14119
      364    14120        */
      365    14121    2   SUPPORT_ACTIONS:
      366    14122
      367    14123    3        DO CASE(PC.ACTION);
      368    14124
      369    14125    3          CASE(%BCT);
      370    14126    3            PCOM_.ABS(PC.OPND1) = PCOM_.ABS(PC.OPND1) - 1;
      371    14127
      372    14128    3            IF PCOM_.ABS(PC.OPND1) = 0 THEN
      373    14129    3              IC = PINCRW(IC,2);
      374    14130    3            ELSE
      375    14131    3              IC = PINCRW(IC,PCS.OPND2);
      376    14132
      377    14133    3          CASE(%EQLR);
      378    14134    3            IF PILL -> ETT.LOP = PILL -> ETT.ROP THEN
      379    14135    3              IC = PINCRW(IC,PCS.OPND2);
      380    14136    3            ELSE
      381    14137    3              IC = PINCRW(IC,PCS.OPND1);
      382    14138
      383    14139    3          CASE(%GOSUB);
      384    14140    3            SUB_STACK(SUB_IDX) = PINCRW(IC,1);
      385    14141    3            SUB_IDX = SUB_IDX + 1;
      386    14142    3            IC = PINCRW(IC,PCS.OPND1);
      387    14143
      388    14144    3          CASE(%IFID);
      389    14145                 %LOP_ROP_TO(OP=PCC.CODE1);
      390    14159    3            IF OPND -> ETT.ID = PCC.CODE2 THEN
      391    14160    3              IC = PINCRW(IC,PCS.OPND2);
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:13   
      392    14161    3            ELSE
      393    14162    3              IC = PINCRW(IC,PCS.OPND3);
      394    14163
      395    14164    3          CASE(%IFITIS);
      396    14165
      397    14166    3            P$ = PILL -> ETT.ROP;
      398    14167    3            IF P$ -> ETT.ID = %TRIAD  THEN
      399    14168
      400    14169    4              DO CASE( P$ -> ETT.LOP -> CONST.TYPE );
      401    14170
      402    14171    4                CASE( %TYR );
      403    14172
      404    14173    4                  IF BINBIT(P$ -> ETT.LOP -> CONST.CONSTI.WORD1, 36) =  %
             14173                           SINGLE_FLOAT_ZERO   THEN
      405    14174    4                    IC = PINCRW( IC, PCS.OPND2 );
      406    14175    4                  ELSE
      407    14176    4                    IC = PINCRW( IC, PCS.OPND3 );
      408    14177
      409    14178    4                CASE( %TYQ );
      410    14179
      411    14180    4                  IF BINBIT(P$ -> ETT.LOP -> CONST.CONSTI.WORD1, 36) = %
             14180                           SINGLE_FLOAT_ZERO  AND
      412    14181    4                     P$ -> ETT.LOP -> CONST.CONSTI.WORD2 = %ZERO  THEN
      413    14182    4                    IC = PINCRW( IC, PCS.OPND2 );
      414    14183    4                  ELSE
      415    14184    4                    IC = PINCRW( IC, PCS.OPND3 );
      416    14185
      417    14186    4                CASE( ELSE );
      418    14187
      419    14188    4                  IF P$ -> ETT.LOP -> CONST.CONSTI.WORD1 = PCS.OPND1  THEN
      420    14189    4                    IC = PINCRW( IC, PCS.OPND2 );
      421    14190    4                  ELSE
      422    14191    4                    IC = PINCRW( IC, PCS.OPND3 );
      423    14192
      424    14193    4               END;
      425    14194
      426    14195    3            ELSE
PL6.E3A0      #001=Z8HI$BURST File=Z8HI.:Z8A6TSI                                 SAT 08/09/97 13:19 Page:14   
      427    14196    3              IC = PINCRW( IC, PCS.OPND3 );
      428    14197    3          CASE(%JMP);
      429    14198    3            IC = PINCRW(IC,PCS.OPND1);
      430    14199
      431    14200    3          CASE(%JMPDV);
