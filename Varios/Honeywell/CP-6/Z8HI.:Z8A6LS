             19050                 /* STBA or STBQ ... compiler will not generate STCA or STCQ */
     2743    19051
     2744    19052    2        IF REF$ -> REF.B.CONST THEN
     2745    19053    2          CALL HANDLE_CONSTANT;
     2746    19054    2        ELSE
