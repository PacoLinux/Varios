     2104    17923    4   CASE(%MAPT);
     2105    17924        /*V*
     2106    17925
     2107    17926                           MAPT
     2108    17927
     2109    17928
     2110    17929        EXAMPLES::
     2111    17930             MAPT   FREE,OPMOD  MAP LAST FREED REGISTER ONTO OPCODE
     2112    17931             MAPT   *,TAGMOD    MAP CURRENT TRIAD ONTO TAG
     2113    17932             MAPT   LOP,TAGMOD  MAP REGISTER CONTAINING LOP ONTO TAG
     2114    17933
     2115    17934
     2116    17935             THIS DIRECTIVE TAKES A REGISTER (SPECIFIED BY THE FIRST
     2117    17936           PARAMETER), AND MAPS IT (OR 'ORS' IT) OVER THE PORTION OF
     2118    17937           THE NEXT GENERATED WORD SPECIFIED BY THE SECOND PARAMETER.
     2119    17938           E.G. OPCODE MODIFICATION OR REGISTER TAG MODIFICATION.
     2120    17939
     2121    17940        */
     2122    17941
