      214     1984    1       AREA.LOAD_LIM = UWA.AC.LOAD_LIMIT;
      215     1985    1       AREA.SECTORS = UWA.AC.SECTORS_PAGE;
      216     1986    1       AREA.SECTORS = AREA.SECTORS * (AREA.MAX_PAGE + 1);
      217     1987    1       AREA.TOTAL_KEYS = UWA.AC.KEYS_ALLOC;
      218     1988    1       RNG = AREA.MAX_DBK_GLOB - AREA.MIN_DBK_GLOB + 1;
      219     1989    1       IF UWA.AC.CALC_NULL = 0 THEN
      220     1990    1           AREA.CALC_HDRS = (RNG + AREA.DBK_CALC - 1) / AREA.DBK_CALC;
PL6.E3A0      #001=DBA_SUMMARY File=ZIG$DDE1.:ZIC0TSI                            FRI 09/05/97 11:43 Page:7    
      221     1991    1       ELSE
      222     1992    2       DO;
      223     1993    2           AREA.CALC_HDRS = 0;
      224     1994    2           GOTO NXTAC1;
      225     1995    2           END;
      226     1996         /**
      227     1997            Reduce number of calc headers to
      228     1998            a prime number
      229     1999            */
      230     2000    1   PRIME:
      231     2001    1       IF AREA.CALC_HDRS > 2 THEN
      232     2002    2       DO;
      233     2003    2           IF MOD(AREA.CALC_HDRS, 2) = 0 THEN
      234     2004    2               AREA.CALC_HDRS = AREA.CALC_HDRS - 1;
      235     2005    3           DO D = 3 TO AREA.CALC_HDRS/2 BY 2 ;
      236     2006    3               X = AREA.CALC_HDRS / D;
      237     2007    3               IF X * D = AREA.CALC_HDRS THEN
      238     2008    4               DO;
      239     2009    4                   AREA.CALC_HDRS = AREA.CALC_HDRS - 1;
      240     2010    4                   GOTO PRIME;
      241     2011    4                   END;
      242     2012    3               END;
      243     2013    2           END;
      244     2014    1   NXTAC1:
      245     2015    1       IF UWA.AC.PAGE_TYP ~= 3
      246     2016    2       THEN DO;
      247     2017    2           AREA.AVAIL_KEYS = AREA.TOTAL_KEYS - AREA.CALC_HDRS;
      248     2018    2           AREA.DATA_PGS = AREA.TOTAL_KEYS / AREA.DBK_PAGE;
      249     2019    2           IF AREA.DATA_PGS * AREA.DBK_PAGE ~= AREA.TOTAL_KEYS THEN
      250     2020    2               AREA.DATA_PGS = AREA.DATA_PGS + 1;
      251     2021    2           AREA.INV_PGS = AREA.MAX_PAGE - AREA.DATA_PGS;
      252     2022    2           AREA.LLINKS = AREA.SECTORS / 5;
      253     2023    2           IF AREA.LLINKS * 5 ~= AREA.SECTORS THEN
      254     2024    2               AREA.LLINKS = AREA.LLINKS + 1;
      255     2025    2           END;
