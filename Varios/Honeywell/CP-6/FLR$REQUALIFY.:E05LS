        7        7        /*P*  NAME: FLR$REQUALIFY
        8        8              PURPOSE: Process REQUALIFY  command
        9        9        */
       10       10        FLR$REQUALIFY: PROC ;
       11       11        /***/
       12       12        /** Includes **/
       13       13        /***/
       14       14        %INCLUDE CP_6;
       15     5573        %INCLUDE CP_6_SUBS;
       16     6113        %INCLUDE B_ERRORS_C;
       17     7240        %INCLUDE XU_MACRO_C;
       18    10346        %INCLUDE XU_SUBS_C;
       19    10438        %INCLUDE FLI_DATA_R;
       20    11852        %INCLUDE FM$GRAN;
       21    11921        %INCLUDE FM$FIT;
       22    12142        %INCLUDE FLN_DATA_R;
       23    12879        %INCLUDE FLE_DATA_R;
       24    18830        /***/
       25    18831        /** External procedures  **/
       26    18832        /***/
       27    18833    1   DCL FLC$CRIT_CHECK ENTRY (2) ALTRET;
       28    18834    1   DCL FLN$DISK_IO_INIT ENTRY;
       29    18835    1   DCL FLP$WR_FILE_INFO ENTRY(9) ALTRET;
       30    18836    1   DCL FLN$DISK_OPEN_NEXT ENTRY(5) ALTRET;
       31    18837    1   DCL FLN$DISK_CLOSE_NEXT ENTRY(1);
       32    18838    1   DCL FLN$DISK_CLOSE_ALL ENTRY;
       33    18839    1   DCL FLI$RPT_ERR ENTRY(5);
       34    18840    1   DCL XSF$LOCCODP ENTRY(2) ALTRET;
       35    18841        %FM$GRAN;
       36    18845        %F$DCB (STCLASS = "BASED(DCB$)");
       37    18902        %CODE06(BASED="BASED(CODE6$)");
PL6.E3A0      #001=FLR$REQUALIFY File=FLR$REQUALIFY.:E05TSI                      TUE 07/29/97 16:17 Page:2    
       38    18906        %VLP_VECTOR (FPTN=FIT_,
       39    18907                     STCLASS = STATIC);
       40    18922
       41    18923        %VLP_ERRCODE (FPTN = ERR);
       42    18968
       43    18969    1   DCL BLANK6 CHAR(0) STATIC INIT('      ');
       44    18970    1   DCL DCB# UBIN ;
       45    18971    1   DCL DCB_NUM UBIN WORD;
       46    18972    1   DCL DCB$ PTR STATIC;
       47    18973    1   DCL BREAK_COUNT SBIN SYMREF;
       48    18974    1   DCL CODE1$ PTR;
       49    18975    1   DCL CODE6$ PTR;
       50    18976         %EJECT;
PL6.E3A0      #001=FLR$REQUALIFY File=FLR$REQUALIFY.:E05TSI                      TUE 07/29/97 16:17 Page:3    
       51    18977        /***/
       52    18978        /** Process REQUALIFY commnad  **/
       53    18979        /***/
       54    18980    1         DISK_OPEN_NEXT.V.OPER.TEST# = %NO#;
       55    18981    1         CALL FLP$WR_FILE_INFO;
       56    18982    1         CALL FLN$DISK_IO_INIT;
       57    18983    2         DO WHILE('1'B);
       58    18984    2             CALL FLN$DISK_OPEN_NEXT(DCB$,DCB_NUM,DCB#,FIT_,ERR) ALTRET(DONE);
       59    18985    2             IF BREAK_COUNT > 0
       60    18986    2             THEN
       61    18987    3             DO;
       62    18988    3                 CALL FLN$DISK_CLOSE_ALL;
       63    18989    3                 RETURN;
       64    18990    3             END;
       65    18991    2             IF ERR
       66    18992    2             THEN
       67    18993    3             DO;
       68    18994    3               IF ERR.ERR# = %E$ADEOF
       69    18995    3                 THEN ;
       70    18996    3                 ELSE
       71    18997    3                     IF ERR.ERR# = %E$NOKEY OR ERR.ERR# = %E$FDEOF
       72    18998    3                       THEN;
       73    18999    3                        ELSE
       74    19000    4                        DO;
       75    19001    4                          CALL FLP$WR_FILE_INFO(DCB$,ADDR(NIL),BLANK6,0,' ', ,ERR,'1'B,
             19001                                   '0'B);
       76    19002    4                          CALL FLI$RPT_ERR(ERR,DCB_NUM);
       77    19003    4                          IF ERR.ERR# = %E$FILEBUSY OR ERR.ERR# = %E$NOACCESS
       78    19004    4                             OR ERR.ERR# = %E$JRNLERR OR ERR.ERR# = %E$BADFIT
       79    19005    4                             OR ERR.ERR# = %E$IOERR OR ERR.ERR# = %E$NOSHRDS
       80    19006    4                             OR ERR.ERR# = %E$BROKEN
       81    19007    4                             THEN IF FPT_EXIT.V.STEPCC# < %MINOR_ERROR
       82    19008    4                                  THEN FPT_EXIT.V.STEPCC# = %MINOR_ERROR;
       83    19009    4                                  ELSE ;
       84    19010    4                             ELSE
       85    19011    5                             DO;
       86    19012    5                               CALL FLN$DISK_CLOSE_ALL;
PL6.E3A0      #001=FLR$REQUALIFY File=FLR$REQUALIFY.:E05TSI                      TUE 07/29/97 16:17 Page:4    
       87    19013    5                               RETURN;
       88    19014    5                             END;
       89    19015    4                        END;
       90    19016    3             END;
       91    19017    2             ELSE
       92    19018    3             DO;  /* NOT ERR */
       93    19019    3                 CALL FLC$CRIT_CHECK(DCB$,FIT_.PTR$)
       94    19020    3                 WHENRETURN
       95    19021    4                 DO;
       96    19022    4                    DISK_CLOSE_NEXT.V.EFTOPER.UPDDESC# = '1'B;
       97    19023    4                    DISK_CLOSE_NEXT.V.DESC = F$DCB.DESC|NEW_DISK_DESC;
       98    19024    4                    IF NEW_DISK_DESC.QS#
       99    19025    4                      THEN
      100    19026    5                      DO;
      101    19027    5                        CODE1$ = PINCRW(FIT_.PTR$,SIZEW(FM$GRAN));
      102    19028    5                        CALL XSF$LOCCODP(CODE1$,01)
      103    19029    6                        WHENRETURN DO;  /* If a CODE01 - must be actively stowed */
      104    19030    6                          DISK_CLOSE_NEXT.V.DESC.ACTIVE# = '1'B;
      105    19031    6                          END;
      106    19032    6                        WHENALTRETURN DO;           /* Must be inactively stowed */
      107    19033    6                          DISK_CLOSE_NEXT.V.DESC.ACTIVE# = '0'B;
      108    19034    6                          END;
      109    19035    5                      END;
      110    19036    4                    CALL FLP$WR_FILE_INFO(DCB$,FIT_.PTR$,BLANK6,0,' ', ,'000000000000'O
             19036                             ,'1'B,'0'B);
      111    19037    4                 END;
      112    19038    3             END; /* NOT ERR */
      113    19039    2             CALL FLN$DISK_CLOSE_NEXT(DCB#);
      114    19040    2         END;/* DO WHILE ('1'B) */
      115    19041    1   DONE:
      116    19042    1         CALL FLN$DISK_CLOSE_ALL;
      117    19043    1         RETURN;
      118    19044    1   END FLR$REQUALIFY;

