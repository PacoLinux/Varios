    6      /* flags */
    7      #define   EM_PAR   0017  /* parameter type */
    8      #define   EM_FLO   0060  /* flow information */
    9
   10      /* types */
   11      #define PAR_NO  0000  /* no parameter */
   12      #define PAR_C   0001  /* constant */
   13      #define PAR_D   0002  /* double word constant */
   14      #define PAR_N   0003  /* numeric (>=0) */
   15      #define PAR_F   0004  /* address offset */
   16      #define PAR_L   0005  /* addressing locals/parameters */
   17      #define PAR_G   0006  /* addressing globals */
   18      #define   PAR_W 0007  /* size: word multiple, fits word, possibly indirect */
   19      #define PAR_S   0010  /* size: word multiple */
   20      #define PAR_Z   0011  /* size: zero or word multiple */
   21      #define PAR_O   0012  /* size: word multiple or word fraction */
   22      #define PAR_P   0013  /* procedure name */
   23      #define PAR_B   0014  /* branch: instruction label */
   24      #define PAR_R   0015  /* register number (0,1,2) */
   25
   26      /* flow */
   27      #define   FLO_NO   0000  /* straight on */
   28      #define   FLO_C 0020  /* conditional branch */
   29      #define   FLO_P 0040  /* procedure: call and return */
   30      #define   FLO_T 0060  /* terminate: no return */
   31

