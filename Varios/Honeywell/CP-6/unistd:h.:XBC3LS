   13      #include <fcntl.h>
   14
   15      #ifndef  __SEEK__
   16      #define  __SEEK__
   17      #define  SEEK_SET    0     /* Seek from beginning of file           */
   18      #define  SEEK_CUR    1     /* Seek from current position of file    */
   19      #define  SEEK_END    2     /* Seek backwards from EOF               */
   20      #endif
   21
   22      extern   int   access(char *, int);

