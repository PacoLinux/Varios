       89     6453    1   COMMON: ;
       90     6454    1        GD$MSG = '0'B;
       91     6455                                           /* Convert data descriptor to L6 format */
       92     6456    1        CALL DFD$CONVERT_DD(ADDR(GD$MSG.DISPMEM_D.DATA_DESC), S_D.DESC$, EXPROP) ALTRET
              6456                 (ARET);
       93     6457    1        IF EXPROP
       94     6458    2        THEN DO;
       95     6459    2             GD$MSG.DISPMEM_D.EXPR_OP = '1'B;
       96     6460    2             EXPR_OP = '1'B;
       97     6461    2           END;
       98     6462    1        ELSE EXPR_OP = '0'B;
       99     6463    1        ZITEM$ = S_D.DESC$;
      100     6464    1        S_D.DESC$ = PINCRW(S_D.DESC$, ZITEM.SIZE);
      101     6465                                                /* Set up message to send to FD */
      102     6466    1        GD$MSG.FNC = %GD#RQSDISPMEM_D;
      103     6467    1        GD$MSG.DISPMEM_D.DD_EXISTS = '1'B;
      104     6468    1        IF S_D.DO$ ~= ADDR(NIL) AND
PL6.E3A0      #001=DFK$EVAL File=DFK$EVAL.:E05TSI                                TUE 07/29/97 14:39 Page:4    
      105     6469    1          (S_D.DO$ -> ALL_ATTACH.CMD = 7 /* C_DUMP */ OR
      106     6470    1          S_D.DO$ -> ALL_ATTACH.CMD = 28 /* C_MODIFY */ OR
      107     6471    1          S_D.DO$ -> ALL_ATTACH.CMD = 38) /* C_MODIFYDW */
      108     6472    1        THEN GD$MSG.DISPMEM_D.DUMPCMD = '1'B;
      109     6473    1        IF S_D.DO$ -> ALL_ATTACH.CMD = 8 /* C_EVAL */
      110     6474    1        THEN GD$MSG.DISPMEM_D.EVALCMD = '1'B;
      111     6475    1        IF DISP
      112     6476    2        THEN DO;
      113     6477    2             GD$MSG.DISPMEM_D.REAL_SIZE = '1'B;
      114     6478    2             GD$MSG.DISPMEM_D.ARRAY_FIRST = S_D.ARRAY.FIRST;
      115     6479    2             GD$MSG.DISPMEM_D.ARRAY_CUR = S_D.ARRAY.CUR;
      116     6480    2             GD$MSG.DISPMEM_D.ARRAY_HIGH = S_D.ARRAY.HIGH;
      117     6481    2           END;
      118     6482
      119     6483                                                /* Write message to FD */
      120     6484    1        CALL DFQ$WRITE_FD (S_D.CURRENT_FPRG_CONTEXT$) ALTRET(ERR);
      121     6485                                                /* Read response from FD */
      122     6486    1        CALL DFQ$READ_FD (S_D.CURRENT_FPRG_CONTEXT$, '0'B) ALTRET(ERR);
      123     6487    1        IF GD$MSG.FNC ~= %GD#RSPDISPMEM
      124     6488    2        THEN DO;
      125     6489    3             DO CASE(GD$MSG.FNC);
      126     6490    3               CASE(%GD#RSPBADADR);
      127     6491    3                  S_D.ERROR = 83;   /* E@BADADDR */
      128     6492    3               CASE(%GD#RSPDD2CPLX);
      129     6493    3                  S_D.ERROR = 37; /* E@EXPR2CMPLX */
      130     6494    3               CASE(%GD#RSPDDNOTL6);
      131     6495    3                  S_D.ERROR = 527;  /* E@DDNOTFEP */
      132     6496    3               CASE(%GD#RSPUNSUPRT);
      133     6497    3                  S_D.ERROR = 503;    /* E@NOTIMP */
      134     6498    3               CASE(%GD#RSPBADPTR);
      135     6499    3                  S_D.ERROR = 104;    /* E@BADPTR */
      136     6500    3               CASE(%GD#RSPNOACCESS);
      137     6501    3                  S_D.ERROR = 78; /* E@ACCESS_FAULT */
      138     6502    3               CASE(ELSE);
      139     6503    3                  IF S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.XDELTA AND
      140     6504    3                    GD$MSG.FNC = %GD#RSPBADMSG
      141     6505    4                  THEN DO;
PL6.E3A0      #001=DFK$EVAL File=DFK$EVAL.:E05TSI                                TUE 07/29/97 14:39 Page:5    
      142     6506    4                       S_D.ERROR = 330; /* E@BADMSG */
      143     6507    4                       GOTO ARET;
      144     6508    4                     END;
      145     6509    3                  IF GD$MSG.FNC = %GD#RSPNAK
      146     6510    4                  THEN DO;
      147     6511    4                       IF S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.XDELTA
      148     6512    5                       THEN DO;
      149     6513    5                            TEMP1 = BITBIN(GD$MSG.ERROR_D.ERRCODE);
      150     6514    5                            TEMP2B.B1 = TEMP1B.B1;
      151     6515    5                            TEMP2B.B2 = TEMP1B.B2;
      152     6516    5                            TEMP2B.B3 = TEMP1B.B3;
      153     6517    5                            TEMP2B.B4 = TEMP1B.B4;
      154     6518    5                            S_D.JDE = TEMP2;
      155     6519    5                          END;
      156     6520    4                       ELSE S_D.JDE = BITBIN(GD$MSG_HOST.ERROR_D.ERRCODE);
      157     6521    4                       CALL DUE$GERRMSG;
      158     6522    4                       RETURN;
      159     6523    4                     END;
      160     6524    3                  REMEMBER NEVER IN S_D.SCREECH_IC;
      161     6525    3                  CALL M$SCREECH (SNAP_MESSAGE) ALTRET(NEVER);
      162     6526    3   NEVER:
      163     6527    3                  S_D.ERROR = 524; /* E@BADMESSAGE */
      164     6528    3                  CALL DDE$ERROR;
      165     6529    3                  S_D.ERROR = 502;    /* E@DELBUG */
      166     6530    3                  CALL DDE$ERROR;
      167     6531    3               END;
      168     6532    2             GOTO ARET;
      169     6533    2           END;
      170     6534    1        ADDRESS = GD$MSG.DISPMEM_D.ADDRESS;
      171     6535    1        SEG = GD$MSG.DISPMEM_D.SEG;
      172     6536    1        DESC# = GD$MSG.DISPMEM_D.DESC#;
      173     6537    1        NOT_IS = GD$MSG.DISPMEM_D.NOT_IS;
      174     6538    1        SIZE = GD$MSG.DISPMEM_D.SIZE;
      175     6539    1        IF ZITEM.PUP.BITS.PUB = 1          /* PACKED */
      176     6540    1        THEN SIZE = SIZE * 4;
      177     6541    1        S_D.IS_PTR = GD$MSG.DISPMEM_D.IS_PTR;
      178     6542    1        IF DISP
PL6.E3A0      #001=DFK$EVAL File=DFK$EVAL.:E05TSI                                TUE 07/29/97 14:39 Page:6    
      179     6543    1        THEN VALUE.SIZE = (SIZE + 15) / 16;
      180     6544    1        ELSE VALUE.SIZE = GD$MSG.DISPMEM_D.DATA_SIZE;
      181     6545    2        DO I = 0 TO VALUE.SIZE - 1;
      182     6546    2             VALUE.OPS(I) = GD$MSG.DISPMEM_D.DATA(I);
      183     6547    2           END;
      184     6548    1        IF DISP
      185     6549    2        THEN DO;
      186     6550    2             S_D.ARRAY.FIRST = '0'B;
      187     6551    2             S_D.ARRAY.CUR = GD$MSG.DISPMEM_D.ARRAY_CUR;
      188     6552    2             S_D.ARRAY.HIGH = GD$MSG.DISPMEM_D.ARRAY_HIGH;
      189     6553    2           END;
      190     6554                  /* Save 16 words, ADDRESS, SEG, DESC#, NOT_IS is S_D for L6 Cache */
      191     6555    1        IF CACHE
      192     6556    2        THEN DO;
      193     6557    2             S_D.MOD_L6.ADDRESS = ADDRESS;
      194     6558    2             S_D.MOD_L6.SEG = SEG;
      195     6559    2             S_D.MOD_L6.DESC# = DESC#;
      196     6560    2             S_D.MOD_L6.NOT_IS = NOT_IS;
      197     6561    2             S_D.MOD_L6.VALUE.SIZE = 16;
      198     6562    3             DO I = 0 TO 15;
      199     6563    3                  S_D.MOD_L6.VALUE.OPS(I) = VALUE.OPS(I);
      200     6564    3                END;
      201     6565    2           END;
      202     6566    1        RETURN;
      203     6567
      204     6568    1   ERR: ;
      205     6569    1        CALL DUE$ERRMSG;
      206     6570    1        S_D.ERROR = 318;          /* E@EVAL_ERROR */
      207     6571    1   ARET: ALTRETURN;
      208     6572    1   END DFK$EVAL;

