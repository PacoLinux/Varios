       48        8        %INCLUDE CP_6;
       49       89        %INCLUDE CP_6_SUBS;
       50      629        %INCLUDE B_ERRORS_C;
       51     1756        %INCLUDE XU_MACRO_C;
       52     4862
       53     4863        %B$TCB;
       54     4866        %B$ALT;
       55     4874        %XUR_INIT;
       56     4973        %FPT_MONINFO (FPTN=MONINFO,SITEINFO=SINFO);
       57     4988        %VLR_SITEINFO (FPTN=SINFO);
       58     5007
       59     5008    1   DCL B$TCB$ PTR SYMREF;
       60     5009    1   DCL M$ME DCB;
       61     5010    1   DCL FID$ PTR;
       62     5011
       63     5012    1   DCL 1 FUD BASED(FID$),
       64     5013    1       2 L UBIN BYTE UNAL,
       65     5014    1       2 C CHAR(FUD.L);
       66     5015
       67     5016    1   DCL 1 FID,
       68     5017    1       2 L UBIN BYTE UNAL,
       69     5018    1       2 C CHAR(512);
       70     5019
       71     5020    1   DCL DAM$DUMPFILE ENTRY(1) ALTRET;
       72     5021    1   DCL DAM$INIT ENTRY ALTRET;
       73     5022    1   DCL DBM$SELECT ENTRY(1) ALTRET;
       74     5023    1   DCL XUR$ERRMSG ENTRY(7) ALTRET;
       75     5024    1   DCL XUR$INIT ENTRY (3) ALTRET;
       76     5025    1   DCL XUR$ERRPTR ENTRY(2) ALTRET;
       77     5026
PL6.E3A0      #004=DAM_DUMPFILE File=TCD$DDU.:TCA2TSI                            SUN 11/16/97 14:09 Page:32   
       78     5027    1   DCL ALTRT SBIN WORD STATIC SYMDEF;
       79     5028    1   DCL 1 DMPVRS STATIC SYMDEF,
       80     5029    1       2 * CHAR(3) INIT(':DF') UNAL,
       81     5030    1       2 VERS CHAR(3) UNAL,
       82     5031    1       2 * CHAR INIT(%BINASC(0));
       83     5032
       84     5033    1         FID.L = FUD.L;
       85     5034    1         FID.C = FUD.C;
       86     5035    1         ALTRT = 0;
       87     5036    1         CALL DAM$DUMPFILE (FID) ALTRET(ERR);
       88     5037    1         CALL DBM$SELECT (2); /* select node 2 and ignore altret*/
       89     5038    1         RETURN;
       90     5039    1   ERR:
       91     5040    2         IF B$TCB$->B$TCB.ALT$->B$ALT.ERR.ERR# = %E$NOFILE THEN DO;
       92     5041    2              ALTRT = 2;
       93     5042    2              RETURN;
       94     5043    2         END;
       95     5044    1         CALL XUR$ERRMSG (B$TCB$->B$TCB.ALT$->B$ALT.ERR,,,,,,);
       96     5045    1         CALL M$EXIT;
       97     5046    1   DAM_ERR: ENTRY;
       98     5047    1         CALL XUR$ERRMSG (B$TCB$->B$TCB.ALT$->B$ALT.ERR,,,,,,);
       99     5048    1         RETURN;
      100     5049    1   DAM_INIT:  ENTRY;
      101     5050    1    CALL XUR$INIT(XUR_INIT);
      102     5051    1    CALL XUR$ERRPTR (,DCBNUM(M$ME));
      103     5052    1    CALL DAM$INIT;
      104     5053    1   CALL M$MONINFO(MONINFO);
      105     5054    1   DMPVRS.VERS=SINFO.VERSION#;
      106     5055    1    RETURN;
      107     5056    1   END;
