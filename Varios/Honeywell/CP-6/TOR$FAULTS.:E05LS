PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:31   
      768     8356        /*F*      NAME:     PROCESS_IO_REQ
      769     8357                  PURPOSE: CALLED BY TOR$MME_TADIOD TO PROCESS IO REQUESTS
      770     8358                           CURRENTLY IN  IO_BUFFER    STRUCTURE.
      771     8359                           FINDS BUFFER WITH I/O REQUEST WHICH HASN'T BEEN SERVICED
      772     8360                           AND SET IO_REQ_SERVICED_FLAG = 1.
      773     8361                           VERIFIES THAT LID FOR THIS REQUEST IS VALID.  IF NOT
      774     8362                           VALID THEN REQUEST IS NOT PROCESSED.
      775     8363                           CALLS  ERROR_MSG    TO OUTPUT MESSAGE IF E,S,M, OR
      776     8364                           P I/O REQUEST BITS ARE SET.  IF ANY OF THESE BITS
      777     8365                           ARE SET THEN REQUEST IS NOT PROCESSED.
      778     8366                           IF R BIT IS SET THEN SETS  IO_BUFFER.READ_FLAG = 1,
      779     8367                           AND CALLS  SERVICE_W_R_REQ.
      780     8368                           IF C BIT IS SET THEN C BIT IS RESET AND  SERVICE_WRITE
      781     8369                           _REQUEST IS CALLED.
      782     8370                           PROCESS IS REPEATED UNTIL ALL I/O BUFFERS HAVE BEEN
      783     8371                           SERVICED.
      784     8372                           RETURNS BACK TO TOR$MME_TADIOD.
      785     8373        */
      786     8374        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:32   
      787     8375    1   PROCESS_IO_REQ: PROC;
      788     8376
      789     8377    2      DCL I SBIN HALF STATIC INIT(-1);
      790     8378    2      DCL MSG_CODE SBIN HALF;
      791     8379    2      DCL TEMP_LID UBIN(18);
      792     8380
      793     8381        /* Code */
      794     8382
      795     8383    2      I = -1;                                              /*INITIALIZE THE COUNTER*/
      796     8384
      797     8385    2   START_LOOP:
      798     8386    2      IF I = 7 THEN GOTO GET_OUT ;
      799     8387    2      I = I+1;
      800     8388
      801     8389    2      IF  IO_BUFFER.BUF_AVAILIABLE_FLAG(I) = 0
      802     8390    2      THEN GOTO START_LOOP;
      803     8391
      804     8392    2      IF  IO_BUFFER.IO_REQ_SERVICED_FLAG(I) = 1
      805     8393    2      THEN GOTO START_LOOP;
      806     8394
      807     8395    2      IO_BUFFER.IO_REQ_SERVICED_FLAG(I) = 1;
      808     8396    2      TEMP_LID = IO_BUFFER.LID(I);
      809     8397    2      IF( IO_BUFFER.IO_REQ_BITS.E(I) = 1)|
      810     8398    2              ( IO_BUFFER.IO_REQ_BITS.M(I) = 1)|
      811     8399    2              ( IO_BUFFER.IO_REQ_BITS.S(I) = 1)|
      812     8400    2              ( IO_BUFFER.IO_REQ_BITS.P(I) = 1)
      813     8401    2      THEN CALL TOR$WRAPUP_PTR ;
      814     8402
      815     8403    2      IF IO_BUFFER.IO_REQ_BITS.R(I) = 1
      816     8404    3      THEN DO ;
      817     8405    3              IO_BUFFER.READ_FLAG(I) = 1;
      818     8406    3              IO_BUFFER.IO_REQ_BITS.R(I) = 0;
      819     8407    3              CALL TOR$WRAPUP_PTR ;
      820     8408    3              GOTO START_LOOP;
      821     8409    3           END;
      822     8410    2      ELSE IF IO_BUFFER.IO_REQ_BITS.C(I) = 1
      823     8411    3         THEN DO ;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:33   
      824     8412    3                 IO_BUFFER.IO_REQ_BITS.C(I) = 0;
      825     8413    3                 CALL SERVICE_WRITE_REQ(I);
      826     8414    3                 GOTO START_LOOP;
      827     8415    3              END;
      828     8416
      829     8417    2   GET_OUT:
      830     8418    2      RETURN;
      831     8419
      832     8420    2   END PROCESS_IO_REQ;
      833     8421        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:34   
      834     8422        /*F*      NAME:     SERVICE_WRITE_REQ
      835     8423                  PURPOSE: CALLED BY TOR$MME_TADIOD TO SERVICED WRITE REQUEST.
      836     8424                           SETS BOUNDS IN WRITE_BUF_VECTOR.
