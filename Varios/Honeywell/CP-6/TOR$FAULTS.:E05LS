       97     7641    1              3 C UBIN(1) UNAL,
       98     7642    1              3 P UBIN(1) UNAL,
       99     7643    1            2 WRITE_WD_CNT_BCD UBIN(12) UNAL,
      100     7644    1            2 CHAR_COUNT SBIN UNAL,
      101     7645    1            2 WRITE_DATA CHAR(1920) UNAL,
      102     7646    1            2 READ_DATA CHAR(84) UNAL;
      103     7647        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:5    
      104     7648        /* External Entries and Entry Pointers */
      105     7649
      106     7650    1      DCL TOT$ASCII_BCD              ENTRY (3) ;
      107     7651    1      DCL TOT$BCD_ASCII              ENTRY (3) ;
      108     7652    1      DCL TOR$CONSOLE_OUT_PTR        ENTRY ;
      109     7653    1      DCL TOR$WRAPUP_PTR             ENTRY ;
      110     7654    1      DCL TOR$EXIT_PTR               ENTRY ;
      111     7655
      112     7656        /* Code */
      113     7657
      114     7658    1      CALL GET_EXCEPTION_CODE (ECC);
      115     7659    2      DO CASE (ECC);
      116     7660    2         CASE (6);
      117     7661    2            CALL MME_HANDLER;
      118     7662    2         CASE (ELSE);
      119     7663    2            CALL TOR$WRAPUP_PTR ;
      120     7664    2      END /* CASE */ ;
      121     7665    1      CALL M$TRTN (TRTN_FPT) ;
      122     7666
      123     7667        /* END TOR$FAULTS */
      124     7668        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:6    
      125     7669        /*F* NAME:         GET_EXCEPTION_CODE
      126     7670             DESCRIPTION:
      127     7671                           ECC CODES AND SUB CODES--
      128     7672                           ECC = 0  (TIMER RUNOUT)
      129     7673                           ECC = 5   (ARITHMETIC FAULT)
      130     7674                              SUB_CODE = 0  OVERFLOW
      131     7675                              SUB_CODE = 1  DIVIDE CHECK
      132     7676                           ECC = 6  (PROGRAMMED FAULT)
      133     7677                              SUB_CODE = 0  MME
      134     7678                              SUB_CODE = 1  DERAIL
      135     7679                              SUB_CODE = 2  FAULT TAG
      136     7680                           ECC = 7  (ERRORS)
      137     7681                              SUB_CODE = 0  MEMORY
      138     7682                              SUB_CODE = 1  COMMAND
      139     7683                              SUB_CODE = 2  LOCKUP
      140     7684                              SUB_CODE = 3  ILLEGAL PROCEDURE
      141     7685                              SUB_CODE = 4  MISSING SEGMENT
      142     7686                              SUB_CODE = 5  MISSING PAGE
      143     7687                              SUB_CODE = 6  SECURITY 2
      144     7688        */
      145     7689
      146     7690    1   GET_EXCEPTION_CODE: PROC (ECC);
      147     7691
      148     7692    2      DCL ECC UBIN (18);
      149     7693    2      ECC = B$TCB$->B$TCB.STK$->B$EXCFR.ECC;
      150     7694    2      SUB_CODE = B$TCB$ -> B$TCB.STK$ -> B$EXCFR.SUBC;
      151     7695    2      RETURN;
      152     7696
      153     7697    2   END GET_EXCEPTION_CODE;
      154     7698        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:7    
      155     7699        /*F* NAME:         MME_HANDLER
      156     7700             DESCRIPTION:
      157     7701           NEXT, GO INTO THE EXCEPTION STACK FRAME AND FETCH THE MME DATA--
      158     7702           REGISTERS, THE MME INSTRUCTION, AND SOME WORDS IMMEDIATELY FOLLOWING
      159     7703           THE MME INSTRUCTION.  THIS IS DONE SO ALL THE MME SERVICE ROUTINES
      160     7704           DON'T HAVE TO DO IT INDIVIDUALLY.   GET_MME-DATA    DOES THIS.
      161     7705           USING THE MME TYPE NUMBER (FROM THE MME INSTRUCTION), CALL THE
      162     7706           APPROPRIATE MME SERVICE ROUTINE.  CALL EITHER TOG$GCOS_MMES
      163     7707           OR TOR$TURTLE_MMES OR TOR$TURTLE WHICH IN TURN CALLS THE SPECIFIC
      164     7708           SERVICE ROUTINE.
      165     7709           ON RETURN FROM THE SERVICE ROUTINE, RETURN THE MME DATA BACK TO THE
      166     7710           EXCEPTION STACK AND THE WORDS IMMEDIATELY FOLLOWING THE MME
      167     7711           INSTRUCTION.   RET_MME_DATA    DOES THIS.
      168     7712           FINALLY, RETURN IS MADE TO THE SUBEXEC BY CALLING M$TRTN.  THIS POPS
      169     7713           THE EXCEPTION STACK FRAME, RESTORES THE REGISTER ENVIRONMENT AND
      170     7714           RETURNS CONTROL TO WHATEVER WAS INTERRUPTETED.
      171     7715        */
      172     7716        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:8    
      173     7717    1   MME_HANDLER:PROC;
      174     7718
      175     7719    2      CALL GET_MME_DATA;
      176     7720    2      IF MME.INST.NO >  0
      177     7721    3      THEN DO CASE (MME.INST.NO);
      178     7722    3              CASE (%GESNAP);
      179     7723    3                 CALL TOR$MME_GESNAP(MME.WD_1,RETURN_DELTA);
      180     7724    3              CASE (%GETIME);
      181     7725    3                 CALL TOR$MME_GETIME(MREG.A,MREG.Q,RETURN_DELTA);
      182     7726    3              CASE(%GEROUT);
      183     7727    3                 CALL TOR$MME_GEROUT(MME.WD_1,MME.WD_2,RETURN_DELTA);
      184     7728    3              CASE (%GEWAKE);
      185     7729    3                 CALL TOR$MME_GEWAKE(MREG.Q,RETURN_DELTA);
      186     7730    3              CASE (%GEFINI);
      187     7731    3                 CALL TOR$EXIT_PTR ;
      188     7732    3              CASE (ELSE);
      189     7733    3                 CALL TOR$WRAPUP_PTR ;
      190     7734    3           END;
      191     7735    3      ELSE DO CASE (-MME.INST.NO);
      192     7736    3              CASE (%TADIOD);
      193     7737    3                 CALL TOR$MME_TADIOD(MREG.A,MME.WD_1,MME.WD_2,MME.WD_3,MME.WD_4,
      194     7738    3                         MME.WD_5,MME.WD_6,RETURN_DELTA);
      195     7739    3              CASE (%TERMIN) ;
      196     7740    3                 CALL TOR$EXIT_PTR ;
      197     7741    3              CASE (ELSE) ;
      198     7742    3                 CALL TOR$WRAPUP_PTR ;
      199     7743    3           END;
      200     7744    2      CALL RET_MME_DATA;
      201     7745    2      CALL FIX_RET_IC;
      202     7746    2      RETURN ;
      203     7747
      204     7748    2   END MME_HANDLER;
      205     7749        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:9    
      206     7750        /*F* NAME:         GET_MME_DATA
      207     7751             DESCRIPTION:
      208     7752           FETCHES MME DATA FROM EXCEPTION STACK FRAME.  RETURNS STRUCTURES MME
      209     7753           AND MREG, AND MME.INSTR$, POINTER TO OF MME INSTRUCTION.
      210     7754           B$TCB DEFINES TASK CONTROL BLOCK (TCB) AND INFORMATION ABOUT
      211     7755           CURRENT EXCEPTION STACK FRAME.  B$EXCFR  DEFINES CURRENT
      212     7756           EXCEPTION STACK FRAME.
      213     7757           B$TCB$ IS A LOADER BUILT POINTER TO THE TCB.
      214     7758        */
      215     7759
      216     7760    1   GET_MME_DATA:PROC;
      217     7761
      218     7762    2      DCL MME_GMAP_DATA (0:6) UBIN  (36) BASED;
      219     7763
      220     7764    2      MME_INSTR.ADR = B$TCB$->B$TCB.STK$->B$EXCFR.IC;
      221     7765    2      MME_INSTR.SEGID = %IS_SEGID;
      222     7766
      223     7767    2      MME = MME_INSTR$->MME_GMAP_DATA;
      224     7768
      225     7769    2      MREG = B$TCB$->B$TCB.STK$->B$EXCFR.REGS;
      226     7770
      227     7771    2      MME_IR = B$TCB$ -> B$TCB.STK$ -> B$EXCFR.IR;
      228     7772    2      RETURN;
      229     7773
      230     7774    2   END GET_MME_DATA;
      231     7775        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:10   
      232     7776        /*F* NAME:         FIX_RET_IC
      233     7777             DESCRIPTION:
      234     7778           FOLLOWING MME SERVICE, USES RETURN_DELTA TO FIX UP IC TO RETURN TO.
      235     7779           RETURN_DELTA IS NUMBER OF INSTRUCTIONS PAST MME INSTRUCTION TO
      236     7780           RETURN TO.  IT MAY BE POSITIVE OR NEGATIVE.  RETURN IC IS
      237     7781           CALCULATED BY ADDING RETURN DELTA TO MME INSTRUCTION IC.  RETURN
      238     7782           IC    IS STORED IN FPT.  CALL TO M$TRTN RETURNS IT TO EXCEPTION
      239     7783           STACK; CONTROL GOES TO RETURN IC WHEN STACK FRAME IS POPPED.
      240     7784        */
      241     7785
      242     7786    1   FIX_RET_IC:PROC;
      243     7787
      244     7788    2      TRTN_FPT.V.IC# = MME_INSTR.ADR + RETURN_DELTA;
      245     7789    2      RETURN;
      246     7790
      247     7791    2   END FIX_RET_IC;
      248     7792        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:11   
      249     7793        /*F* NAME:         RET_MME_DATA
      250     7794             DESCRIPTION:
      251     7795           RETURNS DATA FROM STRUCTURES MREG AND MME TO EXCEPTION STACK FRAME.
      252     7796           AND GMAP PROGRAM.  M$TRTN AUTOMATICALLY RETURNS DATA TO STACK
      253     7797           FRAME.  THIS ROUTINE RETURNS 6 WORDS TO THE LOCATIONS
      254     7798           IMMEDIATELY FOLLOWING THE MME INSTRUCTION.
      255     7799        */
      256     7800
      257     7801    1   RET_MME_DATA:PROC;
      258     7802
      259     7803    2      DCL MME_GMAP_DATA (0:6) UBIN (36) BASED;
      260     7804
      261     7805    2      TRTN_FPT.REGS_ =   VECTOR (MREG);
      262     7806
      263     7807    2      MME_INSTR$->MME_GMAP_DATA = MME;
      264     7808    2      RETURN;
      265     7809
      266     7810    2   END RET_MME_DATA;
      267     7811        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:12   
      268     7812        /*F*    NAME:    TOR$MME_GEROUT
      269     7813                PURPOSE: CALLED BY MME HANDLER TO SERVICE I/O
      270     7814                         REQUEST TO AND FROM THE TERMINAL.
      271     7815                         CHECKS THE BREAK FLAG. IF IT IS SET
      272     7816                         IT CALLS M$INT TO SET AND RESET THE
      273     7817                         BREAK_FPT. IT ALSO CALLS M$WAIT
      274     7818                         WHILE WAITING FOR A BREAK.
      275     7819                         CHECKS TO SEE IF ITS A WRITE OR A
      276     7820                         WRITE/READ. IT THEN CALLS M$WRITE
      277     7821                         OR M$READ.
      278     7822                         AFTER THE OPERATION IS COMPLETE IT
      279     7823                         FORMS THE STATUS AND SENDS IT BACK
      280     7824                         TO THE CALLER.
      281     7825        */
      282     7826        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:13   
      283     7827    1   TOR$MME_GEROUT :PROC(GEMAP_PAR,STATUS_TABLE,RETURN_DELTA);
      284     7828
      285     7829    2      DCL BRK_EXPECTED      BIT (1) STATIC SYMDEF CALIGNED INIT (%FALSE) ;
      286     7830    2      DCL GEMAP_PAR UBIN(36);
      287     7831    2      DCL 1 GEMAP_PAR_TABLE REDEF GEMAP_PAR,
      288     7832    2            2 BUFFER_ADD UBIN(18) UNAL,
      289     7833    2            2 OP_CODE UBIN(6) UNAL,
      290     7834    2            2 * UBIN (12) UNAL;
      291     7835    2      DCL STATUS_TABLE UBIN(36);
      292     7836    2      DCL 1 STATUS_TABLE_PTR REDEF STATUS_TABLE,
      293     7837    2            2 STATUS_ADD UBIN(18) UNAL,
      294     7838    2            2 * UBIN(18) UNAL;
      295     7839    2      DCL RETURN_DELTA SBIN HALF;
      296     7840    2      DCL ERRCODE UBIN(14) UNAL;
      297     7841    2      DCL TABLE$ PTR;
      298     7842    2      DCL 1 TABLE_PTR REDEF TABLE$,
      299     7843    2            2 ADR UBIN(18) UNAL,
      300     7844    2            2 SEGID UBIN(18) UNAL;
      301     7845    2      DCL 1 TABLE BASED ALIGNED,
      302     7846    2            2 WORD_CNT UBIN(18) UNAL,
      303     7847    2            2 INPUT_BUF UBIN(18) UNAL,
      304     7848    2            2 CHAR_CNT UBIN(18) UNAL,
      305     7849    2            2 * UBIN(17) UNAL,
      306     7850    2            2 BREAK_FLAG UBIN(1) UNAL,
      307     7851    2            2 WRITE_DATA(0:383) CHAR(1) UNAL;
      308     7852    2      DCL STATUS$ PTR;
      309     7853    2      DCL 1 STATUS_PTR REDEF STATUS$,
      310     7854    2            2 ADR UBIN(18) UNAL,
      311     7855    2            2 SEGID UBIN(18) UNAL;
      312     7856    2      DCL 1 STATUS BASED ALIGNED,
      313     7857    2            2 * UBIN(33) UNAL,
      314     7858    2            2 OP_COMPLETE UBIN(1) UNAL,
      315     7859    2            2 IO_ERROR UBIN(1) UNAL,
      316     7860    2            2 BREAK_REC UBIN(1) UNAL;
      317     7861        %FPT_WAIT(FPTN = BRK_WAIT_FPT,UNITS=10);
      318     7875        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:14   
      319     7876        /* Code */
      320     7877
      321     7878    2      TABLE_PTR.ADR = GEMAP_PAR_TABLE.BUFFER_ADD;
      322     7879    2      TABLE_PTR.SEGID = %IS_SEGID ;
      323     7880
      324     7881    2      STATUS_PTR.ADR = STATUS_TABLE_PTR.STATUS_ADD;
      325     7882    2      STATUS_PTR.SEGID = %IS_SEGID ;
      326     7883    2      IF TABLE$ -> TABLE.BREAK_FLAG = 1
      327     7884    2      THEN BRK_EXPECTED = %TRUE ;
      328     7885    2      ELSE BRK_EXPECTED = %FALSE ;
      329     7886
      330     7887    3      DO CASE (GEMAP_PAR_TABLE.OP_CODE) ;
      331     7888    3         CASE(3); /* WRITE */
      332     7889    3            CALL WRITE;
      333     7890    3         CASE(4); /* WRITE,READ */
      334     7891    3            CALL WRITE;
      335     7892    3            CALL READ;
      336     7893    3         CASE(ELSE);
      337     7894    3            CALL TOR$WRAPUP_PTR ;
      338     7895    3      END;
      339     7896
      340     7897    2      BRK_EXPECTED = %FALSE ;
      341     7898    2      RETURN_DELTA = 3;
      342     7899    2      RETURN;
      343     7900
      344     7901        /* END TOR$MME_GEROUT */
      345     7902        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:15   
      346     7903        /*F* NAME:         WRITE
      347     7904             DESCRIPTION:
      348     7905        */
      349     7906
      350     7907    2   WRITE : PROC;
      351     7908
      352     7909    3      DCL CHAR_COUNT UBIN(18);
      353     7910    3      DCL ASCII_OUTPUT_BFR(0:383) CHAR(1);
      354     7911    3      DCL 1 VFC_ STATIC,
      355     7912    3            2 * UBIN(2) UNAL INIT(0),
      356     7913    3            2 * UBIN(3) UNAL INIT(4),
      357     7914    3            2 * UBIN(4) UNAL INIT(0);
      358     7915    3      DCL VFC_BYTE REDEF VFC_ CHAR(1);
      359     7916    3      DCL N UBIN(36);
      360     7917
      361     7918        /* Code */
      362     7919
      363     7920    3      CHAR_COUNT = TABLE$->TABLE.CHAR_CNT;
      364     7921    3      ASCII_OUTPUT_BFR(0) = VFC_BYTE;
      365     7922    4      DO N = 1 TO CHAR_COUNT;
      366     7923    4         ASCII_OUTPUT_BFR(N) = TABLE$->TABLE.WRITE_DATA(N-1);
      367     7924    4      END;
      368     7925    3      WRITE_IO.BUF_ = VECTOR(ASCII_OUTPUT_BFR);
      369     7926    3      WRITE_IO.BUF_.BOUND = CHAR_COUNT;
      370     7927    3      CALL M$WRITE(WRITE_IO) ALTRET (WRITE_ERROR) ;
      371     7928    3      STATUS$->STATUS.OP_COMPLETE = 1;
      372     7929    3      RETURN;
      373     7930           %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:16   
      374     7931    3   WRITE_ERROR:
      375     7932    3      ERRCODE = B$TCB$->B$TCB.ALT$->B$ALT.ERR.CODE;
      376     7933    4      DO CASE(ERRCODE);
      377     7934    4         CASE(8);
      378     7935    4            STATUS$->STATUS.IO_ERROR = 1;
      379     7936    4         CASE(ELSE);
      380     7937    4            CALL TOR$WRAPUP_PTR ;
      381     7938    4      END;
      382     7939    3      RETURN;
      383     7940
      384     7941    3   END WRITE;
      385     7942        %EJECT;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:17   
      386     7943        /*F* NAME:         READ
      387     7944             DESCRIPTION:
      388     7945        */
      389     7946
