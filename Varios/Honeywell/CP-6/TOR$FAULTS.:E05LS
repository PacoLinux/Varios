       52     7551           %EJECT ;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:3    
       53     7552        /* TOLTS Macros */
       54     7553
       55     7554           %MME ;
       56     7575           %MREG ;
       57     7597           %MME_INSTR$ ;
       58     7600    1      DCL 1 MME_INSTR REDEF MME_INSTR$ ALIGNED,
       59     7601    1            2 ADR     UBIN HALF UNAL,
       60     7602    1            2 SEGID   UBIN HALF UNAL ;
       61     7603           %RETURN_DELTA ;
       62     7606
       63     7607        /* Variables */
       64     7608
       65     7609    1      DCL B$TCB$ PTR SYMREF;
       66     7610    1      DCL TOR$BREAK_OCCURRED BIT (1) SYMREF CALIGNED ;
       67     7611    1      DCL CHAR_COUNT SBIN;
       68     7612    1      DCL ECC UBIN (18);
       69     7613    1      DCL MME_IR BIT  (13) ALIGNED;
       70     7614    1      DCL SUB_CODE UBIN (36);
       71     7615    1      DCL DATE_BUFFER_ASCII(0:5) CHAR(1) STATIC;
       72     7616    1      DCL IONUM SBIN HALF STATIC INIT(0);
       73     7617           %EJECT ;
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:4    
       74     7618    1      DCL 1 WRITE_BUF_VECTOR STATIC DALIGNED,
       75     7619    1            2 BOUNDS UBIN(20) UNAL,
       76     7620    1            2 FLAGS BIT(9) UNAL INIT('777'O),
       77     7621    1            2 V BIT(2) UNAL INIT('01'B),
       78     7622    1            2 * UBIN(5) UNAL,
       79     7623    1            2 BASE UBIN(20) UNAL,
       80     7624    1            2 * UBIN (4) UNAL INIT(0),
       81     7625    1            2 SEGID BIT(12) UNAL INIT('6000'O);
       82     7626    1      DCL WRITE_BUF_VECTOR_V REDEF WRITE_BUF_VECTOR BIT (72);
       83     7627    1      DCL 1 IO_BUFFER(0:7) STATIC,
       84     7628    1            2 BUF_AVAILIABLE_FLAG UBIN(1) UNAL INIT(0*0),
       85     7629    1            2 IO_REQ_SERVICED_FLAG UBIN(1) UNAL INIT(0*0),
       86     7630    1            2 READ_FLAG UBIN(1) UNAL INIT(0*0),
       87     7631    1            2 TDPCN UBIN(18) UNAL,
