      752     8340    3            IF NUM_REQUESTS ~= 0
PL6.E3A0      #001=TOR$FAULTS File=TOR$FAULTS.:E05TSI                            WED 07/30/97 06:32 Page:30   
      753     8341    3            THEN GOTO ALLOC_IO_BUFFER;
      754     8342    4            ELSE DO;
