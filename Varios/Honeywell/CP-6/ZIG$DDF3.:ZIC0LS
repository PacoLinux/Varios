        7        7        WT_2STAR:
        8        8            PROC(TYPE,STRING);
        9        9    1   DCL ABORT ENTRY;
       10       10    1   DCL STRING CHAR(256) ALIGNED;
       11       11    1   DCL TYPE CHAR(2) ALIGNED;
       12       12    1   DCL OSTRING CHAR(256) EXT;
       13       13    1   DCL X UBIN (36);
       14       14    1   DCL 1 MCS REDEF X,
       15       15    1         2 FILL1 CHAR (2) UNAL,
       16       16    1         2 FILL2 CHAR (2) UNAL;
       17       17        %INCLUDE ZI_SUB4_CAN;
       18      674        %UWA;
       19     1009    1   DCL UPUT ENTRY(3) /*(CHAR(2) aligned,pointer,fixed bin) */;
       20     1010    1       X=136;
       21     1011    2       DO WHILE (SUBSTR(STRING,X,4)='    '&X>1);
       22     1012    2           X=X-4;
       23     1013    2           END;
       24     1014    1       X = X + 4;
       25     1015    1       CALL CONCAT(OSTRING,TYPE,MCS.FILL2,SUBSTR(STRING,0,X));
       26     1016    1       X=X/4+1;
       27     1017    1       CALL UPUT('2*',ADDR(OSTRING),X);
       28     1018    1       IF TYPE = 'DD'
       29     1019    1       THEN UWA.SA.NUMB_DD = UWA.SA.NUMB_DD + 1;
       30     1020    1   END WT_2STAR;

