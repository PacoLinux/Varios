      228    10777    1       CALL SETUP ALTRET(NO_MORE);
      229    10778        /*
      230    10779             ***** Print the definition ...
      231    10780        */
      232    10781    1       F_FDS.FMT_ = VECTOR(LIST_FMT);
      233    10782    1       F_FDS.NVECS# = 1;
      234    10783    2       DO FOREVER;
      235    10784    2           CALL VMA$GET_NEXT_ENTRY (VM_UADDRESS,VM_RADDRESS,NEXT$,VM_ADDLIST$) ALTRET(
             10784                    NO_MORE);
      236    10785    2           ADDRESS_TEXT.L# = LENGTHC(ADDRESS_TEXT.NAME#);
      237    10786    2           CALL VMF$MAKE_ADDRESS2 (ADDRESS_TEXT,VM_UADDRESS);
      238    10787    2           IF ADDRESS_TEXT.L# ~= 0
      239    10788    2           THEN
      240    10789    2               VECT_ARRAY(0) = VECTOR(SUBSTR(ADDRESS_TEXT.NAME#,0,ADDRESS_TEXT.L#));
      241    10790    2           ELSE
      242    10791    2               VECT_ARRAY(0) = VECTOR(NULL_CHARS);
      243    10792    2           CALL VMD$WRITE_BUF (F_FDS);
      244    10793    2       END;                            /* Do forever ... end */
      245    10794        /**/
      246    10795    1   NO_MORE:
      247    10796    1       IF VM_ADD_HEAD.NUM_ENTRIES ~= 0    /* if list contains no entries */
      248    10797    1       THEN
      249    10798    1           CALL VMD$WRITE_BUF (SPACE_LINE);
      250    10799    1       RETURN;
