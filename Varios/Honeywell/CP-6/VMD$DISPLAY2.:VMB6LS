PL6.E3A0      #001=VMD$DI_LIST File=VMD$DISPLAY2.:VMB6TSI                        WED 08/20/97 15:10 Page:9    
      252    10801        /*F*
      253    10802             NAME: VMD$DI_USER
      254    10803             PURPOSE:
      255    10804                   To display the definition of a specified USER.
      256    10805             DESCRIPTION:
      257    10806                   This routine expects the definition to be stored in
      258    10807                   the data segment based upon VM_ADDLIST$.
      259    10808                   It also expects the name of the USER to be passed as
      260    10809                   a parameter in the form of VM_USERKEY.
      261    10810
      262    10811                   The following parameters are required:
      263    10812                      VM_USERKEY   - key used to read the definition.
      264    10813                      DCB#         - number of the DCB to display thru.
      265    10814        */
      266    10815    1   VMD$DI_USER: ENTRY (VM_LISTKEY,DCB#);
      267    10816    1       CALL SETUP ALTRET(NO_MORE);
      268    10817                       /* Only display account info if user has access to the global
      269    10818                          file other than via processor priv */
      270    10819    1       IF DCBADDR(M$MAIL_GLOBAL_NUM)->F$DCB.FFLG.EXEC#
      271    10820    1       THEN
      272    10821    1           GOTO NO_MORE_DI_USER;
      273    10822        /*
      274    10823             ***** Print the definition ...
      275    10824        */
      276    10825    1       F_FDS.FMT_ = VECTOR(USER_FMT);
      277    10826    1       F_FDS.NVECS# = 3;
      278    10827    2       DO FOREVER;
      279    10828    2           CALL VMA$GET_NEXT_ENTRY (VM_UADDRESS,VM_RADDRESS,NEXT$,VM_ADDLIST$) ALTRET(
             10828                    NO_MORE_DI_USER);
      280    10829        /*
      281    10830             ***** Break up the NAME_KEY into accn and uname ...
      282    10831        */
      283    10832    2           ACCN = SUBSTR(VM_RADDRESS.NAME_KEY,0,8);
      284    10833    2           UNAME = SUBSTR(VM_RADDRESS.NAME_KEY,8);
      285    10834    2           VECT_ARRAY(0) = VECTOR(ACCN);
      286    10835    2           VECT_ARRAY(1) = VECTOR(UNAME);
      287    10836    2           ADDRESS_TEXT.L# = LENGTHC(ADDRESS_TEXT.NAME#);
PL6.E3A0      #001=VMD$DI_LIST File=VMD$DISPLAY2.:VMB6TSI                        WED 08/20/97 15:10 Page:10   
      288    10837    2           CALL VMF$MAKE_ADDRESS2 (ADDRESS_TEXT,VM_UADDRESS);
      289    10838    2           IF ADDRESS_TEXT.L# ~= 0
      290    10839    2           THEN
      291    10840    2               VECT_ARRAY(2) = VECTOR(SUBSTR(ADDRESS_TEXT.NAME#,0,ADDRESS_TEXT.L#));
      292    10841    2           ELSE
      293    10842    2               VECT_ARRAY(2) = VECTOR(NULL_CHARS);
      294    10843    2           CALL VMD$WRITE_BUF (F_FDS);
      295    10844    2       END;                            /* Do forever ... end */
      296    10845    1   NO_MORE_DI_USER:
      297    10846    1       RETURN;
      298    10847        %EJECT;
PL6.E3A0      #001=VMD$DI_LIST File=VMD$DISPLAY2.:VMB6TSI                        WED 08/20/97 15:10 Page:11   
      299    10848    1   SETUP: PROC ALTRET;
      300    10849        /*
      301    10850             ***** Setup the name of the list in
      302    10851                   an easier to use form (i.e. TEXTC)...
      303    10852        */
      304    10853    2       LISTNAME.NAME# = VM_LISTKEY.TEXT;
      305    10854    2       LISTNAME.L# = LENGTHC(LISTNAME.NAME#);
      306    10855    2       CALL VMA$COMPRESS_BLANKS (ADDR(LISTNAME));
      307    10856        /*
      308    10857             ***** If this list contains no entries,
      309    10858                   then report as an error ...
      310    10859        */
      311    10860    2       IF VM_ADD_HEAD.NUM_ENTRIES = 0
      312    10861    2       THEN
      313    10862    3       DO;
      314    10863    3           CALL XUR$ERRMSG (E_EMPTYDEF,,DCB#,VECTOR(LISTNAME));
      315    10864    3           ALTRETURN;
      316    10865    3       END;
      317    10866        /*
      318    10867             ***** Initialize ...
      319    10868        */
      320    10869    2       SPACE_LINE.DCB# = DCB#;
      321    10870    2       F_FDS.DCB# = DCB#;
      322    10871    2       NEXT$ = VM_ADD_HEAD.HEAD$;
      323    10872        /*
      324    10873             ***** Print a blank line before (and after) this display ...
      325    10874        */
      326    10875    2       CALL VMD$WRITE_BUF (SPACE_LINE);
      327    10876        /*
      328    10877             ***** Print the heading ...
      329    10878        */
      330    10879    2       VECT_ARRAY(0) = VECTOR(SUBSTR(LISTNAME.NAME#,0,LISTNAME.L#));
      331    10880    2       IF ADDR(SRCH#) ~= ADDR(NIL)
      332    10881    3       THEN DO;
      333    10882    3           IF VM_SEARCH_INFO.DCB#(SRCH#) = M$MAIL_FILE_NUM
      334    10883    4           THEN DO;
      335    10884    4               F_FDS.NVECS# = 1;
PL6.E3A0      #001=VMD$DI_LIST File=VMD$DISPLAY2.:VMB6TSI                        WED 08/20/97 15:10 Page:12   
      336    10885                    %INIT_STR(STR=VM_HEADER_FMT2,CODE=E_HEADER_FMT2,TEST=YES,VECT=
             10885                        "F_FDS.FMT_");
      337    10908    4           END;
      338    10909    3           ELSE IF VM_SEARCH_INFO.DCB#(SRCH#) = M$MAIL_GLOBAL_NUM
      339    10910    4               THEN DO;
      340    10911    4                   F_FDS.NVECS# = 2;
      341    10912                        %INIT_STR(STR=VM_GLOBAL_TEXT,CODE=E_GLOBAL_TEXT,TEST=YES,VECT=
             10912                            "VECT_ARRAY(1)");
      342    10935                        %INIT_STR(STR=VM_HEADER_FMT_LIST,CODE=E_HEADER_FMT_LIST,TEST=YES,
             10935                            VECT="F_FDS.FMT_");
      343    10958    4               END;
      344    10959    4               ELSE DO;
      345    10960    4                   ADDRESS_TEXT.L# = LENGTHC(ADDRESS_TEXT.NAME#);
      346    10961    4                   CALL VMF$MAKE_ADDRESS2 (ADDRESS_TEXT,
      347    10962    4                     VM_PROFILE.SEARCH_LIST$(VM_SEARCH_INFO.MAP(SRCH#))->VM$RADDRESS);
      348    10963    4                   VECT_ARRAY(1) = VECTOR(SUBSTR(ADDRESS_TEXT.NAME#,0,ADDRESS_TEXT.L#))
             10963                            ;
      349    10964    4                   F_FDS.NVECS# = 2;
      350    10965                        %INIT_STR(STR=VM_HEADER_FMT_LIST,CODE=E_HEADER_FMT_LIST,TEST=YES,
             10965                            VECT="F_FDS.FMT_");
      351    10988    4               END;
      352    10989    3           CALL VMD$WRITE_BUF (F_FDS);
      353    10990    3       END;
      354    10991    3       ELSE DO;
      355    10992    3           F_FDS.NVECS# = 1;
      356    10993                %INIT_STR(STR=VM_HEADER_FMT2,CODE=E_HEADER_FMT2,TEST=YES,VECT="F_FDS.FMT_");
      357    11016    3           CALL VMD$WRITE_BUF (F_FDS);
      358    11017    3       END;
      359    11018    2       RETURN;
      360    11019    2   END SETUP;
      361    11020    1   END VMD$DI_LIST;
