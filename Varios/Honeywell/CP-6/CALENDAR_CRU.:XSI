!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!JOB NAME=CALENDAR_CRU,WSN=LOCAL
!RESOURCE TIME=8,MEM=256
!ROUTE Jerry Schutz, LADC
!COPY CALENDAR_C1.SIACCT TO ME(K)
!COPY CALENDAR_C2.SIACCT TO ME(K)
!PARTRGE CALENDAR_SIN1.SIACCT OVER *CALENDAR_OUN1.GNACCT,LP (SR(.SIACCT,.LBACCT))
!PL6 CALENDAR_SI61.SIACCT OVER *CALENDAR_OU61.GNACCT,LP (SCHEMA,SR(.SIACCT,.LBACCT),MXR)
!PL6 CALENDAR_SI62.SIACCT OVER *CALENDAR_OU62.GNACCT,LP (SCHEMA,SR(.SIACCT,.LBACCT),MXR)
!EJECT.GNACCT LP
!LINK B_COPYRIGHT.LBACCT, ;
     *CALENDAR_OUN1.GNACCT,;
     *CALENDAR_OU61.GNACCT,;
     *CALENDAR_OU62.GNACCT ;
     OVER CALENDAR.GNACCT(IP=50,DP=10,STDINVOC,DCB1=M$CALENDAR)
!EXTRACT.XACCT
DA *CALENDAR.GNACCT
EX CALENDAR_SI61.SIACCT
EX CALENDAR_SI62.SIACCT
BU INTO CALENDAR.GNACCT,CALENDAR_C2.SIACCT PRO ,NO,LP
END
!MOD CALENDAR.GNACCT TO (ACS((CALENDAR.XACCT),READ))
