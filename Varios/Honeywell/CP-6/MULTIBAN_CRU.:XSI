!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI,COUACCT=:XGD0OU
!JOB  NAME=MULTIBAN,WSN=LOCAL
!RESOURCE TIME=15,MEM=256
!ROUTE Jerry Waldorf, LADC
!"
!"  By default, the output created by this job is EDGEMARKed.
!"  However, if you do NOT want it EDGEMARKed, do a DEST=LP substitution on
!"  your BATCH or XEQ command such as:
!"    !BATCH MULTIBAN_CRU.XACCT DEST=LP
!"
!DEFAULT DEST=*LS
!IF $FID_EXIST('TO.:SYS') & $MODE='BATCH' THEN TO AC=%$ACCT Your MULTIBAN compile just started.
!LET LISTING = 'DEST'
!LET MONNUM=$SUBSTR($DATE,2,2)
!LET MONTHS='JanFebMarAprMayJunJulAugSepOctNovDec'
!LET RIGHT=$SUBSTR(MONTHS,(MONNUM*3)-3,3) || ' ' || $SUBSTR($DATE,4,2)
!IF $SUBSTR(RIGHT,4,1) ~= '0' THEN GOTO DATE_OK
!LET RIGHT= $SUBSTR(RIGHT,0,4) || $SUBSTR(RIGHT,5,1)
!DATE_OK:
!PL6 MULTIBAN_SI61.SIACCT OVER *MULTIBAN_OU6.GNACCT,DEST (OU,LS,SCHEMA,SR(.SIACCT,.LBACCT))
!PL6 MULTIBAN_SI62.SIACCT INTO *MULTIBAN_OU6.GNACCT,DEST (OU,LS,SCHEMA,SR(.SIACCT,.LBACCT))
!PARTRGE MULTIBAN_SIN.SIACCT INTO *MULTIBAN_OU6.GNACCT,DEST (LS,NSYS,SR(.SIACCT,.LBACCT))
!IF LISTING = 'LP' THEN GOTO DONT_EDGEMARK
!EDGEMARK.XACCT DEST TO LP@(F='PERF',L='MULTIBAN.X',R='%RIGHT')
!DONT_EDGEMARK:
!LINK B_COPYRIGHT.LBACCT, ;
      *MULTIBAN_OU6.GNACCT ;
 OVER MULTIBAN.GNACCT
!IF $FID_EXIST('TO.:SYS') & $MODE='BATCH' THEN TO AC=%$ACCT Your MULTIBAN compile just finished.
