      427     8519    1      PROCESS_ARRAY_VALUED:  PROC;
      428     8520         /*====================*/
      429     8521
      430     8522
      431     8523    2      IF ST$$ -> ST.ID = %ARRAY THEN
      432     8524    3        DO;   /* calculate the actual offset */
      433     8525    3          OFFSET = - DIMS_LTS$ -> DIMS.OD.TOTAL_SPAN;
      434     8526
      435     8527    4          DO J = 1 TO NO_DIMS#;
      436     8528    4            OFFSET = OFFSET + (DIMS_LTS$ -> DIMS.D.SPAN(J) * U_INDEX(J));
      437     8529    4          END;
      438     8530
      439     8531                   /* calculate the SA, CN */
      440     8532
      441     8533    4          DO CASE (ST$$ -> ST.TYPE);
