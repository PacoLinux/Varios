      648    10738             %INCLUDE Z8E_MACROS;
      649    11413
      650    11414    1        DCL P$            PTR;
      651    11415
      652    11416    1        DCL Z8E8$NEW_CSIB ENTRY (1);
      653    11417
      654    11418
      655    11419             /*
      656    11420               The purpose of this routine is to walk the proc chain
      657    11421               at the brother level and create the CSIB chain for the
PL6.E3A0      #002=Z8E8$CSIB File=Z8E8.:Z8A6TSI                                  SAT 08/09/97 11:40 Page:99   
      658    11422               runtime.
      659    11423               The CSIB looks like this:
      660    11424
      661    11425                   dcl 1 csib                aligned,
      662    11426                         2 name_length       sbin,
      663    11427                         2 name_text         char (csib.name_length),
      664    11428                         2 brother$          ptr,
      665    11429                         2 son$              ptr,
      666    11430                         2 father$           ptr,
      667    11431                         2 init_proc$        eptr,
      668    11432                         2 flags             ubin,
      669    11433                         2 b                 redef flags,
      670    11434                           3 {bits}          (0:35) boolean;
      671    11435
      672    11436               This structure is recreated via scalar/qual items, and
      673    11437               preset to represent the internal and external procedure
      674    11438               skeleton.
      675    11439               The brothers are walked at this level to avoid connecting
      676    11440               the consecutive externally visible procedures in the CSIB.
      677    11441
      678    11442             */
      679    11443
      680    11444
      681    11445             %NEW_ILL (@ID    = %SCOPE,
      682    11446                       @CODE  = %INIT_SCOPE,
      683    11447                       @PROC$ = "ADDR(NIL)");
      684    11826
      685    11827    1        P$ = COMCOM.FIRST_PROC$;
      686    11828    1        O1COM.CURRENT_PROC$ = P$;
      687    11829
      688    11830    2        DO WHILE (P$ ~= ADDR(NIL));
      689    11831
      690    11832    2          CALL Z8E8$NEW_CSIB (P$);
      691    11833
      692    11834    2          P$ = P$ -> PROC_.BROTHER;
      693    11835
      694    11836    2        END; /* While */
PL6.E3A0      #002=Z8E8$CSIB File=Z8E8.:Z8A6TSI                                  SAT 08/09/97 11:40 Page:100  
      695    11837
      696    11838    1        O1COM.CURRENT_PROC$ = ADDR(NIL);
      697    11839
      698    11840    1   END Z8E8$CSIB;
