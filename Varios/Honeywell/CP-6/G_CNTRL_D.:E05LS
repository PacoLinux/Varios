        8        8
        9        9                            /*F* NAME: G_CNTRL_D
       10       10
       11       11                            The module G_CNTRL_D contains most monitor data
       12       12                            that may be modified or displayed by the control
       13       13                            function of NETCON and a table of pointers that
       14       14                            locate the data.
       15       15                            */
       16       16        G_CNTRL_D: PROC;
       17       17
       18       18        %INCLUDE GI_BOOT_C;
       19      129        %INCLUDE G_CNTRL_M;
       20      202
       21      203    1   DCL GH_KEEPDELTA UBIN SYMREF;
       22      204    1   DCL GJ_SFILE_MAX_DATA UBIN SYMREF;
       23      205        %LIST;
       24      206
       25      207        %G_CNTRL_TBL (FPTN=G_CNTRL_TBL,
       26      208                      STCLASS="CONSTANT SYMDEF",
       27      209                      BUILD_TBL=YES,
       28      210                      HI_MODIFY=GI_CNTL_BOOTHI,
       29      211                      HI_DISP=GI_CNTL_DISPHI);
               212        %LSET LISTDIR='0'B;
               224    1   DCL 1 G_CNTRL_TBL CONSTANT SYMDEF WALIGNED,
               225    1         2 HI_MODIFY UBIN  INIT(20),
               226                            /*K* HI_MODIFY - UBIN.
               227                            Contains the highest index into G_CNTRL_TBL.PTRS
               228                            for the parameters that may be modified.
               229                            */
               230    1         2 HI_DISP UBIN  INIT(29),
PL6.E3A0      #001=G_CNTRL_D File=G_CNTRL_D.:E05TSI                              TUE 07/29/97 22:17 Page:2    
               231                            /*K* HI_DISP - UBIN.
               232                            Contains the highest index into G_CNTRL_TBL.PTRS
               233                            for the parameters that may be displayed.
               234                            */
               244    1         2 PTR$(0:29) PTR  INIT(
               245    1              ADDR(GH_QMIN),
               246    1              ADDR(GH_QUAN),
               247    1              ADDR(G_XCONTIME),
               248    1              ADDR(G_MAX_USRS(0)),
               249    1              ADDR(G_MAX_USRS(1)),
               250    1              ADDR(G_MAX_USRS(2)),
               251    1              ADDR(G_MAX_USRS(3)),
               252    1              ADDR(G_MAX_USRS(4)),
               253    1              ADDR(GFR_MAX_MEM(0)),
               254    1              ADDR(GFR_MAX_MEM(1)),
               255    1              ADDR(GFR_MAX_MEM(2)),
               256    1              ADDR(GFR_MAX_MEM(3)),
               257    1              ADDR(GFR_MAX_MEM(4)),
               258    1              ADDR(GI_DUMP),
               259    1              ADDR(GH_KEEPDELTA),
               260    1              ADDR(GJ_SFILE_MAX_DATA),
               261    1              ADDR(NIL),
