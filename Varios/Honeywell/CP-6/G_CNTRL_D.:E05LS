PL6.E3A0      #001=G_CNTRL_D File=G_CNTRL_D.:E05TSI                              TUE 07/29/97 22:17 Page:4    
       33      280                            /* Number of users                                        */
       34      281
       35      282    1   DCL G_MAX_USRS (0:4) UBIN WORD STATIC SYMDEF
       36      283    1            INIT(%GI_MUSR#,
       37      284    1                 %GI_MUSR_USER#,
       38      285    1                 %GI_MUSR_CG#,
       39      286    1                 %GI_MUSR_HAND#,
       40      287    1                 %GI_MUSR_GHST#);
       41      288                            /*K* G_MAX_USRS - ARRAY(0:4) UBIN.
       42      289                            This array is indexed by G$JIT.MODE.  Each
       43      290                            entry in the array contains the maximum number
       44      291                            of users that may be started for each user
       45      292                            mode.  The zero-th entry contains the maximum
       46      293                            number of users of all modes allowed on the
       47      294                            system.
       48      295                            */
       49      296
       50      297    1   DCL G_CUR_USRS (0:4) UBIN WORD STATIC SYMDEF INIT(0*0);
       51      298                            /*K* G_CUR_USRS - ARRAY(0:4) UBIN.
       52      299                            This array is indexed by G$JIT.MODE.  Each
       53      300                            entry in the array contains the current number
       54      301                            of users that have been started for each user
       55      302                            mode.  The zero-th entry contains the current
       56      303                            number of users of all modes currently on the
       57      304                            system.
       58      305                            */
       59      306
       60      307        %EJECT;
PL6.E3A0      #001=G_CNTRL_D File=G_CNTRL_D.:E05TSI                              TUE 07/29/97 22:17 Page:5    
       61      308                            /* Memory                                                 */
       62      309
       63      310    1   DCL GFR_MAX_MEM (0:4) UBIN WORD STATIC SYMDEF
       64      311    1            INIT(%GI_MMEM#,
       65      312    1                 %GI_MMEM_USER#,
       66      313    1                 %GI_MMEM_CG#,
       67      314    1                 %GI_MMEM_HAND#,
       68      315    1                 %GI_MMEM_GHST#);
       69      316                            /*K* GFR_MAX_MEM - ARRAY(0:4) SBIN.
       70      317                            This array is indexed by G$JIT.MODE.  Each
       71      318                            entry in the array contains the maximum number
       72      319                            of pages that may be allocated for each user
       73      320                            mode.  The zero-th entry contains the maximum
       74      321                            amount of memory that may be allocated for all
       75      322                            users.
       76      323                            */
       77      324
       78      325    1   DCL GFR_CUR_MEM (0:4) UBIN WORD STATIC SYMDEF INIT(0*5);
       79      326                            /*K* GFR_CUR_MEM - ARRAY(0:4) SBIN.
       80      327                            This array is indexed by G$JIT.MODE.  Each
       81      328                            entry in the array contains the current number
       82      329                            of pages that are allocated for each user
       83      330                            mode.  The zero-th entry contains the current
       84      331                            amount of memory that is allocated for all
       85      332                            users.
       86      333                            */
       87      334
       88      335
       89      336        %EJECT;
