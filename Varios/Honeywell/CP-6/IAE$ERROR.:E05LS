PL6.E3A0      #001=IAE$ERROR File=IAE$ERROR.:E05TSI                              TUE 07/29/97 22:26 Page:1    
        1        1        /*M* IAE$ERROR - Routines to output GOOSE error msgs. */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7
        8        8        IAE$ERROR: PROC (ON_LINE, GOOSE_ERROR, SYSTEM_ERROR, KEY, TEXT$);
        9        9
       10       10        /*P*
       11       11             NAME: IAE$ERROR
       12       12             PURPOSE:
       13       13                  To report GOOSE errors to the proper place(s).
       14       14             DESCRIPTION:
       15       15                  This module contains three routines:
       16       16
       17       17                                 IAE$ERROR
       18       18                                 IAE$CSCG_ERROR
       19       19                                 IAE$ONLINE_ERROR
       20       20
       21       21                  The first routine is called by GOOSE when it doesn't know
       22       22                  from context which of the actual error reporting mechanisms
       23       23                  to use.
       24       24
       25       25                  IAE$CSCG_ERROR is called to report an error to the
       26       26                  operator's console.
       27       27
       28       28                  IAE$ONLINE_ERROR is called to report an error to the
       29       29                  online users terminal.
       30       30        */
       31       31        %EJECT
       32       32        /*F*
       33       33             NAME: IAE$ERROR
       34       34             PURPOSE:
       35       35                  To decide which of the actual error reporting mechanisms to use.
       36       36             DESCRIPTION:
       37       37                  This routine checks the ON_LINE flag to decide whether to
PL6.E3A0      #001=IAE$ERROR File=IAE$ERROR.:E05TSI                              TUE 07/29/97 22:26 Page:2    
       38       38                  call IAE$CSCG_ERROR or IAE$ONLINE_ERROR.
       39       39        */
       40       40        /*D*
       41       41             NAME: IAE$ERROR
       42       42             CALL:
       43       43                  CALL IAE$ERROR (ON_LINE, GOOSE_ERROR, SYSTEM_ERROR, KEY, TEXT$);
       44       44             PARAMETERS:
       45       45                  ON_LINE - flag - set if the current GOOSE user is online
       46       46                                 - reset if the current GOOSE user is a ghost job.
       47       47
       48       48                  GOOSE_ERROR - the GOOSE error to be reported
       49       49
       50       50                  SYSTEM_ERROR - the monitor service or XUG$ error to be reported.
       51       51
       52       52                  KEY          - the message key to use when reporting an error to
       53       53                                 the operator's console.
       54       54
       55       55                  TEXT$        - a pointer to the text of the GOOSE command that
       56       56                                 caused the error.  To be used when reporting
       57       57                                 an error to the operator's console.
       58       58             INTERFACE:
       59       59                  calls:
       60       60                     IAE$CSCG_ERROR
       61       61                     IAE$ONLINE_ERROR
       62       62
       63       63                  called by:
       64       64                     PARSE$COMMAND
       65       65                     DO$COMMAND
       66       66
       67       67             ENVIRONMENT:
       68       68                  None.
       69       69             INPUT:
       70       70                  ON_LINE
       71       71                  GOOSE_ERROR
       72       72                  SYSTEM_ERROR
       73       73                  KEY
       74       74                  TEXT$
PL6.E3A0      #001=IAE$ERROR File=IAE$ERROR.:E05TSI                              TUE 07/29/97 22:26 Page:3    
       75       75             OUTPUT:
       76       76                  None.
       77       77             SCRATCH:
       78       78                  None.
       79       79             DESCRIPTION:
       80       80                  This routine checks ON_LINE to decide which actual error
       81       81                  reporting mechanism to use.  If ON_LINE is set then
       82       82                  we call IAE$ONLINE_ERROR, else we call IAE$CSCG_ERROR.
       83       83        */
       84       84
       85       85        /*
       86       86          Parameters
       87       87        */
       88       88
       89       89    1   DCL  ON_LINE   BIT(1) ALIGNED;
       90       90    1   DCL  GOOSE_ERROR  UBIN;
       91       91    1   DCL  SYSTEM_ERROR  BIT(36) ALIGNED;
       92       92    1   DCL  KEY          UBIN;
       93       93    1   DCL  TEXT$       PTR;
       94       94
       95       95        /*
       96       96             Includes
       97       97        */
       98       98
       99       99        %INCLUDE CP_6;
      100     5658
      101     5659        /*
      102     5660           Auto variables
      103     5661        */
      104     5662
      105     5663    1   DCL ERROR BIT(36);
      106     5664
      107     5665        /*
      108     5666           Fpt's
      109     5667        */
      110     5668
      111     5669
PL6.E3A0      #001=IAE$ERROR File=IAE$ERROR.:E05TSI                              TUE 07/29/97 22:26 Page:4    
      112     5670        /*
      113     5671           Entry points
      114     5672        */
      115     5673
      116     5674    1   DCL IAE$ONLINE_ERROR  ENTRY (2);
      117     5675    1   DCL IAE$CSCG_ERROR  ENTRY (3);
      118     5676
      119     5677        /*
      120     5678             start of IAE$ERROR main procedure
      121     5679        */
      122     5680
      123     5681    1    IF ON_LINE
      124     5682    1    THEN
      125     5683    2      DO;
      126     5684    2         CALL IAE$ONLINE_ERROR (GOOSE_ERROR, SYSTEM_ERROR);
      127     5685    2             END;
      128     5686    1    ELSE
      129     5687    2       DO;
      130     5688    2         IF GOOSE_ERROR = 0
      131     5689    2         THEN
      132     5690    3           DO;
      133     5691    3            ERROR = SYSTEM_ERROR;
      134     5692    3            END;
      135     5693    2         ELSE
      136     5694    3            DO;
      137     5695    3              ERROR = BINBIT(GOOSE_ERROR,36);
      138     5696    3           END;   /*  GOOSE ERROR  */
      139     5697    2         CALL IAE$CSCG_ERROR (KEY,ERROR,TEXT$);
      140     5698    2      END;  /* NOT ONLINE */
      141     5699
      142     5700    1        RETURN;
      143     5701
      144     5702    1   END IAE$ERROR;
      145     5703        %EOD;

