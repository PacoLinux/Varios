    8      /* ERROR CODES 1000-1049 AVAILABLE */
    9      %EQU E$NTRY2BIG=1000;              /* SYSTEM LOG RECORD EXCEEDS 1000 WORDS    */
   10      %EQU E$SLBUSY=1001;                     /* SYSTEM LOG IS BUSY                 */
   11      %EQU E$NEEDPAGE=1002;                   /* NO BUFFER SPACE FOR SYSTEM LOG     */
   12      %EQU E$SLFILE=1003;                     /* FILE NUMBER MAY NOT EXCEED 99      */

