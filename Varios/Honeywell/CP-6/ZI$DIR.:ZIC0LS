    12 - %MACRO DIR(STCLASS=SYMREF,DIRNAME=ZI$DIR);
    13 - %IF TEXTCHAR(STCLASS)='SYMREF';
    14 -     %LSET INIT=CHARTEXT('/*');
    15 - %ELSE;
    16 -     %LSET INIT=CHARTEXT('INIT');
    17 - %ENDIF;
    18 - DCL 1 DIRNAME STCLASS ALIGNED,          /* DEFAULT TO  ZI$DIR SYMREF          */
    19 -       2 RNG_LIST BIT(1) UNAL %INIT('0'B)%CHARTEXT('/**/'), /* 1 if ranges set*/
    20 -       2 INITL BIT(1) UNAL %INIT('0'B)%CHARTEXT('/**/'), /* 1 if initialization*/
    21 -       2 INVREBLD BIT(1) UNAL %INIT('0'B)%CHARTEXT('/**/'), /* 1 if inventory to be rebuilt, 0 if rest
ored from dump*/
    22 -                          /* If the inventory pages are not in the dump, then  */
    23 -                                         /* They are recalculated              */
    24 -       2 LOAD BIT(1) UNAL %INIT('0'B)%CHARTEXT('/**/'), /* 1 if load           */
    25 -       2 UNLOAD BIT(1) UNAL %INIT('0'B)%CHARTEXT('/**/'), /* 1 if unload       */
    26 -       2 PRINT BIT(1) UNAL %INIT('0'B)%CHARTEXT('/**/'), /* 1 if print         */
