
<Cobol84_program> assumed to be the goal symbol.


===============
GRAMMAR LISTING
===============


1    <SYSTEM_GS> ::= =BEGINNING_OF_FILE= <Cobol84_program> =END_OF_FILE=

2    <Cobol84_program> ::= <program_list> {ACT_EOF}

3    <program_list> ::= <program> <end_program_opt>
4         | <program_list> <program> <end_program_opt>

5    <end_program_opt> ::= <end_program_list>
6         |

7    <program> ::= {ACT_CONSECUTIVE_PROG} <control_div_opt> <ident_div> <environ_div_opt> <data_div_opt>
                    <proc_div_opt>

8    <control_div_opt> ::= <control_div>
9         |

10   <environ_div_opt> ::= <environ_div>
11        |

12   <data_div_opt> ::= <data_div>
13        |

14   <proc_div_opt> ::= <proc_div>
15        | {ACT_NO_PROC_DIV}

16   <end_program_list> ::= <end_program>
17        | <end_program_list> <end_program>

18   <end_program> ::= END PROGRAM <data_name> . {ACT_END_PROGRAM}

19   <control_div> ::= CONTROL DIVISION . <substitution_section_opt> <default_section_opt>

20   <substitution_section_opt> ::= <substitution_section>
21        |

22   <substitution_section> ::= SUBSTITUTION SECTION .

23   <default_section_opt> ::= <default_section>
24        |

25   <default_section> ::= DEFAULT SECTION .
26        | DEFAULT SECTION . <default_clause_list> .

27   <default_clause_list> ::= <default_clause>
28        | <default_clause_list> <default_clause>

29   <default_clause> ::= DISPLAY <OPERATIONAL_opt> <sign_clause> {ACT_DISPLAY_SIGN}
30        | COMP-4 <OPERATIONAL_opt> <sign_is_opt> <lead_trail> {ACT_COMP_4_SIGN}
31        | SYMBOLIC QUEUE {ACT_NOT_IMPLEMENTED} <IS_opt> MESSAGE UNIT
32        | USE COMMUNICATION PROGRAM {ACT_COMMUNICATION_PROGRAM}
33        | DEFAULT <FOR_opt> COMP <IS_opt> <comp_default> {ACT_DEFAULT_COMP}
34        | COMP <IS_opt> <comp_default> {ACT_DEFAULT_COMP}
35        | DEFAULT <FOR_opt> NUMERIC LIMIT <IS_opt> <integer> {ACT_NUMERIC_LIMIT}
36        | NUMERIC LIMIT <IS_opt> <integer> {ACT_NUMERIC_LIMIT}
37        | GENERATE {ACT_NOT_IMPLEMENTED} NO DESCRIPTORS
38        | GENERATE {ACT_NOT_IMPLEMENTED} AGGREGATE DESCRIPTORS
39        | GENERATE {ACT_NOT_IMPLEMENTED} SCALAR DESCRIPTORS

40   <comp_default> ::= DISPLAY
41        | COMP-5
42        | COMP-6
43        | COMP-7
44        | COMP-8

45   <ident_div> ::= IDENTIFICATION DIVISION . PROGRAM-ID . <data_name> <common_initial_opt> .
                    <ident_info_opt> {ACT_ID_DIVISION}

46   <common_initial_opt> ::= <IS_opt> <common_initial_choice> <PROGRAM_opt> {ACT_PROP_TOP_MINUS_1}
47        |

48   <common_initial_choice> ::= <common_choice> {ACT_CREATE_LIST_OF_ONE_LX_STK}
49        | <initial_choice> {ACT_CREATE_LIST_OF_ONE_LX_STK}
50        | <common_choice> INITIAL {ACT_NEW_LX_LIST_OF_TWO_LX_STK}
51        | <initial_choice> COMMON {ACT_NEW_LX_LIST_OF_TWO_LX_STK}

52   <common_choice> ::= COMMON {ACT_HIGH_LEV} {ACT_PROP_TOP_MINUS_1}

53   <initial_choice> ::= INITIAL {ACT_HIGH_LEV} {ACT_PROP_TOP_MINUS_1}

54   <PROGRAM_opt> ::= PROGRAM
55        |

56   <ident_info_opt> ::= <para_list>
57        |

58   <para_list> ::= <iden_para>
59        | <para_list> <iden_para>

60   <iden_para> ::= <author_paragraph> {ACT_AUTHOR_PGPH_PRSNT}
61        | <installation_paragraph> {ACT_INSTALL_PGPH_PRSNT}
62        | <date_written_paragraph> {ACT_DATWRIT_PGPH_PRSNT}
63        | <date_compiled_paragraph> {ACT_DATCMPIL_PGPH_PRSNT}
64        | <security_paragraph> {ACT_SECURITY_PGPH_PRSNT}

65   <author_paragraph> ::= AUTHOR .

66   <installation_paragraph> ::= INSTALLATION .

67   <date_written_paragraph> ::= DATE-WRITTEN .

68   <date_compiled_paragraph> ::= DATE-COMPILED . {ACT_SET_COMPILED_DATE}

69   <security_paragraph> ::= SECURITY .

70   <environ_div> ::= ENVIRONMENT DIVISION .
71        | ENVIRONMENT DIVISION . <config_i_o>

72   <config_i_o> ::= <config_section>
73        | <config_section> <input_output_section>
74        | <input_output_section>

75   <config_section> ::= CONFIGURATION SECTION . <computer_special_opt>

76   <computer_special_opt> ::= <source_computer_opt> <object_computer_opt> <special_names_opt>

77   <source_computer_opt> ::= SOURCE-COMPUTER . <computer_name> .
78        | SOURCE-COMPUTER . <computer_name> <WITH_opt> DEBUGGING MODE . {ACT_DEBUGGING_MODE}
79        | SOURCE-COMPUTER .
80        |

81   <object_computer_opt> ::= OBJECT-COMPUTER . <computer_name> . {ACT_OBJECT_COMPUTER}
82        | OBJECT-COMPUTER . <computer_name> <obj_computer_clauses> . {ACT_OBJECT_COMPUTER_2}
83        | OBJECT-COMPUTER .
84        |

85   <special_names_opt> ::= SPECIAL-NAMES .
86        | SPECIAL-NAMES . .
87        | SPECIAL-NAMES . <special_names_clause_list> . {ACT_WRAP_UP_SPECIAL_NAMES}
88        |

89   <obj_computer_clauses> ::= <obj_computer_clause>
90        | <obj_computer_clauses> <obj_computer_clause>

91   <obj_computer_clause> ::= <prog_col_sequence>
92        | <segment_limit>
93        | <memory_size>

94   <prog_col_sequence> ::= <PROGRAM_opt> <collating_sequence>

95   <collating_sequence> ::= <COLLATING_opt> SEQUENCE <IS_opt> <alphabet_name> {ACT_COLLATING_SEQUENCE}

96   <segment_limit> ::= SEGMENT-LIMIT <IS_opt> <integer> {ACT_SEGMENT_LIMIT}

97   <memory_size> ::= MEMORY {ACT_OBSOLETE} <SIZE_opt> <integer> WORDS
98        | MEMORY {ACT_OBSOLETE} <SIZE_opt> <integer> CHARACTERS
99        | MEMORY {ACT_OBSOLETE} <SIZE_opt> <integer> MODULES

100  <special_names_clause_list> ::= <special_names_clause>
101       | <special_names_clause_list> <special_names_clause>

102  <special_names_clause> ::= <switch_phrase>
103       | <mnemonic_assign>
104       | <alphabet_phrase>
105       | <symbolic_phrase>
106       | <currency_phrase>
107       | <decimal_phrase>
108       | <block_phrase>
109       | <class_phrase>
110       | <IBM_channel_phrase>
111       | <VFC_clause>

112  <class_phrase> ::= CLASS <data_name> <IS_opt> <literal_thru_list> {ACT_CLASS_NAME}

113  <mnemonic_assign> ::= <basic_mnemonic_name> <IS_opt> <mnemonic_name> {ACT_BASIC_MNEMONIC_ASSIGN}
114       | PAGE <IS_opt> <mnemonic_name> {ACT_BASIC_MNEMONIC_ASSIGN}

115  <alphabet_phrase> ::= ALPHABET <alphabet_name> <IS_opt> <alphabet_type> {ACT_ALPHABET_NAME}
116       | <alphabet_name> <IS_opt> <alphabet_type> {ACT_ALPHABET_NAME}

117  <symbolic_phrase> ::= SYMBOLIC <CHARACTERS_opt> <symbolic_char_phrases> {ACT_SYMBOLIC_CHAR_NO_ALPHA}
118       | SYMBOLIC <CHARACTERS_opt> <symbolic_char_phrases> <alphabet_name_common> {ACT_SYMBOLIC_CHAR}
119       | SYMBOLIC <CHARACTERS_opt> <symbolic_char_phrases> <data_name> {ACT_SYMBOLIC_CHAR}
120       | SYMBOLIC <CHARACTERS_opt> <symbolic_char_phrases> IN <alphabet_name> {ACT_SYMBOLIC_CHAR_IN_ALPHA}

121  <currency_phrase> ::= CURRENCY <SIGN_opt> <IS_opt> <literal> {ACT_CURRENCY_SIGN}
122       | CURRENCY <SIGN_opt> <IS_opt> <literal> OBJECT {ACT_NOT_IMPLEMENTED} <SIGN_opt> <IS_opt> <literal>
                     {ACT_CURRENCY_SIGN_OBJECT}

123  <decimal_phrase> ::= DECIMAL-POINT <IS_opt> <COMMA_DECIMAL-POINT> {ACT_COMMA_DECIMAL_POINT}
124       | DECIMAL-POINT <IS_opt> <COMMA_DECIMAL-POINT> OBJECT <IS_opt> <COMMA_DECIMAL-POINT>
                    {ACT_OBJECT_COMMA_DEC_PNT}

125  <COMMA_DECIMAL-POINT> ::= COMMA
126       | DECIMAL-POINT

127  <block_phrase> ::= BLOCK <integer> <IS_CONTAINS_opt> <data_name> {ACT_BLOCK_IS_NAME}
128       | BLOCK <integer> <IS_CONTAINS_opt> <data_name> THRU <data_name> {ACT_BLOCK_IS_NAME_THRU_NAME}

129  <switch_phrase> ::= SWITCH-NUMBER <IS_opt> <mnemonic_name> <on_off_phrase> {ACT_SWITCH_AND_ON_OFF}
130       | SWITCH-NUMBER <IS_opt> <mnemonic_name> {ACT_SWITCH_NAME}
131       | SWITCH-NUMBER <on_off_phrase> {ACT_SWITCH_ON_OFF}

132  <on_off_phrase> ::= <on_phrase> {ACT_CREATE_LIST_OF_ONE_LX_STK}
133       | <off_phrase> {ACT_CREATE_LIST_OF_ONE_LX_STK}
134       | <on_phrase> <off_phrase> {ACT_NEW_LX_LIST_OF_TWO_LX_STK}
135       | <off_phrase> <on_phrase> {ACT_NEW_LX_LIST_OF_TWO_LX_STK}

136  <on_phrase> ::= ON <STATUS_opt> <IS_opt> <data_name> {ACT_TWO_ITEM_LIST}

137  <off_phrase> ::= OFF <STATUS_opt> <IS_opt> <data_name> {ACT_TWO_ITEM_LIST}

138  <alphabet_type> ::= <alphabet_name_common>
139       | <alphabet_literal_specs>

140  <alphabet_literal_specs> ::= <alphabet_literal_specs> <alphabet_literal_spec>
                    {ACT_ADD_LX_STK_TO_LIST_TAIL}
141       | <alphabet_literal_spec> {ACT_CREATE_LIST_OF_ONE_LX_STK}

142  <alphabet_literal_spec> ::= <literal> THRU <literal> {ACT_ALPHABET_THRU_CLAUSE}
143       | <literal> <alphabet_also_phrases> {ACT_ADD_LX_STK_TO_LIST_HEAD}
144       | <literal> {ACT_CREATE_LIST_OF_ONE_LX_STK}

145  <alphabet_also_phrases> ::= ALSO <literal> {ACT_CREATE_LIST_OF_ONE_LX_STK}
146       | <alphabet_also_phrases> ALSO <literal> {ACT_ADD_ALSO_LITERAL_TO_TAIL}

147  <symbolic_char_phrases> ::= <symbolic_char_phrase> {ACT_CREATE_LIST_OF_ONE_LX_STK}
148       | <symbolic_char_phrases> <symbolic_char_phrase> {ACT_ADD_LX_STK_TO_LIST_TAIL}

149  <symbolic_char_phrase> ::= <data_name> <IS_ARE_opt> <integer_list> {ACT_SYMBOLIC_CHARACTER_LIST}
150       | <data_name> <data_name_list> <IS_ARE_opt> <integer_list> {ACT_SYMBOLIC_CHAR_LIST}

151  <IBM_channel_phrase> ::= CHANNEL-NUMBER <IS_opt> <data_name> {ACT_CHANNEL_NUMBER}
152       | CSP <IS_opt> <data_name> {ACT_CSP}

153  <VFC_clause> ::= VFC double_quoted_string <IS_opt> <data_name_list> {ACT_VFC_CLAUSE}
154       | double_quoted_string <IS_opt> <data_name_list> {ACT_VFC_CLAUSE}

155  <input_output_section> ::= INPUT-OUTPUT SECTION . FILE-CONTROL . <file_control_entry_list_opt>
156       | INPUT-OUTPUT SECTION . FILE-CONTROL . <file_control_entry_list_opt> <I_O_control_clause>

157  <I_O_control_clause> ::= I-O-CONTROL .
158       | I-O-CONTROL . <I_O_clause_list> .
159       | I-O-CONTROL . .

160  <I_O_clause_list> ::= <I_O_clause>
161       | <I_O_clause_list> <I_O_clause>

162  <I_O_clause> ::= SAME <RECORD_SORT_MERGE_opt> <AREA_opt> <FOR_opt> <file_name> <file_name_list>
                    {ACT_SAME_AREA}
163       | MULTIPLE FILE <TAPE_opt> <CONTAINS_opt> <file_name_position_list> {ACT_MULTIPLE_FILE_TAPE}
164       | RERUN <on_rerun_file_opt> <EVERY_opt> <rerun_every_clause> {ACT_RERUN_CLAUSE}

165  <on_rerun_file_opt> ::= ON CHECKPOINT-FILE
166       |

167  <rerun_every_clause> ::= <integer> RECORDS <OF_opt> <file_name> {ACT_RERUN_EVERY_RECORDS}

168  <RECORD_SORT_MERGE_opt> ::= RECORD
169       | SORT
170       | SORT-MERGE
171       |

172  <file_name_position_list> ::= <file_name_position> {ACT_CREATE_LIST_OF_ONE_LX_STK}
173       | <file_name_position_list> <file_name_position> {ACT_ADD_LX_STK_TO_LIST_TAIL}

174  <file_name_position> ::= <file_name> {ACT_CREATE_LIST_OF_ONE_LX_STK}
175       | <file_name> POSITION <IS_opt> <integer> {ACT_FILE_POSITION}

