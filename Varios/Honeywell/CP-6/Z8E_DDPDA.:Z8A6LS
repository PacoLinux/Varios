178  <file_control_entry_list> ::= <file_control_entry>
179       | <file_control_entry_list> <file_control_entry>

180  <file_control_entry> ::= SELECT <OPTIONAL_opt> <file_name> {ACT_SELECT_CLAUSE_INIT} <select_clause_list>
                     . {ACT_SELECT_CLAUSE_END}
181       | SELECT <OPTIONAL_opt> <file_name> {ACT_SELECT_CLAUSE_INIT} . {ACT_SELECT_CLAUSE_END}

182  <select_clause_list> ::= <select_clause> {ACT_UPDATE_CURRENT_CLAUSE}
