       18     7312
       19     7313        %DCL_FAB;
       20     7435
       21     7436    1   DCL UL_VEC             BIT (72) DALIGNED;
       22     7437    1   DCL LABEL_TYPE         SBIN WORD;
       23     7438    1   DCL OPEN_TYPE          SBIN WORD;
       24     7439    1   DCL 1 BEGIN_LABEL      STATIC DALIGNED,
       25     7440    1         2 NUM#           SBIN WORD INIT (1),
       26     7441    1         2 ULBL           CHAR (80);
       27     7442    1   DCL 1 END_LABEL        STATIC DALIGNED,
       28     7443    1         2 NUM#           SBIN WORD INIT (1),
       29     7444    1         2 ULBL           CHAR (80);
