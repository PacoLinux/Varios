PL6.E3A0      #001=X9WU$WRITE_USER_LABEL File=X9WU.:X8A6TSI                      SAT 08/09/97 11:10 Page:2    
       38     7453
       39     7454         %MACRO BEGIN_LABEL_CHECK;
       40     7455           DO;
       41     7456             IF SUBSTR(BEGIN_LABEL.ULBL,0,3) ~= 'UHL' THEN
       42     7457               DO;
       43     7458                 CALL X8ER$ERROR(%USER_LABEL_NOT_UHL_UTL);
       44     7459                 SUBSTR(BEGIN_LABEL.ULBL,0,3) = 'UHL';
       45     7460               END;
       46     7461           END;
       47     7462         %MEND;
       48     7463        %eject;
PL6.E3A0      #001=X9WU$WRITE_USER_LABEL File=X9WU.:X8A6TSI                      SAT 08/09/97 11:10 Page:3    
       49     7464         %MACRO END_LABEL_CHECK;
       50     7465           DO;
       51     7466             IF SUBSTR(END_LABEL.ULBL,0,3) ~= 'UTL' THEN
       52     7467               DO;
       53     7468                 CALL X8ER$ERROR(%USER_LABEL_NOT_UHL_UTL);
       54     7469                 SUBSTR(END_LABEL.ULBL,0,3) = 'UTL';
       55     7470               END;
       56     7471           END;
       57     7472         %MEND;
       58     7473
       59     7474         %EJECT;
PL6.E3A0      #001=X9WU$WRITE_USER_LABEL File=X9WU.:X8A6TSI                      SAT 08/09/97 11:10 Page:4    
       60     7475         %MACRO WRITE_LABEL (@LABEL_TYPE =,
       61     7476                             @LABEL_AREA = );
       62     7477           DO;
       63     7478               DCB$ = DCBADDR(FAB.DCB_NUM);
       64     7479               IF M$XX.FUN# = %CREATE THEN
       65     7480                 DO;
       66     7481                   IF FAB.LABEL_REC$ -> USE_TABLE.PTR$(@LABEL_TYPE)
       67     7482                       -> USE_LABEL.AREA ~= ' ' THEN
       68     7483                     DO;
       69     7484                       UL_VEC = VECTOR(@LABEL_AREA);
       70     7485                       @LABEL_AREA.ULBL = FAB.LABEL_REC$ ->
       71     7486                         USE_TABLE.PTR$(@LABEL_TYPE) -> USE_LABEL.AREA;
       72     7487                       %IF (TEXTCHAR(@LABEL_AREA) = 'BEGIN_LABEL');
       73     7488                           %BEGIN_LABEL_CHECK;
       74     7489                       %ELSE;
       75     7490                           %END_LABEL_CHECK;
       76     7491                       %ENDIF;
       77     7492                     END;
       78     7493                   ELSE
       79     7494                     UL_VEC = VECTOR(NIL);
       80     7495
       81     7496                 END;
       82     7497               ELSE
       83     7498                 DO;
       84     7499                   UL_VEC = VECTOR(@LABEL_AREA);
       85     7500                   @LABEL_AREA.ULBL = ' ';
       86     7501                 END;
       87     7502
       88     7503           END;
       89     7504         %MEND;
       90     7505        %eject;
PL6.E3A0      #001=X9WU$WRITE_USER_LABEL File=X9WU.:X8A6TSI                      SAT 08/09/97 11:10 Page:5    
       91     7506         %MACRO READ_LABEL (@LABEL_TYPE =,
       92     7507                            @LABEL_AREA = );
       93     7508           DO;
       94     7509             DCB$ = DCBADDR(FAB.DCB_NUM);
       95     7510             IF M$XX.FUN# ~= %CREATE THEN
       96     7511               FAB.LABEL_REC$ -> USE_TABLE.PTR$(@LABEL_TYPE) ->
       97     7512                     USE_LABEL.AREA = @LABEL_AREA.ULBL;
       98     7513           END;
       99     7514         %MEND;
      100     7515
      101     7516         %EJECT;
PL6.E3A0      #001=X9WU$WRITE_USER_LABEL File=X9WU.:X8A6TSI                      SAT 08/09/97 11:10 Page:6    
      102     7517
      103     7518    2      DO CASE (LABEL_TYPE);
      104     7519    2        CASE (%BEGINNING_FILE_LABEL);   /* befor opening the file */
      105     7520
      106     7521    3          DO CASE (OPEN_TYPE);
      107     7522    3            CASE (%OPEN_OUTPUT, %OPEN_OUTPUT_NO_REWIND, %OPEN_EXTEND);
      108     7523
      109     7524
      110     7525    3              IF FAB.LABEL_REC$ -> USE_TABLE.PTR$ (%BEGINNING_FILE_LABEL)
      111     7526    3                      -> USE_LABEL.AREA ~= ' ' THEN
      112     7527    4                DO;
      113     7528    4                  UL_VEC = VECTOR(BEGIN_LABEL);
      114     7529
      115     7530    4                  BEGIN_LABEL.ULBL = FAB.LABEL_REC$ ->
      116     7531    4                      USE_TABLE.PTR$(%BEGINNING_FILE_LABEL) -> USE_LABEL.AREA;
      117     7532
      118     7533                       %BEGIN_LABEL_CHECK;
      119     7542    4                END;
      120     7543    3              ELSE
      121     7544    3                UL_VEC = VECTOR(NIL);
      122     7545
      123     7546    3            CASE (ELSE);
      124     7547
