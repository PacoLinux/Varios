      133     7556                             @LABEL_AREA = END_LABEL );
      134     7591
      135     7592    2        CASE (%ENDING_TAPE_LABEL);
      136     7593
      137     7594               %WRITE_LABEL (@LABEL_TYPE = %ENDING_TAPE_LABEL,
      138     7595                             @LABEL_AREA = END_LABEL);
