PL6.E3A0      #001=Z8JT$TABLE_DUMP File=Z8JT.:Z8A6TSI                            SAT 08/09/97 13:32 Page:16   
      394    11548    1   GET_TABLE_SIZE : PROC (TABLE$, AMOUNT) ;
      395    11549
      396    11550    2   DCL    TABLE$  PTR ;
      397    11551    2   DCL    AMOUNT  CHAR (3) ;
      398    11552
      399    11553    2   DCL 1  TABLE   ALIGNED BASED (TABLE$),
      400    11554    2        2 TOP     PTR,
      401    11555    2        2 FIRST   PTR,
      402    11556    2        2 BOTTOM  UBIN WORD UNAL,
      403    11557    2        2 FLOOR   UBIN WORD UNAL,
      404    11558    2        2 SIZE    UBIN WORD UNAL ;
      405    11559
      406    11560    2          CALL BINCHAR (AMOUNT, TABLE$ -> TABLE.SIZE) ;
      407    11561
      408    11562    3          IF SUBSTR (AMOUNT, 0, 1) = '0' THEN DO ; /* PIC ZZ9 */
      409    11563    3            SUBSTR (AMOUNT, 0, 1) = ' ' ;
