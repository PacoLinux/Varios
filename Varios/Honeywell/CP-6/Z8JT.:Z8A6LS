      278    11366        /*---------------*/
      279    11367    1     Z8JT$DUMP_TIMES:     ENTRY;
      280    11368        /*---------------*/
      281    11369
      282    11370        /*D*   NAME: Z8JT$DUMP_TIMES
      283    11371               CALL: CALL Z8JT$DUMP_TIMES
      284    11372          INTERFACE: Calls Z9AL$DUMP_LINE to print a line.
      285    11373                     Calls Z9AL$WRITE_BLANK_LINE to print a blank line.
      286    11374
      287    11375             INPUTS: COMCOM.TIMES(0:5) -- TIME IN MICRO-SECONDS.
      288    11376             ======                       (TIMES(0) = TOTAL TIME).
      289    11377
      290    11378        DESCRIPTION: Prints the processor times and core used
      291    11379                     for each phase. Called when the user specifies
      292    11380                     the STATS option.  Also prints the size of each table.
      293    11381        */
      294    11382
      295    11383    1          CALL Z9AL$WRITE_BLANK_LINE ;
      296    11384    1          OBUF = ' ';
      297    11385
      298    11386    1          TIME.PHASE = 'PHASE ' ;
      299    11387    1          TIME.DECIMAL = '.' ;
      300    11388    1          TIME.COMMA = ',' ;
      301    11389    1          TIME.SEKONDS = ' SECONDS.' ;
      302    11390    1          TIME.LPM = ' L/M. ' ;
      303    11391    1          TIME.COMA = ',' ;
      304    11392    1          TIME.K    = 'K' ;
      305    11393
      306    11394    2          DO I = 1 TO 5 ;
      307    11395
      308    11396                 /* Print Allocated Memory in "K" Units per Phase */
      309    11397
      310    11398    2            COMCOM.IL_SPACE (I) = (COMCOM.IL_SPACE (I) + 1023) / 1024 ;
      311    11399    2            CALL BINCHAR (TIME.ALOCATD, COMCOM.IL_SPACE (I)) ;
      312    11400    3            IF SUBSTR (TIME.ALOCATD, 0, 1) = '0' THEN DO ; /* PIC ZZ9 */
      313    11401    3              SUBSTR (TIME.ALOCATD, 0, 1) = ' ' ;
      314    11402    4              IF SUBSTR (TIME.ALOCATD, 1, 1) = '0' THEN DO ;
PL6.E3A0      #001=Z8JT$TABLE_DUMP File=Z8JT.:Z8A6TSI                            SAT 08/09/97 13:32 Page:13   
      315    11403    4                SUBSTR (TIME.ALOCATD, 1, 1) = ' ' ;
      316    11404    4              END ;
      317    11405    3            END ;
      318    11406
      319    11407    2            CALL BINCHAR (TIME.NUMBER, I) ;
      320    11408                 %SET_TIME (I = I) ;
      321    11442    2            CALL Z9AL$DUMP_LINE ;
      322    11443
      323    11444    2          END ;
      324    11445
      325    11446    1          TIME.PHASE = 'TOTAL ' ;
      326    11447    1          TIME.NUMBER = ' ' ;
      327    11448    1          TIME.ALOCATD = '   ' ;
      328    11449    1          TIME.K       = ' ' ;
      329    11450               %SET_TIME (I = 0) ;
      330    11484    1          CALL Z9AL$DUMP_LINE ;
      331    11485
      332    11486    1          CALL Z9AL$WRITE_BLANK_LINE ;
      333    11487
      334    11488    1          OBUF = ' ' ;
      335    11489    1          TABLE.LENGTH_OF = 'Length of ' ;
      336    11490    1          TABLE.TABLE_IS  = ' table is ' ;
      337    11491    1          TABLE.K         = 'K.' ;
