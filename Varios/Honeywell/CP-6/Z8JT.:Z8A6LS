        1        1        /*M* Z8JT$TABLE_DUMP -- Dumps I. L. Tables */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7
        8        8
        9        9        /*P*   NAME: Z8JT$TABLE_DUMP
       10       10            PURPOSE: Print out the various tables (ILS, ILL, etc.),
       11       11                     COMCOM and compiler timings.
       12       12        */
       13       13
       14       14        /*D*   NAME: Z8JT$TABLE_DUMP
       15       15               CALL: CALL Z8JT$TABLE_DUMP
       16       16          INTERFACE: Z9AL -- Prints a line.
       17       17        DESCRIPTION::
       18       18
       19       19           YE OLDE TABLE DUMPER:
       20       20           == ==== ===== ======
       21       21
       22       22           DUMP EACH ITEM IN EACH TABLE (ILL, ETT, ST, ETC.)
       23       23           IN THE FOLLOWING FORMAT:
       24       24
       25       25             LOC NAME WORD1 WORD2 ...
       26       26              6 1 8    12  1  12  ...
       27       27
       28       28           THIS MODULE ALSO CONTAINS THE ENTRY Z8JT$DUMP_TIMES
       29       29           CALLED BY THE EXEC WHEN THE "STATS" OPTION IS ON.
       30       30        */
       31       31
       32       32        %EJECT;
PL6.E3A0      #001=Z8JT$TABLE_DUMP File=Z8JT.:Z8A6TSI                            SAT 08/09/97 13:32 Page:40   
       33       33        /*---------------*/
       34       34          Z8JT$TABLE_DUMP:     PROC;

     34 46 000000   000000 700200 xent  Z8JT$TABLE_* TSX0  ! X66_AUTO_0
        46 000001   000040 000000                    ZERO    32,0

       35       35        /*---------------*/
       36       36
       37       37        %MACRO SET_TIME (I = I) ;
       38       38
       39       39                 CALL BINCHAR (CHAR10, COMCOM.TIMES(I)) ;
       40       40                 TIME.SECONDS = SUBSTR (CHAR10, 0, 4) ;
       41       41                 TIME.MILLIS  = SUBSTR (CHAR10, 4, 3) ;
       42       42                 TIME.MICROS  = SUBSTR (CHAR10, 7, 3) ;
       43       43
       44       44                 /* BLANK WHEN ZERO "SECONDS" A LA PIC ZZZ9 */
       45       45
       46       46                 ZERO = '1'B ;
       47       47                 DO J = 0 TO 2 ;
       48       48                   IF ZERO THEN DO ;
       49       49                     IF SUBSTR (TIME.SECONDS, J, 1) = '0'
       50       50                     THEN SUBSTR (TIME.SECONDS, J, 1) = ' ' ;
       51       51                     ELSE ZERO = '0'B ;
       52       52                   END ;
       53       53                 END ;
       54       54
       55       55                 /* COMPUTE LINES PER MINUTE */
       56       56
       57       57                 J = COMCOM.TIMES (I) / 1000 ; /* Milli-seconds */
       58       58                 IF J = 0
       59       59                 THEN J = 9999999 ;
       60       60                 ELSE J = COMCOM.LINES.SOURCE * 60000 / J ;
       61       61
       62       62                 IF J > 999999
       63       63                 THEN CHAR10 = '9999999999' ;
       64       64                 ELSE CALL BINCHAR (CHAR10, J) ;
       65       65
PL6.E3A0      #001=Z8JT$TABLE_DUMP File=Z8JT.:Z8A6TSI                            SAT 08/09/97 13:32 Page:41   
       66       66                 TIME.LPM1 = SUBSTR (CHAR10, 4, 3) ;
       67       67                 TIME.LPM2 = SUBSTR (CHAR10, 7, 3) ;
       68       68
       69       69                 /* BLANK WHEN ZERO "LPM1" A LA PIC ZZ9 */
       70       70
       71       71                 ZERO = '1'B ;
       72       72                 DO J = 0 TO 1 ;
       73       73                   IF ZERO THEN DO ;
       74       74                     IF SUBSTR (TIME.LPM1, J, 1) = '0'
       75       75                     THEN SUBSTR (TIME.LPM1, J, 1) = ' ' ;
       76       76                     ELSE ZERO = '0'B ;
       77       77                   END ;
       78       78                 END ;
       79       79
       80       80        %MEND ;
       81       81        %MACRO DUMP_ITEM;
       82       82            DO;
       83       83                 LENGTH = ILL_STEP (PTR$->ILL.ID - %FIRST_ILL) ;
       84       84                 CALL Z8JT$DUMP_ITEM ( PTR$, LENGTH );
       85       85            END;
       86       86        %MEND;
       87       87        %EJECT;
