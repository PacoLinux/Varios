      444    11617    3            IF PTR$ ~= ADDR(NIL) THEN
      445    11618    4              DO;
      446    11619    5                DO WHILE( PTR$->ILL.ID = %CCOSU);
      447    11620                       %DUMP_ITEM;
      448    11626    5                  PTR$ = PTR$-> CCOSU.ILL$;
      449    11627    5                END;  /* WHILE */
      450    11628
      451    11629    4                IF  PTR$->ILL.ID = %LINOI
      452    11630    4                AND PTR$->LINOI.LINE_NO ~= CURRENT_LINE THEN
      453    11631    4                  CALL DUMP_LINOI;
      454    11632
      455    11633                     %DUMP_ITEM;
      456    11639    4              END;
