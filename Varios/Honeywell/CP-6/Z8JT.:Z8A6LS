      194    11282    1          CALL Z9AL$DUMP_LINE ;
      195    11283    1          IF  COMCOM.CURRENT.PHASE = 2
      196    11284    1          OR  COMCOM.CURRENT.PHASE = 3 THEN
      197    11285    1            CALL DUMP_ILS;
      198    11286    1          ELSE
      199    11287    1            CALL DUMP_ILS2;
