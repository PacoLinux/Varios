   11      /*P*  NAME:        TOU_RQSTS_E
   12            AUTHOR:      Scott S. Sheppard
   13            PURPOSE:     Equates for DEER input requests
   14            DESCRIPTION: This file is used by PARTRidGE.X to create the
   15                         command syntax used by XUR$GET_COMMAND for the
   16                         DEER processor.
   17            REFERENCE:   CP-6 Release C0P0
   18            LAST UPDATE: 12/10/84
   19      */
   20      %EJECT ;

15:18 JUL 28 '97 TOU_RQSTS_E.:E05SI                                        2    
   21      /* DEER Requests */
   22
   23         %EQU NULL                              = 0 ;
   24         %EQU BUILD                             = 1 ;
   25                 %EQU IFAD_FID            = 1 ;
   26                 %EQU ON                  = 2 ;
   27                 %EQU OVER                = 3 ;
   28                 %EQU DECKFILE_FID        = 4 ;
   29         %EQU LIST                              = 2 ;
   30                 %EQU CATSRCH_STRING      = 5 ;
