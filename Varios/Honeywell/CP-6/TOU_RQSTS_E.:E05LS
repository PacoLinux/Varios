   34                 %EQU SHOWDECK_OPTS       = 6 ;
   35                         %EQU ATTRIBUTES  = 1 ;
   36                         %EQU REFERENCES  = 2 ;
   37         %EQU SHOWFILE                          = 4 ;
   38                 %EQU LOGSRCH_STRING      = 7 ;
   39         %EQU NEWCAT                            = 5 ;
   40                 %EQU CATKEY              = 8 ;
   41         %EQU NEWDECK                           = 6 ;
   42                 %EQU OBJKEY              = 9 ;
   43         %EQU REPDECK                           = 7 ;
   44                 %EQU OLD_OBJKEY          = 10 ;
   45                 %EQU NEW_OBJKEY          = 11 ;
   46         %EQU REMREF                            = 8 ;
   47         %EQU DELDECK                           = 9 ;
   48         %EQU DELPATCH                          = 10 ;
   49         %EQU DELCAT                            = 11 ;
   50         %EQU PATCH                             = 12 ;
   51                 %EQU PATCH_OPTS          = 12 ;
   52                         %EQU RETAIN      = 1 ;
   53                         %EQU OVERWRITE   = 2 ;
   54         %EQU UPDATE                            = 13 ;
   55         %EQU END                               = 15 ;
