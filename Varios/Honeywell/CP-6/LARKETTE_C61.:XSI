/*P*
   NAME:       LARKETTE_EQUS_E
   PURPOSE:    To contain common %EQU's for LARKETTE routines.
*/
 
/*
   Message types used for overhead communication between LARK and LARKETTE.
*/
 
%EQU     CONTROL_MSGTYP       = 'CONTROL ' ; /* ALSO EQU'ED IN LARK_GLOBALS */
%EQU     FUNCTION_MSGTYP      = 'FUNCTBLK' ;
%EQU     STATUS_MSGTYP        = 'STATSBLK' ;
 
/*
   Message codes for overhead communication messages.
   ( LARK -> LARKETTE )
*/
 
%EQU     ABORT_LARKETTE       = 101 ;
%EQU     CONNECT_STATION      = 102 ;
%EQU     DISCONNECT_STATION   = 103 ;
%EQU     FUNCTION_BLK_HEADER  = 104 ;
%EQU     RESUME_STATION       = 105 ;
%EQU     STATUS_REQUEST       = 106 ;
%EQU     SUSPEND_STATION      = 107 ;
 
/*
   Message codes for overhead communication messages.
   ( LARKETTE -> LARK )
*/
 
%EQU     CONNECTED_STATION    = 201 ;
%EQU     DEFINED_STATION      = 202 ;
%EQU     DISCONNECTED_STATION = 203 ;
%EQU     COMMAND_ERROR_RETURN = 204 ;
%EQU     NWIO_ERROR_RETURN    = 205 ;
%EQU     PMME_ERROR_RETURN    = 206 ;
%EQU     RESUMED_STATION      = 207 ;
%EQU     STATUS_ABORT_HEADER  = 208 ;
%EQU     STATUS_REPORT_HEADER = 209 ;
%EQU     SUSPENDED_STATION    = 210 ;
 
/*
   Station error codes.
*/
 
%EQU     NOT_CONNECTED        = 501 ;
%EQU     NOT_DEFINED          = 502 ;
%EQU     NOT_DISCONNECTED     = 503 ;
%EQU     NOT_RESUMED          = 504 ;
%EQU     NOT_SUSPENDED        = 505 ;
%EQU     NOT_OPENED           = 506 ;
 
/*
   Other equates.
*/
 
%EQU     CONTROL_EVENT        = 1001 ;       /* ALSO EQU'ED IN LARK_GLOBALS */
%EQU     SIZE_DATA_SEG_BLOCK  = 2048 ;
