PL6.E3A0      #001=X8CV$CONVERT_TO_INTEGER File=X8CV.:X8A6TSI                    SAT 08/09/97 10:47 Page:3    
       70     4232
       71     4233    2       DO CASE ( SEND_DESC.DES_W.TYPE );
       72     4234
       73     4235    2          CASE( %PACK_FIXED_NO_SIGN );    /* Comp */
       74     4236
       75     4237                 %PACK_NDSC( @DESC   = SEND_DSC,
       76     4238                             @SCALE  = SEND_DESC.DES_W.NDSC.SF,
