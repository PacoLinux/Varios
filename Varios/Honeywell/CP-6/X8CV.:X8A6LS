       91     4349
       92     4350    2            REC_DSC.DESCL = 4;   /* only length is set for DTB (n=4) */
       93     4351    2            TEMP_FLD_LEN = SEND_DESC.DES_W.NDSC.N;
       94     4352    3            DO CASE ( SEND_DESC.DES_W.TYPE );
       95     4353    3              CASE( %EBCDIC_FIXED_NO_SIGN );
       96     4354    3                CALL XLATE( TEMP$FLD, T$EBCDIC_TO_ASCII, SEND$FLD );
       97     4355    3              CASE( %GBCD_FIXED_NO_SIGN );
       98     4356    3                SEND_FLD_BIT_LEN = TEMP_FLD_LEN * 6;
       99     4357    3                CALL XLATE_6_TO_9( TEMP$FLD, T$GBCD_TO_ASCII, SEND$FLD6 );
      100     4358    3              CASE( %UNPACK_FIXED_NO_SIGN );
      101     4359    3                TEMP$FLD = SEND$FLD;
      102     4360    3            END;
      103     4361    2            CALL X9CV$CVDI( SEND_DSC, REC_DSC, ADDR(REC_FLD), TEMP_FLD$ );
      104     4362        %EJECT;
