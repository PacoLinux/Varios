      266     7299    3                 CALL CXX$DECBIN(PTR1$->P$S.COUNT,CC_VALUE) ALTRET(E_DECBIN);
      267     7300    4                 DO CASE(PTR1$->P$S.CODE);
      268     7301    4                  CASE(LIM_HOURS);
      269     7302    4                    IF CC_VALUE >95442
      270     7303    4                    THEN
      271     7304    4                       CALL CXP$POSTERR(U$PARAM.ERRBUF$,PTR1$->P$S.CPOS,E_XHRXCD);
      272     7305    4                    ELSE
