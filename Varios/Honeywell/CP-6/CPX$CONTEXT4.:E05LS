      274     7307    4                  CASE(LIM_MINUTS);
      275     7308    4                    IF I ~= 0 AND CC_VALUE >59 OR CC_VALUE >5726622
      276     7309    4                    THEN
      277     7310    4                       CALL CXP$POSTERR(U$PARAM.ERRBUF$,PTR1$->P$S.CPOS,E_XMINXCD);
PL6.E3A0      #001=CPX$CONTEXT4 File=CPX$CONTEXT4.:E05TSI                        TUE 07/29/97 12:07 Page:14   
      278     7311    4                    ELSE
