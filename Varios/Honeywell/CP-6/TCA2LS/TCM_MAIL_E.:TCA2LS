    8      /* Equates for SMTP protocol commands */
    9      %EQU SMTP_HELO  = 1;
   10      %EQU SMTP_MAIL  = 2;
   11      %EQU SMTP_RCPT  = 3;
   12      %EQU SMTP_DATA  = 4;
   13      %EQU SMTP_RSET  = 5;
   14      %EQU SMTP_SEND  = 6;
   15      %EQU SMTP_SOML  = 7;
   16      %EQU SMTP_SAML  = 8;
   17      %EQU SMTP_VRFY  = 9;
   18      %EQU SMTP_EXPN  = 10;
   19      %EQU SMTP_HELP  = 11;
   20      %EQU SMTP_NOOP  = 12;
   21      %EQU SMTP_TURN  = 13;
   22      %EQU SMTP_QUIT  = 14;
   23      %EQU SMTP_SPIL  = 15;  /* LADC addition */
   24
   25      /* Equates for SMTP mailgram headers */
   26      %EQU USER_FROM  = 20;
   27      %EQU USER_SUBJ  = 21;
   28      %EQU USER_REPL  = 22;
   29      %EQU USER_WHEN  = 23;
   30      %EQU USER_FORUM = 24;
   31      %EQU USER_MID   = 25;
   32      %EQU USER_RMID  = 26;
   33      %EQU USER_ACK   = 27;
   34      %EQU USER_RCVD  = 28;
   35
   36      /* Equates for generalized address parser */
   37      %EQU TEXT       = 31;
