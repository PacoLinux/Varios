      132    17179
      133    17180    1       CALL QCC$CBD ( QI_1.OP )
      134    17181    1         ALTRET ( OVERFLOW );
      135    17182
      136    17183    1       RETURN;
      137    17184        %EJECT;
PL6.E3A0      #001=QBC$CONVERSIONS File=QBC$CONVERSIONS.:E05TSI                  WED 07/30/97 04:46 Page:5    
      138    17185    1   QBC$CNVBBX: ENTRY;
      139    17186
