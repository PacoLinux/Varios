      178    17225    1       CALL QCC$CBD ( QI_1.OP )
      179    17226    1         ALTRET ( OVERFLOW );
      180    17227
      181    17228    1       QI_1.OP.CIP.S_A (0) = QI_1.OP.CIP.T_A (0);
