      163    17210
      164    17211    1       CALL QCS$SETUP_SOURCE;
      165    17212
      166    17213    1       QI_1.OP.CIP.T_A (0) = QI_31_DIGIT_SLS_DISPLAY_A (0);
      167    17214    1       QI_1.OP.CIP.T.CD.SIGN = %QI#CD_SIGN_TRAILING_OVER;
      168    17215    1       QI_1.OP.DATA.T$ = ADDR ( QI@OTTER_SPACE );
      169    17216
      170    17217    1       IF QI_1.OP.LEN.S = %QI#MIN_BINARY_LENGTH
