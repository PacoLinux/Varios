        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=4,IND=0,DCI=4,CSI=2,IAD=2,DTI=2,AND,ECI=0,DIN=2,STI=2 */
        8        8
        9        9        /*P*  NAME:         QBC$CONVERSIONS
       10       10              PURPOSE:      Convert datatypes to-and-from others.
       11       11              DESCRIPTION:  Several combinations of conversions to-and-from the
       12       12                            BINARY datatype are done, and one PACKED
       13       13                            (packed decimal) to DISPLAY (unpacked decimal)
       14       14                            is included just to make everything lopsided.
       15       15        */
       16       16
       17       17        QBC$CONVERSIONS: PROC;
       18       18
       19       19          %INCLUDE LCP_6;
       20      210
       21      211          %INCLUDE QI_PERR_C;
       22      379          %INCLUDE QG_MACRO_M;
       23      516          %INCLUDE QI_EQUATES_E;
       24      875          %INCLUDE QI_MACROS_M;
       25     5933          %QI$ALL;
       26    17046
       27    17047          %QI@OTTER_SPACE;
       28    17075
       29    17076    1     DCL QCC$CBD ENTRY (1) ALTRET;
       30    17077    1     DCL QCC$CDB ENTRY (1) ALTRET;
       31    17078    1     DCL QCC$DMC ENTRY (1) ALTRET;
       32    17079    1     DCL QCE$ERROR ENTRY (4);
       33    17080    1     DCL QCS$SETUP_SOURCE ENTRY;
       34    17081    1     DCL QCS$SETUP_TARGET ENTRY;
       35    17082
       36    17083        %EJECT;
PL6.E3A0      #001=QBC$CONVERSIONS File=QBC$CONVERSIONS.:E05TSI                  WED 07/30/97 04:46 Page:2    
       37    17084    1   QBC$CNVBN: ENTRY;
       38    17085
       39    17086        /*D*  NAME:         QBC$CNVBN
       40    17087              CALL:         CALL QBC$CNVBN;
       41    17088              PARAMETERS:   None.
       42    17089              INTERFACE:    Called by QAT$TALLYHO when a CNVBN op-code is
       43    17090                            encountered.
       44    17091                            Calls QCS$SETUP_SOURCE,
       45    17092                                  QCS$SETUP_TARGET,
       46    17093                                  QCC$CBD.
       47    17094              ENVIRONMENT:  Nothing unusual.
       48    17095              INPUT:        QI_1,
       49    17096                            FPL rununit static data following the op-code.
       50    17097              OUTPUT:       Target DAD contains the DISPLAY (unpacked decimal)
       51    17098                            equivalent of the BINARY source DAD.
       52    17099              SCRATCH:      Various otter data.
       53    17100              DESCRIPTION:  We're going from a BINARY DAD
       54    17101                            to a (temp) DISPLAY DAD.
       55    17102        */
       56    17103
       57    17104    1       QI_1.INSTR$ = ADDR ( QI_1.INSTR$ -> QI$OP_CNVBN.NEXT_CODE@P );
       58    17105
       59    17106    1       CALL QCS$SETUP_SOURCE;
       60    17107    1       CALL QCS$SETUP_TARGET;
       61    17108
       62    17109    1       IF QI_1.OP.LEN.S = %QI#MIN_BINARY_LENGTH
       63    17110    1       THEN
       64    17111    1         QI_1.OP.CIP.T.LEN = %QI#MIN_BINARY_DIGIT_COUNT;
       65    17112    1       ELSE
       66    17113    1         QI_1.OP.CIP.T.LEN = %QI#MAX_BINARY_DIGIT_COUNT;
       67    17114
       68    17115    1       QI_1.OP.CIP.T.CD.SIGN = %QI#CD_SIGN_TRAILING_OVER;
       69    17116    1       QI_1.OP.DEC.T = 0;
       70    17117
       71    17118    1       QI_1.DAD.T$ -> QI$DAD.WORD0 = %( QI#IS_TEMP_BIT_MASK + QI#TYPE_DISPLAY );
       72    17119    1       QI_1.DAD.T$ -> QI$DAD.PSEUDO_CIP_A (0) = QI_1.OP.CIP.T_A (0);
       73    17120    1       QI_1.DAD.T$ -> QI$DAD.PSEUDO_CIP.NUMERIC.DEC = 0;
PL6.E3A0      #001=QBC$CONVERSIONS File=QBC$CONVERSIONS.:E05TSI                  WED 07/30/97 04:46 Page:3    
       74    17121
       75    17122    1       GOTO SO_DO_THE_FROM_BINARY_ALREADY;
       76    17123
       77    17124    1   QBC$CNVBNX: ENTRY;
       78    17125
       79    17126        /*D*  NAME:         QBC$CNVBNX
       80    17127              CALL:         CALL QBC$CNVBNX;
       81    17128              PARAMETERS:   None.
       82    17129              INTERFACE:    Called by QAT$TALLYHO when a CNVBNX op-code is
       83    17130                            encountered.
       84    17131                            Calls QCS$SETUP_SOURCE,
       85    17132                                  QCS$SETUP_TARGET,
       86    17133                                  QCC$CBD.
       87    17134              ENVIRONMENT:  Nothing unusual.
       88    17135              INPUT:        QI_1,
       89    17136                            FPL rununit static data following the op-code.
       90    17137              OUTPUT:       Target DAD contains the DISPLAY (unpacked decimal)
       91    17138                            equivalent of the BINARY source DAD.
       92    17139              SCRATCH:      Various otter data.
       93    17140              DESCRIPTION:  We're going from a (temp) BINARY DAD
       94    17141                            to a DISPLAY DAD.
       95    17142        */
       96    17143
       97    17144    1       QI_1.INSTR$ = ADDR ( QI_1.INSTR$ -> QI$OP_CNVBNX.NEXT_CODE@P );
       98    17145
       99    17146    1       CALL QCS$SETUP_SOURCE;
      100    17147    1       CALL QCS$SETUP_TARGET;
      101    17148
      102    17149    1       GOTO SO_DO_THE_FROM_BINARY_ALREADY;
      103    17150
      104    17151    1   QBC$CNVBPX: ENTRY;
      105    17152
      106    17153        /*D*  NAME:         QBC$CNVBPX
      107    17154              CALL:         CALL QBC$CNVBPX;
      108    17155              PARAMETERS:   None.
      109    17156              INTERFACE:    Called by QAT$TALLYHO when a CNVBPX op-code is
      110    17157                            encountered.
PL6.E3A0      #001=QBC$CONVERSIONS File=QBC$CONVERSIONS.:E05TSI                  WED 07/30/97 04:46 Page:4    
      111    17158                            Calls QCS$SETUP_SOURCE,
      112    17159                                  QCS$SETUP_TARGET,
      113    17160                                  QCC$CBD.
      114    17161              ENVIRONMENT:  Nothing unusual.
      115    17162              INPUT:        QI_1,
      116    17163                            FPL rununit static data following the op-code.
      117    17164              OUTPUT:       Target DAD contains the PACKED (packed decimal)
