      124    17171    1       QI_1.INSTR$ = ADDR ( QI_1.INSTR$ -> QI$OP_CNVBPX.NEXT_CODE@P );
      125    17172
      126    17173    1       CALL QCS$SETUP_SOURCE;
      127    17174    1       CALL QCS$SETUP_TARGET;
      128    17175
      129    17176    1   SO_DO_THE_FROM_BINARY_ALREADY:
      130    17177
