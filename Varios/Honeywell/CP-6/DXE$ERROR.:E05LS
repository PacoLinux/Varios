        9        9        /*F* NAME: DXE$ERROR
       10       10             PURPOSE: TO PRINT TEXT ERROR MESSAGES.
       11       11             DESCRIPTION: USES THE CODE IN S_D.ERROR TO FIND THE ERROR MESSAGE TEXT. */
       12       12        /*D* NAME: DXE$ERROR
       13       13             CALL: CALL DCE$ERROR
       14       14             INTERFACE: DCK$GETX1
       15       15                        DCQ$WRITE
       16       16                        DCR$BINTXT
       17       17                        BINCHAR
       18       18                        CONCAT
       19       19                        INSERT
       20       20                        SUBSTR
       21       21             ENVIRONMENT: XDELTA
       22       22             INPUT: S_D.ERROR - ERROR CODE
       23       23                    S_D.ERROR_SYM$ - POINTER TO ERROR SYMBOL TEXTC
       24       24             OUTPUT: ERROR MESSAGE IS PRINTED AS:
       25       25                     DCE-nnnn "symbol" MESSAGE TEXT (FOR USER ERRORS)
       26       26                     DXE-nnnn @.<X1> MESSAGE TEXT (FOR INTERNAL ERRORS)
       27       27             SCRATCH: LOCAL AUTO ONLY.
       28       28             DESCRIPTION: DETERMINES IF ERROR CODE IS USER OR INTERNAL TYPE. IF INTERNAL
       29       29             THEN OBTAINS CALLING ROUTINE'S RETURN ADDRESS BY CALLING "DCK$GETX1".
       30       30             IF USER ERROR FILLS IN "SYMBOL" FIELD WHEN TEXT POINTER ~= ADDR(NIL).
       31       31             OBTAINS ERROR MESSAGE TEXT FROM TABLE. IF ERROR CODE IS NOT IN TABLE
       32       32             THEN DEFAULT MESSAGE IS SUBSTITUTED. WRITES MESSAGE BY CALLING "DCQ$WRITE".
       33       33             RETURNS WHEN DONE. */
       34       34                                                /**/
       35       35        DDE$ERROR: PROC;
       36       36                                                /**/
       37       37        %INCLUDE DD_DEBUG_C;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:2    
       38       58    1   DCL CURPOS SBIN;
       39       59    1   DCL LEFT SBIN;
       40       60    1   DCL SIZE SBIN;
       41       61    1   DCL X1 SBIN;
       42       62                                                /**/
       43       63    1   DCL 1 TEXT$C BASED UNAL,
       44       64    1         2 SIZE UBIN BYTE UNAL,
       45       65    1         2 TXT CHAR(TEXT$C.SIZE) UNAL;
       46       66        %MACRO TXTC (NAME=TXTC,T=T);
       47       67        DCL 1 NAME CONSTANT UNAL,
       48       68              2 SIZE UBIN(9) INIT( LENGTHC(T) ) UNAL,
       49       69              2 TXT CHAR(0) INIT(T) UNAL;
       50       70        %MEND;
       51       71        %TXTC (NAME=PREFIX,T="'D'");
       52       76        %TXTC (NAME=EXX,T="'X'");
       53       81        %TXTC (NAME=CEE,T="'C'");
       54       86        %TXTC (NAME=EDASH,T="'E-'");
       55       91        %TXTC (NAME=DQUOTE,T='"');
       56       96        %TXTC (NAME=LPAREN,T="'('");
       57      101        %TXTC (NAME=RPAREN,T="')'");
       58      106        %TXTC (NAME=AT,T="'@'");
       59      111        %TXTC (NAME=CRLFSPS,T='        ');
       60      116        %MACRO DXERR (NAME=DXERR,C=C,T=T,LAST=",");
       61      117              2 NAME UBIN HALF INIT(C) UNAL,
       62      118              2 * UNAL,
       63      119                3 SIZE UBIN(9) INIT(LENGTHC(T)) UNAL,
       64      120                3 TEXT CHAR(0) INIT(T) UNAL LAST
       65      121        %MEND;
       66      122        %MACRO DX$ERR (NAME=DX$ERR,STCLASS=BASED);
       67      123        DCL 1 NAME STCLASS UNAL,
       68      124              2 CODE UBIN HALF UNAL,
       69      125              2 TEXTC UNAL,
       70      126                3 SIZE UBIN(9) UNAL,
       71      127                3 TEXT CHAR(NAME.TEXTC.SIZE) UNAL;
       72      128        %MEND;
       73      129        %DX$ERR;
       74      136    1   DCL 1 DXERR CONSTANT,
