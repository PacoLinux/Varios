       88     6297    1   DCL DDK$GETX1 ENTRY(1);
       89     6298    1   DCL DDQ$WRITE ENTRY(2);
       90     6299    1   DCL DDR$BINTXT ENTRY(4);
       91     6300        %SUB INTNERR=500 ;                      /* WHERE INTERNAL ERRORS START */
       92     6301        %SUB CODEWIDTH=3 ;                      /* FIELD WIDTH FOR DECIMAL CODE */
       93     6302        %EJECT;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:4    
       94     6303    1        OUT_DEV = DCTX_ME;
       95     6304    1        IN_DEV = DCTX_ME;
       96     6305    1        CALL M_INIT;
       97     6306    1        IF S_D.DEBUGA( %DB@PRNTCODE )
       98     6307    2        THEN DO;
       99     6308    2             CALL M_TADD( PREFIX );
      100     6309    2             IF S_D.ERROR >= 500       /* INTNERR */
      101     6310    2             THEN CALL M_TADD( EXX );
      102     6311    2             ELSE CALL M_TADD( CEE );
      103     6312    2             CALL M_TADD( EDASH );
      104     6313    2             CALL BINCHAR( SUBSTR( S_D.OUTPUT, CURPOS, 3 ), S_D.ERROR );
              6313                      /* CODEWIDTH */
      105     6314    2             CALL M_SPACE( 3 + 1 ); /* CODEWIDTH */
      106     6315    2           END;
      107     6316    1        IF S_D.ERROR >= 500            /* INTNERR */
      108     6317    2        THEN DO;
      109     6318    2             CALL DDK$GETX1( X1 );
      110     6319    2             CALL M_TADD( AT );
      111     6320    2             CALL DDR$BINTXT( PINCRC( ADDR( S_D.OUTPUT ), CURPOS ), X1, %OCT_FMT, SIZE
              6320                      );
      112     6321    2             CALL M_SPACE( SIZE + 1 );
      113     6322    2           END;
      114     6323    1        ELSE IF S_D.ERROR_SYM$ ~= ADDR( NIL )
      115     6324    2             THEN DO;
      116     6325    2                  IF S_D.ERROR = 82 /* E@OLDNOTOLD */
      117     6326    2                  THEN CALL M_TADD( LPAREN );
      118     6327    2                  ELSE CALL M_TADD( DQUOTE );
      119     6328    2                  CALL M_TADD( S_D.ERROR_SYM$ -> TEXT$C );
      120     6329    2                  IF S_D.ERROR = 82 /* E@OLDNOTOLD */
      121     6330    2                  THEN CALL M_TADD( RPAREN );
      122     6331    2                  ELSE CALL M_TADD( DQUOTE );
      123     6332    2                  CALL M_SPACE( 1 );
      124     6333    2                END;
      125     6334    1        E$ = ADDR( DXERR );
      126     6335    2        DO WHILE( E$ < EB$ );
      127     6336    2             IF E$ -> DX$ERR.CODE = S_D.ERROR
      128     6337    2             THEN GOTO GOT_IT;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:5    
      129     6338    2             E$ = PINCRC( E$, SIZEC( DX$ERR.CODE )
      130     6339    2               + SIZEC( DX$ERR.TEXTC.SIZE )
      131     6340    2               + E$ -> DX$ERR.TEXTC.SIZE );
      132     6341    2           END;
      133     6342    1   GOT_IT:
      134     6343    1        CALL M_TADD( E$ -> DX$ERR.TEXTC );
      135     6344    1        IF ( S_D.ERROR = 11 )         /* E@UERR0 */
      136     6345    1          OR ( S_D.ERROR = 500 )     /* E@PARSERR */
      137     6346    1          OR ( S_D.ERROR = 188 )  /* E@WRONG#SUBS */
      138     6347    2        THEN DO;
      139     6348    2             CALL DDR$BINTXT( PINCRC( ADDR( S_D.OUTPUT ), CURPOS ), S_D.ERROR_HICHAR, %
              6348                      UINT_FMT, SIZE );
      140     6349    2             CALL M_SPACE( SIZE );
      141     6350    2           END;
      142     6351    1        CALL M_TRUNC;
      143     6352                                                /**/
      144     6353    1        RETURN;
      145     6354        %EJECT;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:6    
      146     6355        /*F* NAME: M_INIT
      147     6356             PURPOSE: TO INITIALIZE FOR TEXT MOVE SUBROUTINES.
      148     6357             DESCRIPTION: BLANKS OUT BUFFER. INITIALIZES CURPOS TO POINT PAST
      149     6358             THE CR, LF, LF CHARACTERS. RETURNS WHEN DONE. */
      150     6359        /*D* NAME: M_INIT
      151     6360             CALL: CALL M_INIT;
      152     6361             ENVIRONMENT: DXE$ERROR ACTIVE.
      153     6362             INTERFACE:
      154     6363             INPUT: S_D.OUTPUT - BUFFER
      155     6364             OUTPUT: S_D.OUTPUT BLANKED OUT, WITH FIRST 3 CHARACTERS CR, LF, LF.
      156     6365                     CURPOS - CURRENT CHARACTER POSITION.
      157     6366                     LEFT - SPACE REMAINING IN OUTPUT BUFFER.
      158     6367             SCRATCH: LOCAL AUTO ONLY.
      159     6368             DESCRIPTION: BLANKS OUT BUFFER. INITIALIZES CURPOS TO POINT PAST
      160     6369             THE CR, LF, LF CHARACTERS. RETURNS WHEN DONE. */
      161     6370                                                /**/
      162     6371    1   M_INIT: PROC;
      163     6372                                                /**/
      164     6373    2        CURPOS = 0;
      165     6374    2        LEFT = S_D.OUT_WIDTH - CURPOS;
      166     6375                                                /**/
      167     6376    2        RETURN;
      168     6377                                                /**/
      169     6378    2   END M_INIT;
      170     6379        %EJECT;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:7    
      171     6380        /*F* NAME: M_TADD
      172     6381             PURPOSE: TO ADD TEXT TO OUTPUT BUFFER.
      173     6382             DESCRIPTION: ADDS THE TEXTC STRING INPUT TO THE OUTPUT BUFFER. */
      174     6383        /*D* NAME: M_TADD
      175     6384             CALL: CALL M_TADD( TXTC );
      176     6385             ENVIRONMENT: DXE$ERROR ACTIVE.
      177     6386             INTERFACE: DCQ$WRITE
      178     6387             INPUT: S_D.OUTPUT - BUFFER
      179     6388                    CURPOS - CURRENT CHARACTER POSITION.
      180     6389                    LEFT - SPACE REMAINING IN OUTPUT BUFFER.
      181     6390                    TXTC - TEXTC STRING TO BE ADDED TO OUTPUT BUFFER.
      182     6391             OUTPUT: TEXT STRING MOVED TO OUTPUT BUFFER.
      183     6392                     CURPOS - UPDATED CURRENT CHARACTER POSITION.
      184     6393                     LEFT - UPDATED REMAINING SPACE IN BUFFER.
      185     6394             SCRATCH: LOCAL AUTO ONLY.
      186     6395             DESCRIPTION: MOVES AS MUCH OF "TXTC" INTO OUTPUT BUFFER AS WILL FIT.
      187     6396             IF IT ALL FITS THEN UPDATES "CURPOS" AND "LEFT" AND RETURNS.
      188     6397             OTHERWISE, UPON ENCOUNTERING A TRUNCATION ERROR THE FILLED UP BUFFER
      189     6398             IS OUTPUT BY CALLING "DCQ$WRITE", THE STRING CONTROL VARIABLES ARE UPDATED
      190     6399             TO REFLECT HOW MUCH WAS MOVED, AND THE PROCESS IS REPEATED UNTIL THE
      191     6400             INPUT STRING IS EXHAUSTED. */
      192     6401                                                /**/
      193     6402    1   M_TADD: PROC( TXTC );
      194     6403                                                /**/
      195     6404    2   DCL 1 TXTC UNAL,
      196     6405    2         2 SIZE UBIN(9) UNAL,
      197     6406    2         2 TXT CHAR( TXTC.SIZE ) UNAL;
      198     6407    2   DCL ST SBIN;                            /* START POSITION */
      199     6408    2   DCL LE SBIN;                            /* LENGTH LEFT TO MOVE */
      200     6409                                                /**/
      201     6410    2        ST = 0;
      202     6411    2        LE = TXTC.SIZE;
      203     6412    2   MORE:
      204     6413    2        CALL INSERT( S_D.OUTPUT, CURPOS, LEFT, SUBSTR( TXTC.TXT, ST, LE ) ) ALTRET(
              6413                 TRUNC );
      205     6414    2        LEFT = LEFT - LE;
      206     6415    2        CURPOS = CURPOS + LE;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:8    
      207     6416                                                /**/
      208     6417    2        RETURN;
      209     6418                                                /**/
      210     6419    2   TRUNC:
      211     6420    2        CALL DDQ$WRITE( ADDR( S_D.OUTPUT ), S_D.OUT_WIDTH );
      212     6421    2        ST = ST + LEFT;
      213     6422    2        LE = LE - LEFT;
      214     6423    2        CALL CONCAT( S_D.OUTPUT, CRLFSPS.TXT );
      215     6424    2        LEFT = S_D.OUT_WIDTH - LENGTHC( CRLFSPS.TXT );
      216     6425    2        CURPOS = LENGTHC( CRLFSPS.TXT );
      217     6426    2        GOTO MORE;
      218     6427                                                /**/
      219     6428    2   END M_TADD;
      220     6429        %EJECT;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:9    
      221     6430        /*F* NAME: M_SPACE
      222     6431             PURPOSE: TO INSERT SPACES INTO THE OUTPUT BUFFER.
      223     6432             DESCRIPTION: ADDS THE REQUESTED NUMBER OF SPACES TO THE OUTPUT BUFFER. */
      224     6433        /*D* NAME: M_SPACE
      225     6434             CALL: CALL M_SPACE( COUNT );
      226     6435             ENVIRONMENT: DXE$ERROR ACTIVE.
      227     6436             INTERFACE: DCQ$WRITE
      228     6437             INPUT: S_D.OUTPUT - BUFFER, ALREADY BLANK FILLED.
      229     6438                    CURPOS - CURRENT CHARACTER POSITION.
      230     6439                    COUNT - NUMBER OF SPACES TO INSERT.
      231     6440             OUTPUT: SPACES MOVED TO OUTPUT BUFFER
      232     6441                     CURPOS - UPDATED CURRENT CHARACTER POSITION.
      233     6442                     LEFT - UPDATED REMAINING SPACE IN BUFFER.
      234     6443             SCRATCH - LOCAL AUTO ONLY.
      235     6444             DESCRIPTION: SEE M_TADD, EXCEPT IMPENDING TRUNCATION IS DETECTED DIRECTLY,
      236     6445             NOT BY USING "INSERT" SUBROUTINE. */
      237     6446                                                /**/
      238     6447    1   M_SPACE: PROC( COUNT );
      239     6448                                                /**/
      240     6449    2   DCL COUNT SBIN;
      241     6450    2   DCL LE SBIN;
      242     6451                                                /**/
      243     6452    2        LE = COUNT;
      244     6453    2        IF ( LE > LEFT )
      245     6454    3        THEN DO WHILE ( LE > LEFT );
      246     6455    3             LE = LE - LEFT;
      247     6456    3             LEFT = S_D.OUT_WIDTH - LENGTHC( CRLFSPS.TXT );
      248     6457    3             CALL DDQ$WRITE( ADDR( S_D.OUTPUT ), CURPOS );
      249     6458    3             CALL CONCAT( S_D.OUTPUT, CRLFSPS.TXT );
      250     6459    3           END;
      251     6460    2        LEFT = LEFT - LE;
      252     6461    2        CURPOS = S_D.OUT_WIDTH - LEFT;
      253     6462                                                /**/
      254     6463    2        RETURN;
      255     6464                                                /**/
      256     6465    2   END M_SPACE;
      257     6466        %EJECT;
PL6.E3A0      #001=DDE$ERROR File=DXE$ERROR.:E05TSI                              TUE 07/29/97 15:10 Page:10   
      258     6467        /*F* NAME: M_TRUNC
      259     6468             PURPOSE: TO WRITE OUT REMAINING CHARACTER STRING IN OUTPUT BUFFER.
      260     6469             DESCRIPTION: WRITES OUT CONTENTS OF OUTPUT BUFFER & RETURNS. */
      261     6470        /*D* NAME: M_TRUNC
      262     6471             CALL: CALL M_TRUNC;
      263     6472             ENVIRONMENT: DXE$ERROR ACTIVE.
      264     6473             INTERFACE: DCQ$WRITE
      265     6474             INPUT: S_D.OUTPUT - BUFFER
      266     6475                    CURPOS - CURRENT CHARACTER POSITION.
      267     6476             OUTPUT: CONTENTS OF OUTPUT BUFFER WRITTEN OUT.
      268     6477             SCRATCH: LOCAL AUTO ONLY.
      269     6478             DESCRIPTION: WRITES OUT CONTENTS OF OUTPUT BUFFER & RETURNS. */
      270     6479                                                /**/
      271     6480    1   M_TRUNC: PROC;
      272     6481                                                /**/
      273     6482    2        CALL DDQ$WRITE( ADDR( S_D.OUTPUT ), CURPOS );
      274     6483    2        RETURN;
      275     6484                                                /**/
      276     6485    2   END M_TRUNC;
      277     6486                                                /**/
      278     6487    1   END DDE$ERROR;

