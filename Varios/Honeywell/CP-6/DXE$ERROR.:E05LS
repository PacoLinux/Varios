       75      137             %INCLUDE DX_ERR_C;
       76     2469        /*D* THIS ERROR MESSAGE (EB) MUST BE THE LAST ERROR MESSAGE */
       77     2470             %DXERR (NAME=EB,C=262143,LAST=";",T=
              2470                 '**** Unable to find error message text ****');
       78     2476    1   DCL E$ PTR;
       79     2477    1   DCL EB$ PTR CONSTANT INIT( ADDR( DXERR.EB ) );
       80     2478        %INCLUDE DD_ERR_C;
