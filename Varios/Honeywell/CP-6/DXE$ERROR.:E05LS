       82     2876    1   DCL OUT_DEV UBIN SYMREF;
       83     2877    1   DCL IN_DEV UBIN SYMREF;
       84     2878        %INCLUDE DD$DITEM;
       12+    2981             %LIST;
       13+    2982    1        DCL SDD$ PTR;
       14+    2983    1        DCL ZITEM$ PTR;
