       98     5918    1        VLP_ACCT.ACCT# = S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.RUFID.ACCT;
       99     5919    1        VLP_PASS.PASS# = S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.RUFID.PASS;
      100     5920                                                /* May need a DCB */
      101     5921    1        IF S_D.FPRGDCB = 0
      102     5922    2        THEN DO;
      103     5923    2             CALL DFA$GET_FPRG_DCB (DCBNO);
      104     5924    2             S_D.FPRGDCB = DCBNO;
      105     5925    2             S_D.LMDCB = DCBNO;
      106     5926    2           END;
      107     5927    1        ELSE S_D.LMDCB = S_D.FPRGDCB;
      108     5928    1        FPTOPEN.V.DCB# = S_D.LMDCB;
      109     5929                                                /* We are now debugging an FPRG */
      110     5930    1        S_D.SAVEIC_SAVE = S_D.SAVEIC;
      111     5931                                                /* Open file for schema */
PL6.E3A0      #001=DFA$FPRG File=DFA$FPRG.:E05TSI                                TUE 07/29/97 14:31 Page:4    
      112     5932    1        CALL M$OPEN (FPTOPEN) ALTRET(NOOPN);
      113     5933                                                /* Set up schema */
      114     5934    1   SCHEMA:
      115     5935    1        CALL DUI$RU_ACCESS
      116     5936    2        WHENALTRETURN DO;
      117     5937    2             CALL DDE$ERROR;
      118     5938    2             GOTO KILL_IT1;
      119     5939    2           END;
      120     5940    1   SETUP:
      121     5941    1        S_D.SAVEIC = IC;
      122     5942    1        CALL DDS$ES_INQ(S_D.SAVEIC) ALTRET(GO1);
      123     5943    1        CALL DDS$SET_Q2C;
      124     5944    1   GO1: ;
      125     5945    1        RETURN;
      126     5946        /**/
      127     5947    1   NOOPN: ;
      128     5948    1        CALL DUE$ERRMSG;
      129     5949    1        GOTO KILL_IT1;
      130     5950    1   REPORT_XXX: ;
      131     5951    1        CALL DDE$ERROR;
      132     5952    1        CALL M$XXX;
      133     5953        %EJECT;
PL6.E3A0      #001=DFA$FPRG File=DFA$FPRG.:E05TSI                                TUE 07/29/97 14:31 Page:5    
      134     5954        /*F* NAME: DFA$KILL_FPRG
      135     5955             PURPOSE: To kill the currently active FPRG */
      136     5956    1   DFA$KILL_FPRG: ENTRY(IC);
      137     5957        /**/
      138     5958                                                /* Kill current FPRG */
      139     5959    1   KILL_IT1: ;
      140     5960    1        CALL KILL_FPRG ALTRET(REPORT_XXX);
      141     5961                                                /* Release FPRG DCB */
      142     5962    1        IF S_D.FPRGDCB ~= 0
      143     5963    2        THEN DO;
      144     5964    2             IF DCBADDR(S_D.FPRGDCB) ~= ADDR(NIL)
      145     5965    3             THEN DO;
      146     5966    3                  FPTRELDCB = FPT_RELDCB;
      147     5967    3                  FPTRELDCB.V_ = VECTOR(FPTRELDCB.V);
      148     5968    3                  FPTRELDCB.V.DCB# = S_D.FPRGDCB;
      149     5969    3                  CALL M$RELDCB (FPTRELDCB) ALTRET(NORELFPRG);
      150     5970    3                END;
      151     5971    2             S_D.FPRGDCB = 0;
      152     5972    2           END;
      153     5973    1        S_D.LMDCB = D_M$LM#;
      154     5974                                                /* Reestablish HOST mode */
      155     5975    1        S_D.FPRG = '0'B;
      156     5976    1        S_D.CURRENT_FPRG_CONTEXT$ = ADDR(NIL);
      157     5977    1        CALL DUI$INIT_HOST;
      158     5978    1        CALL DDA$RESET_PATCH_SPACE;
      159     5979    1        S_D.FIRST_TIME = '1'B;
      160     5980    1        CALL DUI$RU_ACCESS
      161     5981    2        WHENALTRETURN DO;
      162     5982    2             CALL DDE$ERROR;
      163     5983    2             CALL M$XXX;
      164     5984    2           END;
      165     5985    1        CALL DDS$ES_INQ(S_D.SAVEIC) ALTRET(KILL1);
      166     5986    1        CALL DDS$SET_Q2C;
      167     5987    1   KILL1: ;
      168     5988    1        IF S_D.TRACE_STATE
      169     5989    2        THEN DO;
      170     5990    2             S_D.TRACE_STATE = '0'B;
PL6.E3A0      #001=DFA$FPRG File=DFA$FPRG.:E05TSI                                TUE 07/29/97 14:31 Page:6    
      171     5991    2             CALL DDA$TRACEON;
      172     5992    2           END;
      173     5993    1        RETURN;
      174     5994        /**/
      175     5995    1   NORELFPRG: ;
      176     5996    1        S_D.ERROR = E@RELFPRG;             /* E@RELFPRG */
      177     5997    1        CALL DUE$ERRMSG;
      178     5998    1        CALL DDE$ERROR;
      179     5999    1        CALL M$XXX;
      180     6000        %EJECT;
PL6.E3A0      #001=DFA$FPRG File=DFA$FPRG.:E05TSI                                TUE 07/29/97 14:31 Page:7    
      181     6001    1   KILL_FPRG: PROC ALTRET;
      182     6002        /**/
      183     6003    2   DCL TOBREL UBIN;
      184     6004        /**/
      185     6005                                                /* Release schema memory */
      186     6006    2        TOBREL = POFFW(S_D.SCHSEG$, S_D.SCHSEG_BEGIN$);
      187     6007    2        IF TOBREL > 0
      188     6008    2        THEN CALL DDM$PUTMEM (S_D_SEG$, TOBREL) ALTRET(NORELS_D);
      189     6009    2        S_D.SCHSEG$ = S_D.SCHSEG_BEGIN$;
      190     6010    2        IF S_D.FPRG THEN S_D.SAVEIC = S_D.SAVEIC_SAVE;
      191     6011    2        IF  S_D.FPRGDCB ~= 0
      192     6012    2          AND DCBADDR(S_D.FPRGDCB) ~= ADDR(NIL)
      193     6013    2          AND DCBADDR(S_D.FPRGDCB) -> F$DCB.FCD#
      194     6014    3        THEN DO;
      195     6015    3             FPTCLOSE = FPT_CLOSE;
      196     6016    3             FPTCLOSE.V_ = VECTOR(FPTCLOSE.V);
      197     6017    3             FPTCLOSE.V.DCB# = S_D.FPRGDCB;
      198     6018    3             CALL M$CLOSE (FPTCLOSE) ALTRET(NOCLSFPRG);
      199     6019    3           END;
      200     6020    2        RETURN;
      201     6021        /**/
      202     6022    2   NORELS_D: ;
      203     6023    2        S_D.ERROR = E@RELS_D;              /* E@RELS_D */
      204     6024    2        ALTRETURN;
      205     6025    2   NOCLSFPRG: ;
      206     6026    2        CALL DUE$ERRMSG;
      207     6027    2        S_D.ERROR = E@CLSFPRG;             /* E@CLSFPRG */
      208     6028    2        ALTRETURN;
      209     6029    2   END KILL_FPRG;
      210     6030
      211     6031    1   END DFA$FPRG;
      212     6032        %EOD;

