PL6.E3A0      #001=DFA$FPRG File=DFA$FPRG.:E05TSI                                TUE 07/29/97 14:31 Page:3    
       75     5895                                                /* Cleanup current RU access */
       76     5896    1        IF S_D.ANLZ
       77     5897    1        THEN CALL DUA$KILL_ANLZ;
       78     5898    1        IF S_D.TR$INFO.ACTIVE
       79     5899    2        THEN DO;
       80     5900    2             S_D.TRACE_STATE = '1'B;
       81     5901    2             CALL DDT$TRACEOFF;
       82     5902    2           END;
       83     5903    1        ELSE S_D.TRACE_STATE = '0'B;
       84     5904    1        IF S_D.RUM
       85     5905    1        THEN CALL DUA$KILL_RUM;
       86     5906    1        CALL KILL_FPRG ALTRET(REPORT_XXX);
       87     5907    1        CALL DUI$INIT_L6;
       88     5908    1        S_D.FPRG = '1'B;
       89     5909    1        FPTOPEN = FPT_OPEN;
       90     5910    1        FPTOPEN.V_ = VECTOR(FPTOPEN.V);
       91     5911    1        FPTOPEN.SN_ = VECTOR(VLP_SN);
       92     5912    1        FPTOPEN.NAME_ = VECTOR(VLP_NAME);
       93     5913    1        FPTOPEN.ACCT_ = VECTOR(VLP_ACCT);
       94     5914    1        FPTOPEN.PASS_ = VECTOR(VLP_PASS);
       95     5915    1        VLP_SN.SN#(0) = S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.RUFID.PSN;
       96     5916    1        VLP_NAME.L# = S_D.CURRENT_FPRG_CONTEXT$ -> FPRG$CONTEXT.RUFID.FNAME.SIZE;
