        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8        NAME:    ZIG$SS_READ
        9        9        PURPOSE: TO GET A LINE FOR THE PARSER OF DBACS DIRECTIVES
       10       10                 AND STORE THEM IN A BUFFER
       11       11            CALLING PROGRAMS         SUBROUTINE NAMES
       12       12            ----------------         ----------------
       13       13                 ZIG$SS_PSET               ZIG$M_PSET
       14       14                 ZIG$SS_PDRI               ZIG$M_PDRI
       15       15        */
       16       16        %EJECT;
PL6.E3A0      #001=ZIG$SS_READ File=ZIG$SS_READ.:ZIC0TSI                         FRI 09/05/97 12:15 Page:2    
       17       17        /*
       18       18        ZZZZZ  III   GGG    $    SSS   SSS        RRRR  EEEEE  AAA  DDDD
       19       19            Z   I   G   G  $$$  S   S S   S       R   R E     A   A D   D
       20       20           Z    I   G     $ $   S     S           R   R E     A   A D   D
       21       21          Z     I   G      $$$   SSS   SSS        RRRR  EEEE  AAAAA D   D
       22       22         Z      I   G  GG   $ $     S     S       R R   E     A   A D   D
       23       23        Z       I   G   G  $$$  S   S S   S       R  R  E     A   A D   D
       24       24        ZZZZZ  III   GGG    $    SSS   SSS        R   R EEEEE A   A DDDD
       25       25                                            _____
       26       26        */
       27       27        ZIG$SS_READ:
       28       28            PROC ;
       29       29    1   DCL PUT_OUT_LINE ENTRY (2);
       30       30    1   DCL RDWORD BIT (1) EXT;
       31       31    1   DCL ZIG$DDS5 ENTRY;
       32       32    1   DCL REC CHAR (80) EXT;
       33       33    1   DCL REC1 CHAR (80) EXT;
       34       34    1   DCL EOFFLG UBIN (36) EXT;
       35       35    1   DCL LINE_COUNT UBIN (36) EXT;
       36       36    1   DCL SAVESW BIT (1) EXT;
       37       37    1   DCL NUM_CHAR_READ UBIN (36) EXT;
       38       38    1   DCL LINE CHAR (4000) EXT;
       39       39    1   DCL LAST_COUNT UBIN (36) SYMREF;
       40       40    1   DCL LIST_STRING CHAR(128) EXT;
       41       41    1   DCL FOUND_PER BIT (1) SYMREF;
       42       42    1   DCL NCS UBIN (36) EXT;
       43       43    1   DCL BUFFER_ERR BIT(1) EXT;
       44       44    1   DCL I UBIN (36);
       45       45    1   DCL END_SUBSCHEMA BIT (1) EXT;
       46       46    1   DCL DONE BIT (1);
       47       47    1   DCL K UBIN (36);
       48       48    1   DCL L UBIN (36);
       49       49    1   DCL POUNDS CHAR (20) CONSTANT INIT ('####################');
       50       50    1   DCL LAST_STATUS UBIN (6) EXT;
       51       51    1   DCL WRITSF ENTRY (2);
       52       52         %EJECT;
PL6.E3A0      #001=ZIG$SS_READ File=ZIG$SS_READ.:ZIC0TSI                         FRI 09/05/97 12:15 Page:3    
       53       53         %INCLUDE ZI_SUB_LANG;
       54     1768         %GENERAL;
       55     1892         %TEXT_LINE;
       56     1929        %INCLUDE ZIG_GENERAL;
       57     2037         %INCLUDE XU_MACRO_C;
       58     5143         %INCLUDE ZI_ERRORS_SUBS;
       59     5663         %P$PCB(NAME=SS_TRAN, STCLASS = "SYMREF");
       60     5864         %EJECT;
PL6.E3A0      #001=ZIG$SS_READ File=ZIG$SS_READ.:ZIC0TSI                         FRI 09/05/97 12:15 Page:4    
       61     5865    1   NEXT_REC: PROC;
       62     5866    2       REC1 = ' ';
       63     5867    3       DO WHILE ((REC1 = ' ') OR
       64     5868    3          ((SUBSTR(REC1,7) = ' ' OR
       65     5869    3          SUBSTR(REC1,6,1) = '*') AND SUB_LANGUAGE = #COBOL) OR
       66     5870    3          ((SUBSTR(REC1,6) = ' ' OR SUBSTR(REC1,0,1) = 'C'
       67     5871    3          OR SUBSTR(REC1,0,1) = '*') AND SUB_LANGUAGE = #FORTRAN));
       68     5872    3           CALL ZIG$DDS5; /* Get a record */
       69     5873    3           RDWORD = '1'B;
       70     5874    3           IF NCS=1 THEN RETURN;
       71     5875    3           CALL PUT_OUT_LINE(LINE_COUNT,REC);
       72     5876    3           SAVESW = '0'B;
       73     5877    3           TEXT_LINE.BLOCK_NU = CURRENT_RECORD / 2;
       74     5878    3           TEXT_LINE.LINE_NU = LINE_COUNT;
       75     5879    3           TEXT_LINE.TEXT = REC;
       76     5880    3           CALL WRITSF (CURRENT_RECORD,TEXT_LINE);
       77     5881    3           CURRENT_RECORD = CURRENT_RECORD + 1;
       78     5882    3           LINE_COUNT=LINE_COUNT+1;
       79     5883    3           REC1 = REC;
       80     5884    3           END;
       81     5885        /*********
       82     5886        	scan from back of record to find the first non
       83     5887        	blank CHARACTER.(FIND the end of the record).
       84     5888        **********/
       85     5889    2       CALL SEARCHR(I,K,SEARCHR_NB,REC1);
       86     5890    2       NUM_CHAR_READ = I + 1;
       87     5891    3       DO WHILE ('1'B);
       88     5892    3           CALL INDEX(K,'"',REC1) ALTRET(NO_QUOTE);
       89     5893    3           CALL INSERT(REC1,K,1,'''');
       90     5894    3           CALL INDEX1(L,'"',REC1,K) ALTRET(NO_QUOTE);
       91     5895    3           CALL INSERT(REC1,L,1,'''');
       92     5896    3           IF LAST_STATUS < #AREA_DEF AND SUB_LANGUAGE~=#FORTRAN
       93     5897    3           THEN CALL INSERT(REC,K+1,L-K-1,SUBSTR(POUNDS,0,L-K-1));
       94     5898    3           END;
       95     5899    2   NO_QUOTE:
       96     5900    2       CALL XLATE(REC1,SEM_COM,REC1);
       97     5901    2   END;
PL6.E3A0      #001=ZIG$SS_READ File=ZIG$SS_READ.:ZIC0TSI                         FRI 09/05/97 12:15 Page:5    
       98     5902        /*****
       99     5903                 Find then number of characters read in the last
      100     5904                 read done by exread.
      101     5905        ******/
      102     5906    1       CALL NEXT_REC;
      103     5907    1       IF NCS=1
      104     5908    2       THEN DO;
      105     5909    2           RDWORD='0'B;
      106     5910    2           SAVESW='1'B;
      107     5911    2           CALL WARNING(#NO_SUBSCHEMA_END); /* NO END SUBSCHEMA TEXT */
      108     5912    2           END_SUBSCHEMA='1'B;
      109     5913    2           RETURN;
      110     5914    2           END;
      111     5915        /*E*          ERROR:    ZIG-#NO_SUBSCHEMA_END-C
      112     5916        MESSAGE:      No END was found at the end of the subschema text.
      113     5917        */
      114     5918    1       IF SUB_LANGUAGE = #COBOL
      115     5919    2       THEN DO;
      116     5920    2           REC1 = SUBSTR(REC1,7);
      117     5921    2           I = I - 7;
      118     5922    2           END;
      119     5923    1       IF SUB_LANGUAGE = #FORTRAN
      120     5924    2       THEN DO;
      121     5925    2           REC1 = SUBSTR(REC1,6);
      122     5926    2           I = I - 6;
      123     5927    2           END;
      124     5928    1       LAST_COUNT = SS_TRAN.NCHARS;
      125     5929    1       IF I > 80
      126     5930    1       THEN I = 1;
      127     5931    2       IF SS_TRAN.NCHARS+I+1 < 4000 THEN DO;
      128     5932    2           CALL INSERT(LINE,SS_TRAN.NCHARS,I+1,SUBSTR(REC1,0,I+1),'  ');
      129     5933    2           SS_TRAN.NCHARS = SS_TRAN.NCHARS+I+1;
      130     5934    2           END;
      131     5935    1       ELSE
      132     5936    2           IF NOT BUFFER_ERR THEN DO;
      133     5937    2               BUFFER_ERR = '1'B;
      134     5938    2               CALL ERROR(#BUFFER_FULL); /* Buffer is full */
PL6.E3A0      #001=ZIG$SS_READ File=ZIG$SS_READ.:ZIC0TSI                         FRI 09/05/97 12:15 Page:6    
      135     5939        /*E*     ERROR:    ZIG-#BUFFER_FULL-C
      136     5940        MESSAGE: The size of the command being processed exceeds the 4000
      137     5941                 character buffer reserved for it.  Reduce the size of the
      138     5942                 command by using ALIASes or by using multiple commands.
      139     5943        */
      140     5944    2               END;
      141     5945        /********
      142     5946        	don't add a ';' if this is the end of the directive.
      143     5947        *********/
      144     5948    1       FOUND_PER = '0'B;
      145     5949    1       CALL INDEX2R(I,'. ',REC,NUM_CHAR_READ+1) ALTRET (NO_PERIOD);
      146     5950    1       IF I > NUM_CHAR_READ - 2
      147     5951    2       THEN DO;
      148     5952    2           FOUND_PER = '1'B;
      149     5953    2           END;
      150     5954    1   NO_PERIOD:
      151     5955    1       IF SUB_LANGUAGE=#FORTRAN AND NOT FOUND_PER
      152     5956    2       THEN DO;
      153     5957    2           CALL ZIG$DDS5;
      154     5958    2           IF (SUBSTR(REC,5,1) = ' ' AND NCS = 0 ) OR
      155     5959    2              (SUBSTR(REC,0,1) = '*' OR SUBSTR(REC,0,1) = 'C')
      156     5960    3           THEN DO;
      157     5961    3               CALL INSERT(LINE,SS_TRAN.NCHARS,3,' . ');
      158     5962    3               END;
      159     5963    3           ELSE DO;
      160     5964    3               CALL INSERT(LINE,SS_TRAN.NCHARS,3,' #;');
      161     5965    3               END;
      162     5966    2           SS_TRAN.NCHARS = SS_TRAN.NCHARS + 3;
      163     5967    2           RDWORD = '0'B;
      164     5968    2           SAVESW = '1'B;
      165     5969    2           RETURN;
      166     5970    2           END;
      167     5971    1       IF NOT FOUND_PER
      168     5972    2       THEN DO;
      169     5973    2           CALL INSERT(LINE,SS_TRAN.NCHARS,3,' #;');
      170     5974    2           SS_TRAN.NCHARS=SS_TRAN.NCHARS + 3;
      171     5975    2           END;
PL6.E3A0      #001=ZIG$SS_READ File=ZIG$SS_READ.:ZIC0TSI                         FRI 09/05/97 12:15 Page:7    
      172     5976    1       RETURN;
      173     5977    1   END ZIG$SS_READ;

