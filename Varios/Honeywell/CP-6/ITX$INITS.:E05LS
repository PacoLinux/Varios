PL6.E3A0      #001=ITX$INITS File=ITX$INITS.:E05TSI                              TUE 07/29/97 23:10 Page:8    
      232    15383        /*   Initialize the WSQs for ASLs, DBs, or CPs. */
      233    15384    1        NUMSP.ASL=0;
      234    15385    1        NUMSP.CP=0;
      235    15386    1        NUMSP.DB=0;
      236    15387
      237    15388    2        DO I = 1 TO P_NPROCS;
      238    15389    2             IF B$P$->B$P.FLGS(I) & %P_ASL
      239    15390    2             THEN
      240    15391    2                  NUMSP.ASL = NUMSP.ASL + 1;
      241    15392    2             IF B$P$->B$P.FLGS(I) & %P_DB
      242    15393    2             THEN
      243    15394    2                  NUMSP.DB = NUMSP.DB + 1;
      244    15395    2             IF B$P$->B$P.FLGS(I) & %P_CP
      245    15396    2             THEN
      246    15397    2                  NUMSP.CP = NUMSP.CP + 1;
      247    15398    2           END;
      248    15399
      249    15400    2        DO I = %CPWSQ TO %ASLWSQ;
      250    15401    2             PPNO = 2;
      251    15402    2             CALL MMB$GNPP (PPNO,%MONWSQ);
      252    15403    2             IF PPNO = 0
      253    15404    2             THEN
      254    15405    2                  CALL SC_NCOMPP;
      255    15406    2             IF HW_WSQ0PT THEN
      256    15407    2                  S_WSPTD.BLKNO(I) = B$WSQ0PT$->B$MAP.RPN(PPNO)*(1024/HW_PTB_UNITS);
      257    15408    2             ELSE
      258    15409    2                  S_WSPTD.BLKNO(I) = PPNO * (1024/HW_PTB_UNITS);
      259    15410    2             S_WSPTD.P(I) = '1'B;
      260    15411    2             S_WSPTD.NBLKS(I) = 32;
      261    15412
      262    15413    2             CALL MME$CVM(%MONWSQ,%WINDOWLO,PPNO,SZ);
      263    15414    2             CALL MME$CVM(%MONWSQ,%WINDOWLO+1,PPNO+1,SZ);
      264    15415
      265    15416    3             IF HW_WSQ0PT THEN DO;
      266    15417    3                  B$WINDOW$->B$MAP.RPN(0) = B$WSQ0PT$->B$MAP.RPN(PPNO);
      267    15418    3                  B$WINDOW$->B$MAP.RPN(1) = B$WSQ0PT$->B$MAP.RPN(PPNO+1);
      268    15419    3                END;
PL6.E3A0      #001=ITX$INITS File=ITX$INITS.:E05TSI                              TUE 07/29/97 23:10 Page:9    
      269    15420    3             ELSE DO;
      270    15421    3                  B$WINDOW$->B$MAP.RPN(0) = PPNO;
      271    15422    3                  B$WINDOW$->B$MAP.RPN(1) = PPNO+1;
      272    15423    3                END;
      273    15424    2             B$WINDOW$->B$MAP.CTRL(0) = %PGINMEM | %PGIOM | %PGWRITE;
      274    15425    2             B$WINDOW$->B$MAP.CTRL(1) = %PGINMEM | %PGIOM | %PGWRITE;
      275    15426
      276    15427    3             DO J = 2 TO S_WSPTD.NBLKS(I)*64-1;
      277    15428    3                  B$WINDOW$->B$MAP(J) = MM_FPMC;
      278    15429    3                END;
      279    15430    2           END;
      280    15431
      281    15432        /* Update each slave cpu's wsptd */
      282    15433    2        DO I = 1 TO S_NSCPU;
      283    15434    2             CALL MME$CVM(%MONWSQ,%WINDOWLO,IT_SCPUWSPTD(I),SZ);
      284    15435    2             B$WINDOW$->S$WSPTD(0) = S_WSPTD(0);
      285    15436    2             B$WINDOW$->S$WSPTD(%CPWSQ) = S_WSPTD(%CPWSQ);
      286    15437    2             B$WINDOW$->S$WSPTD(%DBWSQ) = S_WSPTD(%DBWSQ);
      287    15438    2             B$WINDOW$->S$WSPTD(%ASLWSQ) = S_WSPTD(%ASLWSQ);
      288    15439    2           END;
      289    15440    1        DCT$=N$DCT$(B_SCDCTX);
      290    15441    1        CALL NIQ$GET(Q$);
      291    15442    1        CALL NIQ$GETS(Q$->N$REQ.IOS$);
      292    15443    1        Q$->N$REQ.IOFLG.IOS='1'B;
      293    15444    1        DCT$->NI$DCT.SC.IOQ$=Q$;
      294    15445        /**/
      295    15446    1        IF NOT HW_TIME_LEGAL THEN GOTO RCVRDATE;
      296    15447        /**/
      297    15448    1        IF IT_BOOTFLAG >= I_RECOVERY AND IT_BOOTFLAG ~= I_OPRECOVRY THEN
      298    15449    1             GOTO RCVRDATE;
      299    15450    1        TM_MCA_DO_TIME = '1'B;
      300    15451    1        CALL UMT$IDT (IT_DATE, IT_TIME);
      301    15452    1        TM_MCA_DO_TIME = '0'B;
      302    15453    1        CALL NIM$WRENBL (CURMSG, PINCRW(ADDR(CURMSG),SIZEW(CURMSG))->B$CHAR8);
             15453                 /* Pretty sleazy, eh? */
      303    15454    1        PINCRC(ADDR(CURMSG.CHR), 16)->B$CHAR8 = IT_DATE;
      304    15455    1        PINCRC(ADDR(CURMSG.CHR), 25)->B$CHAR5 = IT_TIME;
PL6.E3A0      #001=ITX$INITS File=ITX$INITS.:E05TSI                              TUE 07/29/97 23:10 Page:10   
      305    15456    1        CALL NIM$WRDSBL (CURMSG, PINCRW(ADDR(CURMSG),SIZEW(CURMSG))->B$CHAR8);
             15456                 /* Pretty sleazy, eh? */
      306    15457    1        CALL OCWRITE (CURMSG);
      307    15458    1   RDDATE: CALL OCWRITE(DTMSG);            /*  READ DATE AND TIME                */
      308    15459    1   RERDDATE:;
      309    15460    1        IT_DATE=' ';
      310    15461    1        IT_TIME=' ';
      311    15462    1        CALL OCREAD(IT_DATE);
      312    15463    1        IF DATETYC & ~%TYC_EGV THEN GOTO RERDDATE;
      313    15464    1        IF IT_DATE=' ' THEN GOTO RCVRDATE;
      314    15465    1        CALL OCWRITE (TMMSG);
      315    15466    1   RERDTIME:;
      316    15467    1        CALL OCREAD(IT_TIME);
      317    15468    1        IF DATETYC & ~%TYC_EGV THEN GOTO RERDTIME;
      318    15469    1        CALL UMT$LCLDATE (IT_UTS,IT_DATE,IT_TIME) ALTRET(RDDATE);
      319    15470    1        CALL SSS$SETBTIME(IT_UTS);
      320    15471    2        IF HW_IMX THEN DO;
      321    15472    2             CALL ITK$TMB$BUF_ALLOC (BUF$,BUFX,DQH$) ALTRET (RCVRDATE);
      322    15473    2             BUF$ -> UTS_B = IT_UTS;
      323    15474    2             CALL ITK$TMB$MCA (DQH$->NI$DQH,TM_OC_TIME_WRITE#,BUFX);
      324    15475    2             CALL ITK$TMB$BUF_DEALLOC (BUF$,BUFX);
      325    15476    2           END;
      326    15477    1   RCVRDATE:
      327    15478    1        CALL MMQ$INIT;
      328    15479    1        IT_CLOCKOK='1'B;
      329    15480    1        CALL KCC$CLOCK;
      330    15481    1        I = 2 + MINIMUM(KI_MXFE,%B_CWSQMAX-2); /* -2 IS FOR FEPS > 14 */
      331    15482    1        CALL MMS$WSQINIT (I) ALTRET (SCNPP);
      332    15483    1        FOM_WSQ = %B_CWSQ1;
      333    15484    1        FOS_WSQ=%B_CWSQ2;
      334    15485    1        CWSQ=%B_CWSQ3;
      335    15486    1        PPNO = 0;
      336    15487    2        DO FEI=1 TO KI_MXFE;
      337    15488    2             FEI$ = KI$FEI$(FEI);
      338    15489    2             KI$FEI.FEWSQ =CWSQ;
      339    15490    2             KI$FEI.PTPPNO# = PPNO;
      340    15491    2             SZ = KI$FEI.OQSZ;
PL6.E3A0      #001=ITX$INITS File=ITX$INITS.:E05TSI                              TUE 07/29/97 23:10 Page:11   
      341    15492    2             CALL MMS$GETCQ (CWSQ,P$,SZ ) ALTRET (SCNPP);
      342    15493    2             P$->KI$QHDR='0'B;
