      658    15809    2        DDT$=N$DCT$(I)->NK$LDCT.DDT$;
      659    15810    2        DDT$->FT$DDT.ANST='1'B;
      660    15811    2        DDT$->FT$DDT.POS = FTP_HDR#;
      661    15812    2        CALL NIQ$GET(Q$);
      662    15813    2        Q$->N$REQ.DLA.DCTX=I;
      663    15814    2        Q$->N$REQ.OPFLG.HOLD='1'B;
      664    15815    2        Q$->N$REQ.FC=%N_SPFILE;
      665    15816    2        Q$->N$REQ.RCT=1;
      666    15817    2        CALL NIO$QUE(Q$->N$REQ);
      667    15818    2        CALL NIO$IOSPIN(Q$->N$REQ);
      668    15819    2        CALL NIQ$REL(Q$);
      669    15820    2        ALTRETURN;
      670    15821        /**/
      671    15822    2   END;
      672    15823        /**/
      673    15824    1   AVRINIT: PROC(DCTX) ALTRET;
      674    15825        /**/
      675    15826    2   DCL DCTX UBIN(18) UNAL;                 /* DCT index of drive                 */
      676    15827    2   DCL XDCT UBIN(15) AUTO;
      677    15828    2        XDCT=DCTX;
      678    15829    2        CALL NIO$SPINTCON(XDCT,ENTADDR(FRL$AVR),0) ALTRET(JUSTALT);
      679    15830    2        RETURN;
      680    15831    2   JUSTALT: ALTRETURN;
      681    15832    2   END;
      682    15833        /**/
      683    15834    1   PMINIT: PROC;
      684    15835        /**/
      685    15836    2        CALL SSS$CURTIME (P_STARTUP);      /* Record system-startup time         */
      686    15837                                                /**/
PL6.E3A0      #001=ITX$INITS File=ITX$INITS.:E05TSI                              TUE 07/29/97 23:10 Page:21   
      687    15838    2        CALL SSC$MONTIMR (6000, ENTADDR(PMC$COLLECT), 0); /* once-a-minute dude */
      688    15839                                                /**/
      689    15840    2        P_DCTXMAX = NK_MXLOCAL + 1;        /* Calc. # of entries in table        */
      690    15841                                                /**/
      691    15842    3        DO I = 0 TO NK_MXLOCAL;            /* Initialize DCT-table PM stuff      */
      692    15843                                                /**/
      693    15844    3             P$ = N$DCT$$->N$DCT$(I);
      694    15845                                                /**/
      695    15846    3             IF P$ ~= ADDR(NIL)
      696    15847    4             THEN DO;
      697    15848                                                /**/
      698    15849    4                  CALL PMN$COLLECT (P$->NI$DCT.PM, 0);
      699    15850    4                  P$->NI$DCT.PM.CONNCT = 0;
      700    15851    4                  P$->NI$DCT.PM.ELAPSED_TIME = '0'B;
      701    15852    4                  IF P$->NI$DCT.PMD$ ~= ADDR(NIL)
      702    15853    4                  THEN P$->NI$DCT.PMD$->P$IODATA = P$->NI$DCT.PM;
      703    15854                                                /**/
      704    15855    4                END;
      705    15856                                                /**/
      706    15857    3           END;
      707    15858                                                /**/
      708    15859    3        DO I = 0 TO NI_IOMS - 1 ;          /* Initialize channel-table PM stuff  */
      709    15860    4             DO J = 3 TO NI_CHANS(I) ;
      710    15861                                                /**/
      711    15862    4                  IF ( J >= 8 )       OR
      712    15863    4                       ( HW_IMX AND J = 3 ) OR
      713    15864    5                       ( HW_IOP AND J = 4 ) THEN DO ;
      714    15865                                                /**/
      715    15866    5                       P$ = NI$CHT$->NI$CHT.DQH$(I*%N_MAXCHANS+J);
      716    15867    6                       IF (P$ ~= ADDR(NIL)) THEN DO ;
      717    15868    6                            CALL PMN$COLLECT (P$->NI$DQH.PM, 0);
      718    15869    6                            P$->NI$DQH.PM.CONNCT = 0;
      719    15870    6                            P$->NI$DQH.PM.ELAPSED_TIME = '0'B;
      720    15871    6                          END;
      721    15872    5                     END ;
      722    15873    4                END ;
      723    15874    3           END ;
PL6.E3A0      #001=ITX$INITS File=ITX$INITS.:E05TSI                              TUE 07/29/97 23:10 Page:22   
      724    15875                                                /**/
      725    15876    2        RETURN;
      726    15877    2   END;
      727    15878        /**/
      728    15879    1   OPCINIT: PROC ALTRET;
      729    15880    2   DCL S CHAR(4);
      730    15881    2   DCL R$ PTR;
      731    15882    2        P$=FP_OPCI$$;
      732    15883    3        DO WHILE (P$ ~= ADDR(NIL));
      733    15884    3             CALL NKL$GETDCT (LDCT$) ALTRET(SCNPP);
      734    15885    3             P$->FP$OPCI.LDCTX = LDCT$->NK$LDCT.LDCTX;
      735    15886    3             CALL BINCHAR (S,P$->FP$OPCI.FEI#);
      736    15887    3             CALL CONCAT (LDCT$->NK$LDCT.DEVNM,'OPCA',S);
      737    15888    3             LDCT$->NK$LDCT.RESCOD = NK_OPCTL;
      738    15889    3             LDCT$->NK$LDCT.RLCID = '0'B;
      739    15890    3             LDCT$->NK$LDCT.RLCID.NODE =
      740    15891    3                  KI$FEI$(P$->FP$OPCI.FEI#)->KI$FEI.FEP#;
      741    15892    3             LDCT$->NK$LDCT.DFLG.INPUT = '1'B;
      742    15893    3             LDCT$->NK$LDCT.DFLG.OUTPUT = '1'B;
      743    15894    3             LDCT$->NK$LDCT.DFLG.CM='1'B;
      744    15895    3             LDCT$->NK$LDCT.LKFLG.ACTIVE='1'B;
      745    15896    3             LDCT$->NK$LDCT.LKFLG.QDPPATH = '1'B;
      746    15897    3             LDCT$->NK$LDCT.LKFLG.OBLK = '0'B;
      747    15898    3             R$ = FR$HD$(FR_NPOOL);
      748    15899    4             DO WHILE (R$ ~= ADDR(NIL));
      749    15900    4                  IF (R$->FR$RAT.TYP = 'OD' AND
      750    15901    4                       N$DCT$(R$->FR$RAT.DCTX)->NI$DCT.DQH$->NI$DQH.FEX =
      751    15902    5                       P$->FP$OPCI.FEI#) THEN DO;
      752    15903    5                       N$DCT$(R$->FR$RAT.DCTX)->NI$DCT.OD.LDCT$ = LDCT$;
      753    15904    5                       N$DCT$(R$->FR$RAT.DCTX)->NI$DCT.OD.FEI$ =
      754    15905    5                            KI$FEI$(P$->FP$OPCI.FEI#);
      755    15906    5                       N$DCT$(R$->FR$RAT.DCTX)->NI$DCT.OD.OPCI$ = P$;
      756    15907    5                     END;
      757    15908    4                  R$ = R$->FR$RAT.LNK$;
      758    15909    4                END;
      759    15910    3             P$ = P$->FP$OPCI.LNK$;
      760    15911    3           END;
