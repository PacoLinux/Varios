      382        1        /*T***********************************************************/
      383        2        /*T*                                                         */
      384        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      385        4        /*T*                                                         */
      386        5        /*T***********************************************************/
      387        6        Z9IL$PRINT_LO : PROC;
      388        7
      389        8        /*D*
      390        9               NAME: Z9IL$PRINT_LO
      391       10        DESCRIPTION: Print the assembly language listing of the generated
      392       11                       object code.  This version uses the internal tables
      393       12                       of the compiler and does a much better job of symbolic
      394       13                       information than UNGMAP.  It may be invoked with the
      395       14                       option, "LSTOU".
      396       15        */
      397       16
      398       17        %EQU PHASE = 5;
      399       18
      400       19        %INCLUDE Z8A_DEFC;
      401       87        %INCLUDE Z8A_SUPP_MAC;
      402     2487        %INCLUDE Z8A_COMCOM;
      403     3137        %INCLUDE Z9I_PREPROC;
      404     3157        %INCLUDE Z8I_LISTING;
      405     3245        %INCLUDE Z8A_OBJ;
      406     3742        %INCLUDE Z8A_ST;
      407     7401        %INCLUDE Z8A_LTS;
      408     9478        %INCLUDE Z8A_DNT;
      409     9699        %INCLUDE Z8A_ERR;
