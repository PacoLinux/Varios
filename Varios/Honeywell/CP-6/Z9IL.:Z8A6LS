     2120     7202    1    IF STP$ -> ST.DNT$ ~= ADDR(NIL) THEN
     2121     7203    2     DO;
     2122     7204    3       DO CASE (STP$ -> ST.ID);
     2123     7205
     2124     7206    3         CASE (%TEMP, %SCALAR, %ARRAY);
     2125     7207
     2126     7208    3           TEXTBUF      = ' ';
     2127     7209    3           STRCTLN.NAME = STP$ -> VARIABLE.DNT$ -> DNT.TEXT.NAME;
     2128     7210    3           ADDR_W       = P_ADDRESS/36;
     2129     7211    3           ADDR_C       = (P_ADDRESS - ADDR_W*36) / 9;
     2130     7212    3           ADDR_B       = MOD(P_ADDRESS, 9);
     2131     7213    3           CALL Z8IU$CONVERT_OCTAL_SUP(STRCTLN.LOC, BINBIT(ADDR_W, 36));
     2132     7214    3           STRCTLN.X    = '-';
     2133     7215    3           CALL BINCHAR(STRCTLN.CHPOS, ADDR_C);
     2134     7216    3           STRCTLN.Y            = '-';
     2135     7217    3           CALL BINCHAR(STRCTLN.BTPOS, ADDR_B);
     2136     7218    3           STRCTLN.ALGN         = ALIGN_TYPES(P_ALIGNMENT_TYPE);
     2137     7219    3           STRCTLN.DATA_TYPE    = DATATYP(P_DATA_TYPE);
     2138     7220    3           IF P_LOG_SIZE ~= 1 AND P_LOG_SIZE ~= 36 AND
     2139     7221    3                   NOT P_SIZE_TYPE_FLAG THEN
     2140     7222    4             DO;
     2141     7223    4               CALL BINCHAR (TEMPC, P_LOG_SIZE);
     2142     7224    4               CALL EDITCHAR(TEMPC, 0, 6, MOP6, TEMPC);
     2143     7225    4               CALL INDEX1R (TEMP_UBIN, ' ', TEMPC);
     2144     7226    4               CALL CONCAT  (STRCTLN.SIZE_INFO, '(', SUBSTR(TEMPC,TEMP_UBIN+1), ')');
     2145     7227    4             END;
     2146     7228    3           IF P_REF_FLAG THEN
     2147     7229    3             STRCTLN.REFF = 'R';
     2148     7230    3           IF P_MODIFIED_FLAG THEN
     2149     7231    3             STRCTLN.MODF = 'M';
     2150     7232    3           CALL Z8IU$CONVERT_DEC_LEFT (STRCTLN.LEVEL, P_LEVEL_NUMBER, LENGTHC(STRCTLN.
              7232                    LEVEL));
     2151     7233    3           CALL Z9IL$OUTPUT_TO_LO(ADDR(STRCTLN), LENGTHC(STRCTLN));
     2152     7234
     2153     7235
     2154     7236    3       END;  /* CASE ON ST.ID */
