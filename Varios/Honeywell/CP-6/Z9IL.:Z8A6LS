     2304        1        /*T***********************************************************/
     2305        2        /*T*                                                         */
     2306        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
     2307        4        /*T*                                                         */
     2308        5        /*T***********************************************************/
     2309        6        Z9IL$BUFFER_ERROR : PROC (LINE$, LENGTH);
     2310        7
     2311        8        /*D*
     2312        9               NAME: Z9IL$BUFFER_ERROR
     2313       10        DESCRIPTION: Z9IL$UNGMAP$SI$READ indirectly calls this routine to buffer error
     2314       11                       lines.  Each time it is called the line is entered into an error
     2315       12                       table where it is saved until reproecessed by Z9IL$UNGMAP$SI$READ.
     2316       13        */
     2317       14
     2318       15    1   DCL LINE$ PTR;
     2319       16    1   DCL LENGTH SBIN WORD;
     2320       17
     2321       18        %EQU PHASE = 5;
     2322       19        %INCLUDE Z8A_DEFC;
     2323       87        %INCLUDE Z8A_SUPP_MAC;
     2324     2487        %INCLUDE Z8A_COMCOM;
     2325     3137        %INCLUDE Z9I_PREPROC;
     2326     3157        %INCLUDE Z8I_LISTING;
     2327     3245        %INCLUDE Z8I_ERR_BUF;
     2328     3309
     2329     3310    1   DCL Z8IL$HANDLE_OVERFLOW_LINE ENTRY(3);
     2330     3311    1   DCL ERRTXT CHAR(LENGTH) BASED(LINE$);
     2331     3312
     2332     3313
     2333     3314    1     IF LENGTH > %MAX_LISTING_LINE_LENGTH THEN
     2334     3315    1       CALL Z8IL$HANDLE_OVERFLOW_LINE( LINE$, LENGTH, ENTADDR(Z9IL$BUFFER_ERROR));
     2335     3316    1     ELSE
     2336     3317    2       DO;
     2337     3318              %RESERVE_BERR(SIZE=LENGTH);
     2338     3330    2         BERR$ -> BERR.TEXT.BODY = ERRTXT;
     2339     3331    2       END;
     2340     3332
PL6.E3A0      #010=Z9IL$BUFFER_ERROR File=Z9IL.:Z8A6TSI                          SAT 08/09/97 14:09 Page:387  
     2341     3333    1   END Z9IL$BUFFER_ERROR;
     2342     3334        %EOD;

