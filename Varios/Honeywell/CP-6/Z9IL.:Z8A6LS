     2245     3586    1   DCL EOF_FOUND BIT(1) STATIC INIT('0'B);
     2246     3587
     2247     3588
     2248     3589    2   DO WHILE ('1'B);
     2249     3590
     2250     3591    2     IF ERR$ ~= ADDR(NIL) THEN
     2251     3592    2       IF ERR$ -> ERR.LINE <= FILL_BUFFER.ISN THEN
     2252     3593    3         DO;
     2253     3594    3           CALL Z8IL$PRINT_ERROR(ENTADDR(Z9IL$BUFFER_ERROR));
     2254     3595                %TOP_BERR;
     2255     3598    3         END;
     2256     3599
     2257     3600    2     IF BERR_.TOP ~= ADDR(NIL) THEN
     2258     3601    2       IF BERR$ ~= ADDR(NIL) THEN
     2259     3602              /* PASS BACK AN ERROR LINE */
     2260     3603    3         DO;
     2261     3604    3           ST_REDEF.VFC_CHAR = 'A';
     2262     3605    3           ST_REDEF.LINE = BERR$ -> BERR.TEXT.BODY;
     2263     3606    3           SOURCE$LINE$NUMBER = FILL_BUFFER.ISN;
     2264     3607                %STEP_BERR;
     2265     3617    3           RETURN;
     2266     3618    3         END;
     2267     3619    2       ELSE
     2268     3620              /* RETURN PAGES TO RESERVER */
     2269     3621    2         CALL Z8AR$FREER(ADDR(BERR_));
     2270     3622
     2271     3623    2     IF NOT EOF_FOUND THEN
     2272     3624            /* GET THE NEXT_SOURCE_LINE */
     2273     3625    3       DO;
     2274     3626    3         CALL Z8IL$FILTER_EXPANDED_S_L ALTRET(EOF);
PL6.E3A0      #009=Z9IL$UNGMAP$SI$READ File=Z9IL.:Z8A6TSI                        SAT 08/09/97 14:09 Page:365  
     2275     3627    3         ST_REDEF.VFC_CHAR = 'A';
     2276     3628    3         CALL Z8IL$BUILD_LISTING_SOURCE_LINE;
     2277     3629    3         ST_REDEF.LINE = PLINE;
     2278     3630    3         SOURCE$LINE$NUMBER = FILL_BUFFER.ISN;
     2279     3631    3         RETURN;
     2280     3632    3       END;
     2281     3633
     2282     3634    2     ALTRETURN;
     2283     3635
     2284     3636    2     EOF:
     2285     3637    2       EOF_FOUND = '1'B;
