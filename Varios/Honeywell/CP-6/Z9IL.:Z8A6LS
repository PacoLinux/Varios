     2239     3580    1   DCL Z8AA$PREPARE_EXPANDED_SOURCE   ENTRY ALTRET;
     2240     3581    1   DCL Z8IL$BUILD_LISTING_SOURCE_LINE ENTRY;
     2241     3582    1   DCL Z8IL$PRINT_ERROR               ENTRY(2);
     2242     3583    1   DCL Z9IL$BUFFER_ERROR              ENTRY(2);
     2243     3584    1   DCL Z8AR$FREER                     ENTRY(1);
