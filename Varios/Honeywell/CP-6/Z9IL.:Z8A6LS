       14       14
       15       15
       16       16        Z9IL$CREATE_HEADING: PROC;
       17       17
       18       18        /*D*
       19       19               NAME: Z9IL$CREATE_HEADINGS
       20       20        DESCRIPTION: Create the heading line for listings.
       21       21
       22       22        */
       23       23
       24       24               /* Include files */
       25       25        %EQU PHASE = 5;
       26       26        %INCLUDE Z8A_DEFC;
       27       94        %INCLUDE Z8A_SUPP_MAC;
       28     2494        % INCLUDE Z8A_SYS_FILE;
       29     8769        % INCLUDE Z8A_COMCOM;
       30     9419        % INCLUDE Z9I_PREPROC;
       31     9439        % INCLUDE Z8A_DNT;
       32     9660        % INCLUDE B$JIT_C;
       33     9974
       34     9975    1   DCL B$JIT$ PTR SYMREF;
       35     9976
       36     9977        %B$JIT0;
       37    10066        %U$JIT1;
PL6.E3A0      #001=Z9IL$CREATE_HEADING File=Z9IL.:Z8A6TSI                        SAT 08/09/97 13:59 Page:2    
       38    10140        %A$JIT6X;
       39    10143
       40    10144    1   DCL Z8IU$BLANK_ZERO ENTRY (1);
       41    10145
       42    10146        %INCLUDE Z9I_HEAD;
       43    10180        % INCLUDE Z8I_LISTING;
       44    10268
       45    10269    1   CALL BINCHAR(LISTING_MAIN_HEADING.JOBN, B$JIT.SYSID);
       46    10270    1   CALL Z8IU$BLANK_ZERO(ADDR(LISTING_MAIN_HEADING.JOBN));
       47    10271
       48    10272    1   LISTING_MAIN_HEADING.DATE = COMCOM.COMPILE.DATE;
       49    10273    1   LISTING_MAIN_HEADING.TIME = COMCOM.COMPILE.TIME;
       50    10274
       51    10275    1   IF COMCOM.OU_EXTERNAL_NAME$ ~= ADDR(NIL) THEN
       52    10276    1     LISTING_MAIN_HEADING.PROGRAM_NAME = COMCOM.OU_EXTERNAL_NAME$ -> DNT.TEXT.NAME;
       53    10277    1   ELSE
       54    10278    1     LISTING_MAIN_HEADING.PROGRAM_NAME = %DEFAULT_PROG_NAME;
       55    10279
       56    10280    1   LISTING_MAIN_HEADING.SOURCE_FILE_NAME = COMCOM.SOURCE_FILE_NAME;
       57    10281
       58    10282    1   IF ADDR(LISTING_SUB_HEADING) = ADDR(NIL)  THEN
       59    10283    2     DO;
       60    10284            %SETUP_SUB_HEADING (L0=DEF_LISTING_SUB_HEADING, L1=BLANK);
       61    10294    2     END;
       62    10295
       63    10296    1   END Z9IL$CREATE_HEADING;
       64    10297        %EOD;

