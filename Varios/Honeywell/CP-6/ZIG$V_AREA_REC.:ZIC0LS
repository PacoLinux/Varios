PL6.E3A0      #001=ZIG$V_AREA_REC File=ZIG$V_AREA_REC.:ZIC0TSI                   FRI 09/05/97 12:18 Page:3    
       63     4709    1   DCL I UBIN (36);
       64     4710    1   DCL TPTR$ PTR;
       65     4711    1   DCL SUCC BIT (1);
       66     4712    1   DCL CURRENT_OFF UBIN (36) EXT;
       67     4713    1   DCL STATUS UBIN (6);
       68     4714    1   DCL ERROR_FLG UBIN (36) STATIC;
       69     4715    1   DCL LAST_RC UBIN (18) EXT;
       70     4716        /*
       71     4717            Note:
       72     4718                   The pointer RC$ and RC1$ are pointing to the current
       73     4719                 RC record that this RA record belongs too.  All we have
       74     4720                 to do is find the AC record in the schema subset and
       75     4721                 get its offset and it conect it up to the sets that it
       76     4722                 belongs to.
       77     4723        */
       78     4724        %EJECT;
PL6.E3A0      #001=ZIG$V_AREA_REC File=ZIG$V_AREA_REC.:ZIC0TSI                   FRI 09/05/97 12:18 Page:4    
       79     4725    1       STATUS = #AREA_DEF;
       80     4726    1       ALLOCATE_END = 1;
       81     4727    1       HASH_SCHEMA_NAME = '1'B;
       82     4728    1       CALL ZIG$HASH_IT(TYPE_420.AREA_NAME,#NO_ADD,ERROR_FLG,0,1,12,
       83     4729    1          TPTR$,STATUS);
       84     4730    1       ALLOCATE_END = 0;
       85     4731    1       IF TYPE_420.SOUR_LINE = 0
       86     4732    2       THEN DO;
       87     4733    2           IF ERROR_FLG = 0
       88     4734    3           THEN DO;
       89     4735    3               RECORD_FOUND = '0'B;
       90     4736    3               RETURN;
       91     4737    3               END;
       92     4738    2           RECORD_FOUND = '1'B;
       93     4739    2           END;
       94     4740    1       IF ERROR_FLG = 0
       95     4741    2       THEN DO;
       96     4742    2           I = RC.DEF.RNL;
       97     4743    2           CALL SET_FIELDS(1,I,RC.RNAM);
       98     4744    2           CALL SET_FIELDS(2,30,TYPE_420.SUB_AREA_NAME);
       99     4745    2           CALL ERROR(%ZIG_V_AREA_NO_FOUND);
      100     4746        /*E*     ERROR:    ZIG-ZIG_V_AREA_NO_FOUND-C
      101     4747        MESSAGE: The area "%U2" cannot be found in the subschema hash table
      102     4748                 for the record "%U1".
      103     4749                 <<<< SOFTWARE ERROR >>>>
      104     4750        */
      105     4751    2           RETURN;
      106     4752    2           END;
      107     4753    1       CALL ZIG$G_DYN_MEM_6(0,RA$);
      108     4754    1       I = POFFW(RA$,SSS$);
      109     4755    1       IF MOD(I,2) = 0
      110     4756    2       THEN DO;
      111     4757    2           CALL ZIG$G_DYN_MEM_6(1,RA$);
      112     4758    2           END;
      113     4759    1       IF RC.DEF.PSTO = 1
      114     4760    2       THEN DO;
      115     4761    2           CALL ZIG$G_DYN_MEM_6(%RA_BASE_SIZE+2,RA$);
PL6.E3A0      #001=ZIG$V_AREA_REC File=ZIG$V_AREA_REC.:ZIC0TSI                   FRI 09/05/97 12:18 Page:5    
      116     4762    2           END;
      117     4763    2       ELSE DO;
      118     4764    2           CALL ZIG$G_DYN_MEM_6(%RA_BASE_SIZE,RA$);
      119     4765    2           END;
      120     4766    1       AC$ = PINCRW(SSS$,TPTR$ -> AREA_HASH.SUB_OFF);
      121     4767    1       IF TYPE_420.SOUR_LINE ~= 0
      122     4768    1       THEN CALL FETCHD_6('AC  ',TPTR$ -> AREA_HASH.DBK);
      123     4769    1       RA.ACRAH = TPTR$ -> AREA_HASH.SUB_OFF;
      124     4770    1       RA.RCRAH = POFFW(RC$,SSS$);
      125     4771    1       RA.RCRAN = RC.RCRAN;
      126     4772    1       CURRENT_OFF = POFFW(RA$,SSS$);
      127     4773    1       IF RC.RCRAN ~= 0
      128     4774    1       THEN RC.DEF.MAR = '1'B;
      129     4775    1       RC.RCRAN = CURRENT_OFF;
      130     4776    1       RA.ACRAN = AC.ACRAN;
      131     4777    1       AC.ACRAN = CURRENT_OFF;
      132     4778    1       RA.DEF.TYPE = '000010000000'B;
      133     4779    1       UWA.DIRECT_REF = 0;
      134     4780    2       DO WHILE (UWA.DIRECT_REF ~= TPTR$ -> AREA_HASH.DBK AND
      135     4781    2          TYPE_420.SOUR_LINE ~= 0 );
      136     4782    2           CALL FETCHNS_6('RC-RA   ') ALTRET (NOT_FOUND);
      137     4783    2           CALL FINDOS_6('AC-RA   ') ALTRET (DBCS_ERROR);
      138     4784    2           END;
      139     4785        /*
      140     4786              We have the current RA record for that record-area relationship
      141     4787            now we just need to get the information from it.
      142     4788        */
      143     4789    1       RA.PGRNG.LOWDBK = UWA.RA.LOWDBK;
      144     4790    1       RA.PGRNG.HIGHDBK= UWA.RA.HIGHDBK;
      145     4791    1       IF RC.DEF.PSTO = 1
      146     4792    2       THEN DO;
      147     4793    2           RA.DEF.TYPE = '000010000001'B;
      148     4794    2           RA.NCHRS = UWA.RA.CALC_HDRS;
      149     4795    2           RA.CHRD = UWA.RA.HDR_OFFS;
      150     4796    2           END;
      151     4797    1       IF UWA.AC.MAX_DBK = UWA.RA.HIGHDBK
      152     4798    1          AND UWA.AC.MIN_DBK = UWA.RA.LOWDBK
PL6.E3A0      #001=ZIG$V_AREA_REC File=ZIG$V_AREA_REC.:ZIC0TSI                   FRI 09/05/97 12:18 Page:6    
      153     4799    1          AND AC.FLAGS.TMPLT
      154     4800    2       THEN DO;
      155     4801    2           RA.DEF.FRANGE = '1'B;
      156     4802    2           RA.PGRNG.HIGHDBK = AC.WSOF;
      157     4803    2           END;
      158     4804        /*
      159     4805              Now write out the type 31 descriptors that relate
      160     4806            this area with that record.
      161     4807        */
      162     4808    1       IF (SUBSTR(RC.RNAM,0,1) = '*') OR (RC.DEF.SR = '1'B)
      163     4809    1       THEN RETURN;
      164     4810    1       TYPE_31.RECORD_NAME = RC.RCID / 2;
      165     4811    1       TYPE_31.AREA_NAME = AC.ACID;
      166     4812    1       TYPE_31.SUB_RECORD_NM = RC.RNAM;
      167     4813    1       TYPE_31.SUB_AREA_NM = AC.ANAM;
      168     4814    1       TYPE_31.TEMPORARY = AC.DEF.ATMP;
      169     4815    1       CALL WRITSF(CUR_EXT_NAME_SEG,TYPE_31);
      170     4816    1       CUR_EXT_NAME_SEG = CUR_EXT_NAME_SEG + 1;
      171     4817    1       CONTROL.NUM_REC_AREA_REL = CONTROL.NUM_REC_AREA_REL + 1;
      172     4818    1       RETURN;
      173     4819    1   NOT_FOUND:
      174     4820    1       I = RC.DEF.RNL;
      175     4821    1       CALL SET_FIELDS(1,I,RC.RNAM);
      176     4822    1       CALL SET_FIELDS(2,30,TYPE_420.SUB_AREA_NAME);
      177     4823    1       CALL ERROR(%ZIG_V_AREA_NO_REL);
      178     4824        /*E*     ERROR:    ZIG-ZIG_V_AREA_NO_REL-C
      179     4825        MESSAGE: The record "%U1" does not have a relationship to the area
      180     4826                 "%U2". That relationship must be given in the schema DDL
      181     4827                 for the record.
      182     4828        */
      183     4829    1       RETURN;
      184     4830    1   DBCS_ERROR:
      185     4831    1       CALL ABORT;
      186     4832    1   END ZIG$V_AREA_REC;

