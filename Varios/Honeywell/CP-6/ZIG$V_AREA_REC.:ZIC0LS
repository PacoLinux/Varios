       35       35            PROC;
       36       36    1   DCL ZIG$HASH_IT ENTRY (8);
       37       37    1   DCL WRITSF ENTRY (2);
       38       38    1   DCL CUR_EXT_NAME_SEG UBIN (36) EXT;
       39       39    1   DCL RECORD_FOUND BIT (1) EXT;
       40       40        %INCLUDE ZI_ERRORS_SUBS;
       41      560        %INCLUDE ZI_SUB_LANG;
       42     2275        %INCLUDE ZIG_DIR_STRUCS;
       43     2405        %GENERAL;
       44     2529        %CONTROL_BLOCK;
       45     2637    1   DCL IO_BUF CHAR (128) EXT;
       46     2638        %DESC_TYPE_420(STCLASS="REDEF IO_BUF");
       47     2671        %DESC_TYPE_31;
