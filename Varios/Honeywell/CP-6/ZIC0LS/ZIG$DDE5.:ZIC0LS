       37      558    1       TEMP_PTR = IN_PTR;
       38      559    1       IF X.X1 ~= '777777'O THEN
       39      560    2       DO;
       40      561    2           TEMP_PTRX.SEGID = %ISSID;
       41      562    2           IF IN_BUF = TEST_DATA THEN
       42      563    2               RETURN_VALUE = '1'B;
       43      564    2           END;
       44      565    1       RETURN;
       45      566    1   END;
