      209        7        /*P*
      210        8        NAME:        FBTOA
      211        9        PURPOSE:     CONVERT BINARY TO ASCII.
      212       10        ARGUMENTS:   BIN_CHAR-> INPUT BINARY CHAR.
      213       11                     DEC_CHAR-> OUTPUT ASCII CHAR.
      214       12        CALLING PROGRAMS:
      215       13                     MAIN FUNCTION             CALLING MODULES
      216       14                     -------------             ---------------
      217       15                     DICTIONARY                DDY4
      218       16                     DIRECTIVE PROCESSOR       DDS1
      219       17                     DISPLAY                   DDX7
      220       18                     DROOT                     DDP9
      221       19                     PURGE                     DDS6
      222       20                     SCHEMA DDL TRANSLATOR     DDA9,DDB5,DDB7,DDB9,DDD7
      223       21                     SCHEMA DMCL TRANSLATOR    DDB7,DDB9,DDC5,DDD8,DDP1
      224       22                                               DDP2,DDP8,DDP9
      225       23                     SCHEMA FILE READY/FINISH  DDE7
      226       24                     SROOT                     DDA1
      227       25                     SSROOT                    DDH8
      228       26                     STORAGE STR REPORT WRITER DDE1
      229       27                     XROOT                     DDX1
      230       28        */
      231       29        FBTOA: PROC (BIN_CHAR, DEC_CHAR);
      232       30        /* Input arguments */
      233       31    1   DCL BIN_CHAR BIT (72);
      234       32    1   DCL DEC_CHAR CHAR (16);
      235       33    1   DCL Y BIT(72) ALIGNED;
      236       34    1   DCL 1 X REDEF Y,
      237       35    1         2 X1 UBIN (36),
      238       36    1         2 X2 UBIN (36);
      239       37        %INCLUDE B_MICROPS_C;
PL6.E3A0      #006=FBTOA File=ZIG$DDE5.:ZIC0TSI                                  FRI 09/05/97 11:44 Page:63   
      240      209    1   DCL MOPSTR (0:1) BIT(9) INIT(%MVZB15, %MVC1) CONSTANT;
      241      210    1       Y = BIN_CHAR;
      242      211    1       CALL BINCHAR (DEC_CHAR,X.X2);
      243      212    1       CALL EDITSTR(DEC_CHAR,0,16,MOPSTR,DEC_CHAR);
      244      213    1       RETURN;
      245      214    1   END;
