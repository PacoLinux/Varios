      263        6        SYS_FUNC_OPEN: PROC /*Dcbsf,  dcb number to be opened */
      264        7         /*Orgsf,  file organization */
      265        8         /*Testsf  %YES# = test open only */
      266        9         /*Xtensizesf, granuals reserved at create */
      267       10         /*Acssf    access mode (seq,direct) */
      268       11         /*Funsf    function (in,out,update) */
      269       12         /*Anssf    1=file 2=tape 3=device */
      270       13         /*Bkupsf   create attrib ..backup of file */
      271       14         /*Acnt     list of accounts w/exec priv */
      272       15         /*Fidsf    file or dcb name */
      273       16         /*Existsf  open mode of the file */
      274       17         /*Sharesf  share options on the 'DCB' */
      275       18               ALTRET;
      276       19    1   DCL M$SI DCB;
      277       20    1   DCL SYS_FUNC_ERRMSG ENTRY ALTRET;
