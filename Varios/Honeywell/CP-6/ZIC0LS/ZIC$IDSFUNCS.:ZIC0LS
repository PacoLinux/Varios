      638     1082    2           END;
      639     1083    1       CALL M$WRITE(WRITE_FPT) ALTRET(WRT_ERR);
      640     1084         /*    Put in defaults for next call */
      641     1085    1       WAITSF = %YES#;
      642     1086    1       FULLSF = %NO#;
      643     1087    1       NOTIMESF = %NO#;
      644     1088    1       VFCSF = %YES#;
      645     1089    1       ENCRP_SEEDSF = '0'B;
      646     1090    1       KEYSF = %NO#;
