      493        6        SYS_FUNC_CLOS: PROC ALTRET;
      494        7    1   DCL SYS_FUNC_ERRMSG ENTRY ALTRET;
      495        8        %INCLUDE ZI_GLOBAL_DBUTIL;
      496      186         %SYS_FUNC;
      497      278         %MISC_DCLS;
      498      331        %INCLUDE CP_6;
      499      412        %VLP_SN(FPTN=PSN,
      500      413        N=15); /* Allow a maximum of 15 SN#s to be returned */
      501      426        %INCLUDE CP_6_SUBS;
      502      966        %B$TCB;
      503      969        %B$EXCFR;
      504      987        %F$DCB;
      505     1044        %FPT_CLOSE(FPTN=CLOSE_FPT);
      506     1086    1       IF DCBADDR(DCBSF)->F$DCB.FCD# = '0'B
      507     1087    1       THEN RETURN; /* Ignore CALL..DCB is already closed */
      508     1088    1       CLOSE_FPT.V.DCB#=DCBSF;
      509     1089    1       CLOSE_FPT.V.OPER.RELG# = %YES#;
      510     1090    1       CLOSE_FPT.V.POS# = POSSF; /* Tape position on close */
      511     1091    1       CLOSE_FPT.SN_ = VECTOR(PSN);
      512     1092    1       CALL M$CLOSE(CLOSE_FPT) ALTRET(CLOSE_ERR);
      513     1093    1       POSSF = 0; /* RESET DEFAULT */
      514     1094    1       IF DCBADDR(DCBSF)->F$DCB.ASN# = %TAPE# THEN
      515     1095    2       DO;
      516     1096    2           PSNSF.SN# = PSN.SN#(0);
      517     1097    2           PSNSF.NUM# = PSN.NUM#;
      518     1098    2           PSNSF.VOL# = DCBADDR(DCBSF)->F$DCB.VOL#;
      519     1099    2           END;
      520     1100    1       RETURN;
      521     1101    1   CLOSE_ERR:
      522     1102    1       MONITOR_ERR = B$TCB$->B$TCB.ALT$->B$EXCFR.ERR;
      523     1103    1       INCL_ERR = %YES#;
      524     1104    1       CALL SYS_FUNC_ERRMSG;
PL6.E3A0      #011=SYS_FUNC_CLOS File=ZIC$IDSFUNCS.:ZIC0TSI                      FRI 09/05/97 11:40 Page:144  
      525     1105    1       ALTRETURN;
      526     1106    1   END SYS_FUNC_CLOS;
