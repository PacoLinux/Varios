      615     1012        %B$EXCFR;
      616     1030        %FPT_WRITE(FPTN=WRITE_FPT);
      617     1061    1       WRITE_FPT.V.DCB#=DCBSF;
      618     1062    1       IF BUFLNSF = 0 THEN
      619     1063    1           WRITE_FPT.BUF_ = VECTOR(NIL);
      620     1064    2       ELSE DO;
      621     1065    2           IF BUFLNSF > 0 THEN BUFLNSF = BUFLNSF-1; /* Zero base array*/
      622     1066    2           WRITE_FPT.BUF_ = BUFSF;
      623     1067    2           WRITE_FPT.BUF_.BOUND = BUFLNSF;
      624     1068    2           END;
      625     1069    1       WRITE_FPT.V.WAIT#=WAITSF;
      626     1070    1       WRITE_FPT.V.NOTIME#=NOTIMESF;
      627     1071    1       WRITE_FPT.V.DVBYTE.VFC#=VFCSF;
      628     1072    1       WRITE_FPT.V.FULL#=FULLSF;
      629     1073    1       WRITE_FPT.V.SEED#=ENCRP_SEEDSF;
      630     1074    1       IF KEYVSF.WD_ONE = '    ' THEN
      631     1075    1           WRITE_FPT.KEY_=VECTOR(NIL);
      632     1076    1       ELSE
      633     1077    2       DO;
      634     1078    2           IF DCBADDR(DCBSF)->F$DCB.ORG# = %IDS#
      635     1079    2           THEN WRITE_FPT.KEY_ = VECTOR(KEYVSF.IDSKEY);
      636     1080    2           ELSE
      637     1081    2               WRITE_FPT.KEY_ = VECTOR(KEYVSF);
