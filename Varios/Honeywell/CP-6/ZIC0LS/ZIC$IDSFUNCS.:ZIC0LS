      533        6        SYS_FUNC_READ: PROC ALTRET;
      534        7    1   DCL SYS_FUNC_ERRMSG ENTRY ALTRET;
      535        8        %INCLUDE ZI_GLOBAL_DBUTIL;
      536      186         %SYS_FUNC;
      537      278         %MISC_DCLS;
      538      331        %INCLUDE CP_6;
      539      412        %INCLUDE CP_6_SUBS;
      540      952        %B$TCB;
      541      955        %B$EXCFR;
      542      973        %INCLUDE B_ERRORS_C;
      543     2100        %F$DCB;
      544     2157        %FPT_READ(FPTN=READ_FPT);
      545     2196    1       READ_FPT.V.DCB#=DCBSF;
      546     2197    1       READ_FPT.V.FULL#=FULLSF;
      547     2198    1       READ_FPT.V.KEYS#=KEYSF;
      548     2199    1       IF KEYSF THEN
      549     2200    2       DO;
      550     2201    2           NO_KEYSF = %NO#; /* Reset non existant key flag */
      551     2202    2           IF DCBADDR(DCBSF)->F$DCB.ORG# = %IDS# THEN
      552     2203    2               READ_FPT.KEY_ = VECTOR(KEYVSF.IDSKEY);
      553     2204    2           ELSE
      554     2205    2               READ_FPT.KEY_ = VECTOR(KEYVSF);
      555     2206    2           END;
      556     2207    2       ELSE DO;
      557     2208    2           READ_FPT.V.KEYR# = %NO#;
      558     2209    2           READ_FPT.KEY_ = VECTOR(NIL);
      559     2210    2           END;
      560     2211    1       IF KEYRSF THEN
      561     2212    2       DO;
      562     2213    2           READ_FPT.V.KEYR# = '1'B;
      563     2214    2           READ_FPT.KEY_ = VECTOR(KEYVSF);
      564     2215    2           END;
PL6.E3A0      #012=SYS_FUNC_READ File=ZIC$IDSFUNCS.:ZIC0TSI                      FRI 09/05/97 11:40 Page:161  
      565     2216    1       IF BUFLNSF = 0 THEN
      566     2217    1           READ_FPT.BUF_ = VECTOR(NIL);
      567     2218    2       ELSE DO;
      568     2219    2           IF BUFLNSF > 0 THEN BUFLNSF = BUFLNSF-1; /* Zero base array*/
      569     2220    2           READ_FPT.BUF_ = BUFSF;
      570     2221    2           READ_FPT.BUF_.BOUND = BUFLNSF;
      571     2222    2           END;
      572     2223    1       READ_FPT.V.SEED#=ENCRP_SEEDSF;
      573     2224    1       CALL M$READ(READ_FPT) ALTRET(READ_ERR);
      574     2225         /*   Put in the defaults for next read */
      575     2226    1       FULLSF = %NO#;
      576     2227    1       ENCRP_SEEDSF = '0'B;
      577     2228    1       KEYSF = %NO#;
      578     2229    1       KEYRSF = %NO#;
      579     2230    1       RETURN;
      580     2231    1   READ_ERR:
      581     2232         /*   Put in the defaults for next read */
      582     2233    1       FULLSF = %NO#;
      583     2234    1       ENCRP_SEEDSF = '0'B;
      584     2235    1       KEYSF = %NO#;
      585     2236    1       KEYRSF = %NO#;
      586     2237    1       IF B$TCB$->B$TCB.ALT$->B$EXCFR.ERR.ERR# = %E$NOKEY THEN
      587     2238    2       DO;
      588     2239    2           NO_KEYSF = %YES#;
      589     2240    2           RETURN;
      590     2241    2           END;
      591     2242    1       IF B$TCB$->B$TCB.ALT$->B$EXCFR.ERR.ERR# = %E$EOF
      592     2243    1       THEN ALTRETURN;
      593     2244    2       ELSE DO;
      594     2245    2           MONITOR_ERR = B$TCB$->B$TCB.ALT$->B$EXCFR.ERR;
      595     2246    2           INCL_ERR = %YES#;
      596     2247    2           CALL SYS_FUNC_ERRMSG;
      597     2248    2           END;
      598     2249    1       ALTRETURN;
      599     2250    1   END SYS_FUNC_READ;
