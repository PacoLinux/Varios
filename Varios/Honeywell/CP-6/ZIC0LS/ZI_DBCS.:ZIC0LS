FRI. SEP 05 '97 11:21:33.72  FILE=ZI_DBCS

%MACRO ASL;
%EQU ASLFLAG=1;
DCL I$ROL ENTRY(1)  CONV(2,2)  ;
DCL I$HIA ENTRY(1)  CONV (2,5) ;
DCL I$RDY ENTRY(1)  CONV (2,5) ;
DCL I$FIN ENTRY(1)  CONV (2,5) ;
DCL I$STO ENTRY(1)  CONV (2,5) ;
DCL I$ERS ENTRY(1)  CONV (2,5) ;
DCL I$MOD ENTRY(1)  CONV (2,5) ;
DCL I$CON ENTRY(1)  CONV (2,5) ;
DCL I$DIS ENTRY(1)  CONV (2,5) ;
DCL I$ACN ENTRY(1)  CONV (2,7) ;
DCL I$ACC ENTRY(1)  CONV (2,6) ;
DCL I$IFT ENTRY(1)  CONV (2,5) ;
DCL I$IST ENTRY(1)  CONV (2,5) ;
DCL I$IFM ENTRY(1)  CONV (2,5) ;
DCL I$FKY ENTRY(1)  CONV (2,5) ;
DCL I$FKC ENTRY(1)  CONV (2,5) ;
DCL I$FDP ENTRY(1)  CONV (2,5) ;
DCL I$FDC ENTRY(1)  CONV (2,5) ;
DCL I$FCD ENTRY(1)  CONV (2,5) ;
DCL I$FOR ENTRY(1)  CONV (2,5) ;
DCL I$FCT ENTRY(1)  CONV (2,5) ;
DCL I$FCR ENTRY(1)  CONV (2,5) ;
DCL I$FAS ENTRY(1)  CONV (2,5) ;
DCL I$FAA ENTRY(1)  CONV (2,5) ;
DCL I$FRS ENTRY(1)  CONV (2,5) ;
DCL I$FRA ENTRY(1)  CONV (2,5) ;
DCL I$FDR ENTRY(1)  CONV (2,6) ;
DCL I$FUI ENTRY(1)  CONV (2,5) ;
DCL I$FRI ENTRY(1)  CONV (2,5) ;
DCL I$GET ENTRY(1)  CONV (2,5) ;
DCL I$GTF ENTRY(1)  CONV (2,5) ;
DCL I$CHK ENTRY(1)  CONV (2,3) ;
DCL I$TRON ENTRY(1) CONV(2,1)  ;
DCL I$TROFF ENTRY(1) CONV(2,1) ;
DCL I$STON ENTRY(0) CONV(2,0)  ;
DCL I$STOFF ENTRY(0) CONV(2,0) ;
DCL I$SHIN ENTRY(0) CONV(2,0)  ;
DCL I$SHALL ENTRY(0) CONV (2,0);
DCL I$SHNO ENTRY(0) CONV(2,0)  ;
DCL I$SHANY ENTRY(0) CONV(2,0);
DCL I$SHCUR ENTRY(0) CONV(2,0);
DCL I$SHINCUR ENTRY(0) CONV(2,0);
DCL I$KEEP  ENTRY(1) CONV(2,1);
DCL I$REDUCEQ ENTRY(1) CONV(2,1);
DCL I$CRPT ENTRY(1) CONV(2,1)  ;
DCL I$RPT ENTRY(0)  CONV(2,0)  ;
DCL I$NBF ENTRY(1)  CONV (2,2) ;
DCL I$ETC ENTRY(1)  CONV (2,5) ;
DCL I$INA ENTRY(1)  CONV (2,5) ;
DCL I$CALC ENTRY(1) CONV(2,6);
%MEND;
%MACRO USER;
%EQU ASLFLAG=0;
DCL U$ROL ENTRY(1) ;
DCL U$HIA ENTRY(1) ;
DCL U$RDY ENTRY(1) ;
DCL U$FIN ENTRY(1) ;
DCL U$STO ENTRY(1) ;
DCL U$ERS ENTRY(1) ;
DCL U$MOD ENTRY(1) ;
DCL U$CON ENTRY(1) ;
DCL U$DIS ENTRY(1) ;
DCL U$ACN ENTRY(1) ;
DCL U$ACC ENTRY(1) ;
DCL U$IFT ENTRY(1) ;
DCL U$IST ENTRY(1) ;
DCL U$IFM ENTRY(1) ;
DCL U$FKY ENTRY(1) ;
DCL U$FKC ENTRY(1) ;
DCL U$FDP ENTRY(1) ;
DCL U$FDC ENTRY(1) ;
DCL U$FCD ENTRY(1) ;
DCL U$FOR ENTRY(1) ;
DCL U$FCT ENTRY(1) ;
DCL U$FCR ENTRY(1) ;
DCL U$FAS ENTRY(1) ;
DCL U$FAA ENTRY(1) ;
DCL U$FRS ENTRY(1) ;
DCL U$FRA ENTRY(1) ;
DCL U$FDR ENTRY(1) ;
DCL U$FUI ENTRY(1) ;
DCL U$FRI ENTRY(1) ;
DCL U$GET ENTRY(1) ;
DCL U$GTF ENTRY(1) ;
DCL U$CHK ENTRY(1) ;
DCL U$TRON ENTRY(1);
DCL U$TROFF ENTRY(1);
DCL U$STON ENTRY(0);
DCL U$STOFF ENTRY(0);
DCL U$SHIN ENTRY(0);
DCL U$SHALL ENTRY(0);
DCL U$SHNO ENTRY(0);
DCL U$SHANY ENTRY(0);
DCL U$SHCUR ENTRY(0);
DCL U$SHINCUR ENTRY(0);
DCL U$KEEP  ENTRY(1);
DCL U$REDUCEQ ENTRY(1);
DCL U$CRPT ENTRY(1);
DCL U$RPT ENTRY(0) ;
DCL U$NBF ENTRY(1) ;
DCL U$ETC ENTRY(1) ;
DCL U$INA ENTRY(1) ;
DCL U$CALC ENTRY(1);
%MEND;
%MACRO CLIMB(PARAMS=1,FCN=UNDEF);
%IF %ASLFLAG=1;
         %SET X=%CHARTEXT(CONCAT('I$',TEXTCHAR(FCN)));
%ELSE;
         %SET X=%CHARTEXT(CONCAT('U$',TEXTCHAR(FCN)));
%ENDIF;
%IF %TEXTCHAR(PARAMS)='1';
        CALL %X(%CHARTEXT(CONCAT('IPT_',TEXTCHAR(FCN))));
         %ELSE;
        CALL %X;
         %ENDIF;
%MEND;
         %MACRO IPT_ACC(STCLASS=STATIC,
          IAN=NIL,
          UWAN=NIL,
          RVAN=NIL,
          USEPN=NIL,
          PARM3=NIL,
          NAME=IPT_ACC,
          SCI(RUN=0,TYPE=1,AREA=2,SET=3,NEXT=4,PRIOR=5,OWNER=6)=0,
          OFFSET=0);
         %SET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
         %IF (SUBSTR(STR,0,6)='STATIC')OR(SUBSTR(STR,0,8)='CONSTANT')OR(SUBSTR(STR,0,3)='EXT');
         %SET INIT=CHARTEXT('INIT');
         %ELSE;
         %SET INIT=CHARTEXT('/*');
         %ENDIF;
DCL 1 NAME STCLASS DALIGNED,
      2 V_ VECTOR %INIT(VECTOR(NAME.V))%CHARTEXT('/**/'),
      2 IA_ VECTOR %INIT(VECTOR(IAN))%CHARTEXT('/**/'),
      2 UWA_ VECTOR %INIT(VECTOR(UWAN))%CHARTEXT('/**/'),
      2 RVA_ VECTOR %INIT(VECTOR(RVAN))%CHARTEXT('/**/'),
      2 USEP_ VECTOR %INIT(VECTOR(USEPN))%CHARTEXT('/**/'),
      2 DBK_ VECTOR %INIT(VECTOR(PARM3))%CHARTEXT('/**/'),
      2 V DALIGNED,
        3 CCW,
          4 SCI# UBIN HALF UNAL %INIT(SCI)%CHARTEXT('/**/'),
          4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/'), /* Filler             */
        3 * UBIN UNAL %INIT(0)%CHARTEXT('/**/'), /* Unused                    */
        3 RVA_ITEM,
          4 OFFSET# SBIN HALF UNAL %INIT(OFFSET)%CHARTEXT('/**/'),
          4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/');
%MEND;

