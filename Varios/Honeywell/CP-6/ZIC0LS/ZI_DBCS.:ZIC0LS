%MACRO IPT_MOD(STCLASS=STATIC,
IAN=NIL,
UWAN=NIL,
RVAN=NIL,
USEPN=NIL,
NAME=IPT_MOD,
RC_OFFSET=0,
MC_BIT0='0'B,
MC_BIT1='0'B,
N_FIELDS=0,
N_SETS=0,
FLSL_LIST_SIZE=0);
         %SET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
         %IF (SUBSTR(STR,0,6)='STATIC')OR(SUBSTR(STR,0,8)='CONSTANT')OR(SUBSTR(STR,0,3)='EXT');
         %SET INIT=CHARTEXT('INIT');
         %ELSE;
         %SET INIT=CHARTEXT('/*');
         %ENDIF;
DCL 1 NAME STCLASS DALIGNED,
      2 V_ VECTOR %INIT(VECTOR(NAME.V))%CHARTEXT('/**/'),
      2 IA_ VECTOR %INIT(VECTOR(IAN))%CHARTEXT('/**/'),
      2 UWA_ VECTOR %INIT(VECTOR(UWAN))%CHARTEXT('/**/'),
      2 RVA_ VECTOR %INIT(VECTOR(RVAN))%CHARTEXT('/**/'),
      2 USEP_ VECTOR %INIT(VECTOR(USEPN))%CHARTEXT('/**/'),
      2 V DALIGNED,
        3 RC,
          4 RC_OFFSET# SBIN HALF UNAL %INIT(RC_OFFSET)%CHARTEXT('/**/'),
          4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/'), /* Filler             */
        3 MCW,
          4 MC_BIT0# BIT(1) UNAL %INIT(MC_BIT0)%CHARTEXT('/**/'),
          4 MC_BIT1# BIT(1) UNAL %INIT(MC_BIT1)%CHARTEXT('/**/'),
          4 * BIT(34) %INIT('0'B)%CHARTEXT('/**/'), /* Filler                 */
        3 FLCW,
          4 N_FIELDS# SBIN HALF UNAL %INIT(N_FIELDS)%CHARTEXT('/**/'),
          4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/'), /* Filler             */
        3 SLCW,
          4 N_SETS# SBIN HALF UNAL %INIT(N_SETS)%CHARTEXT('/**/'),
          4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/'), /* Filler             */
        3 FL_SL_LIST(0:FLSL_LIST_SIZE),
          4 RVA_OFFSET# SBIN HALF UNAL %INIT(0*0)%CHARTEXT('/**/'),
          4 *  UBIN HALF UNAL %INIT(0*0)%CHARTEXT('/**/');
%MEND;

