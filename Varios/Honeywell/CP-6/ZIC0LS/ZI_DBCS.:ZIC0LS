   112 - %MACRO CLIMB(PARAMS=1,FCN=UNDEF);
   113 - %IF %ASLFLAG=1;
   114 -          %SET X=%CHARTEXT(CONCAT('I$',TEXTCHAR(FCN)));
   115 - %ELSE;
   116 -          %SET X=%CHARTEXT(CONCAT('U$',TEXTCHAR(FCN)));
   117 - %ENDIF;
   118 - %IF %TEXTCHAR(PARAMS)='1';
   119 -         CALL %X(%CHARTEXT(CONCAT('IPT_',TEXTCHAR(FCN))));
   120 -          %ELSE;
   121 -         CALL %X;
   122 -          %ENDIF;
   123 - %MEND;
   124 -          %MACRO IPT_ACC(STCLASS=STATIC,
   125 -           IAN=NIL,
   126 -           UWAN=NIL,
   127 -           RVAN=NIL,
   128 -           USEPN=NIL,
   129 -           PARM3=NIL,
   130 -           NAME=IPT_ACC,
   131 -           SCI(RUN=0,TYPE=1,AREA=2,SET=3,NEXT=4,PRIOR=5,OWNER=6)=0,
   132 -           OFFSET=0);
   133 -          %SET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
   134 -          %IF (SUBSTR(STR,0,6)='STATIC')OR(SUBSTR(STR,0,8)='CONSTANT')OR(SUBSTR(STR,0,3)='EXT');
   135 -          %SET INIT=CHARTEXT('INIT');
   136 -          %ELSE;
   137 -          %SET INIT=CHARTEXT('/*');
   138 -          %ENDIF;
   139 - DCL 1 NAME STCLASS DALIGNED,
   140 -       2 V_ VECTOR %INIT(VECTOR(NAME.V))%CHARTEXT('/**/'),
   141 -       2 IA_ VECTOR %INIT(VECTOR(IAN))%CHARTEXT('/**/'),
   142 -       2 UWA_ VECTOR %INIT(VECTOR(UWAN))%CHARTEXT('/**/'),
   143 -       2 RVA_ VECTOR %INIT(VECTOR(RVAN))%CHARTEXT('/**/'),
   144 -       2 USEP_ VECTOR %INIT(VECTOR(USEPN))%CHARTEXT('/**/'),
   145 -       2 DBK_ VECTOR %INIT(VECTOR(PARM3))%CHARTEXT('/**/'),
   146 -       2 V DALIGNED,
   147 -         3 CCW,
   148 -           4 SCI# UBIN HALF UNAL %INIT(SCI)%CHARTEXT('/**/'),
   149 -           4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/'), /* Filler             */
   150 -         3 * UBIN UNAL %INIT(0)%CHARTEXT('/**/'), /* Unused                    */
   151 -         3 RVA_ITEM,
   152 -           4 OFFSET# SBIN HALF UNAL %INIT(OFFSET)%CHARTEXT('/**/'),
   153 -           4 * UBIN HALF UNAL %INIT(0)%CHARTEXT('/**/');
   154 - %MEND;
