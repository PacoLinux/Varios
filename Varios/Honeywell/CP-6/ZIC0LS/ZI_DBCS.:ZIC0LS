    23 - DCL I$FKC ENTRY(1)  CONV (2,5) ;
    24 - DCL I$FDP ENTRY(1)  CONV (2,5) ;
    25 - DCL I$FDC ENTRY(1)  CONV (2,5) ;
    26 - DCL I$FCD ENTRY(1)  CONV (2,5) ;
