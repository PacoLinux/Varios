     8 - DCL I$ROL ENTRY(1)  CONV(2,2)  ;
     9 - DCL I$HIA ENTRY(1)  CONV (2,5) ;
    10 - DCL I$RDY ENTRY(1)  CONV (2,5) ;
    11 - DCL I$FIN ENTRY(1)  CONV (2,5) ;
    12 - DCL I$STO ENTRY(1)  CONV (2,5) ;
    13 - DCL I$ERS ENTRY(1)  CONV (2,5) ;
    14 - DCL I$MOD ENTRY(1)  CONV (2,5) ;
    15 - DCL I$CON ENTRY(1)  CONV (2,5) ;
    16 - DCL I$DIS ENTRY(1)  CONV (2,5) ;
    17 - DCL I$ACN ENTRY(1)  CONV (2,7) ;
