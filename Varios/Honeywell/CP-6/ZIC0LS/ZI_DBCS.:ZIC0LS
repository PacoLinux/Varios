%MACRO IPT_FDR(STCLASS=STATIC,
IAN=NIL,
UWAN=NIL,
RVAN=NIL,
USEPN=NIL,
PARM3=NIL,
NAME=IPT_FDR,
RC_OFFSET=0,
AC_INDICATORS='0'B,
SC_INDICATORS='0'B,
RC_INDICATORS='0'B,
N_SETS=0);
         %SET STR=CONCAT(TEXTCHAR(STCLASS),'        ');
         %IF (SUBSTR(STR,0,6)='STATIC')OR(SUBSTR(STR,0,8)='CONSTANT')OR(SUBSTR(STR,0,3)='EXT');
         %SET INIT=CHARTEXT('INIT');
         %ELSE;
         %SET INIT=CHARTEXT('/*');
         %ENDIF;
