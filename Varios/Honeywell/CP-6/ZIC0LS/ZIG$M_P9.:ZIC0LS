        8        8            PROC(WORD1,TYPE,SUCC);
        9        9    1   DCL 01 FR_NMS(0:0) BASED(PFRN),
       10       10    1         02 NAME CHAR(30),
       11       11    1         02 FADDR UBIN(36);
       12       12    1   DCL SIZE UBIN(36) STATIC; /* # of fr record entries*/
       13       13    1   DCL NUMFR UBIN(36) STATIC; /* # of fr records */
       14       14    1   DCL WORD1 CHAR(30);
       15       15    1   DCL SUCC BIT (1);
       16       16    1   DCL ZIG$G_DYN_MEM_6 ENTRY (2);
       17       17    1   DCL TYPE UBIN(36);
       18       18    1   DCL PFRN PTR STATIC;
       19       19    1   DCL Y UBIN (36);
       20       20    1   DCL X UBIN (36);
       21       21    1   DCL I UBIN (36);
       22       22    1   DCL 1 SAVED_NAMES STATIC,
       23       23    1         2 AREA_DBK UBIN (36) ALIGNED,
       24       24    1         2 AREA_NAME CHAR (30) ALIGNED,
       25       25    1         2 SET_DBK UBIN (36) ALIGNED,
       26       26    1         2 SET_NAME CHAR (30) ALIGNED,
       27       27    1         2 KEY_DBK UBIN (36) ALIGNED,
       28       28    1         2 KEY_NAME CHAR (30) ALIGNED,
       29       29    1         2 REC_DBK UBIN (36) ALIGNED,
       30       30    1         2 REC_NAME CHAR (30) ALIGNED;
       31       31    1   DCL DBK UBIN (36) ALIGNED STATIC;
       32       32    1   DCL J UBIN (36) EXT;
       33       33        /*D*     NAME: J
       34       34                   this variable tells what function is being performed
       35       35                 inside of DBACS.
       36       36        */
       37       37        %INCLUDE ZIG_TYPE_SUBS;
PL6.E3A0      #001=ZIG$M_P9 File=ZIG$M_P9.:ZIC0TSI                               FRI 09/05/97 12:08 Page:2    
       38       52        %INCLUDE ZI_SUB4_CAN;
       39      709        %UWA;
       40     1044        %INCLUDE ZIG_DBA_INT;
       41     1092         /* Define dbcs interface routines */
       42     1093         /* Only for fetchns will return to the label be executed
       43     1094            when other than a zero status occurs.
       44     1095            For all other routines the subroutine will abort.
       45     1096            Therefore, no return to the label takes place -
       46     1097            the label is a dummmmy */
       47     1098         /*  S e a r c h     r o u t i n e s */
       48     1099         /* This routine is called at the beginning of the dmcl
       49     1100            translator phase. Its purpose is to generate a table
       50     1101            containing the name and dbk of every 50TH fr name.
       51     1102            The table generated is used by the search routines
       52     1103            to speed up the SEARCH..
       53     1104            The database is assumed to be ready
       54     1105            the oa rec is assumed to be current of record type*/
       55     1106    1   BLD_NM:
       56     1107    1       ENTRY;
       57     1108    1       CALL FETCHD_6('DA  ',UWA.SF.DBKEY_DA); /* Get da record */
       58     1109    1       SAVED_NAMES.AREA_NAME = ' ';
       59     1110    1       SAVED_NAMES.SET_NAME = ' ';
       60     1111    1       SAVED_NAMES.KEY_NAME = ' ';
       61     1112    1       SAVED_NAMES.REC_NAME = ' ';
       62     1113    1       SAVED_NAMES.AREA_DBK = UWA.DA.FRST_AC;
       63     1114    1       SAVED_NAMES.SET_DBK = UWA.DA.FRST_SC;
       64     1115    1       SAVED_NAMES.KEY_DBK = UWA.DA.FRST_KC;
       65     1116    1       SAVED_NAMES.REC_DBK = UWA.DA.FRST_RC;
       66     1117    1       IF J = 6 /* VALIDATION */
       67     1118    1       THEN RETURN;
       68     1119        /*W*
       69     1120              If validation calls SRCHD with TYPE = to FR type then
       70     1121            things will not work the best since we don't set up things
       71     1122            in PFRN that contains a list of fields names and there
       72     1123            dbks.
       73     1124        */
       74     1125    1       NUMFR = UWA.OA.NUMB_FR; /* Get # of fr records */
PL6.E3A0      #001=ZIG$M_P9 File=ZIG$M_P9.:ZIC0TSI                               FRI 09/05/97 12:08 Page:3    
       75     1126    1       IF NUMFR < 50 THEN
       76     1127    1           RETURN; /* Not enough to worry about */
       77     1128    1       SIZE=(NUMFR-1)/10;
       78     1129    1       CALL ZIG$G_DYN_MEM_6(SIZE*9,PFRN);/* Get dynamic mem*/
       79     1130    1       CALL FETCHD_6('SN  ', UWA.DA.FRST_FR); /*Get first fr */
       80     1131    1       X = 0;
       81     1132    2       DO WHILE(X ~= SIZE);
       82     1133    2           FR_NMS.NAME(X) = UWA.SN.SENAME;
       83     1134    2           FR_NMS.FADDR(X) = UWA.DIRECT_REF;
       84     1135    3           DO Y = 1 TO 10; /* Bypass 10 */
       85     1136    3               CALL FETCHNS_6('DA-SN  ') ALTRET(BDONE);
       86     1137    3               END;
       87     1138    2           X = X + 1;
       88     1139    2           END;
       89     1140    1   BDONE:
       90     1141    1       RETURN;
       91     1142    1   SRCHD:
       92     1143    1       ENTRY(WORD1,TYPE,SUCC);
       93     1144    1       SUCC='0'B;
       94     1145    1       IF TYPE = %ZIG_RTYPE_FR THEN
       95     1146    2       DO;
       96     1147    2           IF NUMFR < 50 THEN
       97     1148    3           DO;
       98     1149    3               CALL FETCHD_6('SN  ', UWA.DA.FRST_FR);
       99     1150    3               GOTO SSEARCH;
      100     1151    3               END;
      101     1152    3           DO I = 0 TO SIZE; /* Size = # entries of FR_NMS*/
      102     1153    3               IF WORD1 = FR_NMS.NAME(I) THEN
      103     1154    4               DO;
      104     1155    4                   CALL FETCHD_6('SN  ', FR_NMS.FADDR(I));
      105     1156    4                   GOTO SSEARCH;
      106     1157    4                   END;
      107     1158    3               IF WORD1 < FR_NMS.NAME(I) THEN
      108     1159    4               DO;
      109     1160    4                   CALL FETCHD_6('SN  ', FR_NMS.FADDR(I-1));
      110     1161    4                   GOTO SSEARCH;
      111     1162    4                   END;
PL6.E3A0      #001=ZIG$M_P9 File=ZIG$M_P9.:ZIC0TSI                               FRI 09/05/97 12:08 Page:4    
      112     1163    3               END;
      113     1164         /* Fr must be one of the last - past last FR_NMS */
      114     1165    2           CALL FETCHD_6('SN  ', FR_NMS.FADDR(I));
      115     1166    2           GOTO SSEARCH;
      116     1167    2           END;
      117     1168    1       IF TYPE = %ZIG_RTYPE_OA THEN
      118     1169    2       DO;
      119     1170    2           CALL FETCHD_6('OA  ', UWA.SF.DBKEY_OA);
      120     1171    2           CALL FETCHOS_6('SN-SE   ');
      121     1172    2           IF UWA.SN.SENAME = WORD1 THEN SUCC='1'B;
      122     1173    2           RETURN;
      123     1174    2           END;
      124     1175    2       DO CASE (TYPE);
      125     1176    2       CASE (%ZIG_RTYPE_AC);
      126     1177    2           IF WORD1 >= SAVED_NAMES.AREA_NAME
      127     1178    3           THEN DO;
      128     1179    3               DBK = SAVED_NAMES.AREA_DBK;
      129     1180    3               END;
      130     1181    3           ELSE DO;
      131     1182    3               DBK = UWA.DA.FRST_AC;
      132     1183    3               END;
      133     1184    2       CASE (%ZIG_RTYPE_RC);
      134     1185    2           IF WORD1 >= SAVED_NAMES.REC_NAME
      135     1186    3           THEN DO;
      136     1187    3               DBK = SAVED_NAMES.REC_DBK;
      137     1188    3               END;
      138     1189    3           ELSE DO;
      139     1190    3               DBK = UWA.DA.FRST_RC;
      140     1191    3               END;
      141     1192    2       CASE (%ZIG_RTYPE_SC);
      142     1193    2           IF WORD1 >= SAVED_NAMES.SET_NAME
      143     1194    3           THEN DO;
      144     1195    3               DBK = SAVED_NAMES.SET_DBK;
      145     1196    3               END;
      146     1197    3           ELSE DO;
      147     1198    3               DBK = UWA.DA.FRST_SC;
      148     1199    3               END;
PL6.E3A0      #001=ZIG$M_P9 File=ZIG$M_P9.:ZIC0TSI                               FRI 09/05/97 12:08 Page:5    
      149     1200    2       CASE (%ZIG_RTYPE_KC);
      150     1201    2           IF WORD1 >= SAVED_NAMES.KEY_NAME
      151     1202    3           THEN DO;
      152     1203    3               DBK = SAVED_NAMES.KEY_DBK;
      153     1204    3               END;
      154     1205    3           ELSE DO;
      155     1206    3               DBK = UWA.DA.FRST_KC;
      156     1207    3               END;
      157     1208    2           END;
      158     1209    1       CALL FETCHD_6('SN  ', DBK);
      159     1210    1   SSEARCH:
      160     1211    1       IF TYPE ~= UWA.SN.SETYPE THEN GOTO SDONE;
      161     1212    1       IF WORD1 < UWA.SN.SENAME AND SUBSTR(UWA.SN.SENAME,0,1) ~= '*'
      162     1213    1       THEN GOTO SDONE;
      163     1214    1       IF WORD1 = UWA.SN.SENAME
      164     1215    2       THEN DO;
      165     1216    2           SUCC='1'B;
      166     1217    3           DO CASE (TYPE);
      167     1218    3           CASE (%ZIG_RTYPE_AC);
      168     1219    3               SAVED_NAMES.AREA_NAME = UWA.SN.SENAME;
      169     1220    3               SAVED_NAMES.AREA_DBK = UWA.DIRECT_REF;
      170     1221    3           CASE (%ZIG_RTYPE_RC);
      171     1222    3               SAVED_NAMES.REC_NAME = UWA.SN.SENAME;
      172     1223    3               SAVED_NAMES.REC_DBK = UWA.DIRECT_REF;
      173     1224    3           CASE (%ZIG_RTYPE_SC);
      174     1225    3               SAVED_NAMES.SET_NAME = UWA.SN.SENAME;
      175     1226    3               SAVED_NAMES.SET_DBK = UWA.DIRECT_REF;
      176     1227    3           CASE (%ZIG_RTYPE_KC);
      177     1228    3               SAVED_NAMES.KEY_NAME = UWA.SN.SENAME;
      178     1229    3               SAVED_NAMES.KEY_DBK = UWA.DIRECT_REF;
      179     1230    3               END;
      180     1231    2           RETURN;
      181     1232    2           END;
      182     1233    1       CALL FETCHNS_6('DA-SN   ') ALTRET(SDONE);
      183     1234    1       GOTO SSEARCH;
      184     1235    1   SDONE:
      185     1236    1       CALL FETCHPS_6('DA-SN   ') ALTRET(DA_PRIOR);
PL6.E3A0      #001=ZIG$M_P9 File=ZIG$M_P9.:ZIC0TSI                               FRI 09/05/97 12:08 Page:6    
      186     1237    1       RETURN;
      187     1238    1   DA_PRIOR:
      188     1239    1       CALL FCURR_R_6('DA  ');
      189     1240    1       RETURN;
      190     1241    1   END ZIG$M_P9;

