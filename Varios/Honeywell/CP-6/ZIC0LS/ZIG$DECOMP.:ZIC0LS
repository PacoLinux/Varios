       62     1335    3               THEN I=0;
       63     1336    3               ELSE I=I-79;
       64     1337    3               END;
       65     1338    2           IF CONTINUE
       66     1339    3           THEN DO;
       67     1340    3               I=0;
       68     1341    3               IF FLAG = 1
