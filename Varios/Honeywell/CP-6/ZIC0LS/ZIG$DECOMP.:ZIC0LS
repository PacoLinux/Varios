       14       14            PROC (REC,TYPE,DONE);
       15       15    1   DCL REC CHAR (80);
       16       16    1   DCL TYPE CHAR (8);
       17       17    1   DCL DONE UBIN (36);
       18       18    1   DCL I UBIN (36) STATIC INIT (0);
       19       19    1   DCL INIT1 BIT (1) STATIC INIT ('1'B);
       20       20    1   DCL DONT_HAVE_LINE BIT (1);
       21       21    1   DCL P PTR STATIC;
       22       22    1   DCL 1 STRUC_OUT BASED (P),
       23       23    1         2 BLANK_COUNT UBIN (9) UNAL,
       24       24    1         2 CHAR_COUNT UBIN (9) UNAL,
       25       25    1         2 STRING CHAR (80) UNAL;
       26       26    1   DCL CONTINUE BIT (1) STATIC;
       27       27    1   DCL POS UBIN (36) STATIC;
       28       28    1   DCL LEFT UBIN (36) STATIC;
       29       29    1   DCL STORE_REC CHAR (160) STATIC;
       30       30    1   DCL FLAG UBIN (36) STATIC;
       31       31        %EJECT;
