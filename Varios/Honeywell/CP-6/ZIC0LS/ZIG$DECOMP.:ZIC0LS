      100     1373    2           POS=POS+STRUC_OUT.CHAR_COUNT;
      101     1374    2           IF I>=79
      102     1375    3           THEN DO;
      103     1376    3               LEFT=I-79;
      104     1377    3               CONTINUE='1'B;
      105     1378    3               END;
