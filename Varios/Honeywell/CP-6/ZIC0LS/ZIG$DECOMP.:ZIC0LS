       58     1331    3               IF TYPE='SA-DD   '
       59     1332    3               THEN STORE_REC=UWA.DD.DD_TXT_LINE;
       60     1333    3               ELSE STORE_REC=UWA.DM.DMCL_TXT_LINE;
