      214     1501         /**
      215     1502            Level 3 compares
      216     1503            */
      217     1504    1       IF TSN.SENAME(WIN(2)) < TSN.SENAME(WIN(3)) THEN /* */
      218     1505    1           WIN(4) = WIN(2);
      219     1506    1       ELSE /* */
      220     1507    1           WIN(4) = WIN(3);
      221     1508         /**
      222     1509            Winner now TSN(WIN(4))
