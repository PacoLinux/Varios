      198     1485    1   LEVEL_1_COMP:
      199     1486    1       Y = -1;
      200     1487    2       DO X = 0 TO 4 BY 2;
      201     1488    2           Y = Y + 1;
      202     1489    2           IF TSN.SENAME(X) < TSN.SENAME(X + 1) THEN /* */
      203     1490    2               WIN(Y) = X;
      204     1491    2           ELSE /* */
