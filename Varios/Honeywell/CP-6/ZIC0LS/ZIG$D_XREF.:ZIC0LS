      487     1774    2   DCL ILIT CHAR(5);
      488     1775    2   DCL LIT CHAR(7);
      489     1776    2   DCL X UBIN WORD STATIC;
      490     1777    2   DCL Y UBIN WORD STATIC;
      491     1778    2       CALL INDEX(Y,' ',ILIT);
      492     1779    3       IF IMPLICIT THEN DO;
      493     1780    3           CALL CONCAT(LIT,'(',SUBSTR(ILIT,0,Y),')');
      494     1781    3           Y = Y + 2;
      495     1782    3           END;
      496     1783    2       ELSE /* */
      497     1784    2           LIT = ILIT;
      498     1785    2       IF Y < 4 THEN /* */
      499     1786    2           CALL INDEX(X,'     ',ASTRNG);
      500     1787    2       ELSE /* */
      501     1788    2           CALL INDEX(X,'       ',ASTRNG);
      502     1789    2       IF X = LENGTHC(ASTRNG) THEN /* */
      503     1790    2           ALTRETURN;
      504     1791    2       IF X + Y > 55 THEN /* */
      505     1792    2           ALTRETURN;
      506     1793    2       IF (SUBSTR(LIT,0,3) = 'REF') THEN /* */
      507     1794    3       DO;
      508     1795    3           IF X > 5 THEN /* */
      509     1796    3               X = 9;
      510     1797    3           END;
      511     1798    2       CALL INSERT(ASTRNG,X+1,Y,SUBSTR(LIT,0,Y));
      512     1799    2   END CAT_A;
