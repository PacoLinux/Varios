  256      DCL DKW$ PTR STCLASS;
  257      DCL 1 KEYWRD BASED(DKW$) ALIGNED,
  258            2 NAM_IDX         UBIN(18) UNAL,
  259            2 CON_VALUE       UBIN(18) UNAL,  /*Constant value of the keyword       */
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           8    
  260            2 * (0:6)         UBIN(36) UNAL,
  261            2 LLIST$          PTR;
  262      %MEND;
  263      %MACRO DSC_KEY (STCLASS=EXT);
  264      DCL DK$ PTR STCLASS;
  265      DCL 1 KEY BASED(DK$) ALIGNED,
  266            2 NAM_IDX    UBIN(18) UNAL,  /*Indx to the NAM1 table for this keyword  */
  267            2 RVA_OFF         UBIN(18) UNAL,  /*Rva offset                          */
  268            2 * (0:4)         UBIN(36) UNAL,
  269            2 RFK_DSC$        PTR,            /*Ptr to the Rec/Fld/Key discriptor   */
  270            2 LKEY$           PTR,
  271            2 LLIST$          PTR;
  272      %MEND;
  273      %MACRO DSC_RFS (STCLASS=EXT);
  274      DCL DRFS$ PTR STCLASS ALIGNED;
  275      DCL 1 RFS_DSC BASED(DRFS$) ALIGNED,
  276            2 REC_IDX UBIN(18) UNAL, /*Index to NAM1 of the record for this Rec/Fld/Set desc.*/
  277            2 SET_IDX UBIN(18) UNAL, /*Index to NAM1 of the set for this Rec/Fld/Set desc. */
  278            2 FLD_IDX UBIN(18) UNAL, /*Index to NAM1 of the set for this Rec/Fld/Set desc. */
  279            2 OWN_MEM         BIT(1)   UNAL, /*Tells if the record owns this set
  280                                                   0 = owner
  281                                                   1 = member     */
  282            2 MAN_AUTO        BIT(1)   UNAL, /*TElls if the set  for record is manual or
  283                                                    automatic insertion
  284                                                      0 = automatic
  285                                                      1 = manual       */
  286            2 MND_OPT         BIT(1)   UNAL, /*Tells is the set for this record is
  287                                                 mandatory or optional  insertion
  288                                                       0 = mandatory
  289                                                       1 = optional           */
  290            2 ASCEND_DESCEND  BIT(1)   UNAL, /*Tells if the field making up this key is
  291                                                ascending or descending                              */
  292            2 *               UBIN(14) UNAL,
  293            2 DEF,
  294              3 TYPE        UBIN(12) UNAL,
  295                                         /* set owner element (so) = octal 0100     */
  296                                         /* set member element (sm) = octal 0040    */
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           9    
  297              3 VIA         BIT (1)  UNAL,    /*  1 = this is loc mode set          */
  298              3 *           UBIN (2) UNAL,
  299              3 MEMT        UBIN (3) UNAL,    /*  Member type mask  ( sm only )     */
  300                                              /* 0 = member of chain                */
  301                                              /* 1 = pa member                      */
  302                                              /* 2 = pa record                      */
  303              3 DORD        UBIN (2) UNAL,    /*  Duplicate order                   */
  304              3 OSM         BIT (1)  UNAL,    /*  1 = optional set member           */
  305              3 MSM         BIT (1)  UNAL,    /*  1 = manual set member             */
  306              3 SMOD     BIT (1) UNAL,   /* 1 = this set membership cannot be mod.  */
  307              3 DUPP        BIT (1)  UNAL,    /* 1 = dup keys present               */
  308              3 RNGK        BIT (1)  UNAL,
  309              3 DUPC        BIT (1)  UNAL,    /* 1 = dups not allowed for calc      */
  310              3 *           UBIN(10) UNAL,
  311            2 NUM_CON_FIELDS UBIN(36) UNAL, /*Number of control flds in the  sorted set for this rec*/
  312            2 LFLD$           PTR UNAL,       /* Link list ptr for Fld.*/
  313            2 REC$            PTR,  /*Ptr to next Rec/Fld/Set desc for this record  */
  314            2 SET$            PTR,  /*Ptr to next Rec/Fld/Set desc for this set     */
  315            2 FLD$       PTR,  /*Ptr to the next Rec/Fld/Set desc for this Field    */
  316            2 LLIST$          PTR;            /*Ptr to next desc in link list       */
  317      %MEND;
  318      %MACRO DSC_RA (STCLASS=EXT);
  319      DCL DRA$ PTR STCLASS ALIGNED;
  320      DCL 1 RA_DSC BASED(DRA$) ALIGNED,
  321            2 REC_IDX UBIN(18) UNAL, /*Index to NAM1 of the record for this RA_DSC desc. */
  322            2 AREA_IDX UBIN(18) UNAL, /*Index to NAM1 of the AREA for this RA_DSC desc. */
  323            2 LO_DBK_RNG UBIN(36) UNAL, /*Low  dbk rang of the record for this area */
  324            2 HI_DBK_RNG UBIN(36) UNAL, /* high dbk range for the record within this area */
  325            2 FLAGS,
  326              3 FULL_RNG BIT(1) UNAL, /*Tells if the record rng can grow with tmplt area growth*/
  327              3 *             BIT(35)  UNAL,
  328            2 * (0:1)         UBIN(36) UNAL,
  329            2 REC$            PTR,       /*Ptr to next RA_DSC desc for this record  */
  330            2 AREA$           PTR,       /*Ptr to next RA_DSC desc for this set     */
  331            2 LLIST$          PTR;            /*Ptr to next desc in link list       */
  332      %MEND;
  333      %MACRO DSC_RP (STCLASS=EXT);
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           10   
  334      DCL DRP$ PTR STCLASS;
  335      DCL 1 RP_DSC BASED(DRP$) ALIGNED,
  336            2 REC_IDX         UBIN(18) UNAL,  /*Nam1 index for the record           */
  337            2 PARAM_IDX       UBIN(18) UNAL,  /*NAM1 index for the parameter        */
  338            2 * (0:4)         UBIN(36) UNAL,
  339            2 REC$            PTR,
  340            2 PARAM$          PTR,
  341            2 LLIST$          PTR;
  342      %MEND;
  343      %MACRO DSC_RFK (STCLASS=EXT);
  344      DCL DRFK$ PTR STCLASS;
  345      DCL 1 RFK_DSC BASED(DRFK$) ALIGNED,
  346            2 REC_IDX         UBIN(18) UNAL,  /*NAM1 index for the associated REC   */
  347            2 KEY_IDX         UBIN(18) UNAL,  /*NAM1 index for the associated KEY   */
  348            2 FLD_IDX         UBIN(18) UNAL,  /*NAM1 index for the associated FIELD */
  349            2 NUM_CON_FIELDS  UBIN(36) UNAL,  /*Number of control fields in the key */
  350            2 PRIM_SEC BIT(1) UNAL, /*Tells if this is a primary or sec key for this record*/
  351            2 *               UBIN(17) UNAL,
  352            2 LFLD$           PTR UNAL,       /*Link list ptr for Fld.*/
  353            2 *               UBIN(36) UNAL,
  354            2 REC$            PTR,            /*Ptr to next  RFK_DSC for this key   */
  355            2 KEY$            PTR,            /*Ptr to next RFK_DSC for this record */
  356            2 FLD$            PTR,            /*Ptr to next RFK_DSC for this field  */
  357            2 LLIST$          PTR;            /*Ptr to next record in link list     */
  358      %MEND;
  359      %MACRO TYPE_SUBS;
  360      %SUB #AREA=0;
  361      %SUB #SET=1;
  362      %SUB #RECORD=2;
  363      %SUB #FIELD=3;
  364      %SUB #PARAM=5;
  365      %SUB #LOCK=6;
  366      %SUB #KEYWORD=7;
  367      %SUB #KEY=8;
  368      %MEND;
  369      %MACRO PROC_SUBS;
  370      %SUB #COBOL = 1;
