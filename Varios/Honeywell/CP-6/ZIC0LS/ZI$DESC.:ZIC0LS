   14      /******************************************************/
   15      %MACRO DSC_AREA(STCLASS=EXT);
   16      DCL DA$ PTR STCLASS ALIGNED;
   17      DCL 1 AREA BASED(DA$) ALIGNED,
   18            2 NAM_IDX         UBIN(18) UNAL,  /* Index to area name in NAM1         */
   19            2 RVA_OFF         UBIN(18) UNAL,  /*Word offset inot RVA for element    */
   20            2 AREA_BITS                UNAL,
   21              3 *             BIT(1)   UNAL,
   22              3 CHKSUM        BIT(1)   UNAL,
   23              3 ENCRP         BIT(1)   UNAL,
   24              3 CJRNL         BIT(1)   UNAL,
   25              3 TMPLT         BIT(1)   UNAL,
   26              3 TJRNL         BIT(1)   UNAL,
   27              3 TEMPORARY     BIT(1)   UNAL,
   28              3 *             BIT(29)  UNAL,
   29            2 *               UBIN(36) UNAL,
   30            2 LREC$ PTR UNAL, /*LINK LIST FOR RECORDS ONLY FOUND IN FIRST AREA DESC. */
   31            2 LSET$ PTR UNAL,  /*LINK LIST FOR SETSFOUND ONLY IN FIRST AREA DESC.   */
   32            2 LAREA$     PTR UNAL,  /*LINK LIST FOR AREAS WITHIN A GIVEN SUBSCHEMA  */
   33            2 RA_DSC$ PTR UNAL, /*Ptr to the for record_area descriptor for this area */
   34            2 LKEY$           PTR,
   35            2 LLIST$          PTR;            /*Ptr to next desc. in link list      */
   36      %MEND;
   37      %MACRO DSC_REC(STCLASS=EXT);
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           2    
   38      DCL DR$ PTR STCLASS ALIGNED;
   39      DCL 1 RECORD BASED(DR$) ALIGNED,
   40            2 NAM_IDX         UBIN(18) UNAL,  /*Index to record name in NAM1        */
   41            2 RVA_OFF         UBIN(18) UNAL,  /*Index to element in RVA             */
   42            2 UWA_OFF UBIN(18) UNAL, /*Byte offset into  UWA  relative to  0 base address */
   43            2 FLAG_BITS       BIT(18)  UNAL,
   44            2 FLAGS REDEF FLAG_BITS,
   45              3 RA            UBIN(3) UNAL,   /* NU                                 */
   46              3 RB            UBIN(3) UNAL,   /*primary storage mode. Value are:    */
   47      %SUB #RB_DIRECT=0;                      /* 0 - direct                         */
   48      %SUB #RB_CALC=1;                        /* 1 - calc                           */
   49      %SUB #RB_VIA=2;                         /* 2 - via set                        */
   50      %SUB #RB_SEQUEN=3;                      /* 3 - sequential                     */
   51      %SUB #RB_INDX=4;                        /* 4 - indexed                        */
   52                                              /* 5-7  -  NU                         */
   53              3 RD            BIT(1)  UNAL,   /* 1 = delete not allowed             */
   54              3 RE            BIT(1)  UNAL,   /* 1 = store not allowed              */
   55              3 RF            BIT(1)  UNAL,   /* 1 = record not in subschema        */
   56              3 RG            BIT(1)  UNAL,   /*1 = varible length                  */
   57              3 RH            BIT(1)  UNAL,   /*1 = multi-area record               */
   58              3 RI            BIT(1)  UNAL,   /*1= record has alternate keys        */
   59              3 RL            BIT(1)  UNAL,   /*1 = erase not allowed               */
   60              3 RM        BIT(1) UNAL,   /*don't invoke db proc for area selection  */
   61              3 RN            BIT(1)  UNAL,   /* 1 = PA record                      */
   62              3 *             BIT(3)   UNAL,
   63            2 NUM_CALC_FLDS   UBIN(36) UNAL,
   64            2 RA_DSC$         PTR UNAL,
   65            2 RP_DSC$         PTR UNAL,
   66            2 LREC$           PTR      UNAL,
   67            2 RFS_DSC$        PTR,            /*Ptr to Rec/Fld/Set desc link list   */
   68            2 RFK_DSC$   PTR,  /*Ptr to the Rec/Fld/Key descriptor for this record  */
   69            2 LLIST$          PTR;            /*Ptr to next desc. in link list      */
   70      %MEND;
   71      %MACRO DSC_FLD(STCLASS=EXT);
   72      DCL DF$ PTR STCLASS ALIGNED;
   73      DCL 1 FIELD BASED(DF$) ALIGNED,
   74            2 NAM_IDX         UBIN(18) UNAL,  /*Index to field in NAM1              */
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           3    
   75            2 RVA_OFF         UBIN(18) UNAL,  /*Index to element in RVA             */
   76            2 UWA_OFF UBIN(18) UNAL, /*Byte or Nibble offset into UWA from 0 base   */
   77           /* Byte or Nibble determined by the  BYT_NIB field in the descriptor     */
   78            2 REC_ASSC_IDX UBIN(18) UNAL, /*Index into NAM1 to the record this field belongs to */
   79            2 FLD_ASSC_IDX    UBIN(18) UNAL, /*If this field belongs to a  field group then this
   80                                                 value will be  the index into NAM1 for the owner field
   81                                                 else this value will be zero                        */
   82            2 SYN_NAM UBIN(18) UNAL, /*Word offset fram nam2$ to next field descrip w/same na*/
   83      %SUB #SYN_NAM_INIT=0;                   /*SYN_NAM thread not made yet         */
   84      %SUB #SYN_NAM_NIL=    262143; /* = '777777'O WHICH MEANS SYN_NAM thread end   */
   85            2 FLAG_BITS_1     BIT(36)  UNAL,
   86            2 FLAGS_1 REDEF FLAG_BITS_1,
   87              3 TYPE UBIN(12) UNAL, /*Type code for a field in the subschema file   */
   88              3 FA            UBIN(6) UNAL,   /*index into encode vector            */
   89              3 FB            BIT(1)  UNAL,   /* 1 = encode before comparing        */
   90              3 FC            BIT(1)  UNAL,   /* 1 = encode before calc             */
   91              3 FE            UBIN(3) UNAL,   /* compare code. Values are:          */
   92      %SUB #FE_ALPH=0;                        /*1 = alphanumeric                    */
   93      %SUB #FE_NUM=1;                         /*2 = numeric                         */
   94      %SUB #FE_BIN=2;                         /* 2 = binary                         */
   95      %SUB #FE_BCD=3;                         /* 3 = BCD                            */
   96      %SUB #FE_BIT=4;                         /* 4 = bit                            */
   97      %SUB #FE_FLTBIN=5;                      /* 5 = float binary                   */
   98                                              /* 6 -7 NU                            */
   99              3 FF            BIT(1)  UNAL,   /* 1 =  varible length field          */
  100              3 FG            BIT(1)  UNAL,   /* 1 = this is a synonym field        */
  101              3 FH            UBIN(3) UNAL,   /* usage code                         */
  102      %SUB #FH_DATA_ITM=0;                    /* 0 = data item                      */
  103      %SUB #FH_DATA_PRM=1;                    /* 1 = data base parameter            */
  104      %SUB #FH_DIR_FLD=2;                     /* 2 = loc mode - direct field        */
  105      %SUB #FH_AREA_ID=3;                     /* 3 =  area-ID field                 */
  106      %SUB #FH_CALC_SYN_FLD=4;                /* 4 = calc key synonym field         */
  107      %SUB #FH_SCAN_SYN_FLD=5;                /* 5 = scan key synonym field         */
  108                                              /* 6-7 = NU                           */
  109              3 FI            BIT(1)  UNAL,   /* 1 = this field not modifiable      */
  110              3 FJ            BIT(1)  UNAL,   /* 1 = this field not in subschema    */
  111              3 FL            UBIN(6) UNAL,   /* index into decode vector           */
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           4    
  112            2 FLAG_BITS_2     UBIN(18) UNAL,
  113            2 FLAGS_2 REDEF FLAG_BITS_2,
  114              3 BYT_NIB       BIT(1)   UNAL, /* Tells if the UWA offset is in BYTES or NIBBLES.
  115                                                           0 = byte
  116                                                           1 = nibble               */
  117              3 ELEM          BIT(1)   UNAL, /*Tells whether or not this field is elementary.
  118                                                                 0 = nonelememtary
  119                                                                 1 = elementary                */
  120              3 DATA_TYPE    UBIN(16) UNAL,   /*Data types. Values are:             */
  121      %SUB #FDT_CMP_SIGN=4;                   /* comp     - signed = 4 unsigned  =3 */
  122      %SUB #FDT_CMP_UNSIGN=3;
  123      %SUB #FDT_CMP4_SIGN=4;                  /* comp 4  -  signed = 4 unsigned = 3 */
  124      %SUB #FDT_CMP4_UNSIGNED=3;
  125      %SUB #FDT_CMP3_SIGN=5;                  /* comp 3  -  signed = 5 unsigned = 3 */
  126      %SUB #FDT_CMP3_UNSIGNED=3;
  127      %SUB #FDT_CHAR=0;                       /* character - = 0                    */
  128      %SUB #FDT_DISP_NUM=6;                   /* display numeric - unsigned = 6     */
  129      %SUB #FDT_DISP_NUM001= 10; /*                     signed sign_type = '001'b = 10 */
  130      %SUB #FDT_DISP_NUM010= 9; /*                     signed sign_type = '010'b = 9 */
  131      %SUB #FDT_DISP_NUM011= 8; /*                     signed sign_type = '011'b = 8 */
  132      %SUB #FDT_DISP_NUM100=7; /*                    signed sign_type = 7 ('100'b ????)*/
  133      %SUB #FDT_DBK=1;                        /* DB-key    - = 1                    */
  134      %SUB #FDT_INTEGER=2;                    /* comp 6    - = 2 (integer!!!!)      */
  135      %SUB #FDT_CMP1=         11;             /* comp 1    - = 11                   */
  136      %SUB #FDT_CMP2=           12;           /* comp 2    - = 12                   */
  137      %SUB #FDT_FLT_SINGLE=       13;         /* float single = 13                  */
  138      %SUB #FDT_FLT_DOUBLE=         14;       /* float double = 14                  */
  139      %SUB #FDT_CMPLX_SINGLE=         15;     /* complex single = 15                */
  140      %SUB #FDT_CMPLX_DOUBLE=           16;   /* complex double = 16                */
  141      %SUB #FDT_LOGICAL=                  17; /*  logical        = 17               */
  142            2 CALC_FLD#       UBIN(36) UNAL,
  143
  144            2 EIS_BITS                 UNAL,
  145              3 NDSC                   UNAL,
  146                4 CN          UBIN(3)  UNAL, /*Original character # within data word.
  147                                               Code depends on the data type:
  148                                                       Data type          character          legal code
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           5    
  149                                                        9 bit                  0                 000
  150                                                                               1                 010
  151                                                                               2                 100
  152                                                                               3                 110
  153
  154                                                        4 bit                  0                 000
  155                                                                               1                 001
  156                                                                               2                 010
  157                                                                               3                 011
  158                                                                               4                 100
  159                                                                               5                 101
  160                                                                               6                 110
  161                                                                               7                 111 */
  162                4 TN          BIT(1)   UNAL, /*Tells what is the bit type . Values are:
  163                                                         0 = 4 bit
  164                                                         1 = 9 bit               */
  165                4 S           BIT(2)   UNAL, /*Tells  the sign and decimal type. Values are:
  166                                                         00 = Leading sign,floating point
  167                                                         01 = Leading sign,scaled
  168                                                         10 = Trailing sign,scaled
  169                                                         11 = No sign                 */
  170                4 SF          SBIN(6)  UNAL, /*Scaling factor: a 2's complement binary # that gives the
  171                                                       scale point for scaled decimal integers.
  172                                                       If s = 00 , this field is ignored...Example:
  173                                                       if data=12345,s ~= 00 and sf=-3 then value is */
  174                4 N UBIN(6) UNAL, /* The  number of 9 or 4 bit characters in the data item*/
  175              3 ADSC REDEF NDSC,
  176                4 CN          UBIN(3)  UNAL,
  177                4 TA          BIT(2)   UNAL, /*Code type for alphanumeric characters. Values are:
  178                                                         00 = 9 bit
  179                                                         01 = 6 bit
  180                                                         10 = 4 bit
  181                                                         11 = illegal          */
  182                4 *           BIT(1)   UNAL,
  183                4 N         UBIN(12) UNAL,    /*Size                                */
  184            2 RFS_DSC$        PTR,            /*Ptr to the Rec/Fld/Set desc         */
  185            2 RFK_DSC$        PTR,            /*Ptr to the Rec/Fld/Key desc         */
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           6    
  186            2 LLIST$          PTR;            /*Ptr to next desc. in link list      */
  187      %MEND;
  188      %MACRO DSC_SET (STCLASS=EXT);
  189      DCL DS$ PTR STCLASS ALIGNED;
  190      DCL 1 SET BASED(DS$) ALIGNED,
  191            2 NAM_IDX         UBIN(18) UNAL,  /*Index to set name in NAM1           */
  192            2 RVA_OFF         UBIN(18) UNAL,  /*Index inot the RVA for the element  */
  193            2 OWNER_IDX UBIN(18) UNAL, /* Index into NMA1 for the record that owns this set */
  194            2 *               UBIN(18) UNAL,  /*Not used                            */
  195            2 FLAG_BITS       BIT(24)  UNAL,
  196            2 FLAGS REDEF FLAG_BITS,
  197              3 SA            UBIN(3) UNAL,   /*Set mode. Values are:               */
  198      %SUB #SA_CHAIN=0;                       /* 0 - chain                          */
  199      %SUB #SA_REC_ARRY=1;                    /* 1 - record array                   */
  200      %SUB #SA_PTR_ARRY=2;                    /* 2 - pointer array                  */
  201                                              /* 3-7 - NU                           */
  202              3 SB            UBIN(3) UNAL,   /* NU                                 */
  203              3 SD            UBIN(4) UNAL,   /* Set order. Values are:             */
  204      %SUB #SD_FIRST=0;                       /* 0 - first                          */
  205      %SUB #SD_LAST=1;                        /* 1 - LAST                           */
  206      %SUB #SD_AFTER=2;                       /* 2 - AFTER                          */
  207      %SUB #SD_BEFORE=3;                      /* 3 - BEFORE                         */
  208      %SUB #SD_SORT_KEY=4;                    /* 4 - SORTED BY DEFINED KEYS         */
  209                                              /* 5 - NU                             */
  210      %SUB #SD_SORT_REC= 6;                   /*                                    */
  211                                              /* 7-15 - NU                          */
  212              3 SK            BIT(1)  UNAL,   /* 1 = not in su bschema              */
  213              3 SE            BIT(1)  UNAL,   /* NU                                 */
  214              3 SF            BIT(1)  UNAL,   /* 1 = recursive set (not implemented */
  215              3 SG        BIT(1) UNAL,   /* 1 = alternate owner (not implemented)   */
  216              3 SH            BIT(1)  UNAL,   /* 1 = glo bal set                    */
  217              3 SI            UBIN(3) UNAL,   /* Set type. Values are:              */
  218      %SUB #SI_USER=0;                        /* 0 = user                           */
  219      %SUB #SI_CALC=1;                        /*1 = calc                            */
  220      %SUB #SI_PRIM_KEY=2;                    /*2 = primary key                     */
  221      %SUB #SI_SEC_KEY=3;                     /*3 = secondary key                   */
  222                                              /* 4-7 =  NU                          */
11:17 SEP 05 '97 ZI$DESC.:ZIC0SI                                           7    
  223              3 SJ       UBIN(6) UNAL,   /* # of characters in su bschema set name  */
  224            2 *               UBIN(12) UNAL,  /*not used                            */
  225            2 * (0:2)         UBIN(36) UNAL,
  226            2 LSET$           PTR      UNAL,
  227            2 RFS_DSC$        PTR,  /*Ptr to the Rec/Fld/Set desc. in the link list */
  228            2 LLIST$          PTR;            /*Ptr to the next desc. in link list  */
  229      %MEND;
  230      %MACRO DSC_PARAM (STCLASS=EXT);
  231      DCL DP$ PTR STCLASS ALIGNED;
  232      DCL 1 DSC_PARAM BASED(DP$) ALIGNED,
  233            2 NAM_IDX UBIN(18) UNAL, /*Index inot NMA1 for the data_base_parameter  */
  234            2 RVA_OFF         UBIN(18) UNAL,  /*Index into the RVA for this element */
  235            2 UWA_OFF UBIN(18) UNAL, /*Word offset into UWA for the db parameter    */
  236            2 *               UBIN(18) UNAL,
  237            2 * (0:1) UBIN(36) UNAL,
  238            2 BYT_NIB BIT(1) UNAL,            /* 1=NIB, 0=BYTE                      */
  239            2 *    BIT(1) UNAL,
  240            2 TYPE UBIN(16) UNAL,             /* DATA TYPE OF PARAM - SAME AS FIELD */
  241            2 *   UBIN(18)  UNAL,
  242            2 LEN UBIN(36) UNAL,              /* LEN OF PARAM                       */
  243            2 * UBIN(36) UNAL,
  244            2 RP_DSC$         PTR,  /*Ptr to the Rec/Param desc for this parameter  */
  245            2 LLIST$          PTR;            /*Ptr to next desc in link list       */
  246      %MEND;
  247      %MACRO DSC_DESC (STCLASS=EXT);
  248      DCL DD$ PTR STCLASS;
  249      DCL 1 DESC BASED(DD$) ALIGNED,
  250            2 NAM_IDX         UBIN(18) UNAL,
  251            2 *               UBIN(18) UNAL,  /*Not used                            */
  252            2 * (0:6)         UBIN(36) UNAL,
  253            2 LLIST$          PTR;
  254      %MEND;
