  371      %SUB #FORTRAN = 2;
  372      %SUB #APL = 3;
  373      %SUB #IDP = 4;
  374      %SUB #BASIC = 5;
  375      %MEND;
  376      %MACRO KEYWD_VAL;
  377          %EQU ALL=6;
  378          %EQU ANY=7;
