       85     4032    2       CALL BINCHAR(SECOND_PART,NUM);
       86     4033    2       CALL EDITCHAR(SECOND_PART,0,12,MOPSTR,SECOND_PART);
       87     4034    2       CALL CONCAT(LIST_STRING,SUBSTR(BL,0,START),FIRST_PART,SECOND_PART);
       88     4035    2       CALL LIST(LIST_STRING);
       89     4036    2   END PRINT;
