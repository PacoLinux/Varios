      106     4053    2   DCL HEADING CHAR (30);
      107     4054    2   DCL LEN UBIN (36);
      108     4055    2   DCL 1 NUM(0:0) UBIN (3) UNAL;
      109     4056    2   DCL I UBIN (36);
      110     4057    2   DCL K UBIN (36);
      111     4058    2       CALL INDEX1(I,'^',HEADING);
      112     4059    2       FIRST_PART = SUBSTR(HEADING,0,I);
      113     4060    2       SECOND_OCTAL = ' ';
      114     4061    3       DO I = 1 TO LEN/3;
      115     4062    3           K = I - 1;
      116     4063    3           IF I = 6
      117     4064    4           THEN DO;
      118     4065    4               CALL INSERT(SECOND_OCTAL,6,1,'.');
      119     4066    4               END;
      120     4067    3           IF I>6 THEN K = K + 1;
      121     4068    3           CALL BINCHAR(CHAR_1,NUM(I-1));
      122     4069    3           CALL INSERT(SECOND_OCTAL,K,1,CHAR_1);
      123     4070    3           END;
      124     4071    2       CALL CONCAT(LIST_STRING,SUBSTR(BL,0,START),FIRST_PART,SECOND_OCTAL);
      125     4072    2       CALL LIST(LIST_STRING);
      126     4073    2   END PRINT_O;
