%MACRO UPT(STCLASS=SYMREF);
DCL UPT$ PTR STCLASS;
DCL 1 UPT BASED (UPT$) ALIGNED,
      2 W1,
        3 BOUND UBIN(1) UNAL,
        3 COUNT UBIN(17) UNAL,
        3 * UBIN(18) UNAL,
      2 UPTE(0:0),
        3 UCW,
          4 PT UBIN(9) UNAL,
          4 PB,
            5 PG UBIN(1) UNAL,          /* Get                                */
            5 PR REDEF PG UBIN(1) UNAL, /* Retrevial                          */
            5 PM UBIN(1) UNAL,          /* Modify                             */
            5 PU REDEF PM UBIN(1) UNAL, /* Update                             */
            5 PS UBIN(1) UNAL,          /* Store                              */
            5 PE UBIN(1) UNAL,          /* Erase                              */
            5 PC UBIN(1) UNAL,          /* Connect                            */
            5 PD UBIN(1) UNAL,          /* Disconnect                         */
            5 PF UBIN(1) UNAL,          /* Find                               */
            5 * UBIN(1) UNAL,
            5 LOCK UBIN(1) UNAL,
          4 A UBIN(18) UNAL,
        3 W1 REDEF UCW,
          4 * UBIN(9) UNAL,
          4 EC UBIN(27) UNAL,
        3 W2,
          4 UPADR UBIN(18) UNAL,
          4 * UBIN(18) UNAL;
%MEND;

