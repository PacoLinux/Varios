DCL 1 UWA$ PTR SYMREF;                  /* User work area.                    */
%MACRO       RCW;
DCL 1 RCW BASED ALIGNED,
      2 HW0,
        3 * BIT(15) UNAL,
        3 LOAD BIT(1) UNAL,
        3 * BIT(2) UNAL,
      2 COUNT UBIN(18) UNAL;
%SUB RCW#CKP='400000000000'O;
%SUB RCW#MODES#IN='000520000000'O/*Input*/;
%MEND;

FILE=ZI$USER                       RCW
LEVEL 2   LEVEL 3   LEVEL 4        0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8 0 1 2 3 4 5 6 7 8
                                  I-----------------I-----------------I-----------------I-----------------I
HW0                 LOAD         0!*                            !@!*  !. . . . . . . . . . . . . . . . . .!B
                                  I-----------------I-----------------I-----------------I-----------------I
                                 0!. . . . . . . . . . . . . . . . . .!COUNT                              !
                                  I-----------------I-----------------I-----------------I-----------------I

%MACRO RLCW(STCLASS=BASED);
DCL 1 RLCW STCLASS,
      2 * UBIN(18) UNAL,
      2 AC BIT(1) UNAL,
      2 SC BIT(1) UNAL,
      2 RC BIT(1) UNAL,
      2 * BIT(7) UNAL,
      2 NSETS UBIN(8) UNAL;
%MEND;

