   127 - DCL 1 RCW BASED ALIGNED,
   128 -       2 HW0,
   129 -         3 * BIT(15) UNAL,
   130 -         3 LOAD BIT(1) UNAL,
   131 -         3 * BIT(2) UNAL,
   132 -       2 COUNT UBIN(18) UNAL;
   133 - %SUB RCW#CKP='400000000000'O;
   134 - %SUB RCW#MODES#IN='000520000000'O/*Input*/;
   135 - %MEND;
