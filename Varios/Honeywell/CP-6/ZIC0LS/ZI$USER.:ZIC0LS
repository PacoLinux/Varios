FRI. SEP 05 '97 11:19:36.08  FILE=ZI$USER

%MACRO IA (STCLASS=SYMREF);
/*
       User's information area 'IA'.
                                                           */
DCL 1 IA BASED(IA$) ALIGNED,
      2 SSSP PTR UNAL,
      2 UWAP PTR UNAL,
      2 * PTR,
      2 * UBIN(36) UNAL,
      2 * UBIN(18) UNAL,
      2 SYSDCB UBIN(18) UNAL,           /* Possible DCB number                */
      2 SYSERR BIT(36) UNAL,            /* CP-6 error code if valid           */
      2 ROLLFLAG BIT(18) UNAL,          /* Set if roll-back occured           */
      2 UCALL_LOC UBIN HALF UNAL,       /* caller's memory address            */
      2 UPFLAG,
        3 UPADR UBIN(18) UNAL,          /* address of user procedure          */
        3 FLAGS UNAL,
          4 XCON BIT(1) UNAL, /* Indicates xcon occurred during dbcs call     */
          4 BREAK BIT(1) UNAL, /* Indicates break occurred during dbcs call   */
          4 DUMP BIT(1) UNAL, /* Indicates that a dbcs dump is in progress    */
          4 QUIT BIT(1) UNAL,      /* Indicates that a 'QUIT' has occurred    */
          4 * BIT(13) UNAL,             /* available for future use           */
          4 ETC BIT(1) UNAL,       /* Indicates to user that return is etc    */
      2 RVP PTR UNAL,
      2 USEP PTR UNAL,
      2 SNAME CHAR(30),                 /* schema name                        */
      2 SNCHAR(0:29) REDEF SNAME CHAR(1),
      2 * CHAR(2),
      2 SSNAME CHAR(30),                /* sub-schema name                    */
      2 SSNCHAR (0:29) REDEF SSNAME CHAR(1),
      2 * CHAR(2),
      2 PNAME CHAR(30),                 /* program name                       */
      2 * CHAR(2),
      2 AUTHOR CHAR(30),                /* author                             */
      2 * CHAR(2) UNAL,
      2 EC UBIN(36) UNAL,               /* Environment code                   */
      2 CMPTIME UBIN (36),              /* time compiled                      */
      2 SSDT CHAR(8),                   /*date and time sub-schema translated*/
      2 SSTDT (0:1) REDEF SSDT UBIN(36);
DCL IA$ PTR STCLASS;

