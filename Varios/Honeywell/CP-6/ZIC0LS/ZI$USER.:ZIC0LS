%MACRO UWA;
/*
       User's work area 'UWA'.
                                                           */
DCL 1 UWA BASED (UWA$) ALIGNED,
      2 IFR SBIN,
      2 DBST CHAR(7),
      2 DB_STATUS REDEF DBST,
        3 STMTCODE CHAR(2),
        3 EXCEPTION CHAR(5),
      2 * CHAR(1),
      2 DBAN CHAR(30),
      2 * CHAR(2),
      2 DBSN CHAR(30),
      2 * CHAR(2),
      2 DBRN CHAR(30),
      2 * CHAR(2),
      2 DBPKY CHAR(12),
      2 DBDR UBIN,
      2 DBFN CHAR(30),
      2 * CHAR(2),
      2 DBKYN CHAR(30),
      2 * CHAR(4),                      /* Unused                             */
/* Subschema items and data base parameters */
      2 * CHAR(20);
DCL 1 UWA$ PTR SYMREF;                  /* User work area.                    */

