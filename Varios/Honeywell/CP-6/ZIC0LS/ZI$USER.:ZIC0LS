%MEND;
%MACRO RVA(STCLASS=SYMREF);
/*         Refrence vector                    */
DCL RVA$ PTR STCLASS;
DCL 1 RVA BASED (RVA$) ALIGNED,
      2 W1,
        3 BOUND UBIN(1) UNAL,
        3 COUNT UBIN(17) UNAL,
        3 * UBIN(18) UNAL,
      2 RVE (0:0),
        3 EO UBIN(18) UNAL,             /* Element offset                     */
        3  T REDEF EO UBIN(18) UNAL,
        3 NAME UBIN(18) UNAL;
%MEND;

