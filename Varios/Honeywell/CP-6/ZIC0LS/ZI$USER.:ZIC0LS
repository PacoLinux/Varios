11:19 SEP 05 '97 ZI$USER.:ZIC0TOU
     1 - /*M*  ZI$USER - data structures associated with the user's domain. */
     2 - /*T***********************************************************/
     3 - /*T*                                                         */
     4 - /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
     5 - /*T*                                                         */
     6 - /*T***********************************************************/
     7 - %MACRO IA (STCLASS=SYMREF);
     8 - /*
     9 -        User's information area 'IA'.
    10 -                                                            */
    11 - DCL 1 IA BASED(IA$) ALIGNED,
    12 -       2 SSSP PTR UNAL,
    13 -       2 UWAP PTR UNAL,
    14 -       2 * PTR,
    15 -       2 * UBIN(36) UNAL,
    16 -       2 * UBIN(18) UNAL,
    17 -       2 SYSDCB UBIN(18) UNAL,           /* Possible DCB number                */
    18 -       2 SYSERR BIT(36) UNAL,            /* CP-6 error code if valid           */
    19 -       2 ROLLFLAG BIT(18) UNAL,          /* Set if roll-back occured           */
    20 -       2 UCALL_LOC UBIN HALF UNAL,       /* caller's memory address            */
    21 -       2 UPFLAG,
    22 -         3 UPADR UBIN(18) UNAL,          /* address of user procedure          */
    23 -         3 FLAGS UNAL,
    24 -           4 XCON BIT(1) UNAL, /* Indicates xcon occurred during dbcs call     */
    25 -           4 BREAK BIT(1) UNAL, /* Indicates break occurred during dbcs call   */
    26 -           4 DUMP BIT(1) UNAL, /* Indicates that a dbcs dump is in progress    */
    27 -           4 QUIT BIT(1) UNAL,      /* Indicates that a 'QUIT' has occurred    */
    28 -           4 * BIT(13) UNAL,             /* available for future use           */
    29 -           4 ETC BIT(1) UNAL,       /* Indicates to user that return is etc    */
    30 -       2 RVP PTR UNAL,
    31 -       2 USEP PTR UNAL,
    32 -       2 SNAME CHAR(30),                 /* schema name                        */
    33 -       2 SNCHAR(0:29) REDEF SNAME CHAR(1),
    34 -       2 * CHAR(2),
    35 -       2 SSNAME CHAR(30),                /* sub-schema name                    */
    36 -       2 SSNCHAR (0:29) REDEF SSNAME CHAR(1),
    37 -       2 * CHAR(2),
    38 -       2 PNAME CHAR(30),                 /* program name                       */
    39 -       2 * CHAR(2),
    40 -       2 AUTHOR CHAR(30),                /* author                             */
    41 -       2 * CHAR(2) UNAL,
    42 -       2 EC UBIN(36) UNAL,               /* Environment code                   */
    43 -       2 CMPTIME UBIN (36),              /* time compiled                      */
    44 -       2 SSDT CHAR(8),                   /*date and time sub-schema translated*/
    45 -       2 SSTDT (0:1) REDEF SSDT UBIN(36);
    46 - DCL IA$ PTR STCLASS;
    47 - %EQU EC_DBACS#=0;
    48 - %EQU EC_DBUTIL#=1;
    49 - %EQU EC_COBOL#=2;
    50 - %EQU EC_FORTRAN#=3;
    51 - %EQU EC_PL6#=4;
    52 - %EQU EC_APL#=5;
    53 - %EQU EC_BASIC#=6;
    54 - %EQU EC_IDP#=7;
    55 - %MEND;
    56 - %MACRO RVA(STCLASS=SYMREF);
    57 - /*         Refrence vector                    */
    58 - DCL RVA$ PTR STCLASS;
    59 - DCL 1 RVA BASED (RVA$) ALIGNED,
    60 -       2 W1,
    61 -         3 BOUND UBIN(1) UNAL,
    62 -         3 COUNT UBIN(17) UNAL,
    63 -         3 * UBIN(18) UNAL,
    64 -       2 RVE (0:0),
    65 -         3 EO UBIN(18) UNAL,             /* Element offset                     */
    66 -         3  T REDEF EO UBIN(18) UNAL,
    67 -         3 NAME UBIN(18) UNAL;
    68 - %MEND;
    69 - %MACRO UPT(STCLASS=SYMREF);
    70 - DCL UPT$ PTR STCLASS;
    71 - DCL 1 UPT BASED (UPT$) ALIGNED,
    72 -       2 W1,
    73 -         3 BOUND UBIN(1) UNAL,
    74 -         3 COUNT UBIN(17) UNAL,
    75 -         3 * UBIN(18) UNAL,
    76 -       2 UPTE(0:0),
    77 -         3 UCW,
    78 -           4 PT UBIN(9) UNAL,
    79 -           4 PB,
    80 -             5 PG UBIN(1) UNAL,          /* Get                                */
    81 -             5 PR REDEF PG UBIN(1) UNAL, /* Retrevial                          */
    82 -             5 PM UBIN(1) UNAL,          /* Modify                             */
    83 -             5 PU REDEF PM UBIN(1) UNAL, /* Update                             */
    84 -             5 PS UBIN(1) UNAL,          /* Store                              */
    85 -             5 PE UBIN(1) UNAL,          /* Erase                              */
    86 -             5 PC UBIN(1) UNAL,          /* Connect                            */
    87 -             5 PD UBIN(1) UNAL,          /* Disconnect                         */
    88 -             5 PF UBIN(1) UNAL,          /* Find                               */
    89 -             5 * UBIN(1) UNAL,
    90 -             5 LOCK UBIN(1) UNAL,
    91 -           4 A UBIN(18) UNAL,
    92 -         3 W1 REDEF UCW,
    93 -           4 * UBIN(9) UNAL,
    94 -           4 EC UBIN(27) UNAL,
    95 -         3 W2,
    96 -           4 UPADR UBIN(18) UNAL,
    97 -           4 * UBIN(18) UNAL;
    98 - %MEND;
    99 - %MACRO UWA;
   100 - /*
   101 -        User's work area 'UWA'.
   102 -                                                            */
   103 - DCL 1 UWA BASED (UWA$) ALIGNED,
   104 -       2 IFR SBIN,
   105 -       2 DBST CHAR(7),
   106 -       2 DB_STATUS REDEF DBST,
   107 -         3 STMTCODE CHAR(2),
   108 -         3 EXCEPTION CHAR(5),
   109 -       2 * CHAR(1),
   110 -       2 DBAN CHAR(30),
   111 -       2 * CHAR(2),
   112 -       2 DBSN CHAR(30),
   113 -       2 * CHAR(2),
   114 -       2 DBRN CHAR(30),
   115 -       2 * CHAR(2),
   116 -       2 DBPKY CHAR(12),
   117 -       2 DBDR UBIN,
   118 -       2 DBFN CHAR(30),
   119 -       2 * CHAR(2),
   120 -       2 DBKYN CHAR(30),
   121 -       2 * CHAR(4),                      /* Unused                             */
   122 - /* Subschema items and data base parameters */
   123 -       2 * CHAR(20);
   124 - DCL 1 UWA$ PTR SYMREF;                  /* User work area.                    */
   125 - %MEND;
