    6                                              /* Same as DBACS_DIR_STRUCS for locks */
    7      DCL 01 LOCKS EXT STATIC,                /*Locks directive*/
    8            02 STATUS BIT(3),
    9            02 LIST BIT(1),                   /* Locks report = on*/
   10            02 ASSIGNS BIT(1),                /*On = lock assignments on 5*         */
   11                          /*                               Off = no lock assigns*/
   12            02 FILR BIT(31),
   13            02 LOCKS_KEY CHAR(12),
   14            02 SNAME CHAR(30);

