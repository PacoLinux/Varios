   000633    000001 7100 11
                                       577  *
                                       578  *  FLOAT BINARY TO PACKED DECIMAL ROUTINE
                                       579  *
                                       580  *  DESCRIPTION OF SYMBOLS USED IN ZIG$DBFLD
                                       581  *              DCLSO - A DECIMAL NO. WITH LEADING SIGN HAVING
                                       582  *                      A VALUE OF +0
                                       583  *              DCLS1 - A DECIMAL NO. WITH LEADING SIGN HAVING
                                       584  *                      A VALUE OF +1
                                       585  *              RSDES - EIS DESCRITOR FOR DECIMAL RESULT (FROM
                                       586  *                       CONVERTING FLOAT BINARY TO DECIMAL). THE
                                       587  *                       RESULT IS STORED IN THE 4 BIT BUFFER (BUF4)
                                       588  *                       OF THE DBCB (8 WORDS).
                                       589  *              B36N (1 WORD) - VALUE OF THE EXPONENT
                                       590  *
                                       591  *              BN1 (2 WORDS) - VALUE OF THE MANTISSA
                                       592  *
                                       593  *              B72N (1 WORD) - NEGATIVE EXPONENT
                                       594  *
                                       595  *              B72N+1 (1 WORD) - POSITIVE EXPONENT
                                       596  *
                                       597  *              B32N (1 WORD) - SIGN OF THE EXPONENT
 12:42 09/05/97  FILE=ZIM$DBED.:ZIC0TSI       ENCODE ROUTINES                                            21   BMAP - B00     
                                       598  *
                                       599  *                      IS ACTUALLY STORED IN THE FIRST 8 WORDS OF TEMP 6
                                       600  *                      BIT BUFFER (BUF6)
                                       601  *
                                       602  *              CURLN (3 WORDS) - INTERMEDIATE DECIMAL RESULTS
                                       603  *
                                       604  *              BUF6+8 (LAST 3 WORDS IN 6-BIT BUFFER) - VALUE OF
                                       605  *                       MANTISSA
                                       606  *
                                       607  *    INPUT:  EAQ = FLOATING BINARY NUMBER NORMALIZED
                                       608  *
                                       609  *    OUTPUT:  DECIMAL NUMBER IN 4 BIT BUFFER WITH FLOATING SIGN
                                       610  *
   000634                              611  ZIG$DBFLD NULL
   000634    7 00470 7411 56           612         ICSAV
   000635    7 01056 4561 00           613         STE    B72N,,PR7      SAVE EXPONENT
   000636    7 00652 7571 00           614         STAQ   BN1,,PR7       SAVE MANTISSA
   000637 (0)000644 6010 00            615         TNZ    NOTZERO
   000640    1 000 0 1 120 3004 00     616         MVN    ,(1,,1),1,1
   000641 (0)000742 0 0 1 00 04        617         NDSC9  DCLS0,0,4,1
   000642 (0)000741 0000 00            618         ARG    RSDES
   000643 (0)000720 7100 00            619         TRA    ADJST
   000644                              620  NOTZERO NULL
                                       621  *
                                       622  *      Convert mantissa to digit string
                                       623  *      x = 16**e * f
                                       624  *
   000644    7 01175 4501 00           625         STZ    BUF6+5,,PR7
   000645    0 000 100 3015 00         626         BTD    (1),(1)
   000646    7 00652 0 0 0 00 10       627         NDSC9  BN1,0,8,,,PR7
   000647    7 01170 0 0 1 00 27       628         NDSC9  BUF6,0,23,1,,PR7
                                       629  *
                                       630  *      Process hexadecimal exponent
                                       631  *
   000650    7 01056 2351 00           632         LDA    B72N,,PR7
   000651    000034 7310 00            633         ARS    28
   000652    000007 0350 07            634         ADLA   7,DL           Apply bias
 12:42 09/05/97  FILE=ZIM$DBED.:ZIC0TSI       ENCODE ROUTINES                                            22   BMAP - B00     
   000653    000000 6360 05            635         EAQ    0,AL
   000654 (1)000000 3750 00            636         ANA    =O777777777760 GET 16*E1
   000655    7 01056 7551 00           637         STA    B72N,,PR7
   000656 (0)000664 6000 00            638         TZE    LAST
                                       639  *
                                       640  *      First step of radix conversion.
                                       641  *
   000657    000001 7310 00            642         ARS    1
   000660    7 01056 0351 00           643         ADLA   B72N,,PR7
   000661    0 000 0 0 100 2064 05     644         MP2D   (0,0,0,A),(1)
   000662 (0)001731 0 0 0 00 30        645         NDSC9  FACTOR1,0,24
   000663    7 01170 0 0 0 00 30       646         NDSC9  BUF6,0,24,0,0,PR7
   000664                              647  LAST   NULL
   000664    000017 3760 03            648         ANQ    =O000017,DU
