       94       42    1         2 LVFC CHAR (1) UNAL, /* Carriage control */
       95       43    1         2 DTE CHAR (8) UNAL, /* Mo/da/yr */
       96       44    1         2 * CHAR (2) UNAL,
       97       45    1         2 TIME CHAR (5) UNAL, /* HH:MM */
       98       46    1         2 * CHAR (2) UNAL,
       99       47    1         2 TITLE CHAR (50) UNAL, /* Sub title */
      100       48    1         2 * CHAR (6) UNAL,
      101       49    1         2 PGNO CHAR (6) UNAL; /* Xxxxxx */
      102       50    1   DCL BLANK_LINE CHAR (4) INIT ('A   ') STATIC SYMDEF ALIGNED;
