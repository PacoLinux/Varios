      121      197    1       IF ADDR(P_L) = ADDR(NIL)
      122      198    1       THEN
      123      199    1           L = 64;
      124      200    1       ELSE
      125      201    1           L = P_L;
      126      202    1       SUBHED.HED = SUBSTR(INBUF,0,L);
      127      203    1       WRITE_LO.BUF_ = VECTOR(SUBHED);
