PL6.E3A0      #002=SUBHD File=ZIG$DDX4.:ZIC0TSI                                  FRI 09/05/97 11:52 Page:18   
      114      190    1   SUBLST: ENTRY (INBUF,P_L);
      115      191    1       CALL M$TIME (TIME_FPT) ALTRET (ERR2);
      116      192    1       TTLN.TIME = LOCAL.TIME;
      117      193    1       PGNO = PGNO + 1;
      118      194    1       CALL BINCHAR (TTLN.PGNO,PGNO);
      119      195    1       WRITE_LO.BUF_ = VECTOR(TTLN);
