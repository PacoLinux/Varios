       25       25    1         2 * CHAR (2) INIT (' ') UNAL,
       26       26    1         2 TIME CHAR (5) UNAL,
       27       27    1         2 * CHAR (2) INIT (' ') UNAL,
       28       28    1         2 TITLE CHAR (50) UNAL, /* Sub title */
       29       29    1         2 * CHAR (6) INIT (' PAGE ') UNAL,
       30       30    1         2 PGNO CHAR (6) UNAL;
       31       31    1   DCL M$LO DCB;
       32       32        %INCLUDE CP_6;
       33      113        %F$DCB;
       34      170        %FPT_OPEN (FPTN=OPEN_LO,FUN=CREATE,EXIST=NEWFILE,DCB=M$LO
       35      171         ,CTG=YES,ORG=UR);
       36      280        %EJECT;
PL6.E3A0      #001=HLIST File=ZIG$DDX4.:ZIC0TSI                                  FRI 09/05/97 11:52 Page:2    
       37      281    1       IF PROPN = 0 /* Check if output file is opened? */
       38      282    2       THEN DO;
       39      283    2           OPEN_LO.V.LINES# = DCBADDR(DCBNUM(M$LO))-> F$DCB.LINES#;
       40      284    2           CALL M$OPEN (OPEN_LO) ALTRET (ERR1); /* Open print file */
       41      285    2           PROPN = 1; /* Set file opened flag */
       42      286    2           NLINES=DCBADDR(DCBNUM(M$LO))->F$DCB.LINES#; /*Vertical page height */
       43      287    2           IF NLINES = 0 THEN NLINES = 9999; /* Infinite */
       44      288    2           END;
       45      289    1       TTLN.TITLE = INBUF; /* Move title info to output buffer */
       46      290    1       PGNO = 0; /* Set page number to zero */
       47      291    1       RETURN;
       48      292    1   ERR1:
       49      293    1       PROPN = 0;
       50      294    1       RETURN; /* To be expanded later */
       51      295    1   END;
