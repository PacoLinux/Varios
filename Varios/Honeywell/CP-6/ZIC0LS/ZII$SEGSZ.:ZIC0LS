      178     4914        */
      179     4915        /********************************************************************/
      180     4916    1       ERROR_CODE = #BADSUBNAM;
      181     4917    1       GOTO COM_ERR;
      182     4918    1   ER_LNG:
      183     4919        /*********************************************************/
      184     4920        /*E*
