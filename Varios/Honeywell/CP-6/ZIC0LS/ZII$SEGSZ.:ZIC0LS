        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8            NAME: ZII$SEGSZ
        9        9            PURPOSE:
       10       10                   Called by IDP via ZII$BUILD_TABLES, APL at the execution of
       11       11                   the 'DBSUB' directive, and by BASIC at the execution of
       12       12                   the 'DB SUB' directive. Returns a value in words of
       13       13                   the amount of memory needed to build the tables for
       14       14        ZII$SEGSZ: PROC (SSNAME,SEG_SIZ,PROC_CALL,SCHNAME,LOCK_VAL,SYSSID,TRNDT) ALTRET AVOI
                14        D($PR5,$PR6,$PR7);
       15       15               INPUTS:
       16       16                   SSNAME = subschema name
       17       17                   PROC_CALL = tells what proccessor is calling the interface module
       18       18                   LOCK_VAL  = has the privacy lock for invoking the subschema
       19       19                   SYSSID  =  has an indentification of the user using this subschema
       20       20               OUTPUTS:
       21       21                   SEGSZ = number of words needed to build the required tables
       22       22                   SCHNAME = Schema name of the referenced subschema file
       23       23                   TRNDT  = Date of subschema translation
       24       24        ************************************************************/
       25       25        ZII$SEGSZ: PROC (SSNAME,SEG_SIZ,PROC_CALL,SCHNAME,LOCK_VAL,SYSSID,TRNDT) ALTRET;
       26       26        /******************  PARAMETERS  ******************/
       27       27    1   DCL SSNAME CHAR(30);
       28       28    1   DCL SEG_SIZ UBIN WORD;
       29       29    1   DCL PROC_CALL UBIN; /*Flags to tell which processor is calling me.Values are:
       30       30         1 = cobol
       31       31         2 = fortran
       32       32         3 = apl
       33       33         4 = idp */
       34       34    1   DCL SCHNAME CHAR(30);
       35       35    1   DCL LOCK_VAL CHAR(12);
       36       36    1   DCL SYSSID CHAR(32);
PL6.E3A0      #001=ZII$SEGSZ File=ZII$SEGSZ.:ZIC0TSI                             FRI 09/05/97 12:32 Page:2    
       37       37    1   DCL TRNDT UBIN(36) ;
       38       38        /******************  AUTO DCLS   ******************/
       39       39    1   DCL INDX UBIN(36);
       40       40        /***************  EXT DCLS  ********************/
       41       41    1   DCL REC$ PTR EXT;
       42       42    1   DCL PRC_CAL UBIN(36) INIT(0) EXT;
       43       43    1   DCL 1 SUBSCH EXT,
       44       44    1         2 NAME,
       45       45    1           3 CNT UBIN(9) UNAL,
       46       46    1           3 TEXT CHAR(32) UNAL,
       47       47    1         2 DCB# UBIN(18) ALIGNED;
       48       48    1   DCL RVA_SIZ UBIN WORD INIT(0) EXT ALIGNED;
       49       49    1   DCL IA_SIZ UBIN(36) INIT(46) EXT ALIGNED;
       50       50    1   DCL 1 SUBSCH_CON_BLK EXT,
       51       51    1         2 * (0:63) UBIN(36) INIT(0*64) UNAL;
       52       52    1   DCL SUB_CONBLK$ PTR INIT(ADDR(SUBSCH_CON_BLK)) EXT;
       53       53    1   DCL 1 I_O_BUF EXT,
       54       54    1         2 REC (0:1) CHAR(128) UNAL;
       55       55    1   DCL I_O_IDX UBIN(36) INIT(0) EXT;
       56       56    1   DCL HASH_VAL UBIN(36) INIT(0) EXT;
       57       57    1   DCL SYS_ID CHAR(32) EXT;
       58       58    1   DCL 1 ERROR_CODE UBIN(36) EXT;
       59       59        /***************  INCLUDE FILES  ******************/
       60       60        %INCLUDE CP_6;
       61      141        %INCLUDE CP_6_SUBS;
       62      681        %FPT_GETDCB(FPTN=GETDCB_FPT);
       63      696        %FPT_OPEN(FPTN=OPEN_FPT,FUN=IN,ORG=CONSEC,ACS=SEQUEN,ASN=FILE,
       64      697         SETDCB=YES,SHARE=IN,SCRUB=YES);
       65      806        %FPT_OPEN(FPTN=RES_FPT,REASSIGN=YES,SETDCB=NO);
       66      915        %FPT_CLOSE(FPTN=CLOSE_FPT);
       67      957        %FPT_READ (FPTN=READ_FPT,BUF=I_O_BUF);
       68      996        %INCLUDE ZI_ERRORS_SUBS;
       69     1516        %INCLUDE ZI$STRUCTS;
       70     1588        %UWA1 (STCLASS="EXT");
       71     1611        %NAM1 (STCLASS="EXT");
       72     1640        %NAM2 (STCLASS="EXT");
       73     1654        %INCLUDE ZI_SUB_LANG;
PL6.E3A0      #001=ZII$SEGSZ File=ZII$SEGSZ.:ZIC0TSI                             FRI 09/05/97 12:32 Page:3    
       74     3369        %INCLUDE ZI$DESC;
       75     4219        %PROC_SUBS;
       76     4226        %APL_KEYWD_LIST;
       77     4610        %CONTROL_BLOCK (STCLASS="BASED(SUB_CONBLK$)");
       78     4718        %COMPILATION_UNIT_REC (STCLASS="BASED(REC$)");
       79     4751        %INCLUDE ZI$TOP_STRUCTS;
       80     4812        %TBL_DIM_DESC;
       81     4817        /***************PROGRAM PROCEDURE******************/
       82     4818    1       ERROR_CODE=0; /*Initailize the error code upon 1st entering ZII$SEGSZ*/
       83     4819    1       I_O_IDX=2; /*Initialize these values for start*/
       84     4820    1       NAM1_IDX=-1; /*Initialize the NAM1 index*/
       85     4821    1       SUBSCH.NAME.TEXT=SSNAME;
       86     4822    1       CALL INDEX(INDX,' ',SUBSCH.NAME.TEXT);
              4822                /*Get number of characters in subschema name*/
       87     4823    1       SUBSCH.NAME.CNT=INDX;
       88     4824    1       IF SUBSCH.DCB#=0 OR PROC_CALL~=#IDP THEN
       89     4825    2       DO;
       90     4826    2           GETDCB_FPT.DCBNAME_=VECTOR(SUBSCH.NAME);
              4826                    /*Frame the textc field for a call to M$GETDCB */
       91     4827    2           CALL M$GETDCB(GETDCB_FPT) ALTRET(ER_GETDCB);
       92     4828    2           SUBSCH.DCB#=GETDCB_FPT.V.DCB#; /*Return the DCB# from call to M$GETDCB */
       93     4829    2           OPEN_FPT.V.DCB#=SUBSCH.DCB#;
       94     4830    2           OPEN_FPT.NAME_=VECTOR(SUBSCH);
              4830                    /*Put in the default file name for the subschema*/
       95     4831    2           CALL M$OPEN(OPEN_FPT) ALTRET(ER_OPEN);
              4831                    /*Open the subschema file update to get info*/
       96     4832    2           RES_FPT.V.DCB#=SUBSCH.DCB#;
       97     4833    2           CALL M$OPEN(RES_FPT) ALTRET(ER_OPEN);
       98     4834        /* Open to merge in user's set command if present. */
       99     4835    2           END;
      100     4836    1       READ_FPT.V.DCB#=SUBSCH.DCB#;
      101     4837    1       CALL M$READ(READ_FPT) ALTRET(ER_READ);
              4837                /*Read the control block of the subschema file*/
      102     4838    1       SUBSCH_CON_BLK=I_O_BUF; /*Move control block of subsch into a  temp area*/
      103     4839    1       SUB_CONBLK$=ADDR(SUBSCH_CON_BLK);
              4839                /*Set up ptr to over control blk with structure*/
      104     4840    1       TBL_DIM=CONTROL.NUM_RECORDS;
PL6.E3A0      #001=ZII$SEGSZ File=ZII$SEGSZ.:ZIC0TSI                             FRI 09/05/97 12:32 Page:4    
      105     4841    1       IF CONTROL.VALIDATION_STATUS = -1 THEN
              4841                /*Subschema has not been validated  error */
      106     4842    1           GOTO ER_SUBSCH;
      107     4843    1       IF CONTROL.SUBSCHEMA_NAME ~= SSNAME THEN
              4843                /*Subsch name given doesn't match name in the subschema file*/
      108     4844    1           GOTO ER_SUBNAM;
      109     4845    1       IF PROC_CALL = #BASIC AND
      110     4846    1          CONTROL.LANGUAGE ~= 4 THEN
      111     4847    1           GOTO ER_LNG; /* not a BASIC subschema */
      112     4848    1       IF LOCK_VAL ~= CONTROL.INVOKE_PRIVACY_LOCK THEN GOTO ER_PRIV;
              4848                /*Test for invoking*/
      113     4849    1       PRC_CAL=PROC_CALL; /*Put in the proc type that called us */
      114     4850    1       UWA_SIZ=CONTROL.UWA_SIZE; /*Get the uwa size from the control block*/
      115     4851    1       NAM1_SIZ=(CONTROL.NUM_AREAS+CONTROL.NUM_RECORDS+CONTROL.NUM_FIELDS+CONTROL.
              4851                NUM_SETS
      116     4852    1          +CONTROL.NUM_DB_PARAM+CONTROL.NUM_LOCKS)*10;
      117     4853         /*Get the NAM1 table size from info in the con. blk*/
      118     4854    1       NAM2_SIZ=807+((CONTROL.NUM_AREAS+CONTROL.NUM_RECORDS+CONTROL.NUM_FIELDS+
      119     4855    1          CONTROL.NUM_SETS+CONTROL.NUM_REC_SET_REL+CONTROL.NUM_DB_PARAM+CONTROL.
              4855                   NUM_LOCKS
      120     4856    1          +CONTROL.NUM_REC_AREA_REL+CONTROL.NUM_FIELD_SET+CONTROL.NUM_SET_SET_DESC)*9);
      121     4857         /*Get NAM2 size for info in con. blk + 807 wrds of ptrs*/
      122     4858    1       IF PROC_CALL = #APL THEN /*Are we being called from APL ? */
      123     4859    2       DO; /*Yes.. add size to NAM1 & NAM2  for KEYWORDS */
      124     4860    2           NAM1_SIZ=NAM1_SIZ+(10*(#KEYWDS));
      125     4861    2           NAM2_SIZ=NAM2_SIZ+(9*(#KEYWDS));
      126     4862    2           END;
      127     4863    1       RVA_SIZ=2+CONTROL.NUM_AREAS+CONTROL.NUM_RECORDS+CONTROL.NUM_FIELDS+
      128     4864    1          CONTROL.NUM_SETS;
      129     4865         /*Get RVA size from con. blk +2 wrds for count & NULL element*/
      130     4866    1       IF PROC_CALL ~= #IDP THEN /*Are we being called from IDP ? */
      131     4867         /*NO     Add up all table sizes for SEG_SIZ*/
      132     4868    1           SEG_SIZ = UWA_SIZ+RVA_SIZ+IA_SIZ+NAM1_SIZ+NAM2_SIZ;
      133     4869         /*YES  .. Add all tables except UWA for SEG_SIZ */
      134     4870    1       ELSE SEG_SIZ = RVA_SIZ+IA_SIZ+NAM1_SIZ+NAM2_SIZ;
      135     4871    1       SCHNAME=CONTROL.SCHEMA_NAME; /*Put in  schema name */
      136     4872    1       TRNDT=CONTROL.SUBSCHEMA_UTS;
PL6.E3A0      #001=ZII$SEGSZ File=ZII$SEGSZ.:ZIC0TSI                             FRI 09/05/97 12:32 Page:5    
      137     4873    1       SYS_ID = SYSSID;
      138     4874    1   CLSE_UP:
      139     4875    1       CLOSE_FPT.V.DCB#=SUBSCH.DCB#; /*Close the subschema file*/
      140     4876    1       CALL M$CLOSE(CLOSE_FPT) ALTRET(ER_CLOSE);
      141     4877    1   RET:
      142     4878    1       RETURN;
      143     4879    1   ER_OPEN:
      144     4880        /**************************************************************/
      145     4881        /*
      146     4882              ERROR: ZII-#OPNER-C
      147     4883        MESSAGE: Cannot open the subschema file %U1.
      148     4884        */
      149     4885        /**********************************************************/
      150     4886    1       ERROR_CODE=#OPNER;
      151     4887    1       GOTO COM_ERR;
      152     4888    1   ER_GETDCB:
      153     4889        /**********************************************************/
      154     4890        /*E*
      155     4891              ERROR: ZII-#GETDCBER-C
      156     4892        MESSAGE: Error on GETDCB for subschema %U1.
      157     4893        */
      158     4894        /**********************************************************/
      159     4895    1       ERROR_CODE=#GETDCBER;
      160     4896    1       GOTO COM_ERR;
      161     4897    1   ER_READ: ;
      162     4898    1       GOTO COM_ERR;
      163     4899    1   ER_SUBSCH:
      164     4900        /*********************************************************/
      165     4901        /*E*
      166     4902             ERROR: ZII-#BADSUB-C
      167     4903        MESSAGE: Subschema %U1 is not a valid subschema.
      168     4904        */
      169     4905        /********************************************************/
      170     4906    1       ERROR_CODE=#BADSUB;
      171     4907    1       GOTO COM_ERR;
      172     4908    1   ER_SUBNAM:
      173     4909        /*****************************************************************/
PL6.E3A0      #001=ZII$SEGSZ File=ZII$SEGSZ.:ZIC0TSI                             FRI 09/05/97 12:32 Page:6    
      174     4910        /*E*
      175     4911            ERROR: ZII-#BADSUBNAM-C
      176     4912        MESSAGE: Subschema name on directive does not  match the name in the
