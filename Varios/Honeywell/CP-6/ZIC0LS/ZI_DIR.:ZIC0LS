FRI. SEP 05 '97 11:22:41.28  FILE=ZI_DIR

%MACRO DIR;
DCL DIR$ PTR INIT(ADDR(ZI_DIR)) STATIC SYMDEF;
DCL 1 ZI_DIR STATIC ALIGNED,
      2 RNG_LIST BIT(1) UNAL INIT('0'B), /* 1 if ranges set*/
      2 INITL BIT(1) UNAL INIT('0'B),   /* 1 if initialization*/
      2 INVREBLD BIT(1) UNAL INIT('0'B), /* 1 if inventory to be rebuilt, 0 if restored from dump*/
                         /* If the inventory pages are not in the dump, then  */
                                        /* They are recalculated              */
      2 LOAD BIT(1) UNAL INIT('0'B),    /* 1 if load                          */
      2 UNLOAD BIT(1) UNAL INIT('0'B),  /* 1 if unload                        */
      2 PRINT BIT(1) UNAL INIT('0'B),   /* 1 if print                         */
      2 KEYBLD BIT(1) INIT('0'B) UNAL,  /* 1 if keybld                        */
      2 ENCRPT BIT(1) UNAL INIT('0'B),  /* 1 if encryption                    */
      2 PRNT_OPT BIT(1) UNAL INIT('0'B), /**/
      2 KEYDUMP BIT(1) UNAL INIT('0'B), /* 1 if dump to keyed file, 0 consec  */
      2 DISCFLAG BIT(1) UNAL INIT('1'B), /* 1 if dump to disc, 0 to tape      */
      2 INVDUMP BIT(1) UNAL INIT('1'B), /* 1 if inventory pages to be dumped  */
      2 INVREST BIT(1) UNAL INIT('1'B), /* On load - is inventory to be redone at all?*/
      2 HELP UBIN(36) INIT(0) UNAL,
      2 HELP_SVRITY UBIN(36) INIT(0) UNAL,
      2 ACNT (0:9) CHAR(8) INIT(' '*0) UNAL,
      2 AUTODUMP BIT(1) UNAL INIT('1'B), /* 1 implies look for associated key file*/
                    /* This option applies both to load and dump utilities    */
                         /* The source and target for these associated files  */
               /* Must/will have the same attributes (I.E. Passwords etc)     */
                                   /* As the main source and target files     */
      2 ENCP_SEED BIT(36) ALIGNED INIT('0'O), /* Seed                         */
      2 ENCP_SEED_RD REDEF ENCP_SEED UBIN(36) UNAL, /**/
      2  DEST_SEED BIT(36) ALIGNED INIT('0'O),
      2  DEST_SEED_RD REDEF DEST_SEED UBIN(36) ALIGNED,
      2 SCHFID CHAR(120) UNAL,
      2 DESTFID REDEF SCHFID CHAR(120),
      2 AREAFID CHAR(120) UNAL,
      2 SRCEFID REDEF AREAFID CHAR(120),
      2 ACNTLIST BIT(1) INIT('0'B) UNAL,
      2 DATEFLG BIT(1) INIT('0'B) UNAL,
      2 OPEN_MODE UBIN(2) INIT(0) UNAL,
      2 NOKEYS   BIT(1)  INIT('0'B) UNAL,
      2 PGRG_RESET  BIT(1) INIT('1'B) UNAL,
      2 SCHEMA_ACTIVE BIT(1) INIT('0'B) UNAL,
      2 SCHEMA_NEEDED BIT(1) INIT('0'B) UNAL,
      2 LOCALKEY      BIT(1) INIT('0'B) UNAL,
      2 CHECK         BIT(1) INIT('0'B) UNAL,
      2 NOPRNT        BIT(1) INIT('0'B) UNAL,
      2 ROLL          BIT(1) INIT('0'B) UNAL,
      2 PCALC         BIT(1) INIT('0'B) UNAL,
      2 PITEM         BIT(1)  INIT('1'B) UNAL,
      2 PHEAD         BIT(1) INIT('0'B) UNAL,
      2 FSET          BIT(1) INIT('0'B) UNAL,
      2 CODE          UBIN(5) INIT(0)   UNAL,
      2 PSETS         BIT(1) INIT('1'B) UNAL,
      2 SYSCTL        BIT(1) INIT('0'B) UNAL,
      2 LOCKCODE      UBIN(3) INIT(0)   UNAL,
      2 REPRT_ALL BIT(1) UNAL,
      2 REPRT_REC BIT(1) UNAL,
      2 REPRT_PG       BIT(1)   INIT('0'B) UNAL,
      2 REPRT_AREA     BIT(1)   INIT('0'B) UNAL,
      2 REPRT_ERRS     BIT(1)   INIT('0'B)  UNAL,
      2 REPRT_ZERO     BIT(1)   INIT('0'B)  UNAL,
      2 ALTKEY         BIT(1)   INIT('0'B) UNAL,
      2 CALCKEY       BIT(1)   UNAL,
      2 PRNT_INV      BIT(1)   UNAL,
      2 DATE,
        3 YR CHAR(2) INIT(' ') UNAL,
        3 MO CHAR(2) INIT(' ') UNAL,
        3 DY CHAR(2) INIT(' ') UNAL,
      2 DATEIN REDEF DATE CHAR(6),
      2 TIME,
        3 HR CHAR(2) INIT(' ') UNAL,
        3 MIN CHAR(2) INIT(' ') UNAL,
        3 SEC CHAR(2) INIT(' ') UNAL,
        3 MIL CHAR(2) INIT(' ') UNAL,
      2 TIMEIN REDEF TIME CHAR(8) UNAL,
      2 SET_NM CHAR(30) INIT(' ') UNAL,
      2 POS         UBIN BYTE INIT(0) UNAL,
      2 *           BIT(27)           UNAL,
      2 GAP1 (0:46) CHAR(4) INIT(' '*0) UNAL,
      2 INIT_TMPLT_KEY UBIN(36) INIT(0) UNAL, /*Number of keys to allocate for init of template areas*/
      2 SCHEMA_NM ALIGNED,
        3 NAME UNAL,
          4 CNT UBIN(9) UNAL,
          4 TEXT CHAR(30) UNAL,
        3 DCB# UBIN(18)  ALIGNED,
      2 AREA_NM ALIGNED,
        3 NAME UNAL,
          4 CNT UBIN(9) UNAL,
          4 TEXT CHAR(30) UNAL,
        3 DCB# UBIN(18)  ALIGNED,
      2 SRCE_NM ALIGNED,
        3 NAME UNAL,
          4 CNT UBIN(9) UNAL,
          4 TEXT CHAR(30) UNAL,
        3 DCB# UBIN(18)  ALIGNED,
      2 DEST_NM ALIGNED,
        3 NAME UNAL,
          4 CNT UBIN(9) UNAL,
          4 TEXT CHAR(30) UNAL,
        3 DCB# UBIN(18)  ALIGNED;
DCL 1 INIT_RG (0:19) STATIC SYMDEF,

FILE=ZI_DIR                        ZI_DIR
