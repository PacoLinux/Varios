    68 -       2 REPRT_PG       BIT(1)   INIT('0'B) UNAL,
    69 -       2 REPRT_AREA     BIT(1)   INIT('0'B) UNAL,
    70 -       2 REPRT_ERRS     BIT(1)   INIT('0'B)  UNAL,
    71 -       2 REPRT_ZERO     BIT(1)   INIT('0'B)  UNAL,
