       96     3997        %LIST;
       97     3998        %DSC_PARAM (STCLASS="EXT");
              3999    1   DCL DP$ PTR STCLASS ALIGNED;
              4000    1   DCL 1 DSC_PARAM BASED(DP$) ALIGNED,
              4001    1         2 NAM_IDX UBIN(18) UNAL, /*Index inot NMA1 for the data_base_parameter  */
              4002    1         2 RVA_OFF         UBIN(18) UNAL,  /*Index into the RVA for this element */
              4003    1         2 UWA_OFF UBIN(18) UNAL, /*Word offset into UWA for the db parameter    */
              4004    1         2 *               UBIN(18) UNAL,
              4005    1         2 * (0:1) UBIN(36) UNAL,
