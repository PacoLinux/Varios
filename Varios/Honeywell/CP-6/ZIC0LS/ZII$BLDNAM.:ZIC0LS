      109     4334        %INCLUDE ZI_ERRORS_SUBS;
      110     4854        %FPT_OPEN(FPTN=OPEN_FPT,FUN=IN,ORG=KEYED,ACS=DIRECT,ASN=FILE,SETDCB=YES,SHARE=ALL);
      111     4963        %FPT_CLOSE(FPTN=CLOSE_FPT);
      112     5005        /******************Program Procedure******************/
      113     5006    1       ERROR_CODE=0; /*Initailize the error code upon 1st entering ZII$BLDNAM*/
      114     5007    1       ZI$ENTRY_COUNT=0;
      115     5008        /* ZI$ENTRY_COUNT is the external count of the # of elements in the
      116     5009           NAM1 table minus 1, or the last usable index of NAM1. */
      117     5010    1       S_SW=0;
      118     5011    1       R_SW=0;
      119     5012    1       A_SW=0;
      120     5013    1       K_SW=0;
      121     5014    1       SEG='0'B; /*Zero out the segment  gotten for dump
      122     5015         readability purposes only */
      123     5016    1       LVL_STK='0'B; /*Zero out the level tracker stack used in ZII$BLD_NAM*/
      124     5017    1       STK_IDX = 0; /*Initialize the level stack index for use in ZII$BLD_NAM*/
      125     5018         /*Open the subschema file to retrieve info
      126     5019            for building the tables only*/
      127     5020    1       OPEN_FPT.V.DCB#=SUBSCH.DCB#; /*Set up the  DCB */
      128     5021    1       CALL M$OPEN(OPEN_FPT) ALTRET(ER_OPEN);
      129     5022        /****************************************************************************/
      130     5023        /*                Build the vectors to return to apl and idp  for their use
              5023                 */
      131     5024        /****************************************************************************/
      132     5025    1       IF PRC_CAL ~= #IDP THEN /*Are we being called from  IDP ?? */
PL6.E3A0      #001=ZII$BLDNAM File=ZII$BLDNAM.:ZIC0TSI                           FRI 09/05/97 12:25 Page:5    
      133     5026    2       DO; /*NO.... */
      134     5027    2           UWA$=MEM_.PTR$; /*Set up the UWA vector to return back to */
      135     5028    2           UWA#=(CONTROL.UWA_SIZE-48)*4;
      136     5029    2           UWA_=VECTOR(UWA); /*Uwa  is the 1st table built in memory */
      137     5030    2           UWA_.W1.VSIZE#=UWA_SIZ*4-1;
      138     5031    2           RVA$=PINCRW(UWA$,CONTROL.UWA_SIZE); /*Set up the  RVA vector */
      139     5032    2           END;
      140     5033    2       ELSE DO; /*We were called from IDP */
      141     5034        /*             IDP  is a special case because they want the UWA to be in their work
              5034        space
      142     5035              ,so they will pass a UWa vector instead of me returning one to them         */
      143     5036    2           UWA$=UWA_.PTR$; /*Set up the UWA ptr from the vector they passed*/
      144     5037    2           UWA#=(CONTROL.UWA_SIZE -48)*4;
              5037                    /*Get the size from the subschema control blk*/
      145     5038    2           RVA$=MEM_.PTR$;
      146     5039    2           END;
      147     5040    1       RVA_=VECTOR(RVA); /*Rva is the 2nd table built in memory */
      148     5041    1       RVA_.W1.VSIZE#=RVA_SIZ*4-1;
      149     5042    1       IA$=PINCRW(RVA$,RVA_SIZ); /*Set up the IA vector */
      150     5043    1       IA_=VECTOR(IA); /*Ia is the 3rd table built in memory */
      151     5044    1       NAM1$=PINCRW(IA$,IA_SIZ); /*Ia is a constant size of 46 words */
      152     5045    1       NAM1_=VECTOR(NAM1); /*NAM1 is the 4th table built in memory */
      153     5046    1       NAM1_.W1.VSIZE#=NAM1_SIZ*4-1;
      154     5047    1       NAM2$=PINCRW(NAM1$,NAM1_SIZ); /*Set up the NAM" vector */
      155     5048    1       DESC#=(NAM2_SIZ-#ENTRY)*4; /*Size of the descriptor portion of NAM2 */
      156     5049    1       NAM2_=VECTOR(NAM2); /*NAM2 is the 5th table built in memory */
      157     5050    1       NAM2_.W1.VSIZE#=NAM2_SIZ*4-1;
      158     5051    2       DO I=0 TO #ENTRY-1 BY 1; /* Initialize the ptr array in NAM2 */
      159     5052    2           NAM2.LLIST$(I)=ADDR(NIL);
      160     5053    2           END;
      161     5054        /**/
      162     5055    1       TNAM2$=ADDR(NAM2.LLIST$(#ENTRY-#DSC_SIZ));
      163     5056         /* Init TNAM2$ to point #DSC_SIZ (9) words back from where
      164     5057            table descriptors are to be stored so that when we
      165     5058            PINCRW forward to store the 1st one, we'll be in the
      166     5059            right place. */
      167     5060        /*   Add NULL element for "FIND ANY", etc.
PL6.E3A0      #001=ZII$BLDNAM File=ZII$BLDNAM.:ZIC0TSI                           FRI 09/05/97 12:25 Page:6    
      168     5061             The type code for the NULL element in the RVA is 4.   */
      169     5062    1       RVA.RVE.T(0)=4;
      170     5063    1       RVA.W1.COUNT=1; /*Initialize the rva count */
      171     5064         /**********************************************************************************
              5064        *********
      172     5065
      173     5066            SUBSCHEMA   NAME     SEGMENT    SEARCH
      174     5067
      175     5068         *****************************************************************************/
      176     5069    1       I_O_IDX=2; /*Init I_O_IDX */
      177     5070    1       ZI$KEY_NUMBER=(CONTROL.FIRST_NF_RECORD/2)-1;
      178     5071    1       CALL ZII$READ_SS ALTRET(ER_CHKRNG);
      179     5072    2       DO WHILE (SUBSTR(I_O_BUF(I_O_IDX),0,12)~='/*/*/*/*/*/*');
              5072                /*Test to see if we are at the
      180     5073         end of the  NAME SEGMENT */
      181     5074    2           REC$=ADDR(I_O_BUF(I_O_IDX)); /*Set up the ptr to the record in the  bloc */
      182     5075    2           IF DESC_TYPE_9.TYPE = 9 THEN /*Is this a type 9 descriptor */
      183     5076    2               CALL ZII$BLD_NAM ALTRET(ER_BLD_NAM);
              5076                        /*YES..  Build entry in NAM1 and NAM2 */
      184     5077    2           I_O_IDX=I_O_IDX+1; /*Increment I_O_IDX */
      185     5078    2           CALL ZII$READ_SS ALTRET(ER_CHKRNG);
      186     5079    2           END;
      187     5080        /***********************************************************************************
              5080        ************
      188     5081
      189     5082                         SUBSCHEMA  DESCRIPTOR SEGMENT SEARCH
      190     5083
      191     5084        ******************************************************************************/
      192     5085    1       I_O_IDX=2;
      193     5086    1       ZI$KEY_NUMBER=(CONTROL.FIRST_SDS_RECORD/2)-1;
      194     5087    1       CALL ZII$READ_SS ALTRET(ER_CHKRNG);
      195     5088    2       DO WHILE (SUBSTR(I_O_BUF(I_O_IDX),0,12) ~='/*/*/*/*/*/*');
              5088                /*Chk for end of DESC. SEG */
      196     5089    2           REC$=ADDR(I_O_BUF(I_O_IDX));
      197     5090    2           CALL ZII$ADD_NAM ALTRET(ER_ADD_NAM);
              5090                    /*Add info to NAM2 table for this descriptor elem*/
      198     5091    2           I_O_IDX=I_O_IDX+1; /*Increment I_O_IDX */
PL6.E3A0      #001=ZII$BLDNAM File=ZII$BLDNAM.:ZIC0TSI                           FRI 09/05/97 12:25 Page:7    
      199     5092    2           CALL ZII$READ_SS ALTRET(ER_CHKRNG);
      200     5093    2           END;
      201     5094        /***********************************************************************************
              5094        ***********
      202     5095
      203     5096                        SUBSCHEMA NAME EXTENTION SEGMENT SEARCH
      204     5097
      205     5098        ******************************************************************************/
      206     5099    1       I_O_IDX=2;
      207     5100    1       ZI$KEY_NUMBER=(CONTROL.FIRST_NFE_RECORD/2)-1;
      208     5101    1       CALL ZII$READ_SS ALTRET(ER_CHKRNG);
      209     5102    1       I_O_IDX=MOD(CONTROL.FIRST_NFE_RECORD,2);
      210     5103    1       I=0; /*Initialize I */
      211     5104    2       DO WHILE(I <= 2 ); /*This test  is for the  number of
      212     5105         encountered...should be 3 */
      213     5106    2           REC$=ADDR(I_O_BUF(I_O_IDX)); /*Set up record ptr */
      214     5107    2           IF (SUBSTR(I_O_BUF(I_O_IDX),0,12) = '/*/*/*/*/*/*') THEN
      215     5108    2               I=I+1;
      216     5109    2           ELSE IF DESC_TYPE_9.TYPE = 9 THEN /*Is this a type 9 descriptor / */
      217     5110    3               DO; /*YES..must be a descriptor for a  parameter in subsch*/
      218     5111    3                   NAM1_IDX=NAM1_IDX+1; /*Put  info into NAM1 table */
      219     5112    3                   NAM1.NAME.CNT(NAM1_IDX)=DESC_TYPE_9.NAME_SIZE;
      220     5113    3                   NAM1.NAME.TEXT(NAM1_IDX)=DESC_TYPE_9.NAME;
