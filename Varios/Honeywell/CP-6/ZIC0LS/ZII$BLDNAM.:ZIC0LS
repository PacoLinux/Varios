              4013    1         2 LLIST$          PTR;            /*Ptr to next desc in link list       */
       98     4015        %PLIST;
       99     4016        %DSC_DESC (STCLASS="EXT");
      100     4024        %DSC_RFK (STCLASS="EXT");
      101     4040        %DSC_RA (STCLASS="EXT");
