      236     5129    4                       END;
      237     5130    3                   ELSE
      238     5131    3                       IF DESC_TYPE_9.COMP_3 THEN
      239     5132    4                       DO;
      240     5133    4                           IF DESC_TYPE_9.PIC_HAS_S THEN
      241     5134    4                               DSC_PARAM.TYPE = 5;
