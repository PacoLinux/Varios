        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8          NAME:  ZII$BLDNAM
        9        9          PURPOSE: This mod will build  6 tables needed by the APL and IDP
       10       10                   processers . The tables are:
       11       11                       IA - needed  by IDS
       12       12                       UWA - descriptions of all elements in a given subschema
       13       13                       RVA - reference vector addresses used to locate elements in the UWA
       14       14                NAM1 - table of all elements in the subschema file with ptrs to  their desc.
       15       15                       NAM2 - table  of decriptors used in describing elements in NAM1
       16       16                               handling if present
       17       17                  INPUTS:
       18       18                      MEM_  = vector of memory allocated to build tables
       19       19                  OUTPUTS:
       20       20                      UWA_ = vector of the UWA table
       21       21                      RVA_ = vector of the RVA table
       22       22                      IA_  = vector of the IA table
       23       23                      NAM1_ = vector of  the NAM1 table
       24       24                      NAM2 = vector of the NAM2 table
       25       25        */
       26       26        /**********************************************************************/
       27       27        ZII$BLDNAM: PROC (MEM_,UWA_,RVA_,IA_,NAM1_,NAM2_) ALTRET AVOID($PR5,$PR6,$PR7);
       28       28        %INCLUDE CP_6;
       29      109        %INCLUDE CP_6_SUBS;
       30      649        /*****************PARAMETERS*****************/
       31      650        %VLP_VECTOR(FPTN=MEM_,STCLASS=);
       32      665        %VLP_VECTOR(FPTN=UWA_,STCLASS=);
       33      680        %VLP_VECTOR(FPTN=RVA_,STCLASS=);
       34      695        %VLP_VECTOR(FPTN=IA_,STCLASS=);
       35      710        %VLP_VECTOR(FPTN=NAM1_,STCLASS=);
       36      725        %VLP_VECTOR(FPTN=NAM2_,STCLASS=);
       37      740        /**/
PL6.E3A0      #001=ZII$BLDNAM File=ZII$BLDNAM.:ZIC0TSI                           FRI 09/05/97 12:25 Page:2    
       38      741        /****************BASED DCLS***************/
       39      742    1   DCL 1 SEG BASED(MEM_.PTR$),
       40      743    1         2 * CHAR(1) UNAL,
       41      744    1         2 * CHAR(MEM_.W1.VSIZE#) UNAL;
       42      745        /****************AUTO DCLS****************/
       43      746    1   DCL I UBIN(36); /*Do varible*/
       44      747    1   DCL SEG_SIZ UBIN(36) ALIGNED;
       45      748    1   DCL NF_SIZ REDEF SEG_SIZ UBIN(36);
       46      749    1   DCL SDS_SIZ REDEF SEG_SIZ UBIN(36);
       47      750    1   DCL NFE_SIZ REDEF SEG_SIZ UBIN(36);
       48      751        /****************EXT DCLS**************/
       49      752    1   DCL TNAM2$ PTR EXT;
       50      753    1   DCL HSH_VAL UBIN(36) ALIGNED EXT;
       51      754    1   DCL STK_IDX UBIN(36) EXT;
       52      755    1   DCL 1 LVL_STK EXT,
       53      756    1         2 * (0:49) UBIN(36) UNAL;
       54      757    1   DCL REC$ PTR EXT;
       55      758    1   DCL I_O_BUF (0:1) CHAR(128) EXT; /*Read into this buffer 1 blk at a time*/
       56      759    1   DCL I_O_IDX UBIN(36) EXT;
       57      760    1   DCL PRC_CAL UBIN(36) EXT;
       58      761    1   DCL ERROR_CODE UBIN(36) EXT;
       59      762    1   DCL SUBSCH_CON_BLK (0:63) UBIN(36) EXT;
       60      763    1   DCL RVA_SIZ UBIN(36) EXT;
       61      764    1   DCL IA_SIZ UBIN(36) EXT;
       62      765    1   DCL SUB_CONBLK$ PTR EXT;
       63      766    1   DCL 1 SUBSCH EXT,
       64      767    1         2 NAME ,
       65      768    1           3 CNT UBIN(9) UNAL,
       66      769    1           3 TEXT CHAR(32) UNAL,
       67      770    1         2 DCB# UBIN(18) ALIGNED;
       68      771    1   DCL ZI$ENTRY_COUNT UBIN(36) EXTROOT;
       69      772    1   DCL ZI$KEY_NUMBER UBIN(36) EXTROOT;
       70      773    1   DCL S_SW UBIN WORD EXTROOT;
       71      774    1   DCL R_SW UBIN WORD EXTROOT;
       72      775    1   DCL A_SW UBIN WORD EXTROOT;
       73      776    1   DCL K_SW UBIN WORD EXTROOT;
       74      777        /*****************ENTRY DCLS***************/
PL6.E3A0      #001=ZII$BLDNAM File=ZII$BLDNAM.:ZIC0TSI                           FRI 09/05/97 12:25 Page:3    
       75      778    1   DCL ZII$ADD_KEYWORDS ENTRY ALTRET;
       76      779    1   DCL ZII$BLD_NAM ENTRY ALTRET;
       77      780    1   DCL ZII$BLD_IA ENTRY ALTRET;
       78      781    1   DCL ZII$READ_SS ENTRY ALTRET;
       79      782    1   DCL ZII$ADD_NAM ENTRY ALTRET;
       80      783    1   DCL ZII$FDDESC ENTRY(5) ALTRET;
       81      784    1   DCL ZII$WRT_COMP_REC ENTRY ALTRET;
       82      785        /*****************INCLUDE FILES*************/
       83      786        %INCLUDE ZI$STRUCTS;
       84      858        %UWA1;
       85      881        %NAM1;
       86      910        %NAM2;
       87      924        %INCLUDE ZI_SUB_LANG;
       88     2639        %CONTROL_BLOCK (STCLASS="BASED(SUB_CONBLK$)");
       89     2747        %DESC_TYPE_9 (STCLASS="BASED(REC$)");
       90     2924        %INCLUDE ZI$DESC;
       91     3774        %DSC_AREA (STCLASS="EXT");
       92     3796        %DSC_REC (STCLASS="EXT");
       93     3830        %DSC_FLD (STCLASS="EXT");
