      298     5191    2           END;
      299     5192    1       CALL ZII$BLD_IA ALTRET(ER_BLD_IA); /*Go build the information area */
      300     5193    1       IF PRC_CAL = #APL THEN CALL ZII$ADD_KEYWORDS ALTRET(ER_ADD_KEYWORDS);
      301     5194    1       CLOSE_FPT.V.DCB#=SUBSCH.DCB#;
      302     5195    1       CALL M$CLOSE (CLOSE_FPT) ALTRET(ER_CLOSE);
      303     5196    1       CALL ZII$WRT_COMP_REC ALTRET(ER_WRT_COMP_REC);
      304     5197    1   RET:
