        1        1        /*M*     IDSSET    TO ALLOW A SET TO BE DONE AT RUN TIME */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8        NAME:    IDSSET
        9        9        PURPOSE: TO ALLOW THE COBOL AND FORTRAN USER TO DO A SET
       10       10                 AT RUN TIME.
       11       11                 THE PROGRAM IS CALLED AND PASSED TWO PARAMETERS
       12       12                 THE FIRST PARAMETER:
       13       13                      IS A STRING OF 255 CHARACTERS THAT HAS THE SET
       14       14                      CMD IN THE STRING.
       15       15                      EXAMPLE. ' !SET M$LO LP '
       16       16                      THE STRING CONTAINS THREE FIELDS:
       17       17                           1. THE STRING '!SET' OR 'SET'
       18       18                           2. THE DCB TO BE SET
       19       19                           3. THE FID THAT THE DCB IS TO BE SET TO.
       20       20                      LIMITS:
       21       21                           1. EACH FIELD MUST BE TERMINATED BY AT LEAST
       22       22                                ONE BLANK.
       23       23                           2. NO FIELD CAN CONTAIN A BLANK.
       24       24                      NOTE:
       25       25                           1. THE THIRD FIELD CAN BE TERMINATED BY A '. '
       26       26                                AND THE '.' WILL BE REPLACED BY A ' '.
       27       27                           2. THE FIRST ARG. WILL NOT BE CHANGED.
       28       28                           3. ONLY SET CMDS ARE ALLOWED.
       29       29                 SECOND PARAMETER:
       30       30                      IS A UBIN (36) (ONE WORD) ERROR CODE.
       31       31                      THIS IS A ERROR CODE THAT IS RETURNED TO THE CALLING
       32       32                      CALLING PROGRAM.
       33       33                      NOTE:
       34       34                           1. IF ERROR CODE EQUALS ZERO THEN THE SET CMD
       35       35                                WAS PROCESSED.
       36       36                           2. IF ERROR CODE DOESN'T EQUAL ZERO THEN THE
       37       37                                SET CMD WASN'T PROCESSED AND THE SYSTEM
PL6.E3A0      #001=IDSSET File=IDSSET.:ZIC0TSI                                   FRI 09/05/97 11:32 Page:2    
       38       38                                ERROR CODE IS RETURNED.
       39       39        */
       40       40        IDSSET:
       41       41            PROC (SET_CMD,ERROR_CODE);
       42       42    1   DCL SET_CMD CHAR (256);
       43       43    1   DCL ERROR_CODE UBIN (36);
       44       44    1   DCL WORD_ UBIN (36) BASED;
       45       45    1   DCL B$TCB$ PTR SYMREF;
       46       46    1   DCL 1 SET_CMD_S CHAR (256) AUTO;
       47       47    1   DCL I UBIN (36) AUTO;
       48       48    1   DCL START UBIN (36) AUTO;
       49       49         %INCLUDE CP_6;
       50      130         %INCLUDE CP_6_SUBS;
       51      670         %FPT_YC;
       52      687         %B$TCB;
       53      690         %B$EXCFR;
       54      708         %EJECT;
PL6.E3A0      #001=IDSSET File=IDSSET.:ZIC0TSI                                   FRI 09/05/97 11:32 Page:3    
       55      709    1       FPT_YC.CMD_ = VECTOR(SET_CMD_S);
       56      710    1       I=0;
       57      711    1       CALL SKIP_BLANKS;
       58      712    1       IF SUBSTR(SET_CMD,I,1)='!'
       59      713    2       THEN DO;
       60      714    2           I=I+1;
       61      715    2           CALL SKIP_BLANKS;
       62      716    2           END;
       63      717    1       START=I;
       64      718    1       IF SUBSTR(SET_CMD,I,4)='SET '
       65      719    2       THEN DO;
       66      720    2           I=I+4;
       67      721    2           CALL SKIP_BLANKS;
       68      722    2           CALL NEXT_BLANK;
       69      723    2           CALL SKIP_BLANKS;
       70      724    2           CALL NEXT_BLANK;
       71      725    2           IF SUBSTR(SET_CMD,I-1,1)='.' THEN I=I-2;
       72      726    2           SET_CMD_S=SUBSTR(SET_CMD,START,I-START);
       73      727    2           END;
       74      728    2       ELSE DO;
       75      729    2           SET_CMD_S=' ';
       76      730    2           END;
       77      731    1       CALL M$YC (FPT_YC) ALTRET (ERROR_);
       78      732    1       ERROR_CODE=0;
       79      733    1       RETURN;
       80      734    1   ERROR_:
       81      735    1       ERROR_CODE=ADDR(B$TCB$->B$TCB.ALT$->B$EXCFR.ERR.FCG)->WORD_;
       82      736    1       RETURN;
       83      737         %EJECT;
PL6.E3A0      #001=IDSSET File=IDSSET.:ZIC0TSI                                   FRI 09/05/97 11:32 Page:4    
       84      738    1   SKIP_BLANKS:
       85      739    1       PROC;
       86      740    3       DO WHILE (SUBSTR(SET_CMD,I,1)=' '&I<255);
       87      741    3           I=I+1;
       88      742    3           END;
       89      743    2       RETURN;
       90      744    2   END SKIP_BLANKS;
       91      745    1   NEXT_BLANK:
       92      746    1       PROC;
       93      747    3       DO WHILE (SUBSTR(SET_CMD,I,1)~=' '&I<255);
       94      748    3           I=I+1;
       95      749    4           IF SUBSTR (SET_CMD, I-1, 1) = '''' THEN DO;
       96      750    4               CALL NEXT_QUOTE;
       97      751    4               RETURN;
       98      752    4               END;
       99      753    3           END;
      100      754    2       RETURN;
      101      755    2   END NEXT_BLANK;
      102      756    1   NEXT_QUOTE: PROC;
      103      757    3       DO WHILE (SUBSTR (SET_CMD, I, 1) ~= '''' & I<255);
      104      758    3           I = I + 1;
      105      759    3           END;
      106      760    2       I = I + 1;
      107      761    2       RETURN;
      108      762    2   END NEXT_QUOTE;
      109      763    1   END IDSSET;
      110      764        %EOD;

