%MACRO ZI$UWAB;
DCL 1    UWA SYMREF,
      2 WRD1        SBIN (36)  UNAL,
      2 STAT_CODE    CHAR (2)   UNAL,
      2 DB_STATUS    CHAR (5)   UNAL,
      2 *            CHAR (1)   UNAL,
      2 DB_AREA_NAME    CHAR (30)  UNAL,
      2 *            UBIN (18)  UNAL,
      2 DB_SET_NAME     CHAR (30)  UNAL,
      2 *            UBIN (18)  UNAL,
      2 DB_RECORD_NAME  CHAR (30)  UNAL,
      2 *            UBIN (18)  UNAL,
      2 DB_PRIV_KEY  CHAR (12)  UNAL,
      2 DIR_REF   UBIN (36)  UNAL,
      2 DB_DATA_NAME    CHAR (30)  UNAL,
      2 *            UBIN (18)  UNAL,
      2 DB_KEY_NAME     CHAR (30)  UNAL,
      2 *            UBIN (18)  UNAL,
      2 *            UBIN (36)  UNAL,
      2 SF_REC DALIGNED,
        3 RLSE CHAR (5)  UNAL,
        3 DBKSA UBIN (36)  ALIGNED,
        3 DBKOA UBIN (36)  ALIGNED,
        3 DBKDA UBIN (36)  ALIGNED,
        3 DBKTA UBIN (36)  ALIGNED,
      2 AS_REC DALIGNED,
        3 SS_NAME CHAR (30)  UNAL,
        3 SS_TRAN CHAR (12)  UNAL,
        3 SS_LANG SBIN (36)   ALIGNED,
        3 SS_DATE CHAR (4)  UNAL,
        3 SS_TIME CHAR (4)  UNAL,
      2 PG_REC DALIGNED,
        3 PROG_NAME CHAR (30)  UNAL,
        3 AUTH CHAR (30)  UNAL,
        3 COMP_DATE CHAR (12)  UNAL,
      2 CA_REC DALIGNED,
        3 OWNER_TYPE SBIN (36)   ALIGNED,
        3 OWNER_NAME CHAR (30)  UNAL,
        3 CAT_FILE SBIN (36)   ALIGNED,
        3 CFNAME CHAR (12)  UNAL,
        3 PWORD CHAR (12)  UNAL,
      2 SI_REC DALIGNED,
        3 LOCKED SBIN (36)   ALIGNED,
        3 BINDCD SBIN (36)   ALIGNED,
        3 LI_NE SBIN (36)   ALIGNED,
        3 EXCLUDE SBIN (36)   ALIGNED,
        3 SETYPE SBIN (36)   ALIGNED,
        3 A_LIAS SBIN (36)   ALIGNED,
      2 AL_REC DALIGNED,
        3 ANAME CHAR (30)  UNAL,
