FRI. SEP 05 '97 11:24:47.72  FILE=ZI_UWA_C

%MACRO ZI_UWAS;
DCL 1    UWA STATIC SYMDEF,
      2 WRD1        SBIN (36) INIT (0)   UNAL,
      2 STAT_CODE    CHAR (2)  INIT (' ') UNAL,
      2 DB_STATUS    CHAR (5)  INIT (' ') UNAL,
      2 *            CHAR (1)  INIT (' ') UNAL,
      2 DB_AREA_NAME    CHAR (30) INIT (' ') UNAL,
      2 *            UBIN (18) INIT (0)   UNAL,
      2 DB_SET_NAME     CHAR (30) INIT (' ') UNAL,
      2 *            UBIN (18) INIT (0)   UNAL,
      2 DB_RECORD_NAME  CHAR (30) INIT (' ') UNAL,
      2 *            UBIN (18) INIT (0)   UNAL,
      2 DB_PRIV_KEY  CHAR (12) INIT (' ') UNAL,
      2 DIR_REF   UBIN (36) INIT (0)   UNAL,
      2 DB_DATA_NAME    CHAR (30) INIT (' ') UNAL,
      2 *            UBIN (18) INIT (0)   UNAL,
      2 DB_KEY_NAME     CHAR (30) INIT (' ') UNAL,
      2 *            UBIN (18) INIT (0)   UNAL,
      2 *            UBIN (36) INIT (0)   UNAL,
      2 SF_REC DALIGNED,
        3 RLSE CHAR (5) INIT (' ') UNAL,
        3 DBKSA UBIN (36) INIT (0) ALIGNED,
        3 DBKOA UBIN (36) INIT (0) ALIGNED,
        3 DBKDA UBIN (36) INIT (0) ALIGNED,
        3 DBKTA UBIN (36) INIT (0) ALIGNED,
      2 AS_REC DALIGNED,
        3 SS_NAME CHAR (30) INIT (' ') UNAL,
        3 SS_TRAN CHAR (12) INIT (' ') UNAL,
        3 SS_LANG SBIN (36)  INIT (0) ALIGNED,
        3 SS_DATE CHAR (4) INIT (' ') UNAL,
        3 SS_TIME CHAR (4) INIT (' ') UNAL,
      2 PG_REC DALIGNED,
        3 PROG_NAME CHAR (30) INIT (' ') UNAL,
        3 AUTH CHAR (30) INIT (' ') UNAL,
        3 COMP_DATE CHAR (12) INIT (' ') UNAL,
      2 CA_REC DALIGNED,
        3 OWNER_TYPE SBIN (36)  INIT (0) ALIGNED,
        3 OWNER_NAME CHAR (30) INIT (' ') UNAL,
        3 CAT_FILE SBIN (36)  INIT (0) ALIGNED,
        3 CFNAME CHAR (12) INIT (' ') UNAL,
        3 PWORD CHAR (12) INIT (' ') UNAL,
      2 SI_REC DALIGNED,
        3 LOCKED SBIN (36)  INIT (0) ALIGNED,
        3 BINDCD SBIN (36)  INIT (0) ALIGNED,
        3 LI_NE SBIN (36)  INIT (0) ALIGNED,
        3 EXCLUDE SBIN (36)  INIT (0) ALIGNED,
        3 SETYPE SBIN (36)  INIT (0) ALIGNED,
        3 A_LIAS SBIN (36)  INIT (0) ALIGNED,
      2 AL_REC DALIGNED,
        3 ANAME CHAR (30) INIT (' ') UNAL,
        3 DBKEY_SI SBIN (36)  INIT (0) ALIGNED,
      2 VB_REC DALIGNED,
        3 VBTYPE SBIN (36)  INIT (0) ALIGNED,
        3 LNO SBIN (36)  INIT (0) ALIGNED,
      2 AD_REC DALIGNED,
        3 ADFUNC SBIN (36)  INIT (0) ALIGNED,
        3 ADDATE (0:2) SBIN(36) INIT(0*3) UNAL,
        3 ADDATE_RD REDEF ADDATE CHAR(12),
        3 AD_IDENT CHAR (60) INIT (' ') UNAL,
      2 OA_REC DALIGNED,
        3 NUMB_AC SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_RC SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_SC SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_RA SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_FR SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_SO SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_SM SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_SS SBIN (36)  INIT (0) ALIGNED,
        3 NUMB_CF SBIN (36)  INIT (0) ALIGNED,
        3 CVOEX SBIN (36)  INIT (0) ALIGNED,
        3 GLBL SBIN (36)  INIT (0) ALIGNED,
        3 FILE_CODE CHAR (2) INIT (' ') UNAL,
        3 OA_CALC UBIN (36) INIT (0) ALIGNED,
      2 AC_REC DALIGNED,
        3 DEFAULTS SBIN (36)  INIT (0) ALIGNED,
        3 FLAG1 REDEF DEFAULTS,
          4 ERROR_TYPE (0:26) BIT (1) UNAL,
          4 ERROR_NUM REDEF ERROR_TYPE UBIN (27) UNAL,
          4 STATUS  UBIN (9) UNAL,
        3 FILE_CODE CHAR (2) INIT (' ') UNAL,
        3 SWS REDEF FILE_CODE,
          4 CHECKSUM BIT (1) UNAL,
          4 ENCRYP  BIT (1) UNAL,
          4 CJRNL  BIT (1) UNAL,
          4 TEMPLAT  BIT (1) UNAL,
          4 TJRNL BIT (1) UNAL,
          4 *      BIT (13) UNAL,
        3 INDEX_CODE CHAR (2) INIT (' ') UNAL,
        3 PRIME_KEY SBIN (36)  INIT (0) ALIGNED,
        3 SEC_KEY SBIN (36)  INIT (0) ALIGNED,
        3 PERMTYPE SBIN (36)  INIT (0) ALIGNED,
        3 PG_TYPE SBIN (36)  INIT (0) ALIGNED,
        3 P_TYP REDEF PG_TYPE UNAL,
          4 * BIT(34) UNAL,
          4 P_MD BIT(2) UNAL,
        3 PER_INV SBIN (36)  INIT (0) ALIGNED,
        3 CALC_NULL SBIN (36)  INIT (0) ALIGNED,
        3 BASE_DBK SBIN (36)  INIT (0) ALIGNED,
        3 KEY_ALLOC SBIN (36)  INIT (0) ALIGNED,
        3 KEYS_RESER SBIN (36)  INIT (0) ALIGNED,
        3 MIN_DBK SBIN (36)  INIT (0) ALIGNED,
        3 MAX_DBK SBIN (36)  INIT (0) ALIGNED,
        3 MIN_PAGE SBIN (36)  INIT (0) ALIGNED,
        3 MAX_PAGE SBIN (36)  INIT (0) ALIGNED,
        3 CALC_HDRS SBIN (36)  INIT (0) ALIGNED,
        3 FIR_INV_PG SBIN (36)  INIT (0) ALIGNED,
        3 LPP SBIN (36)  INIT (0) ALIGNED,
        3 IDB SBIN (36)  INIT (0) ALIGNED,
        3 LINE_BITS SBIN (36)  INIT (0) ALIGNED,
        3 INV_THES SBIN (36)  INIT (0) ALIGNED,
        3 LOAD_LIMIT SBIN (36)  INIT (0) ALIGNED,
        3 PGS_INV_PG SBIN (36)  INIT (0) ALIGNED,
        3 INV_UNIT SBIN (36)  INIT (0) ALIGNED,
        3 PG_SIZE SBIN (36)  INIT (0) ALIGNED,
        3 SP_AVAIL SBIN (36)  INIT (0) ALIGNED,
        3 SECTORS_PAGE SBIN (36)  INIT (0) ALIGNED,
        3 CALC_NEXT SBIN (36)  INIT (0) ALIGNED,
        3 CALC_NEXT_STRUCT REDEF CALC_NEXT ALIGNED,
          4 EIS,                        /* Eis descriptor                     */
            5 REG UBIN (3) UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
            5 TA UBIN (2) UNAL,
            5 * BIT (1) UNAL,
            5 COUNT UBIN (12) UNAL,
        3 CALC_PRIOR SBIN (36)  INIT (0) ALIGNED,
        3 CALC_PRIOR_STRUCT REDEF CALC_PRIOR ALIGNED,
          4 EIS,                        /* Eis descriptor                     */
            5 REG UBIN (3) UNAL,
            5 WD UBIN (15) UNAL,   /* assumes word offset is l.e. 2**15-1     */
            5 CHR UBIN (3) UNAL,
            5 TA UBIN (2) UNAL,
            5 * BIT (1) UNAL,
            5 COUNT UBIN (12) UNAL,
      2 RC_REC DALIGNED,
        3 RTYPE SBIN (36)  INIT (0) ALIGNED,
        3 PSMODE SBIN (36)  INIT (0) ALIGNED,
        3 FIXVAR SBIN (36)  INIT (0) ALIGNED,
        3 WITAIN SBIN (36)  INIT (0) ALIGNED,
        3 SO SBIN (36)  INIT (0) ALIGNED,
        3 SM SBIN (36)  INIT (0) ALIGNED,
        3 CYCLE SBIN (36)  INIT (0) ALIGNED,
        3 CALCDUP SBIN (36)  INIT (0) ALIGNED,
        3 ANY_IND SBIN (36)  INIT (0) ALIGNED,
        3 KEY_PRESENT SBIN (36)  INIT (0) ALIGNED,
        3 CV_OFFSET SBIN (36)  INIT (0) ALIGNED,
        3 PA_OFFSET SBIN (36)  INIT (0) ALIGNED,
        3 PA_ENTRY SBIN (36)  INIT (0) ALIGNED,
        3 NUM_SOSM SBIN (36)  INIT (0) ALIGNED,
        3 LSIZE SBIN (36)  INIT (0) ALIGNED,
        3 SLACK SBIN (36)  INIT (0) ALIGNED,
      2 SC_REC DALIGNED,
        3 SETTYPE SBIN (36)  INIT (0) ALIGNED,
        3 SETMODE SBIN (36)  INIT (0) ALIGNED,
        3 SETORD SBIN (36)  INIT (0) ALIGNED,
        3 SETGLO SBIN (36)  INIT (0) ALIGNED,
        3 SETPTR SBIN (36)  INIT (0) ALIGNED,
        3 PTRNO SBIN (36)  INIT (0) ALIGNED,
        3 ALTOWN SBIN (36)  INIT (0) ALIGNED,
        3 RECSET SBIN (36)  INIT (0) ALIGNED,
        3 SEQDUP SBIN (36)  INIT (0) ALIGNED,
        3 PTRSZ SBIN (36)  INIT (0) ALIGNED,
        3 USER_PTS SBIN (36)  INIT (0) ALIGNED,
        3 IXID SBIN (36)  INIT (0) ALIGNED,
        3 KEY_LENGHT SBIN (36)  INIT (0) ALIGNED,
      2 RA_REC DALIGNED,
        3 LDBK SBIN (36)  INIT (0) ALIGNED,
