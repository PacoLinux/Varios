      205     4364    1   IASSCHK: PROC ALTRET;
      206     4365    2       SSS$=GLOBAL.ACTIVE.SSS$;
      207     4366    2       OA$=PINCRW(SSS$,SIZEW(OA1));
      208     4367    3       DO CASE(IA.EC);
      209     4368    3       CASE(%EC_FORTRAN#);
      210     4369    3           GLOBAL.STYPE=%FORTRAN_IDS#;
      211     4370    3       CASE(%EC_APL#);
      212     4371    3           GLOBAL.STYPE=%APL_IDS#;
      213     4372    3       CASE(%EC_BASIC#);
      214     4373    3           GLOBAL.STYPE=%BASIC_IDS#;
      215     4374    3       CASE(%EC_IDP#);
      216     4375    3           GLOBAL.STYPE=%IDP_IDS#;
      217     4376    3       CASE(ELSE);
      218     4377    3           GLOBAL.STYPE='0'B;
      219     4378    3           END;
