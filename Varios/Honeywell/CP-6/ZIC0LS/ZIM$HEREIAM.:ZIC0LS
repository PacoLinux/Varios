      221     4380    2           CALL M$MONINFO(GLOBAL.MONINFO$->FPT$MONINFO) ALTRET(ER_NPAID);
      222     4381    2       IF SUBSTR(PINCRW(OA$,SIZEW(OA))->SSNAME,0,
      223     4382    2          OA.SSNAM.EIS.COUNT)~=IA.SSNAME THEN
      224     4383    2           GOTO ER_NAME;
      225     4384    2       IF GLOBAL.DB.FLAGS.SYSSUB(GLOBAL.ACTIVE.DBNO)=%NO# THEN
      226     4385    3       DO;
      227     4386    3           IF OA.SNAM~=IA.SNAME THEN
      228     4387    3               GOTO ER_NAME;
      229     4388    3           IF ~GLOBAL.DB.FLAGS.NOTRANS(GLOBAL.ACTIVE.DBNO) AND
      230     4389    3              OA.SSDT~=IA.SSDT THEN
      231     4390    3               GOTO ER_DATE;
      232     4391    3           END;
      233     4392    2       RETURN;
      234     4393    2   ER_NPAID: ERROR=ERR#CM#NPAID;
      235     4394    2       GOTO ALT;
      236     4395    2   ER_NAME: ERROR=ERR#CM#INNM;
      237     4396    2       GOTO ALT;
      238     4397    2   ER_DATE: ERROR=ERR#CM#INSS;
      239     4398    2   ALT: ;
