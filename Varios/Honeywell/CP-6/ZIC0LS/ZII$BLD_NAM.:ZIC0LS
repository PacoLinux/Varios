       55     4372    1       NAM1_IDX=NAM1_IDX+1; /*Increment Nam1 index*/
       56     4373    1       NAM1.NAME.CNT(NAM1_IDX)=DESC_TYPE_9.NAME_SIZE;/*Get name size*/
       57     4374    1       NAM1.NAME.TEXT(NAM1_IDX)=DESC_TYPE_9.NAME;/*Get name*/
       58     4375    1       IF DESC_TYPE_9.RLM_NM THEN NAM1.TYPE(NAM1_IDX)=#AREA;
       59     4376    1       ELSE IF DESC_TYPE_9.SET_NM THEN NAM1.TYPE(NAM1_IDX)=#SET;
       60     4377    1           ELSE IF DESC_TYPE_9.REC_NM THEN NAM1.TYPE(NAM1_IDX)=#RECORD;
       61     4378    1               ELSE IF DESC_TYPE_9.SRF_DBK AND
       62     4379    1                      DESC_TYPE_9.KEY_NM THEN NAM1.TYPE(NAM1_IDX)=#KEY;
       63     4380    1                   ELSE IF DESC_TYPE_9.FLD_NM THEN NAM1.TYPE(NAM1_IDX)=#FIELD;
       64     4381    1       CALL ZII$FDDESC(NAM1.NAME.TEXT(NAM1_IDX),NAM1.TYPE(NAM1_IDX),DD$,#STORE,) ALTRET
              4381                (ER_FDDESC);
       65     4382    1       NAM1.DESC$(NAM1_IDX)=DD$; /*Set descriptor pointer in NAM1*/
       66     4383    2       DO CASE(NAM1.TYPE(NAM1_IDX));
       67     4384    2       CASE(#AREA);
       68     4385    2           DA$=DD$; /*Set up the area descriptor pointer*/
       69     4386    2           AREA.LREC$=ADDR(NIL); /*Init the record link list ptr*/
       70     4387    2           AREA.LSET$=ADDR(NIL); /*Init the set link list ptr*/
       71     4388    2           AREA.LAREA$=ADDR(NIL); /*Init the area link list ptr*/
       72     4389    2           AREA.RA_DSC$=ADDR(NIL); /*Init the rec_area link list ptr*/
       73     4390    2           IF A_SW = 0 THEN /*First time through this case*/
PL6.E3A0      #001=ZII$BLD_NAM File=ZII$BLD_NAM.:ZIC0TSI                         FRI 09/05/97 12:26 Page:3    
       74     4391    3           DO;
       75     4392    3               TLA$=DA$;
       76     4393    3               THA$=DA$;
       77     4394    3               A_SW=1;
       78     4395    3               END;
       79     4396    3           ELSE DO;
       80     4397    3               TLA$->AREA.LAREA$=DA$;
       81     4398    3               TLA$=DA$;
       82     4399    3               END;
       83     4400    2           AREA.NAM_IDX=NAM1_IDX; /*Put the NAM1 offset into the NAM2 descriptor*/
       84     4401    2           CALL ZII$GET_RVA(#AREA);
              4401                    /*Get the rva offset for the element relative to base 0 */
       85     4402    2           AREA.RVA_OFF=RVE_IDX;
       86     4403    2       CASE(#RECORD);
       87     4404    2           DR$=DD$; /*Set up the pointer to the record descriptor*/
       88     4405    2           RECORD.RA_DSC$=ADDR(NIL); /*Init rec_area pointer*/
       89     4406    2           RECORD.LREC$=ADDR(NIL); /*Init  record link list pointer*/
       90     4407    2           RECORD.RFK_DSC$=ADDR(NIL); /*Init Rec/Fld/Key ptr*/
       91     4408    2           RECORD.RFS_DSC$=ADDR(NIL); /*Init Rec/Fld/Set ptr*/
       92     4409    2           RECORD.RP_DSC$=ADDR(NIL); /* To init Rec/Param Pointer. */
       93     4410    2           IF R_SW = 0 THEN
       94     4411    3           DO;
       95     4412    3               THA$->AREA.LREC$=DR$;
       96     4413    3               TLR$=DR$;
       97     4414    3               R_SW=1;
       98     4415    3               END;
       99     4416    3           ELSE DO;
      100     4417    3               TLR$->RECORD.LREC$=DR$;
      101     4418    3               TLR$=DR$;
      102     4419    3               END;
      103     4420    2           RECORD.NAM_IDX=NAM1_IDX; /*Get the NAM1 index for the element*/
      104     4421    2           CALL ZII$GET_RVA(#RECORD); /*Get the rva offset*/
      105     4422    2           RECORD.RVA_OFF=RVE_IDX;
      106     4423    2           RECORD.UWA_OFF=DESC_TYPE_9.OFFSET; /*Get the uwa byte offset*/
      107     4424    2           STK_IDX=0;
      108     4425    2           LVL_STK='0'B;
      109     4426    2       CASE(#FIELD);
PL6.E3A0      #001=ZII$BLD_NAM File=ZII$BLD_NAM.:ZIC0TSI                         FRI 09/05/97 12:26 Page:4    
      110     4427    2           DF$=DD$; /*Set up the pointer for the field descriptor*/
      111     4428    2           FIELD.NAM_IDX=NAM1_IDX;
              4428                    /*Get the NAm1 index and put into the NAM2 descriptor*/
      112     4429    2           FIELD.REC_ASSC_IDX=RECORD.NAM_IDX;
      113     4430    2           FIELD.SYN_NAM=0;
      114     4431    2           FIELD.RFK_DSC$=ADDR(NIL);
      115     4432    2           FIELD.RFS_DSC$=ADDR(NIL);
      116     4433    2           IF DESC_TYPE_9.COMP_4 OR DESC_TYPE_9.COMP OR
      117     4434    2              (DESC_TYPE_9.COMP_3 AND ~DESC_TYPE_9.COMP5A) THEN
      118     4435    3           DO; /*Determine if the uwa offset is in bytes or nibbles*/
      119     4436    3               FIELD.UWA_OFF=DESC_TYPE_9.OFFSET;
      120     4437    3               FIELD.FLAGS_2.BYT_NIB='1'B;
      121     4438    3               END;
      122     4439    3           ELSE DO;
