      161     4478    2       CASE(#SET);
      162     4479    2           DS$=DD$; /*Set up the set descriptor pointer*/
      163     4480    2           SET.LSET$=ADDR(NIL); /*Init set link list ptr*/
      164     4481    2           SET.RFS_DSC$=ADDR(NIL); /*Init rec_set ptr*/
      165     4482    2           IF S_SW = 0 THEN
      166     4483    3           DO;
      167     4484    3               THA$->AREA.LSET$=DS$;
      168     4485    3               TLS$=DS$;
      169     4486    3               S_SW=1;
      170     4487    3               END;
      171     4488    3           ELSE DO;
      172     4489    3               TLS$->SET.LSET$=DS$;
      173     4490    3               TLS$=DS$;
      174     4491    3               END;
      175     4492    2           SET.NAM_IDX=NAM1_IDX; /*Get the NAM1 index for the NAM2 descriptor*/
      176     4493    2           CALL ZII$GET_RVA(#SET); /*Get the rva offset*/
      177     4494    2           SET.RVA_OFF=RVE_IDX;
