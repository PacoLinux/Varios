PL6.E3A0      #008=WRITE_LOCAL_CONTEXT File=LR_ERRPTH.:Z8A6TSI                   SAT 08/09/97 09:30 Page:207  
      557      866    1   INSERT_PATH_STRING : PROC;  /* IN WRITE_LOCAL_CONTEXT */
      558      867
      559      868        /* The string has been temporarily created at the end of
      560      869           the string space.  Search for a redundant string before expanding
      561      870           the space to actually include the string.
      562      871        */
      563      872
      564      873    2   IF PROPOSED_PATH_TABLE_LEN >= %PATH_LEN_MAX THEN
      565      874    2     CALL DIAG(15,
      566      875    2   'Path list space has been exhausted during error table generation%%');
      567      876
      568      877    2   COM_SEARCH_STRING_LEN = (EXP_PATH_INDX*2 - COM_STRSP_LEN);
      569      878    2   COM_SEARCH_STRING = ADDR(COM_STRING_SPACE_H(COM_STRSP_LEN/2));
      570      879
      571      880    2   I = 0;
      572      881    3   DO UNTIL (MOD(I,2) ~= 0);
      573      882
      574      883    3     CALL INDEX (COM_STRING_INDX,
      575      884    3                 CSEARCH_STRING,
      576      885    3                 SUBSTR(COM_STRINGS, I, COM_STRSP_LEN - I));
      577      886    3     I = I + COM_STRING_INDX + 1;
      578      887    3   END;
      579      888
      580      889    2   COM_STRING_INDX = I - 1;
      581      890
      582      891    2   IF COM_STRING_INDX = COM_STRSP_LEN THEN
      583      892    2     COM_STRSP_LEN = EXP_PATH_INDX * 2;
      584      893
      585      894    2   PATH.INDX(PROPOSED_PATH_TABLE_LEN) = COM_STRING_INDX/2;
      586      895    2   PATH.LEN(PROPOSED_PATH_TABLE_LEN) = COM_SEARCH_STRING_LEN / 2;
      587      896    2   PROPOSED_PATH_TABLE_LEN = PROPOSED_PATH_TABLE_LEN + 1;
      588      897
      589      898    2   END INSERT_PATH_STRING;
      590      899
      591      900
      592      901
      593      902        %EJECT;
PL6.E3A0      #008=WRITE_LOCAL_CONTEXT File=LR_ERRPTH.:Z8A6TSI                   SAT 08/09/97 09:30 Page:208  
      594      903    1   INSERT_NULL_PATH : PROC;  /* IN WRITE_LOCAL_CONTEXT */
      595      904
      596      905        /* Insert a null string into the path table
      597      906        */
      598      907
      599      908    2   IF PROPOSED_PATH_TABLE_LEN >= %PATH_LEN_MAX THEN
      600      909    2     CALL DIAG(15,
      601      910    2   'Path list space has been exhausted during error table generation%%');
      602      911
      603      912    2   PATH.INDX(PROPOSED_PATH_TABLE_LEN) = -1;
      604      913    2   PATH.LEN (PROPOSED_PATH_TABLE_LEN) = 0;
