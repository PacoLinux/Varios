      400      976
      401      977              %OUT_STR(S="',I='");
      402      980    3       END;
      403      981
      404      982    2     CALL WRITE_LOCAL_CONTEXT(STATE, SOURCE);
      405      983    2   END;  /* DO SOURCE */
      406      984
      407      985    1   STATE_ITEM_INDEX.CLOSURE(STATE) = ITEM_TABLE_LEN;
      408      986
      409      987
