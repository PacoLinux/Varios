       58        6        ORDER_ITEMS : PROC (STATE) NOAUTO;
       59        7
       60        8        /*D*
       61        9               NAME: ORDER_ITEMS
       62       10            PURPOSE: ORDER THE ITEMS IN EACH STATE FOR EASY ACCESS
       63       11                     TO ANY SPECIFIC ITEM.
       64       12        */
       65       13
       66       14
       67       15
       68       16    1   DCL STATE SBIN WORD;
       69       17
       70       18        %INCLUDE LR_GRAM_EXT;
       71      557        %INCLUDE LR_CNFG_EXT;
       72      710        %INCLUDE LR_ERR_EXT;
       73      853
       74      854    1   DCL LIMCHK ENTRY(4);
       75      855
       76      856    1   DCL E    SBIN WORD;
       77      857    1   DCL I    SBIN WORD;
       78      858    1   DCL ITEM SBIN WORD;
       79      859
       80      860
       81      861    2   DO E = 0 TO C#_TO_$(STATE) -> C_SET_B.NR_ELEM - 1;
       82      862
       83      863    3     DO I = 0 TO C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.W.ELEM_COUNT - 1;
       84      864
       85      865    3       ITEM = C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(E) -> ITEM_B.ELEM.ITEM#(I);
       86      866
       87      867    3       CALL LIMCHK (15, ITEM, %NR_ELEM_MAX,
       88      868    3         'The maximum size of a configuration set has been exceeded: %%');
       89      869
PL6.E3A0      #002=ORDER_ITEMS File=LR_ERRPTH.:Z8A6TSI                           SAT 08/09/97 09:27 Page:21   
       90      870    3       ORDERED_ITEM.I(ITEM) = I;
       91      871    3       ORDERED_ITEM.E(ITEM) = E;
       92      872
       93      873    3     END;
       94      874    2   END;
       95      875
       96      876    1   END ORDER_ITEMS;
       97      877        %EOD;

