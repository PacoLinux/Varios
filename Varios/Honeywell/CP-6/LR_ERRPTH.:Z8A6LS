PL6.E3A0      #006=EXPAND_PATH File=LR_ERRPTH.:Z8A6TSI                           SAT 08/09/97 09:29 Page:153  
      412        1        /*T***********************************************************/
      413        2        /*T*                                                         */
      414        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      415        4        /*T*                                                         */
      416        5        /*T***********************************************************/
      417        6        EXPAND_PATH : PROC(STATE, ITEM, SOURCE);
      418        7
      419        8        /*D*
      420        9               NAME: EXPAND_PATH
      421       10            PURPOSE: EXPAND THE PATH IN THE STATE'S CLOSURE GRAPH
      422       11                     FROM THE SOURCE ITEM TO THE DESTINATION ITEM.
      423       12        */
      424       13
      425       14
      426       15    1   DCL STATE  SBIN WORD;
      427       16    1   DCL ITEM   SBIN WORD;
      428       17    1   DCL SOURCE SBIN WORD;
      429       18
      430       19        %INCLUDE LR_GRAM_EXT;
      431      558        %INCLUDE LR_CNFG_EXT;
      432      711        %INCLUDE LR_ERR_EXT;
      433      854
      434      855
      435      856    1   DCL DP SBIN WORD;
      436      857    1   DCL P  SBIN WORD;
      437      858    1   DCL RP SBIN WORD;
      438      859
      439      860
      440      861    1   IF PATH_INFO.DEST(ITEM) ~= SOURCE THEN
      441      862    1     CALL EXPAND_PATH (STATE, BINBIT(PATH_INFO.DEST(ITEM), 36), SOURCE);
      442      863
      443      864    1   DP = C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(ORDERED_ITEM.E(ITEM)) ->
      444      865    1                                   KEY_B.ELEM.DOT_POS(ORDERED_ITEM.I(ITEM)) + 1;
      445      866    1   P = C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(ORDERED_ITEM.E(ITEM)) ->
      446      867    1                         KEY_B.ELEM.PROD(ORDERED_ITEM.I(ITEM));
      447      868
      448      869    2   DO RP = PROD.RIGHT_START(P) + DP TO PROD.RIGHT_START(P) + PROD.RIGHT_CNT(P) - 1;
PL6.E3A0      #006=EXPAND_PATH File=LR_ERRPTH.:Z8A6TSI                           SAT 08/09/97 09:29 Page:154  
      449      870
      450      871    2     IF EXP_PATH_INDX < %MAX_STRSP_LEN_H THEN
      451      872    3       DO;
      452      873    3         COM_STRING_SPACE_H (EXP_PATH_INDX) = RIGHT_PARTS(RP);
      453      874    3         EXP_PATH_INDX = EXP_PATH_INDX + 1;
      454      875    3       END;
      455      876    2     ELSE
      456      877    2       CALL DIAG (15,
               877               'ERROR => Closure item path space exhausted during error table generation.%%'
               877                );
      457      878    2   END;
      458      879
      459      880    1   REACHED(ITEM) = TRUE;
      460      881
      461      882    1   END EXPAND_PATH;
      462      883        %EOD;

