PL6.E3A0      #008=WRITE_LOCAL_CONTEXT File=LR_ERRPTH.:Z8A6TSI                   SAT 08/09/97 09:30 Page:209  
      611      920    1   INSERT_PATH_LIST : PROC;  /* IN WRITE_LOCAL_CONTEXT */
      612      921
      613      922        /* A path list has been built at the end of the array.
      614      923           Search for a redundant string before expanding the
      615      924           space to include the string.
      616      925        */
      617      926
      618      927    2   COM_SEARCH_STRING_LEN = (PROPOSED_PATH_TABLE_LEN - PATH_TABLE_LEN) * SIZEC(PATH(0));
      619      928    2   COM_SEARCH_STRING = ADDR(PATH(PATH_TABLE_LEN));
      620      929
      621      930    2   I = 0;
      622      931    3   DO UNTIL (MOD(I,SIZEC(PATH(0))) = 1);
      623      932
      624      933    3     CALL INDEX (PATH_INDX,
      625      934    3                 CSEARCH_STRING,
      626      935    3                 SUBSTR(PATH_STRINGS, I, PATH_TABLE_LEN*SIZEC(PATH(0)) - I));
      627      936    3     I = I + PATH_INDX + 1;
      628      937    3   END;
      629      938
      630      939    2   PATH_INDX = (I-1) / SIZEC(PATH(0));
      631      940
      632      941    2   IF PATH_INDX = PATH_TABLE_LEN THEN
      633      942    2     PATH_TABLE_LEN = PROPOSED_PATH_TABLE_LEN;
      634      943
      635      944    2   END INSERT_PATH_LIST;
      636      945
      637      946
      638      947        %EJECT;
PL6.E3A0      #008=WRITE_LOCAL_CONTEXT File=LR_ERRPTH.:Z8A6TSI                   SAT 08/09/97 09:30 Page:210  
      639      948        /* MAIN BODY OF WRITE_LOCAL_CONTEXT */
      640      949
      641      950    2   DO ITEM = 0 TO C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      642      951
      643      952    2     REACHED(ITEM) = FALSE;
      644      953
      645      954    2   END;
      646      955
      647      956
      648      957    1   PROPOSED_PATH_TABLE_LEN = PATH_TABLE_LEN;
      649      958
      650      959    2   DO ITEM = 0 TO C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      651      960
      652      961    2     IF NOT REACHED(ITEM) THEN
      653      962    3       DO;
      654      963    3         IF PATH_INFO.COST(ITEM) < INFINITE_COST THEN
      655      964    4           DO;
      656      965    4             REACHED(ITEM) = TRUE;
      657      966    4             EXP_PATH_INDX = COM_STRSP_LEN / 2;
      658      967    4             CALL EXPAND_PATH(STATE, ITEM, SOURCE);
      659      968    4             CALL INSERT_PATH_STRING;
      660      969    4           END;
      661      970    3             ELSE
      662      971    3               IF ITEM < C#_TO_$(STATE) -> C_SET_B.BASIS_COUNT THEN
      663      972    3                  CALL INSERT_NULL_PATH;
      664      973    3       END;
      665      974
      666      975    2   END;
      667      976
      668      977    1   CALL INSERT_PATH_LIST;
      669      978
      670      979    1   CALL OUT_DEC(PATH_INDX);
      671      980
      672      981        %BOUNDS(T=IINDX, V=PATH_INDX);
      673      987
      674      988        %OUT_STR(S="',L='");
      675      991    1   CALL OUT_DEC(COM_SEARCH_STRING_LEN/SIZEC(PATH(0)));
PL6.E3A0      #008=WRITE_LOCAL_CONTEXT File=LR_ERRPTH.:Z8A6TSI                   SAT 08/09/97 09:30 Page:211  
      676      992
      677      993        %BOUNDS(T=ILEN, V="COM_SEARCH_STRING_LEN/SIZEC(PATH(0))");
