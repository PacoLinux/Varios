      754        5        /*T***********************************************************/
      755        6        OUTPUT_PATHS : PROC ;
      756        7
      757        8        /*D*
      758        9               NAME: OUTPUT_PATHS
      759       10            PURPOSE: OUTPUT THE PATH TABLE FOR ALL STATES.
      760       11        */
      761       12
      762       13
      763       14        %INCLUDE LR_GRAM_EXT;
      764      553        %INCLUDE LR_CNFG_EXT;
      765      706        %INCLUDE LR_ERR_EXT;
      766      849
      767      850
      768      851    1   DCL I SBIN WORD;
      769      852
      770      853
      771      854
      772      855        %SELECT_SUBS;
      773      858
      774      859        %OUT_STR(S="'%EQU ITEM_LEN = '");
      775      862    1   CALL OUT_DEC(ITEM_TABLE_LEN);
      776      863        %OUT_STR(S="';'");
      777      866    1   CALL FLUSH;
      778      867
      779      868        %STAT_LENGTH(T=IINDX,V="ITEM_TABLE_LEN+ 1");
      780      871        %STAT_LENGTH(T=ILEN,V="ITEM_TABLE_LEN+1");
      781      874        %STAT_LENGTH(T=SSTATE,V="ITEM_TABLE_LEN+1");
      782      877        %STAT_LENGTH(T=SITEM,V="ITEM_TABLE_LEN+1");
      783      880
      784      881        /* OUTPUT PATHS */
      785      882
      786      883        %OUT_STR(S="'%EQU PATH_LEN = '");
PL6.E3A0      #010=OUTPUT_PATHS File=LR_ERRPTH.:Z8A6TSI                          SAT 08/09/97 09:31 Page:280  
      787      886    1   CALL OUT_DEC(PATH_TABLE_LEN - 1);
      788      887        %OUT_STR(S="';'");
      789      890    1   CALL FLUSH;
      790      891
      791      892        %STAT_LENGTH(T=P_INDX,V="PATH_TABLE_LEN");
      792      895        %STAT_LENGTH(T=P_LEN,V="PATH_TABLE_LEN");
      793      898
      794      899        %SELECT_OU;
      795      902
      796      903        %OUTLINE(S="'%START_TABLE_PATH;'");
      797      909
      798      910    2   DO I = 1 TO PATH_TABLE_LEN - 1;
      799      911
      800      912          %OUT_STR(S="'  %PATH(I='");
      801      915
      802      916          %BOUNDS(T=P_INDX, V="PATH.INDX(I)");
      803      922
      804      923    2     CALL OUT_DEC(BINBIT(PATH.INDX(I), 36));
      805      924          %OUT_STR(S="',L='");
      806      927    2     CALL OUT_DEC(BINBIT(PATH.LEN(I), 36));
      807      928
      808      929          %BOUNDS(T=P_LEN, V="PATH.LEN(I)");
      809      935
      810      936          %OUT_STR(S="');  /* '");
      811      939    2     CALL OUT_DEC(I);
      812      940          %OUT_STR(S="' */'");
      813      943    2     CALL FLUSH;
      814      944
      815      945    2   END;
      816      946
      817      947        %OUTLINE(S="'%END_TABLE_PATH;'");
      818      953
      819      954    1   END OUTPUT_PATHS;
      820      955        %EOD;

