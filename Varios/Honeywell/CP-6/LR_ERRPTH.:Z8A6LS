      283        6        FINDLOCALCONTEXT : PROC (STATE) NOAUTO;
      284        7
      285        8        /*D*
      286        9            NAME: FINDLOCALCONTEXT
      287       10         PURPOSE:
      288       11           Find the regular expressions denoting the local right
      289       12           context of the items in the state.
      290       13
      291       14           This is done by finding least_cost_paths using DIHKSTRA's
      292       15           algorithm and following them, with branches, to get the regular
      293       16           expression.
      294       17        */
      295       18
      296       19    1   DCL STATE SBIN WORD;
      297       20
      298       21        %INCLUDE LR_GRAM_EXT;
      299      560        %INCLUDE LR_CNFG_EXT;
      300      713        %INCLUDE LR_ERR_EXT;
      301      856
      302      857    1   DCL FINDNEIGHBORS       ENTRY(1);
      303      858    1   DCL WRITE_LOCAL_CONTEXT ENTRY(2);
      304      859
      305      860    1   DCL ITEM    SBIN WORD;
      306      861    1   DCL TITEM   SBIN WORD;
      307      862    1   DCL N       SBIN WORD;
      308      863    1   DCL SOURCE  SBIN WORD;
      309      864    1   DCL NB      SBIN WORD;
      310      865    1   DCL DONE    BOOLEAN;
      311      866    1   DCL MINCOST SBIN WORD;
      312      867    1   DCL CHOICE  SBIN WORD;
      313      868
      314      869
PL6.E3A0      #005=FINDLOCALCONTEXT File=LR_ERRPTH.:Z8A6TSI                      SAT 08/09/97 09:29 Page:115  
      315      870
      316      871    1   CALL FINDNEIGHBORS(STATE);
      317      872
      318      873
      319      874
      320      875    1   DO SOURCE = C#_TO_$(STATE) -> C_SET_B.BASIS_COUNT
      321      876    2                 TO C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      322      877
      323      878
      324      879    3     DO ITEM = 0 TO C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      325      880
      326      881    3       CHOSEN(ITEM)         = FALSE;
      327      882    3       PATH_INFO.DEST(ITEM) = 0;
      328      883    3       PATH_INFO.COST(ITEM) = INFINITE_COST;
      329      884
      330      885    3     END;
      331      886
      332      887    2     NB = NEIGHBOR(SOURCE);
      333      888
      334      889    3     DO N = 0 TO C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(NB) -> KEY_B.W.ELEM_COUNT - 1;
      335      890
      336      891    3       ITEM = C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(NB) -> ITEM_B.ELEM.ITEM#(N);
      337      892    3       PATH_INFO.COST(ITEM) = EDGECOST(ITEM);
      338      893    3       PATH_INFO.DEST(ITEM) = SOURCE;
      339      894
      340      895    3     END;
      341      896
      342      897    3     DO UNTIL DONE;
      343      898
      344      899    3       DONE = TRUE;
      345      900
      346      901    3       MINCOST = INFINITE_COST;
      347      902
      348      903    3       N = C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      349      904
      350      905    4       DO ITEM = 0 TO N;
      351      906
PL6.E3A0      #005=FINDLOCALCONTEXT File=LR_ERRPTH.:Z8A6TSI                      SAT 08/09/97 09:29 Page:116  
      352      907    4         IF NOT CHOSEN(ITEM) THEN
      353      908    4           IF PATH_INFO.COST(ITEM) < MINCOST THEN
      354      909    5             DO;
      355      910    5               DONE    = FALSE;
      356      911    5               CHOICE  = ITEM;
      357      912    5               MINCOST = PATH_INFO.COST(ITEM);
      358      913    5             END;
      359      914
      360      915    4       END;
      361      916
      362      917    3       IF NOT DONE THEN
      363      918    4         DO;
      364      919    4           CHOSEN (CHOICE) = TRUE;
      365      920    4           NB = NEIGHBOR(CHOICE);
      366      921    4           IF NB >= 0 THEN
      367      922    5             DO N = 0 TO C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(NB) -> KEY_B.W.ELEM_COUNT
               922                      - 1;
      368      923
      369      924    5               TITEM = C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(NB) ->
      370      925    5                            ITEM_B.ELEM.ITEM#(N);
      371      926
      372      927    5               IF PATH_INFO.COST(CHOICE) + EDGECOST(TITEM) < PATH_INFO.COST(TITEM) THEN
      373      928    6                 DO;
      374      929    6                   PATH_INFO.COST(TITEM) = PATH_INFO.COST(CHOICE) + EDGECOST(TITEM);
      375      930    6                   PATH_INFO.DEST(TITEM) = CHOICE;
      376      931    6                 END;
      377      932
      378      933    5             END;
      379      934    4         END;
      380      935    3     END;  /* DO UNTIL */
      381      936
      382      937          %OUT_STR(S="'  %ITEM(SS='");
      383      940    2     IF C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(ORDERED_ITEM.E(SOURCE)) ->
      384      941    2                              KEY_B.W.SYM_AFTER = 0 THEN
      385      942    3       DO;
      386      943              %OUT_STR(S="'-1,SI=0,I='");
      387      946              %BOUNDS(T=SSTATE, V="-1");
PL6.E3A0      #005=FINDLOCALCONTEXT File=LR_ERRPTH.:Z8A6TSI                      SAT 08/09/97 09:29 Page:117  
      388      952    3       END;
      389      953    2     ELSE
      390      954    3       DO;
      391      955    3         CALL OUT_DEC(BINBIT(C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(ORDERED_ITEM.E(SOURCE)
               955                  ) ->
      392      956    3                                 KEY_B.KEY_VAL$ -> C_SET_B.C_SET#, 36));
      393      957
      394      958              %BOUNDS(T=SSTATE, V="C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(ORDERED_ITEM.E(SOURCE
               958        )) -> KEY_B.KEY_VAL$ -> C_SET_B.C_SET#");
      395      964              %OUT_STR(S="',SI='");
      396      967    3         CALL OUT_DEC(BINBIT(C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(ORDERED_ITEM.E(SOURCE
               967                  )) ->
      397      968    3                                 ITEM_B.ELEM.SUCC_ITEM#(ORDERED_ITEM.I(SOURCE)), 36));
