PL6.E3A0      #009=OUTPUT_STATE_ITEMS File=LR_ERRPTH.:Z8A6TSI                    SAT 08/09/97 09:31 Page:250  
      690        1        /*T***********************************************************/
      691        2        /*T*                                                         */
      692        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      693        4        /*T*                                                         */
      694        5        /*T***********************************************************/
      695        6        OUTPUT_STATE_ITEMS : PROC;
      696        7
      697        8        /*D*
      698        9               NAME: OUTPUT_STATE_ITEMS
      699       10            PURPOSE: OUTPUT THE INDICIES INTO THE ITEM TABLE FOR EACH
      700       11                     STATE.
      701       12        */
      702       13
      703       14        %INCLUDE LR_GRAM_EXT;
      704      553        %INCLUDE LR_CNFG_EXT;
      705      706        %INCLUDE LR_ERR_EXT;
      706      849
      707      850    1   DCL STATE SBIN WORD;
      708      851
      709      852
      710      853        %SELECT_SUBS;
      711      856        %OUT_STR(S="'%EQU STATE_ITEM_LEN = '");
      712      859    1   CALL OUT_DEC(NR_CONFIG - 1);
      713      860        %OUT_STR(S="';'");
      714      863    1   CALL FLUSH;
      715      864
      716      865        %STAT_LENGTH(T=SI_BASIS,V="NR_CONFIG");
      717      868        %STAT_LENGTH(T=SI_CLOSURE,V="NR_CONFIG");
      718      871
      719      872        %SELECT_OU;
      720      875
      721      876        /* FINISH UP ITEM LIST FROM WRITE_LOCAL_CONTEXT */
      722      877
      723      878        %OUTLINE(S="'%END_TABLE_ITEM;'");
      724      884
      725      885        %OUTLINE(S="'%START_TABLE_STATE_ITEM;'");
      726      891
PL6.E3A0      #009=OUTPUT_STATE_ITEMS File=LR_ERRPTH.:Z8A6TSI                    SAT 08/09/97 09:31 Page:251  
      727      892    2   DO STATE = 1 TO NR_CONFIG - 1;
      728      893
      729      894          %OUT_STR(S="'  %STINDX(B='");
      730      897    2     CALL OUT_DEC(BINBIT(STATE_ITEM_INDEX.BASIS(STATE),36));
      731      898
      732      899          %BOUNDS(T=SI_BASIS, V="STATE_ITEM_INDEX.BASIS(STATE)");
      733      905
      734      906          %OUT_STR(S="',C='");
      735      909    2     CALL OUT_DEC(BINBIT(STATE_ITEM_INDEX.CLOSURE(STATE), 36));
      736      910
      737      911          %BOUNDS(T=SI_CLOSURE, V="STATE_ITEM_INDEX.CLOSURE(STATE)");
      738      917
      739      918          %OUT_STR(S="');  /* '");
      740      921    2     CALL OUT_DEC(STATE);
      741      922          %OUT_STR(S="' */'");
      742      925    2     CALL FLUSH;
      743      926
      744      927    2   END;
      745      928
      746      929        %OUTLINE(S="'%END_TABLE_STATE_ITEM;'");
      747      935
      748      936    1   END OUTPUT_STATE_ITEMS;
      749      937        %EOD;

