      230        6        FINDEDGECOSTS : PROC (STATE) NOAUTO;
      231        7
      232        8        /*D*
      233        9               NAME: FINDEDGECOSTS
      234       10            PURPOSE: COMPUTE THE COSTS OF EDGES IN THE CLOSURE GRAPH FOR
      235       11                     EACH STATE.
      236       12        */
      237       13
      238       14    1   DCL STATE SBIN WORD;
      239       15
      240       16        %INCLUDE LR_GRAM_EXT;
      241      555        %INCLUDE LR_CNFG_EXT;
      242      708        %INCLUDE LR_ERR_EXT;
      243      851
      244      852    1   DCL ITEM SBIN WORD;
      245      853    1   DCL RP   SBIN WORD;
      246      854    1   DCL E    SBIN WORD;
      247      855    1   DCL I    SBIN WORD;
      248      856    1   DCL P    SBIN WORD;
      249      857    1   DCL DP   SBIN WORD;
      250      858
      251      859
      252      860    2   DO E = 0 TO C#_TO_$(STATE) -> C_SET_B.NR_ELEM - 1;
      253      861
      254      862    3     DO I = 0 TO C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.W.ELEM_COUNT - 1;
      255      863
      256      864    3       ITEM = C#_TO_$(STATE) -> C_SET_B.ELEM.ITEM$(E) -> ITEM_B.ELEM.ITEM#(I);
      257      865
      258      866    3       EDGECOST(ITEM) = 0;
      259      867    3       P = C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.ELEM.PROD(I);
      260      868    3       DP = C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.ELEM.DOT_POS(I) + 1;
      261      869
PL6.E3A0      #004=FINDEDGECOSTS File=LR_ERRPTH.:Z8A6TSI                         SAT 08/09/97 09:28 Page:88   
      262      870    4       DO RP = PROD.RIGHT_START(P) + DP TO PROD.RIGHT_START(P) + PROD.RIGHT_CNT(P) - 1;
      263      871
      264      872    4       IF SYM_PTRS(RIGHT_PARTS(RP)) = EOF_SYMBOL_PTR THEN
      265      873    4         EDGECOST(ITEM) = EDGECOST(ITEM) + (SYM_PTRS(RIGHT_PARTS(RP))
      266      874    4                                               ->SYMBOL.COST.INS / 2);
      267      875    4       ELSE
      268      876    4         EDGECOST(ITEM) = EDGECOST(ITEM) + SYM_PTRS(RIGHT_PARTS(RP)) -> SYMBOL.COST.INS
               876                  ;
      269      877
      270      878    4       END;
      271      879
      272      880    3     END;
      273      881
      274      882    2   END;
      275      883
      276      884    1   END FINDEDGECOSTS;
      277      885        %EOD;

