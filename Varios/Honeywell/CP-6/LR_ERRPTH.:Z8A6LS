      468        6        FINDNEIGHBORS : PROC(STATE) NOAUTO;
      469        7
      470        8        /*D*
      471        9               NAME: FINDNEIGHBORS
      472       10            PURPOSE: DETERMINE THE CLOSURE GRAPH FOR THIS STATE
      473       11        */
      474       12
      475       13    1   DCL STATE SBIN WORD;
      476       14
      477       15        %INCLUDE LR_GRAM_EXT;
      478      554        %INCLUDE LR_CNFG_EXT;
      479      707        %INCLUDE LR_ERR_EXT;
      480      850
      481      851    1   DCL ITEM SBIN WORD;
      482      852    1   DCL NT   SBIN WORD;
      483      853    1   DCL E    SBIN WORD;
      484      854    1   DCL I    SBIN WORD;
      485      855    1   DCL E2   SBIN WORD;
      486      856
      487      857
      488      858        /* Set all neighbors to NIL */
      489      859
      490      860    2   DO ITEM = 0 TO C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      491      861
      492      862    2     NEIGHBOR(ITEM) = -1;
      493      863
      494      864    2   END;
      495      865
      496      866    2   DO ITEM = 0 TO C#_TO_$(STATE) -> C_SET_B.ITEM_COUNT - 1;
      497      867
      498      868    2     E = ORDERED_ITEM.E(ITEM);
      499      869    2     I = ORDERED_ITEM.I(ITEM);
PL6.E3A0      #007=FINDNEIGHBORS File=LR_ERRPTH.:Z8A6TSI                         SAT 08/09/97 09:30 Page:181  
      500      870
      501      871    2     IF C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.ELEM.DOT_POS(I) = 0 THEN
      502      872    3       DO;
      503      873
      504      874    3         NT = PROD.GOAL_SYM_INDX(C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E) -> KEY_B.ELEM.
               874                  PROD(I));
      505      875
      506      876              /* Add all items which have sym_after = nt , since they must have generated
      507      877                 this item in closure calculations.
      508      878              */
      509      879
      510      880    3         E2 = 0;
      511      881
      512      882    4         DO WHILE C#_TO_$(STATE) -> C_SET_B.ELEM.KEY$(E2) -> KEY_B.W.SYM_AFTER ~= NT;
      513      883    4           E2 = E2 +1 ;
      514      884    4         END;
      515      885
      516      886    3         NEIGHBOR(ITEM) = E2;
      517      887
      518      888    3       END;  /* Closure item */
      519      889
      520      890    2   END;  /* DO ITEM */
      521      891
      522      892    1   END FINDNEIGHBORS;
      523      893        %EOD;

