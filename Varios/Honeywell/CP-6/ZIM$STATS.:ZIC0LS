PL6.E3A0      #001=ZID$STATS File=ZIM$STATS.:ZIC0TSI                             FRI 09/05/97 13:06 Page:7    
      126     1771    1   SVCSTATS: PROC;
      127     1772    2       CALL INSERT(GLOBAL.PRINTBUF.BUFFER,45,1,'-');
      128     1773    2       GLOBAL.TIME.TSTAMP=STATS.TSTAMP;
      129     1774    2       CALL M$TIME(GLOBAL.TIMEC$->FPT$TIME) ALTRET(ER_TIME);
      130     1775    2       CALL INSERT(GLOBAL.PRINTBUF.BUFFER,28,5,
      131     1776    2          SUBSTR(GLOBAL.TIME.TIME,0,5));
      132     1777    2       CALL INSERT(GLOBAL.PRINTBUF.BUFFER,34,10,GLOBAL.TIME.DATE);
      133     1778    2       CALL M$TIME(GLOBAL.TIME$->FPT$TIME) ALTRET(ER_TIME);
      134     1779    2       CALL M$TIME(GLOBAL.TIMEC$->FPT$TIME) ALTRET(ER_TIME);
      135     1780    2       CALL INSERT(GLOBAL.PRINTBUF.BUFFER,47,5,
      136     1781    2          SUBSTR(GLOBAL.TIME.TIME,0,5));
      137     1782    2       CALL INSERT(GLOBAL.PRINTBUF.BUFFER,53,10,GLOBAL.TIME.DATE);
      138     1783    2       IF STATS.VALID THEN
      139     1784    3       DO;
      140     1785    3           IF GLOBAL.STATS.DCB#=0 THEN
      141     1786    4           DO;
      142     1787    4               GLOBAL.DCBNAME.TEXT='STATS';
      143     1788    4               GLOBAL.DCBNAME.LENGTH=5;
      144     1789    4               CALL ZIO$DFLTOPN('NO  ') ALTRET(ER_OPEN);
      145     1790    4               GLOBAL.STATS.DCB#=GLOBAL.DCBNUMBER;
      146     1791    4               END;
      147     1792    3           GLOBAL.WRITE$->FPT$WRITE.V.DCB#=GLOBAL.STATS.DCB#;
      148     1793    3           CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(HDR_ERR);
      149     1794    3           GLOBAL.PRINTBUF.BUFFER = 'C';
      150     1795    3           CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(HDR_ERR);
      151     1796    3           BUFSTAT$ = PINCRC(ADDR(GLOBAL.PRINTBUF.BUFFER),14);
      152     1797    3           BUFSTAT$->STRING30 = 'page hits with XXX buffers.';
      153     1798    3           BUFSTAT$ = PINCRC(ADDR(GLOBAL.PRINTBUF.BUFFER),29);
      154     1799    3           CALL BINCHAR(BUFSTAT$->STRING3,GLOBAL.BCB.COUNT);
      155     1800    3           BUFSTAT$ = PINCRC(ADDR(GLOBAL.PRINTBUF.BUFFER),1);
      156     1801    3           CALL BINCHAR(BUFSTAT,STATS.HITS);
      157     1802    3           CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      158     1803    3           CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(DTL_ERR);
      159     1804    3           BUFSTAT$=PINCRC(ADDR(GLOBAL.PRINTBUF.BUFFER),14);
      160     1805    3           BUFSTAT$->STRING30='I/O attempts.';
      161     1806    3           BUFSTAT$=PINCRC(ADDR(GLOBAL.PRINTBUF.BUFFER),1);
      162     1807    3           CALL BINCHAR(BUFSTAT,STATS.REQS);
PL6.E3A0      #001=ZID$STATS File=ZIM$STATS.:ZIC0TSI                             FRI 09/05/97 13:06 Page:8    
      163     1808    3           CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      164     1809    3           CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(DTL_ERR);
      165     1810    3           GLOBAL.PRINTBUF.BUFFER=H1;
      166     1811    3           CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(HDR_ERR);
      167     1812    3           GLOBAL.PRINTBUF.BUFFER=H2;
      168     1813    3           CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(HDR_ERR);
      169     1814    3           GLOBAL.PRINTBUF.BUFFER = 'A';
      170     1815    4           DO J = 0 TO LAST#RPT#FCN;
      171     1816    4               IF STATS.COUNT.CALLS(J)>0 THEN
      172     1817    5               DO;
      173     1818    5                   BUFSTAT$ = PINCRC(ADDR(GLOBAL.PRINTBUF.BUFFER),1);
      174     1819    5                   BUFSTAT = FCN.NAME(J);
      175     1820    5                   BUFSTAT$ = PINCRC(BUFSTAT$,12);
      176     1821    5                   CALL BINCHAR(BUFSTAT,STATS.COUNT.CALLS(J));
      177     1822    5                   CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      178     1823    5                   BUFSTAT$ = PINCRC(BUFSTAT$,12);
      179     1824    5                   CALL BINCHAR(BUFSTAT,STATS.COUNT.READS(J));
      180     1825    5                   CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      181     1826    5                   BUFSTAT$ = PINCRC(BUFSTAT$,12);
      182     1827    5                   CALL BINCHAR(BUFSTAT,STATS.COUNT.WRITES(J));
      183     1828    5                   CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      184     1829    5                   BUFSTAT$=PINCRC(BUFSTAT$,12);
      185     1830    5                   CALL BINCHAR(BUFSTAT,STATS.COUNT.ENQS(J));
      186     1831    5                   CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      187     1832    5                   BUFSTAT$=PINCRC(BUFSTAT$,12);
      188     1833    5                   CALL BINCHAR(BUFSTAT,STATS.COUNT.LOCKS(J));
      189     1834    5                   CALL EDITCHAR(BUFSTAT,,,MICRO_DEC12,BUFSTAT);
      190     1835    5                   CALL M$WRITE(GLOBAL.WRITE$->FPT$WRITE) ALTRET(DTL_ERR);
      191     1836    5                   END;
      192     1837    4               END;
      193     1838    3           END;
      194     1839    2       STATS='0'B; /* Clear all stats.  Including validity */
      195     1840    2       STATS.VALID=%NO#;
      196     1841    2       STATS.TSTAMP=GLOBAL.TIME.TSTAMP; /* New interval */
      197     1842    2       RETURN;
      198     1843        %EJECT;
PL6.E3A0      #001=ZID$STATS File=ZIM$STATS.:ZIC0TSI                             FRI 09/05/97 13:06 Page:9    
      199     1844        /* can't output a statistic, return */
      200     1845    2   DTL_ERR: ;
      201     1846        /* can't output header line, return */
      202     1847    2   HDR_ERR: ;
      203     1848    2   ER_OPEN: ; /* Cannot open stats dcb */
      204     1849    2   ER_TIME: ;
      205     1850    2       RETURN;
      206     1851    2   END;
      207     1852    1   END ZID$STATS;
