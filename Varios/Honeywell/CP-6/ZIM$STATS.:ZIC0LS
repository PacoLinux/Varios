      115     1760    1       IF ~STATS.VALID THEN
      116     1761    2       DO;
      117     1762    2           CALL M$TIME(GLOBAL.TIME$->FPT$TIME) ALTRET(ER_TIME);
      118     1763    2           STATS.TSTAMP=GLOBAL.TIME.TSTAMP;
      119     1764    2           END;
      120     1765    1   ER_TIME: GLOBAL.PENDING.STATS=%YES#;
