      167     6780    2             CALL M$GETDCB( FPTGETDCB ) ALTRET( ARETG );
      168     6781    2           END;
      169     6782    1        DCB$ = DCBADDR( DCBCUR );
      170     6783    1        IF DCB$ -> F$DCB.FCD#
      171     6784    2        THEN DO;                           /* Close it first */
      172     6785    2             FPTCLOSE = FPT_CLOSE;
      173     6786    2             FPTCLOSE.V_ = VECTOR( FPTCLOSE.V );
      174     6787    2             FPTCLOSE.V.DCB# = DCBCUR;
      175     6788    2             CALL M$CLOSE( FPTCLOSE ) ALTRET( ARETCO );
      176     6789    2           END;
      177     6790        /* Open as specified by user */
      178     6791    2        DO CASE ( CODE );
      179     6792    2          CASE ( 390 );                   /* ON/TO */
      180     6793        /*********FPTOPEN.V.EXIST# = ERROR#;*********/
      181     6794    2          CASE ( 391 );                 /* OVER */
      182     6795    2             FPTOPEN.V.EXIST# = %NEWFILE#;
      183     6796    2          CASE ( 392 );                 /* INTO */
      184     6797    2             FPTOPEN.V.EXIST# = %OLDFILE#;
      185     6798    2          END;
