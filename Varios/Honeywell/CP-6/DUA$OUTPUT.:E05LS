      152     6765        /* Process the fid first */
      153     6766    1        FPTFID = FPT_FID;
      154     6767    1        FPTOPEN = FPT_OPEN_OUTPUT;
      155     6768    1        FPTFID.TEXTFID_ = VECTOR( FIDTEXTC.TEXT );
      156     6769    1        CALL PROCESS_FID ALTRET( ARETFO );
      157     6770        /* Setup to access dcb to use */
      158     6771    1        DCBCUR = 0;
      159     6772    1        IF ADDR( DCBNM ) = ADDR( NIL )
      160     6773    2        THEN DO;                           /* Use D_M$DO# */
      161     6774    2             DCBCUR = D_M$DO#;
      162     6775    2           END;
      163     6776    2        ELSE DO;                           /* Gotta get one */
