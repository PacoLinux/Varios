      188     6801    1        CALL M$OPEN( FPTOPEN ) ALTRET( ARETOO );
      189     6802    1        IF ADDR( DCBNM ) ~= ADDR( NIL )
      190     6803    1        THEN DCBNM = DCBCUR;
