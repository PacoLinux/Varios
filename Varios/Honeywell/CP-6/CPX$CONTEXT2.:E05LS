      463     8198        /*
      464     8199          ****    BREAK UP THE FID
      465     8200        */
      466     8201    2              FPT_FID =CFPT_FID;
      467     8202    2              FPT_FID.V_=VECTOR(FPT_FID.V);
      468     8203    2              FPT_FID.SN_ = VECTOR(FID_SN);
      469     8204    2              FPT_FID.WSN_ = VECTOR(FID_WSN);
      470     8205    2              FPT_FID.ASN_ = VECTOR(FID_ASN);
      471     8206    2              FPT_FID.RES_ = VECTOR(FID_RES);
      472     8207    2              FPT_FID.NAME_ = VECTOR(FID_NAME);
      473     8208    2              FPT_FID.ACCT_ = VECTOR(FID_ACCT);
      474     8209    2              FPT_FID.PASS_ = VECTOR(FID_PASS);
      475     8210    2              FPT_FID.RESULTS_ = VECTOR(FID_RESULTS);
