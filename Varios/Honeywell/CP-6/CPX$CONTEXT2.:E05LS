      142     7877    1   CPX$BATXEQ: ENTRY (P_PARAM);
      143     7878    1           PTR$ = P_PARAM.OUT$;
      144     7879    2           DO CASE (PTR$ -> P$B.CODE);
      145     7880    2            CASE (BA_FID);
      146     7881        /*
      147     7882             ***** DEVICE fids are not acceptable for input ...
      148     7883        */
      149     7884    2              CALL CNVRT_FID ALTRET(BATXEQ_ERR);
      150     7885    2              IF FID_ASN = %DEVICE#
      151     7886    2              THEN
      152     7887    2                 CALL POSTERR (E_XBAFID);
      153     7888    2            CASE (BA_DFID);
      154     7889        /*
      155     7890             ***** Any valid fid is ok for output ...
      156     7891        */
      157     7892    2              CALL CNVRT_FID ALTRET(BATXEQ_ERR);
      158     7893    2            CASE (BA_KEY);
      159     7894        /*
      160     7895             ***** Make sure that a valid KEY was specified ...
      161     7896        */
      162     7897    2            CASE (BA_NUM);
      163     7898        /*
      164     7899             ***** When positioning by number IBEX uses M$PRECORD.
      165     7900                   The number is an SBIN value. Check that not exceeded ...
      166     7901        */
      167     7902    2              CALL CXX$DECBIN (PTR$ -> P$S.COUNT,VALUE) ALTRET(E_DECBIN);
      168     7903    2              IF VALUE > 67108863
      169     7904    2              THEN
      170     7905    2                 CALL POSTERR (E_XBIG67108863);
      171     7906    2            CASE (BA_STRING);
      172     7907    2              IF PTR$ -> P$B.SUBLK$(0) -> P$S.COUNT = 0
      173     7908    2              THEN
      174     7909    2                 CALL POSTERR (E_XNULLSTRING);
      175     7910    2           END;
      176     7911    1   BATXEQ_ERR:
      177     7912    1           RETURN;
      178     7913                  %EJECT;
PL6.E3A0      #001=CPX$CONTEXT2 File=CPX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:06 Page:8    
