      275     8010    1   CPX$DISPLAY: ENTRY (P_PARAM);
      276     8011    1           PTR$ = P_PARAM.OUT$;
      277     8012    1           PTR1$ = PTR$->P$B.SUBLK$(0);
      278     8013        /*
      279     8014        ****    Check for valid dcb name...
      280     8015        */
      281     8016    2           DO CASE(PTR$ -> P$S.CODE);
      282     8017    2            CASE(DDCBOPT);
      283     8018    2              CALL CHK_DCBNAME;
      284     8019    2            CASE(%LDEV_STRM_NAME#) ;
      285     8020    2              IF PTR$->P$S.COUNT ~= 2
      286     8021    2              THEN
      287     8022    2                 CALL POSTERR(E_BDSTRMNAME) ;
