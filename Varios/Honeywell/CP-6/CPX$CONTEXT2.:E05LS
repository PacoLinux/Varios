      485     8220        /*I*      NAME:   CHK_DCBNAME
      486     8221                  PURPOSE: VALIDATE A DCB NAME
      487     8222                  INPUT:   PTR$->P$S
      488     8223                           PTR1$->P$B
      489     8224        */
      490     8225        /**********************************************************************/
      491     8226    1   CHK_DCBNAME: PROC ;

   8226  2 002127   200210 741300       CHK_DCBNAME  STX1  ! FID_RES+2,,AUTO

      492     8227    2   DCL I SBIN;
      493     8228    2   DCL J SBIN;
      494     8229    2   DCL 1 DCBNAME BASED UNAL,
      495     8230    2         2 COUNT UBIN(9) UNAL,
      496     8231    2         2 TEXT(0:0) CHAR(1);
      497     8232        /**/
      498     8233        /**/
      499     8234        /*
