PL6.E3A0      #001=CPX$CONTEXT2 File=CPX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:06 Page:24   
      484     8219        /**********************************************************************/
      485     8220        /*I*      NAME:   CHK_DCBNAME
      486     8221                  PURPOSE: VALIDATE A DCB NAME
      487     8222                  INPUT:   PTR$->P$S
      488     8223                           PTR1$->P$B
      489     8224        */
      490     8225        /**********************************************************************/
      491     8226    1   CHK_DCBNAME: PROC ;
      492     8227    2   DCL I SBIN;
      493     8228    2   DCL J SBIN;
      494     8229    2   DCL 1 DCBNAME BASED UNAL,
      495     8230    2         2 COUNT UBIN(9) UNAL,
      496     8231    2         2 TEXT(0:0) CHAR(1);
      497     8232        /**/
      498     8233        /**/
      499     8234        /*
      500     8235          ****    CHECK DCB LENGTH ... CAN'T EXCEED 29 CHARS, IF FORTRAN TYPE.
      501     8236                  OTHERWISE 31 CHARS. MAX.
      502     8237        */
      503     8238    2              IF PTR$->P$S.COUNT > 29 THEN
      504     8239    3                 DO;
      505     8240    3                    I = PTR$->P$S.COUNT-1;           /*CHK IF UNIT NUMBER SPEC'D*/
      506     8241    3                    J = 0;
      507     8242    4                    DO WHILE (I>=0);
      508     8243    4                       IF (PTR1$->DCBNAME.TEXT(I) >= '0' AND
      509     8244    4                             PTR1$->DCBNAME.TEXT(I) <= '9') THEN
      510     8245    4                          J = J+1;
      511     8246    4                       ELSE ;
      512     8247    4                       I = I-1;
      513     8248    4                    END;
      514     8249    3                    IF (J > 29) OR (PTR$->P$S.COUNT > 31) THEN
      515     8250    3                       CALL POSTERR(E_XBIGDCB);
      516     8251    3                 END;
      517     8252    2              ELSE
      518     8253    2                 IF (PTR$->P$S.TEXT = 'M$UC' OR
      519     8254    2                       PTR$->P$S.TEXT = 'M$LM') THEN
      520     8255    2                    CALL POSTERR(E_XILLDCB);
PL6.E3A0      #001=CPX$CONTEXT2 File=CPX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:06 Page:25   
      521     8256    2              RETURN;
      522     8257    2      END CHK_DCBNAME;
      523     8258        %EJECT;
PL6.E3A0      #001=CPX$CONTEXT2 File=CPX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:06 Page:26   
