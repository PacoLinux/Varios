PL6.E3A0      #001=CPX$CONTEXT2 File=CPX$CONTEXT2.:E05TSI                        TUE 07/29/97 12:06 Page:17   
      351     8086    1   CPX$RUNQM: ENTRY (P_PARAM);
      352     8087    1           PTR$ = P_PARAM.OUT$;
      353     8088        /*
      354     8089          ****    If run unit name has a '?' in it, reject (back up).
      355     8090        */
      356     8091    2           DO CASE(PTR$->P$S.CODE);
      357     8092    2            CASE(RUNITCMND);
      358     8093    2              PTR$ = PTR$ -> P$B.SUBLK$(0);
      359     8094    2              IF PTR$ -> P$S.COUNT = 0 THEN RETURN;
      360     8095    2              CALL INDEX (INDX, '.', SUBSTR (PTR$ -> P$S.TEXT, 0,
      361     8096    2                    PTR$ -> P$S.COUNT));
      362     8097    2              CALL INDEX (VALUE, '?', SUBSTR (PTR$ -> P$S.TEXT, 0, INDX))
      363     8098    3              WHENRETURN DO;
      364     8099    3                    P_PARAM.CHARPOS = PTR$ -> P$S.CPOS;
      365     8100    3                    P_PARAM.REJECT = %YES#;
      366     8101    3                    IF NOT P_PARAM.PCB$ -> P$PCB.NXT
