      295     8030    1   CPX$EJECT: ENTRY (P_PARAM);
      296     8031    2           DO INDX=0 TO P_PARAM.OUT$->P$B.NSUBLKS-1;
      297     8032    2              PTR$ = P_PARAM.OUT$->P$B.SUBLK$(INDX);
      298     8033    3              DO CASE(INDX);
      299     8034    3               CASE(0) ;
      300     8035    3                 IF PTR$->P$S.COUNT ~= 2
      301     8036    3                 THEN
      302     8037    3                    CALL POSTERR(E_BDSTRMNAME) ;
      303     8038    3               CASE(1) ;
      304     8039    3                 IF PTR$->P$S.COUNT ~= 2
      305     8040    3                 THEN
