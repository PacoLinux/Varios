      253     7988    1           PTR$ = P_PARAM.OUT$;
      254     7989    1           CALL CNVRT_FID ALTRET(FID_ERR);
      255     7990        /*
      256     7991          ****    COMMAND ERROR IF OTHER THAN DP#nn.acct OR .acct SPECIFIED
      257     7992          ****    OR IF JUST A PERIOD SPECIFIED.
      258     7993        */
      259     7994    1           IF (FID_RESULTS.ACCT = %NO#) OR (FID_ACCT.ACCT# = ' ') THEN
      260     7995    1              GOTO DIR_ERR;
      261     7996    1           IF ((FID_RESULTS.NAME = %YES#) OR (FID_RESULTS.PASS = %YES#)
      262     7997    1                 OR (FID_RESULTS.WSN = %YES#) OR (FID_ASN ~= %FILE#)) THEN
      263     7998    1              GOTO DIR_ERR;
      264     7999        /*
      265     8000          ****    ONLY ALLOW SPECIFICATION OF ONE SN
      266     8001        */
      267     8002    1           IF FID_SN.NUM# > 1 THEN
      268     8003    1              GOTO DIR_ERR;
