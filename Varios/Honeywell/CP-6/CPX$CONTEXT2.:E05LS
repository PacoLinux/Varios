      338     8073    1           PTR$ = P_PARAM.OUT$;
      339     8074    1           CALL CNVRT_FID ALTRET(RUNIT_ERR);
      340     8075        /*
      341     8076          ****    RUN UNIT NAME MUST BE SPECIFIED
      342     8077        */
      343     8078    2           DO CASE(PTR$->P$S.CODE);
      344     8079    2            CASE(RUNUNITOPT);
      345     8080    2              IF FID_NAME.L# = 0 THEN
      346     8081    2                 CALL POSTERR(E_XNORU);
