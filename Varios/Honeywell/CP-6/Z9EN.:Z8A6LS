      259        1        /*T***********************************************************/
      260        2        /*T*                                                         */
      261        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
      262        4        /*T*                                                         */
      263        5        /*T***********************************************************/
      264        6        Z9EN$READ_RANDOM : PROC ( FILE$,
      265        7                                INTO$,
      266        8                                INVALID_ILS_LIST$,
      267        9                                VALID_ILS_LIST$,    /* NOT INVALID */
      268       10                                KEY_OFFSET);
      269       11
      270       12         /*P*
      271       13               NAME : Z9EN$READ_ {RANDOM|NEXT}
      272       14               PURPOSE : To create ILL for the READ statement.
      273       15         */
      274       16
      275       17    1     DCL FILE$        PTR;  /* To LX_ETT_ITEM for file */
      276       18    1     DCL KEY_OFFSET   SBIN;
      277       19    1     DCL INTO$        PTR;  /* To LX_ETT_ITEM for INTO identifier */
      278       20    1     DCL INVALID_ILS_LIST$ PTR;  /* to ILS_LIST for exception clause */
      279       21    1     DCL VALID_ILS_LIST$   PTR;     /* to NOT ILS_LIST for exception clause */
      280       22
      281       23        %EQU PHASE = 1;
      282       24        %INCLUDE Z8A_DEFC;
