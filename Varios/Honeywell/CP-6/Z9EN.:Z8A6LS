      652    15457    1     FIRST_SET_UP_WRITE : PROC;
      653    15458
      654    15459    2       IF FILE_LTS$ -> FILE_LTS.FILE_STATUS$ ~= ADDR(NIL) AND  /*#42918*/
      655    15460    2           FILE_LTS$ -> FILE_LTS.FILE_STATUS$ -> SCALAR.B.LINKAGE THEN
      656    15461    2         CALL Z9EN$CREATE_MOVE( FILE_LTS$ -> FILE_LTS.FILE_STATUS$,
      657    15462    2                                O1COM.CURRENT_PROC$ -> PROC_.PROC_DEF$ -> PROC_DEF.
             15462                                         ARGLIST$,
      658    15463    2                                %FILE_STATUS_OFF,
      659    15464    2                                FILE_LTS$ );
      660    15465
      661    15466    2       CALL Z8EU$CREATE_IDENTIFIER_FROM_ST
      662    15467    2                (FILE_LTS$ -> FILE_LTS.FAB$,
      663    15468    2                 ADDR(FAB));
      664    15469
