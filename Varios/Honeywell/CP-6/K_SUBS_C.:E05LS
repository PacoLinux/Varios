    8      /*F* NAME: K_function_equs - Host values for K$RWPARM.PARAMER.FUNCTION
    9
   10      These EQUs are used in K$RWPARM.FUNCTION (host version only).
   11      */
   12
   13      %EQU K_WRITE = 1;
   14      %EQU K_CONNECT = 3;
   15      %EQU K_DISCONNECT = 4;
   16      %EQU K_RECV = 5;
   17      %EQU K_REPDISC = 7;
   18      %EQU K_READDONE = 9;
   19      %EQU K_CONNECT_ACK = 10;
   20
   21      /*F* NAME: K$RWPARM_ERR_equ - Host error codes for K$RWPARM.ERR
   22      */
   23
   24      %EQU K$QFULL = 1;       /* Output CQ is full */
   25      %EQU K$BAD_BUFFER = 2;  /* Trap moving message to/from user buffer */
   26      %EQU K$DEFER_IO = 3;    /* Defer message 'til later */
   27      %EQU K$NO_THROT = 4;    /* No throttling for this write */
   28      %EQU K$FEP_DOWN = 5;    /* Fep is down */
   29      %EQU K$WAIT_READ = 6;   /* Wait for READDONE from application */
   30      %EQU K$Q2SMALL = 7;     /* The OCQ is too small for this message */
   31      %EQU K$USER_ERR = 8;    /* Return vlp_errcode in k$rwparm.msg_errcode to user */
   32      %EQU K$BAD_LDCTX = 9;   /* Bad message; crash local FEP immediately */

