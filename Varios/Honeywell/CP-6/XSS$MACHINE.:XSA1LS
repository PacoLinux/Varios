        1        1        /*M*    XSS$MACHINE     ISO Session Protocol Machine                    */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DDA */
        8        8        /*P*
        9        9                NAME:   XSS$MACHINE
       10       10                PURPOSE:
       11       11                    This module is the state machine for the ISO Session layer.
       12       12                    The same code serves for both host and FEP platforms.  The
       13       13                    necessary ugliness for interfacing with higher layers (e.g.,
       14       14                    Presentation, CASE, ACSE) and lower layers (e.g., Transport)
       15       15                    resides in other modules.
       16       16        */
       17       17        /*F*
       18       18                NAME:   XSS$MACHINE
       19       19                PURPOSE:
       20       20                    This module is the state machine for the ISO Session layer.
       21       21                    The same code serves for both host and FEP platforms.  The
       22       22                    necessary ugliness for interfacing with higher layers (e.g.,
       23       23                    Presentation, CASE, ACSE) and lower layers (e.g., Transport)
       24       24                    resides in other modules.
       25       25                DESCRIPTION:
       26       26                    On entry, the parameter VBLK contains all the information of
       27       27                    interest.  The event being reported is combined with the
       28       28                    current state of the connection and looked up in the table of
       29       29                    permitted actions.  Depending on the particular action to be
       30       30                    performed, other routines are called.  In general, internal
       31       31                    Session routines are called, various flags are updated in the
       32       32                    Session connection context, the state of the connection is
       33       33                    possibly updated, and the module RETURNs.
       34       34
       35       35                    If the Session connection aborts, the ALTRETURN is taken.
       36       36
       37       37                    In the code below, the notation TABLE xx refers to a specific
PL6.E3A0      #001=XSS$MACHINE File=XSS$MACHINE.:XSA1TSI                         WED 10/01/97 12:18 Page:2    
       38       38                    protocol state machine table in the ISO Session protocol spec.
       39       39        */
       40       40        XSS$MACHINE: PROC( VBLK) ALTRET;
       41       41        %INCLUDE UM_CP6;
       42      893        %INCLUDE K_ADDRESS_M;
       43     1396        %INCLUDE XSS_SESSION_E;
       44     1961        %INCLUDE XSS_SESSION_M;
       45     4604            %XSS$PRIMITIVES( STCLASS="BASED( SFPT$)");
       46     7983        %INCLUDE XSS_INTERNAL_E;
       47     8281        %INCLUDE XSS_INTERNAL_M;
       48     8870            %XSS$SCTX( NAME=SCTX, STCLASS="BASED( VBLK.CTX$)");
       49     9304            %XSS$VBLK( NAME=VBLK, STCLASS="");
       50     9409            %XSS$MEMBLK( NAME=B$BLOCK);
       51     9447        %INCLUDE XSS_MACHINE_M;
       52     9702        %MACRO FATALDEF( NAME=XSS_FATAL, STCLASS="CONSTANT SYMDEF", LVL=1, LAST=";",
       53     9703                         ERR#=0, SEV=7);
       54     9704            %VLP_ERRCODE( FPTN=NAME, STCLASS=STCLASS, FCG='XS', MID='S',
       55     9705                          MON='0'B, ERR#=ERR#, SEV=SEV);
       56     9706        %MEND;
       57     9707        %INCLUDE XSC_PERR_C;
       58     9784            %FATALDEF( NAME=XSS_F$CYCLE, ERR#=%F$CYCLE);
       59     9832            /*E*  ERROR:  XSS-F$CYCLE-7
       60     9833                MESSAGE:  Primitive processing already in progress.
       61     9834            */
       62     9835            %FATALDEF( NAME=XSS_F$EVNTERR, ERR#=%F$EVNTERR);
       63     9883            /*E*  ERROR:  XSS-F$EVNTERR-7
       64     9884                MESSAGE:  State machine requested wrong event.
       65     9885            */
       66     9886            %FATALDEF( NAME=XSS_F$NOSESS, ERR#=%F$NOSESS);
       67     9934            /*E*  ERROR:  XSS-F$NOSESS-7
       68     9935                MESSAGE:  No handler for this Session primitive.
       69     9936            */
       70     9937        %EQU TOKENS = %CHARTEXT( 'XSS$GENERIC.TOKENS');
       71     9938        %EQU ZROS = '000000000'B;
       72     9939        %EQU ONES = '111111111'B;
       73     9940
       74     9941    1       DCL XSS$BLKPTR ENTRY(2) ALTRET;
PL6.E3A0      #001=XSS$MACHINE File=XSS$MACHINE.:XSA1TSI                         WED 10/01/97 12:18 Page:3    
       75     9942    1       DCL XSS$BUFPTR ENTRY(2) ALTRET;
       76     9943    1       DCL XSS$CLOCK ENTRY(2);
       77     9944    1       DCL XSS$DUMPFPT ENTRY(1) OPTIONAL;
       78     9945    1       DCL XSS$ENCODE ENTRY(1) ALTRET;
       79     9946    1       DCL XSS$GETMEM ENTRY(3) ALTRET;
       80     9947    1       DCL XSS$RELCTX ENTRY(1) ALTRET;
       81     9948    1       DCL XSS$RELMEM ENTRY(1) ALTRET;
       82     9949    1       DCL XSS$SCREECH ENTRY(1);
       83     9950    1       DCL XSS$TSEND ENTRY(1) ALTRET;
       84     9951    1       DCL XSS$EVLOG ENTRY(2);
       85     9952
       86     9953    1       DCL XSS_TRACE SBIN SYMREF;
       87     9954
       88     9955    1       DCL I SBIN;
       89     9956    1       DCL SFPT$ PTR;
       90     9957            /* PRED, PINV, and CONTINUING used by the {IF/ELSE/END}PREDDO macros */
       91     9958    1       DCL ZPRED(0:2) SBIN;
       92     9959    1       DCL PRED(0:107) REDEF ZPRED BIT(1) UNAL;
       93     9960    1       DCL ZPINV(0:2) SBIN;
       94     9961    1       DCL PINV(0:107) REDEF ZPINV BIT(1) UNAL;
       95     9962    1       DCL CONTINUING BIT(1);
       96     9963        %EJECT;
PL6.E3A0      #001=XSS$MACHINE File=XSS$MACHINE.:XSA1TSI                         WED 10/01/97 12:18 Page:4    
       97     9964        /************************************************************************
       98     9965        *                                                                       *
       99     9966        *       Various internal MACROs                                         *
      100     9967        *                                                                       *
      101     9968        ************************************************************************/
      102     9969        %MACRO EVENT( EV=0); /* generate an output event of the specified type */
      103     9970            CALL EVNT( EV) ALTRET( EVNTERR);
      104     9971        %MEND;
      105     9972        %MACRO INVALID; /* an illegal state/event combination has occurred */
      106     9973            CALL OOPS;
      107     9974        %MEND;
      108     9975        %EJECT;
PL6.E3A0      #001=XSS$MACHINE File=XSS$MACHINE.:XSA1TSI                         WED 10/01/97 12:18 Page:5    
      109     9976        /************************************************************************
      110     9977        *                                                                       *
      111     9978        *       Set up to enter the Session state machine                       *
      112     9979        *                                                                       *
      113     9980        ************************************************************************/
      114     9981    1       CALL XSS$EVLOG( 'SS', VBLK.EVENT*512 + SCTX.SCID);
      115     9982            /* check for event loop on this connection.
      116     9983               (Would probably be caused by user application calling XSS$SESSION
      117     9984               directly from its Session handler procedure.  Cannot allow that.) */
      118     9985    1       I = SCTX.HISTX;
      119     9986    1       IF SCTX.HIST.NXTST( I) = 511 THEN
      120     9987    1           CALL XSS$SCREECH( XSS_F$CYCLE);
      121     9988    1       SFPT$ = VBLK.FPT$;
      122     9989            /* update state/event history */
      123     9990    1       I = I + 1;
      124     9991    1       IF I > 7 THEN
      125     9992    1           I = 0;
      126     9993    1       SCTX.HISTX = I;
      127     9994    1       SCTX.HIST.EVENT( I) = VBLK.EVENT;
      128     9995    1       SCTX.HIST.NXTST( I) = 511;  /* will be something legal before RETURN */
      129     9996        %SET INPRED = '0'B;  /* set up {IF/ELSE/END}PREDDO macro invocation check */
      130     9997            /* set predicate flags up */
      131     9998    1       ZPRED(0) = 0;  ZPRED(1) = 0;  ZPRED(2) = 0;
      132     9999    1       ZPINV(0) = -1; ZPINV(1) = -1; ZPINV(2) = -1;
      133    10000            /* fan out on the event and connection state */
      134    10001    2       DO CASE( VBLK.EVENT);
      135    10002        %EJECT;
PL6.E3A0      #001=XSS$MACHINE File=XSS$MACHINE.:XSA1TSI                         WED 10/01/97 12:18 Page:6    
      136    10003        /*****************************************************************************
      137    10004        *
