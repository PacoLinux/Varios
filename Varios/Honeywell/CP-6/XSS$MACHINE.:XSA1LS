     2818    41953    2               RETURN;
     2819    41954                /* during this trip thru the state machine, connection aborted */
     2820    41955    2           ALTRETURN;  /* have E$STATE or other error reported to user */
     2821    41956    2           END;
     2822    41957    2       IF SCTX.STATE > 127 THEN DO;
     2823    41958                /* There was a user request inconsistent with connection state. */
     2824    41959    2           SCTX.STATE = SCTX.STATE - 128;
