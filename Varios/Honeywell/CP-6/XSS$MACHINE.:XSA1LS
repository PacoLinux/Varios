     2810    41945    3               END;
     2811    41946    2           END;
     2812    41947    2       IF SCTX.STATE = %XSS_SST_01 THEN DO;
     2813    41948                /* State is 'idle - no transport connection' so release context */
     2814    41949    2           CALL XSS$RELCTX( VBLK.CTX$);
