      146    10015    3                   CASE( %XSS_SST_01C);
      147    10016                            %EVENT( EV=%XSS_SEV_TDISREQ);
      148    10019    3                       SCTX.STATE = %XSS_SST_01;
      149    10020    3                   CASE( %XSS_SST_16);
      150    10021                            %ACTION03;
      151    10024                            %EVENT( EV=%XSS_SEV_TDISREQ);
