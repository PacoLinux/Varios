      110    17278    4               DO;
      111    17279    4                 QI_1.ERROR_STATUS = %E$FILE_NOT_OPEN;
      112    17280    4                 GOTO CHECK_USE_PROCEDURE;
      113    17281    4               END;
      114    17282
      115    17283    3   CHECK_ALL_THE_FRAMES:
