PL6.E3A0      #001=QBE$ENABLE File=QBE$ENABLE.:E05TSI                            WED 07/30/97 04:47 Page:9    
      198    17438    1   ENABLE_DISABLE_SELECTED_FIELDS: PROC;
      199    17439
      200    17440    2   CHECK_SELECTED_FIELDS:
      201    17441    3       DO FIELD_INDEX = FIELD_START_INDEX TO FIELD_STOP_INDEX;
      202    17442
      203    17443    3         IF GZ_FIELD$ -> QI$GZ_FIELD.DESCRIPTION.IO_FLAGS.IS_OUTPUT
      204    17444    3           AND INSTR$ -> QI$OP_ENABLE.IO_FLAGS.IS_OUTPUT
      205    17445    3         THEN
      206    17446    3           GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.ENABLED_FLAGS.
             17446                    IS_OUTPUT = NEW_STATUS;
      207    17447
      208    17448    3         IF GZ_FIELD$ -> QI$GZ_FIELD.DESCRIPTION.IO_FLAGS.IS_INPUT
      209    17449    3           AND INSTR$ -> QI$OP_ENABLE.IO_FLAGS.IS_INPUT
      210    17450    3         THEN
      211    17451    3           GZ_FIELD$ -> QI$GZ_FIELD.INDICATORS$ -> QI$INDICATORS.ENABLED_FLAGS.IS_INPUT
             17451                    = NEW_STATUS;
      212    17452
      213    17453    3         GZ_FIELD$ = PINCRW ( GZ_FIELD$, SIZEW (QI$GZ_FIELD) );
      214    17454
      215    17455    3       END CHECK_SELECTED_FIELDS;
      216    17456
      217    17457    2       RETURN;
      218    17458
      219    17459    2   END ENABLE_DISABLE_SELECTED_FIELDS;
      220    17460
      221    17461    1   END QBE$ENABLE;

