             17150    1           2 CURR_FRAME$ PTR,
             17151                          /* Pointer to DAD of current frame or ADDR (NIL).  */
             17152    1           2 CURR_NODE$ PTR,
             17153                          /* Pointer to current node or ADDR (NIL).  */
             17154    1           2 USE_DEFAULT_IO UBIN,
             17155                          /* For USE procedures which default applies
             17156                             (see corresponding EQUs).  */
             17157    1           2 IS_TIME_TO_SAVE_AWAY_FIELDS BIT(1) ALIGNED,
             17158                          /* If IS_TIME_TO_SAVE_AWAY_FIELDS, then
             17159                             save the fields within the bounds of
PL6.E3A0      #001=QBE$ENABLE File=QBE$ENABLE.:E05TSI                            WED 07/30/97 04:47 Page:3    
             17160                             FRAME_START_INDEX, FRAME_STOP_INDEX,
             17161                             FIELD_START_INDEX, and FIELD_STOP_INDEX
             17162                             before calling the USE procedure.  */
             17163    1           2 FRAME_START_INDEX UBIN,
             17164                          /* Foist frame of the CURRENT.FORM$
             17165                             involved with the I-O error.  */
             17166    1           2 FRAME_STOP_INDEX UBIN,
             17167                          /* Larst frame of the CURRENT.FORM$
             17168                             involved with the I-O error.  */
             17169    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             17170                          /* If IS_FIELD_INDEXES_VALID, use them;
             17171                             else use all the fields in the frame.  */
             17172    1           2 FIELD_START_INDEX UBIN,
             17173                          /* Starting index into QI$GZ_FIELD
             17174                             which may have values "pushed".  */
             17175    1           2 FIELD_STOP_INDEX UBIN,
             17176                          /* Stopping index into QI$GZ_FIELD
             17177                             which may have values "pushed".  */
             17178    1           2 IS_CURSOR_POSITION_VALID BIT(1) ALIGNED,
             17179                          /* If IS_CURSOR_POSITION_VALID, use it;
             17180                             else allow cursor to default to upper-left-most field.  */
             17181    1           2 CURSOR_POSITION BIT(16) ALIGNED,
             17182                          /* POSition of the field on which to place the cursor.  */
             17183    1           2 STATUS UBIN;
             17184                          /* Returned action code; see corresponding EQUs.  */
             17191            %ENDIF;
