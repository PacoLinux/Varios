       10       10        %INCLUDE CP_6;
       11       91        %INCLUDE B$JIT;
       12      381        %INCLUDE TH$HIST;
       13      476        %INCLUDE TD$STRUCT;
       14      577        %INCLUDE TH_SUBS_C;
       15      959        %INCLUDE TH_DATA_R;
       16     1037        /**/
       17     1038    1   DCL B$JIT$ PTR SYMREF;
       18     1039        /**/
       19     1040        /* ENTRY DEFS */
       20     1041    1   DCL THG$GETREC ENTRY(4) ALTRET;
       21     1042    1   DCL THM$ERRMSG ENTRY(1);
       22     1043        /**/
       23     1044        /* DCBS */
       24     1045    1   DCL M$DEVHIST DCB;
       25     1046        /**/
       26     1047        /* VARS */
       27     1048    1   DCL I UBIN;
       28     1049    1   DCL NUMDEVS UBIN;
       29     1050    1   DCL CURUTS UBIN WORD STATIC INIT(0);
       30     1051    1   DCL ZERO CHAR(8) CONSTANT INIT('0       '); /* USED FOR GETREC CALLS            */
       31     1052    1   DCL HEAD_KEY BIT(18) CONSTANT INIT('001000'O);
       32     1053    1   DCL MAINT_KEY BIT(18) CONSTANT INIT('001002'O);
       33     1054    1   DCL THRESH_KEY BIT(18) CONSTANT INIT('001001'O);
       34     1055    1   DCL UTS_KEY BIT(18) CONSTANT INIT('001003'O);
       35     1056        /**/
       36     1057        /* MACRO INVOCATIONS */
PL6.E3A0      #001=THD$DEVHIST File=THD$DEVHIST.:E05TSI                          WED 07/30/97 06:06 Page:2    
       37     1058        %TD$HEAD (NAME=TH_HEAD,STCLASS=AUTO,NUMDEV#=200);
       38     1094        %TD$HEAD (NAME=TH$HEAD,NUMDEV#=200);
       39     1130        %TD$HEAD (NAME=TD$HEAD);
       40     1166        %TH$MAINT (NAME=TH_MAINT,STCLASS=AUTO,NUMDEV#=200);
       41     1171        %TH$THRESH (NAME=TH_THRESH,STCLASS=AUTO,NUMDEV#=200);
       42     1177        %TH$UTS (NAME=TH_UTS,STCLASS=AUTO);
       43     1182        /**/
       44     1183        /* FPT DEFINITIONS */
       45     1184        %FPT_OPEN (FPTN=OPEN_DEVHIST,
       46     1185                   STCLASS=CONSTANT,
       47     1186                   DCB=M$DEVHIST,
       48     1187                   NAME=DEVHIST,
       49     1188                   ACCT=DEVHIST_ACCT,
       50     1189                   FUN=CREATE,
       51     1190                   ORG=KEYED,
       52     1191                   EXIST=NEWFILE,
       53     1192                   ACS=DIRECT,
       54     1193                   ACCESS=ACSLIST,
       55     1194                   ACSVEH=VEHLIST,
       56     1195                   ASN=FILE);
