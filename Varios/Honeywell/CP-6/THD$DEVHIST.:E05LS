PL6.E3A0      #001=THD$DEVHIST File=THD$DEVHIST.:E05TSI                          WED 07/30/97 06:06 Page:4    
       81     1529    1        TH_RECORD = VECTOR(TH_HEAD);
       82     1530    1        CALL THG$GETREC(%CONFIG,ZERO) ALTRET(NO_CONFIG);
       83     1531    1        CALL M$OPEN(OPEN_DEVHIST) ALTRET(ERR);
       84     1532    1        TH_HEAD = TH_RECORD.PTR$->TH$HEAD;
       85     1533    1        NUMDEVS = TH_HEAD.NUMUNITMPCS + TH_HEAD.NUMTAPEMPCS + TH_HEAD.NUMDISKMPCS +
       86     1534    1                  TH_HEAD.NUMAPMPCS + TH_HEAD.NUMOPCS +
       87     1535    1                  TH_HEAD.NUMFEPS + TH_HEAD.NUMLPS + TH_HEAD.NUMCRS +
       88     1536    1                  TH_HEAD.NUMCPS + TH_HEAD.NUMCONSOLES + TH_HEAD.NUMIOMS +
       89     1537    1                  TH_HEAD.NUMDPS + TH_HEAD.NUMMTS + TH_HEAD.NUMXXS +
       90     1538    1                  TH_HEAD.NUMAPS + TH_HEAD.NUMODS;
       91     1539    1        WRITE_DEVHIST.KEY_ = VECTOR(HEAD_KEY);
       92     1540    1        WRITE_DEVHIST.BUF_ = VECTOR(TH_HEAD);
       93     1541    1        WRITE_DEVHIST.BUF_.BOUND = SIZEV(TD$HEAD) + (8 * (NUMDEVS - 1));
       94     1542    1        CALL M$WRITE(WRITE_DEVHIST) ALTRET(ERR);
       95     1543
       96     1544    2        DO I = 0 TO NUMDEVS - 1;
       97     1545    2           TH_THRESH.DEVNAME(I) = TH_HEAD.DEVNAME(I);
       98     1546    2           TH_THRESH.ATTEN(I) = 9900;
       99     1547    2           TH_THRESH.ALERT(I) = 9500;
      100     1548    2           TH_MAINT.DEVNAME(I) = TH_HEAD.DEVNAME(I);
      101     1549    2           TH_MAINT.UTS(I) = 0;
      102     1550    2        END;                               /* DO I                               */
      103     1551    1        WRITE_DEVHIST.KEY_ = VECTOR(THRESH_KEY);
      104     1552    1        WRITE_DEVHIST.BUF_ = VECTOR(TH_THRESH);
      105     1553    1        WRITE_DEVHIST.BUF_.BOUND = SIZEV(TH_THRESH) - ((201 - NUMDEVS) * 16);
      106     1554    1        CALL M$WRITE(WRITE_DEVHIST) ALTRET(ERR);
      107     1555
      108     1556    1        WRITE_DEVHIST.KEY_ = VECTOR(MAINT_KEY);
      109     1557    1        WRITE_DEVHIST.BUF_ = VECTOR(TH_MAINT);
      110     1558    1        WRITE_DEVHIST.BUF_.BOUND = SIZEV(TH_MAINT) - ((201 - NUMDEVS) * 12);
      111     1559    1        CALL M$WRITE(WRITE_DEVHIST) ALTRET(ERR);
      112     1560
      113     1561    1        TH_UTS.ERRLOG_UTS = 0;
      114     1562    1        CALL M$TIME(CUR_UTS);
      115     1563    1        TH_UTS.OLDEST_UTS = CURUTS;
      116     1564    1        WRITE_DEVHIST.KEY_ = VECTOR(UTS_KEY);
      117     1565    1        WRITE_DEVHIST.BUF_ = VECTOR(TH_UTS);
PL6.E3A0      #001=THD$DEVHIST File=THD$DEVHIST.:E05TSI                          WED 07/30/97 06:06 Page:5    
      118     1566    1        CALL M$WRITE(WRITE_DEVHIST) ALTRET(ERR);
      119     1567    1        CALL M$CLOSE(CLOSE_DEVHIST) ALTRET(ERR);
      120     1568    1   NO_CONFIG:
      121     1569    1        RETURN;
      122     1570    1   ERR: CALL THM$ERRMSG;
      123     1571    1        RETURN;
      124     1572    1   END THD$DEVHIST;

