       95     5351    1           IF QQ_CMDBLK.OUT$ -> OUT$BLK.NSUBLKS = 0 THEN
       96     5352    2             DO;
       97     5353    2               STATION = 'BOTH';           /* Default TPU & station are assumed  */
       98     5354    2             END;
       99     5355    1           ELSE
      100     5356    2             DO;
      101     5357    2               STATION = QQ_CMDBLK.OUT$ -> OUT$BLK.SUBLK$(0) -> OUT$SYM.TEXT;
      102     5358    2             END;
      103     5359
      104     5360                /* Check if it is a SPY TPU option, or no option is present.
      105     5361                   If it is, fake it here and call QQB$DISPLAY to do the work */
      106     5362    1           IF STATION = 'TPU' OR STATION = 'BOTH' THEN
      107     5363    2             DO;
      108     5364    2               QQ_CMDBLK.OUT$ -> OUT$BLK.SUBLK$(0) = ADDR(NIL);
      109     5365    2               CALL QQB$DISPLAY;
      110     5366    2               IF STATION = 'TPU'
      111     5367    2               THEN
      112     5368    2                 RETURN;
      113     5369    2               ELSE
      114     5370    3                 DO;
      115     5371    3                   STATION = '?';
