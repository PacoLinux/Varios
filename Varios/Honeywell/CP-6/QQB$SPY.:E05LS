      148     5404    3                 VLP_PASS.PASS# = ' ';
      149     5405
      150     5406    3                 CALL M$UNFID(FPT_UNFID) ALTRET(LOOP);
      151     5407    3                 MSGBLK.ERRINFO.FIELD1(0) = VECTOR(TEXTC_FID);
      152     5408    3                 TEXTC_FID.L# = UNFID_SIZE;
      153     5409    3                 TEXTC_FID.NAME# = FID;
      154     5410    3                 CALL QQC$BLDMSG(MSGBLK);
      155     5411
      156     5412    3                 QQ_CMDBLK.MINOR_ERR = CODE_BLANK;
      157     5413    3                 CALL QQC$BLDMSG_BLK(MSGBLK);
      158     5414    3                 CALL QQC$BLDMSG(MSGBLK);
