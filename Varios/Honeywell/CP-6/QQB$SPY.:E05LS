      118     5374    3                   CALL QQC$BLDMSG(MSGBLK);
      119     5375    3                 END;
      120     5376    2             END;
      121     5377
PL6.E3A0      #001=QQB$SPY File=QQB$SPY.:E05TSI                                  WED 07/30/97 05:36 Page:7    
