PL6.E3A0      #001=DBD$UTIL1 File=DBD$UTIL1.:E05TSI                              TUE 07/29/97 13:11 Page:9    
      276    13053                                                /*  SYMREFs                           */
      277    13054    1   DCL DBD$FEPMAP$ PTR SYMREF;
      278    13055    1   DCL DAF$WRITE EPTR SYMREF;
      279    13056    1   DCL DB_MUIS UBIN SYMREF;
      280    13057    1   DCL DA_MAPERR BIT(36) SYMREF;
      281    13058    1   DCL DB_USRT$ UBIN SYMREF;
      282    13059    1   DCL DB_CURRENT_USER UBIN SYMREF;
      283    13060        %EJECT;
PL6.E3A0      #001=DBD$UTIL1 File=DBD$UTIL1.:E05TSI                              TUE 07/29/97 13:11 Page:10   
      284    13061        /*F*    NAME: DBD$SPY
      285    13062                PURPOSE: To process the SPY command for FEPs.
      286    13063
      287    13064        */
      288    13065
      289    13066    1   DBD$SPY_RCV: ENTRY;
      290    13067    1      UN = -DB_CURRENT_USER;
      291    13068    1      CALL DAF$WRITE(VECTOR(RCV_SPYHDR));
      292    13069    1      CALL DISPSPY;
      293    13070    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      294    13071    1      RETURN;
      295    13072
      296    13073    1   DBD$SPY: ENTRY(TOP);
      297    13074    1      CALL DAF$INIT(SPYRPT);
      298    13075    1      IF TOP.NSUBLKS=0
      299    13076    2      THEN DO;
      300    13077    3           DO UN=-1 DOWNTO -DB_MUIS;
      301    13078    3                CALL DISPSPY;
      302    13079    3                END;
      303    13080    2           DA_MAPERR = '0'B;
      304    13081    2           END;
      305    13082    2      ELSE DO;
      306    13083    3           DO I=0 TO TOP.NSUBLKS-1;
      307    13084    4                CALL DBA$SELECTION(TOP.SUBLK$(I)->OUT,UN) WHENRETURN DO;
      308    13085    4                     CALL DISPSPY;
      309    13086    4                     END;
      310    13087    3                END;
      311    13088    2           END;
      312    13089    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      313    13090    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      314    13091    1      RETURN;
      315    13092        %EJECT;
PL6.E3A0      #001=DBD$UTIL1 File=DBD$UTIL1.:E05TSI                              TUE 07/29/97 13:11 Page:11   
      316    13093    1   DISPSPY: PROC;
      317    13094    2      CALL DBM$GETMEM(ROS$,ROSBASE,UN,SIZEC(ROSBASE),,SIZEC(ROSBASE))
      318    13095    3      WHENRETURN DO;
      319    13096    3           CALL DBM$PACK4(ROSBASE.DCBPTR$,DCBPTR$);
      320    13097    3           CALL DBM$PACK4(ROSBASE.JIT$,JIT$);
      321    13098    3           CALL DBM$GETMEM(JIT$,G$JIT,UN,SIZEC(G$JIT));
      322    13099    3           CALL DBM$PACK1(G$JIT.USR#,JITUN);
      323    13100    3           CALL DBM$PACK2(G$JIT.SYSID,SYSID);
      324    13101    3           CALL DBM$PACK4(G$JIT.TUEXT,TUEXT);
      325    13102    3           CALL DBM$PACK4(G$JIT.TUSVT, TUSVT);
      326    13103    3           CALL DBM$PACK4(G$JIT.XTIME, XTIME);
      327    13104    3           CALL DBM$PACK4(G$JIT.STIME, STIME);
      328    13105    3           TUEXT=(TUEXT+TUSVT+XTIME+STIME+60)/120; /* SECONDS                    */
      329    13106    3           CPUMINS=TUEXT/60;
      330    13107    3           CPUSECS=MOD(TUEXT,60);
      331    13108    3           CALL DBM$GETMEM(DCBPTR$+%(G_M$LM#*2),DCBPTR,UN,SIZEC(DCB$));
      332    13109    3           CALL DBM$PACK4(DCBPTR,DCB$);
      333    13110    3           CALL DBM$GETMEM(DCB$,FEPDCB,UN,SIZEC(FEPDCB));
      334    13111    3           CALL DAF$WRITE(VECTOR(SPYFMT),
      335    13112    3             VECTOR(MODECHAR(G$JIT.MODE)),
      336    13113    3             VECTOR(JITUN),
      337    13114    3             VECTOR(G$JIT.ACCN),
      338    13115    3             VECTOR(G$JIT.UNAME),
      339    13116    3             VECTOR(SYSID),
      340    13117    3             VECTOR(CPUMINS),
      341    13118    3             VECTOR(CPUSECS),
      342    13119    3             VECTOR(FEPDCB.NAME.C),
      343    13120    3             VECTOR(FEPDCB.ACCT));
      344    13121    3           END;
      345    13122    2      RETURN;
      346    13123
      347    13124    2   END DISPSPY;
      348    13125        %EJECT;
PL6.E3A0      #001=DBD$UTIL1 File=DBD$UTIL1.:E05TSI                              TUE 07/29/97 13:11 Page:12   
      349    13126        /*F*    NAME: DBD$STAT
      350    13127                PURPOSE: To process the STAT command for FEPs.
      351    13128
      352    13129        */
      353    13130    1   DBD$STAT: ENTRY(TOP);
      354    13131
      355    13132    1      CALL DBM$GETMEM(BOBCAT$,G$BOBCAT,0,SIZEC(G$BOBCAT),,SIZEC(G$BOBCAT));
      356    13133    1      CALL DBM$PACK4(G$BOBCAT.MMDATA$,MMDATA$);
      357    13134    1      CALL DBM$GETMEM(MMDATA$,GM$DATA,0,SIZEC(GM$DATA),,SIZEC(GM$DATA));
      358    13135    1      CALL DBM$PACK2(GM$DATA.SHUF_HI,SHUF_HI);
      359    13136
      360    13137    1      CALL DAF$INIT(STAT_RPT);
      361    13138    1      IF TOP.NSUBLKS=0
      362    13139    2      THEN DO;
      363    13140    3           DO UN=-1 DOWNTO -DB_MUIS;
      364    13141    3                CALL DISPSTAT;
      365    13142    3                END;
      366    13143    2           DA_MAPERR = '0'B;
      367    13144    2           END;
      368    13145    2      ELSE DO;
      369    13146    3           DO I=0 TO TOP.NSUBLKS-1;
      370    13147    4                CALL DBA$SELECTION(TOP.SUBLK$(I)->OUT,UN) WHENRETURN DO;
      371    13148    4                     CALL DISPSTAT;
      372    13149    4                     END;
      373    13150    3                END;
      374    13151    2           END;
      375    13152    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      376    13153    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      377    13154    1      RETURN;
