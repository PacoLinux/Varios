      310    18210
      311    18211             /*==========*/
      312    18212        %MACRO IF_THEN_DO (@LOP =, @REL =, @ROP =, @LAB =) ;
      313    18213             /*==========*/
      314    18214
      315    18215               DO ;
      316    18216                 %LSET LINE@ = CHARTEXT (CONCAT (TEXTCHAR (@LOP),
      317    18217                                                 ' -> LX_ETT_ITEM.LINE')) ;
      318    18218                 %LSET COLUMN@ = CHARTEXT (CONCAT (TEXTCHAR (@LOP),
      319    18219                                                 ' -> LX_ETT_ITEM.COLUMN')) ;
      320    18220
      321    18221                 COMPARISON$ = ADDR (COMPARISON) ;
      322    18222                 %INIT_BINARY_OP (@POINTER = COMPARISON$,
      323    18223                                  @LOP$    = @LOP,
      324    18224                                  @OP      = @REL,
      325    18225                                  @ROP$    = @ROP,
      326    18226                                  @LINE    = %LINE@,
      327    18227                                  @COLUMN  = %COLUMN@) ;
      328    18228
      329    18229                 CALL Z8AT$CREATE_TEMPORARY (%PARA, @LAB) ;
      330    18230                 CALL Z8EK$BRANCH_FALSE (@LAB, COMPARISON$) ;
      331    18231               END ;
      332    18232
      333    18233        %MEND ;
      334    18234
      335    18235        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:11   
      336    18236               /*===========================*/
      337    18237               /*V* INSPECT_TALLYING_Pass_2 */
      338    18238               /*===========================*/
      339    18239
      340    18240    1          ONE_LX_ETT$ = ADDR (ONE) ;
      341    18241    1          CALL Z8AC$LX_ETT_ITEM_TYI_CONST (1, ONE_LX_ETT$) ;
      342    18242    1          ONE$ = ONE_LX_ETT$ -> LX_ETT_ITEM.ETT$ ;
      343    18243    1          TWO_LX_ETT$ = ADDR (TWO) ;
      344    18244    1          CALL Z8AC$LX_ETT_ITEM_TYI_CONST (2, TWO_LX_ETT$) ;
      345    18245    1          TWO$ = TWO_LX_ETT$ -> LX_ETT_ITEM.ETT$ ;
      346    18246
      347    18247               %INIT_ILS_LIST (@POINTER = "ADDR(CONVERSION_ILS_LIST)") ;
      348    18255    1          PRODUCE_INSPECT_ILS = TRUE ;
      349    18256    1          ID1 = INSPECT$-> INSPECT_1_STMT.ID_1$-> LX_ETT_ITEM;
      350    18257    1          ID1$ = ADDR(ID1);
      351    18258    1          ID1_ORIG_TYPE = ID1$-> LX_ETT_ITEM.TYPE;
      352    18259    2          DO CASE (ID1_ORIG_TYPE);
      353    18260    2            CASE (%TYO, %TYF, %TYC);
      354    18261    2              TYPE1 = %TYA;
      355    18262    2            CASE (ELSE);
      356    18263    2              TYPE1 = ID1_ORIG_TYPE;
      357    18264    2          END;
      358    18265    1          IF ID1$ -> LX_ETT_ITEM.B.HIGH_LEV THEN INSPECT$ -> INSPECT_1_STMT.HIGH_LEV =
             18265                   TRUE;
      359    18266    1          CALL VERIFY_ALPHA (ID1$) ;
      360    18267
      361    18268    1          LX_LIST$ = INSPECT$ -> INSPECT_1_STMT.INSPECT_1$ ;
      362    18269    1          ID2$ = LX_LIST$ -> LX_LIST.HEAD$ ;
      363    18270    1          IF ID2$ -> LX_ETT_ITEM.B.HIGH_LEV THEN INSPECT$ -> INSPECT_1_STMT.HIGH_LEV =
