     2169    41574    2   DCL  CLAUSE$   PTR ; /* Before or After Clause */
     2170    41575
     2171    41576        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:70   
     2172    41577               /* FIND BEFORE AFTER INDEXES */
