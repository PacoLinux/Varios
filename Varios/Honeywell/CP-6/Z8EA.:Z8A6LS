     2751    42313    3              CALL Z8AT$CREATE_TEMP_WITH_BASE (%SCALAR,
     2752    42314    3                                               AB$ -> LX_ETT_ITEM.ETT$);
     2753    42315
     2754    42316                   %FISH( TRIAD = ID1$ -> LX_ETT_ITEM.ETT$,
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:90   
     2755    42317                          RTN   = ID1_ST$ );
     2756    42325
     2757    42326    3              IF  ID1_ST$ -> SCALAR.B.KANJI
     2758    42327    3              AND PRODUCE_INSPECT_ILS  THEN
     2759    42328    3                CALL BEFORE_OR_AFTER_KANJI;
     2760    42329    3              ELSE
     2761    42330    4                DO;
     2762    42331                       %INIT_BINARY_OP (@POINTER = INDEX$,
     2763    42332                                        @LOP$    = ID4$,
