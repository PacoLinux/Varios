     2587    42113        /*=================*/
     2588    42114    1     SCAN_BEFORE_AFTER: PROC (LX_ITEM$, LINK$, CLAUSE_TYPE, ID1$) ;
     2589    42115        /*=================*/
     2590    42116
     2591    42117        /*I*   NAME: SCAN_BEFORE_AFTER
     2592    42118
     2593    42119              INPUT: LX_ITEM$ -- (1) LX_TERMINAL (EMPTY),
     2594    42120                                 (2) LX_LIST (COUNT=1) [BEFORE or AFTER],
     2595    42121                              or (3) LX_LIST (COUNT=2) [BEFORE and AFTER].
     2596    42122
     2597    42123                     Each item below the initial LX_LIST looks like:
     2598    42124
     2599    42125                                    LX_LIST
     2600    42126                                   /       \
     2601    42127                          LX_TERMINAL      LX_ETT_ITEM
     2602    42128                      (BEFORE or AFTER)    (id-4)
     2603    42129
     2604    42130             OUTPUT: (1) A temp is created for each BEFORE/AFTER clause.
     2605    42131                     (2) An assignment statement is created to initialize
     2606    42132                         the temp:
     2607    42133                                    TEMP = INDEX (id-1, id-4)
     2608    42134
     2609    42135                     (3) The id-4 LX_ETT_ITEM is disposed, and replaced
     2610    42136                         by an LX_ETT_ITEM representing the temp.
     2611    42137
     2612    42138        DESCRIPTION: In addition to the tasks performed listed above
     2613    42139                     under "OUTPUT", the following is done:
     2614    42140
     2615    42141                     (1) If id-4 is not alphanumeric, it must be converted.
     2616    42142                     (2) If the clause type is LEADING or FIRST, and there
     2617    42143                         is no AFTER clause, then one must be simulated.
     2618    42144                         A temp will be created, and the following
     2619    42145                         assignment will be created:
     2620    42146                                                     Temp = 0
     2621    42147
     2622    42148                         An LX_ETT_ITEM will be created for "Temp" and
     2623    42149                         put in the LX_LIST tree (in place of "EMPTY"
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:86   
     2624    42150                         if there is no BEFORE clause).
     2625    42151
     2626    42152        */
     2627    42153
     2628    42154        /* SCAN BEFORE/AFTER */
     2629    42155
     2630    42156    2   DCL AB$         PTR ;          /* AFTER or BEFORE Temp */
     2631    42157    2   DCL AFTER$      PTR ;          /* LX_ITEM or LX_LIST   */
     2632    42158    2   DCL BEFORE$     PTR ;          /* LX_ITEM or LX_LIST   */
     2633    42159    2   DCL CLAUSE_TYPE SBIN WORD ;    /* eg, %RSV_WRD_ALL     */
     2634    42160    2   DCL ID1$        PTR ;          /* LX_ETT_ITEM          */
     2635    42161    2   DCL ID4$        PTR ;          /* LX_ETT_ITEM          */
     2636    42162    2   DCL INDEX$      PTR ;          /* INDEX operation */
     2637    42163    2   DCL LINK$       PTR ;          /* USED IF "EMPTY" IF DISPOSED */
     2638    42164    2   DCL LX_ITEM$    PTR ;          /* LX_TERMINAL, LX_ETT_ITEM, LX_LIST */
     2639    42165    2   DCL LX_LIST$    PTR ;          /* LX_LIST for Before/After clause */
     2640    42166    2   DCL RESULT$     PTR ;          /* %OP_PLUS or %OP_BINARY_MINUS */
     2641    42167
     2642    42168    2   DCL Z8E5$MATH_PACK              ENTRY (2) ;
     2643    42169    2   DCL Z8E7$MOVE                   ENTRY (2) ;
     2644    42170    2   DCL Z8ED$DISPOSE_ABSSYN_TREE    ENTRY (1) ;
     2645    42171
     2646    42172        %DCL_AUTO_LX_ITEM (@NAME = INDEXX) ;
     2647    42176        %DCL_AUTO_LX_ITEM (@NAME = RESULT) ;
     2648    42180
     2649    42181        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:87   
     2650    42182               /* SCAN BEFORE/AFTER */
     2651    42183
     2652    42184    2          AFTER$ = ADDR(NIL) ;
     2653    42185    2          BEFORE$ = ADDR(NIL) ;
     2654    42186    2          INDEX$ = ADDR (INDEXX) ;
     2655    42187    2          RESULT$ = ADDR (RESULT) ;
     2656    42188
     2657    42189    3          IF LX_ITEM$ -> LX_ITEM.ID = %LX_TERMINAL THEN DO ; /* EMPTY */
     2658    42190
     2659    42191    3            IF CLAUSE_TYPE = %RSV_WRD_FIRST
     2660    42192    4            OR CLAUSE_TYPE = %RSV_WRD_LEADING THEN DO ;
     2661    42193
     2662    42194    4              CALL CREATE_AFTER_LX_ETT_ITEM (AFTER$) ;
     2663    42195    4              CALL Z8ED$DISPOSE_ABSSYN_TREE (LX_ITEM$) ;
     2664    42196    4              LX_ITEM$ = AFTER$ ;
     2665    42197    4              IF LINK$ ~= ADDR(NIL)
     2666    42198    4              THEN LINK$ = AFTER$ ;
     2667    42199
     2668    42200    4            END ; /* FIRST or LEADING */
     2669    42201
     2670    42202    3          END ; /* Empty Before/After list */
     2671    42203
     2672    42204    3          ELSE DO ; /* There are 1 or 2 Before/After Clauses */
     2673    42205
     2674    42206    3            LX_LIST$ = LX_ITEM$ -> LX_LIST.HEAD$ ;
     2675    42207
     2676    42208    4            DO CASE (LX_LIST$ -> LX_LIST.ID) ;
     2677    42209
     2678    42210    4              CASE (%LX_TERMINAL) ; /* Delete this case after Z8EF change */
     2679    42211
     2680    42212    4                CALL BEFORE_OR_AFTER_CLAUSE (LX_ITEM$) ;
     2681    42213
     2682    42214    4              CASE (%LX_LIST) ;
     2683    42215
     2684    42216    5                DO WHILE (LX_LIST$ ~= ADDR(NIL)) ;
     2685    42217
     2686    42218    5                  CALL BEFORE_OR_AFTER_CLAUSE (LX_LIST$) ;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:88   
     2687    42219
     2688    42220    5                  LX_LIST$ = LX_LIST$ -> LX_LIST.LINK ;
     2689    42221
     2690    42222    5                END ; /* Do While LX_LIST$ <> Nil */
     2691    42223
     2692    42224    4              CASE (ELSE) ;
     2693    42225
     2694    42226                     /*N* CALL Z8AE$ERROR (Unexpected Case) */
     2695    42227
     2696    42228    4            END ; /* Case of LX_LIST ID */
     2697    42229
     2698    42230    4            IF AFTER$ = ADDR(NIL) THEN DO ;
     2699    42231
     2700    42232    4              IF CLAUSE_TYPE = %RSV_WRD_FIRST
     2701    42233    5              OR CLAUSE_TYPE = %RSV_WRD_LEADING THEN DO ;
     2702    42234
     2703    42235    5                CALL CREATE_AFTER_LX_ETT_ITEM (AFTER$) ;
     2704    42236
     2705    42237    5                LX_ITEM$ -> LX_LIST.COUNT = 2 ;
     2706    42238    5                LX_ITEM$ -> LX_LIST.TAIL$ = AFTER$ ;
     2707    42239    5                LX_ITEM$ -> LX_LIST.HEAD$ -> LX_LIST.LINK = AFTER$ ;
     2708    42240
     2709    42241    5              END ; /* FIRST or LEADING */
     2710    42242
     2711    42243    4            END ; /* AFTER$ = Nil */
     2712    42244
     2713    42245    3          END ; /* There's at least one Before/After clause */
     2714    42246
     2715    42247    2          RETURN ; /* SCAN_BEFORE_AFTER */
     2716    42248
     2717    42249        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:89   
     2718    42250        /*======================*/
     2719    42251    2     BEFORE_OR_AFTER_CLAUSE: PROC (LX_LIST$) ;
     2720    42252        /*======================*/
     2721    42253
     2722    42254        /*I*   NAME: BEFORE_OR_AFTER_CLAUSE
     2723    42255
     2724    42256        DESCRIPTION: See comments for SCAN_BEFORE_AFTER.  This
     2725    42257                     routine handles one clause:
     2726    42258                       BEFORE$ = INDEX (id4B, id1)
     2727    42259                       AFTER$  = INDEX (id4A, id1) + length (id4A)
     2728    42260
     2729    42261              INPUT: LX_LIST$ -- An LX_LIST of (LX_TERMINAL & id4).
     2730    42262
     2731    42263        */
     2732    42264
     2733    42265    3   DCL  LX_LIST$  PTR ;
     2734    42266    3   DCL  BEFORE_AFTER_TYPE SBIN WORD ; /* %RSV_WRD_BEFORE OR AFTER */
     2735    42267
     2736    42268    3              BEFORE_AFTER_TYPE = LX_LIST$ -> LX_LIST.HEAD$ -> LX_TERMINAL.SYMBOL ;
     2737    42269
     2738    42270    3              ID4$ = LX_LIST$ -> LX_LIST.TAIL$ ;
     2739    42271    3              IF INSPECT$-> INSPECT_1_STMT.KEY_WORD$->
     2740    42272    3                 LX_TERMINAL.SYMBOL = %RSV_WRD_EXAMINE  THEN
     2741    42273    3                CALL EXAMINE_ID_CHECK( ID1$, ID4$ );
     2742    42274    3              ELSE
     2743    42275    3                CALL VERIFY_ALPHA_REF_LEV ( ID4$ );
     2744    42276
     2745    42277                   %NEW (@TEMPLATE = LX_ETT_ITEM,
     2746    42278                         @POINTER  = AB$) ;
     2747    42288
     2748    42289                   %INIT_LX_ETT_ITEM (@POINTER = AB$,
     2749    42290                                      @TYPE    = %TYI) ;
