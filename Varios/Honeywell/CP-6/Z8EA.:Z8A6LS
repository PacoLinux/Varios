     1968    41067                          @REL = %OP_LESS_THAN_OR_EQUAL,
     1969    41068                          @ROP = SUB$,
     1970    41069                          @LAB = END_BEFORE_IF$) ;
     1971    41104
     1972    41105    2   END BEFORE_CHECK ;
     1973    41106
     1974    41107        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:64   
     1975    41108        /*=================*/
     1976    41109    1     LEADING_OPTIMIZED: PROC ; /* Search id1 via a TCT */
     1977    41110        /*=================*/
     1978    41111
     1979    41112        /*I*   NAME: LEADING_OPTIMIZED
     1980    41113
     1981    41114              INPUT: ID1$, AFTER$, ID3$ -- LX_ETT_ITEMs.
     1982    41115
     1983    41116             OUTPUT: POSITION$ -- LX_ETT_ITEM result.
     1984    41117
     1985    41118        DESCRIPTION: This procedure is used for optimized
     1986    41119                     INSPECT id1 TALLYING id2 FOR LEADING id3 and
     1987    41120                     INSPECT id1 REPLACING LEADING id3 BY id5.
     1988    41121                     It generates ILL to SEARCH id1 for id3.  The
     1989    41122                     result is the position of the first non id3
     1990    41123                     in id1.
     1991    41124        */
     1992    41125
     1993    41126    2   DCL    TABLE_INDEX     UBIN WORD ;
     1994    41127    2   DCL    CONST$  PTR ;
     1995    41128    2   DCL    CHAR1   CHAR (1) UNAL ;
     1996    41129    2   DCL 1  CH1     REDEF CHAR1,
     1997    41130    2        2 *       BIT  (1) UNAL,
     1998    41131    2        2 PD1     UBIN (4) UNAL, /* 1st packed decimal digit */
     1999    41132    2        2 *       UBIN (4) UNAL ;
     2000    41133    2   DCL 1  CH1B    REDEF CHAR1,
     2001    41134    2        2 MBZ     BIT  (3),
     2002    41135    2        2 BCD_CHAR BIT (6);
     2003    41136    2   DCL BCD_CHAR   BIT (6);
     2004    41137
     2005    41138    3           IF AFTER$ ~= ADDR(NIL) THEN DO ;
     2006    41139
     2007    41140                  /* Use SUBSTR (id1, INDEX(id1, id4)) instead of id1 */
     2008    41141
     2009    41142    3             CALL Z8EA$INSERT_ZERO (ID1$, AFTER$, ADDR(NIL), SUBSTR$) ;
     2010    41143    3             ID1$ -> LX_ETT_ITEM = SUBSTR$ -> LX_ETT_ITEM;
     2011    41144
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:65   
     2012    41145    3           END ; /* AFTER clause exists */
     2013    41146
     2014    41147                /* Create a constant TCT table */
     2015    41148
     2016    41149    2           IF TYPE1 = %TYB THEN
     2017    41150    3             DO ;
     2018    41151    3               ITERATION = 17 ; /* Table LCNST item size = 17 words */
     2019    41152    3               J = 64 ; /* There are 64 possible GBCD characters */
     2020    41153    3             END ;
     2021    41154    2           ELSE
     2022    41155    3             DO ;
     2023    41156    3               ITERATION = 129 ; /* Table LCNST item size = 129 words */
     2024    41157    3               J = 512 ; /* There are 512 possible 9-bit characters */
     2025    41158    3             END ;
     2026    41159                %RESERVE_LTS (ITEM = %LCNST, POINTER = LTS$) ;
     2027    41245    2           SUBSTR (LTS$ -> LCNST.CONST, 0, J) = ' ' ;
     2028    41246
     2029    41247    2           CONST$ = ID3$ -> LX_ETT_ITEM.ETT$ -> ETT.LOP ;
     2030    41248    2           CHAR1 = CONST$ -> CONST.CONSTA ;
     2031    41249
     2032    41250    3           DO CASE (TYPE1);
     2033    41251    3             CASE (%TYE);
     2034    41252    3               CALL XLATE (CHAR1, EBCDIC, CHAR1);
     2035    41253    3             CASE (%TYB);
     2036    41254    3               CALL XLATE_9_TO_6 (BCD_CHAR, GBCD, CHAR1);
     2037    41255    3               CH1B.BCD_CHAR = BCD_CHAR;
     2038    41256    3               CH1B.MBZ = '0'B;
     2039    41257    3           END;
     2040    41258    2           IF  CONST$ -> ST.NDSC.TN = %NDSC4
     2041    41259    2           AND CONST$ -> ST.TYPE    = %TYD
     2042    41260    2           THEN TABLE_INDEX = CH1.PD1 + 48 ;
     2043    41261    2           ELSE TABLE_INDEX = ASCBIN (CHAR1) ;
     2044    41262
     2045    41263    2           SUBSTR (LTS$ -> LCNST.CONST, TABLE_INDEX, 1) = BITASC ('000'O) ;
     2046    41264
     2047    41265                %NEW_CONST (@ST$  = ST$,
     2048    41266                            @TYPE = TYPE1,
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:66   
     2049    41267                            @LONG_CONSTANT = '1'B) ;
     2050    41308    2           ST$ -> CONST.CONST$.PTR$ = LTS$ ;
     2051    41309    2           CALL Z8AF$FORME (%TRIAD, ST$, ADDR(NIL), TABLE$) ;
     2052    41310
     2053    41311    2           TABLE_LX_ETT$ = ADDR (TABLE_LX_ETT) ;
     2054    41312                %INIT_LX_ETT_ITEM (@POINTER = TABLE_LX_ETT$,
     2055    41313                                   @TYPE    = %TYA) ;
     2056    41335    2           TABLE_LX_ETT$ -> LX_ETT_ITEM.ETT$ = TABLE$ ;
     2057    41336
     2058    41337                /* Generate the TCT of id1 using the table */
     2059    41338
     2060    41339    2           POSITION$ = ADDR (POSITION) ;
     2061    41340                %INIT_LX_ETT_ITEM (@POINTER = POSITION$,
     2062    41341                                   @TYPE    = %TYI) ;
     2063    41363    2           CALL Z8AT$CREATE_TEMP_WITH_BASE (%SCALAR,
     2064    41364    2                                            POSITION$ -> LX_ETT_ITEM.ETT$) ;
     2065    41365
     2066    41366    2           CALL Z8EZ$SEARCH (POSITION$,   /* Result of TCT */
     2067    41367    2                             ADDR(NIL),   /* Table Value   */
     2068    41368    2                             TABLE_LX_ETT$, /* Table       */
     2069    41369    2                             ID1$,        /* Searchee      */
     2070    41370    2                             ADDR(NIL)) ; /* Alt Return    */
     2071    41371
     2072    41372    2   END LEADING_OPTIMIZED ;
     2073    41373
     2074    41374        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:67   
     2075    41375        /*========================*/
     2076    41376    1     IF_SUBSTR_ID1_EQUALS_ID3: PROC (END_IF$) ;
     2077    41377        /*========================*/
     2078    41378
     2079    41379        /*I*   NAME: IF_SUBSTR_ID1_EQUALS_ID3
     2080    41380
     2081    41381              INPUT: ID1$, ID3$, LENGTH_ID3$, POINTER$
     2082    41382
     2083    41383             OUTPUT: END_IF$ -- a label is created.
     2084    41384
     2085    41385        DESCRIPTION: If the length of ID3 is not constant or not one,
     2086    41386                     then we must generate another test before testing
     2087    41387                     if SUBSTR (ID1) = ID3.  The first test would be:
     2088    41388                     if POINTER <= LENGTH (ID1) - LENGTH (ID3).
     2089    41389        */
     2090    41390
     2091    41391    2   DCL  END_IF$       PTR ;
     2092    41392    2   DCL  SUB$          PTR ;
     2093    41393
     2094    41394        %DCL_AUTO_LX_ITEM (@NAME = SUB) ;
     2095    41398
     2096    41399    2          CALL Z8AU$TYPE_DESCRIPTOR (ID3$ -> LX_ETT_ITEM.ETT$, ID3_DESC) ;
     2097    41400
     2098    41401    2          IF  ID3_DESC.SIZ$ = ADDR(NIL)
     2099    41402    3          AND ID3_DESC.DESC.ADSC.N = 1 THEN DO ;
     2100    41403
     2101    41404                 /* IF SUBSTR (ID1, POINTER, 1) = ID3 THEN DO */
     2102    41405
     2103    41406    3            CALL Z8EA$INSERT_ZERO (ID1$, POINTER$, LENGTH_ID3$, SUBSTR$) ;
     2104    41407
     2105    41408                 %IF_THEN_DO (@LOP = SUBSTR$,
     2106    41409                              @REL = %OP_EQUAL,
     2107    41410                              @ROP = ID3$,
     2108    41411                              @LAB = END_IF$) ;
     2109    41446
     2110    41447    3          END ; /* Length (id3) = 1 */
     2111    41448
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:68   
     2112    41449    3          ELSE DO ;
     2113    41450
     2114    41451                 /* IF POINTER <= Length (id1) - Length (id3) THEN DO */
     2115    41452
     2116    41453    3            SUB$ = ADDR (SUB) ;
     2117    41454                 %INIT_BINARY_OP (@POINTER = SUB$,
     2118    41455                                  @LOP$    = LENGTH_ID1$,
     2119    41456                                  @OP      = %OP_BINARY_MINUS,
     2120    41457                                  @ROP$    = LENGTH_ID3$,
     2121    41458                                  @LINE    = "ID1$ -> LX_ETT_ITEM.LINE",
     2122    41459                                  @COLUMN  = "ID1$ -> LX_ETT_ITEM.COLUMN") ;
     2123    41477
     2124    41478                 %IF_THEN_DO (@LOP = POINTER$,
     2125    41479                              @REL = %OP_LESS_THAN_OR_EQUAL,
     2126    41480                              @ROP = SUB$,
     2127    41481                              @LAB = END_IF$) ;
     2128    41516
     2129    41517                 /* IF SUBSTR (id1, POINTER, Length (id3)) = id3 THEN DO */
     2130    41518
     2131    41519    3            CALL Z8EA$INSERT_ZERO (ID1$, POINTER$, LENGTH_ID3$, SUBSTR$) ;
     2132    41520
     2133    41521    3            COMPARISON$ = ADDR (COMPARISON) ;
     2134    41522
     2135    41523                 %INIT_BINARY_OP (@POINTER = COMPARISON$,
     2136    41524                                  @LOP$    = SUBSTR$,
     2137    41525                                  @OP      = %OP_EQUAL,
     2138    41526                                  @ROP$    = ID3$,
     2139    41527                                  @LINE    = "ID3$ -> LX_ETT_ITEM.LINE",
     2140    41528                                  @COLUMN  = "ID3$ -> LX_ETT_ITEM.COLUMN") ;
     2141    41546
     2142    41547    3            CALL Z8EK$BRANCH_FALSE (END_IF$, COMPARISON$) ;
     2143    41548
     2144    41549    3          END ;
     2145    41550
     2146    41551    2   END IF_SUBSTR_ID1_EQUALS_ID3 ;
     2147    41552
     2148    41553        %EJECT;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:69   
