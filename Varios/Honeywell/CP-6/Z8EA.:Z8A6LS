      310    18210
      311    18211             /*==========*/
      312    18212        %MACRO IF_THEN_DO (@LOP =, @REL =, @ROP =, @LAB =) ;
      313    18213             /*==========*/
      314    18214
      315    18215               DO ;
      316    18216                 %LSET LINE@ = CHARTEXT (CONCAT (TEXTCHAR (@LOP),
      317    18217                                                 ' -> LX_ETT_ITEM.LINE')) ;
      318    18218                 %LSET COLUMN@ = CHARTEXT (CONCAT (TEXTCHAR (@LOP),
      319    18219                                                 ' -> LX_ETT_ITEM.COLUMN')) ;
      320    18220
      321    18221                 COMPARISON$ = ADDR (COMPARISON) ;
      322    18222                 %INIT_BINARY_OP (@POINTER = COMPARISON$,
      323    18223                                  @LOP$    = @LOP,
      324    18224                                  @OP      = @REL,
      325    18225                                  @ROP$    = @ROP,
      326    18226                                  @LINE    = %LINE@,
      327    18227                                  @COLUMN  = %COLUMN@) ;
      328    18228
      329    18229                 CALL Z8AT$CREATE_TEMPORARY (%PARA, @LAB) ;
      330    18230                 CALL Z8EK$BRANCH_FALSE (@LAB, COMPARISON$) ;
      331    18231               END ;
      332    18232
      333    18233        %MEND ;
      334    18234
      335    18235        %EJECT;
