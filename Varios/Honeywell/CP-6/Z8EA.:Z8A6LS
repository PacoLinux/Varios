     2408    41867    4                END;
     2409    41868    3            END; /* LEVEL >= %INT_LEV */
     2410    41869
     2411    41870        /*================*/
     2412    41871    2     VERIFY_ALPHA_REF: ENTRY (ID$);
     2413    41872        /*================*/
