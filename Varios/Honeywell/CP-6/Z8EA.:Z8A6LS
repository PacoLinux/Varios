     2333    41792
     2334    41793    3            CALL Z8E7$MOVE (TEMP$, ID$) ; /* TYD = TYO */
     2335    41794
     2336    41795                 /* Create Conversion Move to be linked in later */
     2337    41796
     2338    41797    3            SAVED_ILS_LIST = O1COM.ILS_LIST ;
     2339    41798    3            O1COM.ILS_LIST = CONVERSION_ILS_LIST ;
