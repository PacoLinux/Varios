     2177    41582    3          DO CASE (LX_ITEM$ -> LX_ITEM.ID) ;
     2178    41583
     2179    41584    3            CASE (%LX_TERMINAL) ; /* EMPTY */
     2180    41585
     2181    41586                   /* No After or Before clauses */
     2182    41587
     2183    41588    3            CASE (%LX_ETT_ITEM) ;
