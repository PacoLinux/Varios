     2344    41803    3            O1COM.ILS_LIST = SAVED_ILS_LIST ;
     2345    41804
     2346    41805    3            CALL Z8EV$CONVERT_TYA (TEMP$, TEMP$, ID1$) ; /* Convert to Alpha */
     2347    41806
     2348    41807    4            DO CASE (ID$ -> LX_ETT_ITEM.TYPE) ;
     2349    41808    4              CASE (%TYO) ;
     2350    41809    4                TYPE1 = %TYA ;
     2351    41810    4              CASE (%TYF) ;
     2352    41811    4                TYPE1 = %TYE ;
     2353    41812    4              CASE (%TYC) ;
     2354    41813    4                TYPE1 = %TYB ;
     2355    41814    4            END ;
     2356    41815
     2357    41816    3          END ; /* ID$ is type overpunched (TYO) or GBCD or EBCDIC numeric */
     2358    41817
     2359    41818    3          ELSE DO ; /* It ain't overpunched or GBCD or EBCDIC numeric */
     2360    41819
     2361    41820    3            TYPE1 = ID$ -> LX_ETT_ITEM.TYPE ;
     2362    41821
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:76   
     2363    41822    3            TEMP$ -> LX_ITEM = ID$ -> LX_ITEM ;
     2364    41823    3            CALL VERIFY_ALPHA (TEMP$) ;
     2365    41824
     2366    41825    3          END ;
