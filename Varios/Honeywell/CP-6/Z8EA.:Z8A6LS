     3036        1        /*T***********************************************************/
     3037        2        /*T*                                                         */
     3038        3        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
     3039        4        /*T*                                                         */
     3040        5        /*T***********************************************************/
     3041        6        /*================*/
     3042        7          Z8EA$SUBSTR_ZERO: PROC (ALPHA$, START$, LENGTH$, RESULT$) ;
     3043        8        /*================*/
     3044        9
     3045       10        /*I*   NAME: Z8EA$SUBSTR_ZERO
     3046       11
     3047       12        DESCRIPTION: Does a zero based SUBSTR (a la PL6).  Adds one
     3048       13                     to START$, then calls the one based SUBSTR (a la
     3049       14                     Cobol-85).
     3050       15
     3051       16              INPUT: Same as Z8EY$SUBSTR_NO_SBCHK.
     3052       17
     3053       18             OUTPUT: RESULT$.  All args are LX_ETT_ITEMS.
     3054       19
     3055       20        */
     3056       21
     3057       22        %EQU PHASE = 1 ;
     3058       23        %INCLUDE Z8A_DEFC ;
     3059       91        %INCLUDE Z8A_SUPP_MAC ;
     3060     2491        %INCLUDE Z8A_TYPES ;
     3061     2665        %INCLUDE Z8E_MACROS ;
     3062     3340        %INCLUDE Z8E_STACKS ;
     3063     5606
     3064     5607    1   DCL  ALPHA$   PTR ;
     3065     5608    1   DCL  START$   PTR ;
     3066     5609    1   DCL  LENGTH$  PTR ;
     3067     5610    1   DCL  RESULT$  PTR ;
     3068     5611    1   DCL  ADD$     PTR ;
     3069     5612    1   DCL  ONE_LX_ETT$   PTR ;
     3070     5613    1   DCL  CONV_FLAG BOOLEAN;
     3071     5614
     3072     5615    1   DCL  Z8AC$LX_ETT_ITEM_TYI_CONST  ENTRY (2) ;
PL6.E3A0      #002=Z8EA$SUBSTR_ZERO File=Z8EA.:Z8A6TSI                           SAT 08/09/97 11:47 Page:470  
     3073     5616    1   DCL  Z8E5$MATH_PACK  ENTRY (2) ;
     3074     5617    1   DCL  Z8EY$SUBSTR_NO_SBCHK ENTRY (4) ;
     3075     5618    1   DCL  Z8EY$INSERT     ENTRY (4) ;
     3076     5619
     3077     5620        %DCL_AUTO_LX_ITEM (@NAME = ADD) ;
     3078     5624        %DCL_AUTO_LX_ITEM (@NAME = ONE_LX_ETT) ;
     3079     5628
     3080     5629    1          CONV_FLAG = TRUE ;
     3081     5630    1          GOTO SUBSTR_COMMON ;
     3082     5631
     3083     5632        /*================*/
     3084     5633    1     Z8EA$INSERT_ZERO: ENTRY (ALPHA$, START$, LENGTH$, RESULT$) ;
     3085     5634        /*================*/
     3086     5635    1          CONV_FLAG = FALSE ;
     3087     5636    1   SUBSTR_COMMON:
     3088     5637
     3089     5638    1          ADD$ = ADDR (ADD) ;
     3090     5639    1          ONE_LX_ETT$ = ADDR (ONE_LX_ETT) ;
     3091     5640    1          CALL Z8AC$LX_ETT_ITEM_TYI_CONST (1, ONE_LX_ETT$) ;
     3092     5641
     3093     5642               %INIT_BINARY_OP (@POINTER = ADD$,
     3094     5643                                @LOP$    = START$,
     3095     5644                                @OP      = %OP_PLUS,
     3096     5645                                @ROP$    = ONE_LX_ETT$,
     3097     5646                                @LINE    = "ALPHA$ -> LX_ETT_ITEM.LINE",
     3098     5647                                @COLUMN  = "ALPHA$ -> LX_ETT_ITEM.COLUMN") ;
     3099     5665
     3100     5666    1          CALL Z8E5$MATH_PACK (ADD$, ADD$) ;
     3101     5667
     3102     5668    1          IF CONV_FLAG
     3103     5669    1            THEN CALL Z8EY$SUBSTR_NO_SBCHK (ALPHA$, ADD$, LENGTH$, RESULT$) ;
     3104     5670    1            ELSE CALL Z8EY$INSERT (ALPHA$, ADD$, LENGTH$, RESULT$) ;
     3105     5671
     3106     5672    1   END Z8EA$SUBSTR_ZERO ;
