     2415    41874    2          CALL VERIFY_ALPHA (ID$) ;
     2416    41875
     2417    41876    3          IF ID$ -> LX_ETT_ITEM.B.ERROR = '0'B THEN DO ;
     2418    41877
     2419    41878                 %FISH (TRIAD = ID$ -> LX_ETT_ITEM.ETT$, RTN = ST$) ;
     2420    41886
