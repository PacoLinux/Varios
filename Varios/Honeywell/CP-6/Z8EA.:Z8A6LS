     2328    41787
     2329    41788    3            CALL Z8AU$TYPE_DESCRIPTOR (ID$ -> LX_ETT_ITEM.ETT$, TYPE_DESC) ;
     2330    41789    3            BASE$ -> ETT.LOP -> ST.DESCL  = TYPE_DESC.DESC.DESCL ;
     2331    41790    3            BASE$ -> ETT.LOP -> ST.ADSC.CN = 0 ; /*#34773*/
