     2289    41694        /*================*/
     2290    41695    1     VERIFY_ALPHA_DEF: PROC (ID$, TEMP$) ;
     2291    41696        /*================*/
     2292    41697
     2293    41698        /*I*   NAME: VERIFY_ALPHA_DEF
     2294    41699              INPUT: ID$ -- an LX_ETT_ITEM
     2295    41700          INTERFACE: VERIFY_ALPHA
     2296    41701        DESCRIPTION: This routine is called to verify and convert an
     2297    41702                     alphanumeric definition (eg, id1 of INSPECT id1 ...).
     2298    41703                     If the conversion consists of references triads
     2299    41704                     only (OFF, FISS, DNA), then no temp is required
     2300    41705                     and VERIFY_ALPHA can be used, but if
     2301    41706                     ID1$ is overpunched, then the original ID1$ must
     2302    41707                     be preserved before the conversion so that we
     2303    41708                     can convert back to overpunched after the INSPECT
     2304    41709                     or whatever:
     2305    41710                       INSPECT overpunched REPLACING ...
     2306    41711                     becomes
     2307    41712                       MOVE overpunched TO temp.
     2308    41713                       INSPECT temp REPLACING ...
     2309    41714                       MOVE temp TO overpunched.
     2310    41715        */
     2311    41716
     2312    41717    2   DCL   ID$      PTR ; /* Operand to convert to alpha (%TYA) */
     2313    41718    2   DCL   TEMP$    PTR ; /* Created id1 or original id1 */
     2314    41719    2   DCL   BASE$    PTR ;
     2315    41720
     2316    41721    2   DCL 1 TYPE_DESC  ALIGNED,
     2317    41722             %TYPE_DESC ;
     2318    41756
     2319    41757    2          IF ID$ -> LX_ETT_ITEM.TYPE = %TYO OR
     2320    41758    2             ID$ -> LX_ETT_ITEM.TYPE = %TYF OR
     2321    41759    3             ID$ -> LX_ETT_ITEM.TYPE = %TYC THEN DO ;
     2322    41760
     2323    41761                 %INIT_LX_ETT_ITEM (@POINTER = TEMP$,
     2324    41762                                    @TYPE = %TYD) ;
     2325    41784    3            CALL Z8AT$CREATE_TEMP_WITH_BASE (%SCALAR, BASE$) ;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:75   
     2326    41785    3            TEMP$ -> LX_ETT_ITEM.ETT$ = BASE$ ;
