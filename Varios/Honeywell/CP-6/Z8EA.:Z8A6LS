     2475    41941    1     EXAMINE_ID_CHECK:   PROC ( ID1$, ID2$ );
     2476    41942        /*-----------------*/
     2477    41943
     2478    41944    2       DCL ID1$           PTR;
     2479    41945    2       DCL ID2$           PTR;
     2480    41946    2       DCL LINK$          PTR;
     2481    41947
     2482    41948        /*V*         Convert_ID2
     2483    41949              If ID2 is %TYT then it must be converted to:
     2484    41950                 %TYA if it is an alphanumeric literal or figurative constant
     2485    41951                 %TYD if it is a numeric literal
     2486    41952        */
     2487    41953
     2488    41954    2       IF ID2$-> LX_ETT_ITEM.TYPE = %TYT THEN
     2489    41955    2         IF ID2$-> LX_TERMINAL.DNT$-> DNT.B.ALPHA_LITERAL
     2490    41956    2         OR ID2$-> LX_TERMINAL.DNT$-> DNT.B.FIGURATIVE_CONSTANT  THEN
     2491    41957    2           CALL Z8EV$CONVERT_TYA (ID2$, ID2$, ID1$) ;
     2492    41958    2         ELSE
     2493    41959    3           DO;
     2494    41960    3             LINK$ = ID2$ -> LX_TERMINAL.LINK;
     2495    41961    3             CALL Z8EV$CONVERT( %TYD, ID2$, ID2$ );
     2496    41962    3             ID2$ -> LX_TERMINAL.LINK = LINK$;
     2497    41963    3           END;
     2498    41964
     2499    41965        /*V*          Compare_Types_ID1_ID2
     2500    41966              Id1 and Id2 must both be alphanumeric or they must both
     2501    41967              be numeric and regardless of type must be usage display.
     2502    41968        */
     2503    41969
     2504    41970    3       DO CASE ( ID1_ORIG_TYPE );
     2505    41971
     2506    41972    3         CASE (%TYA, %TYN, %TYE, %TYB) ;
     2507    41973
     2508    41974    4           DO CASE ( ID2$-> LX_ETT_ITEM.TYPE );
     2509    41975
     2510    41976    4             CASE (%TYA, %TYN, %TYE, %TYB) ;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:82   
     2511    41977
     2512    41978    4               CALL VERIFY_ALPHA_REF ( ID2$ );
     2513    41979
     2514    41980    4             CASE( ELSE );
     2515    41981
     2516    41982    4               CALL Z8AE$ERROR ( %E$ALPHANUMERIC_TYPE_EXPECTED,
     2517    41983    4                                 ID2$-> LX_ITEM.LINE,
     2518    41984    4                                 ID2$-> LX_ITEM.COLUMN );
     2519    41985    4           END; /* case id2 type */
     2520    41986
     2521    41987    3         CASE( ELSE );
     2522    41988
     2523    41989    4           DO CASE ( ID2$-> LX_ETT_ITEM.TYPE );
     2524    41990
     2525    41991    4              CASE (%TYA, %TYN, %TYE, %TYB) ;
     2526    41992
     2527    41993                     %CONDITIONAL_ERROR (@LX_ITEM$ = ID2$,
     2528    41994                                         @ERROR#   = %E$NUMERIC_TYPE_EXPECTED,
     2529    41995                                         @LINE     = "ID2$-> LX_ITEM.LINE",
     2530    41996                                         @COLUMN   = "ID2$-> LX_ITEM.COLUMN" );
     2531    42026    4              CASE( ELSE );
     2532    42027
     2533    42028    4                CALL Z8CO$DISPL_CHECK ( ID2$ );
     2534    42029
     2535    42030    4            END; /* case id2 type */
     2536    42031    3        END; /* case id1 type */
     2537    42032
     2538    42033
     2539    42034
     2540    42035        /*V*         Id2_Length_Check
     2541    42036             Length check: id2 should be 1 byte long.
     2542    42037        */
     2543    42038    2       CALL Z8AU$TYPE_DESCRIPTOR( ID2$-> LX_ETT_ITEM.ETT$, ID3_DESC );
     2544    42039    2       IF  ID1$ -> LX_ITEM.TYPE ~= %TYN
     2545    42040    2       AND ID3_DESC.DESC.ADSC.N > 1 THEN
