     2434    41900        /*================*/
     2435    41901    1     VERIFY_ALPHA: PROC (ID$) ;
     2436    41902        /*================*/
     2437    41903
     2438    41904        /*I*   NAME: VERIFY_ALPHA
     2439    41905              INPUT: ID$ -- operand to verify and convert to TYA.
     2440    41906             OUTPUT: ID$ -- converted.
     2441    41907        DESCRIPTION: If the type of ID$ is not alphanumeric, then
     2442    41908                     convert it.  If it's not USAGE DISPLAY, then
     2443    41909                     issue a diagnostic.  If it's no-sign-DISPLAY
     2444    41910                     (eg, PIC 9999), then use the DNA triad.  If
     2445    41911                     it's trailing-sign-DISPLAY, then use a DNA and
     2446    41912                     a FISS.  If it's leading-sign-DISPLAY, use a
     2447    41913                     DNA, a FISS, and an OFF.
     2448    41914        */
     2449    41915
     2450    41916    2   DCL  ID$  PTR ;
     2451    41917
     2452    41918    2          CALL Z8CO$DISPL_CHECK (ID$)
     2453    41919
     2454    41920    3          WHENRETURN DO ;
     2455    41921
     2456    41922    3            IF  INSPECT$-> INSPECT_1_STMT.KEY_WORD$->
     2457    41923    3                LX_TERMINAL.SYMBOL = %RSV_WRD_INSPECT
     2458    41924    3            AND ID$ -> LX_ETT_ITEM.TYPE ~= %TYN
     2459    41925    3            AND ID$ -> LX_ETT_ITEM.TYPE ~= %TYA THEN
     2460    41926    3              CALL Z8EV$CONVERT_TYA (ID$, ID$, ID1$) ;
     2461    41927
     2462    41928    3          END ;
     2463    41929
     2464    41930    3          WHENALTRETURN DO ;
     2465    41931
     2466    41932    3            PRODUCE_INSPECT_ILS = FALSE ;
     2467    41933
     2468    41934    3          END ;
     2469    41935
     2470    41936    2          RETURN ;
PL6.E3A0      #001=Z8EA$INSPECT_1 File=Z8EA.:Z8A6TSI                             SAT 08/09/97 11:39 Page:80   
     2471    41937
     2472    41938    2   END VERIFY_ALPHA ;
