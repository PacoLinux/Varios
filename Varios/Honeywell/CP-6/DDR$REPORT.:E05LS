        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MOC,DCI=5,CRT=5 */
        8        8        /*P*  NAME: DDR$REPORT
        9        9             PURPOSE: to report breaks, faults, and traces to the user. */
       10       10        /*D* NAME: DDR$REPORT
       11       11             CALL: CALL DDR$REPORT( EVENT );
       12       12             DESCRIPTION: TYPE IS THE CODE OF THE EVENT TO BE REPORTED.
       13       13                  IF USER DELTA AND THE PSEUDO SENSE SWITCH ARE 'TSTn' WHERE
       14       14                  n IS ANY CHARACTER, DELTA WILL NOT GIVE A VERSION NUMBER.
       15       15                  IF THE SWITCHES ARE 'TST0' THEN REPORT WILL NOT INCLUDE A
       16       16                  POSITION IN ITS MESSAGES. */
       17       17                                                /**/
       18       18        DDR$REPORT: PROC( EVENT );
       19       19                                                /**/
       20       20    1   DCL TEMP UBIN;
