/*X* DMC,DFC */
CP6ARC_NODES(D,OUT) = .fid FILE (HOW) .fid '(' < ',', OPTION > [')']  .END
FILE(57) = .fid
HOW = (ON|OVER|TO)
OVER(1) = 'OVER'
ON(2) = 'ON'
TO(3) = 'TO'
OPTION = (STD | COMP)
STD(11) = 'STD'
COMP(12) = 'C/OMPRESS'
