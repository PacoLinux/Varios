!DEFAULT SIACCT=:XSI,GNACCT=:XSI,LBACCT=:LIBRARY, XACCT=:XSI
!DEFAULT DEST=LP
!JOB  NAME=GOFER,WSN=LOCAL
!RESOURCE TIME=5,MEM=256
!ROUTE Mike Mulhaupt, ACTC
!"
!"  By default, the output created by this job is sent to LP.  If you
!"  don't want output, set DEST=NO
!"
!IF $FID_EXIST('TO.:SYS') & $MODE='BATCH' THEN TO AC=%$ACCT Your GOFER compile just started.
!PL6 GOFER_SI61.SIACCT OVER *GOFER_OU61.GNACCT,DEST (SC,OU,LS,SR(.SIACCT,.LBACCT,.:E04OU))
!BMAP GOFER_SIB1.SIACCT OVER *GOFER_OUB1.GNACCT,DEST
!PARTRGE GOFER_SIN1.SIACCT INTO *GOFER_OUN1.GNACCT,DEST (SR(.SIACCT,.LBACCT))
!LINK B_COPYRIGHT.LBACCT, ;
      *GOFER_OUB1.GNACCT,;
      *GOFER_OU61.GNACCT,;
      *GOFER_OUN1.GNACCT;
 OVER GOFER.GNACCT (PRIV=(FMBLK),NDCB1,NDCB2,NDCB3,NDCB4)
!IF $FID_EXIST('TO.:SYS') & $MODE='BATCH' THEN TO AC=%$ACCT Your GOFER compile just finished.
