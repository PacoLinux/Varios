        1        1        /*M*     ZIG$D_READ       I-D-S/II  DBACS DDL DIRECTIVE READER */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*P*
        8        8        NAME:    ZIG$D_READ
        9        9        PURPOSE: TO GET A LINE FOR THE PARSER OF DBACS DIRECTIVES
       10       10                 AND STORE THEM IN A BUFFER
       11       11            CALLING PROGRAMS         SUBROUTINE NAMES
       12       12            ----------------         ----------------
       13       13                 ZIG$D_PSET               ZIG$D_PSET
       14       14                 ZIG$D_PDRI               ZIG$D_PDRI
       15       15        */
       16       16        ZIG$D_READ:
       17       17            PROC ;
       18       18    1   DCL RDWORD BIT (1) EXT;
       19       19    1   DCL ZIG$DDS5 ENTRY;
       20       20    1   DCL REC CHAR (80) EXT;
       21       21    1   DCL REC1 CHAR (80) EXT;
       22       22    1   DCL EOFFLG UBIN (36) EXT;
       23       23    1   DCL LINE_COUNT UBIN (36) EXT;
       24       24    1   DCL SAVESW BIT (1) EXT;
       25       25    1   DCL NUM_CHAR_READ UBIN (36) EXT;
       26       26    1   DCL LINE CHAR (4000) EXT;
       27       27    1   DCL LAST_COUNT UBIN (36) SYMREF;
       28       28    1   DCL LIST_STRING CHAR(128) EXT;
       29       29    1   DCL ZIG$COMP ENTRY (3); /* Put out a compressed record */
       30       30    1   DCL FOUND_PER BIT (1) SYMREF;
       31       31    1   DCL PUT_OUT_LINE ENTRY(2); /* Put out a record to M$LO */
       32       32    1   DCL INIT1 BIT (1);
       33       33    1   DCL NCS UBIN (36) EXT;
       34       34    1   DCL I UBIN (36);
       35       35    1   DCL K UBIN (36);
       36       36    1   DCL SAVE_I UBIN (36);
       37       37    1   DCL L UBIN WORD;
PL6.E3A0      #001=ZIG$D_READ File=ZIG$D_READ.:ZIC0TSI                           FRI 09/05/97 12:02 Page:2    
       38       38    1   DCL END_DDL BIT (1) EXT;
       39       39    1   DCL DONE BIT (1);
       40       40    1   DCL ZIG$D_DIAG ENTRY (3);
       41       41        %EQU WARN_SEV = 1;
       42       42         %EJECT;
PL6.E3A0      #001=ZIG$D_READ File=ZIG$D_READ.:ZIC0TSI                           FRI 09/05/97 12:02 Page:3    
       43       43         %INCLUDE XU_MACRO_C;
       44     3149         %INCLUDE ZI_ERRORS_SUBS;
       45     3669         %P$PCB(NAME=DDL_P, STCLASS = "SYMREF");
       46     3870         %INCLUDE ZIG_GENERAL;
       47     3978         %EJECT;
PL6.E3A0      #001=ZIG$D_READ File=ZIG$D_READ.:ZIC0TSI                           FRI 09/05/97 12:02 Page:4    
       48     3979    1   NEXT_REC: PROC;
       49     3980    2       CALL ZIG$DDS5; /* Get a record */
       50     3981    2       CALL PUT_OUT_LINE(LINE_COUNT,REC);
       51     3982    2       LINE_COUNT=LINE_COUNT+1;
       52     3983    2       REC1=REC;
       53     3984    2       CALL ZIG$COMP(NUM_CHAR_READ,'DD  ',REC1);
       54     3985    2   END;
       55     3986        /*****
       56     3987                 Find then number of characters read in the last
       57     3988                 read done by exread.
       58     3989        ******/
       59     3990    1   RESTART:
       60     3991    1       CALL NEXT_REC;
       61     3992    1       IF NCS=1
       62     3993    2       THEN DO;
       63     3994    2           RDWORD='0'B;
       64     3995    2           SAVESW='1'B;
       65     3996    2           CALL ZIG$D_DIAG(LINE_COUNT - 2,%WARN_SEV,2185); /* No END_DDL */
       66     3997        /*E*     ERROR:    ZIG-2185-C
       67     3998        MESSAGE: No END_DDL found at the end of the DDL text.
       68     3999        */
       69     4000    2           END_DDL='1'B;
       70     4001    2           RETURN;
       71     4002    2           END;
       72     4003    1       L = 0;
       73     4004    1       CALL INDEX1(I,';',REC1,L);
       74     4005    2       DO WHILE (I ~= LENGTHC(REC1));
       75     4006    2           CALL INSERT(REC1,I,1,' ');
       76     4007    2           L = I;
       77     4008    2           CALL INDEX1(I,';',REC1,L);
       78     4009    2           END;
       79     4010    1       I=0;
       80     4011    1       CALL INDEX(I,'COMMENT"',REC1) ALTRET(NO_COMMENT);
       81     4012    1       SAVE_I = I;
       82     4013    1       DONE='0'B;
       83     4014    1       I=I+7;
       84     4015    2       DO WHILE (SUBSTR(REC1,I,1)~='"'&~DONE);
PL6.E3A0      #001=ZIG$D_READ File=ZIG$D_READ.:ZIC0TSI                           FRI 09/05/97 12:02 Page:5    
       85     4016    2           IF SUBSTR(REC1,I,1)~=' '
       86     4017    3           THEN DO;
       87     4018    3               I=0;
       88     4019    3               DONE='1'B;
       89     4020    3               END;
       90     4021    3           ELSE DO;
       91     4022    3               I=I+1;
       92     4023    3               IF I>NUM_CHAR_READ
       93     4024    4               THEN DO;
       94     4025    4                   CALL NEXT_REC;
       95     4026    4                   I=0;
       96     4027    4                   SAVE_I = 0;
       97     4028    4                   END;
       98     4029    3               END;
       99     4030    2           END;
      100     4031    1       I=I+1;
      101     4032    2       DO WHILE(SUBSTR(REC1,I,1)~='"'&~DONE);
      102     4033    2           I=I+1;
      103     4034    2           IF I>NUM_CHAR_READ
      104     4035    3           THEN DO;
      105     4036    3               CALL NEXT_REC;
      106     4037    3               I=0;
      107     4038    3               SAVE_I = 0;
      108     4039    3               END;
      109     4040    3           DO WHILE (SUBSTR(REC1,I,2)='""');
      110     4041    3               I=I+2;
      111     4042    3               END;
      112     4043    2           END;
      113     4044    1       IF ~DONE
      114     4045    2       THEN DO;
      115     4046    2           CALL CONCAT(REC1,SUBSTR(REC1,0,SAVE_I),SUBSTR(REC1,I));
      116     4047    2           NUM_CHAR_READ=NUM_CHAR_READ - I + SAVE_I;
      117     4048    2           END;
      118     4049    1   NO_COMMENT:
      119     4050    1       IF REC1 = ' '
      120     4051    1       THEN GOTO RESTART;
      121     4052        /*
PL6.E3A0      #001=ZIG$D_READ File=ZIG$D_READ.:ZIC0TSI                           FRI 09/05/97 12:02 Page:6    
      122     4053              Goto easy way out.
      123     4054        */
      124     4055    1       LAST_COUNT = DDL_P.NCHARS;
      125     4056    1       I = NUM_CHAR_READ-1;
      126     4057        /*********
      127     4058        	scan from back of record to find the first non
      128     4059        	blank CHARACTER.(FIND the end of the record).
      129     4060        **********/
      130     4061    1       CALL SEARCHR(I,K,SEARCHR_NB,REC1);
      131     4062    1       CALL INSERT(LINE,DDL_P.NCHARS,I+2,SUBSTR(REC1,0,I+1),' ');
      132     4063    1       DDL_P.NCHARS = DDL_P.NCHARS + I + 1;
      133     4064        /********
      134     4065        	don't add a ';' if this is the end of the directive.
      135     4066        *********/
      136     4067    1       FOUND_PER='0'B;
      137     4068    1       CALL INDEX2R(K,'. ',REC) ALTRET (NO_PERIOD);
      138     4069    1       IF K > I-2
      139     4070    2       THEN DO;
      140     4071    2           FOUND_PER = '1'B;
      141     4072    2           END;
      142     4073    1   NO_PERIOD:
      143     4074    1       IF ~FOUND_PER
      144     4075    2       THEN DO;
      145     4076    2           CALL INSERT(LINE,DDL_P.NCHARS,3,'  ;');
      146     4077    2           DDL_P.NCHARS=DDL_P.NCHARS + 3;
      147     4078    2           END;
      148     4079        /*********
      149     4080        	replace all ';' with '%' so that X$PARSE doesn't
      150     4081        	think that what is in '"' is a comment and skips
      151     4082        	over it.
      152     4083        *********/
      153     4084    1   END ZIG$D_READ;

