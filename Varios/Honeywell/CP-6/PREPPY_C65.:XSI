/*X* DDA */
 
%EQU    #OPTIONS        = 1 ; /* Options list node                            */
%EQU    #CSYS           = 2 ; /* Create system file                           */
%EQU    #NSYS           = 3 ; /* Don't use system file                        */
%EQU    #START          = 4 ; /* Start                                       */
%EQU    #FEP             = 5 ; /* Generate FEP ou                              */
%EQU    #CP6            = 6 ; /* Generate L66-CP6 ou                         */
%EQU    #SEV            = 8 ; /* Generate lo dm pm xr ur if under severity lvl */
%EQU    #AUTO_VAL       = 9 ; /* Autotemp approx. Value                      */
%EQU    #LEV            = 10 ; /* Nesting level in LS                         */
%EQU    #NLEVEL         = 11 ; /* No nesting level in ls                      */
%EQU    #SYN            = 12 ; /* Syntax check only                           */
%EQU    #DEBUG          = 13 ; /* Debug indicator value                       */
%EQU    #LU             = 14 ; /* List updates                                */
%EQU    #NLU            = 15 ; /* Dont list updates                           */
%EQU    #FORMAT         = 16 ; /* Format listing                              */
%EQU    #NFORMAT        = 17 ; /* Dont format listing                         */
%EQU    #OPTIM          = 18 ; /* Optimize                                    */
%EQU    #NLPS           = 19 ; /* Dont list preprocessed source               */
%EQU    #LPS            = 20 ; /* List preprocessed source                    */
%EQU    #SYS            = 21 ; /* Use the system file                         */
%EQU    #SHARED_LIB     = 22 ; /* Use a specified shared library              */
%EQU    #NOSH           = 23 ; /* Don't use any shared library                */
%EQU    #ASL            = 24 ; /* Use a specified alternate shared library    */
%EQU    #EOD            = 25 ; /* Write a %EOD to the preprocessed source     */
%EQU    #NEOD           = 26 ; /* Don't write a %EOD to the preprocessed source     */
%EQU    #USC            = 27 ; /* Use standard character set */
%EQU    #NUSC           = 28 ; /* Do not error on non-standard chars */
