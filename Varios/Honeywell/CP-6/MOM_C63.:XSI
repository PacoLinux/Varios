        UENTRY=DG_USERCOUNTS,
        RMULT=1,
        NFLDS=12,
        FIELD1=SCRN1_FIELDS.CNT_ONLINE,
        FIELD2=SCRN1_FIELDS.CNT_EDIT,
        FIELD3=SCRN1_FIELDS.CNT_MAIL,
        FIELD4=SCRN1_FIELDS.CNT_PCL,
        FIELD5=SCRN1_FIELDS.CNT_COMPILE,
        FIELD6=SCRN1_FIELDS.CNT_STAT,
        FIELD7=SCRN1_FIELDS.CNT_IBEX,
        FIELD8=SCRN1_FIELDS.CNT_EXEC,
        FIELD9=SCRN1_FIELDS.CNT_BATCH,
        FIELD10=SCRN1_FIELDS.CNT_GHOST,
        FIELD11=SCRN1_FIELDS.CNT_TP,
        FIELD12=SCRN1_FIELDS.CNT_TOTAL);
%ROUTINE(NAME=STATS,
        UENTRY=DG_STATS,
        RMULT=1,
        NFLDS=5,
        FIELD1=SCRN1_FIELDS.STATS_EXEC,
        FIELD2=SCRN1_FIELDS.STATS_SERV,
        FIELD3=SCRN1_FIELDS.STATS_WAIT,
        FIELD4=SCRN1_FIELDS.STATS_MON,
        FIELD5=SCRN1_FIELDS.STATS_IDLE);
%ROUTINE(NAME=INTERVAL,
        UENTRY=DG_INTERVAL,
        RMULT=1,
        NFLDS=1,
        FIELD1=SCRN1_FIELDS.INTERVAL,
        LAST=";");
 
