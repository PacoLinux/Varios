/*
                         This is the primary-screen routine table.
*/
%LVL1DCL(NAME=SCRN1_ROUTINES);
%ROUTINE(NAME=SITE,
        UENTRY=DG_SITE,
        RMULT=86400,
        NFLDS=1,
        FIELD1=SCRN1_FIELDS.SITE);
%ROUTINE(NAME=RUNNING_BATCH,
        UENTRY=DG_RBATCH,
        RMULT=1,
        NFLDS=44,
        FIELD1=SCRN1_FIELDS.BATCH_ACCT1,
