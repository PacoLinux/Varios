       71    12884                                                              /**/
       72    12885        /*
       73    12886            ***            Begin procedure                    ***
       74    12887        */
       75    12888        /*
       76    12889           Append an s to USERS_KEY.
       77    12890           Read :USERS keyed.
       78    12891           If the read returns then the object is a project and not a user.
       79    12892           We wish to disallow that.  So, issue a warning message that the
       80    12893           object is a project and will not be removed.  And then return.
       81    12894           If the read altreturns, then the object is possibly a user.
       82    12895           In which case, remove the s we added earlier, reset the count
       83    12896           on the USERS_KEY, and continue processing.
       84    12897        */
       85    12898    1   I = USERS_KEY.COUNT;
       86    12899    1   USERS_KEY.UKEY.S = 'S';
       87    12900    1   USERS_KEY.COUNT = 21;
       88    12901    2   CALL ASB$READ_USERS (AS_USERS) WHENRETURN DO;
       89    12902        /*E*  ERROR:  ASR-E$PROJ_NOT_USER_REMOVE-0
       90    12903              MESSAGE:  %U1,%U2 is a project not a user.
       91    12904                        %U1,%U2 cannot be removed with a REMOVE user command.
       92    12905        */
       93    12906    2      CALL INDEX (I,' ',SUBSTR(USERS_KEY.TXTC,0,8));
       94    12907    2      AS_FIELD1.COUNT = I;
       95    12908    2      AS_FIELD1.TEXT = SUBSTR(USERS_KEY.TXTC,0,I);
       96    12909    2      CALL INDEX (I,' ',SUBSTR(USERS_KEY.TXTC,8,12));
       97    12910    2      AS_FIELD2.COUNT = I;
       98    12911    2      AS_FIELD2.TEXT = SUBSTR(USERS_KEY.TXTC,8,I);
       99    12912           %SCREAM (ERR#=%E$PROJ_NOT_USER_REMOVE, LEVEL=WARN, MID='R');
      100    12927    2      ALTRETURN;
      101    12928    2      END; /*  Whenreturn  */
      102    12929    1   USERS_KEY.COUNT = I;
      103    12930    1   CALL ASB$ENQ ALTRET( ENQ_ERR ) ;
PL6.E3A0      #001=ASR$R_USER File=ASR$REMOVE_USER.:E05TSI                       TUE 07/29/97 10:51 Page:4    
      104    12931    1   ENQDEQ_FLG = AS_TRUE ;
      105    12932    1   CALL ASB$DELREC_USERS;
      106    12933    1   CALL ASB$DELREC_HLP;
      107    12934    1   CALL ASB$LIST( USERS_KEY );
      108    12935    2   IF FACCOUNT_OPT THEN DO;        /* Delete file management account */
      109    12936    2      IF NOT DSEG_FLG THEN
      110    12937    3         CALL ASB$GETSEG WHENALTRETURN DO ;
      111    12938                 %SCREAM (ERR#=%E$GETSEGERR, LEVEL=ERROR, MID='R' );
      112    12953    3            END; /*  Whenaltreturn from ASB$GETSEG  */
      113    12954    2      SUPER_TO_PIG.TYPE = %DELETE_ACCT;
      114    12955    2      SUPER_TO_PIG.WIDTH = LO_WIDTH;
      115    12956    2      SUPER_TO_PIG.PROJ = (AS_PROJECT_ADMIN ~= AS_NONE );
      116    12957    2      SUPER_TO_PIG.RESUPER = AS_LOGON_KEY.TEXT;
      117    12958    2      SUPER_TO_PIG.PROJECT_TEXT = AS_PROJECT_ADMIN.TEXT;
      118    12959    2      SUPER_TO_PIG.PACKSET = AS_USERS.HPSN;
      119    12960    2      SUPER_TO_PIG.ACCT    = AS_USERS.ACCT;
      120    12961    2      SUPER_TO_PIG.ATTR.COUNT = 0;
      121    12962    2      CALL ASB$PIG( SUPER_TO_PIG );
      122    12963    2      END;
      123    12964    2   IF AS_PROJECT_ADMIN ~= AS_NONE THEN DO;
      124    12965    2      USERS_KEY.SKEY.USER = SUBSTR(USERS_KEY.TXTC,0,USERS_KEY.COUNT );
      125    12966    2      USERS_KEY.SKEY.CREATOR = AS_PROJECT_ADMIN.TEXT;
      126    12967    2      USERS_KEY.SKEY.PROJ = 0;
      127    12968    2      USERS_KEY.COUNT = USERS_KEY.COUNT + 21;
      128    12969    2      CALL ASB$ENQ_USERS ALTRET( ENQ_ERR );
      129    12970    2      CALL ASB$DELREC_USERS;
      130    12971    2      END;
      131    12972    1   ELSE
      132    12973    2      IF AS_USERS.BUDGET.MACCT ~= AS_NONE THEN DO;
      133    12974    2         KEY = USERS_KEY;
      134    12975    2         USERS_KEY.COUNT = USERS_KEY.COUNT + 21;
      135    12976    2         USERS_KEY.SKEY.PROJ = 0;
      136    12977    2         USERS_KEY.SKEY.CREATOR = AS_USERS.BUDGET.MACCT_.TEXT;
      137    12978    2         USERS_KEY.SKEY.USER = KEY.TEXT;
      138    12979    2         CALL ASB$ENQ_USERS   ALTRET( ENQ_ERR );
      139    12980    2         CALL ASB$DELREC_USERS;
      140    12981    2         CALL CONCAT( USERS_KEY.TXTC, AS_USERS.BUDGET.MACCT_.TEXT, 'S' );
PL6.E3A0      #001=ASR$R_USER File=ASR$REMOVE_USER.:E05TSI                       TUE 07/29/97 10:51 Page:5    
      141    12982    2         USERS_KEY.COUNT = 21;
      142    12983    2         CALL ASB$ENQ_USERS ALTRET( ENQ_ERR );
      143    12984    2         CALL ASB$READ_USERS( AS_USERS ) ALTRET( ENQ_ERR );
      144    12985    2         AS_USERS.BUDGET.PROJ_CREATED = AS_USERS.BUDGET.PROJ_CREATED - 1;
      145    12986    2         CALL ASB$WRITE_USERS( AS_USERS );
      146    12987    2         USERS_KEY = KEY;
      147    12988    2         END;
      148    12989    1   CALL ASB$DEQ;
      149    12990    1   ENQDEQ_FLG = AS_FALSE ;
      150    12991    1   ENQ_ERR: ;
      151    12992    1   RETURN ;
