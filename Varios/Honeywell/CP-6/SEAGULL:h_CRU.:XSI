!DEFAULT GNACCT=:XSI, SIACCT=:XSI, UIACCT=:XTUI
!DEFAULT LBACCT=:LIBRARY,XACCT=:XSI
!JOB NAME=SEAGULL:h_CRU, WSN=LOCAL
!RESOURCE TIME=5, MEM=256
!ROUTE Jordan Boucher, LADC
!"
!FORMAT.X SEAGULL:h_ID1.SIACCT, .UIACCT OVER *SG:START, NO (NFMT, OPTUI)
!FORMAT.X SEAGULL:h_ID2.SIACCT, .UIACCT OVER *SG:END, NO (NFMT, OPTUI)
!
!"Now add all the SEAGULL_C? header/include files to the end...
!
!FORMAT.X SEAGULL_Cc01.SIACCT, .UIACCT OVER *SG:01, NO (NFMT, OPTUI)
!FORMAT.X SEAGULL_Cc02.SIACCT, .UIACCT OVER *SG:02, NO (NFMT, OPTUI)
!FORMAT.X SEAGULL_Cc03.SIACCT, .UIACCT OVER *SG:03, NO (NFMT, OPTUI)
!FORMAT.X SEAGULL_Cc04.SIACCT, .UIACCT OVER *SG:04, NO (NFMT, OPTUI)
!FORMAT.X SEAGULL_Cc06.SIACCT, .UIACCT OVER *SG:06, NO (NFMT, OPTUI)
!
!COPY *SG:START OVER  SEAGULL:h.GNACCT (NLN)
!COPY *SG:01    INTO  SEAGULL:h.GNACCT (NLN)
!COPY *SG:02    INTO  SEAGULL:h.GNACCT (NLN)
!COPY *SG:03    INTO  SEAGULL:h.GNACCT (NLN)
!COPY *SG:04    INTO  SEAGULL:h.GNACCT (NLN)
!COPY *SG:06    INTO  SEAGULL:h.GNACCT (NLN)
!COPY *SG:END   INTO  SEAGULL:h.GNACCT (NLN)
