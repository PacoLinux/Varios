      SUBROUTINE ADDGAC(A,B,C)
C*T***********************************************************  
C*T*                                                         *  
C*T* Copyright (c) Bull HN Information Systems Inc., 1989    *  
C*T*                                                         *  
C*T***********************************************************  
      REAL A,B,C
      A = B + C
      RETURN
      END 
      SUBROUTINE DIVGAC(A,B,C)
      REAL A,B,C
      A = B/C
      RETURN
      END 
      SUBROUTINE CONVRT(A,I)
      REAL A
      INTEGER I
      I = A
      RETURN
      END 
