        9         9             char *
       10        10             strindex(s, c)
       11        11              register char *s, c;
       12        12             {
       13        13    1         while (*s)
       14        14    1            if (*s++ == c)
       15        15    1               return --s;
       16        16    1         return (char *)0;
       17        17    1        }
