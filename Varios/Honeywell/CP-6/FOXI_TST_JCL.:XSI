!FORTRAN FOXI_TST_SIF1.SIACCT OVER *FOXI_TST_OUF1_X.GNACCT  (NLS,X,UNIT(200,400))
!FORTRAN FOXI_TST_SIF1.SIACCT OVER *FOXI_TST_OUF1_XZ.GNACCT (NLS,X,Z,UNIT(200,400))
!FORTRAN FOXI_TST_SIF1.SIACCT OVER *FOXI_TST_OUF1_Y.GNACCT  (NLS,Y,UNIT(200,400))
!"
!LINK *FOXI_TST_OUF1_X.GNACCT  OVER FOXI_TST_X.GNACCT   (UN=FOXI.GNACCT, SIDCB=F$200, OUDCB=F$400)
!LINK *FOXI_TST_OUF1_XZ.GNACCT OVER FOXI_TST_XZ.GNACCT  (UN=FOXI.GNACCT, SIDCB=F$200, OUDCB=F$400)
!LINK *FOXI_TST_OUF1_Y.GNACCT  OVER FOXI_TST_Y.GNACCT   (UN=FOXI.GNACCT, SIDCB=F$200, OUDCB=F$400)
!"
!DEL *PM_DATA, *PM_DATA_X
!Under PMON
!FOXI_TST_X.GNACCT FOXI_SI61.SIACCT OVER *JUNK1
!MOD *PM_DATA TO *PM_DATA_X
!"
!DEL *PM_DATA, *PM_DATA_XZ
!Under PMON
!FOXI_TST_XZ.GNACCT FOXI_SI61.SIACCT OVER *JUNK1
!MOD *PM_DATA TO *PM_DATA_XZ
!"
!DEL *PM_DATA, *PM_DATA_Y
!Under PMON
!FOXI_TST_Y.GNACCT FOXI_SI61.SIACCT OVER *JUNK2
!MOD *PM_DATA TO *PM_DATA_Y
!"
!PMDISP.XACCT FOXI_TST_X.GNACCT,  *PM_DATA_X
!PMDISP.XACCT FOXI_TST_XZ.GNACCT, *PM_DATA_XZ
!PMDISP.XACCT FOXI_TST_Y.GNACCT,  *PM_DATA_Y
!"
