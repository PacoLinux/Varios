        6         6             /* return length of s
        7         7             */
        8         8             int
        9         9             strlen(s)
       10        10              char *s;
       11        11             {
       12        12    1         register char *b = s;
       13        13    1
       14        14    1         while (*b++)
       15        15    1            ;
       16        16    1         return b - s - 1;
       17        17    1        }
