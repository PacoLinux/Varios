       91     5289    2        DO CASE ( S_D.ONE_TO_TWO.EXEC_DOMAIN );
       92     5290    2          CASE ( %HOST );
       93     5291    2             IF S_D.DO$ -> EVAL$CMD.DDESC
       94     5292    3             THEN DO;
       95     5293    3                  CALL DDK$EVAL(ADDRESS, SEG, DESC#, NOT_IS) ALTRET(EVERR);
       96     5294    3                  IF S_D.DO$->EVAL$CMD.INSTR
       97     5295    4                  THEN DO;
       98     5296    4                       ADR.UHALF = 0;
       99     5297    4                       IF NOT_IS
      100     5298    5                       THEN DO;
      101     5299    5                            S_D.ERROR = E@CANTEVAL; /* E@CANTEVAL */
      102     5300    5                            ALTRETURN;
      103     5301    5                          END;
