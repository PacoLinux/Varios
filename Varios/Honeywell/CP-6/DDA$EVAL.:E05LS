       38       38    1               5 PNTR UBIN(3) UNAL,
       39       39    1               5 OFFSET UBIN(15) UNAL,
       40       40    1             4 * UBIN(11) UNAL,
       41       41    1             4 BIT29 BIT(1) UNAL,
       42       42    1             4 * UBIN(6) UNAL,
       43       43    1           3 L6V1 REDEF L,
       44       44    1             4 * BIT(4) UNAL,
       45       45    1             4 L16 UBIN(16) UNAL,
       46       46    1             4 R16 UBIN(16) UNAL,
       47       47    1           3 R UBIN UNAL,
       48       48    1           3 L6V2 REDEF R,
       49       49    1             4 * BIT(4) UNAL,
       50       50    1             4 L16 UBIN(16) UNAL,
       51       51    1             4 R16 UBIN(16) UNAL,
       52       52    1           3 * (0:13) UBIN UNAL;
       53       53    1   DCL FMT UBIN;
       54       54    1   DCL D_XDELTA BIT(1) SYMREF;
       55       55          /* Misc */
       56       56    1   DCL UBIN36 UBIN BASED;
