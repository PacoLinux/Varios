        1        1        /*M*  DDA$EVAL  - DO routine for EVAL command */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MCL=4,DCI=5,CRT=5  */
        8        8        /*F*  NAME: DDA$EVAL
        9        9              PURPOSE:  To evaluate a generalized data reference, printing
       10       10                  the address of the item described.
       11       11              DESCRIPTION:  DDA$EVAL calls on DDK$EVAL to resolve a data
       12       12                  description to segment identification and offset
       13       13                  information.  For non-IS segments, this is printed
       14       14                  in segid -> offset format. For
       15       15                  values within the instruction segment, the value (address)
       16       16                  is printed in the specified format (default is OCTAL for
       17       17                  Host users and HEX for L6 users). */
       18       18        DDA$EVAL: PROC ALTRET;
       19       19          /* Locals */
       20       20    1   DCL ADDRESS SBIN;
       21       21    1   DCL 1 ADR REDEF ADDRESS,
       22       22    1         2 UHALF UBIN HALF HALIGNED,
       23       23    1         2 LHALF UBIN HALF HALIGNED;
       24       24    1   DCL 1 L6ADR REDEF ADDRESS,
       25       25    1         2 * BIT(4) UNAL,
       26       26    1         2 L16 UBIN(16) UNAL,
       27       27    1         2 R16 UBIN(16) UNAL;
       28       28    1   DCL SEG UBIN;
       29       29    1   DCL DESC# UBIN;
       30       30    1   DCL NOT_IS BIT(1);
       31       31    1   DCL 1 VALUE DALIGNED,
       32       32    1         2 SIZE UBIN UNAL,
       33       33    1         2 VAL,
       34       34    1           3 L UBIN UNAL,
       35       35    1           3 V REDEF L,
       36       36    1             4 ADR UBIN(18) UNAL,
       37       37    1             4 PADR REDEF ADR,
