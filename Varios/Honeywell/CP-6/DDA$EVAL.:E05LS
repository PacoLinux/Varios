      105     5303    4                         ( S_D.DO$->EVAL$CMD.AWDX )
      106     5304    4                       THEN VALUE.VAL.V.PADR.OFFSET = ADDRESS;
PL6.E3A0      #001=DDA$EVAL File=DDA$EVAL.:E05TSI                                TUE 07/29/97 13:27 Page:4    
      107     5305    4                       ELSE VALUE.VAL.V.ADR = ADDRESS;
      108     5306    4                     END;
      109     5307    3                  ELSE VALUE.VAL.L = ADDRESS;
      110     5308    3                  S_D.L$.VALUE.L = VALUE.VAL.L;
      111     5309    3                  S_D.L$.SIZE = 36;
      112     5310    3                END;
      113     5311    3             ELSE DO;
      114     5312    3                  VALUE.VAL = S_D.L$.VALUE;
      115     5313    3                  NOT_IS = '0'B;
      116     5314    3                END;
      117     5315    2          CASE ( %L6 );
