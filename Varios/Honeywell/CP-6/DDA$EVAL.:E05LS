       74     5272    1   DCL DDQ$WRITENU ENTRY(2);
       75     5273    1   DCL DDR$BUMPC_D$ ENTRY(1) ALTRET;
       76     5274    1   DCL DFS$INSTR ENTRY(4) ALTRET;
       77     5275    1   DCL DFK$EVAL ENTRY(8) ALTRET;
       78     5276                                                /**/
       79     5277    1        VALUE = '0'B;
       80     5278    1        IF S_D.DO$->EVAL$CMD.INSTR
       81     5279    1        THEN IF S_D.ONE_TO_TWO.EXEC_DOMAIN = %HOST
       82     5280    2             THEN DO;
       83     5281    2                  VALUE.VAL.L = S_D.D$ -> UBIN36;
       84     5282    2                  S_D.DESC$ = PINCRW(S_D.D$, 1);
       85     5283    2                END;
       86     5284    1             ELSE S_D.DESC$ = S_D.D$;
       87     5285    1        ZITEM$ = S_D.DESC$;
       88     5286    1        FMT = S_D.DO$ -> EVAL$CMD.FMT;
       89     5287    1        IF FMT = %DEFAULT_FMT
