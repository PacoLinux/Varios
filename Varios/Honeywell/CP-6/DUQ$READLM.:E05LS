      211     7825    1        FPTWRITELM.V_ = VECTOR( FPTWRITELM.V );
      212     7826    1        FPTWRITELM.V.DCB# = S_D.LMDCB;
      213     7827    1        FPTWRITELM.BUF_ = VECTOR( BUFF$ER );
      214     7828    1        AGAIN = '0'B;
      215     7829        /* Build key */
      216     7830    2        DO CASE( TYPE );
      217     7831    2          CASE( %TYPHEAD );                /* Head */
      218     7832    2             FPTWRITELM.KEY_ = VECTOR( B_RUHEADKEY );
      219     7833    2          CASE( %TYPBIRTH );               /* Birth */
PL6.E3A0      #001=DDQ$READLM File=DUQ$READLM.:E05TSI                            TUE 07/29/97 14:57 Page:7    
      220     7834    2             FPTWRITELM.KEY_ = VECTOR( B_BIRTHKEY );
      221     7835    2          CASE( %TYPPROG, %TYPROS );       /* Program, Ros */
      222     7836    3             DO CASE(S_D.ONE_TO_TWO.EXEC_DOMAIN);
      223     7837    3               CASE(%HOST);
      224     7838    3                  BRUKEY = B_RUKEY;
      225     7839    3                  BRUKEY.TYPE = TYPE;
      226     7840    3                  BRUKEY.NODE = NODE;
