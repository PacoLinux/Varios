      120     7734    1   DCL DDE$ERROR ENTRY(0);
      121     7735    1   DCL DUE$ERRMSG ENTRY(0);
      122     7736                                                /**/
      123     7737    1        M$LM$ = DCBADDR( S_D.LMDCB );
      124     7738    1        FPTREADLM = FPT_READLM;
      125     7739    1        FPTREADLM.V_ = VECTOR( FPTREADLM.V );
      126     7740    1        FPTREADLM.V.DCB# = S_D.LMDCB;
      127     7741    1        FPTREADLM.BUF_ = VECTOR( BUFF$ER );
      128     7742        /* Build key */
      129     7743    2        DO CASE( TYPE );
      130     7744    2          CASE( %TYPHEAD );                /* Head */
      131     7745    2             FPTREADLM.KEY_ = VECTOR( B_RUHEADKEY );
      132     7746    2          CASE( %TYPBIRTH );               /* Birth */
      133     7747    2             FPTREADLM.KEY_ = VECTOR( B_BIRTHKEY );
      134     7748    2          CASE( %TYPDNAM, %TYPEDEF, %TYPSDEF, %TYPPROG, %TYPROS );
              7748                   /* Defnames, Entdefs, Symdefs, Program, Ros */
      135     7749    3             DO CASE (S_D.ONE_TO_TWO.EXEC_DOMAIN);
      136     7750    3               CASE(%HOST);
      137     7751    3                  BRUKEY = B_RUKEY;
      138     7752    3                  BRUKEY.TYPE = TYPE;
      139     7753    3                  BRUKEY.NODE = NODE;
      140     7754    3                  BRUKEY.PAGE = PAGE;
      141     7755    3                  FPTREADLM.KEY_ = VECTOR( BRUKEY );
      142     7756    3               CASE(%L6);
      143     7757    3                  BFEPRUKEY = B_FEPRUKEY;
      144     7758    3                  BFEPRUKEY.TYPE = TYPE;
      145     7759    3                  BFEPRUKEY.NODE = NODE;
      146     7760    3                  BFEPRUKEY.PAGE = PAGE;
PL6.E3A0      #001=DDQ$READLM File=DUQ$READLM.:E05TSI                            TUE 07/29/97 14:57 Page:5    
      147     7761    3                  FPTREADLM.KEY_ = VECTOR( BFEPRUKEY );
      148     7762    3               CASE(ELSE);
      149     7763    3                  S_D.ERROR = 502;    /* E@DELBUG */
      150     7764    3                  ALTRETURN;
      151     7765    3               END;
      152     7766    2          CASE( %TYPLOGBLK, %TYPINTNTRY, %TYPEXST, %TYPVREBL, %TYPDBGNAM ); /* Debug */
      153     7767    2             BDBGKEY = B_DBGKEY;
      154     7768    2             BDBGKEY.NODE = NODE;
      155     7769    2             BDBGKEY.NPOINTER = NP;
      156     7770    2             BDBGKEY.TYPE = TYPE;
      157     7771    2             BDBGKEY.PAGE = PAGE;
      158     7772    2             FPTREADLM.KEY_ = VECTOR( BDBGKEY );
      159     7773    2          CASE( ELSE );
      160     7774    2             S_D.ERROR = 502;         /* E@DELBUG */
      161     7775    2             CALL DDE$ERROR;
      162     7776    2             ALTRETURN;
      163     7777    2          END;                             /* "DO CASE( TYPE )" */
      164     7778        /* Read record */
      165     7779    1        CALL M$READ( FPTREADLM ) ALTRET( ARET );
      166     7780    1        ARS = M$LM$ -> F$DCB.ARS#;
      167     7781                                                /**/
      168     7782    1        RETURN;
      169     7783                                                /**/
      170     7784    1   ARET:
      171     7785    1        ERRCODE = B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR.CODE;
      172     7786    1        IF ( ERRCODE = %E$LD ) AND ( ARS < 0 )
      173     7787    2        THEN DO;
      174     7788    2             ARS = -1 * M$LM$ -> F$DCB.ARS#;
      175     7789    2             RETURN;
      176     7790    2           END;
      177     7791    1        IF ( ERRCODE ~= %E$LD ) AND ( ERRCODE ~= %E$NOKEY )
      178     7792    1        THEN
      179     7793    1   ARETW:    CALL DUE$ERRMSG;
      180     7794    1        ALTRETURN;
      181     7795                                                /**/
      182     7796        %EJECT;
