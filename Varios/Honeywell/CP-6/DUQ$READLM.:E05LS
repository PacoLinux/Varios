      231     7845    3                  BFEPRUKEY.TYPE = TYPE;
      232     7846    3                  BFEPRUKEY.NODE = NODE;
      233     7847    3                  BFEPRUKEY.PAGE = PAGE;
      234     7848    3                  FPTWRITELM.KEY_ = VECTOR( BFEPRUKEY );
      235     7849    3               CASE(ELSE);
      236     7850    3                  S_D.ERROR = 502;    /* E@DELBUG */
      237     7851    3                  ALTRETURN;
      238     7852    3               END;
      239     7853    2          CASE( %TYPRUM );                 /* Rum */
      240     7854    2             BRUMKEY = B_RUMKEY;
      241     7855    2             BRUMKEY.UTS = S_D.RUMUTS;
      242     7856    2   AGAIN_:   S_D.RUMSEQ = S_D.RUMSEQ + 1;
      243     7857    2             BRUMKEY.SEQ = S_D.RUMSEQ;
      244     7858    2             BRUMKEY.NODE = NODE;
      245     7859    2             FPTWRITELM.KEY_ = VECTOR( BRUMKEY );
      246     7860    2             FPTWRITELM.V.NEWKEY# = '1'B;
      247     7861    3             IF BRUMKEY.SEQ = 1 THEN DO;
      248     7862    3                  AGAIN = '1'B;
      249     7863    3                  FPTWRITELM.BUF_ = VECTOR(WHOBUF);
      250     7864    3                  WHOBUF.X = '" RUMmed by ';
      251     7865    3                  WHOBUF.Y = ',';
      252     7866    3                  WHOBUF.ACCT = B$JIT.ACCN;
      253     7867    3                  WHOBUF.NAME = B$JIT.UNAME;
      254     7868    3                END;
      255     7869    2          CASE( ELSE );
      256     7870    2             S_D.ERROR = 502;         /* E@DELBUG */
PL6.E3A0      #001=DDQ$READLM File=DUQ$READLM.:E05TSI                            TUE 07/29/97 14:57 Page:8    
      257     7871    2             CALL DDE$ERROR;
      258     7872    2             ALTRETURN;
      259     7873    2          END;                             /* "DO CASE( TYPE )" */
      260     7874        /* Write record */
      261     7875    1        CALL M$WRITE( FPTWRITELM ) ALTRET( ARETW );
