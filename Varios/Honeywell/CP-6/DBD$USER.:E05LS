PL6.E3A0      #001=DBD$USER File=DBD$USER.:E05TSI                                TUE 07/29/97 13:10 Page:10   
      304    10422        /*F* NAME: DBD$ROS
      305    10423             PURPOSE: To format the initial portion of the FEP ROS.
      306    10424        */
      307    10425    1   DBD$ROS: ENTRY(TOP);
      308    10426    1      DISP_WHAT = %DISP_ROS#;
      309    10427    1      CALL DAF$INIT(ROSRPT);
      310    10428    1      GOTO COMMON_ENTRY;
      311    10429
      312    10430        /*F* NAME: DBD$TCB
      313    10431             PURPOSE: To format the FEP Task Control Block.
      314    10432        */
      315    10433    1   DBD$TCB: ENTRY(TOP);
      316    10434    1      DISP_WHAT = %DISP_TCB#;
      317    10435    1      CALL DAF$INIT(TCBRPT);
      318    10436    1      GOTO COMMON_ENTRY;
      319    10437
      320    10438
      321    10439        /*F* NAME: DBD$DCB
      322    10440             PURPOSE: To format the FEP Data Control Block.
      323    10441        */
      324    10442    1   DBD$DCB: ENTRY(TOP);
      325    10443    1      DISP_WHAT = %DISP_DCB#;
      326    10444    1      CALL DAF$INIT(DCBRPT);
      327    10445    1      GOTO COMMON_ENTRY;
      328    10446
      329    10447        /*F* NAME: DBD$ECCB
      330    10448             PURPOSE: To format the FEP Exceptional Condition Control Block.
      331    10449        */
      332    10450    1   DBD$ECCB: ENTRY(TOP);
      333    10451    1      DISP_WHAT = %DISP_ECCB#;
      334    10452    1      CALL DAF$INIT(ECCBRPT);
      335    10453    1      GOTO COMMON_ENTRY;
      336    10454
      337    10455        %EJECT;
PL6.E3A0      #001=DBD$USER File=DBD$USER.:E05TSI                                TUE 07/29/97 13:10 Page:11   
      338    10456    1   COMMON_ENTRY : ;
      339    10457    1      USR_REP_FLG = 0;
      340    10458    2      IF TOP.NSUBLKS=0 THEN DO;
      341    10459    3           DO UN=-1 DOWNTO -DB_MUIS;
      342    10460    3                USERNUM = -UN;
      343    10461    4                DO CASE(DISP_WHAT);
      344    10462    4                 CASE(%DISP_ROS#); CALL DISP_ROS;
      345    10463    4                 CASE(%DISP_DCB#); CALL DISP_DCB;
      346    10464    4                 CASE(%DISP_TCB#); CALL DISP_TCB;
      347    10465    4                 CASE(%DISP_ECCB#); CALL DISP_ECCB;
      348    10466    4                   END;
      349    10467    3                END;
      350    10468    2           END;
      351    10469    2      ELSE DO;
      352    10470    3           DO I=0 TO TOP.NSUBLKS-1;
      353    10471    4                CALL DBA$SELECTION(TOP.SUBLK$(I)->OUT,UN) WHENRETURN DO;
      354    10472    4                     IF UN < 0 THEN
      355    10473    4                          USERNUM = -UN;
      356    10474    4                     ELSE
      357    10475    5                          CALL DBM$SYSID2UN(UN) WHENRETURN DO;
      358    10476    5                               USERNUM = DB_CUN;
      359    10477    5                               END;
      360    10478    5                          WHENALTRETURN DO;
      361    10479    5                               USERNUM = UN;
      362    10480    5                               END;
      363    10481    5                     DO CASE(DISP_WHAT);
      364    10482    5                      CASE(%DISP_ROS#); CALL DISP_ROS;
      365    10483    5                      CASE(%DISP_DCB#); CALL DISP_DCB;
      366    10484    5                      CASE(%DISP_TCB#); CALL DISP_TCB;
      367    10485    5                      CASE(%DISP_ECCB#); CALL DISP_ECCB;
      368    10486    5                        END;
      369    10487    4                     END;
      370    10488    3                END;
      371    10489    2           END;
      372    10490    1      IF USR_REP_FLG>0 THEN DA_MAPERR='0'B;
      373    10491    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      374    10492    1      CALL DAF$WRITE(VECTOR(BLANK_LINE));
PL6.E3A0      #001=DBD$USER File=DBD$USER.:E05TSI                                TUE 07/29/97 13:10 Page:12   
      375    10493    1      RETURN;
      376    10494        %EJECT;
PL6.E3A0      #001=DBD$USER File=DBD$USER.:E05TSI                                TUE 07/29/97 13:10 Page:13   
      377    10495    1   DISP_ROS: PROC;
      378    10496    2      CALL GET_ROSBASE
      379    10497    3      WHENALTRETURN DO; RETURN; END;
      380    10498
      381    10499    2      CALL DAF$WRITE(VECTOR(ROSFMT10),
      382    10500    2        VECTOR(USERNUM), VECTOR(DCBPTR$), VECTOR(AVAIL$), VECTOR(NUMDCBS) );
      383    10501    2      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      384    10502
      385    10503    2      CALL DAF$WRITE(VECTOR(ROSFMT20),
      386    10504    2        VECTOR(ALT$), VECTOR(STK$), VECTOR(AVSZ), VECTOR(CURRSZ) );
      387    10505    2      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      388    10506
