PL6.E3A0      #001=DAD$DCT File=DAD$DCT.:E05TSI                                  TUE 07/29/97 12:42 Page:25   
      746    12084    1   DISP_A_LDCT: PROC;
      747    12085
      748    12086    2   DCL 1 DFLAG,
      749    12087    2         2 F BIT(14) UNAL,
      750    12088    2         2 * BIT(22) UNAL;
      751    12089
      752    12090    2   DCL 1 LKFLAG REDEF DFLAG,
      753    12091    2         2 F BIT(18) UNAL,
      754    12092    2         2 * BIT(18) UNAL;
      755    12093
      756    12094    2   DCL 1 QFLAG REDEF DFLAG,
      757    12095    2         2 F BIT(7) UNAL,
      758    12096    2         2 * BIT(29) UNAL;
      759    12097
      760    12098        %NK_DFLG_TEXT (NAME=DFLG_TEXT, STCLASS=STATIC);
      761    12109
      762    12110        %NK_LKFLG_TEXT (NAME=LKFLG_TEXT, STCLASS=STATIC);
      763    12122
      764    12123        %NK_QFL_TEXT (NAME=QFL_TEXT, STCLASS=STATIC);
      765    12132
      766    12133        %PLIST;
      767    12134
      768    12135    2      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      769    12136    2      CALL DAB$GATE(L_LOCK);
      770    12137    2      CALL DAF$WRITE(VECTOR(GATFMT),VECTOR(DA_GATETXT));
      771    12138    2      CALL DAF$WRITE(VECTOR(BLANK_LINE));
      772    12139
      773    12140    2      DFLAG.F = LDCT.DFLG;
      774    12141    2      DFLG_TEXT.SREST = 'set: ';
