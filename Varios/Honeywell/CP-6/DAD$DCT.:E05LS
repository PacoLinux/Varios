      479    11817    2           END/*else do*/;
      480    11818
      481    11819
      482    11820    1      IF (CMD = %LDCTCMD)
      483    11821    2      THEN DO;
      484    11822    2           IF PASS < MAX_PASS
      485    11823    3           THEN DO;
      486    11824    3                PASS = PASS+1;
      487    11825    3                GOTO DISP_LDCT_LOOP;
      488    11826    3                END;
      489    11827
      490    11828    3           ELSE DO;
