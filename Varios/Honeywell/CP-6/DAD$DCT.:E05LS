      427    11765    2      THEN DO;
      428    11766    2           LDCT_TYPE = 0/*formatted lines*/;
      429    11767    2           MAX_PASS = 2/*N* for now*/;
      430    11768    3           DO I=DCTXLO TO DCTXHI;
      431    11769    4                IF N$DCT$(I)~=ADDR(NIL) THEN DO;
