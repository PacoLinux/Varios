      704    12042    1   DISP_DCT: PROC;
      705    12043    2      DCTX=DCT.DCTX;
      706    12044    2      DEVNM=DCT.DEVNM;
      707    12045    3      IF DCT.FL$=ADDR(NIL) THEN DO;
      708    12046    3   FL0:    FL=0;
      709    12047    3           END;
      710    12048    3      ELSE DO;
