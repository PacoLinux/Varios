      712    12050    3           FL=P$->DCT.DCTX;
      713    12051    3           END;
      714    12052    2      SQ=ADDR(DCT.SQ$)->P.WA;
      715    12053    2      DFLG=BITBIN(DCT.DFLG);
      716    12054    2      IOCHAN.DVN=DCT.DVN;
      717    12055    3      IF DCT.DQH$=ADDR(NIL) THEN DO;
      718    12056    3   CHAN0:  IOCHAN.IOM=0;
      719    12057    3           IOCHAN.CHAN=0;
      720    12058    3           END;
      721    12059    3      ELSE DO;
      722    12060    3           CALL DAM$MAP(DCT.DQH$,DQH$) ALTRET(CHAN0);
      723    12061    3           IOCHAN.IOM=DQH.IOCHAN.IOM;
      724    12062    3           IOCHAN.CHAN=DQH.IOCHAN.CHANNEL;
      725    12063    3           END;
      726    12064    2      STATE=IOSTATE(DCT.STATE);
      727    12065    2      TDUSER=DCT.TDUSER#;
      728    12066    2      MSG=NOYES(BITBIN(DCT.MSG#~=0));
      729    12067    2      STATUS.DOWN=NOYES(BITBIN(DCT.STATUS.DOWN));
      730    12068    2      STATUS.TND=TDTST(DCT.STATUS.TND);
      731    12069    2      CALL DAF$WRITE(VECTOR(DCTFMT),
      732    12070    2        VECTOR(DCTX),
      733    12071    2        VECTOR(DEVNM),
      734    12072    2        VECTOR(FL),
      735    12073    2        VECTOR(SQ),
