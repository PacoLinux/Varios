      435    11773    4   NXTDCT:           END;
      436    11774    3                END;
      437    11775    2           END;
      438    11776
      439    11777
      440    11778    2      ELSE DO;
      441    11779    2           LDCT_TYPE = 1/*display all fields*/;
      442    11780    2           MAX_PASS = 5;
