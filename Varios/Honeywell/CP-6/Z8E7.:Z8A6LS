     1950    32916        /*========*/
     1951    32917    1     MOVE_ALL: PROC (REC$, LIT$);
     1952    32918        /*========*/
     1953    32919
     1954    32920    2        DCL LIT$          PTR;
     1955    32921    2        DCL REC$          PTR;
     1956    32922
     1957    32923
     1958    32924    2        DCL Z8EY$SUBSTR          ENTRY (4);
     1959    32925    2        DCL Z8EQ$MERGE_ITERATED_DO_WHILE ENTRY (4);
     1960    32926    2        DCL Z8EU$DISPOSE_TEMP    ENTRY (1);
     1961    32927    2        DCL EXIT_LABEL$          PTR;
     1962    32928
     1963    32929          /*V*  This procedure generates a move loop for  constructs like
     1964    32930                         MOVE ALL "XXX" TO REC
     1965    32931
     1966    32932              The algorithm is :
     1967    32933
     1968    32934                   NUM_MOVES = DIV (LEN(REC), LEN(LIT));
     1969    32935                   REMAINING_CHARS = MOD (LEN(REC), LEN(LIT))
     1970    32936
     1971    32937                   <Emit only if NUM_MOVES ~= constant 0>
     1972    32938
     1973    32939                       DO I = 1 TO LEN(REC) - LEN(LIT) + 1  BY LEN(LIT);
     1974    32940
     1975    32941                         MOVE LIT TO SUBSTR ( REC, I, LEN (LIT) );
     1976    32942
     1977    32943                       END;
     1978    32944
     1979    32945                   <Emit only if REMAINING_CHARS ~= constant 0>
     1980    32946
     1981    32947                       IF REMAINING_CHARS > 0
     1982    32948                         MOVE SUBSTR(LIT, 1, REMAINING_CHARS)
     1983    32949                             TO SUBSTR (REC,
     1984    32950                                          I,  (or 1 if above loop was not emitted)
     1985    32951                                          REMAINING_CHARS);
     1986    32952          */
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:72   
     1987    32953
     1988    32954
