     2493    35040    2            END;
     2494    35041
     2495    35042    1        ETT_ITEM_COUNT = 0;
     2496    35043    1        ETT$$ = LOP$$ -> LX_ETT_ITEM.ETT$;
     2497    35044        /*   MOVE <anything> TO <GBCD-identifier> (<reference-modification>)
     2498    35045             or TO <EBCDIC-identifier> (<reference-modification>) causes problems.
     2499    35046             Z8EY converts to %TYA and then extracts the substring. It must do so
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:89   
     2500    35047             as, if it extracted the substring first, it would cause a regression.
     2501    35048             MOVE <GBCD-identifier> (<reference-modification>) TO <numeric-identifier>
     2502    35049             would not work. If it first extracted the substring, then the substring
     2503    35050             would be left-justified into an alphanumeric temp. That temp would then
     2504    35051             be moved to the receiving operand, and the numeric move instructions all
     2505    35052             assume the operand to be right-justified. Z8EY cannot know if it is
     2506    35053             working on a sending operand or a receiving operand. Therefore, in this
     2507    35054             case, Z8E7 needs to walk the chain of ETT items, via LOPs, and see if
     2508    35055             there is a CVT anywhere in the chain before the ST item. If one is found,
     2509    35056             Z8E7 must replicate the whole chain of ETT items between the ILL item and
     2510    35057             the CVT, omitting the CVT itself. The CVT must be moved, with the
     2511    35058             opposite conversion, to the ROP side. */
     2512    35059    1   FIND_CVT:
     2513    35060    2        DO WHILE ETT$$ -> ETT.ID < %FIRST_ST;
     2514    35061    2          IF ETT$$ -> ETT.ID = %CVT THEN
     2515    35062    3            DO; /* Need to move CVT to sending side */
     2516    35063    3              DNT$$ = ETT$$ -> ETT.ROP ->
     2517    35064                       %IF CP6;
     2518    35065    3                  ETT.LOP ->
     2519    35066                       %ENDIF;
     2520    35067    3                  ST.DNT$;
     2521    35068                   %IF CP6;
     2522    35069    3              CALL CHARBIN (LIB_OFFSET, SUBSTR (DNT$$ -> DNT.TEXT.NAME, 6, 4));
     2523    35070                   %ENDIF;
     2524    35071    3              IF /* Statement set up this way thanks to FORMAT.X */
     2525    35072                       %IF CP6;
     2526    35073    3                  LIB_OFFSET = %OP_EBCDIC_TO_ASCII
     2527    35074                       %ELSE;
     2529    35076                       %ENDIF;
     2530    35077    3                  THEN
     2531    35078    3                LIB_OFFSET = %OP_ASCII_TO_EBCDIC;
     2532    35079    3              ELSE
     2533    35080    3                LIB_OFFSET = %OP_ASCII_TO_GBCD;
     2534    35081    3              CALL Z8CO$CREATE_LIBRARY_BASE (LIB_OFFSET, TABLE_ETT$);
     2535    35082    3              CALL Z8AF$FORME (%CVT,
     2536    35083    3                               ROP$$ -> LX_ETT_ITEM.ETT$,
     2537    35084    3                               TABLE_ETT$,
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:90   
     2538    35085    3                               ROP$$ -> LX_ETT_ITEM.ETT$);
     2539    35086    3              ETT$$ = ETT$$ -> ETT.LOP;
     2540    35087    4              DO WHILE ETT_ITEM_COUNT > 0;
     2541    35088    4                TEMP$ = LOP$$ -> LX_ETT_ITEM.ETT$;
     2542    35089    5                DO J = 2 TO ETT_ITEM_COUNT;
     2543    35090    5                  TEMP$ = TEMP$ -> ETT.LOP;
     2544    35091    5                END;
     2545    35092    4                TEMP_ROP$ = TEMP$ -> ETT.ROP;
     2546    35093    4                ID1 = TEMP$ -> ETT.ID;
     2547    35094    4                IF ID1 = %OFF AND LIB_OFFSET = %OP_ASCII_TO_GBCD THEN
     2548    35095    4                  TEMP_ROPCON.TYPE = %ALPHA6;
     2549    35096    4                CALL Z8AF$FORME (ID1, ETT$$, TEMP_ROP$, ETT$$);
     2550    35097    4                ETT_ITEM_COUNT = ETT_ITEM_COUNT - 1;
     2551    35098    4              END; /* WHILE */
     2552    35099    3              LOP$$ -> LX_ETT_ITEM.ETT$ = ETT$$;
     2553    35100    3              EXIT FIND_CVT;
     2554    35101    3            END;
