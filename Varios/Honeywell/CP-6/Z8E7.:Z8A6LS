      297    19429        /*===============*/
      298    19430    1     ON_SIZE_PREPASS: PROC (LOP$, ROP$, LABEL$);
      299    19431        /*===============*/
      300    19432
      301    19433    2        DCL LOP$          PTR;
      302    19434    2        DCL ROP$          PTR;
      303    19435    2        DCL LABEL$        PTR;
      304    19436    2        DCL NEL$$         PTR;
      305    19437    2        DCL LOP_ST$       PTR;
      306    19438
      307    19439    2        DCL 1 TYPE_DESC   ALIGNED,
      308    19440                   %TYPE_DESC;
      309    19474
      310    19475
      311    19476    2        CALL Z8AU$TYPE_DESCRIPTOR(LOP$ -> LX_ETT_ITEM.ETT$, TYPE_DESC);
      312    19477
      313    19478    3        DO CASE(TYPE_DESC.TYPE);
      314    19479    3          CASE(%TYD, %TYI);
      315    19480    3            CALL OVERFLOW_CHECKS_FROM_ETT (LOP$, ROP$, LABEL$);
      316    19481
      317    19482    3          CASE(%TYQ, %TYR);
      318    19483    3              CALL OVERFLOW_CHECKS_FROM_ETT_FLT (LOP$, ROP$, LABEL$);
      319    19484
      320    19485    3          CASE(%TYO);
      321    19486    3            IF COMCOM.OB.ELS THEN
      322    19487    3              CALL OVERFLOW_CHECKS_FROM_ETT (LOP$, ROP$, LABEL$);
      323    19488    3            ELSE
      324    19489    4              DO;
      325    19490    4                TYPE_DESC.DESC.NDSC.N = TYPE_DESC.DESC.NDSC.N + 1;
      326    19491    4                TYPE_DESC.TYPE        = %TYD;
      327    19492
      328    19493    4                CALL OVERFLOW_CHECKS_FROM_DESC (LOP$, ROP$, LABEL$, TYPE_DESC);
      329    19494    4              END;
      330    19495
      331    19496    3          CASE(%TYF);
      332    19497    3            IF TYPE_DESC.DESC.NDSC.TN = %NDSC9 OR COMCOM.OB.ELS THEN
      333    19498                   /* Variable is not overpunched or ELS bit is set */
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:15   
      334    19499    3              CALL OVERFLOW_CHECKS_FROM_ETT (LOP$, ROP$, LABEL$);
      335    19500    3            ELSE
      336    19501    4              DO;
      337    19502    4                TYPE_DESC.DESC.NDSC.N = TYPE_DESC.DESC.NDSC.N + 1;
      338    19503    4                TYPE_DESC.TYPE        = %TYD;
      339    19504
      340    19505    4                CALL OVERFLOW_CHECKS_FROM_DESC (LOP$, ROP$, LABEL$, TYPE_DESC);
      341    19506    4              END;
      342    19507
      343    19508    3          CASE(%TYA, %TYE, %TYB);
      344    19509                 %FISH (TRIAD = LOP$ -> LX_ETT_ITEM.ETT$,
      345    19510                        RTN   = LOP_ST$);
      346    19518
      347    19519    3            IF LOP_ST$ -> VARIABLE.B.CLASS = %CLASS_NUMERIC_EDITED THEN
      348    19520    4              DO;
      349    19521                     %GET_LTS (@ID    = %NEL,
      350    19522                               @ST$   = LOP_ST$,
      351    19523                               @RETURN$ = NEL$$);
      352    19566
      353    19567                     %INIT_TYPE_DESC (@TYPE_DESC = TYPE_DESC,
      354    19568                                      @TYPE      = %TYD,
      355    19569                                      @DESC      = NEL$$ -> NEL.DESC);
      356    19606
      357    19607    4                CALL OVERFLOW_CHECKS_FROM_DESC (LOP$, ROP$, LABEL$, TYPE_DESC);
      358    19608    4              END;
      359    19609
      360    19610    3          CASE(%TYJ);
      361    19611                 %INIT_TYPE_DESC (@TYPE_DESC = TYPE_DESC,
      362    19612                                  @TYPE      = %TYI);
      363    19656
      364    19657    3            CALL OVERFLOW_CHECKS_FROM_DESC (LOP$, ROP$, LABEL$, TYPE_DESC);
      365    19658
      366    19659    3            CALL RANGE(ROP$, - 32768,  32767, LABEL$);
      367    19660
      368    19661    3          CASE(%TYK);
      369    19662                 %INIT_TYPE_DESC (@TYPE_DESC = TYPE_DESC,
      370    19663                                  @TYPE      = %TYI);
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:16   
      371    19707
      372    19708    3            CALL OVERFLOW_CHECKS_FROM_DESC (LOP$, ROP$, LABEL$, TYPE_DESC);
      373    19709
      374    19710    3            CALL RANGE(ROP$, -2147483648, 2147483647, LABEL$);
      375    19711
      376    19712    3          CASE(%TYH);
      377    19713                 %INIT_TYPE_DESC (@TYPE_DESC = TYPE_DESC,
      378    19714                                  @TYPE      = %TYI);
      379    19758
      380    19759    3            CALL OVERFLOW_CHECKS_FROM_DESC (LOP$, ROP$, LABEL$, TYPE_DESC);
      381    19760
      382    19761    3            CALL RANGE(ROP$, -131072, 131071, LABEL$);
      383    19762
      384    19763    3        END /*V* Case */;
      385    19764
      386    19765    2   END ON_SIZE_PREPASS;
      387    19766        %EJECT;
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:17   
      388    19767        /*==========================*/
      389    19768    1     SIGN_AND_TRUNCATION_CHECKS: PROC (REC$, SEND$) ALTRET;
      390    19769        /*==========================*/
      391    19770
      392    19771    2        DCL REC$          PTR; /*V* To lx_ett for receiving item */
      393    19772    2        DCL SEND$         PTR; /*V* To lx_ett for sending item   */
      394    19773
      395    19774    2        DCL 1 REC_DESC    ALIGNED,
      396    19775                  %TYPE_DESC;
      397    19809    2        DCL 1 SEND_DESC   ALIGNED,
      398    19810                  %TYPE_DESC;
      399    19844
      400    19845    2        DCL REC_ST$       PTR;
      401    19846    2        DCL SEND_ST$      PTR;
      402    19847    2        DCL NEL$$         PTR;
      403    19848
      404    19849    2        DCL LINE          UBIN;
      405    19850    2        DCL COLUMN        UBIN;
      406    19851    2        DCL SEND_LEN      UBIN;
      407    19852    2        DCL REC_LEN       UBIN;
      408    19853    2        DCL SEND_SIGN     BIT (2) ALIGNED;
      409    19854    2        DCL REC_SIGN      BIT (2) ALIGNED;
      410    19855    2        DCL SEND_SF       SBIN;
      411    19856    2        DCL REC_SF        SBIN;
      412    19857    2        DCL SEND_LEFT     SBIN;
      413    19858    2        DCL REC_LEFT      SBIN;
      414    19859    2        DCL SEND_RIGHT    SBIN;
      415    19860    2        DCL REC_RIGHT     SBIN;
      416    19861
      417    19862
      418    19863
      419    19864             /*==========*/
      420    19865    2          NUM_CHECKS: PROC ;
      421    19866             /*==========*/
      422    19867
      423    19868
      424    19869    3            IF REC_ST$ -> SCALAR.B.CLASS = %CLASS_NUMERIC_EDITED THEN
PL6.E3A0      #001=Z8E7$STMT_ASSIGNMENT File=Z8E7.:Z8A6TSI                       SAT 08/09/97 11:35 Page:18   
      425    19870    4              DO;
      426    19871    4                IF SEND_ST$ -> SCALAR.B.CLASS = %CLASS_NUMERIC_EDITED THEN
      427    19872    4                  CALL FILL_DESC_FROM_NEL (SEND_ST$, SEND_DESC);
      428    19873
      429    19874    4                CALL FILL_DESC_FROM_NEL (REC_ST$, REC_DESC);
      430    19875    4              END;
      431    19876    3            ELSE
      432    19877    3              IF SEND_ST$ -> SCALAR.B.CLASS = %CLASS_NUMERIC_EDITED THEN
      433    19878    3                CALL FILL_DESC_FROM_NEL (SEND_ST$, SEND_DESC);
      434    19879
      435    19880                 %UNPACK_NDSC (@DESC   = REC_DESC.DESC,
      436    19881                               @TYPE   = REC_DESC.TYPE,
      437    19882                               @LENGTH = REC_LEN,
      438    19883                               @SIGN   = REC_SIGN,
      439    19884                               @SCALE  = REC_SF);
      440    19939
      441    19940                 %UNPACK_NDSC (@DESC   = SEND_DESC.DESC,
