       69    13579               DO ;
       70    13580                   CALL BINCHAR(ALPHA_#, ITEM) ;
       71    13581                   ITEM = ITEM + 1 ;
       72    13582                   IF ITEM > COUNT_LIMIT THEN
       73    13583                     ITEM = 1 ;
       74    13584               END ;
