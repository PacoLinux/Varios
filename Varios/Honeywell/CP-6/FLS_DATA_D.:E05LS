       20     5578        %MACRO BLOCK_TAG;
       21     5579             2 BEGX UBIN HALF HALIGNED,   /* Offset from block data base to first granule */
       22     5580              2 NBR UBIN BYTE UNAL,
       23     5581              2 BEG BIT(1) UNAL,                /* File beginning */
       24     5582              2 FIN BIT(1) UNAL,                /* File ending */
       25     5583              2 * BIT(1) UNAL,
       26     5584              2 TYPE UBIN(6) UNAL,
       27     5585              2 SIZE UBIN HALF HALIGNED,  /* Number of bytes in block data */
       28     5586              2 DESC BIT(18) HALIGNED,
       29     5587              2 UTS UBIN WORD,
       30     5588              2 NAME,
       31     5589                3 L UBIN BYTE CALIGNED,
       32     5590                3 C CHAR(31),
       33     5591              2 WHERE REDEF NAME,
       34     5592                3 PSN CHAR(6),
       35     5593                3 ACCT CHAR(8),
       36     5594                3 * CHAR(18);
       37     5595        %MEND;
