       36     5498    1   DCL DDE$ERROR ENTRY;
       37     5499    1   DCL DFQ$WRITE_FD ENTRY(1) ALTRET;
PL6.E3A0      #001=DFA$KEEP File=DFA$KEEP.:E05TSI                                TUE 07/29/97 14:33 Page:2    
       38     5500    1   DCL DFQ$READ_FD ENTRY(2) ALTRET;
       39     5501    1   DCL DUE$ERRMSG   ENTRY;
       40     5502                                                /**/
       41     5503    1        REMEMBER ERR IN S_D.SCREECH_IC;
       42     5504    1        IF S_D.DO$ -> KEEP$CMD.HOST
       43     5505    1        THEN CALL DO_HOST;
       44     5506    1        ELSE IF S_D.DO$ -> KEEP$CMD.FPRG
       45     5507    2             THEN DO;
       46     5508    2                  TFPRG$ = S_D.FPRG_CONTEXT$;
       47     5509    2                  IF TFPRG$ = ADDR(NIL)
       48     5510    3                  THEN DO;
       49     5511    3                       S_D.ERROR = E@NOFPRG; /* E@NOFPRG */
       50     5512    3                       GOTO ERR;
       51     5513    3                     END;
       52     5514    3                  DO WHILE (TFPRG$ -> FPRG$CONTEXT.RES ~= S_D.DO$ -> KEEP$CMD.FPRG_RES)
              5514                           ;
       53     5515    3                       TFPRG$ = TFPRG$ -> FPRG$CONTEXT.NEXT$;
       54     5516    3                       IF TFPRG$ = ADDR(NIL)
       55     5517    4                       THEN DO;
       56     5518    4                            S_D.ERROR = E@NOFPRG; /* E@NOFPRG */
       57     5519    4                            GOTO ERR;
       58     5520    4                          END;
       59     5521    3                     END;
       60     5522    2                  CALL DO_FPRG;
       61     5523    2                END;
       62     5524    2             ELSE DO;
       63     5525    2                  CALL DO_HOST;
       64     5526    2                  IF S_D.FPRG_CONTEXT$ = ADDR(NIL)
       65     5527    3                  THEN DO;
       66     5528    3                       S_D.ERROR = E@NOFPRG; /* E@NOFPRG */
       67     5529    3                       GOTO ERR;
       68     5530    3                     END;
       69     5531    2                  TFPRG$ = S_D.FPRG_CONTEXT$;
       70     5532    3                  DO WHILE(TFPRG$ ~= ADDR(NIL));
       71     5533    3                       CALL DO_FPRG;
       72     5534    3                       TFPRG$ = TFPRG$ -> FPRG$CONTEXT.NEXT$;
       73     5535    3                     END;
PL6.E3A0      #001=DFA$KEEP File=DFA$KEEP.:E05TSI                                TUE 07/29/97 14:33 Page:3    
       74     5536    2                END;
       75     5537    1        RETURN;
       76     5538    1   ERR: ;
       77     5539    1        ALTRETURN;
       78     5540        %EJECT;
PL6.E3A0      #001=DFA$KEEP File=DFA$KEEP.:E05TSI                                TUE 07/29/97 14:33 Page:4    
       79     5541    1   DO_HOST:PROC;
       80     5542    2        IF S_D.DO$->KEEP$CMD.CMD= C_KEEP   /* C_KEEP */
       81     5543    3        THEN DO;
       82     5544    3             S_D.TRAP = S_D.TRAP & ~S_D.DO$->KEEP$CMD.OPTIONS;
       83     5545    3             S_D.IGNORE = S_D.IGNORE & ~S_D.DO$->KEEP$CMD.OPTIONS;
       84     5546    3           END;
       85     5547    2        ELSE IF S_D.DO$->KEEP$CMD.CMD= C_TRAP /* C_TRAP */
       86     5548    3             THEN DO;
       87     5549    3                  S_D.TRAP = S_D.DO$->KEEP$CMD.OPTIONS;
       88     5550    3                  S_D.IGNORE = S_D.IGNORE & ~S_D.DO$->KEEP$CMD.OPTIONS;
       89     5551    3                END;
       90     5552    2             ELSE IF S_D.DO$->KEEP$CMD.CMD= C_IGNORE /* C_IGNORE */
       91     5553    3                  THEN DO;
       92     5554    3                       S_D.IGNORE = S_D.DO$->KEEP$CMD.OPTIONS;
       93     5555    3                       S_D.TRAP = S_D.TRAP & ~S_D.DO$->KEEP$CMD.OPTIONS;
       94     5556    3                     END;
       95     5557    3                  ELSE DO;
       96     5558    3                       S_D.ERROR = E@DELBUG; /* E@DELBUG */
       97     5559    3                       CALL DDE$ERROR;
       98     5560    3                       RETURN;
       99     5561    3                     END;
      100     5562    2        S_D.KEEP = ~(S_D.TRAP | S_D.IGNORE); /* Put all else in keep */
      101     5563    2        RETURN;
      102     5564    2   END;
      103     5565        %EJECT;
PL6.E3A0      #001=DFA$KEEP File=DFA$KEEP.:E05TSI                                TUE 07/29/97 14:33 Page:5    
      104     5566    1   DO_FPRG: PROC;
      105     5567    2        GD$MSG = '0'B;
      106     5568    2        GD$MSG.FNC = %GD#RQSKTI;
      107     5569    2        IF S_D.DO$->KEEP$CMD.CMD= C_KEEP   /* C_KEEP */
      108     5570    3        THEN DO;
      109     5571    3             TFPRG$ -> FPRG$CONTEXT.TRAP = TFPRG$ -> FPRG$CONTEXT.TRAP & ~S_D.DO$->
              5571                      KEEP$CMD.L6_OPTIONS;
      110     5572    3             TFPRG$ -> FPRG$CONTEXT.IGNORE = TFPRG$ -> FPRG$CONTEXT.IGNORE & ~S_D.DO$->
              5572                      KEEP$CMD.L6_OPTIONS;
      111     5573    3             GD$MSG.KTI.CMD_TYPE = %GD#KEEP_CMD;
      112     5574    3           END;
      113     5575    2        ELSE IF S_D.DO$->KEEP$CMD.CMD= C_TRAP /* C_TRAP */
      114     5576    3             THEN DO;
      115     5577    3                  TFPRG$ -> FPRG$CONTEXT.TRAP = S_D.DO$->KEEP$CMD.L6_OPTIONS;
      116     5578    3                  TFPRG$ -> FPRG$CONTEXT.IGNORE = TFPRG$ -> FPRG$CONTEXT.IGNORE & ~S_D.
              5578                           DO$->KEEP$CMD.L6_OPTIONS;
      117     5579    3                  GD$MSG.KTI.CMD_TYPE = %GD#TRAP_CMD;
      118     5580    3                END;
      119     5581    2             ELSE IF S_D.DO$->KEEP$CMD.CMD= C_IGNORE /* C_IGNORE */
      120     5582    3                  THEN DO;
      121     5583    3                       TFPRG$ -> FPRG$CONTEXT.IGNORE = S_D.DO$->KEEP$CMD.L6_OPTIONS;
      122     5584    3                       TFPRG$ -> FPRG$CONTEXT.TRAP = TFPRG$ -> FPRG$CONTEXT.TRAP & ~S_D
              5584                                .DO$->KEEP$CMD.L6_OPTIONS;
      123     5585    3                       GD$MSG.KTI.CMD_TYPE = %GD#IGNORE_CMD;
      124     5586    3                     END;
      125     5587    3                  ELSE DO;
      126     5588    3                       S_D.ERROR = E@DELBUG; /* E@DELBUG */
      127     5589    3                       CALL DDE$ERROR;
      128     5590    3                       RETURN;
      129     5591    3                     END;
      130     5592    2        TFPRG$ -> FPRG$CONTEXT.KEEP = ~(TFPRG$ -> FPRG$CONTEXT.TRAP | TFPRG$ ->
              5592                 FPRG$CONTEXT.IGNORE); /* Put all else in keep */
      131     5593    2        GD$MSG.KTI.TRAP = TFPRG$ -> FPRG$CONTEXT.TRAP;
      132     5594    2        GD$MSG.KTI.KEEP = TFPRG$ -> FPRG$CONTEXT.KEEP;
      133     5595    2        GD$MSG.KTI.IGNORE = TFPRG$ -> FPRG$CONTEXT.IGNORE;
      134     5596    2        CALL DFQ$WRITE_FD (TFPRG$) ALTRET(ERRMSG);
      135     5597    2        CALL DFQ$READ_FD (TFPRG$, '0'B) ALTRET(ERRMSG);
PL6.E3A0      #001=DFA$KEEP File=DFA$KEEP.:E05TSI                                TUE 07/29/97 14:33 Page:6    
      136     5598    2        IF GD$MSG.FNC ~= %GD#RSPACK
