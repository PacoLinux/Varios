       23     4536        %S_D_PTRS;
       24     4573        %INCLUDE GD$MSG_M;
       25     4737        %GD$MSG(FPTN=GD$MSG,STCLASS="BASED(S_D_L6_MESSAGE$)",L6=YES,L6PTR="UBIN(32) UNAL");
       26     4893        %INCLUDE GD_EQUS_E;
       27     5054        %INCLUDE DD_SUBS_C;
       28     5325        %INCLUDE CP_6;
       29     5406        %INCLUDE DU_SCODE_C;
       30     5419        %FPT_SCREECH(FPTN=SNAP_MESSAGE,STCLASS=CONSTANT,SCODE=SC_MESSAGE);
       31     5433        %VLP_SCODE(FPTN=SC_MESSAGE, STCLASS=CONSTANT, FCG=DU, MID=X, MON=NO,
       32     5434                   CODE=%S$DUMESSAGE, SEV=SEV_SNAP, CUSR=YES, ROSEG=YES,
       33     5435                   ISEG=YES, DDSEG=YES, DBSEG=YES, ASLSEG=YES);
