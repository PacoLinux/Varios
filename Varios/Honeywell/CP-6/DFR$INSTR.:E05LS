     1206     5701    4                   GOTO FD_NONBLK;
     1207     5702    4                 END;
     1208     5703    3              END;
     1209     5704    2   FD_NONBLK: CALL INSERT( INPUT, POSITION,, '^' );
     1210     5705    2           CALL INDEX( POSITION, ' ', DESTINATION );
     1211     5706    2           POSITION = POSITION+1;
     1212     5707    2           CALL INSERT( DESTINATION, POSITION,, INPUT );
     1213     5708    2           CALL INDEX( POSITION, '^', DESTINATION );
     1214     5709    2           RETURN;
     1215     5710    2   END DFR$MERGE;
