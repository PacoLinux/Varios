      148     4643    2   TYPE_DECODE: DO CASE ( INSTRUCTION_TYPE(INSTRUCTION.INDEXX) );
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:5    
      149     4644                                                                                    /**/
      150     4645    2            CASE(0);                       /* unknown instruction                */
      151     4646    3              DO;
      152     4647    3                 END;                      /* end case 0                         */
      153     4648    2            CASE(1);                       /* generic or commercial instruction  */
      154     4649                                                                                    /**/
      155     4650    3   GENERIC_DECODE: DO CASE( GENERIC_TYPE(INSTRUCTION.FUNCTION) );
      156     4651                                                                                    /**/
      157     4652    3               CASE(0);                    /* unknown generic                    */
      158     4653    4                 DO;
      159     4654    4                    END;                   /* case 0                             */
      160     4655    3               CASE(1);                    /* generics                           */
      161     4656    3                 RASSED_INSTR.BUFFER = GENERIC_INSTRUCTIONS(INSTRUCTION.FUNCTION);
      162     4657    3                 RASSED_INSTR.WORDS_USED = 1; /* end case 1                      */
      163     4658    3                 CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
      164     4659                                                                                    /**/
      165     4660    3               CASE(2);                    /* second word determines function    */
      166     4661    3                 INSTR = INSTR_ARRAY(1);
      167     4662    4                 IF INSTR = 0 THEN DO;     /* It's a stt instruction             */
      168     4663    4   STT:             RASSED_INSTR.BUFFER = 'STT';
      169     4664    4                    RASSED_INSTR.WORDS_USED = 2;
      170     4665    4                    RASSED_INSTR.COUNT = 3;
      171     4666    4                    END;
      172     4667    4                 ELSE DO;
      173     4668                                                                                    /**/
      174     4669    5                    IF RLQ_INSTR.RLQ_INDICATOR = 0 THEN DO; /* It's an rlq instr */
