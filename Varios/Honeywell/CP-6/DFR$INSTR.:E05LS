     1161     5656    2   DCL J UBIN WORD;
     1162     5657                                                                                    /**/
     1163     5658    3           IF VALUE = 0 THEN ZERO_VALUE: TEXT = '0^'; ELSE NON_ZERO_VALUE: DO;
     1164     5659    3              R_TEMP_TEXT = ' ';
     1165     5660    3              TEXT = ' ';
     1166     5661    3              CALL BINXCHAR( TEXT, VALUE );
     1167     5662    4              IF R_TEXT(0) = '-' THEN DO;
     1168     5663    4                 TEMP_TEXT(0) = '-';
     1169     5664    4                 J = 1;
     1170     5665    4                 END;
     1171     5666    3              ELSE J=0;
     1172     5667    3              I = 1;
     1173     5668    4   ZERO_REMOVE: DO WHILE ( R_TEXT(I) = '0' );
     1174     5669    4                 I = I+1;
     1175     5670    4                 END ZERO_REMOVE;
     1176     5671    4   FILL_BUFFER: DO WHILE ( I <= 9 );
     1177     5672    4                 TEMP_TEXT(J) = R_TEXT(I);
     1178     5673    4                 I = I+1;
     1179     5674    4                 J = J+1;
     1180     5675    4                 END FILL_BUFFER;
     1181     5676    3              TEMP_TEXT(J) = '^';
     1182     5677    3              R_TEXT = TEMP_TEXT;
     1183     5678    3              END NON_ZERO_VALUE;
     1184     5679    2           RETURN;
     1185     5680    2   END CONVERT_BIN_TO_DEC;
