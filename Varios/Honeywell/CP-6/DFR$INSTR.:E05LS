PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:16   
      464     4959    1   SHIFT_SINGLE_REG: PROC;
      465     4960    2   DCL VALUE SBIN WORD;
      466     4961    2   DCL VALUE_TEXT CHAR(10);
      467     4962    2   DCL REGISTER SBIN WORD;
      468     4963    2   DCL POSITION UBIN WORD;
      469     4964                                                                                    /**/
      470     4965    2           REGISTER = INSTRUCTION_TYPE(INSTRUCTION.INDEXX)/2-3;
      471     4966    2           RASSED_INSTR.BUFFER = SHIFT_INSTRUCTIONS( (REGISTER-1)*8+FORM.HEX3 );
      472     4967    2           VALUE = SHIFT_SINGLE.VALUE;
      473     4968    2           GOTO PROCESS_VALUE;
      474     4969        %EJECT;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:17   
      475     4970    2   SHIFT_DOUBLE_REG: ENTRY;
      476     4971                                                                                    /**/
      477     4972    2           REGISTER = ( INSTRUCTION_TYPE(INSTRUCTION.INDEXX)-1 )/2-3;
      478     4973    2           RASSED_INSTR.BUFFER = SHIFT_D_INSTRUCTIONS( (REGISTER-1)*4 + (FORM.HEX3S.
              4973                    TYPE-4) + 28 );
      479     4974    2           VALUE = SHIFT_DOUBLE.VALUE;
      480     4975                                          /* Index into shift_d_instructions
      481     4976                                             as if it were a two dimensioned
      482     4977                                            array with register as row and type as column */
      483     4978    2   PROCESS_VALUE: ;   /* At this point, we turn a binary number into
      484     4979                              a printable string of its decimal equ. */
      485     4980    2           VALUE_TEXT = ' ';
      486     4981    2           CALL CONVERT_BIN_TO_DEC( VALUE_TEXT, VALUE );
      487     4982    2           CALL INDEX( POSITION, ' ', RASSED_INSTR.BUFFER );
      488     4983    2           POSITION = POSITION+2;
      489     4984    2           CALL INSERT( RASSED_INSTR.BUFFER, POSITION,, VALUE_TEXT );
      490     4985    2           CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
      491     4986    2           RASSED_INSTR.WORDS_USED = 1;
      492     4987    2           RETURN;
      493     4988    2   END SHIFT_SINGLE_REG;
      494     4989        %EJECT;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:18   
      495     4990    1   I_O_INSTR: PROC ALTRET;
      496     4991    2   DCL END_POINT1 UBIN WORD;               /* End of DECODED_TEXT                */
      497     4992    2   DCL END_POINT2 UBIN WORD;               /* End of RASSED_INSTR.BUFFER         */
      498     4993    2   DCL WORDS_USED UBIN WORD;               /* Temp for words used                */
      499     4994    2   DCL WORDS_USED_2ND UBIN WORD;      /* # words used in second addr syl decode  */
      500     4995    2   DCL I UBIN WORD;
      501     4996                                                                                    /**/
      502     4997    2           RASSED_INSTR.BUFFER = INSTRUCTIONS(INSTRUCTION.INDEXX);
      503     4998    2           CALL DECODE_ADDR_SYL ALTRET( BAD_IO_SYL );
      504     4999    2           CALL INDEX( END_POINT1, ' ', DECODED_TEXT );
      505     5000    2           CALL INSERT( DECODED_TEXT, END_POINT1,, '), ^' );
      506     5001    2           CALL INDEX( END_POINT2, ' ', RASSED_INSTR.BUFFER );
      507     5002    2           END_POINT2 = END_POINT2 + 1;
      508     5003    2           CALL INSERT( RASSED_INSTR.BUFFER, END_POINT2,, '(', DECODED_TEXT );
      509     5004    2           CALL INDEX( END_POINT2, '^', RASSED_INSTR.BUFFER );
      510     5005    2           WORDS_USED = RASSED_INSTR.WORDS_USED;
      511     5006                                                                                    /**/
      512     5007    3           DO I = RASSED_INSTR.WORDS_USED TO 15;
      513     5008    3              INSTR_ARRAY(I - RASSED_INSTR.WORDS_USED) = INSTR_ARRAY(I);
      514     5009    3              END;
      515     5010                                                                                    /**/
      516     5011    2           INSTR = INSTR_ARRAY(0);
      517     5012    2           CALL DECODE_ADDR_SYL ALTRET( BAD_IO_SYL );
      518     5013    2           CALL INDEX( END_POINT1, ' ', DECODED_TEXT );
      519     5014    2           CALL INSERT( DECODED_TEXT, END_POINT1,, ')^' );
      520     5015    2           CALL INSERT( RASSED_INSTR.BUFFER, END_POINT2,, '(', DECODED_TEXT );
      521     5016    2           WORDS_USED_2ND = RASSED_INSTR.WORDS_USED;
      522     5017    2           RASSED_INSTR.WORDS_USED = WORDS_USED + RASSED_INSTR.WORDS_USED;
      523     5018    2           CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
      524     5019                                                                                    /**/
      525     5020    3           IF IOLD_INSTR_FLAG THEN DO;     /* Special case for .I.O.L.D. Instr   */
      526     5021    4              DO I = WORDS_USED_2ND TO 15;
      527     5022    4                 INSTR_ARRAY(I - WORDS_USED_2ND) = INSTR_ARRAY(I);
      528     5023    4                 END;
      529     5024    3              WORDS_USED = RASSED_INSTR.WORDS_USED;
      530     5025    3              INSTR = INSTR_ARRAY(0);
      531     5026    3              CALL DECODE_ADDR_SYL ALTRET( BAD_IO_SYL );
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:19   
      532     5027    3              CALL INDEX( END_POINT1, ' ', DECODED_TEXT );
      533     5028    3              CALL INSERT( DECODED_TEXT, END_POINT1,, ')^' );
      534     5029    3              CALL INSERT( RASSED_INSTR.BUFFER, RASSED_INSTR.COUNT,, ', (',
              5029                       DECODED_TEXT );
      535     5030    3              CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
      536     5031    3              RASSED_INSTR.WORDS_USED = RASSED_INSTR.WORDS_USED + WORDS_USED;
      537     5032    3              END;
      538     5033                                                                                    /**/
      539     5034    2   GOOD_IO: RETURN;
      540     5035                                                                                    /**/
      541     5036    2   BAD_IO_SYL: RASSED_INSTR.COUNT = 0;
      542     5037    2           RASSED_INSTR.BUFFER = ' ';
      543     5038    2           RASSED_INSTR.WORDS_USED = 0;
      544     5039    2           RETURN;
      545     5040    2   END I_O_INSTR;
      546     5041        %EJECT;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:20   
      547     5042    1   DECODE_DD: PROC ALTRET;
      548     5043    2   DCL DDW1 UBIN;
      549     5044    2   DCL 1 DDW REDEF DDW1,
      550     5045    2         2 * BIT(20),
      551     5046    2         2 C UBIN(3)UNAL,
      552     5047    2         2 L UBIN(5)UNAL,
      553     5048    2         2 T BIT(1),
      554     5049    2         2 M UBIN(3)UNAL,
      555     5050    2         2 AN UBIN(4)UNAL;
      556     5051    2   DCL DDW2 SBIN;
      557     5052    2   DCL 1 DDW2U REDEF DDW2,
      558     5053    2         2 * BIT(20),
      559     5054    2         2 S SBIN(16)UNAL;
      560     5055    2   DCL 1 SCW REDEF DDW2,
      561     5056    2         2 * BIT(20),
      562     5057    2         2 DIR BIT(1),
      563     5058    2         2 * BIT(2),
      564     5059    2         2 C UBIN(5)UNAL,
      565     5060    2         2 RND BIT(1),
      566     5061    2         2 * BIT(7);
      567     5062    2   DCL INDX SBIN;
      568     5063    2           DDW1 = INSTR_ARRAY(RASSED_INSTR.WORDS_USED);
      569     5064    2           DDW2U.S = INSTR_ARRAY(RASSED_INSTR.WORDS_USED+1);
      570     5065    2           DDW2 = DDW2U.S;
      571     5066    2           INDX = RASSED_INSTR.COUNT;
      572     5067    3           IF INDX ~= 4 THEN DO;
      573     5068    3              CALL INSERT(RASSED_INSTR.BUFFER,INDX,2,',(');
      574     5069    3              INDX = INDX+1;
      575     5070    3              END;
      576     5071    2           ELSE CALL INSERT(RASSED_INSTR.BUFFER,4,1,'(');
      577     5072    2           INDX = INDX+1;
      578     5073    3           IF DDW.AN = 0 THEN DO;
      579     5074    3              CALL INSERT(RASSED_INSTR.BUFFER,INDX,8,'RDD0000)');
      580     5075    3              CALL BINCHAR(SUBSTR(RASSED_INSTR.BUFFER,INDX+3,4),DDW1/16);
      581     5076    3              RASSED_INSTR.WORDS_USED = RASSED_INSTR.WORDS_USED+1;
      582     5077    3              RASSED_INSTR.COUNT = INDX+8;
      583     5078    3              RETURN;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:21   
      584     5079    3              END;
      585     5080    2           RASSED_INSTR.WORDS_USED = RASSED_INSTR.WORDS_USED+2;
      586     5081    2           CALL INSERT(RASSED_INSTR.BUFFER,INDX,7,'C',BINASC(DDW.C+48),',L00,');
      587     5082    2           CALL BINCHAR(SUBSTR(RASSED_INSTR.BUFFER,INDX+4,2),DDW.L);
      588     5083    2           INDX = INDX+7;
      589     5084    3            IF DDW.T THEN DO;
      590     5085    3               CALL INSERT(RASSED_INSTR.BUFFER,INDX,2,'T,');
      591     5086    3               INDX = INDX+2;
      592     5087    3               END;
      593     5088    3            IF DDW.AN = 8 THEN DO CASE(DDW.M);
      594     5089    3               CASE(0,1,2,3);
      595     5090    3                  CALL INSERT(RASSED_INSTR.BUFFER,INDX,1,'$O');
      596     5091    3                  INDX = INDX+1;
      597     5092    3                  GOTO DSP_RM;
      598     5093    3               CASE(4);
      599     5094    3                  CALL INSERT(RASSED_INSTR.BUFFER,INDX,2,'*$O');
      600     5095    3                  INDX = INDX+2;
      601     5096    3                  DDW.M = 0; /* NO POST INDEX */
      602     5097    3                  GOTO DSP_RM;
      603     5098    3               CASE(7);
      604     5099    3                  CALL CONVERT_BIN_TO_DEC(SUBSTR(RASSED_INSTR.BUFFER,INDX,1),DDW2);
      605     5100    3                  CALL INDEX(INDX,'^',RASSED_INSTR.BUFFER);
      606     5101    3                  CALL INSERT(RASSED_INSTR.BUFFER,INDX,5,',IMO)');
      607     5102    3                  RASSED_INSTR.COUNT = INDX+5;
      608     5103    3                  RETURN;
      609     5104    3               CASE(ELSE);
      610     5105    3                  ALTRETURN;
      611     5106    3               END;
      612     5107    3            IF DDW.AN>8 THEN DO;
      613     5108    3               CALL INSERT(RASSED_INSTR.BUFFER,INDX,1,'*');
      614     5109    3               INDX = INDX+1;
      615     5110    3               END;
      616     5111    2            CALL INSERT(RASSED_INSTR.BUFFER,INDX,2,'B',BINASC(MOD(DDW.AN,8)+48));
      617     5112    2            INDX = INDX+2;
      618     5113    2   DSP_RM:;
      619     5114    3            IF DDW2 ~= 0 THEN DO;
      620     5115    3               CALL INSERT(RASSED_INSTR.BUFFER,INDX,1,'+');
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:22   
      621     5116    3               IF DDW2>0 THEN INDX = INDX+1;
      622     5117    3               CALL CONVERT_BIN_TO_DEC(SUBSTR(RASSED_INSTR.BUFFER,INDX,1),DDW2);
      623     5118    3               CALL INDEX(INDX,'^',RASSED_INSTR.BUFFER);
      624     5119    3               END;
      625     5120    3            IF DDW.M ~= 0 THEN DO;
      626     5121    3               CALL INSERT(RASSED_INSTR.BUFFER,INDX,3,',R',BINASC(DDW.M+48));
      627     5122    3               INDX = INDX+3;
      628     5123    3               END;
      629     5124    2            CALL INSERT(RASSED_INSTR.BUFFER,INDX,1,')');
      630     5125    2            RASSED_INSTR.COUNT = INDX+1;
      631     5126    2            RETURN;
      632     5127        /**/
      633     5128    2   DECODE_SCW: ENTRY ALTRET;
      634     5129    2            DDW1 = INSTR_ARRAY(RASSED_INSTR.WORDS_USED);
      635     5130    2            DDW2 = INSTR_ARRAY(RASSED_INSTR.WORDS_USED+1);
      636     5131    2            RASSED_INSTR.WORDS_USED = RASSED_INSTR.WORDS_USED+2;
      637     5132    2            INDX = RASSED_INSTR.COUNT;
      638     5133    3            IF DDW1 = 120 THEN DO;
      639     5134    3               CALL INSERT(RASSED_INSTR.BUFFER,INDX,3,',R5');
      640     5135    3               RASSED_INSTR.COUNT = INDX+3;
      641     5136    3               RETURN;
      642     5137    3               END;
      643     5138    2            IF DDW1 ~= 376 THEN ALTRETURN;
      644     5139    2            IF SCW.DIR THEN CALL INSERT(RASSED_INSTR.BUFFER,0,3,'DRS');
      645     5140    2            ELSE CALL INSERT(RASSED_INSTR.BUFFER,0,3,'DFS');
      646     5141    2            CALL BINCHAR(SUBSTR(RASSED_INSTR.BUFFER,INDX+1,2),SCW.C);
      647     5142    3            IF SCW.RND THEN DO;
      648     5143    3               CALL INSERT(RASSED_INSTR.BUFFER,INDX+3,4,',RND');
      649     5144    3               INDX = INDX+4;
      650     5145    3               END;
      651     5146    2            RASSED_INSTR.COUNT = INDX+3;
      652     5147    2            RETURN;
      653     5148    2   END DECODE_DD;
      654     5149        %EJECT;
