      304     4799    1   A_OK:   GOTO CHECK;
      305     4800                                                                                    /**/
      306     4801    1   BAD_ADDR_SYL: RASSED_INSTR.COUNT = 0;
      307     4802    1           RASSED_INSTR.BUFFER = ' ';
      308     4803    1           RASSED_INSTR.WORDS_USED = 0;
      309     4804    1   CHECK:
      310     4805                /* If we can't format the 16 words into an instruction, then
      311     4806                   format the 1st word into a hexadecimal number */
      312     4807
      313     4808    1           IF RASSED_INSTR.WORDS_USED = 0 THEN
      314     4809    2           DO;
      315     4810    2              CALL DFR$BINHEX ( INSTRUCT(0), TEXT_HEX, L_ZEROS, HEX_SIZE );
      316     4811    2              RASSED_INSTR.COUNT = HEX_SIZE + 1;
      317     4812    2              TEXT_HEX = SUBSTR ( TEXT_HEX, L_ZEROS, HEX_SIZE );
      318     4813    2              CALL INSERT ( RASSED_INSTR.BUFFER, 0, RASSED_INSTR.COUNT, '.', TEXT_HEX )
              4813                       ;
      319     4814    2              RASSED_INSTR.WORDS_USED = 1;
      320     4815    2           END;
      321     4816
      322     4817    1           INSTR_SIZE = RASSED_INSTR.WORDS_USED;
      323     4818
      324     4819
      325     4820                /* Check if mnemonics output is desired and if the output buffer
      326     4821
      327     4822                   is big enough */
      328     4823    1           IF OUT = '0'B THEN RETURN;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:10   
      329     4824    1           IF S_D.D$ >= S_D.DESCL$ THEN GOTO BUFULL;
      330     4825    1           BUF_L = POFFC ( S_D.DESCL$, S_D.D$ );
      331     4826    1           SUBSTR ( S_D.D$ -> BUF_T, 0, BUF_L ) =
      332     4827    1              SUBSTR ( RASSED_INSTR.BUFFER, 0, RASSED_INSTR.COUNT );
      333     4828    1           S_D.D$ = PINCRC ( S_D.D$, RASSED_INSTR.COUNT );
      334     4829    1           IF S_D.D$ > S_D.DESCL$ THEN GOTO BUFULL;
      335     4830    1           RETURN;
      336     4831
      337     4832    1   BUFULL: S_D.ERROR = E@BUFFULL;                        /* E@BUFFULL */
      338     4833    1           ALTRETURN;
      339     4834        %EJECT;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:11   
      340     4835    1   BRANCH: PROC;
      341     4836    2   DCL VALUE_TEXT CHAR(10);           /* Text string for decimal value of string */
      342     4837    2   DCL PREL_DISPLACEMENT SBIN(20) UNAL;
      343     4838    2   DCL DISPLACEMENT UBIN WORD;
      344     4839    2   DCL 1 R_DISPLACEMENT REDEF DISPLACEMENT,
      345     4840    2         2 * UBIN(16) UNAL,
      346     4841    2         2 HIGH_ORDER_BITS UBIN(4) UNAL,
      347     4842    2         2 LOW_ORDER_BITS UBIN(16) UNAL;
      348     4843    2   DCL POSITION UBIN WORD;
      349     4844    2   DCL SPREL CHAR(7) CONSTANT INIT(',SPREL^');
      350     4845    2   DCL VALUE SBIN WORD;
      351     4846    2   DCL DDR$POSIT ENTRY(5) ALTRET;
      352     4847    2   DCL SYM_BUF$ PTR;
      353     4848    2   DCL SYM_TEXT CHAR(64);
      354     4849    2   DCL SYM_LEN UBIN WORD;
      355     4850    2   DCL OFFSET SBIN WORD;                   /* Offset to that symbol              */
      356     4851    2   DCL SYMBOL_ADDR UBIN  WORD;             /* Addr of the symbol to be found     */
      357     4852    2   DCL T_SIGNED_16BITS SBIN(16) UNAL;
      358     4853    2   DCL SIGNED_16BITS SBIN(16) UNAL;
      359     4854    2   DCL SIGN UBIN WORD;
      360     4855                                                                                    /**/
      361     4856    2           SYM_BUF$ = ADDR (SYM_TEXT);
      362     4857    2           DISPLACEMENT = 0;
      363     4858    2           RASSED_INSTR.BUFFER = INSTRUCTIONS( INSTRUCTION.INDEXX );
      364     4859    3   DECODE_BRANCH: DO CASE( BRANCH_FRAME.VALUE );
      365     4860                                                                                    /**/
      366     4861    3            CASE( 0 );                /* Branch to address ( 20 bits worth )     */
      367     4862    3              R_DISPLACEMENT.HIGH_ORDER_BITS = INSTR_ARRAY(1);
      368     4863    3              R_DISPLACEMENT.LOW_ORDER_BITS = INSTR_ARRAY(2);
      369     4864    3              SYMBOL_ADDR = DISPLACEMENT;
      370     4865    3              CALL DDR$POSIT( SYMBOL_ADDR, SYM_BUF$, 64, SYM_LEN );
      371     4866    3              IF TAB
      372     4867    4              THEN DO;
      373     4868    4                   S_D.L6_TAB = '1'B;
      374     4869    4                   S_D.L6_TAB_VALUE = SYMBOL_ADDR;
      375     4870    4                 END;
      376     4871    4              IF SYM_LEN = 0 THEN DO ;  /* DDR$POSIT didn't find a match     */
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:12   
      377     4872    4                 CALL DFR$BINHEX( DISPLACEMENT, TEXT_HEX, L_ZEROS );
      378     4873    4              IF TAB
      379     4874    5              THEN DO;
      380     4875    5                      S_D.L6_TAB = '1'B;
      381     4876    5                      S_D.L6_TAB_VALUE = DISPLACEMENT;
      382     4877    5                   END;
      383     4878    4                 CALL INDEX( POSITION, ' ', RASSED_INSTR.BUFFER );
      384     4879    4                 POSITION = POSITION + 1;
      385     4880    4                 IF L_ZEROS = 8 THEN CALL INSERT( RASSED_INSTR.BUFFER, POSITION,, '.0^'
              4880                          );
      386     4881    4                 ELSE
      387     4882    4                    CALL INSERT( RASSED_INSTR.BUFFER, POSITION,, '.', SUBSTR( TEXT_HEX,
              4882                             L_ZEROS, 8-L_ZEROS ), '^' );
      388     4883    4                 CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
      389     4884    4                 END;
      390     4885    4              ELSE DO;
      391     4886    4                 CALL INDEX( POSITION, ' ', RASSED_INSTR.BUFFER );
      392     4887    4                 POSITION = POSITION + 1;
      393     4888    4                 CALL INSERT( RASSED_INSTR.BUFFER, POSITION,, SUBSTR( SYM_TEXT, 0,
              4888                          SYM_LEN ) );
      394     4889    4                 RASSED_INSTR.COUNT = POSITION + SYM_LEN;
      395     4890    4                 END;
      396     4891    3              RASSED_INSTR.WORDS_USED = 3;
      397     4892                                                                                    /**/
      398     4893    3            CASE( 1 );                     /* Prel branch                        */
      399     4894    3              SIGNED_16BITS = INSTR_ARRAY(1);
      400     4895    3              DISPLACEMENT = SIGNED_16BITS;
      401     4896    4              IF SIGNED_16BITS < 0 THEN DO;
      402     4897    4                 T_SIGNED_16BITS = SIGNED_16BITS;
      403     4898    4                 T_SIGNED_16BITS = -T_SIGNED_16BITS;
      404     4899    4                 DISPLACEMENT = T_SIGNED_16BITS;
      405     4900    4                 END;
      406     4901    3              IF SIGNED_16BITS >= 0 THEN DISPLACEMENT = DISPLACEMENT + 1 ;
      407     4902    3              ELSE
      408     4903    3                 DISPLACEMENT = DISPLACEMENT - 1;
      409     4904    3              CALL DFR$BINHEX( DISPLACEMENT, TEXT_HEX, L_ZEROS );
      410     4905    3              IF TAB
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:13   
      411     4906    4              THEN DO;
      412     4907    4                   S_D.L6_TAB = '1'B;
      413     4908    4                   IF SIGNED_16BITS < 0
      414     4909    4                   THEN S_D.L6_TAB_VALUE = S_D.O$.ADDRESS - DISPLACEMENT;
      415     4910    4                   ELSE S_D.L6_TAB_VALUE = S_D.O$.ADDRESS + DISPLACEMENT;
      416     4911    4                END;
      417     4912    3              CALL INDEX( POSITION, ' ', RASSED_INSTR.BUFFER );
      418     4913    3              POSITION = POSITION + 1;
      419     4914    3              IF SIGNED_16BITS < -1 THEN CALL INSERT( RASSED_INSTR.BUFFER, POSITION,,
              4914                       '$O-.', SUBSTR( TEXT_HEX, L_ZEROS, 8-L_ZEROS ), PREL );
      420     4915    3              ELSE IF SIGNED_16BITS = -1 THEN CALL INSERT( RASSED_INSTR.BUFFER,
              4915                       POSITION,, '$O', PREL );
      421     4916    3                   ELSE CALL INSERT( RASSED_INSTR.BUFFER, POSITION,, '$O+.', SUBSTR(
              4916                            TEXT_HEX, L_ZEROS, 8-L_ZEROS ), PREL );
      422     4917    3              CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
      423     4918    3              RASSED_INSTR.WORDS_USED = 2;
      424     4919                                                                                    /**/
      425     4920    3            CASE ( ELSE );            /* Sprel branch values from -64 to +63     */
      426     4921    3              VALUE = BRANCH_FRAME.VALUE;
      427     4922    3              IF VALUE < 0 THEN VALUE = -VALUE;
      428     4923                                                                                    /**/
      429     4924    3              CALL DFR$BINHEX( VALUE, TEXT_HEX, L_ZEROS );
      430     4925    3              IF TAB
      431     4926    4              THEN DO;
      432     4927    4                   S_D.L6_TAB = '1'B;
      433     4928    4                   IF BRANCH_FRAME.VALUE < 0
      434     4929    4                   THEN S_D.L6_TAB_VALUE = S_D.O$.ADDRESS - VALUE;
      435     4930    4                   ELSE S_D.L6_TAB_VALUE = S_D.O$.ADDRESS + VALUE;
      436     4931    4                 END;
      437     4932    3              CALL INDEX( POSITION, ' ', RASSED_INSTR.BUFFER );
      438     4933    3              POSITION = POSITION + 1;
      439     4934    3              IF BRANCH_FRAME.VALUE < 0 THEN CALL INSERT( RASSED_INSTR.BUFFER, POSITION
              4934                       ,, '$O-.',
      440     4935    3                   SUBSTR( TEXT_HEX, L_ZEROS, 8-L_ZEROS ), SPREL );
      441     4936    3                   ELSE CALL INSERT( RASSED_INSTR.BUFFER, POSITION,, '$O+.', SUBSTR(
              4936                            TEXT_HEX, L_ZEROS, 8-L_ZEROS ), SPREL );
      442     4937    3              CALL INDEX( RASSED_INSTR.COUNT, '^', RASSED_INSTR.BUFFER );
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:14   
      443     4938    3              RASSED_INSTR.WORDS_USED = 1;
      444     4939    3            END DECODE_BRANCH;
      445     4940    2           RETURN;
      446     4941    2   END BRANCH;
      447     4942        %EJECT;
PL6.E3A0      #001=DFR$INSTR File=DFR$INSTR.:E05TSI                              TUE 07/29/97 14:41 Page:15   
      448     4943    1   SHORT_VALUE_IMMEDIATE: PROC;
      449     4944    2   DCL VALUE_TEXT CHAR(10);
      450     4945    2   DCL VALUE SBIN WORD;
      451     4946    2   DCL POSITION UBIN WORD;
      452     4947                                                                                    /**/
      453     4948    2           RASSED_INSTR.BUFFER = INSTRUCTIONS( INSTRUCTION.INDEXX );
      454     4949    2           VALUE = SV.VALUE;
