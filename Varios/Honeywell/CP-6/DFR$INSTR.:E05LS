      655     5150    1   DECODE_ADDR_SYL: PROC ALTRET;
      656     5151    2   DCL ADDRESS UBIN WORD;
      657     5152    2   DCL 1 R_ADDRESS REDEF ADDRESS,
      658     5153    2         2 * UBIN(16) UNAL,
      659     5154    2         2 HIGH_ORDER_BITS UBIN(4) UNAL,
      660     5155    2         2 LOW_ORDER_BITS UBIN(16) UNAL;
      661     5156    2   DCL 1 R_ADDRESS_32BITS REDEF ADDRESS,
      662     5157    2         2 * UBIN(4) UNAL,
      663     5158    2         2 HIGH_ORDER_BITS UBIN(16) UNAL,
      664     5159    2         2 LOW_ORDER_BITS UBIN(16) UNAL;
      665     5160    2   DCL DISPLACEMENT UBIN WORD;
      666     5161    2   DCL SYM_BUF$ PTR;
      667     5162    2   DCL SYM_TEXT CHAR(64);
      668     5163    2   DCL SYM_LEN UBIN WORD;
      669     5164    2   DCL OFFSET SBIN WORD;
      670     5165    2   DCL SIGNED_VALUE SBIN(16) UNAL;
      671     5166    2   DCL SIGNED_16BITS SBIN(16) UNAL;
      672     5167    2   DCL T_SIGNED_16BITS SBIN(16) UNAL;
      673     5168    2   DCL T_SIGNED_VALUE SBIN(16);
      674     5169    2   DCL UNSIGNED_VALUE UBIN(16) UNAL;
      675     5170    2   DCL UNSIGNED_VALUE_20BITS UBIN(20) UNAL;
      676     5171    2   DCL SIGNED_VALUE_32BITS SBIN(32) ;
      677     5172    2   DCL T_SIGNED_VALUE_32BITS SBIN(32) UNAL;
      678     5173    2   DCL SIGN SBIN WORD;
      679     5174    2   DCL TEXTX CHAR(5);
      680     5175    2   DCL POSITION UBIN WORD;
      681     5176    2   DCL START UBIN WORD;
      682     5177    2   DCL LEN UBIN WORD;
      683     5178    2   DCL B_INDEXED BIT(1) ALIGNED;
      684     5179    2   DCL N UBIN;
      685     5180    2   DCL 1 V,
      686     5181    2         2 * UBIN,
      687     5182    2         2 X(0:1) UBIN,
      688     5183    2         2 *(0:13) UBIN;
