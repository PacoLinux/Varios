   10      %MACRO FM$RANEA (BASED=BASED,NAME=FM$RANEA);
   11      DCL 1 NAME BASED ALIGNED,
   12            2 * UBIN,
   13            2 CNT SBIN,
   14            2 STAMP BIT(36),
   15            2 BLKNO UBIN,
   16            2 VPNO UBIN,
   17            2 PGS UBIN,
   18            2 TYP UBIN;
   19      %MEND;
   20      /**/

