       10       10        /*P*      NAME: X9ER$PRINT_ERROR
       11       11               PURPOSE: X9ER$PRINT_ERROR RECEIVES ERROR MESSAGES AND PRINTS
       12       12                        THEM OUT USING THE SYSTEM WRITE ROUTINE ( M$WRITE ).
       13       13
       14       14        */
       15       15            %INCLUDE   CP_6;
       16     5575            %INCLUDE   CP_6_SUBS;
       17     6115            %INCLUDE    X8A_LIBCOM;
       18     6335
       19     6336    1       DCL X9LP$GETDCB#        ENTRY;
       20     6337            %FPT_WRITE      (FPTN=WRITE_ERROR, VFC=YES, DCB=M$DO);
       21     6368            %FPT_WRITE       ( FPTN=WRITE_M$LO, VFC = YES );
       22     6399            %FPT_CORRES      ( DCB1 = M$DO );
       23     6413
       24     6414    1       DCL M$DO         DCB;
       25     6415    1       DCL ERROR_MSG    CHAR(80);
       26     6416    1       DCL 1 ERR_MSG,
       27     6417    1            2 VFC       CHAR(1)  UNAL,
       28     6418    1            2 ERROR_MSG CHAR(80) UNAL;
       29     6419
       30     6420    1       IF NOT LIBCOM.M_LO_ASSIGNED THEN
       31     6421    2         DO;
       32     6422    2           CALL X9LP$GETDCB#;
       33     6423    2           LIBCOM.M_LO_ASSIGNED = %TRUE;
       34     6424    2         END;
       35     6425
       36     6426    1       WRITE_M$LO.V.DCB# = LIBCOM.M_LO;
       37     6427    1       FPT_CORRES.V.DCB2# = LIBCOM.M_LO;
PL6.E3A0      #001=X9ER$PRINT_ERROR File=X9ER.:X8A6TSI                           SAT 08/09/97 10:55 Page:2    
       38     6428    1       ERR_MSG.VFC       = 'a';    /* lowercase a */
       39     6429    1       ERR_MSG.ERROR_MSG = ERROR_MSG;
       40     6430
       41     6431    1      CALL M$CORRES ( FPT_CORRES )
       42     6432    1      WHENRETURN
       43     6433    2        DO;
       44     6434    2         WRITE_ERROR.BUF_ = VECTOR( ERR_MSG );
       45     6435    2         CALL M$WRITE ( WRITE_ERROR );
       46     6436    2        END;
       47     6437
       48     6438    1       WRITE_M$LO.BUF_ = VECTOR ( ERR_MSG );
       49     6439    1       CALL M$WRITE ( WRITE_M$LO );
       50     6440    1       ERR_MSG.ERROR_MSG = ' ';
       51     6441
       52     6442    1   END X9ER$PRINT_ERROR; /* proc */
       53     6443        %EOD;

