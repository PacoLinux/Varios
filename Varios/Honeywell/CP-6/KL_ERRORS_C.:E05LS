   28      %EQU E$NGPRS=1216 ;           /* SPECIFIED PROFILE NO GOOD - TS ALLOWED ON    */
   29      %EQU E$NOPRS=1217 ;                /* DEFAULT PROFILE NO GOOD - TS ALLOWED ON */
