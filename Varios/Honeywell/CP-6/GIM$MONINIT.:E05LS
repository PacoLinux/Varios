      500    17599
      501    17600        /* Initialize PPUT.
      502    17601        */
      503    17602    1      G$BOBCAT.PPUT$ = G$PPUT$;
      504    17603    2      DO CURR = G$BOBCAT.MEM_LOW TO GM_SHUF_HI;
      505    17604    2           GM$PPUTA(CURR) = GM_PPUTS;
      506    17605    2           END;
      507    17606    2      DO CURR = GM_SHUF_HI + 1 TO G$BOBCAT.MEM_HI;
      508    17607    2           GM$PPUTA(CURR) = GM_PPUTNS;
      509    17608    2           END;
      510    17609
      511    17610        /* Statistical and control cells */
      512    17611    1      IF (GM_MEM_LOW >= GM_MEM_HI) OR (GM_SHUF_HI > GM_MEM_HI)
      513    17612    1      THEN CALL SCREECH;
      514    17613
      515    17614    1      GM_UHJIT_PGSZ = (SIZEW(G$UHJIT)+255) / 256;
      516    17615    1      GM_HHJIT_PGSZ = (SIZEW(G$HHJIT)+255) / 256;
      517    17616    1      GM_ROS_PGSZ = (SIZEW(G$ROS)+255) / 256;
      518    17617    1      GM_INIT_PGSZ = GM_UHJIT_PGSZ + GM_ROS_PGSZ + GM_TSTKU_PGSZ;
      519    17618        /* Shufflable allocable memory pool */
      520    17619    1      SIZE = GM_SHUF;
      521    17620    1      CALL GMA$LOGBASE2 (LOG,SIZE);
      522    17621    1      GM_MEAT_SHUF.HEAD (LOG) = GM_MEM_LOW;
      523    17622    1      GM_MEAT_SHUF.TAIL (LOG) = GM_MEM_LOW;
      524    17623    1      GM_MEAT_SHUF.COUNT (LOG) = 1;
      525    17624    1      PPUT$ = ADDR (GM$PPUTA (GM_MEM_LOW));
      526    17625    1      GM$PPUT.C.FIRSTP = '1'B;
      527    17626    1      GM$PPUT.SIZE = SIZE;
      528    17627    1      CALL GMA$MAP (GM_MEM_LOW,SIZE);
      529    17628    1      GM$EXTENT.FLINK = 0;
      530    17629    1      GM$EXTENT.BLINK = 0;
      531    17630    1      PPUT$ = ADDR (GM$PPUTA (GM_SHUF_HI));
      532    17631    1      GM$PPUT.SIZE = SIZE;
      533    17632    1      GM$PPUT.C.LASTP = '1'B;
      534    17633        /* Non-shufflable allocable memory pool */
      535    17634    1      SIZE = GM_NSHF;
      536    17635    1      IF SIZE > 0
PL6.E3A0      #002=GIM$FINISH_INIT File=GIM$MONINIT.:E05TSI                      TUE 07/29/97 21:47 Page:72   
      537    17636    1      THEN
      538    17637    2      DO;
      539    17638    2           CALL GMA$LOGBASE2 (LOG,SIZE);
      540    17639    2           CURR = GM_SHUF_HI + 1;
      541    17640    2           GM_MEAT_NSHF.HEAD (LOG) = CURR;
      542    17641    2           GM_MEAT_NSHF.TAIL (LOG) = CURR;
      543    17642    2           GM_MEAT_NSHF.COUNT (LOG) = 1;
      544    17643    2           PPUT$ = ADDR (GM$PPUTA (CURR));
      545    17644    2           GM$PPUT.C.FIRSTP = '1'B;
      546    17645    2           GM$PPUT.SIZE = SIZE;
      547    17646    2           CALL GMA$MAP (CURR,SIZE);
      548    17647    2           GM$EXTENT.FLINK = 0;
      549    17648    2           GM$EXTENT.BLINK = 0;
      550    17649    2           PPUT$ = ADDR (GM$PPUTA (GM_MEM_HI));
      551    17650    2           GM$PPUT.SIZE = SIZE;
      552    17651    2           GM$PPUT.C.LASTP = '1'B;
      553    17652    2           END;
      554    17653
      555    17654        /* Initialize the event buffer and transport trace buffer in PPUT
      556    17655           below GM_MEM_LOW.
      557    17656        */
      558    17657    1      G_EVBUF# = 100;
      559    17658    1      KNT_TRC_BUF# = 100;
      560    17659    1      SIZE = (SIZEW(GM$PPUT) * GM_MEM_LOW-1);
      561    17660
      562    17661    2      DO WHILE (  (SIZEW(G$EVBUF)*G_EVBUF#) +8 +(SIZEW(KNT$TRC_BUF)*KNT_TRC_BUF#) >
             17661               SIZE);
      563    17662    2           G_EVBUF# = G_EVBUF# -1;
      564    17663    2           KNT_TRC_BUF# = KNT_TRC_BUF# -1;
      565    17664    2           END;
      566    17665
      567    17666    1      G$EVBUF$ = G$BOBCAT.PPUT$;
      568    17667    1      G$BOBCAT.EVBUF$ = G$BOBCAT.PPUT$;
      569    17668    1      G$BOBCAT.EVBUF# = G_EVBUF#;
      570    17669
      571    17670    1      TRC_BUF$ = PINCRW(G$EVBUF$, SIZEW(G$EVBUF)*(G_EVBUF#+1) );
      572    17671    1      TRC_BUF = ( (TRC_BUF+7) / 8) * 8;
