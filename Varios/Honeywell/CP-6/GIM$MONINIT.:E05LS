      118     6069
      119     6070        /* Initialize User Table Info.
      120     6071        */
      121     6072    1      NEXT = ( (NEXT+7) /8) * 8;
      122     6073    1      G$BOBCAT.USRT$ = NEXT$;
      123     6074    1      G$USRT$ = G$BOBCAT.USRT$;
      124     6075    1      G_MUAIS = G$BOBCAT.MUIS;
      125     6076
      126     6077    1      I$ = G$BOBCAT.USRT$;
      127     6078    2      DO I = 1 TO G$BOBCAT.MUIS;
      128     6079    2           G$USER(I) = '0'B;
      129     6080    2           G$USER.US(I) = %GH_NULL;
      130     6081    2           G$USER.FL(I) = I + 1;
      131     6082    2           G$USER.BL(I) = I - 1;
      132     6083    2           END;
      133     6084
      134     6085    1      G$USER.FL(G$BOBCAT.MUIS) = 0;
      135     6086    1      G$USER(0) = '0'B;
      136     6087
      137     6088        /* Initialize the Schedular State Table head and tail for each state.
      138     6089        */
      139     6090    2      DO I = 0 TO %GH_NULL-1;
      140     6091    2           GH_HQ(I)=0;
      141     6092    2           GH_TQ(I)=0;
      142     6093    2           END;
      143     6094    1      GH_HQ(%GH_NULL)=1;
      144     6095    1      GH_TQ(%GH_NULL)=G$BOBCAT.MUIS;
      145     6096
      146     6097    1      NEXT$ = PINCRW(NEXT$,SIZEW(G$USER)*(G$BOBCAT.MUIS+1));
      147     6098
      148     6099        /* Initialize DOLIST entries.
      149     6100        */
      150     6101    1      G$BOBCAT.DOLIST$ = NEXT$;
      151     6102    1      G_DL$ = NEXT$;
      152     6103    1      I$ = NEXT$;
      153     6104    1      IF G$BOBCAT.MUIS >10
      154     6105    1      THEN J=G$BOBCAT.MUIS;
PL6.E3A0      #001=GIM$BUILD_BOBCAT File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:47 Page:6    
      155     6106    1      ELSE J=10;
      156     6107
      157     6108    2      DO I = 1 TO J;
      158     6109    2           IF I=J
      159     6110    2           THEN G$DO.LNK$ = ADDR(NIL);
      160     6111    2           ELSE G$DO.LNK$ = PINCRW(I$,SIZEW(G$DO));
      161     6112    2           I$ = G$DO.LNK$;
      162     6113    2           END;
      163     6114
      164     6115    1      NEXT$ = PINCRW(NEXT$,SIZEW(G$DO)*J);
      165     6116    1      NEXT = ( (NEXT+1) /2) * 2;
      166     6117
      167     6118        /* Now inialize the KN$DCT$ entrys.
      168     6119        */
      169     6120    1      G$BOBCAT.DCT$$ = NEXT$;
      170     6121    1      KN_DCT$$ = NEXT$;
