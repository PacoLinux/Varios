      181     6132
      182     6133    2      DO I = 1 TO G$BOBCAT.LDCT#;
      183     6134    2           G$BOBCAT.DCT$$->KN$DCT$ (I) = ADDR(NIL);
      184     6135
      185     6136    2           TV$ = NEXT$;
      186     6137    2           TV$->KN$LDCT = '0'B;
      187     6138    2           NEXT$ = PINCRW(NEXT$,SIZEW(KN$LDCT));
      188     6139    2           TV$->KN$LDCT.LDCTX = I;
      189     6140    2           TV$->KN$LDCT.LNK$ = NEXT$;
      190     6141    2           END;
      191     6142
PL6.E3A0      #001=GIM$BUILD_BOBCAT File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:47 Page:7    
      192     6143    1      KN_LDCT_END$ = TV$;
      193     6144    1      TV$->KN$LDCT.LNK$ = ADDR(NIL);
