PL6.E3A0      #003=GIM$BUILD_CHNTBL File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:49 Page:128  
      841      888    1                                     'GP', /* '01F9'X                            */
      842      889    1                                     'GP', /* '01FA'X                            */
      843      890    1                                     'GP', /* '01FB'X                            */
      844      891    1                                     'GP', /* '01FC'X                            */
      845      892    1                                     'GP', /* '01FD'X                            */
      846      893    1                                     'GP', /* '01FE'X                            */
      847      894    1                                     'GP', /* '01FF'X                            */
      848      895    1                                        '  '*0);
      849      896
      850      897        /* Table, indexed by the FLAP's extended device ID, yielding
      851      898        additional G_ATTRs to be ORed into the G_ATTRs derived from
      852      899        the adapter's extended device ID.
      853      900        */
      854      901        %EQU Y#FLAP_ATTR_HI = 16;
      855      902    1   DCL Y_FLAP_ATTR (0:%Y#FLAP_ATTR_HI) BIT(16) CONSTANT INIT(
      856      903    1      '0'X,                                /* 00 (none)                          */
      857      904    1      %(G_ATTR_CL_RS232),                  /* 01 (non-NRZI)                      */
      858      905    1      %(G_ATTR_CL_RS422C),                 /* 02 (RS-449 unbalanced)             */
      859      906    1      %(G_ATTR_CL_RS422C),                 /* 03 (RS-449 balanced)               */
      860      907    1      %(G_ATTR_CL_CURLP),                  /* 04                                 */
      861      908    1      %(G_ATTR_CL_WIDE),                   /* 05 (X.21)                          */
      862      909    1      %(G_ATTR_CL_ACU),                    /* 06 (RS366)                         */
      863      910    1      %(G_ATTR_CL_301|G_ATTR_CL_WIDE),     /* 07 (303)                           */
      864      911    1      %(G_ATTR_CL_V35|G_ATTR_CL_WIDE),     /* 08                                 */
      865      912    1      %(G_ATTR_CL_RS232),                  /* 09                                 */
      866      913    1      %(G_ATTR_CL_MIL188C),                /* 0A                                 */
      867      914    1      '0'X*0);                             /* spares                             */
      868      915
      869      916        /* Auto */
      870      917    1   DCL CTRINDX UBIN;
      871      918    1   DCL LSTCTRINDX UBIN;
      872      919    1   DCL DEVICE BIT(16);
      873      920    1   DCL DEVICEU REDEF DEVICE UBIN;
      874      921    1   DCL LASTCHN$ PTR;
      875      922    1   DCL I UBIN;
      876      923        %GH_IO (STCLASS="");
      877      939    1   DCL CHANNEL UBIN;
PL6.E3A0      #003=GIM$BUILD_CHNTBL File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:49 Page:129  
      878      940
      879      941        /* Entries */
      880      942    1   DCL GHH$IO_IN ENTRY(1) ALTRET;
      881      943
      882      944    1      GH_IO.CAS.FNC = %GH#IO_INPDVCID;
      883      945    1      G$BOBCAT.CHNLOW = 0;
      884      946    1      G$BOBCAT.CHNLOW$ = CHN$;
      885      947
      886      948        /*
      887      949             Find the highest channel
      888      950         */
      889      951    2      DO CHANNEL = 16 TO 1023 BY 2;
      890      952    2           GH_IO.CAS.CHN = CHANNEL;
      891      953    2           CALL GHH$IO_IN(GH_IO)
      892      954    3           WHENRETURN DO;
      893      955    3                G$BOBCAT.MAXCHN = CHANNEL;
      894      956    3                G$BOBCAT.CHNHI = CHANNEL * 64 + 63;
      895      957    3                END;
      896      958    2           END;
      897      959
      898      960    1      CHANNEL = G$BOBCAT.CHNHI / 64;
      899      961    1      CHANNEL = MINIMUM(1023,CHANNEL + GI_PARAMS.PARAMS(%GI_BOOT_PCHANS));
      900      962    1      G$BOBCAT.CHNHI = CHANNEL * 64 + 63;
      901      963
      902      964        /*
      903      965             Now we go through all the channels and fill in the channel table
      904      966         */
      905      967    2      DO CHANNEL = 0 TO G$BOBCAT.CHNHI/64 BY 2;
      906      968
      907      969    2           KH$CHN = GI_CHN;                /* initialize table                   */
      908      970    2           GH_IO.CAS.FNC = %GH#IO_INPDVCID;
      909      971    2           GH_IO.CAS.CHN = CHANNEL;
      910      972    2           IF CHANNEL >= 16                /* Dont do CPU channels               */
      911      973    3           THEN DO;
      912      974    3                CALL GHH$IO_IN(GH_IO)
      913      975    4                WHENRETURN DO;
      914      976    4                     KH$CHN.DVCID = GH_IO.STATUS;
PL6.E3A0      #003=GIM$BUILD_CHNTBL File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:49 Page:130  
      915      977    4                     END;
      916      978    3                KH$CHN.CHNBIT0_9 = CHANNEL;
      917      979    3                KH$CHN.TERMID.TERM.CHANNEL = CHANNEL * 64;
      918      980    3                KH$CHN.TERMID.LEV = 1;
      919      981    3                IF GI_KEEPDELTA AND CHANNEL = GD_CHN /* Debugger has this channel */
      920      982    3                THEN
      921      983    3                     KH$CHN.STT = %KH#CHN_STT_RMTDBG;
      922      984    3                GH_IO.CAS.FNC = %GH#IO_INPEXTDVCID;
      923      985    3                IF KH$CHN.DVCID = '2178'X OR KH$CHN.DVCID = '2978'X
      924      986    3                THEN
      925      987    3                     CALL GHH$IO_IN(GH_IO)
      926      988    4                     WHENRETURN DO;
      927      989    4                          KH$CHN.EXTDVCID = GH_IO.STATUS;
      928      990    4                          END;
      929      991        /*
      930      992             Determine if this is the first channel on the MLCP or MLC-16
      931      993             and set the CTR bit if it is
      932      994         */
      933      995    4                DO SELECT (KH$CHN.DVCID&'FF00'X);
      934      996    4                     SELECT '2100'X;       /* MLC-8 (MLCP)                       */
      935      997    4                     CTRINDX = (CHANNEL/16)*16;
      936      998    4                     IF KH$CHN.DVCID = '2178'X /* Need extended                  */
      937      999    4                     THEN
      938     1000    4                          DEVICE = KH$CHN.EXTDVCID & 'FF00'X; /* top byte only   */
      939     1001    4                     ELSE
      940     1002    4                          DEVICE = KH$CHN.DVCID;
      941     1003    4                     SELECT '2900'X;       /* MLC-16 (always use extended ID)    */
      942     1004    4                     CTRINDX = (CHANNEL/32)*32;
      943     1005    4                     DEVICE = KH$CHN.EXTDVCID & 'FF00'X;
              1005                              /* top byte (adapter type) only */
      944     1006        /*
      945     1007        If the FLAP extended device ID is legal, prime the ATTR bits with
      946     1008        those corresponding to this FLAP.
      947     1009        */
      948     1010    4                     IF BITBIN(KH$CHN.EXTDVCID & '00FF'X) <= %Y#FLAP_ATTR_HI
      949     1011    4                     THEN
      950     1012    4                          KH$CHN.ATTR = Y_FLAP_ATTR(BITBIN(KH$CHN.EXTDVCID & '00FF'X));
PL6.E3A0      #003=GIM$BUILD_CHNTBL File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:49 Page:131  
      951     1013    4                     SELECT ELSE;
      952     1014    4                CTRINDX = CHANNEL;
      953     1015    4                DEVICE = KH$CHN.DVCID;
      954     1016    4                     END;                  /* END DO SELECT KH$CHN.DVCID         */
      955     1017    3           IF CTRINDX ~= LSTCTRINDX
      956     1018    3           THEN
      957     1019    3                KH$CHN.CTR = '1'B;
      958     1020    3           LSTCTRINDX = CTRINDX;
      959     1021
      960     1022        /*
      961     1023             Find the RES and ATTR for this device class
      962     1024         */
      963     1025
      964     1026    4           DO I = 0 TO %Y#DVCCLS_HI;
      965     1027    4                IF DEVICE = GI_DVCCLS.DVCID(I)
      966     1028    5                THEN DO;
      967     1029    5                     KH$CHN.RESOURCE = GI_DVCCLS.DVCRES(I);
      968     1030    5                     KH$CHN.ATTR = KH$CHN.ATTR | GI_DVCCLS.DVC_ATTR(I);
      969     1031    5                     IF KH$CHN.CHNBIT0_9 = GI_BOOTCHAN
      970     1032    5                     THEN
      971     1033    5                          KH$CHN.STT = %KH#CHN_STT_UN_INT;
      972     1034    5                     I = %Y#DVCCLS_HI;
      973     1035    5                     END;
      974     1036    4                END;
      975     1037
      976     1038    3                END;
      977     1039    2           G$BOBCAT.CHNHI$ = CHN$;
      978     1040    2           CHN$ = PINCRW(CHN$,SIZEW(KH$CHN));
      979     1041    2           END/*do 0 to chanhi*/;
      980     1042
      981     1043        /*
      982     1044             Initialize the spare channel slots for controlers and devices
      983     1045         */
      984     1046    1      IF G$BOBCAT.DEVICES ~= 0
      985     1047    2      THEN DO;
      986     1048    2           G$BOBCAT.CHNFREE$ = CHN$;
      987     1049    2           KH$CHN = GI_CHN;
PL6.E3A0      #003=GIM$BUILD_CHNTBL File=GIM$MONINIT.:E05TSI                     TUE 07/29/97 21:49 Page:132  
      988     1050    2           LASTCHN$ = CHN$;
      989     1051    2           CHN$ = PINCRW(CHN$,SIZEW(KH$CHN));
      990     1052    3           DO I = 1 TO G$BOBCAT.DEVICES;
      991     1053    3                KH$CHN = GI_CHN;
      992     1054    3                LASTCHN$->KH$CHN.LNK$ = CHN$;
      993     1055    3                LASTCHN$ = CHN$;
      994     1056    3                CHN$ = PINCRW(CHN$,SIZEW(KH$CHN));
      995     1057    3                END/*do 1 to devices#*/;
      996     1058    2           END/*do if devices ~=0*/;
      997     1059
      998     1060    2      ELSE DO;
      999     1061    2           G$BOBCAT.CHNFREE$ = ADDR(NIL);
     1000     1062    2           END;
     1001     1063
     1002     1064    1      RETURN;
     1003     1065
     1004     1066    1   END GIM$BUILD_CHNTBL;

