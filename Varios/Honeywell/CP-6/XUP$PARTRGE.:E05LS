     1536     9510        %SUB FID=12/*FID*/;
     1537     9511        %SUB QUOTE=13/*QUOTE*/;
     1538     9512        %SUB SLASH=14/*SLASH*/;
     1539     9513        %SUB CONNECT=45/*CONNECT*/;
     1540     9514        %SUB XTRASYM=30/*XTRASYM*/;
     1541     9515        %SUB SYMSIZ=31/*SYMSIZ*/;
     1542     9516        %SUB NAMENUM=40/*NAMENUM*/;
     1543     9517        %EJECT;
PL6.E3A0      #002=XUP$BLDNODE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:40 Page:291  
     1544     9518    1   DCL CTRAN(0:30) CONSTANT SBIN INIT(0,0,%TYPE_ALTERN#,0,%TYPE_KONCAT#,
     1545     9519    1      %TYPE_LIST#,%TYPE_RALTERN#,%TYPE_LITERAL#,%TYPE_LEXEME#*18,0*0);
     1546     9520    1   DCL LEXTRAN (0:30) CONSTANT SBIN INIT(0*8, %LEX_DSTRING#, %LEX_BLANK#, 0, %LEX_END#,
     1547     9521    1      %LEX_FID#, %LEX_DSTRING#*2, %LEX_DECIMAL#, %LEX_OCTAL#, %LEX_SYMBOL#,
     1548     9522    1      %LEX_ALPHA#, %LEX_ASYM#, %LEX_ACCOUNT#, %LEX_ANUMERIC#, %LEX_PL6SYM#,
     1549     9523    1      %LEX_HEX#, %LEX_JOBNAME#, %LEX_NULL#, 0*0);
     1550     9524              /* NOTE: THE ENTRIES IN THIS TABLE MUST BE ORDERED AS IN THE NODES.*/
     1551     9525                    /*  BEGIN PROCEDURE  **/
     1552     9526    1      C=OUT$BLK.CODE;
     1553     9527    1      FLAG=-1;
     1554     9528    1      OUT$=ADDR(OUT$);
     1555     9529    1      IF NODE_.SEG.SEG=ADDR(OUT$)->SEG.SEG THEN FLAG=1;/*0=NAME,1=NOT*/
     1556     9530    1      IF SYM_.SEG.SEG=ADDR(OUT$)->SEG.SEG THEN FLAG=0;/*0=NAME,1=NOT*/
     1557     9531    1      IF C<=0 THEN C=KONCAT;
     1558     9532    1      IF C=OPTION OR C=REPEAT OR C=XTRASYM OR (OUT$BLK.NSUBLKS=2
     1559     9533    1        AND (OUT$BLK.SUBLK$(1)->OUT_BLK.CODE=CONNECT
     1560     9534    2        OR OUT$BLK.SUBLK$(1)->OUT_BLK.CODE=SYMSIZ)) THEN DO;
     1561     9535    2         IF FLAG=0 THEN P$=ADDR(OUT$);
     1562     9536    2         ELSE P$=ADDR(P$);
     1563     9537    2         IF OUT$BLK.NSUBLKS=1 OR C>=KONCAT
     1564     9538    2         THEN CALL XUP$BLDNODE(OUT$BLK.SUBLK$(0)->DATA,P$->DATA);
     1565     9539    3         ELSE DO;
     1566     9540    3            OUT$BLK.CODE=KONCAT;
     1567     9541    3            CALL XUP$BLDNODE(OUT$BLK,P$->DATA);
     1568     9542    3            END;
     1569     9543    2         IF FLAG~=0 THEN OUT$=P$;
     1570     9544    2         ELSE P$=OUT$;
     1571     9545    3         DO SELECT(C);
     1572     9546    3            SELECT(OPTION);
     1573     9547    3            P$->NODE.OPT='1'B;
     1574     9548    3            SELECT(REPEAT);
     1575     9549    3            P$->NODE.RPEAT='1'B;
     1576     9550    3            SELECT(KONCAT);
     1577     9551    3            IF OUT$BLK.SUBLK$(1)->OUT_BLK.CODE=CONNECT THEN P$->NODES.SKBL='0'B;
     1578     9552    4            ELSE DO;
     1579     9553    4               CALL CHARBIN(C,OUT$BLK.SUBLK$(1)->OUT$SYM.TEXT);
     1580     9554    4               P_PCB.HI_CHAR=OUT$BLK.SUBLK$(1)->OUT_BLK.CPOS;
PL6.E3A0      #002=XUP$BLDNODE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:40 Page:292  
     1581     9555    4               IF C>511 THEN UNWIND TO SYNTAXERR;
     1582     9556    4               P$->NODES.MAXL=C;
     1583     9557    4               IF P$ -> NODES.LEXTYPE ~= %LEX_DSTRING# AND NOT P$ -> NODES.XTRA
     1584     9558    5               THEN DO;
     1585     9559    5                  P$ -> NODES.XTRA = '1'B;
     1586     9560    5                  P$ -> NODES.COUNT = 0;
     1587     9561    5                  NODE_.NEXT$ = PINCRW(P$, SIZEW(P$ -> NODES));
     1588     9562    5                  END;
     1589     9563    4               END;
     1590     9564    3            SELECT(XTRASYM);
     1591     9565    3            P$=OUT$BLK.SUBLK$(1)->OUT_BLK.SUBLK$(0);
     1592     9566    3            TMPLIT.C=0;
     1593     9567    3            I=0;
     1594     9568    4            DO WHILE (I<P$->TC.C);
     1595     9569    4               CH=SUBSTR(P$->TC.T,I,1);
     1596     9570    5               IF CH='#' AND I+3<P$->TC.C THEN DO;
     1597     9571    5                  CH=SUBSTR(P$->TC.T,I,4);
     1598     9572    6                  IF CHC.U1.H=6 AND CHC.U2.H=6 AND CHC.U3.H=6 THEN DO;
     1599     9573    6                     CHC.U00=CHC.U1.L;
     1600     9574    6                     CHC.U01=CHC.U2.L;
     1601     9575    6                     CHC.U02=CHC.U3.L;
     1602     9576    6                     I=I+3;
     1603     9577    6                     END;
     1604     9578    5                  END;
     1605     9579    4               TMPLIT.T(TMPLIT.C)=CH;
