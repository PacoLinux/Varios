     1757     9731    3         DO CASE (OUT$ -> NODES.LEXTYPE);
     1758     9732    3         CASE (%LEX_BLANK#, %LEX_END#);
     1759     9733    3            C = 4;
     1760     9734    3         CASE (%LEX_DSTRING#);
     1761     9735    3            C = LENGTHC(OUT$ -> NODED);
     1762     9736    3         CASE (ELSE);
     1763     9737    3            IF OUT$->NODES.XTRA THEN C=LENGTHC(OUT$ -> NODES);
     1764     9738    3            ELSE C=4;
     1765     9739    3         END;
     1766     9740    2      END;
     1767     9741    1      J=(C+3)/4;
     1768     9742              /* IF NO SUBNODES WERE GENERATED, MAYBE THERE IS ALREADY THIS NODE */
     1769     9743    1      IF POFFW(NODE_.NEXT$,OUT$)~=J*FLAG THEN RETURN;
     1770     9744    1      P$=NODE_.FIRST$;
     1771     9745    2      DO WHILE (P$<OUT$);
     1772     9746    3         IF OUT$->NODEC=P$->NODEC THEN DO; /* FOUND IT */
     1773     9747    3            NODE_.NEXT$=OUT$;
     1774     9748    3            OUT$=P$;
     1775     9749    3            RETURN;
     1776     9750    3            END;
     1777     9751    3         DO CASE (P$ -> NODE.TYPE);
     1778     9752    3         CASE (%TYPE_LITERAL#);
     1779     9753    3            P$ = PINCRW(P$, (LENGTHC(P$ -> LIT)+3)/4);
     1780     9754    3         CASE (%TYPE_LEXEME#);
     1781     9755    4            DO CASE (P$ -> NODES.LEXTYPE);
     1782     9756    4            CASE (%LEX_BLANK#, %LEX_END#);
     1783     9757    4               P$ = PINCRW(P$, 1);
     1784     9758    4            CASE (%LEX_DSTRING#);
     1785     9759    4               P$ = PINCRW(P$, 2);
     1786     9760    4            CASE (ELSE);
