     1886     8799    3         C=C+1;
     1887     8800    3         END;
     1888     8801    2      XR='0'B;
     1889     8802    2      XR.L#=-LINES.L#(C);
     1890     8803    2      SYM_.NEXT$=PINCRW(SYM_.NEXT$,1);
     1891     8804    2      RETURN;
