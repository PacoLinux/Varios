     1812     8673        %PARSE$SYM(NAME=PSYM,STCLASS=" ");
     1813     8711    1   DCL RET$ PTR;
     1814     8712    1   DCL NOCORERR REMEMBER SYMREF;
     1815     8713        %FPT_GDS (FPTN=SYMFPT, STCLASS=SYMREF);
     1816     8729    1   DCL 1 WORK_ DALIGNED SYMREF,
     1817     8730    1         2 SIZE UBIN(18),
