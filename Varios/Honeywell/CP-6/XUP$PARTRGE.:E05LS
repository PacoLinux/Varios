     1848     8761    1         2 * PTR,
     1849     8762    1         2 C UBIN WORD;
     1850     8763    1   DCL SYMSZS(0:31)SBIN SYMREF;
     1851     8764    1      P$=SYM_.FIRST$;
     1852     8765    2      DO WHILE (P$<SYM_.NEXT$);
     1853     8766    3         IF SYM.VAL>=0 THEN DO;
     1854     8767    4            IF SYM.T=PSYM.TEXT THEN DO;
     1855     8768    4               SYM.USED='1'B;
     1856     8769    4               RET$=P$;
     1857     8770    4               P$=PINCRW(P$,SIZEW(SYM));
     1858     8771    5               DO WHILE(P$->XR.LNK~=0);
     1859     8772    5                  P$=PINCRW(SYM_.FIRST$,P$->XR.LNK);
     1860     8773    5                  END;
     1861     8774    4               P$->XR.LNK=POFFW(SYM_.NEXT$,SYM_.FIRST$);
     1862     8775    4               CALL XREF;
     1863     8776    4               ALTRETURN;
     1864     8777    4               END;
     1865     8778    3            P$=PINCRW(P$,SIZEW(SYM));
     1866     8779    3            END;
     1867     8780    2         ELSE P$=PINCRW(P$,1);
     1868     8781    2         END;
     1869     8782    1      C = (PSYM.COUNT + 11) / 4;
     1870     8783    1      IF POFFW(SYM_.NEXT$,SYM_.FIRST$)+C-1>SYM_.SIZE
     1871     8784    1      THEN CALL M$GDS(SYMFPT) ALTRET(ERROR);
     1872     8785    1      SYMF.C=PSYM.COUNT;
     1873     8786    1      SYMSZS(SYMF.C)=SYMSZS(SYMF.C)+1;
     1874     8787    1      SYMSZS(0)=SYMSZS(0)+1;
     1875     8788    1      SYM.T=PSYM.TEXT;
     1876     8789    1      SYM.NODE$=ADDR(NIL);
PL6.E3A0      #003=XUP$BLDSYM File=XUP$PARTRGE.:E05TSI                           WED 07/30/97 09:41 Page:364  
     1877     8790    1      SYM_.NEXT$=PINCRW(P$,C);
     1878     8791    1      RET$=P$;
     1879     8792    1      CALL XREF;
     1880     8793    1      RETURN;
