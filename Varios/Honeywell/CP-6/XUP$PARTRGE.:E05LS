     1824     8737    1         2 S# SBIN(27)UNAL,
     1825     8738    1         2 UPC CHAR(1);
     1826     8739    1   DCL 1 SYM_ DALIGNED SYMREF,
     1827     8740    1         2 SIZE UBIN(18),
     1828     8741    1         2 FIRST$ PTR,
     1829     8742    1         2 P$ PTR,
     1830     8743    1         2 NEXT$ PTR;
     1831     8744    1   DCL P$ PTR;
     1832     8745    1   DCL C UBIN;
     1833     8746    1   DCL 1 SYM BASED(P$),
     1834     8747    1         2 NODE$ PTR,
     1835     8748    1         2 VAL REDEF NODE$ SBIN,
     1836     8749    1         2 DEF BIT(1),
     1837     8750    1         2 USED BIT(1),
     1838     8751    1         2 X BIT (1),
     1839     8752    1         2 * BIT (6),
PL6.E3A0      #003=XUP$BLDSYM File=XUP$PARTRGE.:E05TSI                           WED 07/30/97 09:41 Page:363  
     1840     8753    1         2 PRF UBIN HALF CALIGNED,
     1841     8754    1         2 C UBIN BYTE CALIGNED,
     1842     8755    1         2 T CHAR(SYM.C);
     1843     8756    1   DCL 1 XR BASED(SYM_.NEXT$)ALIGNED,
     1844     8757    1         2 L# SBIN(18)UNAL,
     1845     8758    1         2 D BIT(1),
     1846     8759    1         2 LNK UBIN(17)UNAL;
