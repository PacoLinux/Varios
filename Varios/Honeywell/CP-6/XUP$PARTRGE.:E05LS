PL6.E3A0      #001=XUP$PARTRGE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:39 Page:42   
     1343    19796    1   GENERATE_DNAME: PROC (STRING, NAME_POINTER);
     1344    19797
     1345    19798    2   DCL STRING CHAR (32);
     1346    19799    2   DCL SLEN UBIN;
     1347    19800    2   DCL NAME_POINTER UBIN;
     1348    19801
     1349    19802    2      CALL INDEX (SLEN, '!', STRING);
     1350    19803
     1351    19804    2      P_NAME.COUNT = SLEN;
     1352    19805    2      P_NAME.TEXT = SUBSTR (STRING, 0, SLEN);
     1353    19806
     1354    19807    2      CALL XUO$DBGNAME (P_NAME, NAME_POINTER);
     1355    19808
     1356    19809    2      RETURN;
     1357    19810
     1358    19811    2   END GENERATE_DNAME;
     1359    19812        %EJECT;
PL6.E3A0      #001=XUP$PARTRGE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:39 Page:43   
     1360    19813    1   GENERATE_LEVEL2: PROC (NAME_POINTER, DATATYPE, DATASIZE, ELEMENTS);
     1361    19814
     1362    19815    2   DCL NAME_POINTER UBIN;
     1363    19816    2   DCL DATATYPE UBIN;
     1364    19817    2   DCL DATASIZE UBIN;
     1365    19818    2   DCL ELEMENTS UBIN;
     1366    19819
     1367    19820    2   DCL ACTUAL_ELEMENTS UBIN;
     1368    19821    2   DCL ALIGNMENT UBIN;
     1369    19822    2   DCL ARRAY_TYPE UBIN;
     1370    19823    2   DCL ELEMENT_SIZE UBIN;
     1371    19824    2   DCL NEW_OFFSET UBIN;
     1372    19825
     1373    19826    2      IF ADDR(ELEMENTS) = ADDR(NIL)
     1374    19827    3      THEN DO;
     1375    19828
     1376    19829    3         ARRAY_TYPE = %SCALAR_ARRAY;
     1377    19830    3         ACTUAL_ELEMENTS = 1;
     1378    19831    3         ELEMENT_SIZE = 0;
     1379    19832
     1380    19833    3         END;
     1381    19834
     1382    19835    3      ELSE DO;
     1383    19836
     1384    19837    3         ARRAY_TYPE = %VECTOR0_ARRAY;
     1385    19838    3         ACTUAL_ELEMENTS = ELEMENTS;
     1386    19839
     1387    19840    4         DO CASE (DATATYPE);
     1388    19841
     1389    19842    4         CASE (%CHAR_DATATYPE);
     1390    19843
     1391    19844    4            ELEMENT_SIZE = 9;
     1392    19845
     1393    19846    4         CASE (%POINTER_DATATYPE);
     1394    19847
     1395    19848    4            ELEMENT_SIZE = 36;
     1396    19849
PL6.E3A0      #001=XUP$PARTRGE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:39 Page:44   
     1397    19850    4         CASE (ELSE);
     1398    19851
     1399    19852    4            ELEMENT_SIZE = 1;
     1400    19853
     1401    19854    4         END;
     1402    19855
     1403    19856    3         END;
     1404    19857
     1405    19858    3      DO CASE (DATATYPE);
     1406    19859
     1407    19860    3      CASE (%CHAR_DATATYPE);
     1408    19861
     1409    19862    3         ALIGNMENT = 3 /* byte aligned */;
     1410    19863    3         NEW_OFFSET = LOC_OFFSET + DATASIZE * ACTUAL_ELEMENTS * 9;
     1411    19864
     1412    19865    3      CASE (%POINTER_DATATYPE);
     1413    19866
     1414    19867    3         ALIGNMENT = 1 /* word aligned */;
     1415    19868    3         NEW_OFFSET = LOC_OFFSET + DATASIZE * ACTUAL_ELEMENTS * 36;
     1416    19869
     1417    19870    3      CASE (ELSE);
     1418    19871
     1419    19872    3         ALIGNMENT = 4 /* bit aligned */;
     1420    19873    3         NEW_OFFSET = LOC_OFFSET + DATASIZE * ACTUAL_ELEMENTS;
     1421    19874
     1422    19875    3      END;
     1423    19876
     1424    19877    2      CALL XUO$VREBL (LOC_OFFSET,
     1425    19878    2        1 /* static */,
     1426    19879    2        DATATYPE,
     1427    19880    2        '0'B /* not used */,
     1428    19881    2        '0'B /* not modified */,
     1429    19882    2        DATASIZE,
     1430    19883    2        2 /* level */,
     1431    19884    2        ALIGNMENT,
     1432    19885    2        0 /* size is actual */,
     1433    19886    2        ARRAY_TYPE,
PL6.E3A0      #001=XUP$PARTRGE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:39 Page:45   
     1434    19887    2        %OPERSECT,
     1435    19888    2        0 /* implied ptr; unused */,
     1436    19889    2        SECTION /* for relocation */,
     1437    19890    2        ELEMENT_SIZE,
     1438    19891    2        0 /* scale factor; unused */,
     1439    19892    2        ACTUAL_ELEMENTS,
     1440    19893    2        NAME_POINTER,
     1441    19894    2        HIGHEST_VREBL_ENTRY);
     1442    19895
     1443    19896    2      LOC_OFFSET = NEW_OFFSET;
     1444    19897
     1445    19898    2      RETURN;
     1446    19899
     1447    19900    2   END GENERATE_LEVEL2;
     1448    19901
     1449    19902    1   END XUP$PARTRGE;
