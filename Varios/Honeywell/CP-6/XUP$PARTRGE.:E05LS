     1750     9724    1   DUPCHK:;
     1751     9725    2      DO CASE(OUT$->NODE.TYPE);
     1752     9726    2      CASE(%TYPE_LITERAL#);
     1753     9727    2         C=LENGTHC(OUT$ -> LIT);
     1754     9728    2      CASE(%TYPE_ALTERN#,%TYPE_KONCAT#,%TYPE_LIST#,%TYPE_RALTERN#);
     1755     9729    2         C=OUT$->NODE.NSUBNDS*4+4;
