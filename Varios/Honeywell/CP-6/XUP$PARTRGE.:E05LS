     1959     3459    2      DO CASE(OUT_BLK.CODE);
     1960     3460    2      CASE(XDEF);
     1961     3461    2         SYM.X='1'B;
     1962     3462    2      CASE(OUT);
     1963     3463    2         NODECODE.OUT_F='1'B;
     1964     3464    2      CASE(PRE);
     1965     3465    2         NODECODE.PRE_F='1'B;
     1966     3466    2      CASE(SUCC);
     1967     3467    2         NODECODE.SUCC_F='1'B;
     1968     3468    2      CASE(FAIL);
     1969     3469    2         NODECODE.FAIL_F='1'B;
     1970     3470    2      CASE(NOOUT);
     1971     3471    2         NODECODE.NOUT_F='1'B;
     1972     3472    2      CASE(RTEXT);
     1973     3473    2         NODECODE.RTEXT_F='1'B;
     1974     3474    2         NODECODE.OUT_F='1'B;
     1975     3475    2      CASE(MAKEUC);
     1976     3476    2         NODECODE.MAKEUC_F='1'B;
     1977     3477    2      CASE(ELSE); /* SUBSTRUCTURE, LITCODE=N */
     1978     3478    3         IF OUT_BLK.NSUBLKS=1 THEN DO; /* CODE ALONE */
     1979     3479    3            CALL CHARBIN(I,ADDR(OUT_BLK) -> OUT$SYM.TEXT);
     1980     3480    4            IF I>65535 THEN DO;
     1981     3481    4               P_PCB.HI_CHAR=OUT_BLK.CPOS;
     1982     3482    4               UNWIND TO SYNTAXERR;
     1983     3483    4               END;
     1984     3484    3            NODECODE.CODE=I;
     1985     3485    3            NODECODE.OUT_F='1'B;
     1986     3486    3            END;
     1987     3487    3         ELSE DO CASE(OUT_BLK.SUBLK$(0)->OUT$SYM.CODE);
     1988     3488    3         CASE(LCODE);
     1989     3489    3            CALL CHARBIN(LITCODE,OUT_BLK.SUBLK$(1)->OUT$SYM.TEXT);
     1990     3490    3            LIT_PRESENT = '1'B;
     1991     3491    3         CASE(ELSE); /* MORE OF OTHER STUFF */
     1992     3492    3            I=0;
     1993     3493    4            DO WHILE(I<OUT_BLK.NSUBLKS);
     1994     3494    4               CALL XUP$BLDOPT(OUT_BLK.SUBLK$(I)->OUT$SYM);
     1995     3495    4               I=I+1;
PL6.E3A0      #004=XUP$BLDOPT File=XUP$PARTRGE.:E05TSI                           WED 07/30/97 09:41 Page:385  
     1996     3496    4               END;
     1997     3497    3         END;
     1998     3498    2      END;
     1999     3499    1   END;
