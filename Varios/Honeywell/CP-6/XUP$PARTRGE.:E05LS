     1524     9405        %P$NODE(NAME=NODE,STCLASS="BASED(NODE_.NEXT$)");
PL6.E3A0      #002=XUP$BLDNODE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:40 Page:290  
     1525     9499        %SUB OPTION=1/*OPTION*/;
     1526     9500        %SUB ALTERN=2/*ALTERN*/;
     1527     9501        %SUB REPEAT=3/*REPEAT*/;
     1528     9502        %SUB KONCAT=4/*KONCAT*/;
     1529     9503        %SUB LIST=5/*LIST*/;
     1530     9504        %SUB REDUCE=6/*REDUCE*/;
     1531     9505        %SUB LITERAL=7/*LITERAL*/;
     1532     9506        %SUB GENERIC_DSTRING=8/*Generic DSTRING*/;
     1533     9507        %SUB BLANK=9/*BLANK*/;
