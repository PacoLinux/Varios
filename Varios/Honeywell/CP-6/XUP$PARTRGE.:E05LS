     1456        6        XUP$BLDNODE:PROC(OUT$BLK,OUT$) ALTRET;
     1457        7        %INCLUDE CP_6;
     1458     5566    1   DCL NOCORERR REMEMBER SYMREF;
     1459     5567    1   DCL SYNTAXERR REMEMBER SYMREF;
     1460     5568    1   DCL XUP$BLDSYM ENTRY(2) ALTRET;
     1461     5569                    /* INCLUDES:*/
     1462     5570        %INCLUDE XU_MACRO_C;
     1463     8676        %INCLUDE XU_SUBS_C;
     1464     8768                    /* STATIC DATA */
     1465     8769        %PARSE$OUT(STCLASS=BASED,NAME=OUT_BLK);
     1466     8814        %PARSE$OUT(STCLASS=" ");
     1467     8859        %PARSE$OUT(NAME=OPTBLK, STCLASS=AUTO);
     1468     8904        %PARSE$SYM(STCLASS=BASED);
     1469     8942        %P$PCB(NAME=P_PCB,STCLASS=SYMREF);
     1470     9143        %FPT_GDS (FPTN=NODEFPT, STCLASS=SYMREF);
     1471     9159    1   DCL LEXES$(0:29) PTR SYMREF;
     1472     9160    1   DCL 1 TMPLIT STATIC, /* SPACE FOR NAMED LITERAL TEXT */
     1473     9161    1         2 C SBIN,
     1474     9162    1         2 T (0:510) CHAR (1) CALIGNED,
     1475     9163    1         2 TXT REDEF T CHAR (511);
     1476     9164    1   DCL LITCODE SBIN SYMREF;
     1477     9165    1   DCL LIT_PRESENT BIT(1) SYMREF;
     1478     9166        %MACRO REC(NAME=);
     1479     9167        DCL 1 NAME DALIGNED SYMREF,
     1480     9168              2 SIZE UBIN(18),
     1481     9169              2 FIRST$ PTR,
     1482     9170              2 SEG REDEF FIRST$,
     1483     9171                3 * BIT(18),
     1484     9172                3 SEG UBIN(18) UNAL,
     1485     9173              2 F$SBIN REDEF FIRST$ UBIN,
     1486     9174              2 P$ PTR,
     1487     9175              2 NEXT$ PTR;
PL6.E3A0      #002=XUP$BLDNODE File=XUP$PARTRGE.:E05TSI                          WED 07/30/97 09:40 Page:289  
     1488     9176        %MEND;
     1489     9177        %REC(NAME=SYM_);
     1490     9188        %REC(NAME=NODE_);
     1491     9199    1   DCL OUT$ PTR;
     1492     9200    1   DCL FLAG SBIN;
     1493     9201    1   DCL C SBIN;
     1494     9202    1   DCL P$ PTR;
     1495     9203    1   DCL J REDEF P$ SBIN;
     1496     9204    1   DCL I SBIN;
     1497     9205    1   DCL 1 CHC STATIC,
     1498     9206    1         2 U00 BIT(3),
     1499     9207    1         2 U01 BIT(3),
     1500     9208    1         2 U02 BIT(3),
     1501     9209    1         2 U1,
     1502     9210    1           3 H UBIN(6) UNAL,
