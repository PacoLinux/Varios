        1        1        /*M* FOG$GRAN Symbiont file granule builder for MOOS and OUTSYM */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMC,PLM=6,IND=0,IDT=2,SDI=2,CTI=0,ENU=0,AND,DCI=4,CSU=2,ECU=4,THI=0,DTI=0,STI=1
                 7        ,IAD=0,PRB */
        8        8        /**/
        9        9        /*P* NAME:         FOG$GRAN
       10       10             PURPOSE:      To build symbiont file granules for MOOS and
       11       11                           OUTSYM
       12       12             DESCRIPTION:  MOOS and OUTSYM occasionally need to build
       13       13                           symbiont file granules for themselves.  These
       14       14                           granules are built in memory and then shipped
       15       15                           off to device handlers in order to get certain
       16       16                           records printed.  Examples of things printed
       17       17                           in this manner are:
       18       18
       19       19                           o After a space operation, MOOS emits the
       20       20                             correct tabs to the device in one of these
       21       21                             hand-built granules.
       22       22
       23       23                           o OUTSYM builds the device alignment image
       24       24                             using FOG$GRAN
       25       25
       26       26                           o The M$SYMBIO WRITE order (which passes a
       27       27                             record) causes MOOS to build a granule
       28       28                             using FOG$GRAN. */
       29       29        /**/
       30       30        FOG$GRAN: PROC (FO$GPARM) ALTRET;
       31       31        /**/
       32       32
       33       33
       34       34
       35       35        /**/
       36       36        /* INCLUDES */
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:2    
       37       37        /**/
       38       38        %INCLUDE FM$GRAN;
       39      107        %INCLUDE FOO_MAC_C;
       40      855        %INCLUDE B$TABLES_C;
       41      963
       42      964
       43      965
       44      966        /**/
       45      967        /* PARAMETERS */
       46      968        /**/
       47      969        %FO_GPARM (FPTN=FO$GPARM,STCLASS="");
       48      991
       49      992
       50      993
       51      994        /**/
       52      995        /* BASED STRUCTURES */
       53      996        /**/
       54      997        %B$XL_UR;
       55     1001        %B$XL_UPPER_UR;
       56     1005        %FM$GRAN;
       57     1009        %FM$CON;
       58     1013    1   DCL 1 B$PAGE BASED ALIGNED,
       59     1014    1         2 * (0:1023) UBIN WORD UNAL;
       60     1015    1   DCL B$CHARS CHAR(RECL) BASED ALIGNED;
       61     1016    1   DCL B$BYTE CHAR(1) BASED UNAL;
       62     1017
       63     1018
       64     1019
       65     1020        /**/
       66     1021        /* AUTO */
       67     1022        /**/
       68     1023    1   DCL GRAN$ PTR;
       69     1024    1   DCL RECL UBIN WORD;
       70     1025    1   DCL NAVX UBIN WORD;
       71     1026    1   DCL AVAIL UBIN WORD;
       72     1027    1   DCL REQD UBIN WORD;
       73     1028    1   DCL CTL$ PTR;
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:3    
       74     1029    1   DCL REC$ PTR;
       75     1030    1   DCL REPEATS UBIN WORD;
       76     1031    1   DCL WRECL UBIN WORD;
       77     1032    1   DCL I UBIN WORD;
       78     1033    1   DCL IR REDEF I SBIN;
       79     1034    1   DCL URXLATE UBIN WORD;
       80     1035        %EJECT;
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:4    
       81     1036        /**/
       82     1037        /*F* NAME:         FOG$GRAN
       83     1038             PURPOSE:      To initialize a granule page for insertion of
       84     1039                           records.
       85     1040             DESCRIPTION:  FO$GPARM.BUF$ points to a 1024-word buffer,
       86     1041                           which is initialized for use by FOG$ADD.*/
       87     1042        /**/
       88     1043
       89     1044    1         CALL SETUP;
       90     1045    1         RETURN;
       91     1046
       92     1047        /**/
       93     1048        /*F* NAME:         FOG$BLD
       94     1049             PURPOSE:      To enter the first record into a granule.
       95     1050             DESCRIPTION:  The granule is initialized as by FOG$GRAN,
       96     1051                           and then the first record is added, as by
       97     1052                           FOG$ADD. */
       98     1053        /**/
       99     1054    1   FOG$BLD: ENTRY (FO$GPARM) ALTRET;
      100     1055        /**/
      101     1056
      102     1057    1         CALL SETUP;
      103     1058    1         GOTO ADD0;
      104     1059
      105     1060    1   SETUP: PROC;
      106     1061    2         GRAN$=FO$GPARM.BUF$;
      107     1062    2         GRAN$->B$PAGE='0'B;
      108     1063    2         GRAN$->FM$GRAN.NAVX=LENGTHW(FM$GRAN);
      109     1064    2         GRAN$->FM$GRAN.AVAIL=1024-LENGTHW(FM$GRAN);
      110     1065    2         GRAN$->FM$GRAN.KEYCNT = 0;
      111     1066    2         RETURN;
      112     1067    2   END SETUP;
      113     1068
      114     1069        /**/
      115     1070        /*F* NAME:         FOG$BLDUR
      116     1071             PURPOSE:      To add enter the first record of a granule, with
      117     1072                           UR translation.
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:5    
      118     1073             DESCRIPTION:  Like FOG$BLD, but UR-xlates the record. */
      119     1074        /**/
      120     1075    1   FOG$BLDUR: ENTRY (FO$GPARM) ALTRET;
      121     1076        /**/
      122     1077
      123     1078    1         CALL SETUP;
      124     1079    1         GOTO ADD01;
      125     1080
      126     1081        /**/
      127     1082        /*F* NAME:         FOG$BLDURUC
      128     1083             PURPOSE:      To enter the first record of a granule, with
      129     1084                           UR upper case translation.
      130     1085             DESCRIPTION:  Like FOG$BLD, but UR-xlates (upper case only) the
      131     1086                           record. */
      132     1087        /**/
      133     1088    1   FOG$BLDURUC: ENTRY (FO$GPARM) ALTRET;
      134     1089    1         CALL SETUP;
      135     1090    1         GOTO ADD02;
      136     1091
      137     1092        /**/
      138     1093        /**/
      139     1094        /*F* NAME:         FOG$ADD
      140     1095             PURPOSE:      To add a record to a granule
      141     1096             DESCRIPTION:  FO$GPARM.BUF$ points to the granule buffer.
      142     1097                           FO$GPARM.REC$ points to the record to be added,
      143     1098                                         which must be word aligned.
      144     1099                           FO$GPARM.RECL is the character length of the
      145     1100                                         buffer pointed to by REC$
      146     1101                           FO$GPARM.VFC  is the vfc character to use; if
      147     1102                                         zero, the vfc is already in the
      148     1103                                         record, or perhaps not present.
      149     1104                           FO$GPARM.DVBYTE is the DVBYTE to stick in the
      150     1105                                           record control word.
      151     1106                           FO$GPARM.REPEATS is the number of times the record
      152     1107                                            is to be repeated (implemented
      153     1108                                            by repeating the control word).
      154     1109
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:6    
      155     1110                           FO$GPARM is defined in FOO_MAC_C.
      156     1111
      157     1112                           The routine ALTRETs if the granule being built
      158     1113                           has insufficient space to accommodate the
      159     1114                           record. */
      160     1115        /**/
      161     1116    1   FOG$ADD: ENTRY (FO$GPARM) ALTRET;
      162     1117
      163     1118    1         GRAN$=FO$GPARM.BUF$;
      164     1119    1   ADD0: ;
      165     1120    1         URXLATE=0;
      166     1121    1         GOTO ADD03;
      167     1122    1   ADD01:;
      168     1123    1         URXLATE=1;
      169     1124    1         GOTO ADD03;
      170     1125    1   ADD02:;
      171     1126    1         URXLATE=2;
      172     1127    1   ADD03:;
      173     1128    1         RECL=FO$GPARM.RECL;
      174     1129    1         IF FO$GPARM.VFCR ~= '0'B THEN RECL=RECL+1;
      175     1130    1         REPEATS=FO$GPARM.REPEATS;
      176     1131    1         IF REPEATS = 0 THEN REPEATS=1;
      177     1132    1         WRECL=(RECL+3)/4;
      178     1133    1         NAVX=GRAN$->FM$GRAN.NAVX;
      179     1134    1         AVAIL=GRAN$->FM$GRAN.AVAIL;
      180     1135    1         IF WRECL+REPEATS > AVAIL THEN ALTRETURN;
      181     1136    1         CTL$=PINCRW(GRAN$,NAVX+AVAIL-1);
      182     1137    1         AVAIL=AVAIL-WRECL-REPEATS;
      183     1138    1         GRAN$->FM$GRAN.AVAIL=AVAIL;
      184     1139    1         CTL$->FM$CON='0'B;
      185     1140    1         CTL$->FM$CON.FAK=1;
      186     1141    1         CTL$->FM$CON.GACTB=RECL;
      187     1142    1         CTL$->FM$CON.GX=NAVX;
      188     1143    1         CTL$->FM$CON.CTL=FO$GPARM.DVBYTE;
      189     1144    2           DO IR /* I */ = 1 TO REPEATS-1;
      190     1145    2           PINCRW(CTL$,-I)->FM$CON=CTL$->FM$CON;
      191     1146    2           END;
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:7    
      192     1147    1         IF FO$GPARM.VFCR = '0'B THEN
      193     1148    2           DO CASE (URXLATE);
      194     1149                                                     /**/
      195     1150    2             CASE (0);                /* FOG$BLD */
      196     1151    2               PINCRW (GRAN$,NAVX) -> B$CHARS
      197     1152    2               = FO$GPARM.REC$ -> B$CHARS;
      198     1153                                                     /**/
      199     1154    2             CASE (1);                /* FOG$BLDUR */
      200     1155    2               CALL XLATE (PINCRW(GRAN$,NAVX) -> B$CHARS,
      201     1156    2               B$XL_UR,
      202     1157    2               FO$GPARM.REC$ -> B$CHARS);
      203     1158                                                     /**/
      204     1159    2             CASE (2);                /* FOG$BLDURUC */
      205     1160    2               CALL XLATE (PINCRW(GRAN$,NAVX) -> B$CHARS,
      206     1161    2               B$XL_UPPER_UR,
      207     1162    2               FO$GPARM.REC$ -> B$CHARS);
      208     1163    2           END;
      209     1164    1         ELSE
      210     1165    2           DO;
      211     1166    2           REC$=PINCRW(GRAN$,NAVX);
      212     1167    2           REC$->B$BYTE=FO$GPARM.VFC;
      213     1168    2           RECL=RECL-1;
      214     1169    3             DO CASE (URXLATE);
      215     1170                                                     /**/
      216     1171    3               CASE (0);                /* FOG$BLD */
      217     1172    3                 PINCRC (REC$,1) -> B$CHARS
      218     1173    3                 = FO$GPARM.REC$ -> B$CHARS;
      219     1174                                                     /**/
      220     1175    3               CASE (1);                /* FOG$BLDUR */
      221     1176    3                 CALL XLATE (PINCRC (REC$,1) -> B$CHARS,
      222     1177    3                 B$XL_UR,
      223     1178    3                 FO$GPARM.REC$ -> B$CHARS);
      224     1179                                                     /**/
      225     1180    3               CASE (2);                /* FOG$BLDURUC */
      226     1181    3                 CALL XLATE (PINCRC (REC$,1) -> B$CHARS,
      227     1182    3                 B$XL_UPPER_UR,
      228     1183    3                 FO$GPARM.REC$ -> B$CHARS);
PL6.E3A0      #001=FOG$GRAN File=FOG$GRAN.:E05TSI                                TUE 07/29/97 17:08 Page:8    
      229     1184    3             END;
      230     1185    2           END;
      231     1186    1         NAVX=NAVX+WRECL;
      232     1187    1         GRAN$->FM$GRAN.NAVX=NAVX;
      233     1188    1         RETURN;
      234     1189    1   END FOG$GRAN;

