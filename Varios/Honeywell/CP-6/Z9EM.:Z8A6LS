      588       15             %EQU PHASE = 1;
      589       16
      590       17             %INCLUDE Z8A_DEFC;
      591       85             %INCLUDE Z8A_SUPP_MAC;
      592     2485             %INCLUDE Z8A_COMCOM;
      593     3135             %INCLUDE Z8A_DNT;
      594     3356             %INCLUDE Z8A_LTS;
      595     5433             %INCLUDE Z8A_ST;
      596     9092             %INCLUDE Z8A_IO_EQUS;
      597     9215
      598     9216    1        DCL Z8AH$NEW_DNT  ENTRY (3);
      599     9217
      600     9218    1        DCL P$            PTR;
      601     9219    1        DCL DISK$         PTR;
      602     9220    1        DCL FILE$         PTR;
      603     9221
      604     9222
      605     9223    1        CALL Z8AH$NEW_DNT (%DISK, 4, DISK$);
      606     9224
      607     9225             %NEW_LTS_PTR_ARRAY (@POINTER = P$,
      608     9226                                 @SIZE    = %MIN_LTS_PTR_ARRAY_SIZE);
      609     9319
      610     9320    1        P$ -> LTS_PTR_ARRAY.P$ (0) = DISK$;
