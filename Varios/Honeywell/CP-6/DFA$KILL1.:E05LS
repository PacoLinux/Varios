PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:1    
        1        1        /*M* DFA$KILL1 - Remove stored command from FIXCOM & VARCOM */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MOC,DCI=5,CRT=5 */
        8        8        /*F* NAME: DFA$KILL1
        9        9             PURPOSE: To delete the specified stored command from an FPRG
       10       10             DESCRIPTION: The stored command to be deleted is passed
       11       11                          in "S_D.KILL1". The command's size is determined.
       12       12                          All stored commands that follow the one to be deleted
       13       13                          are packed over the command to be deleted.  The first
       14       14                          attachment index (FAI) of each command that followed
       15       15                          the deleted command is updated to reflect the prior
       16       16                          packing. The deleted command's entry in FIXCOM is
       17       17                          cleared.  The pointer to the next available free space
       18       18                          in VARCOM is updated to reflect the space just obtained.
       19       19                          S_D.FIXLAST is set to reflect the new last command.
       20       20                          A KILL BREAKPOINT message is built in S_D.L6_MESSAGE_BUF
       21       21                          and DFQ$WRITE_FD is called to write the message to FD.  */
       22       22        /*D* NAME: DFA$KILL1
       23       23             CALL: CALL DFA$KILL1
       24       24             INTERFACE:
       25       25             ENVIRONMENT:
       26       26             INPUT: S_D.KILL1 - ID OF COMMAND TO BE DELETED.
       27       27             OUTPUT:
       28       28             SCRATCH:
       29       29             DESCRIPTION: THE STORED COMMAND TO BE DELETED IS PASSED
       30       30             IN "S_D.KILL1". THE COMMAND'S SIZE IS DETERMINED.
       31       31             ALL STORED COMMANDS THAT FOLLOW THE ONE TO BE DELETED ARE PACKED
       32       32             OVER THE COMMAND TO BE DELETED. THE FIRST ATTACHMENT INDEX (FAI) OF
       33       33             EACH COMMAND THAT FOLLOWED THE DELETED COMMAND IS UPDATED TO REFLECT
       34       34             THE PRIOR PACKING. THE DELETED COMMAND'S ENTRY IN FIXCOM IS CLEARED.
       35       35             THE POINTER TO THE NEXT AVAILABLE FREE SPACE IN VARCOM IS
       36       36             UPDATED TO REFLECT THE SPACE JUST OBTAINED. S_D.FIXLAST IS
       37       37             SET TO REFLECT THE NEW LAST COMMAND. Builds a KILL BREAKPOINT
PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:2    
       38       38             Message in S_D.L6_MESSAGE_BUF and calls DFQ$WRITE_FD to write
       39       39             the message to FD.  */
       40       40                                                /**/
       41       41        DFA$KILL1: PROC(WRITE, CCMD, KILL);
       42       42        /**/
       43       43    1   DCL WRITE BIT(1) ALIGNED;
       44       44    1   DCL CCMD UBIN;
       45       45    1   DCL KILL BIT(1) ALIGNED;
       46       46        /* Auto */
       47       47    1   DCL KILL$ PTR;                          /* ADDR OF KILLED FIXCOM ENTRY */
       48       48    1   DCL DELFAI UBIN;                        /* KILLED .FAI */
       49       49    1   DCL DELTAL UBIN;                        /* KILLED .TAL */
       50       50    1   DCL ST$ PTR;                            /* TEMP ENTRY IN FIXCOM */
       51       51    1   DCL EN$ PTR;                            /* TEMP ENTRY IN FIXCOM */
       52       52    1   DCL CHARVAR CHAR(CVSIZE) BASED ALIGNED; /* FOR GARBAGE COLLECTION MOVE */
       53       53    1   DCL CVSIZE UBIN;                        /* FOR GARBAGE COLLECTION MOVE */
       54       54    1   DCL CHAR18 CHAR(18) BASED;
       55       55    1   DCL BB$ PTR;
       56       56    1   DCL SIZE UBIN;
       57       57    1   DCL I UBIN;
       58       58    1   DCL TFPRG$ PTR;
       59       59    1   DCL RES CHAR(8) CALIGNED;
       60       60    1   DCL RES_SIZE UBIN;
       61       61    1   DCL TEMP$ PTR;
       62       62    1   DCL TEMP1 REDEF TEMP$ UBIN(36) ALIGNED;
       63       63    1   DCL 1 TEMP1B REDEF TEMP$ ALIGNED,
       64       64    1         2 * UBIN(4) UNAL,
       65       65    1         2 B1 UBIN(8) UNAL,
       66       66    1         2 B2 UBIN(8) UNAL,
       67       67    1         2 B3 UBIN(8) UNAL,
       68       68    1         2 B4 UBIN(8) UNAL;
       69       69    1   DCL TEMP2 UBIN(36) ALIGNED;
       70       70    1   DCL 1 TEMP2B REDEF TEMP2 ALIGNED,
       71       71    1         2 * BIT(1) UNAL,
       72       72    1         2 B1 UBIN(8) UNAL,
       73       73    1         2 * BIT(1) UNAL,
       74       74    1         2 B2 UBIN(8) UNAL,
PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:3    
       75       75    1         2 * BIT(1) UNAL,
       76       76    1         2 B3 UBIN(8) UNAL,
       77       77    1         2 * BIT(1) UNAL,
       78       78    1         2 B4 UBIN(8) UNAL;
       79       79        /* Includes and Based structures */
       80       80        %INCLUDE DD$BASED;
       81     1383        %FIXCOM (NAME=FXCM);
       82     1399        %FPRG$CONTEXT;
       83     1462        %SHOW$CMD(NAME=K$ILL);
       84     1484        %INCLUDE DD_SUBS_C;
       85     1755        %INCLUDE DD_PSUBS_C;
       86     2230        %INCLUDE XUH_MACRO_C;
       87     2314        %INCLUDE DD_SD_M;
       88     3650        %S_D;
       89     4968        %S_D_PTRS;
       90     5005        %INCLUDE GD$MSG_M;
       91     5169        %GD$MSG(FPTN=GD$MSG,STCLASS="BASED(S_D_L6_MESSAGE$)",L6=YES,L6PTR="UBIN(32) UNAL");
       92     5325        %GD$MSG(FPTN=GD$MSG_HOST,STCLASS="BASED(S_D_HOST_MESSAGE$)",L6=NO);
       93     5481        %INCLUDE GD_EQUS_E;
       94     5642        %INCLUDE DD_ERR_C;
       95     6039        %INCLUDE DD$DITEM;
       12+    6142             %LIST;
       13+    6143    1        DCL SDD$ PTR;
       14+    6144    1        DCL ZITEM$ PTR;
       15+    6145    1        DCL SDD_L6$ PTR;
       16+    6146    1        DCL ZITEM_L6$ PTR;
       96     6720        /* Entry DCLs */
       97     6721    1   DCL DDE$ERROR ENTRY;
       98     6722    1   DCL DDQ$WRITE ENTRY(2);
       99     6723    1   DCL DDR$BINTXT ENTRY(4);
      100     6724    1   DCL DFQ$READ_FD ENTRY(2) ALTRET;
      101     6725    1   DCL DFQ$WRITE_FD ENTRY(1) ALTRET;
      102     6726    1   DCL DUE$ERRMSG   ENTRY;
      103     6727    1   DCL DUE$GERRMSG   ENTRY;
      104     6728        /**/
      105     6729                                                /* Initialization */
      106     6730    1        KILL$ = ADDR(S_D.FIXCOM(S_D.KILL1));
PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:4    
      107     6731    1        DELFAI = KILL$ -> FXCM.FAI;
      108     6732    1        DELTAL = KILL$ -> FXCM.TAL;
      109     6733    1        ST$ = ADDR(S_D.FIXCOM(1));
      110     6734    1        EN$ = ADDR(S_D.FIXCOM(S_D.FIXLAST));
      111     6735                  /* Find the correct FPRG context and zero out the breakpoint entry */
      112     6736    1        TFPRG$ = S_D.FPRG_CONTEXT$;
      113     6737    1        IF TFPRG$ = ADDR(NIL)
      114     6738    1        THEN GOTO FPRG_ERR;
      115     6739    1        RES = S_D.FIXCOM.FPRG_RES(S_D.KILL1);
      116     6740    1        RES_SIZE = S_D.FIXCOM.FPRG_RES_SIZE(S_D.KILL1);
      117     6741    2        DO WHILE (SUBSTR(RES, 0, RES_SIZE) ~= SUBSTR(TFPRG$->FPRG$CONTEXT.RES,
      118     6742    2          0, TFPRG$->FPRG$CONTEXT.RES_SIZE));
      119     6743    2             TFPRG$ = TFPRG$ -> FPRG$CONTEXT.NEXT$;
      120     6744    2             IF TFPRG$ = ADDR(NIL)
      121     6745    2             THEN GOTO FPRG_ERR;
      122     6746    2           END;
      123     6747    1        IF TFPRG$ -> FPRG$CONTEXT.STATE ~= %STOPPED AND
      124     6748    1          TFPRG$ -> FPRG$CONTEXT.STATE ~= %ATEXIT
      125     6749    2        THEN DO;
      126     6750    2             S_D.ERROR = E@FPRGNOTSTOPPED; /* E@FPRGNOTSTOPPED */
      127     6751    2             CALL DDE$ERROR;
      128     6752    2             S_D.OUTPUT = 'Breakpoint ';
      129     6753    2             BB$ = PINCRC(ADDR(S_D.OUTPUT), SIZEC('Breakpoint '));
      130     6754    2             CALL DDR$BINTXT(BB$, S_D.KILL1, %UINT_FMT, SIZE);
      131     6755    2             BB$ = PINCRC(BB$, SIZE);
      132     6756    2             BB$ -> CHAR18 = ' can not be killed';
      133     6757    2             CALL DDQ$WRITE(ADDR(S_D.OUTPUT), SIZEC('Breakpoint ')+18+SIZE);
      134     6758    2             GOTO RETN;
      135     6759    2           END;
      136     6760    1        IF S_D.FIXCOM.INACTIVE(S_D.KILL1)
      137     6761    1        THEN GOTO ENDREST;
      138     6762    1        I = 1;
      139     6763    2        DO WHILE (S_D.KILL1 ~= TFPRG$ -> FPRG$CONTEXT.FIXCOM_INDEX(I));
      140     6764    2             I = I + 1;
      141     6765    2             IF I > 20
      142     6766    2             THEN GOTO NO_MATCH;
      143     6767    2           END;
PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:5    
      144     6768    1        TFPRG$ -> FPRG$CONTEXT.FIXCOM_INDEX(I) = 0;
      145     6769    1        IF CCMD = C_OE                     /* C_OE */
      146     6770    1        THEN TFPRG$ -> FPRG$CONTEXT.ONEXIT = 0;
      147     6771    1        IF CCMD = C_ONCALLS                /* C_ONCALLS */
      148     6772    1        THEN TFPRG$ -> FPRG$CONTEXT.ONCALLS = 0;
      149     6773    1        IF WRITE
      150     6774    2        THEN DO;
      151     6775                                           /* Write a KILL BREAKPOINT message to FD */
      152     6776    2             GD$MSG = '0'B;
      153     6777    2             GD$MSG.FNC = %GD#RQSKILLATN;
      154     6778    2             GD$MSG.KILLRQS.BRK# = I;
      155     6779    2             CALL DFQ$WRITE_FD (TFPRG$) ALTRET(ERR);
      156     6780    2             CALL DFQ$READ_FD (TFPRG$, '0'B) ALTRET(ERR);
      157     6781    2             IF GD$MSG.FNC = %GD#RSPNAK
      158     6782    3             THEN DO;
      159     6783    3                  IF TFPRG$ -> FPRG$CONTEXT.XDELTA
      160     6784    4                  THEN DO;
      161     6785    4                       TEMP1 = BITBIN(GD$MSG.ERROR_D.ERRCODE);
      162     6786    4                       TEMP2B.B1 = TEMP1B.B1;
      163     6787    4                       TEMP2B.B2 = TEMP1B.B2;
      164     6788    4                       TEMP2B.B3 = TEMP1B.B3;
      165     6789    4                       TEMP2B.B4 = TEMP1B.B4;
      166     6790    4                       S_D.JDE = TEMP2;
      167     6791    4                     END;
      168     6792    3                  ELSE S_D.JDE = BITBIN(GD$MSG_HOST.ERROR_D.ERRCODE);
      169     6793    3                  CALL DUE$GERRMSG;
      170     6794    3                  GOTO RETN;
      171     6795    3                END;
      172     6796    2             IF GD$MSG.FNC ~= %GD#RSPACK
      173     6797    3             THEN DO;
      174     6798    3                  IF TFPRG$ -> FPRG$CONTEXT.XDELTA AND
      175     6799    3                    GD$MSG.FNC = %GD#RSPBADMSG
      176     6800    3                  THEN S_D.ERROR = E@BADMSG; /* E@BADMSG */
      177     6801    3                  ELSE S_D.ERROR = E@DELBUG; /* E@DELBUG */
      178     6802    3                  CALL DDE$ERROR;
      179     6803    3                END;
      180     6804    2           END;
PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:6    
      181     6805                                                /* MOVE THE WHOLE MAGILLA UP */
      182     6806    1   ENDREST:
      183     6807    1        IF KILL
      184     6808    2        THEN DO;
      185     6809    3             DO WHILE ( ST$ <= EN$ );
      186     6810    3                  IF ( ST$ -> FXCM.CMD ~= 0 )
      187     6811    4                  THEN DO;
      188     6812    4                       S_D.FIXLAST = POFFW( ST$, ADDR( S_D.FIXCOM ) ) / SIZEW( FXCM );
      189     6813    4                       IF ( ST$ -> FXCM.FAI > DELFAI )
      190     6814    4                       THEN ST$ -> FXCM.FAI = ST$ -> FXCM.FAI - DELTAL;
      191     6815    4                     END;
      192     6816    3                  ST$ = PINCRW( ST$, SIZEW( FXCM ) );
      193     6817    3                END;
      194     6818    2             CVSIZE = POFFC( S_D.VARNEXT$, PINCRW( S_D.VARCOM$, DELFAI + DELTAL ) );
      195     6819    2             PINCRW( S_D.VARCOM$, DELFAI ) -> CHARVAR = PINCRW( S_D.VARCOM$, DELFAI +
              6819                      DELTAL ) -> CHARVAR;
      196     6820                                                /* FINAL CLEANUP */
      197     6821    2             KILL$ -> FXCM = '0'B;
      198     6822    2             S_D.VARNEXT$ = PINCRW( S_D.VARNEXT$, -DELTAL );
      199     6823                                                /* Check for suicide kill */
      200     6824    2             IF S_D.IXFIX = S_D.KILL1
      201     6825    3             THEN DO;
      202     6826    3                  S_D.FAULTCMD$ = ADDR(NIL);
      203     6827    3                  S_D.ENTRYCMD$ = ADDR(NIL);
      204     6828    3                END;
      205     6829    2           END;
      206     6830                                                /**/
      207     6831    1   RETN: ;
      208     6832    1        RETURN;
      209     6833    1   FPRG_ERR: ;
      210     6834    1        S_D.ERROR = E@DELBUG;              /* E@DELBUG */
      211     6835    1        CALL DDE$ERROR;
      212     6836    1        GOTO RETN;
      213     6837    1   NO_MATCH: ;
      214     6838    1        IF S_D.DO$ ~= ADDR(NIL)
      215     6839    1          AND S_D.DO$ -> K$ILL.TYPE = S@ALL /* KILL ALL */
      216     6840    1        THEN GOTO RETN;
PL6.E3A0      #001=DFA$KILL1 File=DFA$KILL1.:E05TSI                              TUE 07/29/97 14:34 Page:7    
      217     6841    1        S_D.ERROR = E@DELBUG;              /* E@DELBUG */
      218     6842    1        CALL DDE$ERROR;
      219     6843    1        GOTO RETN;
      220     6844    1   ERR: ;
      221     6845    1        CALL DUE$ERRMSG;
      222     6846    1        GOTO RETN;
      223     6847    1   END DFA$KILL1;
      224     6848        %EOD;

