       78     6252    1       FPT_CORRES.V.DCB2# = DCBNUM(M$SI);
       79     6253    1       CALL M$CORRES( FPT_CORRES ) ALTRET(SAME_ONE );
       80     6254    1       VY_ECHO = @TRUE_B;
       81     6255    2       DO WHILE( '0'B );
       82     6256    2   SAME_ONE:
       83     6257    2           VY_ECHO = @FALSE_B;
       84     6258    2           END;
PL6.E3A0      #001=VYI$INIT File=VYI$INIT.:E05TSI                                WED 07/30/97 08:54 Page:5    
       85     6259    2       IF B$JIT$ -> B$JIT.MODE = %M_INT# THEN DO;
       86     6260    2           VY_ONLINE = 1; /* Online */
       87     6261    2           VY_ERR_CODE.SEV = 0;
       88     6262    2           END;
       89     6263    2       ELSE DO;
