    8      /* This file contains string substitutions used in Memory Manage-  */
    9      /* ment in adapting itself to the physical configuration and       */
   10      /* installation-set requirements for memory utilization.           */
   11
   12      %EQU   RSVPGS = 15;                     /* # OF PAGES RESERVED FOR STEALING           */
   13      %EQU   STLMPGS = 30;                    /* MAX # OF STOLEN PAGES IN SYSTEM            */
   14      %EQU   RDS1PGS = 4;                     /* PAGES RESERVED FOR DS1 = CFU'S             */
   15      %EQU   RDS2PGS = 20;                    /* PAGES RESERVED FOR DS2 = AUTOSAVE  */
   16      %EQU   RDS3PGS = 1;                     /* PAGES RESERVED FOR DS3 = ENQ/DEQ           */
   17      %EQU   RDS4PGS = 0;                     /* PAGES RESERVED FOR DS4                     */
   18      %EQU   RDS5PGS = 0;                     /* PAGES RESERVED FOR DS5                     */
   19      %EQU   RDS6PGS = 0;                     /* PAGES RESERVED FOR DS6                     */
   20      %EQU   RDS7PGS = 1;                /* RESERVED FOR OFF-LINE VOLINIT*/
   21      %EQU   RDS8PGS = 1;                /* PAGES RESERVED FOR LDCTS        */
   22      %EQU   HELDPGS = 50;               /* TOTAL PAGES HELD BACK FOR I/O CACHE        */
