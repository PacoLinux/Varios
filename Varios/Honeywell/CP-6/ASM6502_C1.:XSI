C
C     COMMON OP CONTAINS DATA AND CONSTANTS USED IN OPCODE PROCESSING
C
      COMMON /OP/ OPTBL(2,200),OPTYPE(200),OPBASE(200),OPTIME(200),
     1  OPCODE(2),OPLIM,OPIDX
C
C     COMMON SYMTAB CONTAINS SYMBOL TABLE AND LABEL DATA
C
      COMMON /SYMTAB/ SYMBOL(5,1000),LABEL(2),HISYM
C
C     COMMON ARG HOLDS ARGUMENT DATA AND CONSTANTS
C
      COMMON /ARG/ ARGMAT(12,13),ARGTIM(12,13),ARGBGN,ARGEND,ARGCNT,
     1  ARGTYP
C
C     COMMON IO HOLDS I/O BUFFERS AND UNIT ASSIGNMENTS
C
      COMMON /IO/ INPBUF(256),LOBUF(33),XRBUF(5),IUSTK(10),
     1  LNSTK(10),IUNIT,SIUNIT,INUNIT,MCUNIT,XRUNIT,OUUNIT,LOUNIT,
     2  TMUNIT,CURPOS,XRREC,MCREC,ARS
C
C     COMMON CONS HOLDS CONSTANT VALUES
C
      COMMON /CONS/ KMLT,KDIV,KMOD,KADD,KSUB,KASL,KASR,KLSS,KLEQ,KEQL,
     1  KNEQ,KGEQ,KGTR,KNOT,KAND,KIOR,KEOR,KEOC,KPMI,KLPR,KRPR,ERRA,
     2  ERRD,ERRL,ERRM,ERRO,ERRP,ERRR,ERRS,ERRT,ERRU,ERRX,OPTLS,OPTXR,
     3  OPTOU,OPTSC,OPTREL,OPTOC,OPTAO,FLAGLS,LOLINE,LOPAGE,LOLIM,
     4  DIDMAC,LOWID,BLANK,HOLDOP
C
C     COMMON MISC HOLDS MISCELLANEOUS STUFF
C
      REAL CPU1,CPU2
      COMMON /MISC/ PASS,NBITS,LOCCTR,TEXT(256),FLAGIF,GO2LBL(2),
     1  OBJCNT,OBJECT(256),ERRORS,LNCNT,OPTION,ERRCNT,SRACCT(2,8),
     2  TIME(2),DATE(2),CPU1,CPU2,TITLE(120),CLOCK,FLAGMC,PARITY,
     3  REFCNT,RELOC,RELBAS,HOLDCL,HOLDSR(2,8),NBITS2,NBITS3,MBITS,
     4  MBITS2,MBITS3
C
C     COMMON MACRO HOLDS MACRO INVOCATION STUFF
C
      COMMON /MACRO/ MCHEAD(256,10),MCDUM(2,32,10),MCPTR(32,10),
     1  MCLEN(32,10)
C
C     COMMON OBJ HOLDS OBJECT UNIT STUFF
C
      COMMON /OBJ/ OUBUF0(39),OUBUF3(39),OUBUF4(39),OUBUF5(39),
     1  A0,B0,C0,A3,B3,C3,A4,B4,C4,A5,B5,C5
C
C     COMMON BREAK CONTROLS BREAK KEY STUFF
C
      COMMON /BREAK/ BREAK
