        1        1        /*M* DUA$HELP - HELP PROCESSOR FOR USER DELTA */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MOC,DCI=5,CRT=5 */
        8        8        %SET LISTSUB='1'B;
        9        9        /*P* NAME: DUA$HELP
       10       10             DESCRIPTION:  This routine processes three commands, HELP,
       11       11                       QMARK1 (?), and QMARK2 (??).
       12       12
       13       13                  HELP.  X$HELP is used to print the requested HELP info.
       14       14                  S_D.HELPING mode is then entered.
       15       15
       16       16                  ?.  One is added to the severity field of S_D.LAST_ERR.
       17       17                  M$ERRMSG is called to print the message, unless S_D.HELPING
       18       18                  mode was set. In that case X$HELP is called with the MORE#
       19       19                  bit set.
       20       20
       21       21                  ??.  All remaining error messages are printed using M$ERRMSG,
       22       22                  unless S_D.HELPING was set. In that case X$HELP is called
       23       23                  with the ALL# bit set.
       24       24
       25       25                  For proper results, DUA$HELP depends on DUE$ERRMSG and DDE$ERROR
       26       26                  to set S_D.LAST_ERROR to the last error code used.
       27       27
       28       28                  DUA$HELP does not ALTRET.  If S_D.ERROR was set,
       29       29                  DDE$ERROR is called before returning. */
       30       30
       31       31        DUA$HELP: PROC;
       32       32
       33       33        %INCLUDE CP_6;
       34      114        %INCLUDE DD$BASED;
       35     1417        %INCLUDE XUH_MACRO_C;
       36     1501        %INCLUDE DD_SD_M;
       37     2837        %S_D;
PL6.E3A0      #001=DUA$HELP File=DUA$HELP.:E05TSI                                TUE 07/29/97 14:47 Page:2    
       38     4155        /* Auto */
       39     4156    1   DCL GOT_ONE BIT(1);
       40     4157    1   DCL SEVHI UBIN;
       41     4158        %FPT_ERRMSG (FPTN=FPT$ERRMSG,
       42     4159                     STCLASS=);
       43     4186        /* Based */
       44     4187        %F$DCB;
       45     4244        %HELP$CMD;
       46     4255        /* Constant */
       47     4256        %FPT_ERRMSG (FPTN=FPT_ERRMSG,
       48     4257                     STCLASS=CONSTANT,
       49     4258                     SUBMESS=NO,
       50     4259                     INCLCODE=NO);
       51     4286        /* Symref */
       52     4287    1   DCL D_M$DO# UBIN SYMREF;
       53     4288        /* Entry */
       54     4289    1   DCL DDE$ERROR ENTRY(0);
       55     4290    1   DCL DUE$HERRMSG ENTRY(0);
       56     4291    1   DCL X$HELP ENTRY(1) ALTRET;
       57     4292        /* Misc */
       58     4293        %INCLUDE DD_ERR_C;
       59     4690        %INCLUDE DD_SUBS_C;
       60     4961        %INCLUDE NK_VFC_C;
       61     5035        %INCLUDE XU_PERR_C;
       62     5226
       63     5227    1        IF S_D.DO$ -> HELP$CMD.CMD = 12 /* C_HELP */
       64     5228    2        THEN DO;
       65     5229    2             S_D.XUH_PARAM.HELP$ = S_D.DO$ -> HELP$CMD.ASYM$;
       66     5230    2             S_D.HELPING = '1'B;
       67     5231    2             S_D.XUH_PARAM.TOPICS# = '0'B;
       68     5232    2   HELP_MODE: ;
       69     5233    2             IF NOT DCBADDR( D_M$DO# ) -> F$DCB.DDEV.UCOUT# AND
       70     5234    2               NOT S_D.BATCH AND S_D.COPY
       71     5235    2             THEN S_D.XUH_PARAM.OUTDCB2# = DCBNUM( M$UC );
       72     5236    2             ELSE S_D.XUH_PARAM.OUTDCB2# = 0;
       73     5237    2             IF S_D.USVFC OR S_D.LWEXTEND
       74     5238    3             THEN DO;
PL6.E3A0      #001=DUA$HELP File=DUA$HELP.:E05TSI                                TUE 07/29/97 14:47 Page:3    
       75     5239    3                  S_D.USVFC = '0'B;
       76     5240    3                  S_D.LWEXTEND = '0'B;
       77     5241    3                  S_D.XUH_PARAM.VFC# = %VFC_TRANSITION1;
       78     5242    3                END;
       79     5243    2             ELSE S_D.XUH_PARAM.VFC# = %VFC_S1A;
       80     5244    2             CALL X$HELP( S_D.XUH_PARAM ) ALTRET( XHER );
       81     5245    2             RETURN;
       82     5246    2           END;
       83     5247    1        ELSE IF S_D.HELPING
       84     5248    2             THEN DO;
       85     5249    2                  IF S_D.XUH_PARAM.TOPICS# = '0'B
       86     5250    2                  THEN IF S_D.DO$ -> HELP$CMD.CMD = 80 /* C_QMARK1 */
       87     5251    2                       THEN S_D.XUH_PARAM.MORE# = '1'B;
       88     5252    2                       ELSE S_D.XUH_PARAM.ALL# = '1'B;
       89     5253    2                  ELSE S_D.HELPING = '0'B;
       90     5254    2                  GOTO HELP_MODE;
       91     5255    2                END;
       92     5256        /* Neither HELP nor S_D.HELPING, use M$ERRMSG for ?, ?? */
       93     5257    1        IF S_D.LAST_ERROR = '0'B
       94     5258    1        THEN GOTO NO_HELP;
       95     5259    1        IF S_D.LAST_ERROR.SEV = 7
       96     5260    1        THEN GOTO NO_MORE_HELP;
       97     5261    1        FPT$ERRMSG = FPT_ERRMSG;
       98     5262    1        FPT$ERRMSG.V_ = VECTOR( FPT$ERRMSG.V );
       99     5263    1        FPT$ERRMSG.CODE_ = VECTOR( S_D.LAST_ERROR );
      100     5264    1        FPT$ERRMSG.BUF_ = VECTOR( S_D.OUTPUT );
      101     5265    1        FPT$ERRMSG.V.OUTDCB1# = D_M$DO#;
      102     5266    1        IF NOT DCBADDR( D_M$DO# ) -> F$DCB.DDEV.UCOUT# AND
      103     5267    1          NOT S_D.BATCH
      104     5268    1        THEN FPT$ERRMSG.V.OUTDCB2# = DCBNUM( M$UC );
      105     5269    1        IF S_D.USVFC OR S_D.LWEXTEND
      106     5270    2        THEN DO;
      107     5271    2             S_D.USVFC = '0'B;
      108     5272    2             S_D.LWEXTEND = '0'B;
      109     5273    2             FPT$ERRMSG.V.VFC# = %VFC_TRANSITION1;
      110     5274    2           END;
      111     5275    1        ELSE FPT$ERRMSG.V.VFC# = %VFC_S1A;
PL6.E3A0      #001=DUA$HELP File=DUA$HELP.:E05TSI                                TUE 07/29/97 14:47 Page:4    
      112     5276    1        IF S_D.DO$ -> HELP$CMD.CMD = 80 /* C_QMARK1 */
      113     5277    1        THEN SEVHI = S_D.LAST_ERROR.SEV + 1;
      114     5278    1        ELSE SEVHI = 7;
      115     5279    1        GOT_ONE = '0'B;
      116     5280    2        DO UNTIL ( S_D.LAST_ERROR.SEV = SEVHI );
      117     5281    2             S_D.LAST_ERROR.SEV = S_D.LAST_ERROR.SEV + 1;
      118     5282    2             CALL M$ERRMSG( FPT$ERRMSG ) ALTRET( ERER );
      119     5283    2             GOT_ONE = '1'B;
      120     5284    2           END;
      121     5285    1   ERER: ;
      122     5286    1        IF NOT GOT_ONE
      123     5287    1        THEN GOTO NO_MORE_HELP;
      124     5288
      125     5289    1        RETURN;
      126     5290
      127     5291    1   XHER: ;
      128     5292    1        IF S_D.XUH_PARAM.ERR.MON
      129     5293    1        THEN
      130     5294    2   XUH_ERR: DO;
      131     5295    2             S_D.HELPING = '0'B;
      132     5296    2             S_D.JITERR = S_D.XUH_PARAM.ERR;
      133     5297    2             S_D.JDCBERR = S_D.XUH_PARAM.ERR_DCB#;
      134     5298    2             CALL DUE$HERRMSG;
      135     5299    2             RETURN;
      136     5300    2           END XUH_ERR;
      137     5301    1        IF S_D.XUH_PARAM.ERR.ERR# = %E$BDTOPIC
      138     5302    2        THEN DO;
      139     5303    2             S_D.XUH_PARAM.TOPICS# = '1'B;
      140     5304    2             S_D.JITERR = S_D.XUH_PARAM.ERR;
      141     5305    2             S_D.JDCBERR = 0;
      142     5306    2             CALL DUE$HERRMSG;
      143     5307    2             RETURN;
      144     5308    2           END;
      145     5309    1        IF S_D.XUH_PARAM.ERR.ERR# = %E$SYNERR
      146     5310    2        THEN DO;
      147     5311    2             S_D.HELPING = '0'B;
      148     5312    2             S_D.ERROR = 11;          /* E@UERR0 */
PL6.E3A0      #001=DUA$HELP File=DUA$HELP.:E05TSI                                TUE 07/29/97 14:47 Page:5    
      149     5313    2             S_D.ERROR_HICHAR = S_D.XUH_PARAM.CHARPOS + LENGTHC( 'HELP' );
      150     5314        /*N* ADD S_D.ACMDS_OFF */
      151     5315    2             CALL DDE$ERROR;
      152     5316    2             RETURN;
      153     5317    2           END;
      154     5318    1        IF S_D.XUH_PARAM.ERR.ERR# = %E$NULLKEYWORD
      155     5319    1        THEN
      156     5320    2   NO_MORE_HELP: DO;
      157     5321    2             S_D.HELPING = '0'B;
      158     5322    2             S_D.ERROR = 175;   /* E@NO_MORE_HELP */
      159     5323    2             CALL DDE$ERROR;
      160     5324    2             RETURN;
      161     5325    2           END NO_MORE_HELP;
      162     5326    1        ELSE GOTO XUH_ERR;
      163     5327    1   NO_HELP: ;
      164     5328    1        S_D.ERROR = 174;             /* E@NO_HELP */
      165     5329    1        CALL DDE$ERROR;
      166     5330
      167     5331    1        RETURN;
      168     5332
      169     5333    1   END DUA$HELP;

