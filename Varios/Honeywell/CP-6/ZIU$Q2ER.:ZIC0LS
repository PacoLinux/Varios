      337     2830    1       IF UWA.AC_REC.SWS.ENCRYP THEN
      338     2831    1           ER.PRINT23.P_ENCRP='ENCRYPTED';
      339     2832    1       ELSE
      340     2833    1           ER.PRINT23.P_ENCRP='NOT ENCRYPTED';
      341     2834    1       IF UWA.AC_REC.SWS.TEMPLAT THEN
