      358     2851    2           IF BREAK_FLAG>0 THEN RETURN;
      359     2852    2           V2=P_CNT;
      360     2853    2           IF V2<0 THEN
      361     2854    2               P_CNT=-1*P_CNT; /* RESET DONT PRINT FLAG */
      362     2855    2           BUFLNSF=P_CNT-1;
