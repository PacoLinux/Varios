      374     2867    3               CALL BINCHAR(ER.INDX4.P_LENGTH,VLP_ALTKEYS.K.KEYL#(V1));
      375     2868    3               CALL ZERO_SUP(ER.INDX4.P_LENGTH,LENGTHC(ER.INDX4.P_LENGTH),'0'B);
      376     2869    3               IF VLP_ALTKEYS.K.UNIQUE#(V1) THEN
      377     2870    3                   ER.INDX4.P_DUP='      NO';
      378     2871    3               ELSE
      379     2872    3                   ER.INDX4.P_DUP='      OK';
      380     2873    3               ER.INDX4.P_NAME=RECORD_LIST.NAME(V1+1);
      381     2874    3               V1=V1+1;
      382     2875    3               CALL BINCHAR(ER.INDX4.P_TYP2.N,V1);
      383     2876    3               CALL ZERO_SUP(ER.INDX4.P_TYP2.N,LENGTHC(ER.INDX4.P_TYP2.N),'0'B);
PL6.E3A0      #001=EXCTN_REPRT_SETUP File=ZIU$Q2ER.:ZIC0TSI                      FRI 09/05/97 13:21 Page:12   
      384     2877    3               ER.INDX4.P_TYP2.ALT='  ALT';
      385     2878    3               END;
      386     2879    2           END;
