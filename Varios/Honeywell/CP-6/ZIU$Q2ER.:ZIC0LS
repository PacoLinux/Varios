      350     2843    1       RETURN;
      351     2844        /****/
      352     2845    1   EXECUTION_OUTPUT: ENTRY; /* DO CALLS TO SYS_FUNC_WRIT */
      353     2846        /****/
      354     2847    1       P$=ADDR(ER);
      355     2848    1       DCBSF = DCBNUM(M$LO);
      356     2849    1       V1=0; /* KEEP TRACK # OF ALTKEYS PRINTED WHEN INDEX AREA */
