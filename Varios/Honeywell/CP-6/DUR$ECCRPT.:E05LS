        1        1        /*M* DUR$ECCRPT - WRITE FAULT MESSAGE BASED ON ECC-ECCS CODES IN TCB */
        2        2        /*T***********************************************************/
        3        3        /*T*                                                         */
        4        4        /*T* Copyright (c) Bull HN Information Systems Inc., 1997    */
        5        5        /*T*                                                         */
        6        6        /*T***********************************************************/
        7        7        /*X* DMR,PLM=5,IND=5,CSU=3,EXM=0,ECU=3,MOC,DCI=5,CRT=5 */
        8        8        /*P* NAME: DUR$ECCRPT.
        9        9             DESCRIPTION: WILL PRINT A MESSAGE OF THE FORM:
       10       10               [IN DELTA:] <FAULT_NAME> @ IC/ [OPCODE]
       11       11
       12       12                       */
       13       13        /*D* NAME: DUR$ECCRPT.
       14       14             INPUT: WHOS_FAULT.
       15       15
       16       16                  0 = User Land fault
       17       17                  1 = Fault in DELTA - Prefix with '** IN DELTA:'
       18       18                  2 = Fault on derailed instruction - use S_D>.SAVEIC
       19       19                      instead of ic from TCB, since TCB may point to
       20       20                      the special location.
       21       21                  3 = XEQ fault - Don't use any ic, and prefix with
       22       22                      'DURING XEQ:'.
       23       23                  4 = Entry to DDA$DELTA - Report from TCB, used for
       24       24                      debugging, prefix with '** FOR USER'.
       25       25                  5 = Entry to DUX$XCON1 - Report from TCB, used for
       26       26                      debugging, prefix with '** DELTA XCON'.
       27       27                  6 = ALTRET from the M$DRTN in DDA$DELTA - Report from the
       28       28                      TCB, used for debugging, prefix with '** DRTN ALT'.
       29       29                                     */
       30       30        DUR$ECCRPT: PROC (WHOS_FAULT);
       31       31    1   DCL REM UBIN;
       32       32    1   DCL BB$ PTR;
       33       33    1   DCL NCARS SBIN;
       34       34    1   DCL IC UBIN;
       35       35    1   DCL WHOS_FAULT UBIN;
       36       36    1   DCL ADD_INST BIT(1);
       37       37    1   DCL JITERR BIT(1);
PL6.E3A0      #001=DUR$ECCRPT File=DUR$ECCRPT.:E05TSI                            TUE 07/29/97 14:58 Page:2    
       38       38    1   DCL D_XDELTA BIT(1) SYMREF;
