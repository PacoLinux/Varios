      303     5276                                              so I stuck this stuff down here.  HEP */
      304     5277    1   LIMIT_PROC: PROC ALTRET;
      305     5278                       /* We were building the message for limit exceeded, remember ? */
      306     5279    2        CALL DDR$MOVE('Limit exceeded ', 15, BB$, REM) ALTRET(ALT);
      307     5280    2        IF B$XCON.LIMIT.PO THEN CALL DDR$MOVE('(PO)', 4, BB$, REM) ALTRET(ALT);
      308     5281    2        ELSE IF B$XCON.LIMIT.PLO THEN CALL DDR$MOVE('(PLO)', 5, BB$, REM) ALTRET(ALT);
      309     5282    2             ELSE IF B$XCON.LIMIT.LO THEN CALL DDR$MOVE('(LO)', 4, BB$, REM) ALTRET(ALT
              5282                      );
      310     5283    2                  ELSE IF B$XCON.LIMIT.DGO THEN CALL DDR$MOVE('(DO)', 4, BB$, REM)
              5283                           ALTRET(ALT);
      311     5284    2                       ELSE IF B$XCON.LIMIT.PDISK THEN CALL DDR$MOVE('(PDIS)', 6, BB$,
              5284                                REM) ALTRET(ALT);
      312     5285    2                            ELSE IF B$XCON.LIMIT.TDISK THEN CALL DDR$MOVE('(TDIS)', 6,
              5285                                     BB$, REM) ALTRET(ALT);
      313     5286    2                                 ELSE IF B$XCON.LIMIT.TAPE THEN CALL DDR$MOVE('(TAPE)',
              5286                                          6, BB$, REM) ALTRET(ALT);
      314     5287    2                                      ELSE IF B$XCON.LIMIT.STACK THEN CALL DDR$MOVE(
              5287                                               '(STACK)', 7, BB$, REM) ALTRET(ALT);
      315     5288    2                                           ELSE IF B$XCON.LIMIT.TIME THEN CALL DDR$MOVE
              5288                                                    ('(TIME)', 6, BB$, REM) ALTRET(ALT);
      316     5289    2                                                ELSE CALL DDR$MOVE('(?)', 3, BB$, REM)
              5289                                                         ALTRET(ALT);
      317     5290    2        RETURN;
      318     5291    2   ALT: ALTRETURN;                         /* TOO_BIG */
      319     5292    2   END LIMIT_PROC;
      320     5293    1   END DUR$ECCRPT;

