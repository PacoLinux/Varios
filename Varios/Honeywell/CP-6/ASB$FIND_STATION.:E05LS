       42    23465        %ASB$READ_HLP ;
       43    23468        %ASA$ERROR ;
       44    23471        %EJECT;
PL6.E3A0      #001=ASB$GET_NEXT_STATION File=ASB$FIND_STATION.:E05TSI            TUE 07/29/97 09:21 Page:4    
       45    23472        /********************************************************************/
       46    23473        /*   Parameter        parameter        parameter        parameter   */
       47    23474        /********************************************************************/
       48    23475    1   DCL TWSN    UBIN;
       49    23476        /********************************************************************/
       50    23477        /*   Local based     local based     local based     local based    */
       51    23478        /********************************************************************/
       52    23479        %KL_WSNCVT(NAME = KL$WSNCVT,STCLASS="BASED(DSEG1$)");
       53    23486        %KL_TWSNREC(NAME = KL$TWSNREC,STCLASS="BASED(DSEG2$)");
       54    23511        %EJECT;
PL6.E3A0      #001=ASB$GET_NEXT_STATION File=ASB$FIND_STATION.:E05TSI            TUE 07/29/97 09:21 Page:5    
       55    23512        /********************************************************************/
       56    23513        /*   Auto data       auto data       auto data       auto data      */
       57    23514        /********************************************************************/
       58    23515    1   DCL I UBIN;
       59    23516    1   DCL STAT_ARS UBIN;
       60    23517    1   DCL TWSNCHAR   CHAR(4);
       61    23518        %EJECT;
PL6.E3A0      #001=ASB$GET_NEXT_STATION File=ASB$FIND_STATION.:E05TSI            TUE 07/29/97 09:21 Page:6    
       62    23519        /********************************************************************/
       63    23520        /*   Procedure       procedure       procedure       procedure      */
       64    23521        /********************************************************************/
       65    23522    1   USERS_KEY.COUNT = 4;
       66    23523    1   USERS_KEY.TXTC = 'TERM';
       67    23524    1   STAT_ARS = DSEG1SIZE;
       68    23525    2   CALL ASB$READ_HLP(DSEG1$ ,STAT_ARS, 1 ) WHENALTRETURN DO;
       69    23526    2      CALL ASA$ERROR ( USERS_KEY, E$UNABLE_TO_READ_HLP);
       70    23527    2      ALTRETURN;
       71    23528    2      END;  /*  Whenaltreturn  */
       72    23529    2   DO WHILE  KL$WSNCVT.WSN(TWSN) = ' ' AND TWSN <= KL$WSNCVT.LTWSN ;
       73    23530    2      TWSN = TWSN + 1;
       74    23531    2      END;  /*  Do while  */
       75    23532    1   IF TWSN > KL$WSNCVT.LTWSN THEN
       76    23533    1      ALTRETURN;
       77    23534    1   CALL BINCHAR (TWSNCHAR,TWSN);
       78    23535    1   USERS_KEY.COUNT = 8;
       79    23536    1   CALL CONCAT (USERS_KEY.TXTC, 'TWSN', TWSNCHAR);
       80    23537    1   STAT_ARS = DSEG2SIZE;
       81    23538    2   CALL ASB$READ_HLP(DSEG2$ ,STAT_ARS, 2 ) WHENALTRETURN DO;
       82    23539    2      CALL ASA$ERROR ( USERS_KEY, E$UNABLE_TO_READ_HLP);
       83    23540    2      ALTRETURN;
       84    23541    2      END;  /*  Whenaltreturn  */
       85    23542
       86    23543    1   RETURN ;
       87    23544    1   END ASB$GET_NEXT_STATION;
       88    23545        %EOD;

