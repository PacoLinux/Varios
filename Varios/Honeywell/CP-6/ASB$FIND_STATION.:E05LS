      146    23499    1   DCL CHANGE  BIT(1);
      147    23500        %EJECT;
PL6.E3A0      #002=ASB$INIT_GET_STATION File=ASB$FIND_STATION.:E05TSI            TUE 07/29/97 09:22 Page:35   
      148    23501        /********************************************************************/
      149    23502        /*   Procedure       procedure       procedure       procedure      */
      150    23503        /********************************************************************/
      151    23504    1   CHANGE = AS_FALSE;
      152    23505        /*
      153    23506          Setup and get the 'TERM' record from :HLP
      154    23507        */
      155    23508    1   USERS_KEY.COUNT = 4;
      156    23509    1   USERS_KEY.TW.NAME = 'TERM';
      157    23510    1   STAT_ARS = DSEG1SIZE;
      158    23511    1   CALL ASB$ENQ_HLP;
      159    23512    2   CALL ASB$READ_HLP(DSEG1$ ,STAT_ARS, 1 ) WHENALTRETURN DO;
      160    23513    2      CALL ASA$ERROR ( USERS_KEY, E$UNABLE_TO_READ_HLP);
      161    23514    2      CALL ASB$DEQ_HLP;
      162    23515    2      RETURN;
      163    23516    2      END;
      164    23517    2   DO I = 0 TO KL$WSNCVT.LTWSN;
      165    23518    2      IF KL$WSNCVT.WSN(I) ~= ' ' THEN
      166    23519    2         CALL ASV$VALIDATE_STATION (KL$WSNCVT.WSN(I), I)
      167    23520    3         WHENALTRETURN DO;
      168    23521    3            CHANGE = %YES#;
      169    23522    3            END; /*  Whenaltreturn  */
      170    23523    2      END; /*  Do i  */
      171    23524    1   IF CHANGE THEN
      172    23525    2   DO;
      173    23526    2      USERS_KEY.COUNT = 4;
      174    23527    2      USERS_KEY.TXTC  = 'TERM';
      175    23528    2      CALL ASB$WRITE_HLP( DSEG1$, STAT_ARS );
      176    23529    2      END; /*  If change  */
      177    23530    1   CALL ASB$DEQ_HLP;
      178    23531
      179    23532    1   RETURN ;
      180    23533    1   END ASB$INIT_GET_STATION;

