        5        5        /*T***********************************************************/
        6        6        ASB$GET_NEXT_STATION: PROC (TWSN) ALTRET;
        7        7
        8        8        /*D*
        9        9             NAME: ASB$GET_NEXT_STATION
       10       10             CALL: CALL ASB$GET_NEXT_STATION (TWSN)
       11       11             INPUT:  TWSN - The number of the station to get.
       12       12             OUTPUT:
       13       13             DESCRIPTION:
       14       14                  This routine will check the specified twsn in the TERM
       15       15                  record for non-blank.  If it is blank, it will cycle
       16       16                  through the rest of the TERM record for a non-blank twsn.
       17       17                  If it doesn't find a valid twsn, it will altreturn.
       18       18                  If it does find a valid twsn, it will read the 'TWSNtwsn'
       19       19                  record into data segment 2 and return.
       20       20        */
       21       21        /********************************************************************/
       22       22        /*   Include files   include files   include files   include files  */
       23       23        /********************************************************************/
       24       24        %INCLUDE AS_PERR_C;
       25      626        %INCLUDE AS_SUPER_C;
