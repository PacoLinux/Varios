      200     9674                      DCB  = M$IFAD,
      201     9675                      WAIT = YES) ;
      202     9714
      203     9715        /* Code */
      204     9716
      205     9717    2      RETRY_COUNT = 0 ;
      206     9718
      207     9719    2      BLOCK.CONTROL = %NORMAL ;
      208     9720    2      BLOCK.BSN = FILE_BSN ;
      209     9721    2      FILE_BSN = FILE_BSN + 1 ;
      210     9722
