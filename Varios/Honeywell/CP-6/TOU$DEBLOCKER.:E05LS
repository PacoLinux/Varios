PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:16   
      375    10019    1   DB_RETURN_EOF: PROC ;
      376    10020
      377    10021        /* Code */
      378    10022
      379    10023    2      FILE_FILE = FILE_FILE + 1 ;
      380    10024    2      FILE_BSN = 1 ;
      381    10025
      382    10026    2      RECORD.SIZE = 0 ;
      383    10027    2      BLOCK.CONTROL = %NORMAL ;
      384    10028
      385    10029    2      IF FILE_SIZE = 0
      386    10030    2      THEN FILE_EOF = 2 ;
      387    10031    2      ELSE FILE_EOF = 1 ;
      388    10032
      389    10033    2      FILE_SIZE = 0 ;
      390    10034    2      BLOCK_SEQ = ' ' ;
      391    10035    2      RETURN ;
