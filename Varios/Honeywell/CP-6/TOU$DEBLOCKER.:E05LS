PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:17   
      395    10039    1   DB_GET_RCW: PROC ALTRET ;
      396    10040
      397    10041        /* Code */
      398    10042
      399    10043    2      RCW_ = BLOCK.DATA(BLOCK.OFFSET) ;
      400    10044    3      DO CASE (RCW.SIZE) ;
      401    10045    3         CASE (0) ;
      402    10046    3            IF RCW.FILE_MARK = '17'O
      403    10047    3            THEN BLOCK.CONTROL = %NORMAL ;
      404    10048    4            ELSE DO ;
      405    10049    4                    CALL DB_ERR_MSG (%E$DB_OBJ_FILE_MARK) ;
      406    10050    4                    ALTRETURN ;
      407    10051    4                 END /* ELSE */ ;
      408    10052    3         CASE (14, 27) ;
      409    10053    3            IF RCW.SIZE = 14 & RCW.MEDIA_CODE = %BCD OR
      410    10054    3                    RCW.SIZE = 27 & RCW.MEDIA_CODE = %BINARY
      411    10055    3            THEN IF BLOCK.OFFSET + RCW.SIZE + 2 > BLOCK.CONTROL
      412    10056    4               THEN DO ;
      413    10057    4                       CALL DB_ERR_MSG (%E$DB_IFAD_LBLOCK) ;
      414    10058    4                       ALTRETURN ;
      415    10059    4                    END /* THEN */ ;
      416    10060    3               ELSE /* Record returned from data block. */ ;
      417    10061    4            ELSE DO ;
      418    10062    4                    CALL DB_ERR_MSG (%E$DB_IFAD_MCODE) ;
      419    10063    4                    ALTRETURN ;
      420    10064    4                 END /* ELSE */ ;
      421    10065    3         CASE (ELSE) ;
      422    10066    3            CALL DB_ERR_MSG (%E$DB_IFAD_SIZE) ;
      423    10067    3            ALTRETURN ;
      424    10068    3      END /* CASE */ ;
      425    10069    2      RETURN ;
      426    10070
      427    10071    2   END DB_GET_RCW ;
      428    10072        %EJECT ;
PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:18   
      429    10073    1   DB_RETURN_RECORD: PROC ;
      430    10074
      431    10075        /* Code */
      432    10076
      433    10077    2      RECORD.SIZE = RCW.SIZE ;
      434    10078    3      DO CUR# = 0 TO RCW.SIZE - 1 ;
      435    10079    3         RECORD.DATA(CUR#) = BLOCK.DATA(CUR# + 1 + BLOCK.OFFSET) ;
      436    10080    3      END ;
      437    10081    2      BLOCK.OFFSET = BLOCK.OFFSET + RCW.SIZE + 1 ;
