      112     9586
      113     9587    1      IF GET_NEW_BLOCK
      114     9588    2      THEN DO ;
      115     9589    2              CALL DB_FILL_BLOCK ALTRET (ERROR) ;
      116     9590    2              CALL DB_SET_IFAD_BLOCK ALTRET (ERROR) ;
      117     9591    2           END /* THEN */ ;
      118     9592    1      ELSE /* Use current block of current file */ ;
      119     9593
      120     9594    1      IF BLOCK.CONTROL = %EOF
      121     9595    1      THEN CALL DB_RETURN_EOF ;
