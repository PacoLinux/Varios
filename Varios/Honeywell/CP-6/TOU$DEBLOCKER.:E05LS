PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:10   
      224     9736    1   DB_CHECK_IFAD_STATUS: PROC ALTRET ;
      225     9737
      226     9738    2      DCL B$TCB$            PTR SYMREF ;
      227     9739    2      DCL 1 EOF_RCW CONSTANT ALIGNED,
      228     9740    2            2 SIZE        UBIN HALF UNAL INIT (0),
      229     9741    2            2 FILE_MARK   BIT  (6)  UNAL INIT ('17'O),
      230     9742    2            2 *           UBIN (12) UNAL INIT (0) ;
      231     9743    2      DCL EOF_RCW_ REDEF EOF_RCW UBIN WORD ALIGNED ;
      232     9744           %B$ALT ;
      233     9752           %B$TCB ;
      234     9755
      235     9756        /* Code */
      236     9757
      237     9758    2      IF B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR.ERR# = %E$EOF
      238     9759    3      THEN DO ;
      239     9760    3              BLOCK.CONTROL = %EOF ;
      240     9761    3              BLOCK.OFFSET = 0 ;
      241     9762    3              BLOCK.BCW.BSN = BLOCK.BSN ;
      242     9763    3              BLOCK.BCW.SIZE = 1 ;
      243     9764    3              BLOCK.DATA(BLOCK.OFFSET) = EOF_RCW_ ;
      244     9765    3              RETURN ;
      245     9766    3           END /* THEN */ ;
      246     9767    3      ELSE DO ;
      247     9768    3              BLOCK.CONTROL = %ERROR ;
      248     9769    3              CALL DB_ERR_MSG (%E$DB_IFAD_READ) ;
      249     9770    3              CALL TOU$DU_SYSTEM_ERRMSG ;
      250     9771    3              ALTRETURN ;
      251     9772    3           END /* ELSE */ ;
      252     9773
      253     9774    2   END DB_CHECK_IFAD_STATUS ;
      254     9775        %EJECT ;
PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:11   
      255     9776    1   DB_SET_IFAD_BLOCK: PROC ALTRET ;
      256     9777
      257     9778        /* Code */
      258     9779
      259     9780    3      DO WHILE (BLOCK.CONTROL = %NORMAL) ;
      260     9781    3         CALL DB_CHECK_IFAD_CONTROL ALTRET (ERROR) ;
      261     9782    3         IF (BLOCK.BCW.BSN ~= BLOCK.BSN)
      262     9783    3         THEN CALL DB_RECOVER_BSN_ERROR ALTRET (ERROR) ;
      263     9784    3         ELSE /* No Error */ ;
      264     9785    3      END ;
      265     9786    2      BLOCK.OFFSET = 0 ;
      266     9787    2      IF BLOCK.CONTROL = %EOF
      267     9788    2      THEN /* No file size to assign. */ ;
      268     9789    2      ELSE FILE_SIZE = FILE_SIZE + BLOCK.CONTROL ;
      269     9790    2      RETURN ;
      270     9791
      271     9792    2   ERROR:
      272     9793    2      ALTRETURN ;
      273     9794
      274     9795        /* END DB_SET_IFAD_BLOCK */
      275     9796        %EJECT ;
PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:12   
      276     9797    2   DB_CHECK_IFAD_CONTROL: PROC ALTRET ;
      277     9798
      278     9799           %F$DCB ;
      279     9856
      280     9857        /* Code */
      281     9858
      282     9859    3      IFAD_ARS# = DCBADDR (DCBNUM (M$IFAD)) -> F$DCB.ARS# ;
      283     9860
      284     9861    3      BLOCK.CONTROL = IFAD_ARS# / 4 ;
      285     9862
      286     9863    3      IF MOD (IFAD_ARS#, 4) > 1
      287     9864    3      THEN CALL DB_ERR_MSG (%E$DB_IFAD_RCD_SIZE) ;
      288     9865    3      ELSE IF MOD (IFAD_ARS#, 4) = 1 AND MOD (BLOCK.CONTROL, 2) ~= 1
      289     9866    3         THEN CALL DB_ERR_MSG (%E$DB_IFAD_RCD_SIZE) ;
      290     9867    3         ELSE IF BLOCK.BCW.SIZE ~= BLOCK.CONTROL - 1
      291     9868    3            THEN CALL DB_ERR_MSG (%E$DB_IFAD_BLOCK_SIZE) ;
      292     9869    3            ELSE RETURN ;
      293     9870    3      ALTRETURN ;
      294     9871
      295     9872    3   END DB_CHECK_IFAD_CONTROL ;
      296     9873        %EJECT ;
PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:13   
      297     9874    2   DB_RECOVER_BSN_ERROR: PROC ALTRET ;
      298     9875
      299     9876        /* Code */
      300     9877
      301     9878    3      BLOCK.CONTROL = %ERROR ;
      302     9879    3      RETRY_COUNT = RETRY_COUNT + 1 ;
      303     9880    3      IF RETRY_COUNT > 64
      304     9881    4      THEN DO ;
      305     9882    4              CALL DB_ERR_MSG (%E$DB_BSN_ERROR) ;
      306     9883    4   ERROR:
      307     9884    4              ALTRETURN ;
      308     9885    4           END /* THEN */ ;
      309     9886    4      ELSE DO ;
      310     9887    4              CALL DB_REPOSITION_IFAD ALTRET (ERROR) ;
      311     9888    4              CALL DB_REFILL_BLOCK ALTRET (ERROR) ;
      312     9889    4              RETURN ;
      313     9890    4           END /* ELSE */ ;
      314     9891
      315     9892        /* END DB_RECOVER_BSN_ERROR */
      316     9893        %EJECT ;
PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:14   
      317     9894    3   DB_REPOSITION_IFAD: PROC ALTRET ;
      318     9895
      319     9896    4      DCL B$TCB$   PTR SYMREF ;
      320     9897           %B$TCB ;
      321     9900           %B$ALT ;
      322     9908           %FPT_PRECORD (FPTN = POSITION_IFAD,
      323     9909                         DCB  = M$IFAD) ;
      324     9930
      325     9931        /* Code */
      326     9932
      327     9933    4      POSITION_IFAD.V.N# = BLOCK.BSN - 1 - BLOCK.BCW.BSN ;
      328     9934    4      IF POSITION_IFAD.V.N# = 0
      329     9935    4      THEN /* Do Nothing */ ;
      330     9936    4      ELSE CALL M$PRECORD (POSITION_IFAD) ALTRET (BAD_IFAD_POSITION) ;
      331     9937    4      RETURN ;
      332     9938
      333     9939    4   BAD_IFAD_POSITION:
      334     9940    5      DO CASE (B$TCB$ -> B$TCB.ALT$ -> B$ALT.ERR.ERR#) ;
      335     9941    5         CASE (%E$EOF) ;
      336     9942    5            CALL DB_ERR_MSG (%E$DB_BSN_EOF) ;
      337     9943    5            ALTRETURN ;
      338     9944    5         CASE (%E$BOT) ;
      339     9945    5            RETURN ;
      340     9946    5         CASE (ELSE) ;
      341     9947    5            CALL DB_ERR_MSG (%E$DB_IFAD_POSITION) ;
      342     9948    5            CALL TOU$DU_SYSTEM_ERRMSG ;
      343     9949    5            ALTRETURN ;
      344     9950    5      END /* CASE */ ;
      345     9951
      346     9952    4   END DB_REPOSITION_IFAD ;
      347     9953        %EJECT ;
PL6.E3A0      #001=TOU$DEBLOCKER File=TOU$DEBLOCKER.:E05TSI                      WED 07/30/97 06:40 Page:15   
      348     9954    3   DB_REFILL_BLOCK: PROC ALTRET ;
      349     9955
      350     9956           %FPT_READ (FPTN = READ_IFAD,
      351     9957                      BIN  = YES,
      352     9958                      BUF  = BLOCK_.BUFFER,
      353     9959                      DCB  = M$IFAD,
      354     9960                      WAIT = YES) ;
      355     9999
      356    10000        /* Code */
      357    10001
      358    10002    4      CALL M$READ (READ_IFAD) ALTRET (BAD_IFAD_READ) ;
      359    10003    4      IF BLOCK.CONTROL = %ERROR
      360    10004    4      THEN BLOCK.CONTROL = %NORMAL ;
      361    10005    4      ELSE /* BLOCK.CONTROL was NORMAL or EOF */ ;
      362    10006    4      RETURN ;
      363    10007
      364    10008    4   BAD_IFAD_READ:
      365    10009    4      CALL DB_CHECK_IFAD_STATUS ALTRET (ERROR) ;
      366    10010    4      RETURN ;
      367    10011
      368    10012    4   ERROR:
      369    10013    4      ALTRETURN ;
      370    10014
      371    10015    4   END DB_REFILL_BLOCK ;
