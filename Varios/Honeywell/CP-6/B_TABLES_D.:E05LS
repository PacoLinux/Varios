        7        7
        8        8        /*P* NAME: B_TABLES_D
        9        9        PURPOSE: Common SEARCH and XLATE tables.
       10       10        DESCRPTION:
       11       11                This module contains the standard set of common SEARCH and
       12       12                XLATE tables.  This set of tables is intended for the English
       13       13                language.  B_ALTTBLS_D contains an alternate set of tables
       14       14                that can be used for a different language.
       15       15        */
       16       16
       17       17        B_TABLES_D: PROC;
       18       18
       19       19        %INCLUDE B_TABLES_C;
       20      710        %INCLUDE B$TABLES_C;
       21      818
       22      819        %LIST;
       23      820        %B_TABLES( NAME=B_TABLES );
               821    1   DCL 1 NAME CONSTANT SYMDEF DALIGNED,
               822            %B_SR_NON_BLANK;    /*   0  Search for non-blank                */
               823    1     2 B_SR_NON_BLANK( 0:511 ) BIT(9) CALIGNED INIT(
               824    1       %B$SR#CTL * 32,                 /*  NULL thru US                */
               825    1       '0'B,                           /*  SP                          */
               826    1       %B$SR#PUNC * 2,                 /*  ! "                         */
               827    1       %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
               828    1       %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
               829    1       %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
               830    1       %B$SR#PUNC,                     /*  ,                           */
               831    1       %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
               832    1       %B$SR#PUNC,                     /*  /                           */
               833    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 8,
               834                                            /*  0 thru 7                    */
PL6.E3A0      #001=B_TABLES_D File=B_TABLES_D.:E05TSI                            TUE 07/29/97 11:10 Page:2    
               835    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
               836    1       %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
               837    1       %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
               838    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
               839    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      G thru Z                */
               840    1       %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
               841    1       %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
               842    1       %B$SR#PUNC,                     /*  `                           */
               843    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
               844    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      g thru z                */
               845    1       %B$SR#PUNC * 4,                 /*  { | } ~                     */
               846    1       %B$SR#CTL,                      /*  DEL                         */
               847    1       %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
               849            %B_SR_NON_ZERO;     /*   1  Search for non-zero                 */
               850    1     2 B_SR_NON_ZERO( 0:511 ) BIT(9) CALIGNED INIT(
               851    1       %B$SR#CTL * 32,                 /*  NULL thru US                */
               852    1       %B$SR#PUNC * 3,                 /*  SP ! "                      */
               853    1       %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
               854    1       %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
               855    1       %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
               856    1       %B$SR#PUNC,                     /*  ,                           */
               857    1       %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
               858    1       %B$SR#PUNC,                     /*  /                           */
               859    1       '0'B,                           /*  0                           */
               860    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 7,
               861                                            /*  1 thru 7                    */
               862    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
               863    1       %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
               864    1       %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
               865    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
               866    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      G thru Z                */
               867    1       %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
               868    1       %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
               869    1       %B$SR#PUNC,                     /*  `                           */
               870    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
               871    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*      g thru z                */
               872    1       %B$SR#PUNC * 4,                 /*  { | { ~                     */
PL6.E3A0      #001=B_TABLES_D File=B_TABLES_D.:E05TSI                            TUE 07/29/97 11:10 Page:3    
               873    1       %B$SR#CTL,                      /*  DEL                         */
               874    1       %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
               876            %B_SR_NON_DEC;      /*   2  Search for non-decimal              */
               877    1     2 B_SR_NON_DEC( 0:511 ) BIT(9) CALIGNED INIT(
               878    1       %B$SR#CTL * 32,                 /*  NULL thru US                */
               879    1       %B$SR#PUNC * 3,                 /*  SP ! "                      */
               880    1       %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
               881    1       %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
               882    1       %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
               883    1       %B$SR#PUNC,                     /*  ,                           */
               884    1       %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
               885    1       %B$SR#PUNC,                     /*  /                           */
               886    1       '0'B * 10,                      /*  0 thru 9                    */
               887    1       %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
               888    1       %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
               889    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  A thru F        */
               890    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*  G thru Z                    */
               891    1       %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
               892    1       %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
               893    1       %B$SR#PUNC,                     /*  `                           */
               894    1       %( B$SR#ALPHA | B$SR#HEX | B$SR#SYM ) * 6,  /*  a thru f        */
               895    1       %( B$SR#ALPHA | B$SR#SYM ) * 20,/*  g thru z                    */
               896    1       %B$SR#PUNC * 4,                 /*  { | } ~                     */
               897    1       %B$SR#CTL,                      /*  DEL                         */
               898    1       %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
               900            %B_SR_NON_ALPHA;    /*   3  Search for non-alphabetic           */
               901    1     2 B_SR_NON_ALPHA( 0:511 ) BIT(9) CALIGNED INIT(
               902    1       %B$SR#CTL * 32,                 /*  NULL thru US                */
               903    1       %B$SR#PUNC * 3,                 /*  SP ! "                      */
               904    1       %( B$SR#PUNC | B$SR#SYM ) * 2,  /*  # $                         */
               905    1       %B$SR#PUNC * 6,                 /*  % & ' ( ) *                 */
               906    1       %( B$SR#PUNC | B$SR#NUM ),      /*  +                           */
               907    1       %B$SR#PUNC,                     /*  ,                           */
               908    1       %( B$SR#PUNC | B$SR#NUM ) * 2,  /*  - .                         */
               909    1       %B$SR#PUNC,                     /*  /                           */
               910    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#OCT | B$SR#SYM ) * 8,
               911                                            /*  0 thru 7                    */
PL6.E3A0      #001=B_TABLES_D File=B_TABLES_D.:E05TSI                            TUE 07/29/97 11:10 Page:4    
               912    1       %( B$SR#DEC | B$SR#NUM | B$SR#HEX | B$SR#SYM ) * 2, /*  8 9     */
               913    1       %B$SR#PUNC * 6,                 /*  : ; < = > ?                 */
               914    1       %( B$SR#PUNC | B$SR#SYM ),      /*  @                           */
               915    1       '0'B * 26,                      /*  A thru Z                    */
               916    1       %B$SR#PUNC * 4,                 /*  [ \ ] ^                     */
               917    1       %( B$SR#PUNC | B$SR#SYM ),      /*  _                           */
               918    1       %B$SR#PUNC,                     /*  `                           */
               919    1       '0'B * 26,                      /*  a thru z                    */
               920    1       %B$SR#PUNC * 4,                 /*  { | } ~                     */
               921    1       %B$SR#CTL,                      /*  DEL                         */
               922    1       %B$SR#OTHER * 384 ),            /*  non-ASCII characters        */
               924            %B_XL_ASC_EBC;      /*   4  Translate ASCII to EBCDIC           */
               925    1     2 B_XL_ASC_EBC( 0:511 ) UBIN(9) CALIGNED INIT(
               926            /*Most sig-     Least significant digits    */
