        8        8
        9        9        /*P*  NAME:         QCU$USE
       10       10              PURPOSE:      Handle INVALID clauses and USE procedures after errors.
       11       11              DESCRIPTION:  An IO error has occurred, and this routine has been
       12       12                            called to check for any INVALID clause on the statement
       13       13                            being executed, or for any USE procedure which is to be
       14       14                            invoked.
       15       15        */
       16       16
       17       17        QCU$USE: PROC ( QCU@USE );
       18       18
       19       19          %INCLUDE LCP_6;
       20      210
       21      211          %INCLUDE QG_MACRO_M;
       22      348          %INCLUDE QI_EQUATES_E;
       23      707          %INCLUDE QI_PERR_C;
       24      875          %INCLUDE QI_MACROS_M;
       25     5933          %QI$ALL;
       26    17046          %QI_FPTS;
       27    19091
       28    19092    1     DCL FIELD_INDEX UBIN;
       29    19093    1     DCL FRAME$ PTR;
       30    19094    1     DCL FRAME_INDEX UBIN;
       31    19095    1     DCL FRAME_STATUS$ PTR;
       32    19096    1     DCL GZ_FIELD$ PTR;
       33    19097    1     DCL IO_OR_VERIFY_INDEX UBIN;
       34    19098    1     DCL USE_PROCEDURE_TYPE_INDEX UBIN;
       35    19099
       36    19100    1     DCL QCE$ERROR ENTRY (4);
       37    19101    1     DCL QCE$ERROR_INTERNAL ENTRY (4);
PL6.E3A0      #001=QCU$USE File=QCU$USE.:E05TSI                                  WED 07/30/97 05:15 Page:2    
       38    19102    1     DCL QCU$USE_CALL ENTRY (1);
       39    19103
       40    19104          %SET LISTMAC = '1'B;
       41    19105
       42    19106          %INCLUDE QCU_USE_M;
       43    19160          %QCU@USE ( STCLASS = PARAM );
             19161            %BEGIN_MACRO;
             19162            %SET LISTDIR = '0'B;
             19175    1     DCL 1 QCU@USE PARAM ALIGNED,
             19176    1           2 CURR_FRAME$ PTR,
             19177                          /* Pointer to DAD of current frame or ADDR (NIL).  */
             19178    1           2 CURR_NODE$ PTR,
             19179                          /* Pointer to current node or ADDR (NIL).  */
             19180    1           2 USE_DEFAULT_IO UBIN,
             19181                          /* For USE procedures which default applies
             19182                             (see corresponding EQUs).  */
             19183    1           2 IS_TIME_TO_SAVE_AWAY_FIELDS BIT(1) ALIGNED,
             19184                          /* If IS_TIME_TO_SAVE_AWAY_FIELDS, then
             19185                             save the fields within the bounds of
             19186                             FRAME_START_INDEX, FRAME_STOP_INDEX,
             19187                             FIELD_START_INDEX, and FIELD_STOP_INDEX
             19188                             before calling the USE procedure.  */
             19189    1           2 FRAME_START_INDEX UBIN,
             19190                          /* Foist frame of the CURRENT.FORM$
             19191                             involved with the I-O error.  */
             19192    1           2 FRAME_STOP_INDEX UBIN,
             19193                          /* Larst frame of the CURRENT.FORM$
             19194                             involved with the I-O error.  */
             19195    1           2 IS_FIELD_INDEXES_VALID BIT(1) ALIGNED,
             19196                          /* If IS_FIELD_INDEXES_VALID, use them;
             19197                             else use all the fields in the frame.  */
             19198    1           2 FIELD_START_INDEX UBIN,
             19199                          /* Starting index into QI$GZ_FIELD
             19200                             which may have values "pushed".  */
             19201    1           2 FIELD_STOP_INDEX UBIN,
             19202                          /* Stopping index into QI$GZ_FIELD
             19203                             which may have values "pushed".  */
PL6.E3A0      #001=QCU$USE File=QCU$USE.:E05TSI                                  WED 07/30/97 05:15 Page:3    
             19204    1           2 IS_CURSOR_POSITION_VALID BIT(1) ALIGNED,
             19205                          /* If IS_CURSOR_POSITION_VALID, use it;
             19206                             else allow cursor to default to upper-left-most field.  */
             19207    1           2 CURSOR_POSITION BIT(16) ALIGNED,
             19208                          /* POSition of the field on which to place the cursor.  */
             19209    1           2 STATUS UBIN;
             19210                          /* Returned action code; see corresponding EQUs.  */
             19217            %ENDIF;
       44    19220
       45    19221          %SET LISTMAC = '0'B;
       46    19222        %EJECT;
PL6.E3A0      #001=QCU$USE File=QCU$USE.:E05TSI                                  WED 07/30/97 05:15 Page:4    
       47    19223    1   QCU$USE_INVALID_IO: ENTRY ( QCU@USE );
       48    19224
       49    19225        /*D*  NAME:         QCU$USE_INVALID_IO
       50    19226              CALL:         CALL QCU$USE_INVALID_IO ( QCU@USE );
       51    19227              PARAMETERS:   QCU@USE is a structure which contains information
       52    19228                            concerning the FORM or TRANSACTION file which the
       53    19229                            error occured in, and the frame and field involved,
       54    19230                            if any.  It's generated by invoking the QCU@USE %MACRO
       55    19231                            within the QCU_USE_M %INCLUDE file (see commentary
       56    19232                            within that file).
       57    19233              INTERFACE:    See QCU$USE_IO commentary.
       58    19234              ENVIRONMENT:  Called for interpreter-detected "external" errors.
       59    19235              INPUT:        QI_1, etc.
       60    19236              OUTPUT:       QI_1, etc.
       61    19237              SCRATCH:      Various otter stuff.
       62    19238              DESCRIPTION:  Called by routines executing statements which may
       63    19239                            have INVALID clauses associated with them, for IO
       64    19240                            errors.  Any appropriate USE procedures are called.
       65    19241        */
       66    19242
       67    19243    1       IO_OR_VERIFY_INDEX = %QI#STANDARD;
       68    19244    1       GOTO NEXT_USE;
       69    19245
