PL6.E3A0      #001=Z8AM$COBOL_80_COMPILER File=Z8AM.:Z8A6TSI                     SAT 08/09/97 10:43 Page:5    
       96    12133             /* Save Date/Time/UTS of compilation */
       97    12134
       98    12135             %IF GCOS ;
      100    12137             %ENDIF ;
      101    12138    1        CALL Z9AM$SAVE_DATE ;
      102    12139    1        CALL Z9A1$INITIALIZE; /* CP-6- or GCOS-specific initialization. */
      103    12140
      104    12141    1        CALL X66_AUTO_EX ALTRET (AUTO_OVERFLOW) ; /* If auto stack overflow */
      105    12142
      106    12143             %IF CP6 ;
      107    12144    1        CALL Z9AT$START ; /* Set wrapup and break control */
      108    12145             %ENDIF ;
      109    12146
      110    12147    1        CALL Z9AP$BRING_IN_PHASE (0);
      111    12148
      112    12149             /* Set COMCOM.SEVERITY based on INFO, NWARN, etc. options */
      113    12150
      114    12151    1        IF COMCOM.OB.INFO
      115    12152    1        THEN COMCOM.SEVERITY = 000 ; /* Print all errors */
      116    12153    1        ELSE
      117    12154    1        IF NOT COMCOM.OB.NWARN /* WARN option */
      118    12155    1        THEN COMCOM.SEVERITY = 100 ; /* Exclude only INFO */
      119    12156             %IF GCOS;
      124    12161             %ENDIF;
      125    12162    1        ELSE
      126    12163    1        IF COMCOM.OB.SV5
      127    12164    1        THEN COMCOM.SEVERITY = 900 ; /* Print only abort message */
      128    12165    1        ELSE COMCOM.SEVERITY = 200 ; /* NWARN, SV3, SV4 */
      129    12166
      130    12167
      131    12168    1        CALL Z9AP$BRING_IN_PHASE (1);
      132    12169
      133    12170    1        CALL Z9AP$BRING_IN_PHASE (2) ;
      134    12171
      135    12172             /*V* Don't call Phases 3 or 4 if there is a 5-star error */
      136    12173         /*
      137    12174             IF COMCOM.MAX_ERROR >= 900
PL6.E3A0      #001=Z8AM$COBOL_80_COMPILER File=Z8AM.:Z8A6TSI                     SAT 08/09/97 10:43 Page:6    
      138    12175             THEN COMCOM.OB.CHECK = '1'B ;
      139    12176         */
      140    12177
      141    12178    2        IF COMCOM.OB.CHECK = '1'B THEN DO ;
      142    12179    2          COMCOM.OB.LISTOUT = '0'B ;
      143    12180               %IF CP6 ;
      144    12181    2          COMCOM.OB.LO      = '0'B ;
      145    12182    2          COMCOM.OB.DECK    = '0'B ;
      146    12183               %ENDIF ;
      147    12184    2          COMCOM.OB.DSS     = '0'B ;
      148    12185    2        END ;
      149    12186    1        ELSE
      150    12187    1        IF  COMCOM.OB.LISTOUT = '0'B
      151    12188             %IF CP6 ;
      152    12189    1        AND COMCOM.OB.LO      = '0'B
      153    12190             %ENDIF ;
      154    12191    1        AND COMCOM.OB.DECK    = '0'B
      155    12192    1        THEN COMCOM.OB.CHECK  = '1'B ;
      156    12193
      157    12194    2        IF COMCOM.OB.CHECK = '0'B THEN DO ;
      158    12195
      159    12196    2          CALL Z9AP$BRING_IN_PHASE(3);
      160    12197    2          CALL Z8AR$FREER (ADDR(GST_)) ;
      161    12198    2          CALL Z8AR$FREER (ADDR(DPI_)) ;
      162    12199
      163    12200    2          CALL Z9AP$BRING_IN_PHASE(4);
      164    12201
      165    12202    2        END /* If Then Do */ ;
      166    12203
      167    12204    1        IF  COMCOM.TREE_LINE = ' '
      168    12205    2        AND COMCOM.OB.IT = '0'B THEN DO ;
      169    12206    2          IF COMCOM.OB.DSS OR COMCOM.OB.LISTOUT OR COMCOM.OB.XREF
      170    12207    2          OR COMCOM.OB.MAP OR COMCOM.OB.ARRAY_PRESET
      171    12208    2          THEN ; /* Don't purge QLT */
      172    12209    3          ELSE DO ; /* Debug schema,LSTOU,XREF off, no array presets */
      173    12210    3            CALL Z8AR$FREER (ADDR(QLT_)) ;
      174    12211    3            COMCOM.PREV_QLT$ = ADDR(NIL) ;
PL6.E3A0      #001=Z8AM$COBOL_80_COMPILER File=Z8AM.:Z8A6TSI                     SAT 08/09/97 10:43 Page:7    
      175    12212    3          END ;
      176    12213
      177    12214    2          CALL Z8AR$FREER (ADDR(ILL_)) ;
      178    12215    2          CALL Z8AR$FREER (ADDR(ILS_)) ;
      179    12216    2        END ;
      180    12217
      181    12218    1        CALL Z9AP$BRING_IN_PHASE(5);
      182    12219
      183    12220    1        IF  COMCOM.TREE_LINE = ' '
      184    12221    1        AND COMCOM.OB.IT = '0'B THEN
      185    12222    1          CALL Z8AR$FREER (ADDR(ETT_)) ;
      186    12223
      187    12224             /* Compute Total Time */
      188    12225
      189    12226    1        IF COMCOM.OB.STATS
      190    12227    1        THEN COMCOM.TIMES(0) = PREVIOUS - COMCOM.TIMES(0);
      191    12228
      192    12229    1        IF COMCOM.OB.STATS
      193    12230    1        THEN CALL Z9AP$STATS;
      194    12231
      195    12232    1        IF COMCOM.TREE_LINE ~= ' '
      196    12233    2        OR COMCOM.OB.IT = '1'B THEN DO ;
      197    12234    2          CALL Z8JS$START_TREEWALKER ;
      198    12235    2        END ;
      199    12236
      200    12237    1        CALL Z9AC$CLOSE_LO;
      201    12238    1        CALL Z9AR$FREE_DYNAMIC_SEGMENT;
      202    12239
      203    12240    1        IF COMCOM.MAX_ERROR >= 300 THEN CALL Z9AM$ERR_EXIT ;
      204    12241
      205    12242    1        RETURN ;
      206    12243
      207    12244    1   AUTO_OVERFLOW:
      208    12245
      209    12246    1        CALL Z8AE$ERROR (%E$AUTO_STACK_OVERFLOW_ERROR) ;
      210    12247
      211    12248    1        CALL Z9AT$TERMINATOR ;
PL6.E3A0      #001=Z8AM$COBOL_80_COMPILER File=Z8AM.:Z8A6TSI                     SAT 08/09/97 10:43 Page:8    
      212    12249
      213    12250             %IF GCOS ;
      216    12253             %ENDIF ;
      217    12254
      218    12255    1   END Z8AM$COBOL_80_COMPILER;

