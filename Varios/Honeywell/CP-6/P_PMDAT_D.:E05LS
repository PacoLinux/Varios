      152     2462    1   DCL P_SCHD# (0:7) SBIN STATIC SYMDEF INIT (0*0);       /* Number of schedules */
      153     2463    1   DCL   P_ETMF SBIN STATIC SYMDEF INIT(1);                              /* ETMF */
      154     2464    1   DCL   P_NINTS SBIN STATIC SYMDEF INIT(0);           /* Number of interactions */
      155     2465    1   DCL   P_RT90 SBIN STATIC SYMDEF INIT(2);                 /* 90% response time */
