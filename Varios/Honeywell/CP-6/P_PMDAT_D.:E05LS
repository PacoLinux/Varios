       75     1937                       ENTRIES$=P_DCTXMAX,
       76     1938                       WORDS="SIZEW(NI$DCT)");
       77     1947           %PM_ENTRY  (NAME=NI$DVT$,
       78     1948                       BASED_IMAGE="NI$DVT$(0)",
       79     1949                       ENTRIES$=NI_NDVTS,
       80     1950                       WORDS=6);
