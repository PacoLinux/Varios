/*T*                                                         */
/*T***********************************************************/
/*X* DFC=YES, DMC=YES, EDMC = YES                                  */
/*B*
TCC#?           define the nodes used in the PARTRGE metalanguage
        to define the commands accepted by the CMLOAD program.
 
        Constant                        Command
        --------------------            ---------                  */
/*B*    TCC#END                         END                        */
/*B*    TCC#LOAD                        LOAD                       */
/*B*    TCC#READ                        READ                       */
/*B*    TCC#REPORT                      REPORT                     */
/*B*    TCC#RESET                       RESET                      */
/*                                                                 */
        %SUB    TCC#LOAD                = "1" ;
        %SUB    TCC#READ                = "2" ;
        %SUB    TCC#REPORT              = "3" ;
        %SUB    TCC#END                 = "4" ;
        %SUB    TCC#RESET               = "5" ;
/*B*
TCO#?           define the nodes used in the PARTRGE metalanguage to
        define the options accepted by the CMLOAD program.
 
        Constant                        Option
        --------------------            ---------                  */
/*B*    TCO#ALL                         ALL                        */
/*B*    TCO#D800                        800                        */
/*B*    TCO#D1600                       1600                       */
/*B*    TCO#D6250                       6250                       */
/*B*    TCO#DCNN                        DCNN                       */
/*B*    TCO#IPC                         IPC                        */
/*B*    TCO#LIST                        LIST                       */
/*B*    TCO#ONE                         ONE                        */
/*B*    TCO#SS                          SUBSYSTEM                  */
/*                                                                 */
        %SUB    TCO#ALL                 = "1" ;
        %SUB    TCO#D800                = "2" ;
        %SUB    TCO#D1600               = "3" ;
        %SUB    TCO#D6250               = "4" ;
        %SUB    TCO#DCNN                = "5" ;
        %SUB    TCO#IPC                 = "6" ;
        %SUB    TCO#LIST                = "7" ;
        %SUB    TCO#ONE                 = "8" ;
        %SUB    TCO#SS                  = "9" ;
