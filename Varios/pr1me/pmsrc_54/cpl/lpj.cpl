&args bin:entry=BIN;~
      debug_sw:-debug, -d

COMO *>COMO>LPJ.COMO
&if [null %debug_sw%] & [exists BIN.NODEBUG] &then ~
    &s bin := BIN.NODEBUG

&data BIND
LOAD *>%bin%>LPJ

/* dynamic common owned by the library */

DYNT GBL$STATIC_EXTERNAL

LI *>UTIL>FSULIB
LI VAPPLB
LI *>UTIL>VNETLB
LI
MA *>MAP>LPJ.MAP
DYNT -ALL
FILE
&end
COMO -END
