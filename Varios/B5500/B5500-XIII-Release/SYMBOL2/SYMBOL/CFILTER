069900 START. IF IMGFLG = 1 PERFORM OLDOUT.                             FILTER
070000     MOVE NEXTWORKIN TO WORKIN.                                   FILTER
070100     ADD 1 TO IMGCNT.                                             FILTER
070200     GO TO CARDIN TAPEIN TAPEUN                                   FILTER
070300     CASTARD DISKIN CANDEIN                                       PATCH027
070400     DEPENDING ON INSELECT.                                       FILTER
070500     DISPLAY "INSELECT CONTROL ERR".
070600     STOP RUN.                                                    FILTER
070700 TAPEIN.                                                          FILTER
070800     ADD 1 TO TAPECNT.                                            FILTER
070900     IF TAPECNT < 6                                               FILTER
071000     IF CAE = 0                                                   FILTER
071100         IF SOLTSEQ (TAPECNT) < CRDSEQ                            FILTER
071200             MOVE INTAPE(TAPECNT) TO NEXTWORKIN                   FILTER
071300             GO TO STARTOUT                                       FILTER
071400         ELSE NEXT SENTENCE                                       FILTER
071500     ELSE MOVE INTAPE(TAPECNT) TO NEXTWORKIN GO TO STARTOUT       FILTER
071600     ELSE MOVE 0 TO TAPECNT                                       FILTER
071700     READ TAPEINPUT AT END GO TO FINALIZE ELSE GO TO TAPEIN.      FILTER
071800     MOVE INCARD TO NEXTWORKIN.                                   FILTER
071900     IF CRDSEQ ! SOLTSEQ(TAPECNT) SUBTRACT 1 FROM TAPECNT.        FILTER
072000     READ CARDINPUT AT END MOVE 1 TO CAE                          FILTER
072100     CLOSE CARDINPUT WITH RELEASE.                                FILTER
072200     GO TO STARTOUT.                                              FILTER
072300 CASTARD.                                                         FILTER
072400     ADD 1 TO CASTKEY.                                            FILTER
072500     IF CASTKEY > 5 ADD 1 TO CASTKEYA                             FILTER
072600     MOVE 0 TO CASTKEY                                            FILTER
072700     IF CASTATP = 0                                               FILTER
072800         READ CASTA INVALID KEY DISPLAY "INV CASTA" STOP RUN ELSE FILTER
072900         GO TO CASTARD                                            FILTER
073000     ELSE READ CASTATAPE AT END DISPLAY "INV CASTA" STOP RUN ELSE FILTER
073100         GO TO CASTARD.                                           FILTER
073200     IF CASTKEYA = ENDCASTA IF CASTKEY = ENDCAST                  FILTER
073300         MOVE 0 TO CASTKEY                                        FILTER
073400         MOVE HOLDIT TO NEXTWORKIN                                FILTER
073500         MOVE HOLDINSEL TO INSELECT GO TO STARTOUT.               FILTER
073600     IF CASTATP ! 0 MOVE CASTRCDT(CASTKEY) TO NEXTWORKIN ELSE     FILTER
073700     MOVE CASTRCD(CASTKEY) TO NEXTWORKIN. GO TO STARTOUT.         FILTER
073800 CARDIN.                                                          FILTER
073900     READ CARDINPUT INTO NEXTWORKIN AT END GO TO FINALIZE.        FILTER
074000     GO TO STARTOUT.                                              FILTER
074100 TAPEUN.                                                          FILTER
074200     IF CAE = 0                                                   FILTER
074300     IF UNSEQ < CRDSEQ                                            FILTER
074400         MOVE UNTAPEIMG TO NEXTWORKIN                             FILTER
074500         READ TAPE-UN-INPUT AT END GO TO FINALIZE ELSE            FILTER
074600     GO TO STARTOUT ELSE NEXT SENTENCE                            FILTER
074700     ELSE MOVE UNTAPEIMG TO NEXTWORKIN                            FILTER
074800     READ TAPE-UN-INPUT AT END GO TO FINALIZE ELSE GO TO STARTOUT.FILTER
074900     MOVE INCARD TO NEXTWORKIN.                                   FILTER
075000     IF UNSEQ = CRDSEQ                                            FILTER
075100         READ TAPE-UN-INPUT AT END GO TO FINALIZE.                FILTER
075200     READ CARDINPUT AT END MOVE 1 TO CAE                          FILTER
075300     CLOSE CARDINPUT WITH RELEASE.                                FILTER
075400     GO TO STARTOUT.                                              PATCH027
075500 DISKIN.                                                          PATCH027
075600     IF CAE = 0                                                   PATCH027
075700     IF DISKINSEQ < CRDSEQ                                        PATCH027
075800         MOVE DISKINREC TO NEXTWORKIN                             PATCH027
075900         READ DISKINPUT AT END GO TO FINALIZE                     PATCH027
076000                        ELSE GO TO STARTOUT                       PATCH027
076100     ELSE NEXT SENTENCE                                           PATCH027
076200     ELSE MOVE DISKINREC TO NEXTWORKIN                            PATCH027
076300         READ DISKINPUT AT END GO TO FINALIZE                     PATCH027
076400                        ELSE GO TO STARTOUT.                      PATCH027
076500     MOVE INCARD TO NEXTWORKIN.                                   PATCH027
076600     IF DISKINSEQ = CRDSEQ                                        PATCH027
076700         READ DISKINPUT AT END GO TO FINALIZE.                    PATCH027
076800     READ CARDINPUT AT END MOVE 1 TO CAE                          PATCH027
076900                    CLOSE CARDINPUT WITH RELEASE.                 PATCH027
077000     GO TO STARTOUT.                                              PATCH027
077100 CANDEIN.                                                         PATCH027
077200     IF CAE = 0                                                   PATCH027
077300     IF CANDESEQ < CRDSEQ                                         PATCH027
077400         IF COLSEVEN = "$" OR "-"                                 PATCH027
077500            MOVE CANDESEQ TO NEXTCANDESEQ                         PATCH027
077600            MOVE CTXT TO NXTCANDETXT                              PATCH027
077700             READ DISKINPUT AT END GO TO FINALIZE                 PATCH027
077800                            ELSE GO TO STARTOUT                   PATCH027
077900    ELSE MOVE CANDESEQ TO NEXTCANDESEQ                            PATCH027
078000         MOVE CANDETXT TO NEXTCANDETXT                            PATCH027
078100         READ DISKINPUT AT END GO TO FINALIZE                     PATCH027
078200                        ELSE GO TO STARTOUT                       PATCH027
078300     ELSE NEXT SENTENCE                                           PATCH027
078400     ELSE IF COLSEVEN = "$" OR "-"                                PATCH027
078500             MOVE CANDESEQ TO NEXTCANDESEQ                        PATCH027
078600             MOVE CTXT TO NXTCANDETXT                             PATCH027
078700             READ DISKINPUT AT END GO TO FINALIZE                 PATCH027
078800                            ELSE GO TO STARTOUT                   PATCH027
078900     ELSE MOVE CANDESEQ TO NEXTCANDESEQ                           PATCH027
079000         MOVE CANDETXT TO NEXTCANDETXT                            PATCH027
079100         READ DISKINPUT AT END GO TO FINALIZE                     PATCH027
079200                        ELSE GO TO STARTOUT.                      PATCH027
079300     MOVE INCARD TO NEXTWORKIN.                                   PATCH027
079400     IF CANDESEQ = CRDSEQ                                         PATCH027
079500         READ DISKINPUT AT END GO TO FINALIZE.                    PATCH027
079600     READ CARDINPUT AT END MOVE 1 TO CAE                          PATCH027
079700                    CLOSE CARDINPUT WITH RELEASE.                 PATCH027
079800 STARTOUT.                                                        FILTER
079900     IF ALTDATA = 1 PERFORM CHGDATA.                              FILTER
080000     MOVE 1 TO IMGFLG.                                            FILTER
080100     IF MAR-A NOT = SPACE                                         FILTER
080200     MOVE 8 TO SCANCNT                                            FILTER
080300     ELSE MOVE 12 TO SCANCNT.                                     FILTER
080400     MOVE WORKIMP(SCANCNT) TO CHRCHK.                             FILTER
080500 CHGDATA.                                                         FILTER
080600     EXAMINE WORKIN REPLACING ALL "&" BY "+".                     FILTER
080700     EXAMINE WORKIN REPLACING ALL "#" BY "=".                     FILTER
080800     EXAMINE WORKIN REPLACING ALL "%" BY "(".                     FILTER
080900     EXAMINE WORKIN REPLACING ALL "[" BY ")".                     FILTER
081000     EXAMINE WORKIN REPLACING ALL "@" BY QUOTE.                   FILTER
081100 SCANNER SECTION.                                                 FILTER
081200 NU-WD. MOVE 1 TO CHKFLG.                                         FILTER
081300     PERFORM IMGCHK THRU IMGCHKX.                                 FILTER
081400 NOTE-WD. MOVE 2 TO CHKFLG.                                       FILTER
081500     PERFORM IMGCHK THRU IMGCHKX.                                 FILTER
081600 VAL-WD. MOVE 3 TO CHKFLG.                                        FILTER
081700     PERFORM IMGCHK THRU IMGCHKX.                                 FILTER
081800 PIC-WD. MOVE 4 TO CHKFLG.                                        FILTER
081900     PERFORM IMGCHK THRU IMGCHKX.                                 FILTER
082000 IMGCHK.                                                          FILTER
082100     MOVE SPACE TO WORKWORD.                                      FILTER
082200     MOVE 1 TO WORKCNT.                                           FILTER
082300     MOVE 1 TO FSTMSW.                                            FILTER
082400 IMGCHK-1.                                                        FILTER
082500     IF SCANCNT NOT < 73                                          FILTER
082600         PERFORM NU-IMAGE                                         PATCH031
082700         IF WORKINCON = "$"                                       PATCH031
082800             MOVE 8 TO SCANCNT                                    PATCH031
082900             PERFORM BCL-SWITCH THRU BS-X                         PATCH031
083000             MOVE 73 TO SCANCNT                                   PATCH031
083100             MOVE SPACE TO WORKBUFFER                             PATCH031
083200             MOVE 1 TO WORKCNT                                    PATCH031
083300             GO TO IMGCHK-1.                                      PATCH031
083400     MOVE WORKIMP(SCANCNT) TO CHRCHK.
083500     MOVE [CHRCHK TO CHRCK].                                      FILTER
083600     IF CHRCK = 48 ADD 1 TO SCANCNT                               FILTER
083700         MOVE WORKIMP(SCANCNT) TO CHRCHK GO TO IMGCHK-1.          FILTER
083800     IF CHKFLG = 1 OR 3                                           FILTER
083900         IF CHRCK = 46 OR CHRCK = 58 AND PREN = 0 ADD 1 TO SCANCNTFILTER
084000             MOVE WORKIMP(SCANCNT) TO CHRCHK GO TO IMGCHK-1       FILTER
084100     ELSE                                                         FILTER
084200     IF SCNSW = 1 AND CHKFLG = 1                                  FILTER
084300         IF FSTMSW = 1 MOVE 0 TO FSTMSW                           FILTER
084400             IF CHRCHK < "A" OR CHRCK = 60 OR 32                  FILTER
084500                 IF CHRCK = 63 NEXT SENTENCE                      FILTER
084600     ELSE IF CHRCK = 26 OR 44 OR 16                               FILTER
084700         IF WORKIMP(SCANCNT+1) = SPACE GO TO IMGCKX ELSE          FILTER
084800         IF CHRCK = 16 MOVE 5 TO PRINTFLG-5 GO TO IMGCKX ELSE     FILTER
084900     IF WORKIMP(SCANCNT+1) > "Z" IF CHRCK = 26 GO TO IMGCHK-3     FILTER
085000      ELSE IF WORKIMP(SCANCNT + 1) LESS THAN 9 AND GREATER        PATCH017
085100           THAN 0 NEXT SENTENCE ELSE MOVE 5 TO PRINTFLG-5         PATCH017
085200           ELSE GO TO IMGCKX ELSE                                 PATCH017
085300     IF CHRCK = 43 AND WORKIMP(SCANCNT + 1) = "*" GO TO CON-12    FILTER
085400     ELSE IF CHRCK = 31 PERFORM ENDCP THRU ENDCX                  FILTER
085500     MOVE 1 TO FSTMSW GO TO IMGCHK-1 ELSE GO TO IMGCKX.           FILTER
085600     IF CHRCK = 31 PERFORM ENDCP THRU ENDCX GO TO IMGCHK-1.       FILTER
085700     IF CHKFLG ! 4                                                FILTER
085800     IF (CHRCK = 26 AND WORKIMP(SCANCNT+1) = " ") OR              FILTER
085900     (CHKFLG = 1 AND CHRCK = 32)                                  FILTER
086000     GO TO IMGCKX ELSE GO TO IMGCHK-3.                            FILTER
086100     MOVE CHRCHK TO CHAR(WORKCNT).                                FILTER
086200     ADD 1 TO SCANCNT ADD 1 TO WORKCNT.                           FILTER
086300     MOVE WORKIMP(SCANCNT) TO CHRCHK.                             FILTER
086400     IF (CHRCHK = SPACE OR ";") OR (PRENSW = 1 AND CHRCHK = "(")  PATCH016
086500          GO TO IMGCHKX.                                          PATCH016
086600     GO TO CON-12.                                                FILTER
086700 SCANSW.                                                          FILTER
086800     MOVE [CHRCHK TO CHRCK].                                      FILTER
