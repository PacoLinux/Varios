    OCTFILL(PROCIDS);                                                   A000438
               WRITE (FNOK    ); COMMENT TO ESTABLISH SINGLE SPACING;   A0004385
         CVN~0; PNO~0; VERIFY                                           A000439
         END;                                                           A000440
         BEGIN                                                          A000441
  PROCEDURE P1;                                                         A001001
    COMMENT 2.4.4 LEGITIMATE USE OF COMMENT;                            A001002
    BEGIN                                                               A001003
         BEGIN TEMP [1]~1; BEGIN TEMP[2]~2; TEMP[3]~3 END;              A0010035
            BEGIN END                                                   A001004
    END;                                                                A001005
      BEGIN                                                             A001006
                          COMMENT TEMP [1]~77;                          A001007
        TEMP [4]~4;                                                     A001008
                          COMMENT BEGIN TEMP [2]~77;                    A001009
        BEGIN                                                           A001010
          TEMP [5]~5;                                                   A001011
                          COMMENT (,.|:~()[]"GO TO L END WHILE ELSE     A001012
                                  UNTIL # ARRAY;                        A001013
          TEMP [6]~6                                                    A001014
        END;                                                            A001015
        BEGIN                                                           A001017
                          COMMENT;                                      A001018
        END                                                             A001019
      END;                                                              A001021
      CVN~6; PNO~1; FILL ANS[*] WITH OCT1, OCT2, OCT3, OCT4, OCT5,      A001023
                                       OCT6 ;                           A001024
      VERIFY                                                            A001025
      END;                                                              A0010255
  PROCEDURE P2; COMMENT 2.5 LEGITIMATE IDENTIFIERS;                     A002001
    BEGIN                                                               A002002
      LABEL EXIT, GO0TO0EXIT;                                           A002003
      BOOLEAN B1,NOTT,T,Y,TANDY,B2;                                     A002004
      INTEGER  I,I1,FOR0I1,  I0STEP01UNTIL01DO,                         A002005
      ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789A1B2C3D4E5F6G7H8I9J10K11L12,  A002006
      ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789A1B2C3D4E5F6G7H8I9J10K11L1;   A002007
      GO0TO0EXIT:                                                       A002008
      NOTT~TRUE; T~TRUE; FOR0I1~4;Y~TRUE;I~3;I1~1;TANDY~FALSE;          A002009
      I0STEP01UNTIL01DO~5;                                              A002010
      B1~NOTT;B2~TANDY;                                                 A002011
    ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789A1B2C3D4E5F6G7H8I9J10K11L12~6;  A002012
    ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789A1B2C3D4E5F6G7H8I9J10K11L1~7;   A002013
      FOR0I1~I0STEP01UNTIL01DO;                                         A002014
      EXIT: IF B1 THEN TEMP [1]~1;                                      A002015
            IF NOTT THEN TEMP [2]~2;                                    A002016
            IF T THEN TEMP [3]~3; IF Y THEN TEMP [4]~4;                 A002017
            IF TANDY THEN ELSE TEMP[5]~5; IF B2 THEN ELSE TEMP[6]~6;    A002018
IF ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789A1B2C3D4E5F6G7H8I9J10K11L12 = 6  A002019
      THEN TEMP [7]~7;                                                  A002020
IF  ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789A1B2C3D4E5F6G7H8I9J10K11L1 = 7  A002021
      THEN TEMP [8]~8;                                                  A002022
         IF I=3 THEN TEMP [9]~9; IF I1=1 THEN TEMP [10]~10;             A002023
         IF FOR0I1=5 THEN TEMP [11]~11;                                 A002024
            IF I0STEP01UNTIL01DO =5 THEN TEMP [12]~12;                  A002025
      FILL ANS [*] WITH OCT1,OCT2,OCT3,OCT4,OCT5,OCT6,OCT7,OCT10,OCT11, A002026
                    OCT12,OCT13,OCT14;                                  A002027
      CVN~12; PNO~2;                                                    A002028
      VERIFY                                                            A002029
    END;                                                                A002030
  PROCEDURE P3; COMMENT  THE USE OF NUMERS EXCLUDING SIZE LIMITATIONS.  A003001
                EXTENDED ALGOL MANUAL SEC 2.6. KM. JULY 26 1963;        A003002
    BEGIN                                                               A003003
         COMMENT  UNSIGNED NUMBERS;                                     A003004
         TEMP[1]~9;                                                     A003005
         TEMP[2]~99;                                                    A003006
         TEMP[3]~.5;                                                    A003007
         TEMP[4]~9.5;                                                   A003008
         TEMP[5]~@2;                                                    A003009
         TEMP[6]~@+3;                                                   A003010
         TEMP[7]~@-3;                                                   A003011
         TEMP[8]~2@2;                                                   A003012
         TEMP[9]~2@-2;                                                  A003013
         TEMP[10]~4@+2;                                                 A003014
         TEMP[11]~4.5@-2;                                               A003015
         TEMP[12]~4.5@+2;                                               A003016
         COMMENT  SIGNED NUMBERS;                                       A003017
         TEMP[13]~-9;                                                   A003018
         TEMP[14]~-99;                                                  A003019
         TEMP[15]~-.5;                                                  A003020
         TEMP[16]~+.5;                                                  A003021
         TEMP[17]~-9.5;                                                 A003022
         TEMP[18]~+9.5;                                                 A003023
         TEMP[19]~-@2;                                                  A003024
         TEMP[20]~-@+3;                                                 A003025
         TEMP[21]~-@1;                                                  A003026
         TEMP[22]~-2@2;                                                 A003027
         TEMP[23]~-2@-2;                                                A003028
         TEMP[24]~+4@-2;                                                A003029
         TEMP[25]~+4.5@-2;                                              A003030
         TEMP[26]~-2.5@+2;                                              A003031
         COMMENT EFFECT OF ZERO ON SELECTED VALUES;                     A003032
         TEMP[27]~-.0;                                                  A003033
         TEMP[28]~-@0;                                                  A003034
         TEMP[29]~-2.5@-0;                                              A003035
         TEMP[30]~+2.5@-0;                                              A003036
         TEMP[31]~-0@+10;                                               A003037
         TEMP[32]~-0@+0;                                                A003038
         TEMP[33]~-.0@-0;                                               A003039
         TEMP[34]~-.5@+0;                                               A003040
         TEMP[35]~4@-0;                                                 A003041
         TEMP[36]~0@+4;                                                 A003042
         TEMP[37]~0@2;                                                  A003043
         FILL ANS[*] WITH  9, 99, OCT1010000000000004,                  A003044
                     OCT1010000000000114, 100, 1000,                    A003045
                     OCT1204061115645707, 200, OCT1161217270243656,     A003046
                     400, OCT1162702436560510, 450,                     A003047
                     OCT2000000000000011,  OCT2000000000000143,         A003048
                     OCT3010000000000004,  OCT1010000000000004,         A003049
                     OCT3010000000000114,  OCT1010000000000114,         A003050
                     OCT3000000000000144, OCT3000000000001750,          A003051
                     OCT3131200000000000, OCT3123100000000000,          A003052
                     OCT3161217270243656, OCT1162436560507534,          A003053
                     OCT1162702436560510, OCT3123720000000000,          A003054
                     OCT1000000000000000, OCT3141000000000000,          A003055
                     OCT3142400000000000, OCT1142400000000000,          A003056
                     OCT1000000000000000,  OCT1000000000000000,         A003057
                     OCT1000000000000000, OCT3154000000000000,          A003058
                     OCT1144000000000000, 0, 0;                         A003059
         CVN ~ 37;                                                      A003060
         PNO ~ 3;                                                       A003061
         VERIFY                                                         A003062
    END;                                                                A003063
  PROCEDURE P4; COMMENT 2.7 STRINGS;                                    A004001
    BEGIN                                                               A004002
         TEMP[1]~""";                                                   A004003
         TEMP[2]~" ";                                                   A004004
         TEMP[3]~"ABCDEF";                                              A004005
         TEMP[4]~"A C E ";                                              A004006
         TEMP[5]~"A=J~I$";                                              A004007
         TEMP[6]~"[>-<89";                                              A004008
         TEMP[7]~"SQRT Y";                                              A004009
         FILL ANS [*] WITH OCT77, OCT60, OCT212223242526,               A004010
              OCT216023602560, OCT217541373152, OCT331654361011,        A004011
              OCT625051636070;                                          A004012
         CVN~7; PNO~4;VERIFY                                            A004013
    END;                                                                A004014
  PROCEDURE P5;COMMENT 3.1 VARIABLES;                                   A005001
    BEGIN                                                               A005002
         COMMENT SIMPLE VARIABLES ;                                     A005003
         INTEGER ALGOL, BAGOL, SVE , AC,I;                              A005004
         COMMENT SUBSCRIPTED VARIABLES;                                 A005006
         ARRAY SUBSC[0:5], TWO[0:1,0:1],TOW[0:1,0:1],VAR[0:5];          A005007
         ALGOL~123; BAGOL~ALGOL;   SVE~ "ASSBLY"; AC~ "SAVE";           A0050075
         FOR I ~ 0 STEP 1 UNTIL 5 DO                                    A005008
              BEGIN  SUBSC[I] ~ I END;                                  A005009
         FOR I ~ 1 STEP 1 UNTIL 5 DO VAR[I] ~ SUBSC[I];                 A005010
         I ~ 6;                                                         A005011
         VAR[I-6] ~ ALGOL;TWO[1,1] ~ 220;TOW[1,1] ~ TWO[I-5,I-5];       A005012
         TEMP[1] ~ ALGOL;      TEMP[2] ~ BAGOL;      TEMP[3] ~ SVE ;    A005013
         TEMP[4] ~ AC;     TEMP[5] ~ VAR[1];     TEMP[6] ~ VAR[2];      A005014
         TEMP[7] ~ VAR[3];     TEMP[8] ~ VAR[4];     TEMP[9] ~ VAR[5];  A005015
         TEMP[10] ~ VAR[0];     TEMP[11] ~ TOW[1,1];                    A005016
         FILL ANS [*] WITH OCT173, OCT173, OCT216262224370,OCT62216525, A005017
                          OCT1, OCT2, OCT3, OCT4, OCT5, OCT173,OCT334;  A005018
         PNO~5; CVN~11; VERIFY                                          A005019
    END;                                                                A005020
  PROCEDURE P6; COMMENT 3.2 PARTIAL WORD DESIGNATORS;                   A006001
    BEGIN                                                               A006002
         INTEGER I,VAR;                                                 A006003
         ARRAY VARAY[1:2,1:5];                                          A006004
         COMMENT VARIABLES;                                             A006005
         I ~ 79      ;                                                  A0060055
         VAR ~ 549755813887;                                            A006006
         TEMP[1]~VAR.[1:47];                                            A006007
         VAR ~ 134;                                                     A006008
         TEMP[2]~ VAR.[40:8];                                           A006009
         TEMP[3]~ VAR.[40:6];                                           A006010
         VARAY[1,1]~ 1962;                                              A006011
         TEMP[4]~ VARAY[1,1].[45:2];                                    A006012
         VARAY[1,1].[43:1] ~ 2;                                         A006013
         TEMP[5] ~ VARAY [1,1];                                         A006014
         VAR ~113;                                                      A006015
         TEMP[6] ~ (VAR + 7).[39:6];                                    A006016
         VAR~4316;                                                      A006017
         TEMP[7] ~ VAR.[43:2] + I.[39:9];                               A006018
         FILL ANS[*] WITH OCT0007777777777777, OCT206, OCT041, OCT1,    A006019
                     OCT3652, OCT17, OCT122;                            A006020
         CVN~7; PNO~6;   VERIFY                                         A006021
    END;                                                                A006022
  PROCEDURE P7; COMMENT 3.3 FUNCTION DESIGNATORS;                       A007001
    BEGIN                                                               A007002
         INTEGER I,J,L,K;                                               A007003
         ARRAY A[1:1];                                                  A007004
         INTEGER PROCEDURE NOFORMALPARAMETERS;                          A007005
         NOFORMALPARAMETERS~1;                                          A007006
         REAL PROCEDURE ARITH (VAR1,VAR2,VAR3);                         A007007
                   VALUE VAR1,VAR2,VAR3;                                A007009
                   INTEGER VAR1,VAR2,VAR3;                              A007010
              BEGIN                                                     A0070105
                   TEMP[2]~ VAR1;                                       A007011
                   TEMP[4]~ VAR3;                                       A007013
                   TEMP[3]~ VAR2;                                       A007012
                   ARITH ~ VAR1 + VAR2 + VAR3 + TEMP[1]                 A007014
              END;                                                      A007015
         REAL PROCEDURE ARAY(A);                                        A007016
              REAL A ;                                                  A007017
              BEGIN                                                     A007018
                   A    ~6; ARAY ~ 4;                                   A007019
              END;                                                      A007020
         BOOLEAN PROCEDURE BOOL(B);                                     A007021
              VALUE B;  BOOLEAN B ;                                     A007022
              BEGIN                                                     A0070225
                   BOOL ~ B                                             A007023
              END;                                                      A007024
         REAL PROCEDURE STRNG(ONE,TWO);                                 A007025
              VALUE ONE,TWO;REAL ONE,TWO;                               A007026
              BEGIN                                                     A0070265
              STRNG ~ ONE + TWO;                                        A007027
              END ;                                                     A0070275
         I~1; J~15; K~14; L~3;                                          A0070276
         TEMP[1] ~ NOFORMALPARAMETERS;                                  A007028
         TEMP[5] ~ ARITH(I+J,J | L,L + 56);  TEMP[7] ~ 6;               A007029
         TEMP[6] ~  ARAY( TEMP[7]);                                     A007030
         TEMP[8] ~ REAL (TEMP[3]=3);                                    A007031
         TEMP[9] ~ STRNG(K)"STRING"(J);                                 A007032
         FILL ANS[*] WITH OCT1, OCT20, OCT55, OCT73, OCT171, OCT4,      A007033
                             OCT6, OCT0, OCT35;                         A007034
         CVN~ 9; PNO ~7;VERIFY                                          A007035
    END;                                                                A007036
  PROCEDURE P9; COMMENT TRANSFER FUNCTIONS 3.3.5.  KM. JUNE 10 63;      A009001
    BEGIN                                                               A009002
         REAL X,Y,Z,C;                                                  A009003
         INTEGER H;                                                     A009004
         BOOLEAN  A,B,D;                                                A009005
         REAL PROCEDURE WHICHWASFORWARD(Q,R); REAL Q, R;  FORWARD;      A009006
         REAL PROCEDURE CHECKTRANSFER(XX, YY);                          A009007
              REAL XX, YY;                                              A009008
              BEGIN                                                     A009009
                   CHECKTRANSFER~XX DIV YY;                             A009010
              END;                                                      A009011
         BOOLEAN PROCEDURE  CHECKREAL( F,G);                            A009012
              BOOLEAN F, G;                                             A009013
              BEGIN                                                     A009014
                   CHECKREAL ~ F AND G ;                                A009015
              END;                                                      A009016
         REAL PROCEDURE WHICHWASFORWARD(Q,R);                           A0090161
              REAL  Q,R;                                                A0090162
              BEGIN                                                     A0090163
                   WHICHWASFORWARD ~ Q+R;                               A0090164
              END;                                                      A0090165
         TEMP[1]~5; TEMP[2]~6;                                          A009017
         X~3.1; Y~2; H~7; Z~-3.3;  A~TRUE; B ~ FALSE;                   A009018
              COMMENT  CHECKOUT OF ENTIER  TRANSFER  FUNCTION;          A009019
         TEMP[3]~ ENTIER (X MOD Y);                                     A009020
         TEMP[4]~ENTIER( CHECKTRANSFER(X,Y) + H.[46:1]);                A009021
         TEMP[5]~ENTIER(-2.999999999);                                  A009022
         TEMP[6]~ENTIER(X);                                             A009023
              TEMP[7]~ENTIER(-Z);                                       A009024
         TEMP[8]~ENTIER(IF A THEN CHECKTRANSFER(5.7,-2.1) ELSE          A009025
                            "AB"+"A");                                  A009026
         TEMP[9]~ENTIER( IF A THEN REAL(A) ELSE REAL(B));               A009027
         TEMP[10]~ENTIER( IF A THEN WHICHWASFORWARD (2.1,3.2) ELSE      A009028
                             2.7);                                      A009029
         TEMP[11]~ENTIER ((IF BOOLEAN(0) THEN REAL(A) ELSE IF A         A009030
                                  THEN( IF BOOLEAN(H.[46:1])  THEN      A009031
                      REAL(BOOLEAN(IF A THEN 1 ELSE 2.4))  ELSE 5.4)    A009032
                        ELSE 1 )  +2.3);                                A009033
         COMMENT    CHECKOUT OF  REAL  TRANSFER  FUNCTION;              A009034
         C~REAL(A EQV A ); C~C.[47:1];TEMP[12]~ENTIER(C);               A009035
         C~REAL(CHECKREAL(TRUE,BOOLEAN(0)) OR BOOLEAN(H.[47:1]));       A009036
         TEMP[13] ~ ENTIER (C);                                         A009037
         C ~ REAL (B);  TEMP[14] ~ ENTIER(C);                           A009038
         C ~ REAL( IF A THEN A OR B ELSE A AND B); TEMP[15]~ENTIER(C);  A009039
         C~REAL (IF A THEN BOOLEAN(H.[47:1]) ELSE BOOLEAN(H.[42:1]));   A009040
         TEMP[16]~ENTIER(C);                                            A009041
         C ~ REAL( IF B THEN BOOLEAN(IF A THEN 2.5 ELSE X) ELSE         A009042
              ( IF A THEN BOOLEAN(REAL(A)) ELSE B ) );                  A009043
         TEMP[17]~ENTIER(C);                                            A009044
                        COMMENT CHECK OUT OF BOOLEAN TRANSFER FUNC;     A009045
         D ~ BOOLEAN( Y DIV X);   TEMP[18]~ENTIER(REAL(D));             A009046
         D ~ BOOLEAN( IF A THEN CHECKTRANSFER(6, 2) ELSE "A");          A009047
         TEMP[19]~ENTIER(REAL(D));                                      A009048
         D ~ BOOLEAN (IF B OR B THEN   TEMP[1]+TEMP[2] ELSE             A009049
                       REAL(BOOLEAN("A")));                             A009050
         TEMP[20]~ENTIER(REAL(D));                                      A009051
                           COMMENT GENERAL USE OF TRANSFER FUNCTIONS;   A009052
       TEMP[21]~ENTIER(REAL(BOOLEAN(REAL(BOOLEAN(REAL(BOOLEAN(5))))))); A009053
     TEMP[22]~ENTIER( (IF A THEN REAL(BOOLEAN(H)) ELSE  REAL(BOOLEAN    A009054
              (-5))) + ENTIER(REAL(BOOLEAN(H))));                       A009055
         FILL ANS[*] WITH OCT5,  OCT6,      OCT1,  OCT2,                A009063
                        OCT2000000000000003, OCT3, OCT3,                A009064
             OCT2000000000000002, OCT1, OCT5, OCT3, OCT1, OCT1, OCT0,   A009065
                  OCT1, OCT1, OCT1, OCT0, OCT3, OCT21, OCT5,            A009066
              OCT16;                                                    A009067
         CVN~22;                                                        A009068
         PNO~9;                                                         A009069
         VERIFY                                                         A009070
    END;                                                                A009071
  PROCEDURE P10; COMMENT  GEN. ARITHMETIC EXPRESSION, IF CLAUSE.        A010001
                 3.4.3.2 DEC.11. KM. ;                                  A010002
    BEGIN                                                               A010003
         BOOLEAN A,B;                                                   A010004
         INTEGER X,Y  ;                                                 A010005
         REAL      P,Q  ;                                               A010006
         REAL PROCEDURE GENARITH (X,Y,A);                               A010007
         REAL   X,Y;  BOOLEAN A;                                        A010008
         BEGIN                                                          A010009
              GENARITH~ IF A THEN X/Y ELSE X MOD Y;                     A010010
     END     ;                                                          A010011
         INTEGER PROCEDURE GENERAL (X,Y,A);                             A010012
         INTEGER   X,Y;                                                 A010013
         BOOLEAN   A;                                                   A010014
         BEGIN                                                          A010015
         GENERAL ~ IF A THEN X.[46:2] ELSE (X+Y).[46:2]                 A010016
         END;                                                           A010017
         A~ TRUE; B~ FALSE;  X~4; Y~66;   P~ 7/2; Q~ 7/4;               A010018
         TEMP[1] ~ 5; TEMP[2] ~ 7;                                      A010019
         TEMP[3]~ IF B THEN 7/5 ELSE (GENARITH(7,5,TRUE) +              A010020
                   GENERAL(5,7,FALSE)).[44:4];                          A010021
         TEMP[4]~ IF A THEN "ABC" + "ABC" ELSE  X.[47:1]+3 ;            A010022
         TEMP[5]~ IF A THEN - GENERAL(7,5,TRUE) ELSE - (P MOD Q MOD P); A010023
         TEMP[6]~ IF A AND B THEN  -"A" ELSE - "ABC" - "CDE";           A010024
         TEMP[7] ~ IF B THEN Q ELSE -(GENARITH(10,5,FALSE).[45:2]);     A010025
         TEMP[8]~IF NOT A THEN (GENERAL(2,4,TRUE) MOD X.[45:3]) ELSE    A010026
                           ("AB" MOD "A") + "B";                        A010027
         TEMP[9] ~ IF B THEN P+Q ELSE (4 DIV 4) + X.[45:1] ;            A010028
         TEMP[10]~ IF B THEN X ELSE IF B THEN X ELSE IF B THEN X ELSE Y;A010029
         TEMP[11]~IF B THEN GENERAL(X,Y,A) ELSE IF B THEN               A010030
                   GENERAL (4,1,TRUE) MOD GENARITH (4,1,TRUE) ELSE      A010031
         IF A THEN GENARITH("ABC" , "ABC" , (A AND B)) ELSE             A010032
                   0;                                                   A010033
         TEMP[12]~ IF A THEN TEMP[1] MOD TEMP[2] ELSE TEMP[1] | 5;      A010034
         FILL ANS[*] WITH OCT5, OCT7, OCT6, OCT424446,                  A010035
                        OCT2000000000000003, OCT2000000000444650,OCT0,  A010036
                        OCT23, OCT2, OCT102, OCT0, OCT5;                A010037
         CVN~ 12;                                                       A010039
         PNO~ 10;                                                       A010040
         VERIFY                                                         A010041
    END;                                                                A010042
  PROCEDURE P11;                                                        A011001
      COMMENT 3.4.4.1,+ LEGITIMATE USE OF ";                            A011002
    BEGIN                                                               A011003
      REAL PROCEDURE FUNC; FUNC~4;                                      A011004
      TEMP [1]~+5;                                                      A011005
      TEMP [2]~+(+(+(+(+(+(+3))))));                                    A011006
      TEMP [3]~+5+(+3);                                                 A011007
      TEMP [4]~+""";                                                    A011008
      TEMP [5]~+TEMP[1];                                                A011009
      TEMP [6]~+TEMP[2].[40:7];                                         A011010
      TEMP [7]~+FUNC;                                                   A011011
      TEMP [8]~+6+"13"+TEMP[7]+TEMP[3].[44:1]+FUNC;                     A011012
      TEMP[9]~+"+3"+"-6"   ;                                            A011013
      FILL ANS[*] WITH OCT5, OCT3, OCT10, OCT77, OCT5, OCT1,OCT4,       A011014
             OCT122, OCT7411 ;                                          A011015
      CVN~ 9; PNO~11;                                                   A011016
      VERIFY                                                            A011017
    END;                                                                A011018
  PROCEDURE P12; COMMENT THE ARITHMETIC OPERATOR  (-);                  A012001
  BEGIN                                                                 A012002
