                   SI ~ F3;                                             A112018
                   DI ~ F4 ;                                            A112019
                   8(DI~DI+V1;SI~SI+V1);                                A122020
                   2 (DS~1 WDS; DS~8 CHR; DS~8 OCT; DS~8 DEC;           A112021
                     GO TO L1; DS~ 1 WDS; L1: DS ~8 ADD; DS ~8 SUB;     A112022
                      DS ~ 8 ZON; DS~ 8 NUM) ;                          A112023
                   GO TO L2;                                            A112024
                   TALLY ~ TALLY +1;                                    A112025
                   L2:  3 (DS ~2 LIT "E"; DS ~ 6 SET; DS ~ 6 RESET;     A112026
                   GO TO L3; JUMP OUT; L3: SKIP 6 DB; SKIP 6 SB;        A112027
                   DS ~ 3 LIT "EK");                                    A112028
                   TALLY ~ 0;                                           A112029
                   DI ~ F5;                                             A112030
                   2 (TALLY ~ TALLY+1;2 (TALLY ~ TALLY+1; 2 (TALLY ~    A112031
                   TALLY+1; JUMP OUT);JUMP OUT; TALLY ~ TALLY+1);TALLY  A112032
                   ~ TALLY +1)  ;                                       A112033
