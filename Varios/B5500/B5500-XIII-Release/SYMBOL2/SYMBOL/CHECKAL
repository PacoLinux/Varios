  PROCEDURE P113;  COMMENT  JUMPOUT STATEMENT  EK  10 DEC  62;          A113001
    BEGIN                                                               A113002
         INTEGER A,B,C,D,E;                                             A113003
         STREAM PROCEDURE STPRJ (F1,F2,F3,F4,F5);                       A113005
              BEGIN                                                     A113006
                   LABEL L1,L2,L3;                                      A113007
                   DI ~ F1;                                             A113008
                   TALLY ~ 0;                                           A113009
                   4 ( TALLY ~ TALLY +1; JUMP OUT)  ;                   A113010
                   F1 ~ TALLY;                                          A113011
                   SI ~ LOC F1;                                         A113012
                   DS ~ 1 WDS;                                          A113013
                   TALLY ~ 0;                                           A113014
                   DI ~ F2;                                             A113015
                        4(8(TALLY ~ TALLY+1; JUMP OUT); TALLY~TALLY+1;  A113016
                   GO TO L1; TALLY ~ TALLY+1; L1: JUMP OUT) ;           A113017
                   TALLY ~ TALLY+1;                                     A113018
                   F2 ~ TALLY;                                          A113019
                   SI ~ LOC F2;                                         A113020
                   DS ~ 1 WDS;                                          A113021
                   DI ~ F3;                                             A113022
                   TALLY ~ 0;                                           A113023
                   8(8(TALLY~TALLY+1; JUMP OUT); 8(TALLY~TALLY+1;       A113024
         JUMP OUT); JUMP OUT TO L2; TALLY~TALLY+1;JUMP OUT);            A113025
                   TALLY ~ TALLY+1;                                     A113026
                   L2:  F3 ~ TALLY;                                     A113027
                   SI ~ LOC F3;                                         A113028
                   DS ~ 1 WDS;                                          A113029
                   DI ~ F4;                                             A113030
                   TALLY ~ 0;                                           A113031
         3(GO TO L3; 3(TALLY~TALLY+1); L3: JUMP OUT; TALLY~TALLY+1);    A113032
                   TALLY~TALLY+1;                                       A113034
                   F4 ~ TALLY;                                          A113035
         SI~LOC F4;                                                     A113036
                   DS ~ 1 WDS;                                          A113037
                   DI ~ F5;                                             A113038
                   TALLY ~ 0;                                           A113039
                   2(TALLY~TALLY+1; 3(TALLY~TALLY+1; 4 (TALLY ~         A113040
         TALLY+1; JUMP OUT); TALLY~TALLY+1; JUMP OUT); JUMP OUT;        A113041
                   TALLY ~ TALLY+1) ;                                   A113042
                   F5 ~ TALLY;                                          A113043
                   SI ~ LOC F5;                                         A113044
                   DS ~ 1 WDS;                                          A113045
         END;                                                           A113046
         STPRJ (A,B,C,D,E);                                             A113047
         TEMP[1] ~ A;                                                   A113048
         TEMP[2] ~ B;                                                   A113049
         TEMP[3] ~ C;                                                   A113050
         TEMP[4] ~ D;                                                   A113051
         TEMP[5] ~ E;                                                   A113052
         FILL ANS[*] WITH OCT1, OCT3, OCT2, OCT1, OCT4;                 A113053
         CVN ~ 5;                                                       A113054
         PNO ~ 113;                                                     A113055
         VERIFY                                                         A113056
    END;                                                                A113057
  PROCEDURE P115; COMMENT 5.12.12 COMPOUND STREAM STATEMENT 8/5/63 LG;  A115001
    BEGIN                                                               A115002
         INTEGER ARRAY STRM [0:20];                                     A115003
         INTEGER I,A,B,C,D;                                             A115004
                                                                        A115005
      STREAM PROCEDURE CSS (DEST,SRCE,A,B,C,D);                         A115006
              VALUE A,B,C,D;                                            A1150061
         BEGIN                                                          A115007
              LOCAL T14,T15,T16,T17,T1;                                 A115008
              LABEL L1,L2,L3,L4,L5,L6,L7,LA,LB;                         A115009
              SI ~ SRCE ; DI ~ DEST ;                                   A115010
              BEGIN COMMENT DUMMY STATEMENT DS~ 1 WDS END;END;          A115011
              BEGIN END;                                                A115012
              BEGIN                                                     A115013
               BEGIN                                                    A115014
                BEGIN SI ~ SI+8; DS ~ 1 WDS END                         A115015
               END                                                      A115016
              END;                                                      A115017
                    DS ~ 1 WDS;                                         A115018
              BEGIN DS ~ 1 WDS END;                                     A115019
              BEGIN DS ~ 1 WDS END;                                     A115020
                    GO TO L1;                                           A115021
              BEGIN DS ~ 1 WDS; L1: DS ~ 1 WDS; DS ~ 1 WDS END;         A115022
              BEGIN DS ~ 1 WDS; GO TO L2; DS ~ 1 WDS END;               A115023
          L2:       DS ~ 1 WDS;                                         A115024
              BEGIN                                                     A115025
                    GO TO LA;                                           A115027
                    DS ~ 1 WDS ;                                        A115028
               LA:  DS ~ 1 WDS ;                                        A115029
                    GO TO LB;                                           A115030
                    DS ~ 1 WDS;                                         A115031
               LB:  DS ~ 1 WDS;                                         A115032
                    GO TO L3  ; DS ~ 1 WDS                              A115033
              END;                                                      A115034
                    DS ~ 1 WDS; DS ~ 1 WDS;                             A115035
          L4: BEGIN DS ~ 1 WDS;                                         A115036
                    DS ~ 1 WDS; GO TO L5;                               A115037
                    DS ~ 1 WDS                                          A115038
              END; BEGIN DS ~ 1 WDS;                                    A115039
                    L3:  DS ~ 1 WDS;                                    A115040
                         GO TO L4; DS ~ 1 WDS                           A115041
                   END;                                                 A115042
          L5:       T14 ~ DI; DI ~ DI + 8;                              A115043
                    T15 ~ DI; DI ~ DI + 8;                              A115044
                    T16 ~ DI; DI ~ DI + 8;                              A115045
                    T17 ~ DI;                                           A115046
                    TALLY ~ A ; T1 ~ TALLY; SI ~ LOC T1; DI ~ LOC A;    A115047
          L6:       IF 8 SC= DC THEN                                    A115048
                   BEGIN SI ~ LOC A;                                    A115049
                         DI ~ T14  ;                                    A115050
                         DS ~ 1 WDS                                     A115051
                   END                                                  A115052
                                 ELSE                                   A115053
                   BEGIN SI ~ LOC B;                                    A115054
                         DI ~ T15  ;                                    A115055
                         DS ~ 1 WDS                                     A115056
                   END                     ;                            A115057
                    IF TOGGLE THEN                                      A115058
                   BEGIN SI ~ LOC C;                                    A115059
                         DI ~ T16  ;                                    A115060
                         DS ~ 1 WDS;                                    A115061
                         SI ~ LOC A;                                    A115062
                         DI ~ LOC B;                                    A115063
                         GO TO L6                                       A115064
                   END                                                  A115065
                              ELSE                                      A115067
                   BEGIN SI ~ LOC D ;                                   A115068
                         DI ~ T17;                                      A115069
                         DS ~ 1 WDS                                     A115070
                   END                    ;                             A115071
         END STREAMPROCEDURE CSS ;                                      A115072
         COMMENT START OF EXECUTE OF P115;                              A115101
         A ~ 14; B ~ 15; C ~ 16; D ~ 17;                                A115102
         FOR I ~ 0 STEP 1 UNTIL 20 DO STRM [I] ~ I ;                    A115103
         CSS (TEMP,STRM,A,B,C,D);                                       A115104
         FOR I ~ 1 STEP 1 UNTIL 19 DO ANS[I] ~ STRM[I];                 A115105
         CVN ~ 17;                                                      A115106
         PNO ~ 115;                                                     A115107
         VERIFY ;                                                       A115108
    END P115 ;                                                          A115109
  PROCEDURE P116; COMMENT 5.12.13.4 SOURCE COMPARE WITH LITERAL;        A116001
    BEGIN                                                               A116002
         STREAM PROCEDURE  CHECKSORCELIT(INARAY);                       A116003
              BEGIN                                                     A116004
              LOCAL  P, Q;                                              A116005
         LABEL A, B, C, D, E, F, G, H, NED;                             A116055
                   DI~ LOC P;  DS~ 8 LIT "19AZ*  }";                    A116006
                   DI~ LOC Q;  DS~ 7 LIT "0. ()/+"; DS~1 LIT """;       A116007
                   DI~ INARAY; DI~DI+7;                                 A116008
                   SI~ LOC P;                                           A116009
                   IF SC = "1" THEN BEGIN DS~1 LIT "1"; DI~DI+7;        A116010
                   SI~SI+2  END; IF TOGGLE THEN GO TO A;  GO TO NED;    A116011
              A:   IF SC > "+" THEN BEGIN DS~ 1 LIT "2"; DI~DI+7;       A116012
                   SI~SI+2  END;  IF TOGGLE THEN GO TO B; GO TO NED;    A116013
              B:   IF SC { "*" THEN BEGIN DS~ 1 LIT "3"; DI~DI+7;       A116014
                   SI~SI+1  END;  IF TOGGLE THEN GO TO C; GO TO NED;    A116015
              C:   IF SC { " " THEN BEGIN DS~ 1 LIT "4"; DI~DI+7;       A116016
                   SI~SI+1 END; IF TOGGLE THEN GO TO D; GO TO NED;      A116017
              D:   IF SC= " " THEN BEGIN DS~ 1 LIT "5"; DI~DI+7;END;    A116018
                   SI~LOC Q;  IF TOGGLE THEN GO TO E;  GO TO NED;       A116019
              E:   IF SC! "~" THEN BEGIN DS~ 1 LIT "6"; DI~DI+7;        A116020
                   SI~SI+1 END; IF TOGGLE THEN GO TO F; GO TO NED;      A116021
              F:   IF SC="." THEN BEGIN DS~1 LIT "7"; DI~DI+7;          A116022
                   SI~SI+6 END; IF TOGGLE THEN GO TO G; GO TO NED;      A116023
              G:   IF SC = """ THEN BEGIN DS~1 LIT "7";DI~DI+7 END;     A116024
                   IF TOGGLE THEN GO TO H; GO TO NED;                   A116025
              H:   IF SC= """ THEN 2(IF SC= """ THEN 2(IF SC>"0" THEN   A116026
                                  JUMP OUT 2 TO NED); IF TOGGLE THEN    A116027
                                  JUMP OUT TO NED; DS~1 LIT "1";        A116028
                                  DI~DI+7);                             A116029
              NED:   END;                                               A116030
         CHECKSORCELIT(TEMP[1]);                                        A116031
         FILL ANS[*] WITH  1, 2, 3, 4, 5, 6, 7, 7, 1;                   A116032
         CVN~9;                                                         A116033
         PNO~116;                                                       A116034
         VERIFY                                                         A116035
    END;                                                                A116036
  PROCEDURE P117; COMMENT STREAM SOURCE AND DESTINATION COMPARE.        A117001
                  5.12.13.5     JUNE 24 1963;                           A117002
    BEGIN                                                               A117003
         LABEL DAB;                                                     A1170035
              INTEGER  I;                                               A117004
         STREAM PROCEDURE SORCEDESTINCOMPARE(ARAYIN1, ARAYIN25, SIXTY4, A117005
                                             ARAYIN15, ARAYINEND);      A117006
              VALUE    SIXTY4;                                          A117007
              BEGIN                                                     A117008
                   LOCAL  Q, R;                                         A117009
              LABEL A, B, C, D, E, BAD;                                 A117010
                   DI~ARAYIN1 ; DS~20 LIT "ABCD4444IJKLMNOPQRST";       A117011
                                 DS~20 LIT "*9/+-0..}{          ";      A117012
                                 DS~20 LIT "1234567891          ";      A117013
                                 DS~20 LIT "                    ";      A117014
                                 DS~20 LIT "                    ";      A117015
                                 DS~20 LIT "********************";      A117016
                                 DS~20 LIT "ABCDEFGHIJKLMNOPQRST";      A117017
                   DI~LOC R;SI~LOC ARAYINEND; DS~1 WDS; SI~ARAYIN1;     A117018
                   DI~LOC Q;  DS~8 LIT "ABCD4444"; DI~LOC Q;            A117019
                   IF 8 SC=DC  THEN BEGIN DI~R; SI~SI-8; DS~8 CHR ;     A117020
                        SI~SI-8; R~DI END;  IF TOGGLE THEN GO TO A;     A117021
                        GO TO BAD;                                      A117022
              A:   DI~LOC Q; DS~8 LIT "AACD4444";  DI~LOC Q;            A117023
                   IF 4 SC!DC THEN BEGIN TALLY~0; DI~R; DS~4 OCT;       A117024
                        R~DI END; IF TOGGLE THEN GO TO B; GO TO BAD;    A117025
              B:   DI~LOC Q; DS~8 LIT "*9/:-0.."; DI~LOC Q; SI~SI+12;   A117026
                   IF 4 SC > DC THEN BEGIN IF 4 SC } DC THEN BEGIN      A117027
                        DI~R; DS~8 CHR ; R~DI END END; IF TOGGLE        A117028
                        THEN GO TO C; GO TO BAD;                        A1170285
              C:   SI~SI+4; DI~LOC Q; DS~8 LIT "ABCD5678"; DI~LOC Q;    A117029
                   IF 4 SC< DC THEN GO TO D; IF TOGGLE THEN GO TO       A117030
                   BAD;                                                 A117031
              D:   IF 4 SC { DC THEN BEGIN DI~R; DS~2 OCT; R~DI END;    A117032
                   IF TOGGLE THEN GO TO E;  GO TO BAD;                  A117033
              E:   DI~ARAYIN15;  SI~SI+10;                              A117034
                   IF SIXTY4  SC > DC  THEN GO TO BAD;  IF TOGGLE THEN  A117035
                   GO TO BAD; DI~R; DS~8 CHR ; R~DI;                    A117036
                   DI~LOC Q; DS~8 LIT "Q"; DI~LOC Q;                    A117037
                   IF SC = DC THEN GO TO BAD; IF TOGGLE THEN GO TO BAD; A117038
                   DI~R; DI~DI+4; DS~4 CHR ;   R~DI;                    A117039
                   DI~LOC Q;                                            A117040
                   IF 0 SC = DC THEN BEGIN DI~R; DI~DI+1; DS~3 CHR;     A117041
                   END; IF TOGGLE THEN GO TO BAD; DI~R;                 A117042
                   DI~DI-8; DS~3 LIT"A";                                A1170425
              BAD: END;                                                 A117043
         SORCEDESTINCOMPARE(TEMP[1], TEMP[25],63,TEMP[15], TEMP[44]);   A117044
    DAB: FOR I~44 STEP 1 UNTIL 50 DO  TEMP[I-43]~TEMP[I];               A117045
         FILL ANS [*]WITH OCT2122232404040404, OCT10534,                A117046
                          OCT1757606060606060, OCT133,                  A117047
                        OCT2425262730314142, OCT44454647,               A117048
                          OCT0050516200000000;                          A1170485
         CVN~7;                                                         A117049
         PNO~117;                                                       A117050
         VERIFY                                                         A117051
    END;                                                                A117052
  PROCEDURE P118; COMMENT STREAM PROCEDURE SOURCE BIT 5.12.13.6 KM.;    A118001
    BEGIN                                                               A118002
         STREAM PROCEDURE SBCHECK(UJK);                                 A118003
              BEGIN                                                     A118004
         LOCAL Q, R, F, E, D, C, B, A;                                  A1180045
         LABEL  L1, L2, L3;                                             A1180046
                   DI ~ LOC R; DS ~ 8 LIT "AABBCCDH"; TALLY ~ 0;        A118006
                   DI ~ LOC Q; DS ~ 8 LIT    "ABCDEFGH";                A118007
                   SI ~ LOC Q;                                          A118008
                   COMMENT  SB IS 0;                                    A118009
                   IF SB THEN BEGIN A~TALLY; GO TO  L1 END;             A118010
                   TALLY~2; A~ TALLY;                                   A118011
              L1:  SKIP 5 SB;  DI~LOC R;  TALLY~0;                      A118012
                   COMMENT  SB IS 1;                                    A118013
                   IF SB THEN BEGIN TALLY~TALLY+3; B~TALLY; GO TO L2    A118014
                   END; IF TOGGLE THEN BEGIN DS~3 CHR;TALLY~TALLY+2 END;A118015
              L2:  SKIP 2 SB;   TALLY ~ 0;                              A118016
                   COMMENT  SB IS 1;                                    A118017
                   IF  SB  THEN  BEGIN  IF 3 SC=DC THEN BEGIN C~TALLY   A118018
                   END; IF SC= "F" THEN TALLY~TALLY+4 END; TALLY~TALLY  A118019
                   +1;  C~TALLY;                                        A118020
                   TALLY ~ 0;                                           A118021
         SKIP 6 SB;                                                     A1180215
                   COMMENT SB IS 0;                                     A118022
                   IF SB THEN BEGIN  D~TALLY; GO TO L3 END;             A118023
                   BEGIN SKIP 2 SB;  TALLY~TALLY+6; D~TALLY END;        A118024
              L3: SKIP 1 SB; TALLY~0;                                   A118026
                   COMMENT SB IS 1;                                     A118027
                   IF SB THEN BEGIN 8(TALLY ~ TALLY+7; IF SB THEN       A118028
                   JUMP OUT) END;  E~TALLY;                             A118029
                   TALLY~ 0;                                            A118030
                   COMMENT  SB IS 1;                                    A118031
                   IF SB THEN BEGIN IF SC ="H" THEN BEGIN SI~LOC Q      A118032
                   ; DI~LOC R; IF 1 SC = DC THEN TALLY~TALLY+8 END      A118033
                   END; TALLY~TALLY+1; F ~ TALLY;                       A118034
               SI~LOC A; DI~UJK; DS~ 6 WDS;                             A1180345
              END;                                                      A118035
         SBCHECK(TEMP[1]);                                              A118036
         FILL ANS[*] WITH OCT2, OCT3, OCT5, OCT6, OCT7, OCT11;          A118037
         CVN~6;                                                         A118038
         PNO ~ 118;                                                     A118039
         VERIFY                                                         A118040
    END;                                                                A118041
  PROCEDURE P119; COMMENT STR PROC TOGGLE STATEMENT 5-12-13.7 KM.;      A119001
         BEGIN                                                          A119002
         INTEGER ONE, TWO,ZERO,SIX;                                     A119003
         STREAM PROCEDURE TESTTOGGLE(ARAYIN,INARAY,ONE,ZERO,TWO,SIX);   A119004
              VALUE ONE, ZERO, TWO, SIX;                                A119005
         BEGIN LABEL L1, EXITRIN, L2, L3;                               A119006
                   LOCAL A,B,C;                                         A119007
                   TALLY~0;                                             A119008
                   DI~LOC A; DS~8 LIT "A";                              A119009
                   SI ~ LOC A;                                          A119010
                   IF SC= "A" THEN GO TO L1;  COMMENT SET TOGGLE TRUE;  A119011
                   GO TO EXITRIN; COMMENT TOGGLE WAS NOT SET TRUE;      A119012
                   DI ~ INARAY;                                         A119013
              L1:  IF TOGGLE THEN DI ~ ARAYIN;                          A119014
                   IF TOGGLE THEN DI ~ DI+7;                            A119015
                   IF TOGGLE THEN SI~LOC A;                             A119016
                   IF TOGGLE THEN B~DI;                                 A119017
                   IF TOGGLE THEN DS~1 CHR; SI~SI-1;                    A119018
                   IF TOGGLE THEN DS~ ONE WDS; SI~SI-3;                 A119019
                   IF TOGGLE THEN DS~ ONE OCT;                          A119020
                   IF TOGGLE THEN DS~2 ADD;  COMMENT TOGGLE IS FALSE;   A119021
                   IF TOGGLE THEN GO TO EXITRIN; SI~SI-3;               A119022
                   IF SC="A" THEN GO TO L2 ; GO TO EXITRIN;             A119023
              L2:  COMMENT TOGGLE IS TRUE;   DI ~ DI+4;                 A119024
                   IF TOGGLE THEN DS ~ TWO ZON;                         A119025
                   IF TOGGLE THEN DS~ 8 LIT "B";                        A119026
                   IF TOGGLE THEN SKIP 42 DB;                           A119027
                   IF TOGGLE THEN DS ~ SIX SET;                         A119028
                   IF TOGGLE THEN 8(TALLY~TALLY+1);                     A119029
                   C~TALLY; SI~LOC C; DS~1 WDS; SI~LOC A;               A119030
                   IF 8 SC = DC THEN TALLY~0;                           A119031
                   IF TOGGLE THEN GO TO EXITRIN;                        A119032
                   C~TALLY; SI~LOC C; DS ~ 1 WDS; SI~LOC A;             A119033
                   IF SB THEN GO TO EXITRIN;                            A119034
                   IF TOGGLE THEN GO TO EXITRIN;  IF SC= "A" THEN GO TO A119035
                   L3;GO TO EXITRIN;                                    A119036
                   L3: IF TOGGLE THEN BEGIN 3( IF SC="E" THEN BEGIN IF  A119037
                   TOGGLE THEN JUMP OUT END; IF TOGGLE THEN TALLY ~7;   A119038
              JUMP OUT) END; C~TALLY; SI~LOC C; DS~1 WDS;               A119039
            EXITRIN:  END;                                              A119040
              ONE ~ 1;  TWO ~ 2; SIX ~ 6;  ZERO ~ 0;                    A119041
         TESTTOGGLE (TEMP[1],TEMP[22],ONE,ZERO,TWO,SIX);                A119042
                                                                        A119043
         FILL ANS[*] WITH OCT21, OCT2121212121212121, OCT1,             A119044
                          OCT0101000000002020, OCT2222222222222222,     A119045
                           OCT77, OCT10, OCT0, OCT10, OCT10;            A119046
         CVN~10;                                                        A119047
         PNO~119;                                                       A119048
         VERIFY                                                         A119049
    END;                                                                A119050
  PROCEDURE P120; COMMENT STREAM PROCEDURE SOURCE FOR ALPHA TEST        A120001
                   5.12.13.8, FEB 6.1963 KM.;                           A120002
    BEGIN                                                               A120003
              STREAM PROCEDURE TESTALPHA(ARAYIN);                       A120004
                   BEGIN                                                A120005
                        LOCAL A;                                        A120006
                        LABEL  L1,L2,L3,L4;                             A120007
         DI~LOC A; DS~8 LIT "ABC*109*"; TALLY~0;                        A120008
                        SI~LOC A; DI ~  ARAYIN;                         A120009
                        IF SC= ALPHA THEN BEGIN IF TOGGLE THEN DS~8 LIT A120010
                        "A"; DS~8 LIT"B"; GO TO L1 END; IF TOGGLE THEN  A120011
                        DS ~ 8 LIT "C" ; DS ~ 8 LIT "D";                A120012
                   L1:  SI~SI+3;                                        A120013
                        IF SC= ALPHA THEN BEGIN IF TOGGLE THEN DS~8 LIT A120014
                      "A"; DS~8 LIT "B"; GO TO L2 END; IF TOGGLE THEN   A120015
                        DS ~ 8 LIT "C"; DS~8 LIT "D";                   A120016
                   L2:  SI~ SI+3;                                       A120017
                        IF SC=ALPHA THEN BEGIN IF TOGGLE THEN DS~8 LIT  A120018
                        "A";DS~8 LIT"B"; GO TO L3 END; IF TOGGLE THEN   A120019
                        DS~8 LIT "C"; DS~8 LIT "D";                     A120020
                   L3:  SI~SI+1;                                        A120021
                        IF SC=ALPHA THEN BEGIN IF TOGGLE THEN DS~8      A120022
                        LIT"A"; DS~8 LIT"B"; GO TO L4 END; IF TOGGLE    A120023
                        THEN DS~8 LIT "C";DS~8 LIT "D";                 A120024
                   L4:  END;                                            A120025
         TESTALPHA(TEMP[1]);                                            A120026
         FILL ANS[*] WITH OCT2121212121212121, OCT2222222222222222,     A120027
                          OCT2424242424242424, OCT2121212121212121,     A120028
                          OCT2222222222222222, OCT2424242424242424;     A1200285
         CVN~6;                                                         A120029
         PNO ~ 120;                                                     A120030
         VERIFY                                                         A120031
    END;                                                                A120031
  PROCEDURE P121;COMMENT FOR STATEMENT MULTIPLE LIST ELEMENTS;          A121001
    BEGIN                                                               A121002
         REAL      E,F,C;                                               A121003
         INTEGER D,G,H;                                                 A1210035
         BOOLEAN   A,B;                                                 A121004
         INTEGER PROCEDURE  SEVEN (FML);                                A121005
         INTEGER  FML;                                                  A121006
         BEGIN SEVEN~ FML  MOD (FML+5) END;                             A121007
         E~ 15.5; F~ 7.5; C~ 2.0; A~ TRUE; B~ FALSE ;  D~ 2732 ;        A121008
         G~ 2; H~ 4;                                                    A121009
         TEMP[1]~ 15; TEMP[2]~ 3;                                       A121010
         FOR C~ TEMP[1]+ TEMP[2], SEVEN(10),"AB"+ "A", 5 DO BEGIN       A121011
         G~ G+1; TEMP[G]~ C+5 END;                                      A121012
         FOR C~  F-E + 16 STEP 1 UNTIL SEVEN (9),                       A121013
         D.[44:2] + D.[40:3] STEP D.[45:1]                              A121014
         UNTIL TEMP[1]-6, SEVEN(6)+2 STEP "AB" MOD "A" UNTIL "A" - 8 ,  A121015
         15 STEP 3 UNTIL                                                A121016
         20  DO BEGIN G~ G+1; TEMP [G]~ C END;                          A121017
         FOR C~ F-E  STEP (-F)  WHILE G< 16, SEVEN(2) | (-4)            A121018
         WHILE A OR B, 8                                                A121019
         STEP(-F) UNTIL - (SEVEN(0)) DO BEGIN G~ G+1; H~ H+1;           A121020
         A~ BOOLEAN(H.[45:1]);                                          A121021
         TEMP[G]~ SEVEN (C) END;                                        A121022
         FILL ANS [*] WITH OCT17, OCT3, OCT27, OCT17, OCT2150,          A121023
                        OCT12, OCT10, OCT11,        OCT10, OCT11,       A121024
                        OCT10, OCT11, OCT17, OCT22, OCT2000000000000002,A121025
                        OCT2000000000000005, OCT2000000000000002,       A121026
                        OCT2000000000000002, OCT10,                     A121027
                     OCT1;                                              A121028
         CVN~  20 ;                                                     A121029
         PNO~  121 ;                                                    A121030
         VERIFY                                                         A121031
    END;                                                                A121032
  PROCEDURE P122; COMMENT PRECEDENCE OF LOGICAL OPERATORS  EK           A122001
                        29 NOV 62;                                      A122002
    BEGIN                                                               A122003
         INTEGER X1,X2,X3,X4;                                           A122004
         BOOLEAN A1,A2,A3,A4,A5,A6,A7;                                  A122005
         X1 ~ X2 ~ 1;                                                   A122006
         X3 ~  3;                                                       A122007
         X4 ~ 4;                                                        A122008
         A1 ~ A2 ~ A3 ~ A4 ~ TRUE;                                      A122009
         A5 ~ A6 ~ A7 ~ FALSE;                                          A122010
              IF NOT X1!X2 THEN TEMP[1]~7 ELSE TEMP[1]~5;               A122011
         IF X4>X1+X2+X3   THEN TEMP[2]~ 5 ELSE TEMP[2] ~ 7;             A122012
         IF A1 OR A5 AND A6  THEN  TEMP[3] ~ 7 ELSE TEMP[3] ~ 5;        A122013
         IF A1 EQV A5 IMP A2 OR A6 AND A5 THEN TEMP[4]~7 ELSE TEMP[4]~5;A122014
         IF A6 EQV A5 AND A2 THEN TEMP[5]~ 7  ELSE TEMP[5] ~ 5;         A122015
         IF X4 < X3 EQV X1=X2 IMP A1  THEN TEMP[6]~ 5 ELSE TEMP[6]~ 7;  A122016
         IF A1 IMP A5 IMP A6  THEN TEMP[7] ~ 7 ELSE TEMP [7] ~ 5;       A122017
         IF X1=X2 OR NOT X1!X3 AND A5 IMP A6 EQV A5 OR X4>X1+X2+X3 AND  A122018
           NOT A5 IMP A4 AND X1=X2 THEN TEMP[8] ~ 5 ELSE TEMP[8] ~ 7;   A122019
         IF X4<X3 EQV NOT A1 IMP X1=X2 OR NOT X3>X4 THEN TEMP [9] ~ 5   A122020
              ELSE TEMP[9] ~ 7;                                         A122021
         IF NOT A5 OR X4<X1+X2 IMP A5 EQV X1=X2 IMP NOT X4<X1 AND A2    A122022
              THEN TEMP[10] ~ 5 ELSE TEMP[10] ~ 7;                      A122023
         IF NOT X4>X1 EQV X4>X1 AND A5 IMP NOT(A1 OR A5) THEN TEMP[11]  A122024
         ~5 ELSE TEMP[11] ~ 7;                                          A122025
         IF A5 EQV X4>X1+X2 OR A1 IMP FALSE THEN TEMP[12] ~ 7 ELSE      A122026
              TEMP[12]~ 5;                                              A122027
         IF A4 OR A1 AND  A6  AND NOT (A1 OR A5 AND A6 IMP X4<X1) THEN  A122028
              TEMP[13] ~ 7 ELSE TEMP[13] ~ 5;                           A122029
         IF NOT A1 EQV X4=X1 OR (A1 AND TRUE) AND A6 OR FALSE THEN      A122030
              TEMP[14] ~ 7 ELSE  TEMP[14]~ 5;                           A122031
         FILL ANS[*] WITH OCT7, OCT7, OCT7, OCT7, OCT7, OCT7, OCT7,     A122032
                          OCT7,OCT7, OCT7, OCT7, OCT7, OCT7, OCT7;      A122033
         CVN ~ 14;                                                      A122034
         PNO ~ 122;                                                     A122035
         VERIFY                                                         A122036
    END;                                                                A122037
  PROCEDURE P123;  COMMENT GO TO STATEMENT (PROCEDURE)  EK  20 DEC 62;  A123001
    BEGIN                                                               A123002
         LABEL L12, LLP, LL, L, LO3, LO2;                               A123003
         INTEGER I,J,K,SUM,B,C,A;                                       A123004
         BOOLEAN T,T2,T3,T4,T5,T6,T7,T8,TEST;                           A123005
         PROCEDURE PR1( I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,          A123006
                        LO2, LO3, LLP, L);                              A1230061
              INTEGER I,J,K,SUM,B,C,A;                                  A1230063
              BOOLEAN T , T2, T3, T4, T5, T7, TEST;                     A1230065
              LABEL     LO2, LO3, LLP, L;                               A1230067
              BEGIN LABEL L1;                                           A1230068
              L1:   BEGIN                                               A123007
                        LABEL LL1,LL2,LL3,LL4,L2,LL5,LLN,L1,L3,LM5,LL6; A123008
                        IF TEST THEN GO TO LL1;                         A123009
                        LL2:   IF T THEN GO TO L2;                      A123010
                        LL3:  I ~ I+1; TEMP[I] ~ I;                     A123011
                        GO TO IF  I< 2 THEN LL2 ELSE LL4;               A123012
                        LL4: GO TO IF I<3 THEN LL2 ELSE LL5;            A123013
                        LL5: IF I<4 THEN GO TO LL3;                     A123014
                        GO TO L2;                                       A123015
                        L2: IF J < 10 THEN GO TO IF SUM } K THEN LM5    A123016
                        ELSE LO3  ELSE GO TO LL6;                       A123017
                        LM5:  J ~ J+1;                                  A123018
                        IF T2  THEN GO TO LLP;                          A123019
                        FOR C ~ 1 STEP 1 UNTIL 10 DO SUM ~ SUM + 1;     A123020
                        GO TO L2;                                       A123021
                        LL6: BEGIN                                      A123022
                                  LABEL LNN,LTN,LTT;                    A123023
                                 REAL   DBSUM;                          A1230235
                                  IF T3 THEN GO LNN;                    A123024
                                  DBSUM ~ SUM + SUM ;                   A123025
                                  TEMP[B] ~ DBSUM; GO TO LTT;           A123026
                                  LNN: TEMP[B] ~ "BYPASS";              A123027
                                  IF T4  THEN GO TO LTN ;               A123028
                                  LTT:  GO TO L3;                       A123029
                                  LTN:                                  A123030
                             END;                                       A123031
                        L3:  BEGIN                                      A123032
                                  INTEGER X1;                           A123033
                                  IF T7 THEN GO TO L;                   A123034
                                  X1 ~ A;                               A123035
                                  IF T5 THEN GO TO LLN;                 A123036
                                  TEMP[X1] ~ A;                         A123037
                                  GO TO LLN;                            A123038
                             END;                                       A123039
                        LL1:  TEMP[6]  ~ "TST";                         A123040
                        GO TO LO2;                                      A123041
                        LLN:                                            A123042
                   END; END;                                            A123043
         T3 ~ T4 ~ T5 ~ TRUE; B~ 7;                                     A123044
         TEST ~ T ~ T2 ~ T7 ~ FALSE;                                    A123045
         I ~ J ~ K ~ SUM ~ 0;                                           A123046
          PR1(I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,LO2,LO3,LLP,L);     A123047
         TEMP[5] ~ SUM;                                                 A123048
         TEST ~ TRUE;                                                   A123049
         PR1 (I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,LO2,LO3,LLP,L);     A123050
         LO2: TEST ~ T3 ~ T5 ~ FALSE;                                   A123051
         T ~ TRUE;                                                      A123052
         B ~ 8;                                                         A123053
         A ~ 9;                                                         A123054
         PR1 (I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,LO2,LO3,LLP,L);     A123055
         TEMP[10] ~ "TEN";                                              A123056
         K ~ 1022;                                                      A123057
         J ~ 0;                                                         A123058
         PR1 (I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,LO2,LO3,LLP,L);     A123059
         LO3: TEMP[11] ~ "K300";                                        A123060
         J ~ K ~ 0;                                                     A123061
         T2 ~ TRUE;                                                     A123062
         PR1 (I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,LO2,LO3,LLP,L);     A123063
         LLP: TEMP[12] ~ "T2";                                          A123064
         J ~ 10;                                                        A123065
         T3 ~ T7 ~ TRUE;                                                A123066
         PR1 (I,J,K,SUM,B,C,A,T,T2,T3,T4,T5,T7,TEST,LO2,LO3,LLP,L);     A123067
         L:   TEMP[13] ~ "LUCKY";                                       A123068
         FILL ANS[*] WITH OCT1, OCT2, OCT3, OCT4, OCT144, OCT636263,    A123069
                          OCT227047216262, OCT227047216262,             A123070
                          OCT11, OCT632545, OCT42030000,                A123071
                         OCT6302, OCT4364234270;                        A1230715
         CVN ~ 13;                                                      A123072
         PNO ~ 123;                                                     A123073
         VERIFY                                                         A123074
    END;                                                                A123075
  PROCEDURE P124; COMMENT GO TO STATEMENT (SWITCHES)  EK  17 DEC 62;    A124001
    BEGIN                                                               A124002
         LABEL L1,L2,L3,L4,L5,L6,L7,L8,L9,L10,L11,L12,L14,L15,L16,L17;  A124003
         INTEGER X1,X2,Y1,Y2,I,K,L;                                     A124004
         SWITCH SW2 FORWARD ;                                           A1240046
         SWITCH SW3 FORWARD ;                                           A1240047
         SWITCH SW1 ~ L1,L2,L3,SW2[Y1],SW2[Y2+7],L6,L7,L15;             A124005
         SWITCH SW2 ~ L3,L4,L5,SW1[X1],SW1[X2],L8,L9,L16;               A124006
         SWITCH SW3 ~ L10, IF I=K THEN L11 ELSE L12,SW3[4],SW1[X2+3];   A124007
         Y1 ~ 1;                                                        A124008
         Y2 ~ Y1;                                                       A124009
         X1 ~ 7;                                                        A124010
         X2 ~ 5;                                                        A124011
         I ~ K ~ 2;                                                     A124012
         GO TO SW1[1];                                                  A124013
         L1:   TEMP[1] ~ 1;                                             A124014
         L2:   GO TO IF Y1= 1 THEN SW1[3] ELSE L5;                      A124015
         L3:  TEMP[2] ~ 2;                                              A124016
         Y1~Y1+1;                                                       A124017
         GO TO L2;                                                      A124018
         L5:  BEGIN                                                     A124019
                   INTEGER X1,X2;                                       A124020
                   X1 ~ 4;                                              A124021
                   X2 ~ 6;                                              A124022
                   GO TO SW2[X1];                                       A124023
              END;                                                      A124024
         L7:  TEMP[3] ~ 3;                                              A124025
         GO TO SW2[X2+2];                                               A124026
         L9:  TEMP[4] ~ 4;                                              A124027
         L ~ 0;                                                         A124028
         L14:  L ~ L+1;                                                 A124029
         GO TO SW3[L];                                                  A124030
         L10:  TEMP[5] ~ 5;                                             A124031
         GO TO L14;                                                     A124032
         L11:  TEMP[6] ~ 6;                                             A124033
         K ~ K+1;                                                       A124034
         GO TO SW3[L];                                                  A124035
         L12: TEMP[7] ~ 7;                                              A124036
         GO TO L14;                                                     A124037
         L15:  TEMP[8]~8;                                               A124038
         GO TO SW1[(X1-1)];                                             A124039
         L6:  BEGIN                                                     A124040
                   INTEGER X2;                                          A124041
                   X2 ~ 10;                                             A124042
                        BEGIN                                           A124043
                             INTEGER X1;                                A124044
                             X2 ~ 8;                                    A124045
                             GO TO SW2[X2-3]                            A124046
                        END                                             A124047
                   END;                                                 A124048
         L8: L4: L16: TEMP[9]~9;                                        A124049
                       BEGIN                                            A1240491
                         LABEL LA,LB,LC;                                A1240491
                         SWITCH SW4 ~ LA,LB,LC,L17;                     A1240492
                         Y1 ~ 2 ; GO TO SW4[Y1];                        A1240494
                   LA:   TEMP[10] ~ 0 ; GO TO LC;                       A1240495
                   LB:   TEMP[10] ~ 10;                                 A1240496
                   LC:   GO TO SW4[4] END;                              A1240497
                   L17:  TEMP[11] ~ 11;                                 A1240498
         FILL ANS[*] WITH OCT1, OCT2, OCT3, OCT4, OCT5, OCT6, OCT7,     A124050
                             OCT10, OCT11, OCT12,OCT13;                 A124051
         CVN ~11;                                                       A124052
         PNO ~ 124;                                                     A124053
         VERIFY                                                         A124054
    END;                                                                A124055
  PROCEDURE P125; COMMENT 5.11.3.2 PROCEDURE BODY VARIATIONS;           A125001
    BEGIN                                                               A125002
         LABEL L1,L2;                                                   A125003
         INTEGER I,J;BOOLEAN B;                                         A125004
         I ~ 6;J~12; B~ TRUE;                                           A125005
         COMMENT ASSIGNMENT STATEMENT;                                  A125007
         TEMP[1] ~ 1; TEMP[2]~TEMP[2]~TEMP[2]~I.[45:1];                 A125008
         TEMP[3].[46:2]~7;                                              A125009
         BEGIN                                                          A125010
         INTEGER PROCEDURE ARITH (I);                                   A125011
              INTEGER I;                                                A1250115
              BEGIN                                                     A1250117
                   TEMP[4] ~ (I+6)-(+2/(I-5)); ARITH ~ TEMP[4] END;     A1250119
         PROCEDURE BOOL (B);                                            A125012
              BOOLEAN B;                                                A1250125
              BEGIN                                                     A1250127
              TEMP[5] ~ REAL(B)  END;                                   A1250129
              PROCEDURE GOTO;                                           A125013
              BEGIN LABEL L1; GO TO L1; L1: TEMP[7]~4 END;              A125014
              PROCEDURE DUMMY;BEGIN END;                                A125015
         PROCEDURE CALL(I); INTEGER I; BEGIN TEMP[6] ~ 1 + ARITH(I) END;A125016
         CALL (I); BOOL(B); GOTO; DUMMY;                                A125017
         END;                                                           A125018
         FILL ANS[*] WITH OCT1, OCT1, OCT3, OCT12, OCT1, OCT13, OCT4;   A125019
         CVN~7; PNO~125;VERIFY                                          A125020
    END;                                                                A125021
  PROCEDURE P126; COMMENT 5.11.3.2 RECURSIVE PROCEDURES. CCZ, 11/28/62; A126  1
    BEGIN INTEGER I;                                                    A126  2
         REAL PROCEDURE FACTORIAL (N); VALUE N;  INTEGER N;             A126  3
         BEGIN FACTORIAL ~ IF N = 0 THEN 1 ELSE N| FACTORIAL(N-1) END;  A126  4
      FOR I ~ 1 STEP 1 UNTIL 10 DO TEMP[I] ~ FACTORIAL(I);              A126  5
         FILL ANS [*] WITH OCT1, OCT2, OCT6, OCT30, OCT170, OCT1320,    A126006
                        OCT11660, OCT116600, OCT1304600, OCT15657400;   A126007
      CVN ~ 10;                                                         A126  8
      PNO ~ 126;                                                        A126  9
      VERIFY                                                            A126 10
    END;                                                                A126 11
  PROCEDURE P127; COMMENT 5.11.4 TYPED PROCEDURES, C. ZETHRAEUS, 11/29; A127001
    BEGIN                                                               A127002
         REAL PROCEDURE RP(I); COMMENT RP= 4;                           A1270023
              INTEGER I;                                                A1270025
              BEGIN                                                     A127003
              REAL X, Y, Z; BOOLEAN B ;                                 A1270035
I~I+1; B~BOOLEAN(I.[44:1]); IF NOT B THEN Z~RP(I)+1;                    A1270045
              X~RP~Y~Z+1;                                               A1270047
              END;                                                      A127005
         INTEGER PROCEDURE IP1; COMMENT IP1 = 6;                        A127006
              BEGIN  INTEGER I;                                         A127007
              FOR I~ 1 STEP 1 UNTIL 6 DO  IP1~1;                        A127008
         END;                                                           A1270095
         INTEGER PROCEDURE IP2; COMMENT IP2 = 3;                        A127011
              BEGIN LABEL L1; INTEGER IPX;                              A127012
         FOR IPX~1 STEP 1 UNTIL 4 DO IF IPX = 3 THEN GO TO L1;          A127013
           L1: IP2~IPX END;                                             A127014
              INTEGER I;     I~0;                                       A1270145
                   TEMP[1]~RP(I); TEMP[2]~IP1; TEMP[3]~IP2;             A127015
         FILL ANS[*] WITH OCT17, OCT1, OCT3;                            A127016
         CVN ~ 3;                                                       A127017
         PNO ~ 127;                                                     A127018
         VERIFY                                                         A127019
    END;                                                                A127020
  PROCEDURE P128; COMMENT 5.11 PROCEDURE WEB, C.ZETHREAUS, 11/29/62;    A128001
    BEGIN PROCEDURE W66; TEMP[1] ~ TEMP[1] +1;                          A128002
          PROCEDURE W56; W66;                                           A128003
          PROCEDURE W46; W56;                                           A128004
          PROCEDURE W36; W46;                                           A128005
          PROCEDURE W26; W36;                                           A128006
          PROCEDURE W16; W26;                                           A128007
              PROCEDURE W65; BEGIN W66; W56 END;                        A128008
          PROCEDURE W55; BEGIN W65; W56; W46 END;                       A128009
          PROCEDURE W45; BEGIN W55; W46; W36 END;                       A128010
          PROCEDURE W35; BEGIN W45; W36; W26 END;                       A128011
          PROCEDURE W25; BEGIN W35; W26; W16 END;                       A128012
          PROCEDURE W15; BEGIN W25; W16 END;                            A128013
          PROCEDURE W64; BEGIN W65; W55 END;                            A128014
          PROCEDURE W54; BEGIN W64; W55; W45 END;                       A128015
          PROCEDURE W44; BEGIN W54; W45; W35 END;                       A128016
          PROCEDURE W34; BEGIN W44; W35; W25 END;                       A128017
          PROCEDURE W24; BEGIN W34; W25; W15 END;                       A128018
          PROCEDURE W14; BEGIN W24; W15 END;                            A128019
          PROCEDURE W63; BEGIN W64; W54 END;                            A128020
          PROCEDURE W53; BEGIN W63; W54; W44 END;                       A128021
          PROCEDURE W43; BEGIN W53; W44; W34 END;                       A128022
          PROCEDURE W33; BEGIN W43; W34; W24 END;                       A128023
          PROCEDURE W23; BEGIN W33; W24; W14 END;                       A128024
          PROCEDURE W13; BEGIN W23; W14 END;                            A128025
          PROCEDURE W62; BEGIN W63; W53 END;                            A128026
          PROCEDURE W52; BEGIN W62; W53; W43 END;                       A128027
          PROCEDURE W42; BEGIN W52; W43; W33 END;                       A128028
          PROCEDURE W32; BEGIN W42; W33; W23 END;                       A128029
          PROCEDURE W22; BEGIN W32; W23; W13 END;                       A128030
          PROCEDURE W12; BEGIN W22; W13 END;                            A128031
          PROCEDURE W61; BEGIN W62; W52 END;                            A128032
          PROCEDURE W51; BEGIN W61; W52; W42 END;                       A128033
          PROCEDURE W41; BEGIN W51; W42; W32 END;                       A128034
          PROCEDURE W31; BEGIN W41; W32; W22 END;                       A128035
          PROCEDURE W21; BEGIN W31; W22; W12 END;                       A128036
         PROCEDURE W11; BEGIN W21; W12 END;                             A128037
         W11;                                                           A128038
         FILL ANS[*] WITH OCT35314;                                     A128039
         CVN~1; PNO~128; VERIFY                                         A128040
    END;                                                                A128041
  PROCEDURE P131; COMMENT STREAM ELSE STATEMENT 5.12.13.1 9/4/63 ;      A131001
    BEGIN                                                               A131002
    BOOLEAN TRU,FLS;                                                    A131003
    INTEGER TOGT;                                                       A131004
    STREAM PROCEDURE THENELSE(TA,TB,TC,TD,TE,TF,TG,TH,TI,TJ,TK,         A131005
                             TOGT,TRU,FLS);                             A131006
       VALUE TOGT,TRU,FLS;                                              A131007
         BEGIN                                                          A131008
           LOCAL T1;                                                    A131009
           LABEL L1,L2,L3,L4,L5,L6,L7,L8,L9,L10,L11,L12,L13;            A131010
           COMMENT SET TOGGLE TRUE ;                                    A131011
              SI ~ LOC  TOGT; DI ~ LOC T1; DS ~ 1 WDS ; DI ~ DI-1;      A131012
              SI ~ SI-1;                                                A131013
           COMMENT TEST THEN STMT IS EXECUTED,ELSE STMT IS IGNORED;     A131014
              IF 1 SC = DC THEN GO TO L1 ELSE GO TO L2;                 A131015
              SI ~ LOC TRU; DI ~ TB; DS ~ 1 WDS;                        A131016
          L1: SI ~ LOC TRU; DI ~ TA; DS ~ 1 WDS; GO TO L3;              A131017
          L2: SI ~ LOC FLS; DI ~ TA; DS ~ 1 WDS;                        A131018
          L3: IF TOGGLE THEN BEGIN SI ~ LOC TRU; DI ~ TC; DS ~ 1 WDS    A131019
                             END                                        A131020
                        ELSE GO TO L4 ; GO TO L5;                       A131021
          L4: SI ~ LOC FLS; DI ~ TC; DS ~ 1 WDS;                        A131022
          L5: IF TOGGLE THEN TALLY ~ TOGT                               A131023
                        ELSE TALLY ~ FLS;                               A131024
              T1 ~ TALLY; SI ~ LOC T1; DI ~ TD; DS ~ 1 WDS;             A131025
           COMMENT TEST THEN STMT IS IGNORED, ELSE STMT IS EXECUTED;    A131026
              SI ~ LOC TOGT; DI ~ LOC FLS; IF 8 SC = DC                 A131027
                   THEN TALLY ~ FLS                                     A131028
                   ELSE TALLY ~ TRU;                                    A131029
              T1 ~ TALLY;SI ~ LOC T1; DI ~ TE; DS ~ 1 WDS;              A131030
              SI ~ LOC TRU; DI ~ TF; DS ~ 1 WDS;                        A131031
              IF TOGGLE THEN BEGIN SI ~ LOC FLS; DI ~ TG; DS ~ 1 WDS    A131032
                             END                                        A131033
                        ELSE GO TO L6;                                  A131034
              SI ~ LOC TRU; DI ~ TH; DS ~ 1 WDS; GO TO L7;              A131035
          L6: SI ~ LOC TRU; DI ~ TG; DS ~ 1 WDS;                        A131036
          L7: IF TOGGLE THEN GO TO L8                                   A131037
                        ELSE BEGIN SI ~ LOC TRU; DI ~ TI; DS ~ 1 WDS    A131038
                             END; GO TO L8;                             A131039
              SI ~ LOC TRU; DI ~ TJ; DS ~ 1 WDS;                        A131040
          L8: IF TOGGLE THEN GO TO L10                                  A131041
                        ELSE BEGIN                                      A131042
                             TALLY ~ FLS;                               A131044
                             8(IF TOGGLE THEN GO TO L12                 A131045
                                       ELSE BEGIN                       A131046
                                            IF TOGGLE THEN GO TO L13    A131047
                                                      ELSE              A131050
                                                         TALLY ~ TRU;   A131051
                                                         JUMP OUT;      A131052
                                              END;                      A131053
                                 L12: TALLY ~ FLS; JUMP OUT;            A131054
                                 L13: TALLY ~ TOGT; JUMP OUT;)          A131055
                             END;                                       A131056
          L9: T1 ~ TALLY; SI ~ LOC T1; DI ~ TK; DS ~ 1WDS ;             A131057
              GO TO L11;                                                A131058
         L10: TALLY ~ FLS  ; GO TO L9;                                  A131059
         L11: END THENELSE;                                             A131060
    TRU ~ TRUE;                                                         A131100
    FLS ~ FALSE;                                                        A131101
    TOGT ~ 9;                                                           A131102
    THENELSE (TEMP[1],TEMP[2],TEMP[3],TEMP[4],TEMP[5],                  A131103
              TEMP[6],TEMP[7],TEMP[8],TEMP[9],TEMP[10],TEMP[11],        A131104
              TOGT,TRU,FLS);                                            A1311041
    FILL ANS[*] WITH OCT1, OCT0, OCT1, OCT11, OCT1, OCT1,               A131105
                     OCT1, OCT0, OCT1, OCT0, OCT1;                      A131106
    CVN ~ 11 ;                                                          A131107
    PNO ~ 131;                                                          A131108
     VERIFY                                                             A131109
    END P131 ;                                                          A131110
