                   SI ~ LOC F6;                                         A112039
                   DS ~ 1 WDS;                                          A112040
                   TALLY ~ 0; DI ~ F7;                                  A112041
                   3(3(TALLY ~ TALLY+1); TALLY ~ TALLY+1);              A112043
                     F7 ~ TALLY;                                          A11204
                   SI~ LOC F7;                                          A112045
                   DS ~ 1 WDS;                                          A112046
                   DI ~ F9 ;                                            A112047
                   SI ~ F8 ;                                            A112048
                   8 (IF SC = "A" THEN JUMP OUT; DS ~ 1 WDS);           A112049
                   4 (IF 1 SC = DC THEN BEGIN IF 1 SC = DC THEN         A112050
                      DS ~ 2 CHR END);                                  A112051
                   2( SKIP 1 SB; SKIP 1 DB;                             A1120511
                      IF  SB    THEN 2 (4(DS ~ 1 CHR);););              A112052
                   DI ~ DI-2; SI ~ SI-2;                                A1120521
                   2 (IF SC = ALPHA THEN BEGIN IF TOGGLE THEN           A112053
                      DS ~ 4 CHR END) ;                                 A112054
                   TALLY ~ 0;                                           A112055
                   3 (IF 8 SC = DC THEN 8 (IF SC = ALPHA THEN           A112056
                   JUMP OUT 2 TO L5;                                    A1120561
                      TALLY ~ TALLY+1);TALLY ~ TALLY +1);               A112057
               L6: DI ~ F10;                                            A112058
                   F10 ~ TALLY;                                         A112059
                   SI ~ LOC F10;                                        A112060
                   DS ~ 1 WDS; GO TO L7;                                A112061
               L5: DS ~ 5 LIT "0"; DS ~ 3 LIT "SWD";                    A112062
                   GO TO L6 ; L7:                                       A1120621
         END;                                                           A112063
         FILL C[*] WITH OCT212223, OCT12345670, OCT3654321012345670,    A112064
         OCT207,  OCT33, OCT0101010101010101, OCT0202020202020202,      A112065
