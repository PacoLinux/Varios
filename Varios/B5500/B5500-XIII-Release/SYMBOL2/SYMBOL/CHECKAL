  PROCEDURE P14; COMMENT ARITHMETIC OPERATOR /   EK   7 NOV 62;         A014001
    BEGIN                                                               A014002
         INTEGER J,K;                                                   A014003
         REAL A,B;                                                      A014004
         ARRAY Y[1:3];                                                  A014005
         REAL PROCEDURE PROD (A,B); REAL A, B;                          A014006
              BEGIN                                                     A014007
                   PROD~A|B                                             A014008
              END;                                                      A014009
         TEMP[1]~1023/2;                                                A014010
         TEMP[2]~1023/1023;                                             A014011
         TEMP[3]~ -0/1;                                                 A014012
         J~ 2;                                                          A014013
         K~ 10;                                                         A014014
         A~125;                                                         A014015
         B~5;                                                           A014016
         Y[1]~Y[2]~Y[3]~15;                                             A014017
         TEMP[5]~ "#KEHE"/"#KEHE";                                      A014018
         TEMP[6]~Y[2]/B;                                                A014019
         TEMP[7]~ K/J;                                                  A014020
         TEMP[8]~A/B;                                                   A014021
         TEMP[9]~K/B;                                                   A014022
         TEMP[10]~PROD(A,B)/125;                                        A014023
         TEMP[11]~A|B/(20+B);                                           A014024
         TEMP[12]~A.[39:3]/1;                                           A014025
         FILL ANS[*] WITH OCT1010000000007774, OCT1,                    A0140255
                           OCT2000000000000000, OCT0, OCT1, OCT3,       A014026
                           OCT5, OCT31, OCT2, OCT5, OCT31, OCT1;        A014027
         CVN~ 12;                                                       A014028
         PNO~ 14;                                                       A014029
